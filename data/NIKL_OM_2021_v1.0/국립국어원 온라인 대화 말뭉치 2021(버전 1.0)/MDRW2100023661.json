      "utterance": [
        {
          "id": "MDRW2100023661.1.1",
          "form": "그래도 고소하긴하겠다",
          "original_form": "그래도 고소하긴하겠다",
          "speaker_id": "2",
          "time": "20210830 14:03"
        },
        {
          "id": "MDRW2100023661.1.2",
          "form": "믹서기 있지!ㅎㅎ",
          "original_form": "믹서기 있지!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:03"
        },
        {
          "id": "MDRW2100023661.1.3",
          "form": "맞아 맛있어",
          "original_form": "맞아 맛있어",
          "speaker_id": "1",
          "time": "20210830 14:03"
        },
        {
          "id": "MDRW2100023661.1.4",
          "form": "그럼 한번 해봐! 대신 빨리 상하니까 조금씩 만들어",
          "original_form": "그럼 한번 해봐! 대신 빨리 상하니까 조금씩 만들어",
          "speaker_id": "1",
          "time": "20210830 14:04"
        },
        {
          "id": "MDRW2100023661.1.5",
          "form": "오늘한번 해먹어 봐야겠군",
          "original_form": "오늘한번 해먹어 봐야겠군",
          "speaker_id": "2",
          "time": "20210830 14:04"
        },
        {
          "id": "MDRW2100023661.1.6",
          "form": "하루이틀안에 먹어야하나?ㅠ",
          "original_form": "하루이틀안에 먹어야하나?ㅠ",
          "speaker_id": "2",
          "time": "20210830 14:04"
        },
        {
          "id": "MDRW2100023661.1.7",
          "form": "아냐 그정도는 아니고 음.. 3~5일?ㅋㅋ",
          "original_form": "아냐 그정도는 아니고 음.. 3~5일?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:06"
        },
        {
          "id": "MDRW2100023661.1.8",
          "form": "근데 물은 안생겨?",
          "original_form": "근데 물은 안생겨?",
          "speaker_id": "2",
          "time": "20210830 14:06"
        },
        {
          "id": "MDRW2100023661.1.9",
          "form": "생겼던것같기도?",
          "original_form": "생겼던것같기도?",
          "speaker_id": "1",
          "time": "20210830 14:07"
        },
        {
          "id": "MDRW2100023661.1.10",
          "form": "먹기전에 섞어 먹음 되니까",
          "original_form": "먹기전에 섞어 먹음 되니까",
          "speaker_id": "1",
          "time": "20210830 14:07"
        },
        {
          "id": "MDRW2100023661.1.11",
          "form": "섞으면 사라질정도야",
          "original_form": "섞으면 사라질정도야",
          "speaker_id": "1",
          "time": "20210830 14:07"
        },
        {
          "id": "MDRW2100023661.1.12",
          "form": "그래도 꽤 보관할수있네",
          "original_form": "그래도 꽤 보관할수있네",
          "speaker_id": "2",
          "time": "20210830 14:07"
        },
        {
          "id": "MDRW2100023661.1.13",
          "form": "그정도면 괜찮겠군",
          "original_form": "그정도면 괜찮겠군",
          "speaker_id": "2",
          "time": "20210830 14:08"
        },
        {
          "id": "MDRW2100023661.1.14",
          "form": "두부 물 한번 안 짜고 그냥 한거지?",
          "original_form": "두부 물 한번 안 짜고 그냥 한거지?",
          "speaker_id": "2",
          "time": "20210830 14:08"
        },
        {
          "id": "MDRW2100023661.1.15",
          "form": "응ㅋㅋ 가끔먹으면 맛있어",
          "original_form": "응ㅋㅋ 가끔먹으면 맛있어",
          "speaker_id": "1",
          "time": "20210830 14:08"
        },
        {
          "id": "MDRW2100023661.1.16",
          "form": "응 안짰어!",
          "original_form": "응 안짰어!",
          "speaker_id": "1",
          "time": "20210830 14:08"
        },
        {
          "id": "MDRW2100023661.1.17",
          "form": "다이어트할 때 먹으면 딱이겠다",
          "original_form": "다이어트할 때 먹으면 딱이겠다",
          "speaker_id": "2",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100023661.1.18",
          "form": "근데 마요네즈같은 제형이 나와?",
          "original_form": "근데 마요네즈같은 제형이 나와?",
          "speaker_id": "2",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100023661.1.19",
          "form": "응! 완전 부드러워ㅋㅋ",
          "original_form": "응! 완전 부드러워ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023661.1.20",
          "form": "뻑뻑하다싶음 그때 기름 조금 넣어줘도 될듯?",
          "original_form": "뻑뻑하다싶음 그때 기름 조금 넣어줘도 될듯?",
          "speaker_id": "1",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023661.1.21",
          "form": "오호! 그렇구만 오늘 시도해보겠어!",
          "original_form": "오호! 그렇구만 오늘 시도해보겠어!",
          "speaker_id": "2",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100023661.1.22",
          "form": "찌개용 써야하나?",
          "original_form": "찌개용 써야하나?",
          "speaker_id": "2",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100023661.1.23",
          "form": "응 찌개용이 나을거 같아",
          "original_form": "응 찌개용이 나을거 같아",
          "speaker_id": "1",
          "time": "20210830 14:19"
        },
        {
          "id": "MDRW2100023661.1.24",
          "form": "먹어보고 후기알려줘ㅋㅋ",
          "original_form": "먹어보고 후기알려줘ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:20"
        },
        {
          "id": "MDRW2100023661.1.25",
          "form": "오케  찌개용 사야지",
          "original_form": "오케  찌개용 사야지",
          "speaker_id": "2",
          "time": "20210830 14:20"
        },
        {
          "id": "MDRW2100023661.1.26",
          "form": "그랭ㅋㅋ",
          "original_form": "그랭ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:20"
        }
      ]
    }
  ]
