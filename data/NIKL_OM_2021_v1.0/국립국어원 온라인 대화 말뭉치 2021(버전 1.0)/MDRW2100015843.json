      "utterance": [
        {
          "id": "MDRW2100015843.1.1",
          "form": "커피 좋아하세여?",
          "original_form": "커피 좋아하세여?",
          "speaker_id": "2",
          "time": "20210826 01:59"
        },
        {
          "id": "MDRW2100015843.1.2",
          "form": "네네 ㅎname2님 닉넴보니 커피쿠크다스 생각나요",
          "original_form": "네네 ㅎ&name2&님 닉넴보니 커피쿠크다스 생각나요",
          "speaker_id": "1",
          "time": "20210826 02:01"
        },
        {
          "id": "MDRW2100015843.1.3",
          "form": "ㅋㅋㅋㅋ커피큐크다스 맛잇어용",
          "original_form": "ㅋㅋㅋㅋ커피큐크다스 맛잇어용",
          "speaker_id": "2",
          "time": "20210826 02:01"
        },
        {
          "id": "MDRW2100015843.1.4",
          "form": "저도 그거좋아해요",
          "original_form": "저도 그거좋아해요",
          "speaker_id": "1",
          "time": "20210826 02:01"
        },
        {
          "id": "MDRW2100015843.1.5",
          "form": "빨간색보다 커피맛이 좋아유",
          "original_form": "빨간색보다 커피맛이 좋아유",
          "speaker_id": "2",
          "time": "20210826 02:02"
        },
        {
          "id": "MDRW2100015843.1.6",
          "form": "커피는 달달커피마셔요ㅎ",
          "original_form": "커피는 달달커피마셔요ㅎ",
          "speaker_id": "1",
          "time": "20210826 02:02"
        },
        {
          "id": "MDRW2100015843.1.7",
          "form": "그 큐크다스 빵같응거 나왓덩데 드셔보셧어요?",
          "original_form": "그 큐크다스 빵같응거 나왓덩데 드셔보셧어요?",
          "speaker_id": "2",
          "time": "20210826 02:02"
        },
        {
          "id": "MDRW2100015843.1.8",
          "form": "그래요? 어디서요?",
          "original_form": "그래요? 어디서요?",
          "speaker_id": "1",
          "time": "20210826 02:02"
        },
        {
          "id": "MDRW2100015843.1.9",
          "form": "마트가면 한번씩 있더라구요! 추천드림다",
          "original_form": "마트가면 한번씩 있더라구요! 추천드림다",
          "speaker_id": "2",
          "time": "20210826 02:03"
        },
        {
          "id": "MDRW2100015843.1.10",
          "form": "커피는 그럼 아아는 안드세여?",
          "original_form": "커피는 그럼 아아는 안드세여?",
          "speaker_id": "2",
          "time": "20210826 02:03"
        },
        {
          "id": "MDRW2100015843.1.11",
          "form": "네 물을 잘안마시는데 아아도 그냥쓴물느낌이라^^",
          "original_form": "네 물을 잘안마시는데 아아도 그냥쓴물느낌이라^^",
          "speaker_id": "1",
          "time": "20210826 02:04"
        },
        {
          "id": "MDRW2100015843.1.12",
          "form": "아메 좋아하세요?",
          "original_form": "아메 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 02:04"
        },
        {
          "id": "MDRW2100015843.1.13",
          "form": "아ㅋㅋㅋㅋ쓴물 ㅠ 네 저는 좋아해요 ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ쓴물 ㅠ 네 저는 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:06"
        },
        {
          "id": "MDRW2100015843.1.14",
          "form": "음료는 단거는 잘 안먹는편이에여 ㅋㅋㅋ",
          "original_form": "음료는 단거는 잘 안먹는편이에여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:06"
        },
        {
          "id": "MDRW2100015843.1.15",
          "form": "엇 곶감은 좋아하시는데",
          "original_form": "엇 곶감은 좋아하시는데",
          "speaker_id": "1",
          "time": "20210826 02:07"
        },
        {
          "id": "MDRW2100015843.1.16",
          "form": "의외네요",
          "original_form": "의외네요",
          "speaker_id": "1",
          "time": "20210826 02:07"
        },
        {
          "id": "MDRW2100015843.1.17",
          "form": "아 곶감은 다른분이신가봐요ㅎ",
          "original_form": "아 곶감은 다른분이신가봐요ㅎ",
          "speaker_id": "1",
          "time": "20210826 02:07"
        },
        {
          "id": "MDRW2100015843.1.18",
          "form": "ㅋㅋㅋㅋㅋ저도 곶감은 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 곶감은 좋아해요",
          "speaker_id": "2",
          "time": "20210826 02:08"
        },
        {
          "id": "MDRW2100015843.1.19",
          "form": "디저트류 단거 좋아하는데 음료만 별로 안좋아해여 ㅋㅋ",
          "original_form": "디저트류 단거 좋아하는데 음료만 별로 안좋아해여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:08"
        },
        {
          "id": "MDRW2100015843.1.20",
          "form": "아ㅎ크리스피 도넛 먹을땐 아메필요해요",
          "original_form": "아ㅎ크리스피 도넛 먹을땐 아메필요해요",
          "speaker_id": "1",
          "time": "20210826 02:09"
        },
        {
          "id": "MDRW2100015843.1.21",
          "form": "오 그때는 드시나봐요ㅋㅋㅋㅋ",
          "original_form": "오 그때는 드시나봐요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:09"
        },
        {
          "id": "MDRW2100015843.1.22",
          "form": "도넛 먹고싶네요 ㅠㅠ",
          "original_form": "도넛 먹고싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 02:09"
        },
        {
          "id": "MDRW2100015843.1.23",
          "form": "사실 오늘 파바가서 먹긴했어요 ㅎ",
          "original_form": "사실 오늘 파바가서 먹긴했어요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 02:10"
        },
        {
          "id": "MDRW2100015843.1.24",
          "form": "아주가끔요ㅎ 일년에 몇번",
          "original_form": "아주가끔요ㅎ 일년에 몇번",
          "speaker_id": "1",
          "time": "20210826 02:10"
        },
        {
          "id": "MDRW2100015843.1.25",
          "form": "요기요하셨나요?",
          "original_form": "요기요하셨나요?",
          "speaker_id": "1",
          "time": "20210826 02:10"
        },
        {
          "id": "MDRW2100015843.1.26",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:10"
        },
        {
          "id": "MDRW2100015843.1.27",
          "form": "ㅋㅋㅋㅋ네네 식빵좋아해서 살겸 도넛도 담앗어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ네네 식빵좋아해서 살겸 도넛도 담앗어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:11"
        },
        {
          "id": "MDRW2100015843.1.28",
          "form": "파바 도넛 의외로 맛있더라구여",
          "original_form": "파바 도넛 의외로 맛있더라구여",
          "speaker_id": "2",
          "time": "20210826 02:11"
        },
        {
          "id": "MDRW2100015843.1.29",
          "form": "빵좋아하시는 군요",
          "original_form": "빵좋아하시는 군요",
          "speaker_id": "1",
          "time": "20210826 02:11"
        },
        {
          "id": "MDRW2100015843.1.30",
          "form": "엊그제 나이트쿠폰으로 거의공짜로샀는더",
          "original_form": "엊그제 나이트쿠폰으로 거의공짜로샀는더",
          "speaker_id": "1",
          "time": "20210826 02:12"
        },
        {
          "id": "MDRW2100015843.1.31",
          "form": "오늘 15000원이상 담아야돼서 패스했어요",
          "original_form": "오늘 15000원이상 담아야돼서 패스했어요",
          "speaker_id": "1",
          "time": "20210826 02:12"
        },
        {
          "id": "MDRW2100015843.1.32",
          "form": "오 글쿤요 어젠 뭐사셧어요?",
          "original_form": "오 글쿤요 어젠 뭐사셧어요?",
          "speaker_id": "2",
          "time": "20210826 02:13"
        },
        {
          "id": "MDRW2100015843.1.33",
          "form": "에그베이컨토스트랑 고구마갈릭페스츄리",
          "original_form": "에그베이컨토스트랑 고구마갈릭페스츄리",
          "speaker_id": "1",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100015843.1.34",
          "form": "허니 카스테라요",
          "original_form": "허니 카스테라요",
          "speaker_id": "1",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100015843.1.35",
          "form": "오 고구마갈릭페스튜리 맛잇나요? 사려다 말앗는데",
          "original_form": "오 고구마갈릭페스튜리 맛잇나요? 사려다 말앗는데",
          "speaker_id": "2",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015843.1.36",
          "form": "전  마늘빵 좋아해서 맛나더라구요",
          "original_form": "전  마늘빵 좋아해서 맛나더라구요",
          "speaker_id": "1",
          "time": "20210826 02:18"
        },
        {
          "id": "MDRW2100015843.1.37",
          "form": "고구마도 있어서",
          "original_form": "고구마도 있어서",
