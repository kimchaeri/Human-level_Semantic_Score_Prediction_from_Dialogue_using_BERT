      "utterance": [
        {
          "id": "MDRW2100023004.1.1",
          "form": "그래도 초성힌트 안 쓰시고 맞추셨네요!",
          "original_form": "그래도 초성힌트 안 쓰시고 맞추셨네요!",
          "speaker_id": "2",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100023004.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 아무거나 다 던지고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 아무거나 다 던지고",
          "speaker_id": "2",
          "time": "20210829 20:14"
        },
        {
          "id": "MDRW2100023004.1.3",
          "form": "힌트 있는 거 다 쓴 것 같은데ㅋㅋㅋ",
          "original_form": "힌트 있는 거 다 쓴 것 같은데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:14"
        },
        {
          "id": "MDRW2100023004.1.4",
          "form": "그럼 이미 먹은 거 말고",
          "original_form": "그럼 이미 먹은 거 말고",
          "speaker_id": "2",
          "time": "20210829 20:14"
        },
        {
          "id": "MDRW2100023004.1.5",
          "form": "크기가 다양하다는 힌트가 주요했어요ㅋㅋㅋ",
          "original_form": "크기가 다양하다는 힌트가 주요했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:14"
        },
        {
          "id": "MDRW2100023004.1.6",
          "form": "지금 먹고싶은거 맞추기 해볼래요?!",
          "original_form": "지금 먹고싶은거 맞추기 해볼래요?!",
          "speaker_id": "2",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100023004.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100023004.1.8",
          "form": "오오오 추리왕",
          "original_form": "오오오 추리왕",
          "speaker_id": "2",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100023004.1.9",
          "form": "아 좋아요 좋아요",
          "original_form": "아 좋아요 좋아요",
          "speaker_id": "1",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100023004.1.10",
          "form": "전 아직도 초록소스 정체를 모르겠어요",
          "original_form": "전 아직도 초록소스 정체를 모르겠어요",
          "speaker_id": "2",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100023004.1.11",
          "form": "와사빈가요",
          "original_form": "와사빈가요",
          "speaker_id": "2",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100023004.1.12",
          "form": "그거 아보카도로 만든 거요",
          "original_form": "그거 아보카도로 만든 거요",
          "speaker_id": "1",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100023004.1.13",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100023004.1.14",
          "form": "과카몰리!",
          "original_form": "과카몰리!",
          "speaker_id": "1",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100023004.1.15",
          "form": "와 전 초록이래서",
          "original_form": "와 전 초록이래서",
          "speaker_id": "2",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100023004.1.16",
          "form": "와사비나 키위소스인줄 알았잖아요",
          "original_form": "와사비나 키위소스인줄 알았잖아요",
          "speaker_id": "2",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023004.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023004.1.18",
          "form": "이번엔 name1 위시음식 맞춰볼게요",
          "original_form": "이번엔 &name1& 위시음식 맞춰볼게요",
          "speaker_id": "2",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023004.1.19",
          "form": "면 요리인가요?",
          "original_form": "면 요리인가요?",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023004.1.20",
          "form": "저 지금 저녁 먹을건데",
          "original_form": "저 지금 저녁 먹을건데",
          "speaker_id": "1",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023004.1.21",
          "form": "저녁 메뉴 맞히기는 어떠세요",
          "original_form": "저녁 메뉴 맞히기는 어떠세요",
          "speaker_id": "1",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023004.1.22",
          "form": "좋아요좋아요",
          "original_form": "좋아요좋아요",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023004.1.23",
          "form": "질문 주세요ㅋㅋㅋㅋ",
          "original_form": "질문 주세요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023004.1.24",
          "form": "부리또 또 드시나요?",
          "original_form": "부리또 또 드시나요?",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023004.1.25",
          "form": "아니요 부리또 좋아하지만 그 정돈 아니에요ㅋㅋㅋㅋㅋ",
          "original_form": "아니요 부리또 좋아하지만 그 정돈 아니에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023004.1.26",
          "form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023004.1.27",
          "form": "매운 음식 드세요?",
          "original_form": "매운 음식 드세요?",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023004.1.28",
          "form": "앗 맞아요 매운 음식 나의사랑",
          "original_form": "앗 맞아요 매운 음식 나의사랑",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023004.1.29",
          "form": "면 요리인가요?",
          "original_form": "면 요리인가요?",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023004.1.30",
          "form": "음 아닙니다",
          "original_form": "음 아닙니다",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023004.1.31",
          "form": "근데 저희집은 당면을 넣어먹기도 해요",
          "original_form": "근데 저희집은 당면을 넣어먹기도 해요",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100023004.1.32",
          "form": "좀 어려울 거 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "좀 어려울 거 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100023004.1.33",
          "form": "음 음 음 음 음 음 음",
          "original_form": "음 음 음 음 음 음 음",
          "speaker_id": "2",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100023004.1.34",
          "form": "닭고기인가요?",
          "original_form": "닭고기인가요?",
          "speaker_id": "2",
          "time": "20210829 20:21"
        }
      ]
    }
