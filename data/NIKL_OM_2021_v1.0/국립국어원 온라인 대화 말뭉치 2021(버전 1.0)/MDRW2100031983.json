      "utterance": [
        {
          "id": "MDRW2100031983.1.1",
          "form": "name2님은 내일 출근하세요~?",
          "original_form": "&name2&님은 내일 출근하세요~?",
          "speaker_id": "1",
          "time": "20210905 10:19"
        },
        {
          "id": "MDRW2100031983.1.2",
          "form": "네 출근... 해야죠 ㅠㅠ",
          "original_form": "네 출근... 해야죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 10:20"
        },
        {
          "id": "MDRW2100031983.1.3",
          "form": "name1님도 출근 하시나요?",
          "original_form": "&name1&님도 출근 하시나요?",
          "speaker_id": "2",
          "time": "20210905 10:20"
        },
        {
          "id": "MDRW2100031983.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 10:21"
        },
        {
          "id": "MDRW2100031983.1.5",
          "form": "으악 누가 주4일제 도입해주면",
          "original_form": "으악 누가 주4일제 도입해주면",
          "speaker_id": "1",
          "time": "20210905 10:21"
        },
        {
          "id": "MDRW2100031983.1.6",
          "form": "대통령으로.....",
          "original_form": "대통령으로.....",
          "speaker_id": "1",
          "time": "20210905 10:21"
        },
        {
          "id": "MDRW2100031983.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:21"
        },
        {
          "id": "MDRW2100031983.1.8",
          "form": "아 ㅋㅋㅋㅋ 너무 좋은 생각이에요",
          "original_form": "아 ㅋㅋㅋㅋ 너무 좋은 생각이에요",
          "speaker_id": "2",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100031983.1.9",
          "form": "주 4일 한다면 언제 쉬고 싶으세요?",
          "original_form": "주 4일 한다면 언제 쉬고 싶으세요?",
          "speaker_id": "2",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100031983.1.10",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "1",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100031983.1.11",
          "form": "어렵네요",
          "original_form": "어렵네요",
          "speaker_id": "1",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100031983.1.12",
          "form": "월요일이요",
          "original_form": "월요일이요",
          "speaker_id": "1",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100031983.1.13",
          "form": "대신 화요병이 생기겠죠?ㅋㅋㅋ",
          "original_form": "대신 화요병이 생기겠죠?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031983.1.14",
          "form": "ㅋㅋㅋ 맞아욬ㅋㅋ",
          "original_form": "ㅋㅋㅋ 맞아욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031983.1.15",
          "form": "그래서 일하는데 맥이 끊기긴 하겠지만",
          "original_form": "그래서 일하는데 맥이 끊기긴 하겠지만",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031983.1.16",
          "form": "수요일!!",
          "original_form": "수요일!!",
          "speaker_id": "1",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031983.1.17",
          "form": "수요일즈음 쉬고 싶어요 ㅋㅋㅋ",
          "original_form": "수요일즈음 쉬고 싶어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031983.1.18",
          "form": "도 좋죠",
          "original_form": "도 좋죠",
          "speaker_id": "1",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031983.1.19",
          "form": "이틀 일하고 하루 놀고",
          "original_form": "이틀 일하고 하루 놀고",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031983.1.20",
          "form": "이틀 일하고 하루 놀고 ㅋㅋ",
          "original_form": "이틀 일하고 하루 놀고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031983.1.21",
          "form": "작년에 가을에 수요일 계속 휴일인적있었는데 세상 좋았어요",
          "original_form": "작년에 가을에 수요일 계속 휴일인적있었는데 세상 좋았어요",
          "speaker_id": "1",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031983.1.22",
          "form": "진짜 최고였는데 그쵸 ㅋㅋ",
          "original_form": "진짜 최고였는데 그쵸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031983.1.23",
          "form": "맞아요 !! 대체공휴일이 이제 많이 안남아서 슬퍼요",
          "original_form": "맞아요 !! 대체공휴일이 이제 많이 안남아서 슬퍼요",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100031983.1.24",
          "form": "그래도 올해는 대체 공휴일로 많이 생겨서 속으로 은근하 위로를 하고 있어요 ㅋㅋ",
          "original_form": "그래도 올해는 대체 공휴일로 많이 생겨서 속으로 은근하 위로를 하고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031983.1.25",
          "form": "ㅋㅋㅋㅋ맞습니당",
          "original_form": "ㅋㅋㅋㅋ맞습니당",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031983.1.26",
          "form": "이거마저 없었으면 정말 지옥이었을거에요 흑흑",
          "original_form": "이거마저 없었으면 정말 지옥이었을거에요 흑흑",
