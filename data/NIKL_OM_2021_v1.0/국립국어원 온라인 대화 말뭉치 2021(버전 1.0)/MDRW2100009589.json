      "utterance": [
        {
          "id": "MDRW2100009589.1.1",
          "form": "학교 다니고 계세요?",
          "original_form": "학교 다니고 계세요?",
          "speaker_id": "1",
          "time": "20210823 01:13"
        },
        {
          "id": "MDRW2100009589.1.2",
          "form": ":)",
          "original_form": ":)",
          "speaker_id": "1",
          "time": "20210823 01:13"
        },
        {
          "id": "MDRW2100009589.1.3",
          "form": "아니요 학교 안다녀요",
          "original_form": "아니요 학교 안다녀요",
          "speaker_id": "2",
          "time": "20210823 01:13"
        },
        {
          "id": "MDRW2100009589.1.4",
          "form": "학교다니시나요",
          "original_form": "학교다니시나요",
          "speaker_id": "2",
          "time": "20210823 01:13"
        },
        {
          "id": "MDRW2100009589.1.5",
          "form": "지금 대학생이에요",
          "original_form": "지금 대학생이에요",
          "speaker_id": "1",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009589.1.6",
          "form": "캠퍼스 라이프~",
          "original_form": "캠퍼스 라이프~",
          "speaker_id": "2",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009589.1.7",
          "form": "지금은 방학이라 백수인데",
          "original_form": "지금은 방학이라 백수인데",
          "speaker_id": "1",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009589.1.8",
          "form": "요즘 대면 수업 했나요?",
          "original_form": "요즘 대면 수업 했나요?",
          "speaker_id": "2",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009589.1.9",
          "form": "30일부터는 다시 자유를 뺏겨요",
          "original_form": "30일부터는 다시 자유를 뺏겨요",
          "speaker_id": "1",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009589.1.10",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009589.1.11",
          "form": "아 개학 하는구나",
          "original_form": "아 개학 하는구나",
          "speaker_id": "2",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009589.1.12",
          "form": "저는 특수목적대라 그런지",
          "original_form": "저는 특수목적대라 그런지",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009589.1.13",
          "form": "학기마다",
          "original_form": "학기마다",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009589.1.14",
          "form": "4주 대면 개학 했어요",
          "original_form": "4주 대면 개학 했어요",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009589.1.15",
          "form": "어떤걸 전공하고있나요?",
          "original_form": "어떤걸 전공하고있나요?",
          "speaker_id": "2",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009589.1.16",
          "form": "학년씩 돌아가면서요",
          "original_form": "학년씩 돌아가면서요",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009589.1.17",
          "form": "교육이요",
          "original_form": "교육이요",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009589.1.18",
          "form": "그리고 실습은 대면으로 하고요",
          "original_form": "그리고 실습은 대면으로 하고요",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009589.1.19",
          "form": "실습은 어때요",
          "original_form": "실습은 어때요",
          "speaker_id": "2",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009589.1.20",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009589.1.21",
          "form": "죽는 줄 알았어요",
          "original_form": "죽는 줄 알았어요",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009589.1.22",
          "form": "너무 바빠서",
          "original_form": "너무 바빠서",
          "speaker_id": "1",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100009589.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100009589.1.24",
          "form": "많이 배우기도 배웠는데",
          "original_form": "많이 배우기도 배웠는데",
          "speaker_id": "1",
          "time": "20210823 01:18"
        }
      ]
