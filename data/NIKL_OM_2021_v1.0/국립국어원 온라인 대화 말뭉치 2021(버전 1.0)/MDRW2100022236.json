      "utterance": [
        {
          "id": "MDRW2100022236.1.1",
          "form": "혹시 백신 맞으셨어요?",
          "original_form": "혹시 백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022236.1.2",
          "form": "저 어제 맞았는데",
          "original_form": "저 어제 맞았는데",
          "speaker_id": "1",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100022236.1.3",
          "form": "네 저는 다가오는 금요일에 2차 맞아요",
          "original_form": "네 저는 다가오는 금요일에 2차 맞아요",
          "speaker_id": "2",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022236.1.4",
          "form": "백신 맞고 괜찮으세요?",
          "original_form": "백신 맞고 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022236.1.5",
          "form": "어제 밤에 열나서 약먹고 잤어요",
          "original_form": "어제 밤에 열나서 약먹고 잤어요",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022236.1.6",
          "form": "갑자기 막 춥고 ㅜㅜ",
          "original_form": "갑자기 막 춥고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022236.1.7",
          "form": "에고 백신 맞고 건강이 안좋으셨군요",
          "original_form": "에고 백신 맞고 건강이 안좋으셨군요",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022236.1.8",
          "form": "지금은 괜찮으세요?",
          "original_form": "지금은 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022236.1.9",
          "form": "지금은 좀 나아졌어요",
          "original_form": "지금은 좀 나아졌어요",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022236.1.10",
          "form": "며칠 조심해야 한다고 하더라구여",
          "original_form": "며칠 조심해야 한다고 하더라구여",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022236.1.11",
          "form": "네네 후유증이 좀 지나서 나타나는 경우도 있어서",
          "original_form": "네네 후유증이 좀 지나서 나타나는 경우도 있어서",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022236.1.12",
          "form": "당분간 무리한 운동은 하지 마시고",
          "original_form": "당분간 무리한 운동은 하지 마시고",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022236.1.13",
          "form": "편히 쉬면서 지내셔요",
          "original_form": "편히 쉬면서 지내셔요",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022236.1.14",
          "form": "name2님은 어떠셨어요?",
          "original_form": "&name2&님은 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022236.1.15",
          "form": "큰이상 없었나요?",
          "original_form": "큰이상 없었나요?",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022236.1.16",
          "form": "네 저는 다음날 팔만 많이 아프고",
          "original_form": "네 저는 다음날 팔만 많이 아프고",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022236.1.17",
          "form": "건강히 잘 지나갔어요",
          "original_form": "건강히 잘 지나갔어요",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022236.1.18",
          "form": "열도 안나구요?",
          "original_form": "열도 안나구요?",
          "speaker_id": "1",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022236.1.19",
          "form": "네 열도 거의 안났어요",
          "original_form": "네 열도 거의 안났어요",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022236.1.20",
          "form": "근데 1차때 저랑 비슷하게 경미한 증상이었던 친구가",
          "original_form": "근데 1차때 저랑 비슷하게 경미한 증상이었던 친구가",
          "speaker_id": "2",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022236.1.21",
          "form": "2차때엔 심하게 앓았다 해서 걱정은 돼요",
          "original_form": "2차때엔 심하게 앓았다 해서 걱정은 돼요",
          "speaker_id": "2",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022236.1.22",
          "form": "보통 2차때 많이 아픈가봐요",
          "original_form": "보통 2차때 많이 아픈가봐요",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022236.1.23",
          "form": "드물게 1차에 아프고",
          "original_form": "드물게 1차에 아프고",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022236.1.24",
          "form": "1차때 아픈 사람 중엔",
          "original_form": "1차때 아픈 사람 중엔",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022236.1.25",
          "form": "자기도 모르는 사이 코로나를 앓아서",
          "original_form": "자기도 모르는 사이 코로나를 앓아서",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022236.1.26",
          "form": "2차의 증상이 나타나는 경우도 있다고 하더라고요",
          "original_form": "2차의 증상이 나타나는 경우도 있다고 하더라고요",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022236.1.27",
          "form": "화이자는 2차때 더 증상이 심하게 나오는 게 맞다고",
          "original_form": "화이자는 2차때 더 증상이 심하게 나오는 게 맞다고",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022236.1.28",
          "form": "예방주사가 이렇게 아프면",
          "original_form": "예방주사가 이렇게 아프면",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022236.1.29",
          "form": "어떡하라는건지ㅜㅜ",
          "original_form": "어떡하라는건지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022236.1.30",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022236.1.31",
          "form": "질병 예방하려고 맞는건데",
          "original_form": "질병 예방하려고 맞는건데",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022236.1.32",
          "form": "아프다고 하니 그것도 참...",
          "original_form": "아프다고 하니 그것도 참...",
          "speaker_id": "2",
          "time": "20210829 14:18"
        }
      ]
    }
