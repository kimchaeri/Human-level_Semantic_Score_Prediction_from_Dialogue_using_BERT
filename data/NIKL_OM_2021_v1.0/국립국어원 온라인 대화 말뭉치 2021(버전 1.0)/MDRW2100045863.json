      "utterance": [
        {
          "id": "MDRW2100045863.1.1",
          "form": "가을 좋아하세요?",
          "original_form": "가을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045863.1.2",
          "form": "네좋아해요 ㅋㅋ",
          "original_form": "네좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045863.1.3",
          "form": "짧아서 문제지만",
          "original_form": "짧아서 문제지만",
          "speaker_id": "1",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100045863.1.4",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100045863.1.5",
          "form": "앞으로는 완전 없어질듯 ㅎㅎ",
          "original_form": "앞으로는 완전 없어질듯 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100045863.1.6",
          "form": "트렌치코트의 계절ㅠㅠㅠ",
          "original_form": "트렌치코트의 계절ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100045863.1.7",
          "form": "안돼요ㅠㅠㅠ",
          "original_form": "안돼요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100045863.1.8",
          "form": "일주일 입으면 잘 입는 거죠ㅋㅋㅠㅠ",
          "original_form": "일주일 입으면 잘 입는 거죠ㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100045863.1.9",
          "form": "ㅋㅋㅋ 저는 키가 작아서 트렌치 안어울려서 슬퍼요",
          "original_form": "ㅋㅋㅋ 저는 키가 작아서 트렌치 안어울려서 슬퍼요",
          "speaker_id": "1",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100045863.1.10",
          "form": "뭔가 어정쩡한느낌",
          "original_form": "뭔가 어정쩡한느낌",
          "speaker_id": "1",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100045863.1.11",
          "form": "저도 그렇긴해요ㅠㅠ",
          "original_form": "저도 그렇긴해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045863.1.12",
          "form": "그래도 입고싶어서 어거지루 입어요ㅋㅋㅋ",
          "original_form": "그래도 입고싶어서 어거지루 입어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045863.1.13",
          "form": "그래도 봄 가을은 옷 입기 너무 재밌어요",
          "original_form": "그래도 봄 가을은 옷 입기 너무 재밌어요",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045863.1.14",
          "form": "맞이요",
          "original_form": "맞이요",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045863.1.15",
          "form": "이번에또 나갈곳도없으면서",
          "original_form": "이번에또 나갈곳도없으면서",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045863.1.16",
          "form": "자켓 샀네요....ㅎ",
          "original_form": "자켓 샀네요....ㅎ",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045863.1.17",
          "form": "가을에 한번입을듯...ㅋㅋㅋ",
          "original_form": "가을에 한번입을듯...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045863.1.18",
          "form": "에이! 그래도 이쁘잖아요ㅎㅎㅎㅎ",
          "original_form": "에이! 그래도 이쁘잖아요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045863.1.19",
          "form": "저는 자라에서 가죽원피스 샀는데",
          "original_form": "저는 자라에서 가죽원피스 샀는데",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045863.1.20",
          "form": "입고나가지도 못할 거",
          "original_form": "입고나가지도 못할 거",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045863.1.21",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045863.1.22",
          "form": "예뻐서 샀어요ㅠㅠ",
          "original_form": "예뻐서 샀어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045863.1.23",
          "form": "근데 진짜 못입을 거 같아요ㅋㅋㅋㅋ",
          "original_form": "근데 진짜 못입을 거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045863.1.24",
          "form": "아니 평소에 자주입을걸 사야하는데",
          "original_form": "아니 평소에 자주입을걸 사야하는데",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045863.1.25",
          "form": "저도 자꾸",
          "original_form": "저도 자꾸",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045863.1.26",
          "form": "가끔 한번입을거만사요",
          "original_form": "가끔 한번입을거만사요",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045863.1.27",
          "form": "화려한 거만 눈에 들어와요ㅠㅠ",
          "original_form": "화려한 거만 눈에 들어와요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045863.1.28",
          "form": "그래서 입을옷 또 없고",
          "original_form": "그래서 입을옷 또 없고",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045863.1.29",
          "form": "근데 진짜 입을 수 있을까 싶고ㅜㅜ",
          "original_form": "근데 진짜 입을 수 있을까 싶고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045863.1.30",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "2",
          "time": "20210908 08:16"
        }
      ]
    }
  ]
