        },
        {
          "id": "MDRW2100027907.1.3",
          "form": "여행 좋아하니?",
          "original_form": "여행 좋아하니?",
          "speaker_id": "1",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027907.1.4",
          "form": "좋아하지",
          "original_form": "좋아하지",
          "speaker_id": "2",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027907.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027907.1.6",
          "form": "바쁘니?",
          "original_form": "바쁘니?",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027907.1.7",
          "form": "핸드폰으로 해야 해서 들킬까 봐",
          "original_form": "핸드폰으로 해야 해서 들킬까 봐",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027907.1.8",
          "form": "뒤에 부사장님 계시거든",
          "original_form": "뒤에 부사장님 계시거든",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027907.1.9",
          "form": "여행 가고 싶다",
          "original_form": "여행 가고 싶다",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027907.1.10",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027907.1.11",
          "form": "name3 결혼잔에 여행한번 가야되는거 아니냐",
          "original_form": "&name3& 결혼잔에 여행한번 가야되는거 아니냐",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027907.1.12",
          "form": "모든 말에 여행이라는 말을 놓자",
          "original_form": "모든 말에 여행이라는 말을 놓자",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027907.1.13",
          "form": "말을 포함하자",
          "original_form": "말을 포함하자",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027907.1.14",
          "form": "구래",
          "original_form": "구래",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027907.1.15",
          "form": "여디니 결혼하면 우리랑 여행 안가준다고",
          "original_form": "여디니 결혼하면 우리랑 여행 안가준다고",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027907.1.16",
          "form": "하기 전에 여행가야돼",
          "original_form": "하기 전에 여행가야돼",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027907.1.17",
          "form": "맞아 name3가 우리랑 여행 안 가 줄 거야",
          "original_form": "맞아 &name3&가 우리랑 여행 안 가 줄 거야",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027907.1.18",
          "form": "그럼 어디로 여행갈래?",
          "original_form": "그럼 어디로 여행갈래?",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027907.1.19",
          "form": "독도가자",
          "original_form": "독도가자",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027907.1.20",
          "form": "독도 왜 가고 싶은데?",
          "original_form": "독도 왜 가고 싶은데?",
          "speaker_id": "2",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027907.1.21",
          "form": "제주도 지겨우니까",
          "original_form": "제주도 지겨우니까",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027907.1.22",
          "form": "너 겨울에 제주도 가서 그런거잖아",
          "original_form": "너 겨울에 제주도 가서 그런거잖아",
          "speaker_id": "2",
          "time": "20210902 16:40"
        },
        {
          "id": "MDRW2100027907.1.23",
          "form": "나도 제주도 가고싶어",
          "original_form": "나도 제주도 가고싶어",
          "speaker_id": "2",
          "time": "20210902 16:41"
        },
        {
          "id": "MDRW2100027907.1.24",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100027907.1.25",
          "form": "나 제주도 가기 전에 운전 마스터한다",
