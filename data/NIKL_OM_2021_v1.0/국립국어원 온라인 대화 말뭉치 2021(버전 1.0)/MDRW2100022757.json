      "utterance": [
        {
          "id": "MDRW2100022757.1.1",
          "form": "티를 한 장 샀는데",
          "original_form": "티를 한 장 샀는데",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022757.1.2",
          "form": "분명 상세페이지에는 하나도 안 비치고 탄탄한 것처럼 되어있길래",
          "original_form": "분명 상세페이지에는 하나도 안 비치고 탄탄한 것처럼 되어있길래",
          "speaker_id": "2",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022757.1.3",
          "form": "이거다 하고 샀단말이예요 저 탈브라해서",
          "original_form": "이거다 하고 샀단말이예요 저 탈브라해서",
          "speaker_id": "2",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022757.1.4",
          "form": "티 한 장만 입는디",
          "original_form": "티 한 장만 입는디",
          "speaker_id": "2",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022757.1.5",
          "form": "근데 도착해서 입어보니까",
          "original_form": "근데 도착해서 입어보니까",
          "speaker_id": "2",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022757.1.6",
          "form": "너무 얇아서 가슴이랑 눈 마주쳐서",
          "original_form": "너무 얇아서 가슴이랑 눈 마주쳐서",
          "speaker_id": "2",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022757.1.7",
          "form": "ㅋㄱㅋㄱㅋㅋㄱㄱㅋㄱㅋㄱㅋ",
          "original_form": "ㅋㄱㅋㄱㅋㅋㄱㄱㅋㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022757.1.8",
          "form": "아이컨텍하셨군요,,",
          "original_form": "아이컨텍하셨군요,,",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022757.1.9",
          "form": "ㅋㅋㅋㅋㅋ네 정말 상세샷을 믿었는데!!!",
          "original_form": "ㅋㅋㅋㅋㅋ네 정말 상세샷을 믿었는데!!!",
          "speaker_id": "2",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022757.1.10",
          "form": "진짜 재질사기 자주당해요",
          "original_form": "진짜 재질사기 자주당해요",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022757.1.11",
          "form": "전 검은 블라우스 샀는데",
          "original_form": "전 검은 블라우스 샀는데",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022757.1.12",
          "form": "겉이 반질반질뻔쩍거리는게",
          "original_form": "겉이 반질반질뻔쩍거리는게",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022757.1.13",
          "form": "무슨 조폭마냥",
          "original_form": "무슨 조폭마냥",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022757.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022757.1.15",
          "form": "진짜 알았으면 안 샀지!! 소리 절로 나오지 않나요",
          "original_form": "진짜 알았으면 안 샀지!! 소리 절로 나오지 않나요",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022757.1.16",
          "form": "그거입구 검은 슬랙스 입고 회사갔다가",
          "original_form": "그거입구 검은 슬랙스 입고 회사갔다가",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022757.1.17",
          "form": "동기가 혹시 일수나왔냐고해서",
          "original_form": "동기가 혹시 일수나왔냐고해서",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022757.1.18",
          "form": "ㅌㅋㅋㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022757.1.19",
          "form": "어 멋지냐?,,",
          "original_form": "어 멋지냐?,,",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022757.1.20",
          "form": "컨셉임 ...",
          "original_form": "컨셉임 ...",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022757.1.21",
          "form": "일수블라우스와",
          "original_form": "일수블라우스와",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022757.1.22",
          "form": "아이컨텍 티샤쓰와",
          "original_form": "아이컨텍 티샤쓰와",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022757.1.23",
          "form": "인민자켓",
          "original_form": "인민자켓",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022757.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022757.1.25",
          "form": "ㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱ",
          "original_form": "ㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022757.1.26",
          "form": "골때리네요",
          "original_form": "골때리네요",
          "speaker_id": "1",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022757.1.27",
          "form": "통수 몇 번 맞고",
          "original_form": "통수 몇 번 맞고",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022757.1.28",
          "form": "저는 갈색 검은색 이런 옷 좋아하거든요",
          "original_form": "저는 갈색 검은색 이런 옷 좋아하거든요",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022757.1.29",
          "form": "이젠 되도록이면 스파에서 사요",
          "original_form": "이젠 되도록이면 스파에서 사요",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022757.1.30",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022757.1.31",
          "form": "근데 퍼스널컬러가 너무 궁금한거에요",
          "original_form": "근데 퍼스널컬러가 너무 궁금한거에요",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022757.1.32",
          "form": "아씨 이걸 7~8만원이나 줘야돼? 너무 비싼데",
          "original_form": "아씨 이걸 7~8만원이나 줘야돼? 너무 비싼데",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022757.1.33",
          "form": "넘...넘 궁금한거죠",
          "original_form": "넘...넘 궁금한거죠",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022757.1.34",
          "form": "자꾸 주변친구들도 해봤다고 하고",
          "original_form": "자꾸 주변친구들도 해봤다고 하고",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022757.1.35",
          "form": "삘받아서 다녀왔는데 여름쿨라이트라고",
          "original_form": "삘받아서 다녀왔는데 여름쿨라이트라고",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022757.1.36",
          "form": "지금입는 옷 다 버리라는거에요",
          "original_form": "지금입는 옷 다 버리라는거에요",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022757.1.37",
          "form": "어유 세상에",
          "original_form": "어유 세상에",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022757.1.38",
          "form": "근데 그냥 무시중",
          "original_form": "근데 그냥 무시중",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022757.1.39",
          "form": "옷당 다 갈아엎으래요?",
          "original_form": "옷당 다 갈아엎으래요?",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022757.1.40",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022757.1.41",
          "form": "옷장",
          "original_form": "옷장",
          "speaker_id": "2",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022757.1.42",
          "form": "ㅋㅋㅋㄷㅋㄷㅋㄷㅋㄷㅋㄷㅋㄷㅋ",
          "original_form": "ㅋㅋㅋㄷㅋㄷㅋㄷㅋㄷㅋㄷㅋㄷㅋ",
          "speaker_id": "1",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022757.1.43",
          "form": "돈이으딧노!!",
          "original_form": "돈이으딧노!!",
          "speaker_id": "1",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022757.1.44",
          "form": "와 아까워 어케 엎어요",
          "original_form": "와 아까워 어케 엎어요",
          "speaker_id": "2",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022757.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022757.1.46",
          "form": "갈색블랙 대잔치 다 바꾸려면",
          "original_form": "갈색블랙 대잔치 다 바꾸려면",
          "speaker_id": "2",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022757.1.47",
          "form": "여쿨라가 진짜 입을만한 색상이 너무 없더라구요",
          "original_form": "여쿨라가 진짜 입을만한 색상이 너무 없더라구요",
          "speaker_id": "1",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022757.1.48",
          "form": "텅장되겠는데요",
          "original_form": "텅장되겠는데요",
          "speaker_id": "2",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022757.1.49",
          "form": "저 진짜 회사 직원이",
          "original_form": "저 진짜 회사 직원이",
          "speaker_id": "1",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022757.1.50",
          "form": "혹시 갈색애호가냐고 물어봤을정도로",
          "original_form": "혹시 갈색애호가냐고 물어봤을정도로",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022757.1.51",
          "form": "갈색톤 상의가 많고든요 ㅋㅋㅋㅋㅋ",
          "original_form": "갈색톤 상의가 많고든요 ㅋㅋㅋㅋㅋ",
