      "utterance": [
        {
          "id": "MDRW2100023090.1.1",
          "form": "지금 못 시키나?",
          "original_form": "지금 못 시키나?",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023090.1.2",
          "form": "지금은 약간 늦지 않았나?",
          "original_form": "지금은 약간 늦지 않았나?",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023090.1.3",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023090.1.4",
          "form": "내일 시켜먹을까?",
          "original_form": "내일 시켜먹을까?",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023090.1.5",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023090.1.6",
          "form": "어떤 스타일 와플을 먹고 싶은데?",
          "original_form": "어떤 스타일 와플을 먹고 싶은데?",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023090.1.7",
          "form": "그냥 아무거나",
          "original_form": "그냥 아무거나",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023090.1.8",
          "form": "벨기에식도 좋아해?",
          "original_form": "벨기에식도 좋아해?",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023090.1.9",
          "form": "벨기에 식이 어떤데?",
          "original_form": "벨기에 식이 어떤데?",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023090.1.10",
          "form": "조금 두껍고 더 쫀쫀한 거",
          "original_form": "조금 두껍고 더 쫀쫀한 거",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023090.1.11",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023090.1.12",
          "form": "맛있지 근데 바삭한 느낌은 없어",
          "original_form": "맛있지 근데 바삭한 느낌은 없어",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023090.1.13",
          "form": "난 바삭하지 않아도 맛있으면 됨",
          "original_form": "난 바삭하지 않아도 맛있으면 됨",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023090.1.14",
          "form": "그런 거면 홍콩식이 진짜 맛있어",
          "original_form": "그런 거면 홍콩식이 진짜 맛있어",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023090.1.15",
          "form": "다 먹어봤어?",
          "original_form": "다 먹어봤어?",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023090.1.16",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023090.1.17",
          "form": "난 떡 와플도 먹어봤어",
          "original_form": "난 떡 와플도 먹어봤어",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023090.1.18",
          "form": "난 나라별로는 안먹어봐서",
          "original_form": "난 나라별로는 안먹어봐서",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023090.1.19",
          "form": "나는 먹다보니까 다 먹게 되었어",
          "original_form": "나는 먹다보니까 다 먹게 되었어",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023090.1.20",
          "form": "여행 가서 먹었던 것 중에 제일 맛있거나 기억에 남는 거  뭐야?",
          "original_form": "여행 가서 먹었던 것 중에 제일 맛있거나 기억에 남는 거  뭐야?",
          "speaker_id": "1",
          "time": "20210829 21:51"
        }
      ]
