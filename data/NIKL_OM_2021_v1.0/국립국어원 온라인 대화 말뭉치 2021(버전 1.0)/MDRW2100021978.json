      "utterance": [
        {
          "id": "MDRW2100021978.1.1",
          "form": "와 근데 장거리연애 힘들지 않으세요?",
          "original_form": "와 근데 장거리연애 힘들지 않으세요?",
          "speaker_id": "1",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021978.1.2",
          "form": "힘들죠ㄹㅎ",
          "original_form": "힘들죠ㄹㅎ",
          "speaker_id": "2",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021978.1.3",
          "form": "젊었을때야 하지",
          "original_form": "젊었을때야 하지",
          "speaker_id": "2",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021978.1.4",
          "form": "나이들어선 못해요ㅎㅎㅎ",
          "original_form": "나이들어선 못해요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021978.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ전 못 할 거 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ전 못 할 거 같아요",
          "speaker_id": "1",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021978.1.6",
          "form": "지금은 여자친구 있으신가여?",
          "original_form": "지금은 여자친구 있으신가여?",
          "speaker_id": "1",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021978.1.7",
          "form": "전 아내가있답니당ㅎㅎㅎㅎㄹ",
          "original_form": "전 아내가있답니당ㅎㅎㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021978.1.8",
          "form": "결혼했고ㅎ 곧 이혼해요ㅎㅎ",
          "original_form": "결혼했고ㅎ 곧 이혼해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021978.1.9",
          "form": "물어보셔서ㅎㅎ",
          "original_form": "물어보셔서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021978.1.10",
          "form": "엥 진짜여??",
          "original_form": "엥 진짜여??",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021978.1.11",
          "form": "헐 글쿠나...",
          "original_form": "헐 글쿠나...",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021978.1.12",
          "form": "전 아직 결혼도 못 해봐서 ㅋㅋ",
          "original_form": "전 아직 결혼도 못 해봐서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021978.1.13",
          "form": "아아ㅎㅎ",
          "original_form": "아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021978.1.14",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021978.1.15",
          "form": "이건 너무 실례죠?",
          "original_form": "이건 너무 실례죠?",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021978.1.16",
          "form": "그냥 궁금해서 물어봐요",
          "original_form": "그냥 궁금해서 물어봐요",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021978.1.17",
          "form": "좋은 짝을 안만나셨어요?",
          "original_form": "좋은 짝을 안만나셨어요?",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021978.1.18",
          "form": "아녀? 걍 ㅋㅋ 마땅한 사람이 없어서여",
          "original_form": "아녀? 걍 ㅋㅋ 마땅한 사람이 없어서여",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021978.1.19",
          "form": "네 ㅋㅋㅋ 이렇다 할 사람이 없네용",
          "original_form": "네 ㅋㅋㅋ 이렇다 할 사람이 없네용",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021978.1.20",
          "form": "다 만족하면서 살순없으니깐ㅎ",
          "original_form": "다 만족하면서 살순없으니깐ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021978.1.21",
          "form": "전 결혼을 했잖아요 ㅎㅎ",
          "original_form": "전 결혼을 했잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021978.1.22",
          "form": "근데 이혼을 하게됫어요ㅎㅎ",
          "original_form": "근데 이혼을 하게됫어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021978.1.23",
          "form": "어쩔수없는거에요ㅎ 사람일이라는건 모르듯이ㅎㅎ",
          "original_form": "어쩔수없는거에요ㅎ 사람일이라는건 모르듯이ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021978.1.24",
          "form": "그니깐 하고싶은더 하고사세요ㅎㅎㅎㅎ",
          "original_form": "그니깐 하고싶은더 하고사세요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021978.1.25",
          "form": "ㅋㅋㅋㅋ 좋은 말이네여",
          "original_form": "ㅋㅋㅋㅋ 좋은 말이네여",
          "speaker_id": "1",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021978.1.26",
          "form": "전 약간... 인연을 믿는 편이라 ㅋㅋㅋ",
          "original_form": "전 약간... 인연을 믿는 편이라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021978.1.27",
          "form": "정말 뭐 짝이있음 결혼을 하겠고 못하면 못 하겠거니~ 하고 생각하고 살거있어요",
          "original_form": "정말 뭐 짝이있음 결혼을 하겠고 못하면 못 하겠거니~ 하고 생각하고 살거있어요",
          "speaker_id": "1",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021978.1.28",
          "form": "언제 어디서든 나타날지몰라요ㅎ",
          "original_form": "언제 어디서든 나타날지몰라요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021978.1.29",
          "form": "아니면 주위에 있을지도ㅎㅎ",
          "original_form": "아니면 주위에 있을지도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021978.1.30",
          "form": "항상 안테나를 키고 생활하셔야해요 ㅎ",
          "original_form": "항상 안테나를 키고 생활하셔야해요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021978.1.31",
          "form": "흠...ㅋㅋㅋㅋㅋ 일단 옆에는 없는 것 같아요",
          "original_form": "흠...ㅋㅋㅋㅋㅋ 일단 옆에는 없는 것 같아요",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021978.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ 더 살아보면 나타날지도 모르겠지만요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 더 살아보면 나타날지도 모르겠지만요",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021978.1.33",
          "form": "아니에요ㅎ",
          "original_form": "아니에요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021978.1.34",
          "form": "잘찾아보세요ㅎㅎ",
          "original_form": "잘찾아보세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021978.1.35",
          "form": "옆에서 마느리님 보고계실지도몰라요ㅎㅎ",
          "original_form": "옆에서 마느리님 보고계실지도몰라요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021978.1.36",
          "form": "그건 좀 무서운데요 ㅋㅋㅋㅋㅋ",
          "original_form": "그건 좀 무서운데요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021978.1.37",
          "form": "일단 이 놈은(?) 아닌 것 같구...",
          "original_form": "일단 이 놈은(?) 아닌 것 같구...",
          "speaker_id": "1",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021978.1.38",
          "form": "ㅎㅎ 좋아하시는분없으세요?",
          "original_form": "ㅎㅎ 좋아하시는분없으세요?",
          "speaker_id": "2",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021978.1.39",
          "form": "마음가는분?",
          "original_form": "마음가는분?",
          "speaker_id": "2",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021978.1.40",
          "form": "지금은 없어요 ㅋㅋ",
          "original_form": "지금은 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:41"
        },
        {
          "id": "MDRW2100021978.1.41",
          "form": "이쁘신게 그래요ㅎ",
          "original_form": "이쁘신게 그래요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:41"
        },
        {
          "id": "MDRW2100021978.1.42",
          "form": "원래 이쁜사람들은그래ㅎㅎ",
          "original_form": "원래 이쁜사람들은그래ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:42"
        },
        {
          "id": "MDRW2100021978.1.43",
          "form": "절 머 언제 보셨다고 ㅋㅋㅋㅋㅋ",
          "original_form": "절 머 언제 보셨다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:42"
        }
      ]
