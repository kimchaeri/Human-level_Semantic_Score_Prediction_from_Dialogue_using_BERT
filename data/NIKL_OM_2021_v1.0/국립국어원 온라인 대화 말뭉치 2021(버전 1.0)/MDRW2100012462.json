      "utterance": [
        {
          "id": "MDRW2100012462.1.1",
          "form": "난 애들옷 신랑꺼 싹다 돌려요~",
          "original_form": "난 애들옷 신랑꺼 싹다 돌려요~",
          "speaker_id": "2",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100012462.1.2",
          "form": "우리신랑 옷 맨날 렁닝",
          "original_form": "우리신랑 옷 맨날 렁닝",
          "speaker_id": "1",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100012462.1.3",
          "form": "탱크탑되서 ㅋㅋㅋㅋㄱ",
          "original_form": "탱크탑되서 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012462.1.4",
          "form": "그만 좀 돌리라고 ㅋㅋㅋ 배꼽티라 입을게 없다고 ㅋㅋㅋㅋ",
          "original_form": "그만 좀 돌리라고 ㅋㅋㅋ 배꼽티라 입을게 없다고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012462.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012462.1.6",
          "form": "너는게 없나 그럼??",
          "original_form": "너는게 없나 그럼??",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012462.1.7",
          "form": "탱크탑이 나쁜말이래여?",
          "original_form": "탱크탑이 나쁜말이래여?",
          "speaker_id": "2",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012462.1.8",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012462.1.9",
          "form": "아무말 안하는데??",
          "original_form": "아무말 안하는데??",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012462.1.10",
          "form": "나한테 그랬어요 ㅋㅋㅋㅋㅋ",
          "original_form": "나한테 그랬어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012462.1.11",
          "form": "이제 내한테 그럼 ㅋㅋㅋㅋ",
          "original_form": "이제 내한테 그럼 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012462.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 감정기복 우리보다 심하네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 감정기복 우리보다 심하네",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012462.1.13",
          "form": "갱년긴가....",
          "original_form": "갱년긴가....",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012462.1.14",
          "form": "그니깐요 ㅋㅋㅋㅋ",
          "original_form": "그니깐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012462.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 이제 니한테 꼽혔네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 이제 니한테 꼽혔네",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012462.1.16",
          "form": "조심해라",
          "original_form": "조심해라",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012462.1.17",
          "form": "죽자고 달려든다",
          "original_form": "죽자고 달려든다",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012462.1.18",
          "form": "이거 두번 세번치기도 손꾸락아픈데 ㅋㅋㅋ",
          "original_form": "이거 두번 세번치기도 손꾸락아픈데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012462.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012462.1.20",
          "form": "양말은 줄어드는듯요ㅋㅋㅋ",
          "original_form": "양말은 줄어드는듯요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012462.1.21",
          "form": "신랑은 자나??",
          "original_form": "신랑은 자나??",
          "speaker_id": "1",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012462.1.22",
          "form": "신랑 아파가 못자는중 ㅋㅋㅋㅋ",
          "original_form": "신랑 아파가 못자는중 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012462.1.23",
          "form": "어깨아파서??",
          "original_form": "어깨아파서??",
          "speaker_id": "1",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012462.1.24",
          "form": "글고 원래 밤잠 없어여 ㅋㅋㅋ신랑 시킴되는데 아프다하니..내가 한거라요ㅠ",
          "original_form": "글고 원래 밤잠 없어여 ㅋㅋㅋ신랑 시킴되는데 아프다하니..내가 한거라요ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012462.1.25",
          "form": "아하 그렇구만 힘들다",
          "original_form": "아하 그렇구만 힘들다",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012462.1.26",
          "form": "이제 약빨떨어지니 아픈가봐요~",
          "original_form": "이제 약빨떨어지니 아픈가봐요~",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012462.1.27",
          "form": "아직 수술 안 했잖아 수술했나??",
          "original_form": "아직 수술 안 했잖아 수술했나??",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012462.1.28",
          "form": "mri를 60만원 주고 찍었는데",
          "original_form": "mri를 60만원 주고 찍었는데",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012462.1.29",
          "form": "수술했아요~오늘 ㅋㅋㅋ",
          "original_form": "수술했아요~오늘 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012462.1.30",
          "form": "아 오늘 했구나 아이구",
          "original_form": "아 오늘 했구나 아이구",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012462.1.31",
          "form": "실비에서 25만원만 나온다네요ㅡㅡ",
          "original_form": "실비에서 25만원만 나온다네요ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012462.1.32",
          "form": "왜 그것밖에 안 나온대??",
          "original_form": "왜 그것밖에 안 나온대??",
          "speaker_id": "1",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012462.1.33",
          "form": "그게난 더 열받아서 ㅋㅋㅋㅋ",
          "original_form": "그게난 더 열받아서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012462.1.34",
          "form": "1일 통원 한도가25만원이래요..",
          "original_form": "1일 통원 한도가25만원이래요..",
          "speaker_id": "2",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012462.1.35",
          "form": "아 그래 실비는 통원 한도가 있으니까 엠알아이 찍을려면 입원해야 되는데",
          "original_form": "아 그래 실비는 통원 한도가 있으니까 엠알아이 찍을려면 입원해야 되는데",
          "speaker_id": "1",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012462.1.36",
          "form": "그니깐요ㅠ",
          "original_form": "그니깐요ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012462.1.37",
          "form": "입원하면 안 된다나??하루 입원하지",
          "original_form": "입원하면 안 된다나??하루 입원하지",
          "speaker_id": "1",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012462.1.38",
          "form": "근데 누가 입원시켜죠요ㅠㅠ",
          "original_form": "근데 누가 입원시켜죠요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012462.1.39",
          "form": "요즘엔.그거 말 많아서 안해준다데요ㅠ",
          "original_form": "요즘엔.그거 말 많아서 안해준다데요ㅠ",
