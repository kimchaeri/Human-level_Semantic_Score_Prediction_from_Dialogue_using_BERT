      "utterance": [
        {
          "id": "MDRW2100015439.1.1",
          "form": "내일 뭐 드실지 알려주세영ㅋㅋ",
          "original_form": "내일 뭐 드실지 알려주세영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015439.1.2",
          "form": "아직 계획된건 따로 없는데",
          "original_form": "아직 계획된건 따로 없는데",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015439.1.3",
          "form": "아마 비비고 곰탕 남은거 마저 먹을거 같아요",
          "original_form": "아마 비비고 곰탕 남은거 마저 먹을거 같아요",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015439.1.4",
          "form": "저번에 대량구매를 해버려서ㅋㅋ",
          "original_form": "저번에 대량구매를 해버려서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015439.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015439.1.6",
          "form": "엄청 구매하셨군요! 다 곰탕맛으로 사신건가요?",
          "original_form": "엄청 구매하셨군요! 다 곰탕맛으로 사신건가요?",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015439.1.7",
          "form": "곰탕이랑 육개장 샀었어요",
          "original_form": "곰탕이랑 육개장 샀었어요",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015439.1.8",
          "form": "오 육개장은 다 드신거에요?",
          "original_form": "오 육개장은 다 드신거에요?",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015439.1.9",
          "form": "육개장은 다 먹었어요",
          "original_form": "육개장은 다 먹었어요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015439.1.10",
          "form": "맛있었나보군요 ㅋㅋ",
          "original_form": "맛있었나보군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015439.1.11",
          "form": "칼국수면 말아 먹다보니 어느새 끝나 있더라구요ㅋㅋ",
          "original_form": "칼국수면 말아 먹다보니 어느새 끝나 있더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015439.1.12",
          "form": "오호 육개장비비고에 칼국수면 ㅋㅋㅋ",
          "original_form": "오호 육개장비비고에 칼국수면 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015439.1.13",
          "form": "꿀팁 기억할게욬ㅋㅋ",
          "original_form": "꿀팁 기억할게욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015439.1.14",
          "form": "말만들어도 맛있는조합.. 이건 되는조핮",
          "original_form": "말만들어도 맛있는조합.. 이건 되는조핮",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015439.1.15",
          "form": "맞아요 최강 조합입니다ㅋㅋ",
          "original_form": "맞아요 최강 조합입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015439.1.16",
          "form": "씨제이가 진짜 맛을 잘내나봐요",
          "original_form": "씨제이가 진짜 맛을 잘내나봐요",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015439.1.17",
          "form": "비비고얘기하니까 식스센스에서 유재석이 감탄했던 대기업맛이 생각나네요 ㅋㅋㅋㅋ",
          "original_form": "비비고얘기하니까 식스센스에서 유재석이 감탄했던 대기업맛이 생각나네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015439.1.18",
          "form": "음식장인인줄 알았던 가게가 알고보니 씨제이 비비고였던ㅋㅋㅋ",
          "original_form": "음식장인인줄 알았던 가게가 알고보니 씨제이 비비고였던ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015439.1.19",
          "form": "아 저도 그거 봤어요ㅋㅋ 대기업이 대단하긴 해요 사람을 그렇게 속여먹다니",
          "original_form": "아 저도 그거 봤어요ㅋㅋ 대기업이 대단하긴 해요 사람을 그렇게 속여먹다니",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015439.1.20",
          "form": "ㅋㅋㅋ그만큼 맛있다는거죠 깊은맛도 내고",
          "original_form": "ㅋㅋㅋ그만큼 맛있다는거죠 깊은맛도 내고",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015439.1.21",
          "form": "얼마나 많은사람들이 전세계인의 입맛을 잡으려고 연구하고 개발하겠어요 ㅎㅎㅎ",
          "original_form": "얼마나 많은사람들이 전세계인의 입맛을 잡으려고 연구하고 개발하겠어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015439.1.22",
          "form": "신기해요 각자 자기만의 레시피를 만들어서 발명? 하는거 ㅎㅎ",
          "original_form": "신기해요 각자 자기만의 레시피를 만들어서 발명? 하는거 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015439.1.23",
          "form": "그 비율 찾아내는데 얼마나 걸릴까요? 매번 궁금해요ㅎㅎ",
          "original_form": "그 비율 찾아내는데 얼마나 걸릴까요? 매번 궁금해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015439.1.24",
          "form": "어마어마한 시간이 걸리겠...져? ㅋㅋㅋ",
          "original_form": "어마어마한 시간이 걸리겠...져? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015439.1.25",
          "form": "연구원들도 엄청 많을테구요",
          "original_form": "연구원들도 엄청 많을테구요",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015439.1.26",
          "form": "하다못해 치킨브랜드에서 새로운 맛 치킨 개발하는것도 오랜시간걸린다던데..",
          "original_form": "하다못해 치킨브랜드에서 새로운 맛 치킨 개발하는것도 오랜시간걸린다던데..",
          "speaker_id": "1",
          "time": "20210825 23:38"
        }
      ]
