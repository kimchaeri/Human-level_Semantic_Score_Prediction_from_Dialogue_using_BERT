      "utterance": [
        {
          "id": "MDRW2100003781.1.1",
          "form": "20세기라서 옛날 노래만 나옴",
          "original_form": "20세기라서 옛날 노래만 나옴",
          "speaker_id": "2",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003781.1.2",
          "form": "엄정화가 1위일듯 싶네",
          "original_form": "엄정화가 1위일듯 싶네",
          "speaker_id": "1",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003781.1.3",
          "form": "김종민 잘생겨서 지금 봐도 그렇게 촌스럽지가 않네",
          "original_form": "김종민 잘생겨서 지금 봐도 그렇게 촌스럽지가 않네",
          "speaker_id": "2",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003781.1.4",
          "form": "예전 남배우들이 성형도 안하고 더 잘생긴듯",
          "original_form": "예전 남배우들이 성형도 안하고 더 잘생긴듯",
          "speaker_id": "1",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003781.1.5",
          "form": "지금 배우들은 너무 비슷해서 헷갈린다",
          "original_form": "지금 배우들은 너무 비슷해서 헷갈린다",
          "speaker_id": "2",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003781.1.6",
          "form": "요새 아이돌 배우들 성형 많이 했드라",
          "original_form": "요새 아이돌 배우들 성형 많이 했드라",
          "speaker_id": "1",
          "time": "20210821 12:05"
        },
        {
          "id": "MDRW2100003781.1.7",
          "form": "그러게 개성이 없어서 안타갑다",
          "original_form": "그러게 개성이 없어서 안타갑다",
          "speaker_id": "2",
          "time": "20210821 12:05"
        },
        {
          "id": "MDRW2100003781.1.8",
          "form": "성형은 모든 사람을 똑같게 만듬ㅋㅋ",
          "original_form": "성형은 모든 사람을 똑같게 만듬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003781.1.9",
          "form": "이병헌도 음원낸거 있었네 몰랐다",
          "original_form": "이병헌도 음원낸거 있었네 몰랐다",
          "speaker_id": "2",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003781.1.10",
          "form": "노래도 부르고 열심히  살았네",
          "original_form": "노래도 부르고 열심히  살았네",
          "speaker_id": "1",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003781.1.11",
          "form": "이제 5위한다",
          "original_form": "이제 5위한다",
          "speaker_id": "2",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003781.1.12",
          "form": "차태현 생각나네",
          "original_form": "차태현 생각나네",
          "speaker_id": "1",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003781.1.13",
          "form": "그럴수도 있겠네",
          "original_form": "그럴수도 있겠네",
          "speaker_id": "2",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003781.1.14",
          "form": "차태현은 노래나 연기나 잘하는건 아닌거 같은데",
          "original_form": "차태현은 노래나 연기나 잘하는건 아닌거 같은데",
          "speaker_id": "1",
          "time": "20210821 12:08"
        },
        {
          "id": "MDRW2100003781.1.15",
          "form": "오 정답이다 5위가 차태현이다 대박",
          "original_form": "오 정답이다 5위가 차태현이다 대박",
          "speaker_id": "2",
          "time": "20210821 12:08"
        },
        {
          "id": "MDRW2100003781.1.16",
          "form": "와 그냥 생각난건데ㅋ",
          "original_form": "와 그냥 생각난건데ㅋ",
          "speaker_id": "1",
          "time": "20210821 12:09"
        },
        {
          "id": "MDRW2100003781.1.17",
          "form": "지금말고 20세기 그당시 인기로 집계하는가보다",
          "original_form": "지금말고 20세기 그당시 인기로 집계하는가보다",
          "speaker_id": "2",
          "time": "20210821 12:09"
        },
        {
          "id": "MDRW2100003781.1.18",
          "form": "그때 차태현이 1위도 했을거야",
          "original_form": "그때 차태현이 1위도 했을거야",
          "speaker_id": "1",
          "time": "20210821 12:09"
        },
        {
          "id": "MDRW2100003781.1.19",
          "form": "ㅋㅋ안무가 왜 저렇게 촌스럽노",
          "original_form": "ㅋㅋ안무가 왜 저렇게 촌스럽노",
