      "utterance": [
        {
          "id": "MDRW2100030186.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030186.1.2",
          "form": "넹 자주 보는 편이예요",
          "original_form": "넹 자주 보는 편이예요",
          "speaker_id": "1",
          "time": "20210904 07:55"
        },
        {
          "id": "MDRW2100030186.1.3",
          "form": "name2님은 최근에 본 영화 있으세요?",
          "original_form": "&name2&님은 최근에 본 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210904 07:55"
        },
        {
          "id": "MDRW2100030186.1.4",
          "form": "전 모가디슈 ㅋㅋ",
          "original_form": "전 모가디슈 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 08:01"
        },
        {
          "id": "MDRW2100030186.1.5",
          "form": "집에서는 전우치 다시 볼까해서",
          "original_form": "집에서는 전우치 다시 볼까해서",
          "speaker_id": "2",
          "time": "20210904 08:01"
        },
        {
          "id": "MDRW2100030186.1.6",
          "form": "잠깐 틀었었어요",
          "original_form": "잠깐 틀었었어요",
          "speaker_id": "2",
          "time": "20210904 08:01"
        },
        {
          "id": "MDRW2100030186.1.7",
          "form": "전우치 명작이죠!",
          "original_form": "전우치 명작이죠!",
          "speaker_id": "1",
          "time": "20210904 09:00"
        },
        {
          "id": "MDRW2100030186.1.8",
          "form": "저두 모가디슈 봤어요",
          "original_form": "저두 모가디슈 봤어요",
          "speaker_id": "1",
          "time": "20210904 09:01"
        },
        {
          "id": "MDRW2100030186.1.9",
          "form": "존잼",
          "original_form": "존잼",
          "speaker_id": "1",
          "time": "20210904 09:01"
        },
        {
          "id": "MDRW2100030186.1.10",
          "form": "ㅋㅋ 구교환 매력있어서",
          "original_form": "ㅋㅋ 구교환 매력있어서",
          "speaker_id": "2",
          "time": "20210904 09:05"
        },
        {
          "id": "MDRW2100030186.1.11",
          "form": "디피도 봅니다",
          "original_form": "디피도 봅니다",
          "speaker_id": "2",
          "time": "20210904 09:05"
        },
        {
          "id": "MDRW2100030186.1.12",
          "form": "구교환 매력있죠!",
          "original_form": "구교환 매력있죠!",
          "speaker_id": "1",
          "time": "20210904 09:43"
        },
        {
          "id": "MDRW2100030186.1.13",
          "form": "아직 못 봤는데 이번주말 숨도 안쉬고 몰아볼라구요",
          "original_form": "아직 못 봤는데 이번주말 숨도 안쉬고 몰아볼라구요",
          "speaker_id": "1",
          "time": "20210904 09:43"
        },
        {
          "id": "MDRW2100030186.1.14",
          "form": "두번보세요 진짜 핵귀엽",
          "original_form": "두번보세요 진짜 핵귀엽",
          "speaker_id": "2",
          "time": "20210904 10:02"
        },
        {
          "id": "MDRW2100030186.1.15",
          "form": "귀엽나요? 음청 심각한 드라마라고 들었는데 ㅋ",
          "original_form": "귀엽나요? 음청 심각한 드라마라고 들었는데 ㅋ",
          "speaker_id": "1",
          "time": "20210904 10:19"
        },
        {
          "id": "MDRW2100030186.1.16",
          "form": "심각한 와중에 구교환이 매력이 팡팡",
          "original_form": "심각한 와중에 구교환이 매력이 팡팡",
          "speaker_id": "2",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100030186.1.17",
          "form": "오& 그럼 팝콘 튀겨서 맥주마시면서 볼래요",
          "original_form": "오& 그럼 팝콘 튀겨서 맥주마시면서 볼래요",
          "speaker_id": "1",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100030186.1.18",
          "form": "ㅋㅋ진짜 꼭 보세요!",
          "original_form": "ㅋㅋ진짜 꼭 보세요!",
          "speaker_id": "2",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100030186.1.19",
          "form": "넹 오늘부터 정주행!",
          "original_form": "넹 오늘부터 정주행!",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030186.1.20",
          "form": "6회라 순삭할듯 같아요",
          "original_form": "6회라 순삭할듯 같아요",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030186.1.21",
          "form": "구교환 팬되실거에요",
          "original_form": "구교환 팬되실거에요",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030186.1.22",
          "form": "이미 반교때 홀랑 넘어갔어요",
          "original_form": "이미 반교때 홀랑 넘어갔어요",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100030186.1.23",
          "form": "특히 이번 프로모로. 컬투쇼 나왔을때 매력발산 ㅋㅋ",
