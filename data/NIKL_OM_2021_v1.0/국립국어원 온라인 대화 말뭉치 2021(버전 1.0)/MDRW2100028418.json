      "utterance": [
        {
          "id": "MDRW2100028418.1.1",
          "form": "갱쥐 지금 거실에서 자",
          "original_form": "갱쥐 지금 거실에서 자",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028418.1.2",
          "form": "갱얼지...",
          "original_form": "갱얼지...",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028418.1.3",
          "form": "어젠가 강아지 산책 나갔는데",
          "original_form": "어젠가 강아지 산책 나갔는데",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028418.1.4",
          "form": "진짜 빨리 뛰어다님",
          "original_form": "진짜 빨리 뛰어다님",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028418.1.5",
          "form": "귀여워",
          "original_form": "귀여워",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028418.1.6",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028418.1.7",
          "form": "강아지한테 끌려다니죠",
          "original_form": "강아지한테 끌려다니죠",
          "speaker_id": "2",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100028418.1.8",
          "form": "남의 집 고양이 보고 싶다",
          "original_form": "남의 집 고양이 보고 싶다",
          "speaker_id": "2",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100028418.1.9",
          "form": "나 내일 남의 집 고양이 보러 가",
          "original_form": "나 내일 남의 집 고양이 보러 가",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100028418.1.10",
          "form": "부럽지",
          "original_form": "부럽지",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100028418.1.11",
          "form": "나도 고영 보고 싶다",
          "original_form": "나도 고영 보고 싶다",
          "speaker_id": "2",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028418.1.12",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028418.1.13",
          "form": "고양이랑 강아지 중에선 무슨 파",
          "original_form": "고양이랑 강아지 중에선 무슨 파",
          "speaker_id": "2",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028418.1.14",
          "form": "나는 고영.",
          "original_form": "나는 고영.",
          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028418.1.15",
          "form": "님은",
          "original_form": "님은",
          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028418.1.16",
          "form": "저도 고영이었는데 강강쥐 키우기 시작하면서",
          "original_form": "저도 고영이었는데 강강쥐 키우기 시작하면서",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028418.1.17",
          "form": "더 눈이 많이 가는 듯",
          "original_form": "더 눈이 많이 가는 듯",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028418.1.18",
          "form": "이잉 글쿠만",
          "original_form": "이잉 글쿠만",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028418.1.19",
          "form": "갱얼 언제부터 키웠댔지",
          "original_form": "갱얼 언제부터 키웠댔지",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028418.1.20",
          "form": "3년 됐나",
          "original_form": "3년 됐나",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028418.1.21",
          "form": "집에 동물 있는 게 확실히",
          "original_form": "집에 동물 있는 게 확실히",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028418.1.22",
          "form": "좋긴 한 것 같어",
          "original_form": "좋긴 한 것 같어",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028418.1.23",
          "form": "ㅜㅜ부러어",
          "original_form": "ㅜㅜ부러어",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028418.1.24",
          "form": "근데 난 내 몸 하나도 간수 못해서",
          "original_form": "근데 난 내 몸 하나도 간수 못해서",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028418.1.25",
          "form": "동물 키우면 안될 것 같음",
          "original_form": "동물 키우면 안될 것 같음",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028418.1.26",
          "form": "그건 그래 나도 그래서 혼자 살면 동물 안 키울 거야",
          "original_form": "그건 그래 나도 그래서 혼자 살면 동물 안 키울 거야",
          "speaker_id": "2",
          "time": "20210902 20:06"
        }
      ]
