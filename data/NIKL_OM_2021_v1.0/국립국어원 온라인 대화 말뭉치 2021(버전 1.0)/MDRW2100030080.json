      "utterance": [
        {
          "id": "MDRW2100030080.1.1",
          "form": "언론사에서 오바쩔었어요",
          "original_form": "언론사에서 오바쩔었어요",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100030080.1.2",
          "form": "랑종 무섭긴해도 역대급까진 아닌...",
          "original_form": "랑종 무섭긴해도 역대급까진 아닌...",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100030080.1.3",
          "form": "아 그정도는 아니군요 ㅋㅋ",
          "original_form": "아 그정도는 아니군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100030080.1.4",
          "form": "저는 다크앤드위키드가 훨씬 찝찝하고 무서웠어요",
          "original_form": "저는 다크앤드위키드가 훨씬 찝찝하고 무서웠어요",
          "speaker_id": "2",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100030080.1.5",
          "form": "갑툭튀 긴장감 대박",
          "original_form": "갑툭튀 긴장감 대박",
          "speaker_id": "2",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100030080.1.6",
          "form": "ㅋㅋ무서운거 좋아하시는데요?",
          "original_form": "ㅋㅋ무서운거 좋아하시는데요?",
          "speaker_id": "1",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100030080.1.7",
          "form": "휴 쫄보라소...",
          "original_form": "휴 쫄보라소...",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100030080.1.8",
          "form": "ㅋㅋ무서운거 좋아하는 쫄보시군요",
          "original_form": "ㅋㅋ무서운거 좋아하는 쫄보시군요",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100030080.1.9",
          "form": "샹치 내일 볼거같은데 마블 좋아하시나요",
          "original_form": "샹치 내일 볼거같은데 마블 좋아하시나요",
          "speaker_id": "2",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100030080.1.10",
          "form": "저는 국내파라서....ㅎㅎ",
          "original_form": "저는 국내파라서....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030080.1.11",
          "form": "인질보셨나요 ㅎㅎ",
          "original_form": "인질보셨나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030080.1.12",
          "form": "지금 롯데시네마에서 천원에 볼수있네요",
          "original_form": "지금 롯데시네마에서 천원에 볼수있네요",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030080.1.13",
          "form": "오 천원이요?",
          "original_form": "오 천원이요?",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030080.1.14",
          "form": "네 싸다구라는 이벤트해요",
          "original_form": "네 싸다구라는 이벤트해요",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030080.1.15",
          "form": "오.....좋은 정보 감사합니다",
          "original_form": "오.....좋은 정보 감사합니다",
          "speaker_id": "1",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030080.1.16",
          "form": "곧마감될거같아요 선착이라 ㅜ",
          "original_form": "곧마감될거같아요 선착이라 ㅜ",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030080.1.17",
          "form": "아 빨랑할께요 ㅎㅎ",
          "original_form": "아 빨랑할께요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030080.1.18",
          "form": "12장남앗데요",
          "original_form": "12장남앗데요",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030080.1.19",
          "form": "아 안되겠어요 ㅎㅎ 일 중이라서",
          "original_form": "아 안되겠어요 ㅎㅎ 일 중이라서",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030080.1.20",
          "form": "ㅠㅠ 황정민이 황정민역활이라 독특한 영화였어요",
          "original_form": "ㅠㅠ 황정민이 황정민역활이라 독특한 영화였어요",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030080.1.21",
          "form": "지금도 일중이신가봐요 ㄷㄷ",
          "original_form": "지금도 일중이신가봐요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030080.1.22",
          "form": "아 인질 기억났다 ㅋㅋ",
          "original_form": "아 인질 기억났다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:23"
        }
      ]
