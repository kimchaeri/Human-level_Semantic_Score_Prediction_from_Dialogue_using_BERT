        },
        {
          "id": "MDRW2100004271.1.2",
          "form": "강아지 어떤부분이 좋나요",
          "original_form": "강아지 어떤부분이 좋나요",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004271.1.3",
          "form": "귀여워요",
          "original_form": "귀여워요",
          "speaker_id": "2",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004271.1.4",
          "form": "다른 귀여윤동물은 ?",
          "original_form": "다른 귀여윤동물은 ?",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004271.1.5",
          "form": "고양이도 좋으련만",
          "original_form": "고양이도 좋으련만",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004271.1.6",
          "form": "털 날려도 괜찮나요",
          "original_form": "털 날려도 괜찮나요",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004271.1.7",
          "form": "그게 문제지만",
          "original_form": "그게 문제지만",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004271.1.8",
          "form": "토끼 귀엽죠",
          "original_form": "토끼 귀엽죠",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004271.1.9",
          "form": "물면 아프잖아요",
          "original_form": "물면 아프잖아요",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004271.1.10",
          "form": "ㅋㅋㅋㅋ거북이는요",
          "original_form": "ㅋㅋㅋㅋ거북이는요",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004271.1.11",
          "form": "물 가는게 힘들듯",
          "original_form": "물 가는게 힘들듯",
