        },
        {
          "id": "MDRW2100008479.1.14",
          "form": "지는게 편해요",
          "original_form": "지는게 편해요",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008479.1.15",
          "form": "아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008479.1.16",
          "form": "그래도 안지고 싶은 사람이 많은데",
          "original_form": "그래도 안지고 싶은 사람이 많은데",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008479.1.17",
          "form": "져주시는게 멋지네요",
