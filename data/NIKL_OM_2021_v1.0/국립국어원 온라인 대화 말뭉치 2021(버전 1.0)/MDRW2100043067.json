      "utterance": [
        {
          "id": "MDRW2100043067.1.1",
          "form": "그럼 요즘 게임 뭐해?",
          "original_form": "그럼 요즘 게임 뭐해?",
          "speaker_id": "1",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100043067.1.2",
          "form": "나는 스팀게임 하지",
          "original_form": "나는 스팀게임 하지",
          "speaker_id": "2",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100043067.1.3",
          "form": "레데리2  꼭해라...",
          "original_form": "레데리2  꼭해라...",
          "speaker_id": "2",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100043067.1.4",
          "form": "이미 했지...",
          "original_form": "이미 했지...",
          "speaker_id": "1",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100043067.1.5",
          "form": "갓겜이야",
          "original_form": "갓겜이야",
          "speaker_id": "1",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043067.1.6",
          "form": "아서야....ㅠㅠㅠㅠㅠ",
          "original_form": "아서야....ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043067.1.7",
          "form": "눈물만 나네요",
          "original_form": "눈물만 나네요",
          "speaker_id": "2",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043067.1.8",
          "form": "후반에 아서 기침할때 내마음도 찢어져",
          "original_form": "후반에 아서 기침할때 내마음도 찢어져",
          "speaker_id": "1",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100043067.1.9",
          "form": "아서 착하게 산건 아니지만 그래도...그렇게 죽긴 서글펐다",
          "original_form": "아서 착하게 산건 아니지만 그래도...그렇게 죽긴 서글펐다",
          "speaker_id": "2",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100043067.1.10",
          "form": "그지....엔딩에 나오는 음악도 진짜 ....착붙이여서",
          "original_form": "그지....엔딩에 나오는 음악도 진짜 ....착붙이여서",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100043067.1.11",
          "form": "그니까....",
          "original_form": "그니까....",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100043067.1.12",
          "form": "오픈월드게임중에",
          "original_form": "오픈월드게임중에",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100043067.1.13",
          "form": "가장 내 취향이였어",
          "original_form": "가장 내 취향이였어",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100043067.1.14",
          "form": "나는 젤다가 조금더 취향이긴한데",
          "original_form": "나는 젤다가 조금더 취향이긴한데",
          "speaker_id": "1",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100043067.1.15",
          "form": "레데리도 너무 좋지",
          "original_form": "레데리도 너무 좋지",
          "speaker_id": "1",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100043067.1.16",
          "form": "나는 젤다는 뭐랄까 너무 자유롭다고 해야하나? 스토리에 속도감이 없어서 조금 덜 취향이였어",
          "original_form": "나는 젤다는 뭐랄까 너무 자유롭다고 해야하나? 스토리에 속도감이 없어서 조금 덜 취향이였어",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100043067.1.17",
          "form": "그럴수도 있겠네",
          "original_form": "그럴수도 있겠네",
          "speaker_id": "1",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100043067.1.18",
          "form": "나는 자유로워서 하고싶은거 천천히 다하면서 다니는 모험의 재미가 좋았어",
          "original_form": "나는 자유로워서 하고싶은거 천천히 다하면서 다니는 모험의 재미가 좋았어",
          "speaker_id": "1",
          "time": "20210907 13:58"
        },
        {
          "id": "MDRW2100043067.1.19",
          "form": "레데리는 후반부에 속도감이 붙으면서",
          "original_form": "레데리는 후반부에 속도감이 붙으면서",
          "speaker_id": "2",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100043067.1.20",
          "form": "스토리진행을 안 할 수 없게 해둬서 좋았어",
          "original_form": "스토리진행을 안 할 수 없게 해둬서 좋았어",
          "speaker_id": "2",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100043067.1.21",
          "form": "어느정도 강제적인 오픈월드가 좋은가봐 난",
          "original_form": "어느정도 강제적인 오픈월드가 좋은가봐 난",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043067.1.22",
          "form": "그럴수있지...",
          "original_form": "그럴수있지...",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043067.1.23",
          "form": "오픈월드 너무 자유로워서 싫다는 사람들도 많더라",
          "original_form": "오픈월드 너무 자유로워서 싫다는 사람들도 많더라",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043067.1.24",
          "form": "뭔말인지 너무 잘 알겠다",
          "original_form": "뭔말인지 너무 잘 알겠다",
          "speaker_id": "2",
          "time": "20210907 14:02"
        }
      ]
    }
