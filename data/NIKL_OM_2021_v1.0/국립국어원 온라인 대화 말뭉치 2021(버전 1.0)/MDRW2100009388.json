      "utterance": [
        {
          "id": "MDRW2100009388.1.1",
          "form": "요새도 니가 저녁밥하나?ㅋ",
          "original_form": "요새도 니가 저녁밥하나?ㅋ",
          "speaker_id": "1",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100009388.1.2",
          "form": "웅 ㅠㅠ",
          "original_form": "웅 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100009388.1.3",
          "form": "최근엔 동생이 좀 하고있다 ㅋㅋㅋ",
          "original_form": "최근엔 동생이 좀 하고있다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100009388.1.4",
          "form": "남이 해주니까 편함 ㅋㅋㅋ",
          "original_form": "남이 해주니까 편함 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100009388.1.5",
          "form": "고생한다ㅋ",
          "original_form": "고생한다ㅋ",
          "speaker_id": "1",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100009388.1.6",
          "form": "난 혼자 사니 해주는밥은 못먹지만 해줘야하는 사람도 읍넹ㅋ",
          "original_form": "난 혼자 사니 해주는밥은 못먹지만 해줘야하는 사람도 읍넹ㅋ",
          "speaker_id": "1",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100009388.1.7",
          "form": "ㅋㅋㅋㅋ그게 최고임",
          "original_form": "ㅋㅋㅋㅋ그게 최고임",
          "speaker_id": "2",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100009388.1.8",
          "form": "안먹고싶으면 안먹고",
          "original_form": "안먹고싶으면 안먹고",
          "speaker_id": "2",
          "time": "20210823 21:05"
        },
        {
          "id": "MDRW2100009388.1.9",
          "form": "ㅋㅋㅋ그런듯ㅋ",
          "original_form": "ㅋㅋㅋ그런듯ㅋ",
          "speaker_id": "1",
          "time": "20210823 21:05"
        },
        {
          "id": "MDRW2100009388.1.10",
          "form": "보통 반찬 뭐해줘?",
          "original_form": "보통 반찬 뭐해줘?",
          "speaker_id": "1",
          "time": "20210823 21:05"
        },
        {
          "id": "MDRW2100009388.1.11",
          "form": "설거지도 하기싫으면 걍 뒀다가 나중에 하고 ㅋㅋㅋ",
          "original_form": "설거지도 하기싫으면 걍 뒀다가 나중에 하고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:05"
        },
        {
          "id": "MDRW2100009388.1.12",
          "form": "그때그때 냉장고에 있는재료 때려넣기 ㅋㅋㅋ",
          "original_form": "그때그때 냉장고에 있는재료 때려넣기 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:06"
        },
        {
          "id": "MDRW2100009388.1.13",
          "form": "냉털이지",
          "original_form": "냉털이지",
          "speaker_id": "2",
          "time": "20210823 21:06"
        },
        {
          "id": "MDRW2100009388.1.14",
          "form": "ㅋㅋㅋ좋넹",
          "original_form": "ㅋㅋㅋ좋넹",
          "speaker_id": "1",
          "time": "20210823 21:06"
        },
        {
          "id": "MDRW2100009388.1.15",
          "form": "난 할줄아는게 없어서 매번 같은거 일듯ㅋ",
          "original_form": "난 할줄아는게 없어서 매번 같은거 일듯ㅋ",
          "speaker_id": "1",
          "time": "20210823 21:06"
        },
        {
          "id": "MDRW2100009388.1.16",
          "form": "생선조림 로제떡볶이 감자볶음 버섯전골",
          "original_form": "생선조림 로제떡볶이 감자볶음 버섯전골",
          "speaker_id": "2",
          "time": "20210823 21:06"
        },
        {
          "id": "MDRW2100009388.1.17",
          "form": "두부전골",
          "original_form": "두부전골",
          "speaker_id": "2",
          "time": "20210823 21:06"
        },
        {
          "id": "MDRW2100009388.1.18",
          "form": "오..호화롭다",
          "original_form": "오..호화롭다",
          "speaker_id": "1",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100009388.1.19",
          "form": "버섯볶음",
          "original_form": "버섯볶음",
          "speaker_id": "2",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100009388.1.20",
          "form": "로제 떡볶이를 집에서ㅋ",
          "original_form": "로제 떡볶이를 집에서ㅋ",
          "speaker_id": "1",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100009388.1.21",
          "form": "이름만 저렇지 실제는 걍 한번에 넣고 끓이거나 볶는수준",
          "original_form": "이름만 저렇지 실제는 걍 한번에 넣고 끓이거나 볶는수준",
          "speaker_id": "2",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100009388.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100009388.1.23",
          "form": "버섯전골도 신기ㅋ",
          "original_form": "버섯전골도 신기ㅋ",
          "speaker_id": "1",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100009388.1.24",
          "form": "울집에서 못먹던 메뉴ㅋ",
          "original_form": "울집에서 못먹던 메뉴ㅋ",
          "speaker_id": "1",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100009388.1.25",
          "form": "육수끓으면 버섯이랑 야채넣으면 끝이야",
          "original_form": "육수끓으면 버섯이랑 야채넣으면 끝이야",
          "speaker_id": "2",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100009388.1.26",
          "form": "캬캬캬",
          "original_form": "캬캬캬",
          "speaker_id": "2",
          "time": "20210823 21:08"
        },
        {
          "id": "MDRW2100009388.1.27",
          "form": "그래도..ㅋ 난 육수 낼줄도 몰라ㅋ",
          "original_form": "그래도..ㅋ 난 육수 낼줄도 몰라ㅋ",
          "speaker_id": "1",
          "time": "20210823 21:08"
        },
        {
          "id": "MDRW2100009388.1.28",
          "form": "아 육수팩 마트에 파는거 끓는물에 넣으면 끝이야",
          "original_form": "아 육수팩 마트에 파는거 끓는물에 넣으면 끝이야",
          "speaker_id": "2",
          "time": "20210823 21:09"
        },
        {
          "id": "MDRW2100009388.1.29",
          "form": "보리차 끓이듯이 ㅋㅋ",
          "original_form": "보리차 끓이듯이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:09"
        },
        {
          "id": "MDRW2100009388.1.30",
          "form": "누구나 할 수있지",
          "original_form": "누구나 할 수있지",
          "speaker_id": "2",
          "time": "20210823 21:09"
        },
        {
          "id": "MDRW2100009388.1.31",
          "form": "아하ㅋㅋ 세상 조쿠나",
          "original_form": "아하ㅋㅋ 세상 조쿠나",
          "speaker_id": "1",
          "time": "20210823 21:09"
        },
        {
          "id": "MDRW2100009388.1.32",
          "form": "응 돈만있으면 밀키트 사서 먹으면 편할듯",
          "original_form": "응 돈만있으면 밀키트 사서 먹으면 편할듯",
          "speaker_id": "2",
          "time": "20210823 21:10"
        },
        {
          "id": "MDRW2100009388.1.33",
          "form": "그건그래ㅋ 그래도 밀키트는 뭔가 대기업의 맛이야",
          "original_form": "그건그래ㅋ 그래도 밀키트는 뭔가 대기업의 맛이야",
          "speaker_id": "1",
          "time": "20210823 21:10"
        }
      ]
