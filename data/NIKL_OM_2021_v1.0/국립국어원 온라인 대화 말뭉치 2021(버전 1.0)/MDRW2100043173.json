        },
        {
          "id": "MDRW2100043173.1.3",
          "form": "야무지게드세욥",
          "original_form": "야무지게드세욥",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043173.1.4",
          "form": "된찌랑 뇸뇸 해야지용",
          "original_form": "된찌랑 뇸뇸 해야지용",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043173.1.5",
          "form": "요새 밀키트도 엄청 잘나오더라구요",
          "original_form": "요새 밀키트도 엄청 잘나오더라구요",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043173.1.6",
          "form": "밀키트 좋죠 ㅎ 이제 편의점에서도 나오던데요",
          "original_form": "밀키트 좋죠 ㅎ 이제 편의점에서도 나오던데요",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043173.1.7",
          "form": "이마트 밀키트 완전 좋아해여",
          "original_form": "이마트 밀키트 완전 좋아해여",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100043173.1.8",
          "form": "피코크 맛잇죠 ㅎ",
          "original_form": "피코크 맛잇죠 ㅎ",
          "speaker_id": "1",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100043173.1.9",
          "form": "이마트가면 사실 녹두삼계탕.. 그거는 밀키는 아니지만 ㅎ",
          "original_form": "이마트가면 사실 녹두삼계탕.. 그거는 밀키는 아니지만 ㅎ",
          "speaker_id": "1",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100043173.1.10",
          "form": "그거 자주 사요",
          "original_form": "그거 자주 사요",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043173.1.11",
          "form": "오 맛잇나요? 녹두삼계탕",
          "original_form": "오 맛잇나요? 녹두삼계탕",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043173.1.12",
          "form": "맛있었어요! 피코크꺼",
          "original_form": "맛있었어요! 피코크꺼",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043173.1.13",
          "form": "저는 코스트코말고",
          "original_form": "저는 코스트코말고",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043173.1.14",
          "form": "이마트 젤 좋더라구요",
          "original_form": "이마트 젤 좋더라구요",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043173.1.15",
          "form": "이마트랑 트레이더스 애용합니당",
          "original_form": "이마트랑 트레이더스 애용합니당",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043173.1.16",
          "form": "코스트코 멀어서 자주 몬가고 ㅠㅠ 트트도 멀어여ㅠㅠㅠ",
          "original_form": "코스트코 멀어서 자주 몬가고 ㅠㅠ 트트도 멀어여ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043173.1.17",
          "form": "어디마트 자주가시나용",
          "original_form": "어디마트 자주가시나용",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043173.1.18",
          "form": "저는 동네마트여ㅠ",
          "original_form": "저는 동네마트여ㅠ",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043173.1.19",
          "form": "나머지는 다 그냥 온라인으로 사거든요 ㅎㅎ 아 얼마전에 포도 홈플익스에서 배달 시키니 달고 좋아써요",
          "original_form": "나머지는 다 그냥 온라인으로 사거든요 ㅎㅎ 아 얼마전에 포도 홈플익스에서 배달 시키니 달고 좋아써요",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043173.1.20",
          "form": "요즘은 뭐 마트도 배달되니 ㅋㅋㅋㅋ",
          "original_form": "요즘은 뭐 마트도 배달되니 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043173.1.21",
          "form": "저희 집앞에 바로 장보고 식자재마트 잇는데",
          "original_form": "저희 집앞에 바로 장보고 식자재마트 잇는데",
          "speaker_id": "2",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043173.1.22",
          "form": "오늘 샤인머스켓도 왔는데 ㅎㅎ",
          "original_form": "오늘 샤인머스켓도 왔는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043173.1.23",
          "form": "여기도 좋더라구요",
          "original_form": "여기도 좋더라구요",
          "speaker_id": "2",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043173.1.24",
          "form": "맞아요 배달최고!!!!!!!!",
          "original_form": "맞아요 배달최고!!!!!!!!",
          "speaker_id": "2",
