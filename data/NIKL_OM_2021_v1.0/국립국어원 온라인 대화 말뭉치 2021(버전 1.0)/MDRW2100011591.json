      "utterance": [
        {
          "id": "MDRW2100011591.1.1",
          "form": "여행가고싶다ㅠ",
          "original_form": "여행가고싶다ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011591.1.2",
          "form": "비행기타고 저쭈 멀리 해외로~",
          "original_form": "비행기타고 저쭈 멀리 해외로~",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011591.1.3",
          "form": "나도 여행가고싶다ㅜ",
          "original_form": "나도 여행가고싶다ㅜ",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011591.1.4",
          "form": "진짜 코로나만 풀리면 괌 가고싶다ㅜ",
          "original_form": "진짜 코로나만 풀리면 괌 가고싶다ㅜ",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011591.1.5",
          "form": "솔직히 난 이월드라도 좀 맘편하게 가고픈맘 ㅠ",
          "original_form": "솔직히 난 이월드라도 좀 맘편하게 가고픈맘 ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011591.1.6",
          "form": "괌 좋다 좋아~이제 해외가면 동양사람들 다 돌맞는거 아닌가 몰러ㅠ",
          "original_form": "괌 좋다 좋아~이제 해외가면 동양사람들 다 돌맞는거 아닌가 몰러ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011591.1.7",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011591.1.8",
          "form": "이월드라도 맘편하게 공감가네",
          "original_form": "이월드라도 맘편하게 공감가네",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011591.1.9",
          "form": "첫째 이월드가고싶다고했는데",
          "original_form": "첫째 이월드가고싶다고했는데",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011591.1.10",
          "form": "못감 ㅜㅠ",
          "original_form": "못감 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011591.1.11",
          "form": "나도 연간끊고싶응데ㅠ",
          "original_form": "나도 연간끊고싶응데ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011591.1.12",
          "form": "또 확진자 나오고허면 못가니ㅠ",
          "original_form": "또 확진자 나오고허면 못가니ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011591.1.13",
          "form": "그렇지ㅜㅠ",
          "original_form": "그렇지ㅜㅠ",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011591.1.14",
          "form": "우린 아직 무리다. 둘째 조금만 더 크면ㅜ가고싶은데",
          "original_form": "우린 아직 무리다. 둘째 조금만 더 크면ㅜ가고싶은데",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011591.1.15",
          "form": "되겠나싶고ㅜ",
          "original_form": "되겠나싶고ㅜ",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011591.1.16",
          "form": "그러니깐 진짜 기약 없는 기다림 ㅠㅠ",
          "original_form": "그러니깐 진짜 기약 없는 기다림 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011591.1.17",
          "form": "아휴 진짜 맘편하게 놀러가고싶다ㅜ",
          "original_form": "아휴 진짜 맘편하게 놀러가고싶다ㅜ",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011591.1.18",
          "form": "이 슬픈현실",
          "original_form": "이 슬픈현실",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011591.1.19",
          "form": "제주도도 가고싶고ㅜ",
          "original_form": "제주도도 가고싶고ㅜ",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011591.1.20",
          "form": "그러니..여행얘기는 하면 할수록 술푸더 ㅋㅋㅋㅋㅋ",
          "original_form": "그러니..여행얘기는 하면 할수록 술푸더 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011591.1.21",
          "form": "다들 슬퍼함 ㅋㅋㅋㅋ",
          "original_form": "다들 슬퍼함 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011591.1.22",
          "form": "그러니깐 ㅋㅋ",
          "original_form": "그러니깐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011591.1.23",
          "form": "마스크끼고 가도 자유롭지못하다ㅋ",
          "original_form": "마스크끼고 가도 자유롭지못하다ㅋ",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011591.1.24",
          "form": "그러니~평소에 어디가는거 정말 아무것도 아니라 여겼는데..",
          "original_form": "그러니~평소에 어디가는거 정말 아무것도 아니라 여겼는데..",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011591.1.25",
          "form": "이렇게 되니 진짜 소중한거였음~",
          "original_form": "이렇게 되니 진짜 소중한거였음~",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011591.1.26",
          "form": "그러니깐ㅜㅋㅋ",
          "original_form": "그러니깐ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011591.1.27",
          "form": "티비로 콘서트장보는데",
          "original_form": "티비로 콘서트장보는데",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011591.1.28",
          "form": "가고",
          "original_form": "가고",
          "speaker_id": "1",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011591.1.29",
          "form": "싶었어",
          "original_form": "싶었어",
          "speaker_id": "1",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011591.1.30",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011591.1.31",
          "form": "어떤 사람이 전생이였던거냐고ㅠ",
          "original_form": "어떤 사람이 전생이였던거냐고ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011591.1.32",
          "form": "기억도 안난다며~",
          "original_form": "기억도 안난다며~",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011591.1.33",
          "form": "그렇겠다ㅜㅜ",
          "original_form": "그렇겠다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011591.1.34",
          "form": "맞는말이네",
          "original_form": "맞는말이네",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011591.1.35",
          "form": "설악산가서 가훈 적어오고해야되는데~",
          "original_form": "설악산가서 가훈 적어오고해야되는데~",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011591.1.36",
          "form": "옥돌에다가 ㅋㅋㅋㅋ",
          "original_form": "옥돌에다가 ㅋㅋㅋㅋ",
          "speaker_id": "2",
