      "utterance": [
        {
          "id": "MDRW2100021989.1.1",
          "form": "안녕하세용 인생영화 있으세요?",
          "original_form": "안녕하세용 인생영화 있으세요?",
          "speaker_id": "1",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021989.1.2",
          "form": "제 인생영화는 매트릭스입니다...",
          "original_form": "제 인생영화는 매트릭스입니다...",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021989.1.3",
          "form": "사실 꽤 많죠 ㅎㅎ name1은요?",
          "original_form": "사실 꽤 많죠 ㅎㅎ &name1&은요?",
          "speaker_id": "2",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021989.1.4",
          "form": "오오 완전 유명한 영화죠호",
          "original_form": "오오 완전 유명한 영화죠호",
          "speaker_id": "1",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021989.1.5",
          "form": "저는 이프온리 라는 영화에요1",
          "original_form": "저는 이프온리 라는 영화에요1",
          "speaker_id": "1",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021989.1.6",
          "form": "오 저도 알긴 알아요!",
          "original_form": "오 저도 알긴 알아요!",
          "speaker_id": "2",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021989.1.7",
          "form": "로맨스 좋아하시나요?",
          "original_form": "로맨스 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021989.1.8",
          "form": "로맨스보다는 액션이나 범죄 수사물을 더 좋아하는데",
          "original_form": "로맨스보다는 액션이나 범죄 수사물을 더 좋아하는데",
          "speaker_id": "1",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021989.1.9",
          "form": "이 영화가 유독 여운이 길게 남더라구요ㅎㅎ",
          "original_form": "이 영화가 유독 여운이 길게 남더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021989.1.10",
          "form": "갑자기 저도 그러니까 보고싶네요",
          "original_form": "갑자기 저도 그러니까 보고싶네요",
          "speaker_id": "2",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021989.1.11",
          "form": "저도 액션이나 범죄수사물 좋아하거든요",
          "original_form": "저도 액션이나 범죄수사물 좋아하거든요",
          "speaker_id": "2",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021989.1.12",
          "form": "다만 악에서 구하소서 보셨어용??",
          "original_form": "다만 악에서 구하소서 보셨어용??",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021989.1.13",
          "form": "아니욥... 알긴 압니다",
          "original_form": "아니욥... 알긴 압니다",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021989.1.14",
          "form": "다만 아기를 구하소서 라던데요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "다만 아기를 구하소서 라던데요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021989.1.15",
          "form": "아기ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아기ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021989.1.16",
          "form": "이거 재밌어요",
          "original_form": "이거 재밌어요",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021989.1.17",
          "form": "한국판 테이큰 느낌나요",
          "original_form": "한국판 테이큰 느낌나요",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021989.1.18",
          "form": "오 그래요?ㅋㅋㅋㅋ호오",
          "original_form": "오 그래요?ㅋㅋㅋㅋ호오",
          "speaker_id": "2",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021989.1.19",
          "form": "ㅋㅋㅋ재밌다니 저도 궁금하네요",
          "original_form": "ㅋㅋㅋ재밌다니 저도 궁금하네요",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021989.1.20",
          "form": "조금 잔인하긴한데 박정민 배우가 연기를 넘 잘해요",
          "original_form": "조금 잔인하긴한데 박정민 배우가 연기를 넘 잘해요",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021989.1.21",
          "form": "연기력 중요하죠 ㅎㅎ",
          "original_form": "연기력 중요하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021989.1.22",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021989.1.23",
          "form": "박정민? 황정민 말고 박정민씨가 나오나요",
          "original_form": "박정민? 황정민 말고 박정민씨가 나오나요",
          "speaker_id": "2",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021989.1.24",
          "form": "넹넹 박정민씨가 중요한 역할로 나와요ㅋㅋㅋ",
          "original_form": "넹넹 박정민씨가 중요한 역할로 나와요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:41"
        },
        {
          "id": "MDRW2100021989.1.25",
          "form": "깜짝 놀랄 역할입니다",
          "original_form": "깜짝 놀랄 역할입니다",
          "speaker_id": "1",
          "time": "20210829 00:41"
        }
      ]
    }
