      "utterance": [
        {
          "id": "MDRW2100010383.1.1",
          "form": "어떤?",
          "original_form": "어떤?",
          "speaker_id": "2",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010383.1.2",
          "form": "내일 새벽 태풍이 대구에서 소멸한대",
          "original_form": "내일 새벽 태풍이 대구에서 소멸한대",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010383.1.3",
          "form": "너 본가가 대구잖아",
          "original_form": "너 본가가 대구잖아",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010383.1.4",
          "form": "걱정이 많겠어.",
          "original_form": "걱정이 많겠어.",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010383.1.5",
          "form": "응 맞아ㅠㅠ 너는 어디였지?",
          "original_form": "응 맞아ㅠㅠ 너는 어디였지?",
          "speaker_id": "2",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010383.1.6",
          "form": "너네집은 지나지 않는거야??",
          "original_form": "너네집은 지나지 않는거야??",
          "speaker_id": "2",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010383.1.7",
          "form": "우리집은..지날거야.",
          "original_form": "우리집은..지날거야.",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010383.1.8",
          "form": "그래서 오늘 날씨가 궂네",
          "original_form": "그래서 오늘 날씨가 궂네",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010383.1.9",
          "form": "하루종일 비가와서 축축 쳐진다.",
          "original_form": "하루종일 비가와서 축축 쳐진다.",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010383.1.10",
          "form": "그렇지 않니?",
          "original_form": "그렇지 않니?",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010383.1.11",
          "form": "응 맞아 나 아까전에",
          "original_form": "응 맞아 나 아까전에",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.12",
          "form": "너는 비오는 날을 좋아하니(",
          "original_form": "너는 비오는 날을 좋아하니(",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.14",
          "form": "우산쓰고 걷다가",
          "original_form": "우산쓰고 걷다가",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.15",
          "form": "우산이 부러지기라도 한거야?",
          "original_form": "우산이 부러지기라도 한거야?",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.16",
          "form": "발이 전부 물로 가득해졌어",
          "original_form": "발이 전부 물로 가득해졌어",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.17",
          "form": "아 그런 극적인 이야기는 아니구나. 다행이야.",
          "original_form": "아 그런 극적인 이야기는 아니구나. 다행이야.",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.18",
          "form": "발이 쭈굴해졌겠구나",
          "original_form": "발이 쭈굴해졌겠구나",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.19",
          "form": "슬리퍼를 신지글9ㅆ어.",
          "original_form": "슬리퍼를 신지글9ㅆ어.",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.20",
          "form": "우산이 뿌러졌으면 아마",
          "original_form": "우산이 뿌러졌으면 아마",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.21",
          "form": "그자리에 서서 엉엉 울었을꺼야",
          "original_form": "그자리에 서서 엉엉 울었을꺼야",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.22",
          "form": "오늘같은날 장화를 신은 사람들도 많더라.",
          "original_form": "오늘같은날 장화를 신은 사람들도 많더라.",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010383.1.23",
          "form": "슬리퍼 안에서 물로 가득했어",
          "original_form": "슬리퍼 안에서 물로 가득했어",
          "speaker_id": "2",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010383.1.24",
          "form": "하늘이 울듯 너도 우는구나.",
          "original_form": "하늘이 울듯 너도 우는구나.",
          "speaker_id": "1",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010383.1.25",
          "form": "언제까지 비가올까",
          "original_form": "언제까지 비가올까",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010383.1.26",
          "form": "나는 곧 이사를 해야하는데 그날도 비가오면",
          "original_form": "나는 곧 이사를 해야하는데 그날도 비가오면",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010383.1.27",
          "form": "너무 힘들 것 같아",
          "original_form": "너무 힘들 것 같아",
          "speaker_id": "2",
          "time": "20210823 23:33"
        }
      ]
    }
