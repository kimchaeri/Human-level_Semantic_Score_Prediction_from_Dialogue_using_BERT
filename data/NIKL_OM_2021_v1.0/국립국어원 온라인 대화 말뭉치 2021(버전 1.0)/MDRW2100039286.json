      "utterance": [
        {
          "id": "MDRW2100039286.1.1",
          "form": "맨날 위로만 내죠",
          "original_form": "맨날 위로만 내죠",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039286.1.2",
          "form": "데브는 정신좀 차려야 해요",
          "original_form": "데브는 정신좀 차려야 해요",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039286.1.3",
          "form": "디자이너 분들 연봉 올려드리구요",
          "original_form": "디자이너 분들 연봉 올려드리구요",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039286.1.4",
          "form": "뭐 캐릭터팔이 장사니까 그냥 그러려니 하긴 하는데ㅜㅜㅋ",
          "original_form": "뭐 캐릭터팔이 장사니까 그냥 그러려니 하긴 하는데ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039286.1.5",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039286.1.6",
          "form": "솔직히 디자인이랑 시나리오팀만 보고 갑니다",
          "original_form": "솔직히 디자인이랑 시나리오팀만 보고 갑니다",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039286.1.7",
          "form": "맞아요... 사실 시나리오도 오락가락하긴 해요",
          "original_form": "맞아요... 사실 시나리오도 오락가락하긴 해요",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039286.1.8",
          "form": "팝핑캔디 나왔을때는... 어휴",
          "original_form": "팝핑캔디 나왔을때는... 어휴",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039286.1.9",
          "form": "ㅋㅋㅋㅋㅋ메인시나리오는 괜찮았는데",
          "original_form": "ㅋㅋㅋㅋㅋ메인시나리오는 괜찮았는데",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039286.1.10",
          "form": "이벤트랑 서브들이 약간..? 음? 으?",
          "original_form": "이벤트랑 서브들이 약간..? 음? 으?",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039286.1.11",
          "form": "하게되는.. 서브쪽은 슈크림 이후로 안봤어요ㅋㅋㅋ",
          "original_form": "하게되는.. 서브쪽은 슈크림 이후로 안봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039286.1.12",
          "form": "서브는 스킵이 답입니다ㅋㅋ",
          "original_form": "서브는 스킵이 답입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100039286.1.13",
          "form": "ㅋㅋㅋㅋ이 대화에서 느껴지는 짬은",
          "original_form": "ㅋㅋㅋㅋ이 대화에서 느껴지는 짬은",
          "speaker_id": "2",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039286.1.14",
          "form": "역시 찍먹 해보셨구나",
          "original_form": "역시 찍먹 해보셨구나",
          "speaker_id": "2",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039286.1.15",
          "form": "찍먹은 아니고 64렙 찍고 접었어요",
          "original_form": "찍먹은 아니고 64렙 찍고 접었어요",
          "speaker_id": "1",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039286.1.16",
          "form": "찍먹이 아니라.. 푹먹이었던걸로..",
          "original_form": "찍먹이 아니라.. 푹먹이었던걸로..",
          "speaker_id": "2",
          "time": "20210903 15:46"
        },
        {
          "id": "MDRW2100039286.1.17",
          "form": "뭐야 이러시면 역시 퍼드도 개고수의 짬바가",
          "original_form": "뭐야 이러시면 역시 퍼드도 개고수의 짬바가",
          "speaker_id": "2",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039286.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ64요?!!?!?!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ64요?!!?!?!",
          "speaker_id": "2",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039286.1.19",
          "form": "ㅋㅋㅋㅋ 꽤 진심이었는데",
          "original_form": "ㅋㅋㅋㅋ 꽤 진심이었는데",
          "speaker_id": "1",
          "time": "20210903 15:59"
        },
        {
          "id": "MDRW2100039286.1.20",
          "form": "지쳤어요..",
          "original_form": "지쳤어요..",
          "speaker_id": "1",
          "time": "20210903 15:59"
        },
        {
          "id": "MDRW2100039286.1.21",
          "form": "토벌전은 언제까지 베타일 것이고..",
          "original_form": "토벌전은 언제까지 베타일 것이고..",
          "speaker_id": "2",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100039286.1.22",
          "form": "협력전의 썬배드는 자동도 아니고..",
          "original_form": "협력전의 썬배드는 자동도 아니고..",
          "speaker_id": "2",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100039286.1.23",
          "form": "게임을 못 만들었죠",
          "original_form": "게임을 못 만들었죠",
          "speaker_id": "1",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100039286.1.24",
          "form": "쿠키런 이후로 나름 진심이었어가지구 뎁스 런칭겜 다 사전예약하면서 하다가",
          "original_form": "쿠키런 이후로 나름 진심이었어가지구 뎁스 런칭겜 다 사전예약하면서 하다가",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039286.1.25",
          "form": "지난 뭐더라.. 쿠킹덤 이전에 다른 개발사에서 개발한",
          "original_form": "지난 뭐더라.. 쿠킹덤 이전에 다른 개발사에서 개발한",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100039286.1.26",
          "form": "머시더라",
          "original_form": "머시더라",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100039286.1.27",
          "form": "그거하고 완전 대실망해가지고 쿠킹덤은 건드려볼생각도 안했는데 흥한거 보고 진짜 놀라긴 했잖아요ㅋㅋ",
          "original_form": "그거하고 완전 대실망해가지고 쿠킹덤은 건드려볼생각도 안했는데 흥한거 보고 진짜 놀라긴 했잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100039286.1.28",
          "form": "오븐브레이크 무슨무슨파티파티 쿠킹덤",
          "original_form": "오븐브레이크 무슨무슨파티파티 쿠킹덤",
          "speaker_id": "1",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100039286.1.29",
          "form": "이렇게 나왔을거예요",
          "original_form": "이렇게 나왔을거예요",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100039286.1.30",
          "form": "찾아왔어요 쿠키워즈!!!",
          "original_form": "찾아왔어요 쿠키워즈!!!",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100039286.1.31",
          "form": "올초에 나왔던건 퍼즐월드네요ㅋㅋㅋ 쟤두 안건드림..",
          "original_form": "올초에 나왔던건 퍼즐월드네요ㅋㅋㅋ 쟤두 안건드림..",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100039286.1.32",
          "form": "올초가 아니라 작년초",
          "original_form": "올초가 아니라 작년초",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100039286.1.33",
          "form": "20년에 퍼즐겜이라니.. 너무 감없게 굴긴 했어요",
          "original_form": "20년에 퍼즐겜이라니.. 너무 감없게 굴긴 했어요",
          "speaker_id": "2",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100039286.1.34",
          "form": "퍼즐겜은 이미 전통겜이 다 자리잡고 있어서",
          "original_form": "퍼즐겜은 이미 전통겜이 다 자리잡고 있어서",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100039286.1.35",
          "form": "새로 내기 참 그렇죠",
          "original_form": "새로 내기 참 그렇죠",
          "speaker_id": "1",
          "time": "20210903 16:16"
        }
      ]
