      "utterance": [
        {
          "id": "MDRW2100029806.1.1",
          "form": "오 넹 가봤죠!",
          "original_form": "오 넹 가봤죠!",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100029806.1.2",
          "form": "오 닭발 좋아하세요?",
          "original_form": "오 닭발 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100029806.1.3",
          "form": "그럼요 한신포차 닭발 최애 메뉴에요 ㅎㅎ",
          "original_form": "그럼요 한신포차 닭발 최애 메뉴에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:58"
        },
        {
          "id": "MDRW2100029806.1.4",
          "form": "우아 매운거 잘드세요?",
          "original_form": "우아 매운거 잘드세요?",
          "speaker_id": "1",
          "time": "20210904 21:59"
        },
        {
          "id": "MDRW2100029806.1.5",
          "form": "소주랑 딱이에유",
          "original_form": "소주랑 딱이에유",
          "speaker_id": "2",
          "time": "20210904 21:59"
        },
        {
          "id": "MDRW2100029806.1.6",
          "form": "음 좋아하는데 못먹는 편이에요 ㅠ",
          "original_form": "음 좋아하는데 못먹는 편이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 21:59"
        },
        {
          "id": "MDRW2100029806.1.7",
          "form": "저두요 ㅠㅠㅠ",
          "original_form": "저두요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100029806.1.8",
          "form": "닭발이랑 계란찜 먹고싶어요 ㅋㅋ",
          "original_form": "닭발이랑 계란찜 먹고싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100029806.1.9",
          "form": "맞아요 ㅠㅠ 저는 통 닭도 엄청 좋아해용",
          "original_form": "맞아요 ㅠㅠ 저는 통 닭도 엄청 좋아해용",
          "speaker_id": "2",
          "time": "20210904 22:21"
        },
        {
          "id": "MDRW2100029806.1.10",
          "form": "우아 통닭 ㅜㅜㅜㅜ",
          "original_form": "우아 통닭 ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100029806.1.11",
          "form": "저는 요즘 후라이드에 꽂혔어요 ㅋㅋ",
          "original_form": "저는 요즘 후라이드에 꽂혔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100029806.1.12",
          "form": "오 어디 치킨 좋아해요?",
          "original_form": "오 어디 치킨 좋아해요?",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100029806.1.13",
          "form": "저는 처갓집 아님 페리카나요!!",
          "original_form": "저는 처갓집 아님 페리카나요!!",
          "speaker_id": "1",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100029806.1.14",
          "form": "소금에 먹는게 최고예요 ㅠㅠ",
          "original_form": "소금에 먹는게 최고예요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100029806.1.15",
          "form": "아 정말요? 저는 한번도 먹어본적 없는데",
          "original_form": "아 정말요? 저는 한번도 먹어본적 없는데",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100029806.1.16",
          "form": "한번 먹어봐야겠네요",
          "original_form": "한번 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100029806.1.17",
          "form": "네네 ! 맛잇어요",
          "original_form": "네네 ! 맛잇어요",
          "speaker_id": "1",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100029806.1.18",
          "form": "그냥 먹으면 퍽퍽해서",
          "original_form": "그냥 먹으면 퍽퍽해서",
          "speaker_id": "1",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100029806.1.19",
          "form": "뭐랑 같이 먹어야 하나요?",
          "original_form": "뭐랑 같이 먹어야 하나요?",
