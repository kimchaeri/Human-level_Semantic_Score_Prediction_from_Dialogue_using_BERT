      "utterance": [
        {
          "id": "MDRW2100030419.1.1",
          "form": "안녕하세영",
          "original_form": "안녕하세영",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030419.1.2",
          "form": "네 안녕하세요!!",
          "original_form": "네 안녕하세요!!",
          "speaker_id": "2",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030419.1.3",
          "form": "좋아하는 음식 있으신가여?",
          "original_form": "좋아하는 음식 있으신가여?",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030419.1.4",
          "form": "과자 좋아해요!!",
          "original_form": "과자 좋아해요!!",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030419.1.5",
          "form": "음식 종류는 다 가리진 않긴 해요 ㅋㅋ",
          "original_form": "음식 종류는 다 가리진 않긴 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030419.1.6",
          "form": "ㅋㅋㅋ 저도",
          "original_form": "ㅋㅋㅋ 저도",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030419.1.7",
          "form": "맛있으면 다 좋아요",
          "original_form": "맛있으면 다 좋아요",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030419.1.8",
          "form": "특히 좋아하는 메뉴 있으세요?",
          "original_form": "특히 좋아하는 메뉴 있으세요?",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030419.1.9",
          "form": "전 오늘은 김밥 먹고 싶네요 ㅋㅋ",
          "original_form": "전 오늘은 김밥 먹고 싶네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030419.1.10",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030419.1.11",
          "form": "저는 오늘은",
          "original_form": "저는 오늘은",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030419.1.12",
          "form": "어묵탕....?",
          "original_form": "어묵탕....?",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030419.1.13",
          "form": "뜨끈한게 먹고싶어요ㅠㅠㅠㅠ",
          "original_form": "뜨끈한게 먹고싶어요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030419.1.14",
          "form": "아 맛있겠어요",
          "original_form": "아 맛있겠어요",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030419.1.15",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030419.1.16",
          "form": "전 잔치국수도 먹고 싶어요",
          "original_form": "전 잔치국수도 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030419.1.17",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030419.1.18",
          "form": "잔치국수도 너무 좋죠ㅠㅠㅠㅠ",
          "original_form": "잔치국수도 너무 좋죠ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030419.1.19",
          "form": "날씨 쌀쌀해지니까 국물종류가 땡기는거같아요",
          "original_form": "날씨 쌀쌀해지니까 국물종류가 땡기는거같아요",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030419.1.20",
          "form": "지금 먹을순 없지만 ㅎㅎ",
          "original_form": "지금 먹을순 없지만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030419.1.21",
          "form": "네 진짜 날이 쌀쌀해졌어요",
          "original_form": "네 진짜 날이 쌀쌀해졌어요",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030419.1.22",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030419.1.23",
          "form": "지금은 시간이 너무 늦었죠..",
          "original_form": "지금은 시간이 너무 늦었죠..",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030419.1.24",
          "form": "국물 맛있겠어요",
          "original_form": "국물 맛있겠어요",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030419.1.25",
          "form": "저녁뭐드셨어요?",
          "original_form": "저녁뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030419.1.26",
          "form": "오늘 저녁에 가지볶음이랑 양배추찜 먹었어요",
          "original_form": "오늘 저녁에 가지볶음이랑 양배추찜 먹었어요",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030419.1.27",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030419.1.28",
          "form": "오옷 채식을 하셨네요",
          "original_form": "오옷 채식을 하셨네요",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030419.1.29",
          "form": "전.. 고기를 먹었어요.....",
          "original_form": "전.. 고기를 먹었어요.....",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030419.1.30",
          "form": "채식이라기엔 양배추찜과 먹은 쌈장이 고기가 들어갔어욬ㅋㅋ",
          "original_form": "채식이라기엔 양배추찜과 먹은 쌈장이 고기가 들어갔어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100030419.1.31",
          "form": "맛있으셨겠다!!",
          "original_form": "맛있으셨겠다!!",
          "speaker_id": "2",
          "time": "20210904 01:05"
        }
      ]
