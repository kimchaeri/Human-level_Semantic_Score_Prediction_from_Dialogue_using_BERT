      "utterance": [
        {
          "id": "MDRW2100028784.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028784.1.2",
          "form": "안녕하세요 봉사하시나요?",
          "original_form": "안녕하세요 봉사하시나요?",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028784.1.3",
          "form": "아니요 바빠서 못하고 있어요",
          "original_form": "아니요 바빠서 못하고 있어요",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028784.1.4",
          "form": "동회회나 봉사활동 하세요?",
          "original_form": "동회회나 봉사활동 하세요?",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028784.1.5",
          "form": "아니요 잘 못하는데 봉사는 예전에 교육봉사했었어요",
          "original_form": "아니요 잘 못하는데 봉사는 예전에 교육봉사했었어요",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028784.1.6",
          "form": "애들 가르치는게 재밌더라구요",
          "original_form": "애들 가르치는게 재밌더라구요",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028784.1.7",
          "form": "뜻깊은일 하셨네요",
          "original_form": "뜻깊은일 하셨네요",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028784.1.8",
          "form": "중고등학생?",
          "original_form": "중고등학생?",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028784.1.9",
          "form": "어떻게 시작하게 되셨는지 여쭤봐도 될까요,.?",
          "original_form": "어떻게 시작하게 되셨는지 여쭤봐도 될까요,.?",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028784.1.10",
          "form": "네네 교육불평등을 겪고있는 학생들이 동등한 권리를 누릴 수 있도록 가르쳤습니다",
          "original_form": "네네 교육불평등을 겪고있는 학생들이 동등한 권리를 누릴 수 있도록 가르쳤습니다",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028784.1.11",
          "form": "ㅎㅎ별건아니구요,,",
          "original_form": "ㅎㅎ별건아니구요,,",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028784.1.12",
          "form": "봉사말고 혹시 자주 가는 모임 있으신가요?",
          "original_form": "봉사말고 혹시 자주 가는 모임 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028784.1.13",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028784.1.14",
          "form": "별건아니라셨지만 정말 훌륭",
          "original_form": "별건아니라셨지만 정말 훌륭",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028784.1.15",
          "form": "아닙니다 ㅎㅎ칭찬해주셔서 감사해요",
          "original_form": "아닙니다 ㅎㅎ칭찬해주셔서 감사해요",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028784.1.16",
          "form": "요즘 코로나때문에 못하는게 아쉽네요ㅠ",
          "original_form": "요즘 코로나때문에 못하는게 아쉽네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028784.1.17",
          "form": "그러시겠어요",
          "original_form": "그러시겠어요",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028784.1.18",
          "form": "코로나가 참 많은 일을 힘들게하죠",
          "original_form": "코로나가 참 많은 일을 힘들게하죠",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028784.1.19",
          "form": "네 단체활동이 거의 불가능하다보니ㅠㅠ",
          "original_form": "네 단체활동이 거의 불가능하다보니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028784.1.20",
          "form": "대면해서 모일수가 없으니",
          "original_form": "대면해서 모일수가 없으니",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028784.1.21",
          "form": "특히 봉사나 동호회활동 등에 제약이 참 많은듯요",
          "original_form": "특히 봉사나 동호회활동 등에 제약이 참 많은듯요",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028784.1.22",
          "form": "맞아요 그래서요즘 줌으로 사람들 조금 만나고 있는 것 같아요",
          "original_form": "맞아요 그래서요즘 줌으로 사람들 조금 만나고 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100028784.1.23",
          "form": "줌이편하긴하네요 이제ㅠ",
          "original_form": "줌이편하긴하네요 이제ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100028784.1.24",
          "form": "줌 적응되니까",
          "original_form": "줌 적응되니까",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100028784.1.25",
          "form": "좋은 점도 있긴해요",
          "original_form": "좋은 점도 있긴해요",
          "speaker_id": "1",
          "time": "20210902 22:21"
        }
      ]
