        },
        {
          "id": "MDRW2100030279.1.2",
          "form": "멘토링 언제까지하는거임",
          "original_form": "멘토링 언제까지하는거임",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030279.1.3",
          "form": "보통 한학기인데",
          "original_form": "보통 한학기인데",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030279.1.4",
          "form": "몰아서해서",
          "original_form": "몰아서해서",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030279.1.5",
          "form": "한달?",
          "original_form": "한달?",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030279.1.6",
          "form": "나 저번에 이주만에함",
          "original_form": "나 저번에 이주만에함",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030279.1.7",
          "form": "ㅋㅋㅋ 이주만에 가능?",
          "original_form": "ㅋㅋㅋ 이주만에 가능?",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030279.1.8",
          "form": "학교 갈일은 거의 없나보네",
          "original_form": "학교 갈일은 거의 없나보네",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030279.1.9",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030279.1.10",
          "form": "이거 멘토링할때",
          "original_form": "이거 멘토링할때",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030279.1.11",
          "form": "멘티는 중복참여 불가?",
          "original_form": "멘티는 중복참여 불가?",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030279.1.12",
          "form": "가는할걸",
          "original_form": "가는할걸",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030279.1.13",
          "form": "멘토한명이 멘티여러명가는",
          "original_form": "멘토한명이 멘티여러명가는",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030279.1.14",
          "form": "활동비는 고정?",
          "original_form": "활동비는 고정?",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030279.1.15",
          "form": "더주는지",
          "original_form": "더주는지",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030279.1.16",
          "form": "한팀당 15라고만나옴",
          "original_form": "한팀당 15라고만나옴",
