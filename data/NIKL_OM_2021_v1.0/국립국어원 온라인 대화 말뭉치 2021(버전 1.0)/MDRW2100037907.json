      "utterance": [
        {
          "id": "MDRW2100037907.1.1",
          "form": "어제 천둥이랑 번개 장난아니었어요 ㅠ",
          "original_form": "어제 천둥이랑 번개 장난아니었어요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037907.1.2",
          "form": "아 진짜요? 여기는 그냥 비만 왔는데",
          "original_form": "아 진짜요? 여기는 그냥 비만 왔는데",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100037907.1.3",
          "form": "밤에 그랬나요?",
          "original_form": "밤에 그랬나요?",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037907.1.4",
          "form": "네네 어제밤요 ㅠ 오늘도 비는 많이왘ㅅ네영",
          "original_form": "네네 어제밤요 ㅠ 오늘도 비는 많이왘ㅅ네영",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037907.1.5",
          "form": "무서우셨겠네요ㅠ",
          "original_form": "무서우셨겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100037907.1.6",
          "form": "그러게요 요새 계속 비오더라구요ㅠ",
          "original_form": "그러게요 요새 계속 비오더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100037907.1.7",
          "form": "오늘 날씨 어때요?",
          "original_form": "오늘 날씨 어때요?",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100037907.1.8",
          "form": "여기는 그래도 오늘 좀 맑아졌어요\r\n거기는요?",
          "original_form": "여기는 그래도 오늘 좀 맑아졌어요\r\n거기는요?",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100037907.1.9",
          "form": "여기두요 ㅋㅋ어젠 쌀쌀하더니 오늘은 또덥네요",
          "original_form": "여기두요 ㅋㅋ어젠 쌀쌀하더니 오늘은 또덥네요",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100037907.1.10",
          "form": "그니까요 날씨가 엄청 오락가락해요ㅋㅋ",
          "original_form": "그니까요 날씨가 엄청 오락가락해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100037907.1.11",
          "form": "맞아요 ㅠㅠ곧 추워질거같아요",
          "original_form": "맞아요 ㅠㅠ곧 추워질거같아요",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100037907.1.12",
          "form": "마자요 이제 가을이 오겠어요",
          "original_form": "마자요 이제 가을이 오겠어요",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100037907.1.13",
          "form": "그래도 시원해서 좋아요",
          "original_form": "그래도 시원해서 좋아요",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100037907.1.14",
          "form": "가을 좋아하세요?",
          "original_form": "가을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100037907.1.15",
          "form": "네 저는 봄가을이 제일 좋아요!",
          "original_form": "네 저는 봄가을이 제일 좋아요!",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100037907.1.16",
          "form": "더운것도 싫어하고 추운것도 싫어해서요ㅠ",
          "original_form": "더운것도 싫어하고 추운것도 싫어해서요ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100037907.1.17",
          "form": "그쵸 ㅎㅎ 봄가을 좋아하시는분 많은거같아요",
          "original_form": "그쵸 ㅎㅎ 봄가을 좋아하시는분 많은거같아요",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100037907.1.18",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100037907.1.19",
          "form": "저두 시원하고 쌀쌀한 날씨가 좋아용",
          "original_form": "저두 시원하고 쌀쌀한 날씨가 좋아용",
