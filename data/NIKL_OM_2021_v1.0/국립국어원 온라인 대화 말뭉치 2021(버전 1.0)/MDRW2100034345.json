      "utterance": [
        {
          "id": "MDRW2100034345.1.1",
          "form": "유튜브나 넷플릭스 자주 보시나요?",
          "original_form": "유튜브나 넷플릭스 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100034345.1.2",
          "form": "맨날봐요 ㅋㅋㅋ",
          "original_form": "맨날봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100034345.1.3",
          "form": "아ㅋㅋ저두요ㅋㅋ",
          "original_form": "아ㅋㅋ저두요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100034345.1.4",
          "form": "뭐보세요?",
          "original_form": "뭐보세요?",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100034345.1.5",
          "form": "굿걸스 아세요??",
          "original_form": "굿걸스 아세요??",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034345.1.6",
          "form": "아니요 잘몰라요\n미드인가요?",
          "original_form": "아니요 잘몰라요\n미드인가요?",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100034345.1.7",
          "form": "네 주부들이 돈세탁하는 범죄미드에요 ㅋㅋ",
          "original_form": "네 주부들이 돈세탁하는 범죄미드에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100034345.1.8",
          "form": "신선하고 주인공이 예뻐요 ㅋㅋㅋ",
          "original_form": "신선하고 주인공이 예뻐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100034345.1.9",
          "form": "오 뭔가 되게 재밌을거같네요",
          "original_form": "오 뭔가 되게 재밌을거같네요",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100034345.1.10",
          "form": "주말에 한번 봐야겠어요ㅋㅋ\n주인공 유심히 봐봐야 겠네요ㅋㅋ",
          "original_form": "주말에 한번 봐야겠어요ㅋㅋ\n주인공 유심히 봐봐야 겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100034345.1.11",
          "form": "꿀잼이에요",
          "original_form": "꿀잼이에요",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100034345.1.12",
          "form": "이번달 말에 새시즌 나와서 재탕중이에요 ㅋㅋㅋ",
          "original_form": "이번달 말에 새시즌 나와서 재탕중이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100034345.1.13",
          "form": "오 재탕할정도면 진짜 재밌겠네요",
          "original_form": "오 재탕할정도면 진짜 재밌겠네요",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100034345.1.14",
          "form": "이번주에 봐야겠어요!",
          "original_form": "이번주에 봐야겠어요!",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100034345.1.15",
          "form": "또 재밌는거 있나요?",
          "original_form": "또 재밌는거 있나요?",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100034345.1.16",
          "form": "그리고 남자갱이랑 여주랑 썸타는데 아주 아찔하답니다 ㅋㅋㅋㅋ",
          "original_form": "그리고 남자갱이랑 여주랑 썸타는데 아주 아찔하답니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100034345.1.17",
          "form": "모던패밀리아세요??",
          "original_form": "모던패밀리아세요??",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100034345.1.18",
          "form": "와우! 더 재밌겠네요ㅋㅋㅋ",
          "original_form": "와우! 더 재밌겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100034345.1.19",
          "form": "넵 ㅋㅋ",
          "original_form": "넵 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100034345.1.20",
          "form": "모던패밀리 들어는 봤어요!",
          "original_form": "모던패밀리 들어는 봤어요!",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100034345.1.21",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100034345.1.22",
          "form": "제가 시트콤을 좋아해서 봤는데 너무 재밌어요",
          "original_form": "제가 시트콤을 좋아해서 봤는데 너무 재밌어요",
          "speaker_id": "2",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100034345.1.23",
          "form": "재밌는거 많이 아시네요!",
          "original_form": "재밌는거 많이 아시네요!",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100034345.1.24",
          "form": "재탕을 7번이나 했고 지금도 해요.. 사실 더이상 넷플에 뽑아먹을게 없어서지만요 ㅋㅌㅋ",
          "original_form": "재탕을 7번이나 했고 지금도 해요.. 사실 더이상 넷플에 뽑아먹을게 없어서지만요 ㅋㅌㅋ",
          "speaker_id": "2",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100034345.1.25",
          "form": "아 시트콤이에요?",
          "original_form": "아 시트콤이에요?",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100034345.1.26",
          "form": "자기전에 항상 보면서 잠들거든요",
          "original_form": "자기전에 항상 보면서 잠들거든요",
          "speaker_id": "2",
          "time": "20210828 00:17"
        }
      ]
    }
  ]
