      "utterance": [
        {
          "id": "MDRW2100005508.1.1",
          "form": "애니메이션 좋아해?",
          "original_form": "애니메이션 좋아해?",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005508.1.2",
          "form": "최근엔 잘 못봤는데 옛날에 엄청 많이 봤어",
          "original_form": "최근엔 잘 못봤는데 옛날에 엄청 많이 봤어",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005508.1.3",
          "form": "어떤 거?",
          "original_form": "어떤 거?",
          "speaker_id": "2",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005508.1.4",
          "form": "스펀지밥이나 심슨 등등",
          "original_form": "스펀지밥이나 심슨 등등",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005508.1.5",
          "form": "어릴 때 만화 많이 봤는데 ㅋㅋ",
          "original_form": "어릴 때 만화 많이 봤는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005508.1.6",
          "form": "그니까 ㅋㅋㅋ 재밌었는데 왜 요즘엔 못봤지",
          "original_form": "그니까 ㅋㅋㅋ 재밌었는데 왜 요즘엔 못봤지",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005508.1.7",
          "form": "나 짱구는 못말려 요즘에도 보고 있어",
          "original_form": "나 짱구는 못말려 요즘에도 보고 있어",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005508.1.8",
          "form": "짱구는 못말려 재밌지 ㅋㅋㅋ",
          "original_form": "짱구는 못말려 재밌지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005508.1.9",
          "form": "오징어는 말려도 짱구는 못말려 ㅋㅋ",
          "original_form": "오징어는 말려도 짱구는 못말려 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005508.1.10",
          "form": "요즘에 재미있는 애니메이션 많이 나오는 것 같더라",
          "original_form": "요즘에 재미있는 애니메이션 많이 나오는 것 같더라",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005508.1.11",
          "form": "그러게 옛날에도 재밌었지만 요즘엔 막 3D 이런 거 나오고",
          "original_form": "그러게 옛날에도 재밌었지만 요즘엔 막 3D 이런 거 나오고",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005508.1.12",
          "form": "3D 하니까 아바타 생각나네",
          "original_form": "3D 하니까 아바타 생각나네",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005508.1.13",
          "form": "나 아바타 3D 극장에서 봤어 ㅋㅋ",
          "original_form": "나 아바타 3D 극장에서 봤어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005508.1.14",
          "form": "나도 ㅋㅋㅋ 그땐 난리였지",
          "original_form": "나도 ㅋㅋㅋ 그땐 난리였지",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005508.1.15",
          "form": "나이 먹고는 애니메이션은 잘 안보게 된 것 같아",
          "original_form": "나이 먹고는 애니메이션은 잘 안보게 된 것 같아",
          "speaker_id": "1",
          "time": "20210821 22:52"
        }
