      "utterance": [
        {
          "id": "MDRW2100037702.1.1",
          "form": "너무 요정같아용",
          "original_form": "너무 요정같아용",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100037702.1.2",
          "form": "네 살아있는요정이예요",
          "original_form": "네 살아있는요정이예요",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100037702.1.3",
          "form": "오늘유퀴즈소시나와요",
          "original_form": "오늘유퀴즈소시나와요",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100037702.1.4",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037702.1.5",
          "form": "재밌을거같아요 기대돼용",
          "original_form": "재밌을거같아요 기대돼용",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100037702.1.6",
          "form": "보셨나용 전 다른 일 하느라 못봤어용",
          "original_form": "보셨나용 전 다른 일 하느라 못봤어용",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100037702.1.7",
          "form": "저 십란한밤봤어요..ㅋㄱㅋㄱ화요일방송인데오늘급수요일방송함ㅋㅋㅋ",
          "original_form": "저 십란한밤봤어요..ㅋㄱㅋㄱ화요일방송인데오늘급수요일방송함ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100037702.1.8",
          "form": "진짜자유로운방송인듯 요일도옮기고시간도옮기고",
          "original_form": "진짜자유로운방송인듯 요일도옮기고시간도옮기고",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100037702.1.9",
          "form": "ㅋㅋㅌㅋㅋㅋ앜ㅋㅋ",
          "original_form": "ㅋㅋㅌㅋㅋㅋ앜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100037702.1.10",
          "form": "그래요 유퀴즈는 재방송 마니하니깐",
          "original_form": "그래요 유퀴즈는 재방송 마니하니깐",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100037702.1.11",
          "form": "ㅋㅋㅋㅋㄱㅋ저건아예브이앱에박제되어있는데 십란선택",
          "original_form": "ㅋㅋㅋㅋㄱㅋ저건아예브이앱에박제되어있는데 십란선택",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100037702.1.12",
          "form": "ㄱㅈㅋㅈㄱㅈㄱㅈㅋ",
          "original_form": "ㄱㅈㅋㅈㄱㅈㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100037702.1.13",
          "form": "티비재방은잘보는데역시브이앱다시보기는힘들긴햊..",
          "original_form": "티비재방은잘보는데역시브이앱다시보기는힘들긴햊..",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100037702.1.14",
          "form": "마자요 브이앱은 지나가면 안봐ㅡ....",
          "original_form": "마자요 브이앱은 지나가면 안봐ㅡ....",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100037702.1.15",
          "form": "마자절대안봐진심...",
          "original_form": "마자절대안봐진심...",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100037702.1.16",
          "form": "라디오도 다시 듣기 절대 못하는 류에요...",
          "original_form": "라디오도 다시 듣기 절대 못하는 류에요...",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100037702.1.17",
          "form": "저도요 이서쿤라디오 앞만듣다포기",
          "original_form": "저도요 이서쿤라디오 앞만듣다포기",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100037702.1.18",
          "form": "ㅋㅋㅋㅋㅋ밀린 떡밥은 해결이 어렵다",
          "original_form": "ㅋㅋㅋㅋㅋ밀린 떡밥은 해결이 어렵다",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100037702.1.19",
