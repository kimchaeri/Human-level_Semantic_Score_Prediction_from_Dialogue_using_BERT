      "utterance": [
        {
          "id": "MDRW2100034272.1.1",
          "form": "네 요새는 홈트만 해요",
          "original_form": "네 요새는 홈트만 해요",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100034272.1.2",
          "form": "걷기도 했었는데",
          "original_form": "걷기도 했었는데",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100034272.1.3",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100034272.1.4",
          "form": "요새 계속 비오고 흐리고해서 못나가겠더라구요ㅠ 핑계지만ㅋㅋ",
          "original_form": "요새 계속 비오고 흐리고해서 못나가겠더라구요ㅠ 핑계지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100034272.1.5",
          "form": "요샌 홈트가 안전하긴하죠",
          "original_form": "요샌 홈트가 안전하긴하죠",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100034272.1.6",
          "form": "ㅋㅋㅋㅋ 저랑 같은 핑계에요!! ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 저랑 같은 핑계에요!! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100034272.1.7",
          "form": "산책이라도 하고 싶은데 우산 들고다니긴 싫고..",
          "original_form": "산책이라도 하고 싶은데 우산 들고다니긴 싫고..",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100034272.1.8",
          "form": "마자요 헬스장도 마스크 써야해서 힘들어요",
          "original_form": "마자요 헬스장도 마스크 써야해서 힘들어요",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100034272.1.9",
          "form": "마자요ㅋㅋㄱ",
          "original_form": "마자요ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100034272.1.10",
          "form": "엄청 숨찰거같아요 ㅠ",
          "original_form": "엄청 숨찰거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100034272.1.11",
          "form": "굳이 이렇게까지 해야하나싶고ㅋㅋ",
          "original_form": "굳이 이렇게까지 해야하나싶고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100034272.1.12",
          "form": "맞아여 ㅋㅋㅋ",
          "original_form": "맞아여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100034272.1.13",
          "form": "진짜 마스크 쓰고 런닝머신하는데",
          "original_form": "진짜 마스크 쓰고 런닝머신하는데",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100034272.1.14",
          "form": "투명우산이 내장돼있음 좋겠어요...게임아이템처럼",
          "original_form": "투명우산이 내장돼있음 좋겠어요...게임아이템처럼",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100034272.1.15",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100034272.1.16",
          "form": "죽을거같더라구요",
          "original_form": "죽을거같더라구요",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100034272.1.17",
          "form": "허 생각만해도 숨차요..",
          "original_form": "허 생각만해도 숨차요..",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100034272.1.18",
          "form": "내장돼있으면 최고죠ㅋㅋ",
          "original_form": "내장돼있으면 최고죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100034272.1.19",
          "form": "언덕 조금만 마스크쓰고 올라도 엄청 힘들어요",
          "original_form": "언덕 조금만 마스크쓰고 올라도 엄청 힘들어요",
          "speaker_id": "1",
          "time": "20210827 22:32"
        }
