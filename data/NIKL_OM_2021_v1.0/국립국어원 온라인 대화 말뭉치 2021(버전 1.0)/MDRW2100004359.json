      "utterance": [
        {
          "id": "MDRW2100004359.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004359.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004359.1.3",
          "form": "사고싶은 물건 있으세요?",
          "original_form": "사고싶은 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004359.1.4",
          "form": "저는 무선 이어폰 사고싶어요!",
          "original_form": "저는 무선 이어폰 사고싶어요!",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004359.1.5",
          "form": "아직 유선 이어폰 쓰거든요",
          "original_form": "아직 유선 이어폰 쓰거든요",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004359.1.6",
          "form": "저도 무선이어폰 쓰다가",
          "original_form": "저도 무선이어폰 쓰다가",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004359.1.7",
          "form": "충전하는게 너무 귀찮아서 팔고 유선이어폰 쓰고있네요...",
          "original_form": "충전하는게 너무 귀찮아서 팔고 유선이어폰 쓰고있네요...",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004359.1.8",
          "form": "정말요! 저도 잃어버리는게 고민되어서 아직 써보지는 않았어요 가격도 유선보다 비싸고 해서요",
          "original_form": "정말요! 저도 잃어버리는게 고민되어서 아직 써보지는 않았어요 가격도 유선보다 비싸고 해서요",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004359.1.9",
          "form": "어떤 이어폰 사고싶어요?",
          "original_form": "어떤 이어폰 사고싶어요?",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004359.1.10",
          "form": "저는 젠하이저 거 써봤는데 좋더라구요",
          "original_form": "저는 젠하이저 거 써봤는데 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004359.1.11",
          "form": "그냥 막연히 버즈 살까 싶어요! 아는게 없어서 그냥 가격대비 튼튼한게 좋은거 찾아요",
          "original_form": "그냥 막연히 버즈 살까 싶어요! 아는게 없어서 그냥 가격대비 튼튼한게 좋은거 찾아요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004359.1.12",
          "form": "젠하이저 기억해둬야겠네요ㅎㅎ",
          "original_form": "젠하이저 기억해둬야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004359.1.13",
          "form": "혹시 중고거래 해보셨어요?",
          "original_form": "혹시 중고거래 해보셨어요?",
          "speaker_id": "1",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004359.1.14",
          "form": "네네 해봤어요! 자주합니다",
          "original_form": "네네 해봤어요! 자주합니다",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004359.1.15",
          "form": "그럼 직거래도 해보셨어요??",
          "original_form": "그럼 직거래도 해보셨어요??",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004359.1.16",
          "form": "그럼요! 직거래 위주가 차라리 사기가 없어서 좋아요",
          "original_form": "그럼요! 직거래 위주가 차라리 사기가 없어서 좋아요",
          "speaker_id": "2",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004359.1.17",
          "form": "저도 직거래 딱 한번 해봤는데 소심해서 그런가 좀 떨리더라구요...",
          "original_form": "저도 직거래 딱 한번 해봤는데 소심해서 그런가 좀 떨리더라구요...",
          "speaker_id": "1",
          "time": "20210821 17:24"
        }
      ]
