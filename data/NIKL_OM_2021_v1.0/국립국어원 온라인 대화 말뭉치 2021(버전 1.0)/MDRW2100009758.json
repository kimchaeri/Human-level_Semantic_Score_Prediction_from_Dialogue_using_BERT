      "utterance": [
        {
          "id": "MDRW2100009758.1.1",
          "form": "강한 비와 바람이 예상된다네..",
          "original_form": "강한 비와 바람이 예상된다네..",
          "speaker_id": "2",
          "time": "20210823 09:32"
        },
        {
          "id": "MDRW2100009758.1.2",
          "form": "또 습해지겠군..",
          "original_form": "또 습해지겠군..",
          "speaker_id": "1",
          "time": "20210823 09:33"
        },
        {
          "id": "MDRW2100009758.1.3",
          "form": "어제 빨래 미리 다 해놨지?",
          "original_form": "어제 빨래 미리 다 해놨지?",
          "speaker_id": "2",
          "time": "20210823 09:33"
        },
        {
          "id": "MDRW2100009758.1.4",
          "form": "ㅇㅇ 근데 아직 다 안말랐어",
          "original_form": "ㅇㅇ 근데 아직 다 안말랐어",
          "speaker_id": "1",
          "time": "20210823 09:33"
        },
        {
          "id": "MDRW2100009758.1.5",
          "form": "태풍 오기 전에 다 말라야 할텐데 ㅎㅎ",
          "original_form": "태풍 오기 전에 다 말라야 할텐데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 09:33"
        },
        {
          "id": "MDRW2100009758.1.6",
          "form": "또 마르는데 3일 걸리는 거 아닌가 몰라",
          "original_form": "또 마르는데 3일 걸리는 거 아닌가 몰라",
          "speaker_id": "1",
          "time": "20210823 09:33"
        },
        {
          "id": "MDRW2100009758.1.7",
          "form": "비 너무 많이 안왔으면 좋겠다.",
          "original_form": "비 너무 많이 안왔으면 좋겠다.",
          "speaker_id": "2",
          "time": "20210823 09:34"
        },
        {
          "id": "MDRW2100009758.1.8",
          "form": "예상이랑 조금 다르게 이동하는 것 같던데",
          "original_form": "예상이랑 조금 다르게 이동하는 것 같던데",
          "speaker_id": "1",
          "time": "20210823 09:34"
        },
        {
          "id": "MDRW2100009758.1.9",
          "form": "항상 그렇지 뭐 ㅋㅋ",
          "original_form": "항상 그렇지 뭐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:34"
        },
        {
          "id": "MDRW2100009758.1.10",
          "form": "이번에도 일기예보가 맞지 않길 바래보겠어",
          "original_form": "이번에도 일기예보가 맞지 않길 바래보겠어",
          "speaker_id": "1",
          "time": "20210823 09:34"
        },
        {
          "id": "MDRW2100009758.1.11",
          "form": "비가 안와야 밖에서 운동도 하고 할텐데",
          "original_form": "비가 안와야 밖에서 운동도 하고 할텐데",
          "speaker_id": "2",
          "time": "20210823 09:34"
        },
        {
          "id": "MDRW2100009758.1.12",
          "form": "며칠 날씨 좋더니 계속 흐리네",
          "original_form": "며칠 날씨 좋더니 계속 흐리네",
          "speaker_id": "1",
          "time": "20210823 09:35"
        },
        {
          "id": "MDRW2100009758.1.13",
          "form": "기온도 훅 떨어져서 이제 창문 열면 춥다",
          "original_form": "기온도 훅 떨어져서 이제 창문 열면 춥다",
          "speaker_id": "2",
          "time": "20210823 09:35"
        },
        {
          "id": "MDRW2100009758.1.14",
          "form": "맞바람 치면 새벽에 춥더라고",
          "original_form": "맞바람 치면 새벽에 춥더라고",
          "speaker_id": "1",
          "time": "20210823 09:35"
        },
        {
          "id": "MDRW2100009758.1.15",
          "form": "ㅇㅇ 선풍기 집어넣을 때가 된듯",
          "original_form": "ㅇㅇ 선풍기 집어넣을 때가 된듯",
          "speaker_id": "2",
          "time": "20210823 09:35"
        },
        {
          "id": "MDRW2100009758.1.16",
          "form": "날씨가 점점 흐려지고 있어",
          "original_form": "날씨가 점점 흐려지고 있어",
