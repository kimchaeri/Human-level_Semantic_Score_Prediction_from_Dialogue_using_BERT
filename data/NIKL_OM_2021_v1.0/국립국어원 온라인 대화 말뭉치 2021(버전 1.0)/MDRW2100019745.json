      "utterance": [
        {
          "id": "MDRW2100019745.1.1",
          "form": "최근 보신 책 있나요",
          "original_form": "최근 보신 책 있나요",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019745.1.2",
          "form": "최근에 여러 책을 한꺼번에 읽는 중인데",
          "original_form": "최근에 여러 책을 한꺼번에 읽는 중인데",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019745.1.3",
          "form": "소설은 버드스트라이크",
          "original_form": "소설은 버드스트라이크",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019745.1.4",
          "form": "인문학은 소크라테스 익스프레스",
          "original_form": "인문학은 소크라테스 익스프레스",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019745.1.5",
          "form": "만화는 위국일기 읽어요",
          "original_form": "만화는 위국일기 읽어요",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019745.1.6",
          "form": "와우 많이보시네요",
          "original_form": "와우 많이보시네요",
          "speaker_id": "1",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019745.1.7",
          "form": "저는 채식주의자를 상반기에 본거같아요",
          "original_form": "저는 채식주의자를 상반기에 본거같아요",
          "speaker_id": "1",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019745.1.8",
          "form": "채식주의자 재밌나요? 봐야지 맨날 생각만 했는데",
          "original_form": "채식주의자 재밌나요? 봐야지 맨날 생각만 했는데",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019745.1.9",
          "form": "일부러 피해다니다가",
          "original_form": "일부러 피해다니다가",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019745.1.10",
          "form": "보게됐네요",
          "original_form": "보게됐네요",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019745.1.11",
          "form": "폭력적인 이야기싫어요",
          "original_form": "폭력적인 이야기싫어요",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019745.1.12",
          "form": "폭력적인 이야기에요?",
          "original_form": "폭력적인 이야기에요?",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019745.1.13",
          "form": "네 좀 기괴하고 폭력적이에요",
          "original_form": "네 좀 기괴하고 폭력적이에요",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019745.1.14",
          "form": "그럼 좀 안 끌리네여 ㅋㅋㅋ",
          "original_form": "그럼 좀 안 끌리네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019745.1.15",
          "form": "네 그냥 보지마여ㅠ",
          "original_form": "네 그냥 보지마여ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019745.1.16",
          "form": "상받았던 책이지 않나야",
          "original_form": "상받았던 책이지 않나야",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019745.1.17",
          "form": "마자여 그래서봤는데",
          "original_form": "마자여 그래서봤는데",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019745.1.18",
          "form": "아니면 절대안봐요",
          "original_form": "아니면 절대안봐요",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019745.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019745.1.20",
          "form": "맨부커상 ㅠ",
          "original_form": "맨부커상 ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019745.1.21",
          "form": "ㅋㅋㅋㅋ한강 다른 작품도 읽었나여",
          "original_form": "ㅋㅋㅋㅋ한강 다른 작품도 읽었나여",
          "speaker_id": "2",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100019745.1.22",
          "form": "아니요 앞으로 읽지않을거에요",
          "original_form": "아니요 앞으로 읽지않을거에요",
          "speaker_id": "1",
