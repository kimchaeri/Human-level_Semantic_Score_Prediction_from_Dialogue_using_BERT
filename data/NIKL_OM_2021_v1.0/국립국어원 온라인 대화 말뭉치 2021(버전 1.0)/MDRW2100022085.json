      "utterance": [
        {
          "id": "MDRW2100022085.1.1",
          "form": "고수 못먹구요",
          "original_form": "고수 못먹구요",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100022085.1.2",
          "form": "채팅 날라갔네 ㅋㅋ",
          "original_form": "채팅 날라갔네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100022085.1.3",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100022085.1.4",
          "form": "그리고 개고기 안!먹구요 응",
          "original_form": "그리고 개고기 안!먹구요 응",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100022085.1.5",
          "form": "에........... 그정도?",
          "original_form": "에........... 그정도?",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022085.1.6",
          "form": "저 맛있는 녀석들 보는데",
          "original_form": "저 맛있는 녀석들 보는데",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022085.1.7",
          "form": "김준현이 고수",
          "original_form": "김준현이 고수",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022085.1.8",
          "form": "불매하는 가게나 회사는 있지만 음식은 정말 잘 안가려서 ㅇㅁㅇ..",
          "original_form": "불매하는 가게나 회사는 있지만 음식은 정말 잘 안가려서 ㅇㅁㅇ..",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022085.1.9",
          "form": "크 그쳐 고수 먹을 줄 아는 사람들은 잘 먹는데",
          "original_form": "크 그쳐 고수 먹을 줄 아는 사람들은 잘 먹는데",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022085.1.10",
          "form": "눈 딱감고 열번 먹으면 중독된다고 하던데요",
          "original_form": "눈 딱감고 열번 먹으면 중독된다고 하던데요",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022085.1.11",
          "form": "저 호기롭게 도전했다가 으엉 했어요 ㅠㅠ",
          "original_form": "저 호기롭게 도전했다가 으엉 했어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022085.1.12",
          "form": "열번 먹어봐야 하나 하아..",
          "original_form": "열번 먹어봐야 하나 하아..",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022085.1.13",
          "form": "고수가 좀 힘들죠",
          "original_form": "고수가 좀 힘들죠",
          "speaker_id": "1",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100022085.1.14",
          "form": "외국인들은 깻잎 힘들어하죠",
          "original_form": "외국인들은 깻잎 힘들어하죠",
          "speaker_id": "1",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100022085.1.15",
          "form": "똠양꿍에 들어있길래 이얍! 하고 먹었다가 으엉 했거든여",
          "original_form": "똠양꿍에 들어있길래 이얍! 하고 먹었다가 으엉 했거든여",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100022085.1.16",
          "form": "아 맞아요. 차조기도 좀 향이 세요",
          "original_form": "아 맞아요. 차조기도 좀 향이 세요",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100022085.1.17",
          "form": "깻잎이랑 완전 다르지만, 깻잎도 외국에선 아주 독특하죠 응응",
          "original_form": "깻잎이랑 완전 다르지만, 깻잎도 외국에선 아주 독특하죠 응응",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100022085.1.18",
          "form": "깻잎 페스토.. 깻잎 모히또 ㅋㅋㅋㅋㅋ",
          "original_form": "깻잎 페스토.. 깻잎 모히또 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100022085.1.19",
          "form": "아 그거 밨어요",
          "original_form": "아 그거 밨어요",
          "speaker_id": "1",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100022085.1.20",
          "form": "백종원 레시피",
          "original_form": "백종원 레시피",
          "speaker_id": "1",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100022085.1.21",
          "form": "페스토 의외로 각종 풀로 만들기 좋은거 아세여?ㅡㅡ 흐흫",
          "original_form": "페스토 의외로 각종 풀로 만들기 좋은거 아세여?ㅡㅡ 흐흫",
          "speaker_id": "2",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100022085.1.22",
          "form": "바질 없어도 돼.. 시금치 사오면 돼 ㅋㅋㅋㅋㅋ",
          "original_form": "바질 없어도 돼.. 시금치 사오면 돼 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100022085.1.23",
          "form": "그래요?ㅋㅋ 시판되는거 사다 써서",
          "original_form": "그래요?ㅋㅋ 시판되는거 사다 써서",
          "speaker_id": "1",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100022085.1.24",
          "form": "시판되는거처럼 바질향이 풍부한건 아닌데",
          "original_form": "시판되는거처럼 바질향이 풍부한건 아닌데",
          "speaker_id": "2",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100022085.1.25",
          "form": "다른 풀로 만들면 또 그거의 맛이 있구여",
          "original_form": "다른 풀로 만들면 또 그거의 맛이 있구여",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100022085.1.26",
          "form": "사실 올리브오일이랑 견과류 맛으로 고소한게 좋은 사람은",
          "original_form": "사실 올리브오일이랑 견과류 맛으로 고소한게 좋은 사람은",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100022085.1.27",
          "form": "향 별로 없는 풀로 만들어도 되요",
          "original_form": "향 별로 없는 풀로 만들어도 되요",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100022085.1.28",
          "form": "가지로도 만드는거 아세여?",
          "original_form": "가지로도 만드는거 아세여?",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100022085.1.29",
          "form": "가지는 페스토 느낌이 안날것같은데요",
          "original_form": "가지는 페스토 느낌이 안날것같은데요",
          "speaker_id": "1",
          "time": "20210829 02:35"
        }
      ]
    }
