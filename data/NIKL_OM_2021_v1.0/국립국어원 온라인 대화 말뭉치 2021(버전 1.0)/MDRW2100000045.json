        },
        {
          "id": "MDRW2100000045.1.2",
          "form": "학교 잘다니고 있나",
          "original_form": "학교 잘다니고 있나",
          "speaker_id": "1",
          "time": "20210521 14:10"
        },
        {
          "id": "MDRW2100000045.1.3",
          "form": "요 뭔데ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "요 뭔데ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210521 14:11"
        },
        {
          "id": "MDRW2100000045.1.4",
          "form": "나 요즘 동아리 사람들이랑 자주만남",
          "original_form": "나 요즘 동아리 사람들이랑 자주만남",
          "speaker_id": "2",
          "time": "20210521 14:11"
        },
        {
          "id": "MDRW2100000045.1.5",
          "form": "오 합창단 동아리?",
          "original_form": "오 합창단 동아리?",
          "speaker_id": "1",
          "time": "20210521 14:12"
        },
        {
          "id": "MDRW2100000045.1.6",
          "form": "친구들 많이사겼겠네",
          "original_form": "친구들 많이사겼겠네",
          "speaker_id": "1",
          "time": "20210521 14:13"
        },
        {
          "id": "MDRW2100000045.1.7",
          "form": "응 나 어제도 저녁같이 먹었엉",
          "original_form": "응 나 어제도 저녁같이 먹었엉",
          "speaker_id": "2",
          "time": "20210521 14:13"
        },
        {
          "id": "MDRW2100000045.1.8",
          "form": "20살 신입생 들어왔는데",
          "original_form": "20살 신입생 들어왔는데",
          "speaker_id": "2",
          "time": "20210521 14:13"
        },
        {
          "id": "MDRW2100000045.1.9",
          "form": "착하고 그래서 내가 밥사줬어",
          "original_form": "착하고 그래서 내가 밥사줬어",
          "speaker_id": "2",
          "time": "20210521 14:13"
        },
        {
          "id": "MDRW2100000045.1.10",
          "form": "오 어떤점이 착했어",
          "original_form": "오 어떤점이 착했어",
          "speaker_id": "1",
          "time": "20210521 14:13"
        },
        {
          "id": "MDRW2100000045.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ막 재밌는 스타일은 아닌데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ막 재밌는 스타일은 아닌데",
          "speaker_id": "2",
          "time": "20210521 14:14"
        },
        {
          "id": "MDRW2100000045.1.12",
          "form": "내가 막 장난쳐도",
          "original_form": "내가 막 장난쳐도",
          "speaker_id": "2",
          "time": "20210521 14:14"
        },
        {
          "id": "MDRW2100000045.1.13",
          "form": "잘 받아줘가지궁",
          "original_form": "잘 받아줘가지궁",
          "speaker_id": "2",
          "time": "20210521 14:14"
        },
        {
          "id": "MDRW2100000045.1.14",
          "form": "많이 친해졌당",
          "original_form": "많이 친해졌당",
          "speaker_id": "2",
          "time": "20210521 14:14"
        },
        {
          "id": "MDRW2100000045.1.15",
          "form": "남정네야?",
          "original_form": "남정네야?",
          "speaker_id": "1",
          "time": "20210521 14:15"
        },
        {
          "id": "MDRW2100000045.1.16",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210521 14:15"
        },
        {
          "id": "MDRW2100000045.1.17",
          "form": "어떻게 알았징",
          "original_form": "어떻게 알았징",
          "speaker_id": "2",
          "time": "20210521 14:15"
        },
        {
          "id": "MDRW2100000045.1.18",
          "form": "ㅋㅋㅎ",
          "original_form": "ㅋㅋㅎ",
          "speaker_id": "2",
          "time": "20210521 14:15"
        },
        {
          "id": "MDRW2100000045.1.19",
          "form": "요놈",
          "original_form": "요놈",
          "speaker_id": "1",
          "time": "20210521 14:18"
        },
        {
          "id": "MDRW2100000045.1.20",
