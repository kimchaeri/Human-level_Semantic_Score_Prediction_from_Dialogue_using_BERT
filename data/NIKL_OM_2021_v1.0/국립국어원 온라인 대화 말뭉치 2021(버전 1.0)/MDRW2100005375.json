      "utterance": [
        {
          "id": "MDRW2100005375.1.1",
          "form": "아는형님 하는데 펜싱4인방 나와요",
          "original_form": "아는형님 하는데 펜싱4인방 나와요",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005375.1.2",
          "form": "2부인가봐요",
          "original_form": "2부인가봐요",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005375.1.3",
          "form": "어디서해요?",
          "original_form": "어디서해요?",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005375.1.4",
          "form": "jtbc요",
          "original_form": "jtbc요",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005375.1.5",
          "form": "저번주하고 중간에끝났는데",
          "original_form": "저번주하고 중간에끝났는데",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005375.1.6",
          "form": "아하 채널돌렸어요",
          "original_form": "아하 채널돌렸어요",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005375.1.7",
          "form": "틀기만하면 펜싱선수들이 나오네요",
          "original_form": "틀기만하면 펜싱선수들이 나오네요",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005375.1.8",
          "form": "김준호 선수 영웅재중닮았어요",
          "original_form": "김준호 선수 영웅재중닮았어요",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005375.1.9",
          "form": "아는형님 별로 안좋아하는데 ㅎㅎ",
          "original_form": "아는형님 별로 안좋아하는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005375.1.10",
          "form": "펜싱선수들땜에 보네요",
          "original_form": "펜싱선수들땜에 보네요",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005375.1.11",
          "form": "저도요 ㅎ",
          "original_form": "저도요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005375.1.12",
          "form": "지금나오는 선수이름 뭐죠?",
          "original_form": "지금나오는 선수이름 뭐죠?",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005375.1.13",
          "form": "젤 웃긴거같은데",
          "original_form": "젤 웃긴거같은데",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005375.1.14",
          "form": "선수들이름은 잘몰라요 ㅎ",
          "original_form": "선수들이름은 잘몰라요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005375.1.15",
          "form": "잘생겨서 그냥 흐믓하게 바라볼뿐 ㅋ",
          "original_form": "잘생겨서 그냥 흐믓하게 바라볼뿐 ㅋ",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005375.1.16",
          "form": "ㅎㅎ 무슨 정환인데ㅋ",
          "original_form": "ㅎㅎ 무슨 정환인데ㅋ",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005375.1.17",
          "form": "막내빼고 다결혼했더라구요",
          "original_form": "막내빼고 다결혼했더라구요",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005375.1.18",
          "form": "김정환?",
          "original_form": "김정환?",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005375.1.19",
          "form": "오...다 나이가 있으신가요?",
          "original_form": "오...다 나이가 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005375.1.20",
          "form": "그런가봐요 ㅎ",
          "original_form": "그런가봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005375.1.21",
          "form": "준호라는사람은 아이돌같은데요?",
          "original_form": "준호라는사람은 아이돌같은데요?",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005375.1.22",
          "form": "김정환선수 키작아보이는데 180이 넘는데요",
          "original_form": "김정환선수 키작아보이는데 180이 넘는데요",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005375.1.23",
          "form": "안그래도 그선수 소속사계약했다는데요ㅎ",
          "original_form": "안그래도 그선수 소속사계약했다는데요ㅎ",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005375.1.24",
          "form": "와  작아보이는데요?",
          "original_form": "와  작아보이는데요?",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005375.1.25",
          "form": "그럼 다른선수들이 큰건가요?",
          "original_form": "그럼 다른선수들이 큰건가요?",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005375.1.26",
          "form": "오상욱선수가 190이넘고",
          "original_form": "오상욱선수가 190이넘고",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005375.1.27",
          "form": "김정환선수 은근웃겨요ㅋ",
          "original_form": "김정환선수 은근웃겨요ㅋ",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005375.1.28",
          "form": "소속사 계약이면 연예인하는건가요?ㅎ",
          "original_form": "소속사 계약이면 연예인하는건가요?ㅎ",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005375.1.29",
          "form": "방송에 욕심있나봐요 ㅎ",
          "original_form": "방송에 욕심있나봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005375.1.30",
          "form": "운동이랑 홍보 같이하지않을까요",
          "original_form": "운동이랑 홍보 같이하지않을까요",
          "speaker_id": "1",
          "time": "20210821 22:28"
        }
      ]
    }
