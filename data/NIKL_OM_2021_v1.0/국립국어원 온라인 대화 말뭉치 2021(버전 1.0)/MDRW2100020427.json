      "utterance": [
        {
          "id": "MDRW2100020427.1.1",
          "form": "안녕하세용 즐겨보시는 티비 프로그램 있으세요?",
          "original_form": "안녕하세용 즐겨보시는 티비 프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210828 05:19"
        },
        {
          "id": "MDRW2100020427.1.2",
          "form": "저는 예능 프로그램을 주로 봐요",
          "original_form": "저는 예능 프로그램을 주로 봐요",
          "speaker_id": "2",
          "time": "20210828 05:20"
        },
        {
          "id": "MDRW2100020427.1.3",
          "form": "혹시 대탈출 보세요?",
          "original_form": "혹시 대탈출 보세요?",
          "speaker_id": "1",
          "time": "20210828 05:20"
        },
        {
          "id": "MDRW2100020427.1.4",
          "form": "유퀴즈랑 놀토랑 대탈출",
          "original_form": "유퀴즈랑 놀토랑 대탈출",
          "speaker_id": "2",
          "time": "20210828 05:20"
        },
        {
          "id": "MDRW2100020427.1.5",
          "form": "어 네네네",
          "original_form": "어 네네네",
          "speaker_id": "2",
          "time": "20210828 05:20"
        },
        {
          "id": "MDRW2100020427.1.6",
          "form": "저랑 취향이 비슷하신것같아요ㅎㅎ",
          "original_form": "저랑 취향이 비슷하신것같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 05:20"
        },
        {
          "id": "MDRW2100020427.1.7",
          "form": "대탈출 너무 재밌어요",
          "original_form": "대탈출 너무 재밌어요",
          "speaker_id": "2",
          "time": "20210828 05:20"
        },
        {
          "id": "MDRW2100020427.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 05:20"
        },
        {
          "id": "MDRW2100020427.1.9",
          "form": "특히 지난주꺼 너무 재밌었어요",
          "original_form": "특히 지난주꺼 너무 재밌었어요",
          "speaker_id": "1",
          "time": "20210828 05:21"
        },
        {
          "id": "MDRW2100020427.1.10",
          "form": "아 저 지난주엔 뭐하느라 아직 못봤어요",
          "original_form": "아 저 지난주엔 뭐하느라 아직 못봤어요",
          "speaker_id": "2",
          "time": "20210828 05:21"
        },
        {
          "id": "MDRW2100020427.1.11",
          "form": "오늘 느긋하게 보려고요ㅋㅋㅋ",
          "original_form": "오늘 느긋하게 보려고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:21"
        },
        {
          "id": "MDRW2100020427.1.12",
          "form": "개인적으로는",
          "original_form": "개인적으로는",
          "speaker_id": "1",
          "time": "20210828 05:21"
        },
        {
          "id": "MDRW2100020427.1.13",
          "form": "이번시즌중에서",
          "original_form": "이번시즌중에서",
          "speaker_id": "1",
          "time": "20210828 05:21"
        },
        {
          "id": "MDRW2100020427.1.14",
          "form": "엄청 인셉션 같았다던데",
          "original_form": "엄청 인셉션 같았다던데",
          "speaker_id": "2",
          "time": "20210828 05:21"
        },
        {
          "id": "MDRW2100020427.1.15",
          "form": "제일 재밌었어요",
          "original_form": "제일 재밌었어요",
          "speaker_id": "1",
          "time": "20210828 05:22"
        },
        {
          "id": "MDRW2100020427.1.16",
          "form": "네내",
          "original_form": "네내",
          "speaker_id": "2",
          "time": "20210828 05:22"
        },
        {
          "id": "MDRW2100020427.1.17",
          "form": "오 네네ㅋㅋㅋ",
          "original_form": "오 네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:22"
        },
        {
          "id": "MDRW2100020427.1.18",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210828 05:22"
        },
        {
          "id": "MDRW2100020427.1.19",
          "form": "놀면 뭐하니도",
          "original_form": "놀면 뭐하니도",
          "speaker_id": "1",
          "time": "20210828 05:22"
        },
        {
          "id": "MDRW2100020427.1.20",
          "form": "무도 멤버 나오니까 재밌더라구요",
          "original_form": "무도 멤버 나오니까 재밌더라구요",
          "speaker_id": "1",
          "time": "20210828 05:22"
        },
        {
          "id": "MDRW2100020427.1.21",
          "form": "좀비 교도소가 너무 허무하게 끝나서 ㅠ",
