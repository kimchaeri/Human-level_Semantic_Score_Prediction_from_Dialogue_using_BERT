      "utterance": [
        {
          "id": "MDRW2100046927.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20211108 11:24"
        },
        {
          "id": "MDRW2100046927.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211108 11:30"
        },
        {
          "id": "MDRW2100046927.1.3",
          "form": "셀프인테리어 해보셨나요 ㅎㅎ?",
          "original_form": "셀프인테리어 해보셨나요 ㅎㅎ?",
          "speaker_id": "2",
          "time": "20211108 11:30"
        },
        {
          "id": "MDRW2100046927.1.4",
          "form": "아니요 안해봤어요",
          "original_form": "아니요 안해봤어요",
          "speaker_id": "1",
          "time": "20211108 11:38"
        },
        {
          "id": "MDRW2100046927.1.5",
          "form": "하지만 한번 해보고싶어요",
          "original_form": "하지만 한번 해보고싶어요",
          "speaker_id": "1",
          "time": "20211108 11:38"
        },
        {
          "id": "MDRW2100046927.1.6",
          "form": "엇 왜 안해보셨나용 ㅎㅎ",
          "original_form": "엇 왜 안해보셨나용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211109 00:15"
        },
        {
          "id": "MDRW2100046927.1.7",
          "form": "인테리어 하고싶긴한데 비용이 많이들꺼같아서요",
          "original_form": "인테리어 하고싶긴한데 비용이 많이들꺼같아서요",
          "speaker_id": "1",
          "time": "20211109 00:22"
        },
        {
          "id": "MDRW2100046927.1.8",
          "form": "아 돈이 좀 들긴하겠죠 흑흑",
          "original_form": "아 돈이 좀 들긴하겠죠 흑흑",
          "speaker_id": "2",
          "time": "20211109 00:22"
        },
        {
          "id": "MDRW2100046927.1.9",
          "form": "저도 셀프페인트 도구 장바구니에만 담아놓고",
          "original_form": "저도 셀프페인트 도구 장바구니에만 담아놓고",
          "speaker_id": "2",
          "time": "20211109 00:22"
        },
        {
          "id": "MDRW2100046927.1.10",
          "form": "안산지 반년째에요",
          "original_form": "안산지 반년째에요",
          "speaker_id": "2",
          "time": "20211109 00:22"
        },
        {
          "id": "MDRW2100046927.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211109 00:22"
        },
        {
          "id": "MDRW2100046927.1.12",
          "form": "비용도 그렇고 월세라",
          "original_form": "비용도 그렇고 월세라",
          "speaker_id": "1",
          "time": "20211109 00:23"
        },
        {
          "id": "MDRW2100046927.1.13",
          "form": "내집이 아니라 함부러 못하겠어요",
          "original_form": "내집이 아니라 함부러 못하겠어요",
          "speaker_id": "1",
          "time": "20211109 00:23"
        },
        {
          "id": "MDRW2100046927.1.14",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20211109 00:23"
        },
        {
          "id": "MDRW2100046927.1.15",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "2",
          "time": "20211109 00:25"
        },
        {
          "id": "MDRW2100046927.1.16",
          "form": "그쵸 ㅋㅋㅋ 그거",
          "original_form": "그쵸 ㅋㅋㅋ 그거",
          "speaker_id": "2",
          "time": "20211109 00:25"
        },
        {
          "id": "MDRW2100046927.1.17",
          "form": "집주인 동의 얻어야된다고",
          "original_form": "집주인 동의 얻어야된다고",
          "speaker_id": "2",
          "time": "20211109 00:25"
        },
        {
          "id": "MDRW2100046927.1.18",
          "form": "하더라구요...허허",
          "original_form": "하더라구요...허허",
          "speaker_id": "2",
          "time": "20211109 00:25"
        },
        {
          "id": "MDRW2100046927.1.19",
          "form": "집주인이 원상복구하라고하면",
          "original_form": "집주인이 원상복구하라고하면",
          "speaker_id": "1",
          "time": "20211109 00:25"
        },
        {
          "id": "MDRW2100046927.1.20",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20211109 00:25"
        },
        {
          "id": "MDRW2100046927.1.21",
          "form": "무서워요",
          "original_form": "무서워요",
          "speaker_id": "1",
          "time": "20211109 00:26"
        },
        {
          "id": "MDRW2100046927.1.22",
          "form": "아 그쵸.....",
          "original_form": "아 그쵸.....",
          "speaker_id": "2",
          "time": "20211109 00:26"
        },
        {
          "id": "MDRW2100046927.1.23",
          "form": "서럽군",
          "original_form": "서럽군",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046927.1.24",
          "form": "근데 자기 집이어도",
          "original_form": "근데 자기 집이어도",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046927.1.25",
          "form": "참 귀찮더라고요",
          "original_form": "참 귀찮더라고요",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046927.1.26",
          "form": "ㅋㅋㅋㅋㅋㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046927.1.27",
          "form": "처음에 인테리어를 잘해놓을걸...",
          "original_form": "처음에 인테리어를 잘해놓을걸...",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046927.1.28",
          "form": "뭐 그런 생각만 들고",
          "original_form": "뭐 그런 생각만 들고",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046927.1.29",
          "form": "누워만 있게되는",
          "original_form": "누워만 있게되는",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046927.1.30",
          "form": "키키",
          "original_form": "키키",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046927.1.31",
          "form": "제 성격엔 인테리어해도",
          "original_form": "제 성격엔 인테리어해도",
          "speaker_id": "1",
          "time": "20211109 00:33"
        },
        {
          "id": "MDRW2100046927.1.32",
          "form": "몇일지나면",
          "original_form": "몇일지나면",
          "speaker_id": "1",
          "time": "20211109 00:33"
        },
        {
          "id": "MDRW2100046927.1.33",
          "form": "집 난장판될꺼같아요 ㅋㅋ",
          "original_form": "집 난장판될꺼같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211109 00:33"
        },
        {
          "id": "MDRW2100046927.1.34",
          "form": "ㅎㅎㅎㅎ 집은",
          "original_form": "ㅎㅎㅎㅎ 집은",
          "speaker_id": "2",
          "time": "20211109 00:35"
        },
        {
          "id": "MDRW2100046927.1.35",
          "form": "험하게 쓰는 맛이죠",
          "original_form": "험하게 쓰는 맛이죠",
          "speaker_id": "2",
          "time": "20211109 00:35"
        },
        {
          "id": "MDRW2100046927.1.36",
          "form": "크크\r\n",
          "original_form": "크크\r\n",
          "speaker_id": "2",
          "time": "20211109 00:35"
        }
      ]
    }
