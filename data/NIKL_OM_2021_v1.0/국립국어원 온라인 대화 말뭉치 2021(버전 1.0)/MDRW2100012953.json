      "utterance": [
        {
          "id": "MDRW2100012953.1.1",
          "form": "오늘은 쉬면서 방송 보시나요",
          "original_form": "오늘은 쉬면서 방송 보시나요",
          "speaker_id": "2",
          "time": "20210825 09:28"
        },
        {
          "id": "MDRW2100012953.1.2",
          "form": "아 시간이 별로없어서요",
          "original_form": "아 시간이 별로없어서요",
          "speaker_id": "1",
          "time": "20210825 09:29"
        },
        {
          "id": "MDRW2100012953.1.3",
          "form": "그래도 유튜브 실시간방송봐요",
          "original_form": "그래도 유튜브 실시간방송봐요",
          "speaker_id": "1",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012953.1.4",
          "form": "이따 일진해야해서",
          "original_form": "이따 일진해야해서",
          "speaker_id": "1",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012953.1.5",
          "form": "쭉 틀어놓게요",
          "original_form": "쭉 틀어놓게요",
          "speaker_id": "1",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012953.1.6",
          "form": "아직 못했어요 결정을 ㅡㅡ",
          "original_form": "아직 못했어요 결정을 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012953.1.7",
          "form": "아 어디\u0000로 하실지요?ㅋㅋ",
          "original_form": "아 어디\u0000로 하실지요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012953.1.8",
          "form": "이따 2시정도 경쟁률보고",
          "original_form": "이따 2시정도 경쟁률보고",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012953.1.9",
          "form": "정하세요",
          "original_form": "정하세요",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012953.1.10",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012953.1.11",
          "form": "근데 전 두개뿐이라",
          "original_form": "근데 전 두개뿐이라",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012953.1.12",
          "form": "네 근데 클러놔요 ㅋㅋ",
          "original_form": "네 근데 클러놔요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012953.1.13",
          "form": "틀어",
          "original_form": "틀어",
          "speaker_id": "1",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012953.1.14",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012953.1.15",
          "form": "전 지금 넘늦음 안들어갈까싶어서요",
          "original_form": "전 지금 넘늦음 안들어갈까싶어서요",
          "speaker_id": "1",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012953.1.16",
          "form": "방송보다 2시에 딱 할게요 ㅎㅎ",
          "original_form": "방송보다 2시에 딱 할게요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012953.1.17",
          "form": "정하신건 아니죠?",
          "original_form": "정하신건 아니죠?",
          "speaker_id": "1",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012953.1.18",
          "form": "공모주린이님이 실시간 방송해서 좋아요",
          "original_form": "공모주린이님이 실시간 방송해서 좋아요",
          "speaker_id": "1",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012953.1.19",
          "form": "어 저도 그거",
          "original_form": "어 저도 그거",
          "speaker_id": "2",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012953.1.20",
          "form": "엄마가 보내줘서",
          "original_form": "엄마가 보내줘서",
          "speaker_id": "2",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012953.1.21",
          "form": "아까 그분이 자기 팔았대ㅓ",
          "original_form": "아까 그분이 자기 팔았대ㅓ",
          "speaker_id": "2",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012953.1.22",
          "form": "서",
          "original_form": "서",
          "speaker_id": "2",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012953.1.23",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012953.1.24",
          "form": "바로 팔았는데ㅋㅋㅋ",
          "original_form": "바로 팔았는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012953.1.25",
          "form": "저도 그분꺼 애청자요",
          "original_form": "저도 그분꺼 애청자요",
          "speaker_id": "1",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012953.1.26",
          "form": "목소리도 좋으시고",
          "original_form": "목소리도 좋으시고",
          "speaker_id": "1",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012953.1.27",
          "form": "저는 삼성으로 할듯해요",
          "original_form": "저는 삼성으로 할듯해요",
          "speaker_id": "2",
          "time": "20210825 09:34"
        },
        {
          "id": "MDRW2100012953.1.28",
          "form": "보긴 볼껀데",
          "original_form": "보긴 볼껀데",
          "speaker_id": "2",
          "time": "20210825 09:34"
        },
        {
          "id": "MDRW2100012953.1.29",
          "form": "대신이랑 현대차가 없어서 ㅎㅎ",
          "original_form": "대신이랑 현대차가 없어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:34"
        },
        {
          "id": "MDRW2100012953.1.30",
          "form": "아 저도 살짝 기울어요 삼성",
          "original_form": "아 저도 살짝 기울어요 삼성",
          "speaker_id": "1",
          "time": "20210825 09:34"
        },
        {
          "id": "MDRW2100012953.1.31",
          "form": "아전 다있어서요",
          "original_form": "아전 다있어서요",
          "speaker_id": "1",
          "time": "20210825 09:34"
        },
        {
          "id": "MDRW2100012953.1.32",
          "form": "오히려 더 고민돼요",
          "original_form": "오히려 더 고민돼요",
          "speaker_id": "1",
          "time": "20210825 09:37"
        },
        {
          "id": "MDRW2100012953.1.33",
          "form": "10시부터 실시간이라",
          "original_form": "10시부터 실시간이라",
          "speaker_id": "1",
          "time": "20210825 09:37"
        },
        {
          "id": "MDRW2100012953.1.34",
          "form": "ibk 먼저 해야겠어요 ㅎㅎ",
          "original_form": "ibk 먼저 해야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:37"
        },
        {
          "id": "MDRW2100012953.1.35",
          "form": "바이젠셀 방송 끝나고 한대요",
          "original_form": "바이젠셀 방송 끝나고 한대요",
          "speaker_id": "1",
          "time": "20210825 09:38"
        },
        {
          "id": "MDRW2100012953.1.36",
          "form": "이분 방송 진짜 열심히 하세요 ㅎㅎ",
          "original_form": "이분 방송 진짜 열심히 하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:40"
        },
        {
          "id": "MDRW2100012953.1.37",
          "form": "오늘 잘보고 좋은 선택하세요 ㅎㅎ",
          "original_form": "오늘 잘보고 좋은 선택하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:51"
        },
        {
          "id": "MDRW2100012953.1.38",
          "form": "스팩 비례가 해볼만하지 않나요ㅋㅋ",
          "original_form": "스팩 비례가 해볼만하지 않나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:51"
        },
        {
          "id": "MDRW2100012953.1.39",
          "form": "조금씩 다 넣게요",
          "original_form": "조금씩 다 넣게요",
          "speaker_id": "1",
          "time": "20210825 09:51"
        },
        {
          "id": "MDRW2100012953.1.40",
          "form": "다 하실거죠?",
          "original_form": "다 하실거죠?",
          "speaker_id": "1",
          "time": "20210825 09:51"
        },
        {
          "id": "MDRW2100012953.1.41",
          "form": "ㅋㅋ 잘 먹어야죠",
          "original_form": "ㅋㅋ 잘 먹어야죠",
          "speaker_id": "2",
          "time": "20210825 09:51"
        },
        {
          "id": "MDRW2100012953.1.42",
          "form": "일단은좀 결쟁률 방송보고",
          "original_form": "일단은좀 결쟁률 방송보고",
          "speaker_id": "1",
          "time": "20210825 09:52"
        },
        {
          "id": "MDRW2100012953.1.43",
          "form": "스책먼저 해놓고",
          "original_form": "스책먼저 해놓고",
          "speaker_id": "1",
          "time": "20210825 09:52"
        },
        {
          "id": "MDRW2100012953.1.44",
          "form": "스펙",
          "original_form": "스펙",
          "speaker_id": "1",
