      "utterance": [
        {
          "id": "MDRW2100036685.1.1",
          "form": "우유가 없어서 아쉽 ㅠㅠ",
          "original_form": "우유가 없어서 아쉽 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100036685.1.2",
          "form": "앗 잼바른식빵은 무조건 우유인데ㅜ",
          "original_form": "앗 잼바른식빵은 무조건 우유인데ㅜ",
          "speaker_id": "1",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100036685.1.3",
          "form": "그럼 커피랑?",
          "original_form": "그럼 커피랑?",
          "speaker_id": "1",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100036685.1.4",
          "form": "비오고그러니까 따땃하게 바닐라라떼로 호호호",
          "original_form": "비오고그러니까 따땃하게 바닐라라떼로 호호호",
          "speaker_id": "2",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100036685.1.5",
          "form": "? 홈카페 차리셨나요",
          "original_form": "? 홈카페 차리셨나요",
          "speaker_id": "1",
          "time": "20210831 08:52"
        },
        {
          "id": "MDRW2100036685.1.6",
          "form": "아직 못먹었어요 ㅎㅎ 근데 전 봉지, 커피라서",
          "original_form": "아직 못먹었어요 ㅎㅎ 근데 전 봉지, 커피라서",
          "speaker_id": "2",
          "time": "20210831 08:54"
        },
        {
          "id": "MDRW2100036685.1.7",
          "form": "요샌 봉지커피가 잘 나오죠ㅎ",
          "original_form": "요샌 봉지커피가 잘 나오죠ㅎ",
          "speaker_id": "1",
          "time": "20210831 08:57"
        },
        {
          "id": "MDRW2100036685.1.8",
          "form": "맞아요 ㅎㅎ 이디야꺼 먹으려구요",
          "original_form": "맞아요 ㅎㅎ 이디야꺼 먹으려구요",
          "speaker_id": "2",
          "time": "20210831 08:59"
        },
        {
          "id": "MDRW2100036685.1.9",
          "form": "그러고보니 집에 할리스 바닐라딜라이트 봉지 있어요",
          "original_form": "그러고보니 집에 할리스 바닐라딜라이트 봉지 있어요",
          "speaker_id": "1",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100036685.1.10",
          "form": "제입엔 넘 달지만ㅜ",
          "original_form": "제입엔 넘 달지만ㅜ",
          "speaker_id": "1",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100036685.1.11",
          "form": "이디야는 덜 단가요?",
          "original_form": "이디야는 덜 단가요?",
          "speaker_id": "1",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100036685.1.12",
          "form": "비슷할것 같아요 ㅎㅎㅎ 단거 안좋아하세요??",
          "original_form": "비슷할것 같아요 ㅎㅎㅎ 단거 안좋아하세요??",
          "speaker_id": "2",
          "time": "20210831 09:01"
        },
        {
          "id": "MDRW2100036685.1.13",
          "form": "네ㅜ 단거는 쥐약이에요",
          "original_form": "네ㅜ 단거는 쥐약이에요",
          "speaker_id": "1",
          "time": "20210831 09:01"
        },
        {
          "id": "MDRW2100036685.1.14",
          "form": "아 흑당밀크티는 좋아해요!",
          "original_form": "아 흑당밀크티는 좋아해요!",
          "speaker_id": "1",
          "time": "20210831 09:01"
        },
        {
          "id": "MDRW2100036685.1.15",
          "form": "저도 아메 좋아하는데 가끔 단게 땡기고 이렇게 비올땐 그런게 땡기더라구요",
          "original_form": "저도 아메 좋아하는데 가끔 단게 땡기고 이렇게 비올땐 그런게 땡기더라구요",
          "speaker_id": "2",
          "time": "20210831 09:01"
        },
        {
          "id": "MDRW2100036685.1.16",
          "form": "ㅎㅎ 저도 이따 흑당밀크티 마셔야겠어요ㅎ",
          "original_form": "ㅎㅎ 저도 이따 흑당밀크티 마셔야겠어요ㅎ",
          "speaker_id": "1",
          "time": "20210831 09:02"
        },
        {
          "id": "MDRW2100036685.1.17",
          "form": "간만에 당 충전해야죠",
          "original_form": "간만에 당 충전해야죠",
          "speaker_id": "1",
          "time": "20210831 09:02"
        },
        {
          "id": "MDRW2100036685.1.18",
          "form": "어디꺼 드세요??",
          "original_form": "어디꺼 드세요??",
          "speaker_id": "2",
          "time": "20210831 09:04"
        },
        {
          "id": "MDRW2100036685.1.19",
          "form": "사실 좋아하는 데는 집근처 카페일리터라는 곳인데..",
          "original_form": "사실 좋아하는 데는 집근처 카페일리터라는 곳인데..",
          "speaker_id": "1",
          "time": "20210831 09:04"
        },
        {
          "id": "MDRW2100036685.1.20",
          "form": "회사 근처에는 없네요ㅜ",
          "original_form": "회사 근처에는 없네요ㅜ",
          "speaker_id": "1",
          "time": "20210831 09:04"
        },
        {
          "id": "MDRW2100036685.1.21",
          "form": "그냥 점심먹고 흑당밀크티 파는곳 아무데나 가려구요ㅜ",
          "original_form": "그냥 점심먹고 흑당밀크티 파는곳 아무데나 가려구요ㅜ",
          "speaker_id": "1",
          "time": "20210831 09:05"
        }
      ]
    }
  ]
