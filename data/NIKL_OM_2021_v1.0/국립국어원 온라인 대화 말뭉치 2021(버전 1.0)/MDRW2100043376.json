      "utterance": [
        {
          "id": "MDRW2100043376.1.1",
          "form": "야끼짬뽕",
          "original_form": "야끼짬뽕",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043376.1.2",
          "form": "볶음밥은요?",
          "original_form": "볶음밥은요?",
          "speaker_id": "1",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100043376.1.3",
          "form": "새우볶음밥에",
          "original_form": "새우볶음밥에",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043376.1.4",
          "form": "짜장소스요",
          "original_form": "짜장소스요",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043376.1.5",
          "form": "계란후라이는요",
          "original_form": "계란후라이는요",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043376.1.6",
          "form": "아 따로돈내야해요?",
          "original_form": "아 따로돈내야해요?",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043376.1.7",
          "form": "네 근데 기름에튀겨서 존맛",
          "original_form": "네 근데 기름에튀겨서 존맛",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043376.1.8",
          "form": "천원이하면 추가요",
          "original_form": "천원이하면 추가요",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043376.1.9",
          "form": "콜라 서비스 주시나요?",
          "original_form": "콜라 서비스 주시나요?",
          "speaker_id": "2",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100043376.1.10",
          "form": "그런건 안줘요",
          "original_form": "그런건 안줘요",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043376.1.11",
          "form": "그럼 사이다 주나요?",
          "original_form": "그럼 사이다 주나요?",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043376.1.12",
          "form": "서비스 기대 금물",
          "original_form": "서비스 기대 금물",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043376.1.13",
          "form": "장사잘되는데라",
          "original_form": "장사잘되는데라",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043376.1.14",
          "form": "ㅠ ㅠ 그럼 배달비 빼주세요",
          "original_form": "ㅠ ㅠ 그럼 배달비 빼주세요",
          "speaker_id": "2",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043376.1.15",
          "form": "하이고 요즘 배달비 울매나비싼데",
          "original_form": "하이고 요즘 배달비 울매나비싼데",
          "speaker_id": "1",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043376.1.16",
          "form": "마넌이상 무배요",
          "original_form": "마넌이상 무배요",
          "speaker_id": "1",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043376.1.17",
          "form": "저 지금 야끼짬뽕에 새우볶음밥에 계란추가했는데",
          "original_form": "저 지금 야끼짬뽕에 새우볶음밥에 계란추가했는데",
          "speaker_id": "2",
          "time": "20210907 19:02"
        }
