      "utterance": [
        {
          "id": "MDRW2100031083.1.1",
          "form": "주제 한번 바꿔서",
          "original_form": "주제 한번 바꿔서",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100031083.1.2",
          "form": "name3 잘 있니?",
          "original_form": "&name3& 잘 있니?",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100031083.1.3",
          "form": "고럼요",
          "original_form": "고럼요",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031083.1.4",
          "form": "잘있지요 ㅋㅋㅋ",
          "original_form": "잘있지요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031083.1.5",
          "form": "어제밤에도 나랑 같이 잤는걸",
          "original_form": "어제밤에도 나랑 같이 잤는걸",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031083.1.6",
          "form": "점점 거대해지고 있는 고양인데",
          "original_form": "점점 거대해지고 있는 고양인데",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031083.1.7",
          "form": "안더워?",
          "original_form": "안더워?",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031083.1.8",
          "form": "엄청 열 뿜뿜하잖아",
          "original_form": "엄청 열 뿜뿜하잖아",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031083.1.9",
          "form": "name3도 나랑 안붙어서 괜챦아",
          "original_form": "&name3&도 나랑 안붙어서 괜챦아",
          "speaker_id": "2",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100031083.1.10",
          "form": "같은 침댄데 내외해",
          "original_form": "같은 침댄데 내외해",
          "speaker_id": "2",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100031083.1.11",
          "form": "내외하냐고 ㅋㅋㅋㅋ",
          "original_form": "내외하냐고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100031083.1.12",
          "form": "겨울엔 이제 안고 사는건가",
          "original_form": "겨울엔 이제 안고 사는건가",
          "speaker_id": "1",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100031083.1.13",
          "form": "아니.. 원체 안겨있는거 싫어해서",
          "original_form": "아니.. 원체 안겨있는거 싫어해서",
          "speaker_id": "2",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031083.1.14",
          "form": "겨울이라고 다르지않음...",
          "original_form": "겨울이라고 다르지않음...",
          "speaker_id": "2",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031083.1.15",
          "form": "역시 와기호랑이",
          "original_form": "역시 와기호랑이",
          "speaker_id": "1",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100031083.1.16",
          "form": "밖에 사는 친구는 자주 놀러옴?",
          "original_form": "밖에 사는 친구는 자주 놀러옴?",
          "speaker_id": "1",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100031083.1.17",
          "form": "거의 매일 길냥이들 집 주변 들락거리는듯?",
          "original_form": "거의 매일 길냥이들 집 주변 들락거리는듯?",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031083.1.18",
          "form": "많으면 하루에도 몇번씩 보고",
          "original_form": "많으면 하루에도 몇번씩 보고",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031083.1.19",
          "form": "좀 뜸하다 싶을때도 2~3일 이상 안보이진 않는듯ㅋㅋㅋ",
          "original_form": "좀 뜸하다 싶을때도 2~3일 이상 안보이진 않는듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031083.1.20",
          "form": "개... 는없었나?",
          "original_form": "개... 는없었나?",
          "speaker_id": "1",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031083.1.21",
          "form": "그 뭣이냐 name3 친구 있잖아",
          "original_form": "그 뭣이냐 &name3& 친구 있잖아",
          "speaker_id": "1",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100031083.1.22",
          "form": "전에 보여줬던",
          "original_form": "전에 보여줬던",
          "speaker_id": "1",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100031083.1.23",
          "form": "그친구는 자주안옴?",
          "original_form": "그친구는 자주안옴?",
          "speaker_id": "1",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100031083.1.24",
          "form": "키우는 개는 있는데 떠돌이개는 동네에 없어",
          "original_form": "키우는 개는 있는데 떠돌이개는 동네에 없어",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031083.1.25",
          "form": "이 동네는 떠돌이개가 출몰했다 하면 어느 집에선가 탈출한 것일 확률이 큼..",
          "original_form": "이 동네는 떠돌이개가 출몰했다 하면 어느 집에선가 탈출한 것일 확률이 큼..",
          "speaker_id": "2",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100031083.1.26",
          "form": "ㅇㅇ 키우는 친구",
          "original_form": "ㅇㅇ 키우는 친구",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100031083.1.27",
          "form": "아닛? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닛? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100031083.1.28",
          "form": "그럼 데리고 집으로 가줘",
          "original_form": "그럼 데리고 집으로 가줘",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031083.1.29",
          "form": "전에 보여줬던 냥이가",
          "original_form": "전에 보여줬던 냥이가",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031083.1.30",
          "form": "어떤 냥이었을깤ㅋㅋㅋ",
          "original_form": "어떤 냥이었을깤ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031083.1.31",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031083.1.32",
          "form": "어느집인지 나는 몰라욬ㅋㅋㅋㅋ",
          "original_form": "어느집인지 나는 몰라욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031083.1.33",
          "form": "name3가 애달프게 보던",
          "original_form": "&name3&가 애달프게 보던",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031083.1.34",
          "form": "집에 키우는 개 이름은?",
          "original_form": "집에 키우는 개 이름은?",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031083.1.35",
          "form": "name4!",
          "original_form": "&name4&!",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031083.1.36",
          "form": "얘도 많이 컸지..",
          "original_form": "얘도 많이 컸지..",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031083.1.37",
          "form": "아닛 그 맛있을 것 같은 이름은?!",
          "original_form": "아닛 그 맛있을 것 같은 이름은?!",
          "speaker_id": "1",
