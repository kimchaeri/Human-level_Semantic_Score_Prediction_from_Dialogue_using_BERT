      "utterance": [
        {
          "id": "MDRW2100045523.1.1",
          "form": "맞아...나 그것도 들었어",
          "original_form": "맞아...나 그것도 들었어",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045523.1.2",
