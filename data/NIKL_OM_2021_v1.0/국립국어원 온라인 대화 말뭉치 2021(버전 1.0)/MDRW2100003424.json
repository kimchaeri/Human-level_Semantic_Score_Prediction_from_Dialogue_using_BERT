      "utterance": [
        {
          "id": "MDRW2100003424.1.1",
          "form": "고양이 키우고 싶다",
          "original_form": "고양이 키우고 싶다",
          "speaker_id": "1",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003424.1.2",
          "form": "애들 돌보기도 힘들텐데 고양이는 ㅋ",
          "original_form": "애들 돌보기도 힘들텐데 고양이는 ㅋ",
          "speaker_id": "2",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003424.1.3",
          "form": "그렇긴 한데 요즘 길고양이들 보니 너무 귀여워",
          "original_form": "그렇긴 한데 요즘 길고양이들 보니 너무 귀여워",
          "speaker_id": "1",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003424.1.4",
          "form": "ㅎㅎ 아마 며칠 키우다 보면 금방 후회할걸",
          "original_form": "ㅎㅎ 아마 며칠 키우다 보면 금방 후회할걸",
          "speaker_id": "2",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003424.1.5",
          "form": "요즘 유튜브에 다홍이 있잖아~ 검은 고양이",
          "original_form": "요즘 유튜브에 다홍이 있잖아~ 검은 고양이",
          "speaker_id": "1",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003424.1.6",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003424.1.7",
          "form": "애교도 많고 너무 귀엽더라 ㅎㅎ",
          "original_form": "애교도 많고 너무 귀엽더라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003424.1.8",
          "form": "색시 비염 있어서 털 날리고 안돼",
          "original_form": "색시 비염 있어서 털 날리고 안돼",
          "speaker_id": "2",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003424.1.9",
          "form": "알러지도 있고",
          "original_form": "알러지도 있고",
          "speaker_id": "2",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003424.1.10",
          "form": "그렇겠지 ㅠㅠ 안그래도 공기청정기 달고 사는데",
          "original_form": "그렇겠지 ㅠㅠ 안그래도 공기청정기 달고 사는데",
          "speaker_id": "1",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003424.1.11",
          "form": "ㅇㅇ 힘들거야",
          "original_form": "ㅇㅇ 힘들거야",
          "speaker_id": "2",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003424.1.12",
          "form": "그냥 길고양이 보는 걸로 만족해야겠다",
          "original_form": "그냥 길고양이 보는 걸로 만족해야겠다",
          "speaker_id": "1",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003424.1.13",
          "form": "그나마 고양이는 산책은 안시켜도 된다고 하더만",
          "original_form": "그나마 고양이는 산책은 안시켜도 된다고 하더만",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003424.1.14",
          "form": "ㅎㅎ 강아지처럼 매일 산책은 힘들지",
          "original_form": "ㅎㅎ 강아지처럼 매일 산책은 힘들지",
          "speaker_id": "1",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003424.1.15",
          "form": "아파트 산책로 지나가다보면 매일 고양이 밥 주는 사람 있더만",
          "original_form": "아파트 산책로 지나가다보면 매일 고양이 밥 주는 사람 있더만",
          "speaker_id": "1",
          "time": "20210820 23:00"
        }
      ]
