      "utterance": [
        {
          "id": "MDRW2100003587.1.1",
          "form": "최근에 여행 다녀오신 적 있나요?",
          "original_form": "최근에 여행 다녀오신 적 있나요?",
          "speaker_id": "2",
          "time": "20210821 02:53"
        },
        {
          "id": "MDRW2100003587.1.2",
          "form": "코로나라 작년부터 아무데도 못갔네요ㅠㅠ 아이보리님은요?",
          "original_form": "코로나라 작년부터 아무데도 못갔네요ㅠㅠ 아이보리님은요?",
          "speaker_id": "1",
          "time": "20210821 02:53"
        },
        {
          "id": "MDRW2100003587.1.3",
          "form": "저는 전주여행 다녀왔어요",
          "original_form": "저는 전주여행 다녀왔어요",
          "speaker_id": "2",
          "time": "20210821 02:54"
        },
        {
          "id": "MDRW2100003587.1.4",
          "form": "주로 호텔이랑 한옥마을만 돌아다녔어요",
          "original_form": "주로 호텔이랑 한옥마을만 돌아다녔어요",
          "speaker_id": "2",
          "time": "20210821 02:54"
        },
        {
          "id": "MDRW2100003587.1.5",
          "form": "여름휴가로 다녀오신거예요?",
          "original_form": "여름휴가로 다녀오신거예요?",
          "speaker_id": "1",
          "time": "20210821 02:55"
        },
        {
          "id": "MDRW2100003587.1.6",
          "form": "한옥마을 가보고싶은데 어때요?",
          "original_form": "한옥마을 가보고싶은데 어때요?",
          "speaker_id": "1",
          "time": "20210821 02:55"
        },
        {
          "id": "MDRW2100003587.1.7",
          "form": "네 휴가였어요",
          "original_form": "네 휴가였어요",
          "speaker_id": "2",
          "time": "20210821 02:56"
        },
        {
          "id": "MDRW2100003587.1.8",
          "form": "한옥마을에서 한복 대여해서 입고 돌아다니니까 좋더라구요",
          "original_form": "한옥마을에서 한복 대여해서 입고 돌아다니니까 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 02:57"
        },
        {
          "id": "MDRW2100003587.1.9",
          "form": "기회되면 꼭 가보세요 좋아요",
          "original_form": "기회되면 꼭 가보세요 좋아요",
          "speaker_id": "2",
          "time": "20210821 02:57"
        },
        {
          "id": "MDRW2100003587.1.10",
          "form": "안덥던가요? 여름이라 더울거같은데",
          "original_form": "안덥던가요? 여름이라 더울거같은데",
          "speaker_id": "1",
          "time": "20210821 02:57"
        },
        {
          "id": "MDRW2100003587.1.11",
          "form": "숙소도 한옥으로 가보고싶던데 언제쯤 갈수있을지 모르겠어요",
          "original_form": "숙소도 한옥으로 가보고싶던데 언제쯤 갈수있을지 모르겠어요",
          "speaker_id": "1",
          "time": "20210821 02:58"
        },
        {
          "id": "MDRW2100003587.1.12",
          "form": "다행히 그날 날씨가 선선해서 괜찮았어요.",
          "original_form": "다행히 그날 날씨가 선선해서 괜찮았어요.",
          "speaker_id": "2",
          "time": "20210821 02:59"
        },
        {
          "id": "MDRW2100003587.1.13",
          "form": "코로나 잠잠해지면 가봐야겠네요",
          "original_form": "코로나 잠잠해지면 가봐야겠네요",
          "speaker_id": "1",
          "time": "20210821 03:00"
        },
        {
          "id": "MDRW2100003587.1.14",
          "form": "전 숙소는 호텔 썼는데 한옥도 괜찮을 것 같아요 요즘은 현대화가 잘 되어있어서.",
          "original_form": "전 숙소는 호텔 썼는데 한옥도 괜찮을 것 같아요 요즘은 현대화가 잘 되어있어서.",
          "speaker_id": "2",
          "time": "20210821 03:00"
        },
        {
          "id": "MDRW2100003587.1.15",
          "form": "해외 여행 간다면 어느 나라 가보고 싶으세요?",
          "original_form": "해외 여행 간다면 어느 나라 가보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210821 03:00"
        },
        {
          "id": "MDRW2100003587.1.16",
          "form": "그렇더라구요 티비서 봤는데 좋던데요",
          "original_form": "그렇더라구요 티비서 봤는데 좋던데요",
          "speaker_id": "1",
          "time": "20210821 03:00"
        },
        {
          "id": "MDRW2100003587.1.17",
          "form": "저 캐나다요 유럽도 가보고싶어요",
          "original_form": "저 캐나다요 유럽도 가보고싶어요",
          "speaker_id": "1",
          "time": "20210821 03:01"
        },
        {
          "id": "MDRW2100003587.1.18",
          "form": "다녀오신곳 있으신가요?",
          "original_form": "다녀오신곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 03:01"
        },
        {
          "id": "MDRW2100003587.1.19",
          "form": "저 친구가 상하이에서 대학을 다녀서 몇번 가봤어요",
          "original_form": "저 친구가 상하이에서 대학을 다녀서 몇번 가봤어요",
          "speaker_id": "2",
          "time": "20210821 03:03"
        },
        {
          "id": "MDRW2100003587.1.20",
          "form": "저는 스페인 가보고 싶어요",
          "original_form": "저는 스페인 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210821 03:03"
        },
        {
          "id": "MDRW2100003587.1.21",
          "form": "캐나다도 좋네요",
          "original_form": "캐나다도 좋네요",
          "speaker_id": "2",
          "time": "20210821 03:03"
        },
        {
          "id": "MDRW2100003587.1.22",
          "form": "8월쯤가면 진짜 이쁜거같더라구요  로키산맥이랑",
          "original_form": "8월쯤가면 진짜 이쁜거같더라구요  로키산맥이랑",
          "speaker_id": "1",
          "time": "20210821 03:04"
        },
        {
          "id": "MDRW2100003587.1.23",
          "form": "여행에서 얻은 추억이 힘들 때 도움이 많이 되는 것 같아요",
          "original_form": "여행에서 얻은 추억이 힘들 때 도움이 많이 되는 것 같아요",
          "speaker_id": "2",
          "time": "20210821 03:05"
        },
        {
          "id": "MDRW2100003587.1.24",
          "form": "8월에 한국은 날씨도 더우니까 해외로 나가기 딱 좋네요",
          "original_form": "8월에 한국은 날씨도 더우니까 해외로 나가기 딱 좋네요",
          "speaker_id": "2",
          "time": "20210821 03:06"
        },
        {
          "id": "MDRW2100003587.1.25",
          "form": "힐링도 되고 사진보면 또생각나고 좋은것같아요",
          "original_form": "힐링도 되고 사진보면 또생각나고 좋은것같아요",
          "speaker_id": "1",
          "time": "20210821 03:06"
        }
      ]
