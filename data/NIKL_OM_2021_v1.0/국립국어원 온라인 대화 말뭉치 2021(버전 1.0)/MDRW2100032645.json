      "utterance": [
        {
          "id": "MDRW2100032645.1.1",
          "form": "재택근무 일자리 좋을거같지않아?",
          "original_form": "재택근무 일자리 좋을거같지않아?",
          "speaker_id": "1",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032645.1.2",
          "form": "요즘 재택근무 어떤데",
          "original_form": "요즘 재택근무 어떤데",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032645.1.3",
          "form": "니 메이트? 잘 하구 있나",
          "original_form": "니 메이트? 잘 하구 있나",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032645.1.4",
          "form": "재택은 출퇴근 안하니까 좋긴한데",
          "original_form": "재택은 출퇴근 안하니까 좋긴한데",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032645.1.5",
          "form": "ㅋㅋㅋㅋ내일 파일 받아봐야 제대로 알거같애",
          "original_form": "ㅋㅋㅋㅋ내일 파일 받아봐야 제대로 알거같애",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032645.1.6",
          "form": "전화로만 설명하고 보고 하고 힘들기도 함...",
          "original_form": "전화로만 설명하고 보고 하고 힘들기도 함...",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032645.1.7",
          "form": "ㅋㅋㅋㅋㅋ아 월욜마다?",
          "original_form": "ㅋㅋㅋㅋㅋ아 월욜마다?",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032645.1.8",
          "form": "음 그렇긴하겠다",
          "original_form": "음 그렇긴하겠다",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032645.1.9",
          "form": "ㅇㅇ 이번달은 직접 안만나고 찐 재택느낌으로 일하는데",
          "original_form": "ㅇㅇ 이번달은 직접 안만나고 찐 재택느낌으로 일하는데",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032645.1.10",
          "form": "금욜이나 월욜에 파일 주고받거든",
          "original_form": "금욜이나 월욜에 파일 주고받거든",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032645.1.11",
          "form": "잊어버렸을까봐 걱정됨 ㅋㅋㅋㅋㅋㅋㅎㅎ...",
          "original_form": "잊어버렸을까봐 걱정됨 ㅋㅋㅋㅋㅋㅋㅎㅎ...",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032645.1.12",
          "form": "아 ㅋㅋㅋㅋ그정도가 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ그정도가 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032645.1.13",
          "form": "그래도 자기 업무인데....",
          "original_form": "그래도 자기 업무인데....",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032645.1.14",
          "form": "이렇게 신뢰가없다",
          "original_form": "이렇게 신뢰가없다",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032645.1.15",
          "form": "그정도는 기억하지 않을가",
          "original_form": "그정도는 기억하지 않을가",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032645.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032645.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ근데 코로나때문에 재택이 전반적으로 늘억자나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ근데 코로나때문에 재택이 전반적으로 늘억자나",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032645.1.18",
          "form": "주에 한두번은 좋을거같다아니야?",
          "original_form": "주에 한두번은 좋을거같다아니야?",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032645.1.19",
          "form": "ㅋㅋㅋㅋ맞음 주1회 하니까",
          "original_form": "ㅋㅋㅋㅋ맞음 주1회 하니까",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032645.1.20",
          "form": "좀 쉬어가는 느낌이고 좋긴함ㅋㅋㅋ",
          "original_form": "좀 쉬어가는 느낌이고 좋긴함ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032645.1.21",
          "form": "맞아 의무로했으면 좋겟다ㅋㅋㅋㅋㅋ",
          "original_form": "맞아 의무로했으면 좋겟다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032645.1.22",
          "form": "전날에 일 몰아서 하고 좀 쉬엄싀엄~",
          "original_form": "전날에 일 몰아서 하고 좀 쉬엄싀엄~",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032645.1.23",
          "form": "능률에 더 좋을거은데..",
          "original_form": "능률에 더 좋을거은데..",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032645.1.24",
          "form": "빨래도 돌리고~",
          "original_form": "빨래도 돌리고~",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032645.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ집에서 쉬엄쉬엄 일하고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ집에서 쉬엄쉬엄 일하고",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032645.1.26",
          "form": "ㅋㅋㅋㅋ확실히 수나 목에 재택하면 금요일에 덜 피곤해",
          "original_form": "ㅋㅋㅋㅋ확실히 수나 목에 재택하면 금요일에 덜 피곤해",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032645.1.27",
          "form": "풀 재택근무하는 일자리는 어떤델까",
          "original_form": "풀 재택근무하는 일자리는 어떤델까",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032645.1.28",
          "form": "ㅋㅋㅋ흠....디자인?...",
          "original_form": "ㅋㅋㅋ흠....디자인?...",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032645.1.29",
          "form": "편집? 이런게 보통 풀재택이지 않을가",
          "original_form": "편집? 이런게 보통 풀재택이지 않을가",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032645.1.30",
          "form": "그렇긴하겟다 멐가 딱 결과물만내면대서그렁가",
          "original_form": "그렇긴하겟다 멐가 딱 결과물만내면대서그렁가",
          "speaker_id": "1",
          "time": "20210905 21:55"
        }
      ]
    }
