      "utterance": [
        {
          "id": "MDRW2100014312.1.1",
          "form": "지금 대전인데 여긴 하루종일 비가와요",
          "original_form": "지금 대전인데 여긴 하루종일 비가와요",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014312.1.2",
          "form": "저도 대전인데 비가오네요",
          "original_form": "저도 대전인데 비가오네요",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014312.1.3",
          "form": "그래도 비가오면 시원해서 좋아요",
          "original_form": "그래도 비가오면 시원해서 좋아요",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014312.1.4",
          "form": "맞아요 더운 것 보다는 시원한 게 좋죠",
          "original_form": "맞아요 더운 것 보다는 시원한 게 좋죠",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014312.1.5",
          "form": "이렇게  비오는 날은 빗소리 들으면서 영화보고싶어요",
          "original_form": "이렇게  비오는 날은 빗소리 들으면서 영화보고싶어요",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014312.1.6",
          "form": "저는 비오는 날은 부침개가 먹고싶어요",
          "original_form": "저는 비오는 날은 부침개가 먹고싶어요",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014312.1.7",
          "form": "부침개좋죠",
          "original_form": "부침개좋죠",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014312.1.8",
          "form": "근데 비오면 빨래는 돌리기가 좀 그래요",
          "original_form": "근데 비오면 빨래는 돌리기가 좀 그래요",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014312.1.9",
          "form": "그쵸 눅눅해져서요",
          "original_form": "그쵸 눅눅해져서요",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014312.1.10",
          "form": "그래도 여름이 곧 끝날 것 같아요",
          "original_form": "그래도 여름이 곧 끝날 것 같아요",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014312.1.11",
          "form": "맞아요 겨울이 빨리 오면 좋겠어요",
          "original_form": "맞아요 겨울이 빨리 오면 좋겠어요",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014312.1.12",
          "form": "겨울좋죠 겨울에는 붕어빵!",
          "original_form": "겨울좋죠 겨울에는 붕어빵!",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014312.1.13",
          "form": "전 겨울엔 우동이요",
          "original_form": "전 겨울엔 우동이요",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014312.1.14",
          "form": "이제 날이 선선하니 아주 시원해요",
          "original_form": "이제 날이 선선하니 아주 시원해요",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014312.1.15",
          "form": "맞아요 일교차 좀 심한 것 같아요",
          "original_form": "맞아요 일교차 좀 심한 것 같아요",
          "speaker_id": "1",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014312.1.16",
          "form": "전 그래서 얇은 긴 티 입고다녀요",
          "original_form": "전 그래서 얇은 긴 티 입고다녀요",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014312.1.17",
          "form": "저도 얇은 가디건 가지고다녀야겠어요",
          "original_form": "저도 얇은 가디건 가지고다녀야겠어요",
