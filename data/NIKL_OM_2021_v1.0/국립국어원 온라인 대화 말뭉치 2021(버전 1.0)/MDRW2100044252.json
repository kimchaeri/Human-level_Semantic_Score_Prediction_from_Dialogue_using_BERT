      "utterance": [
        {
          "id": "MDRW2100044252.1.1",
          "form": "키링요?",
          "original_form": "키링요?",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044252.1.2",
          "form": "네 아크릴 키링같은거요",
          "original_form": "네 아크릴 키링같은거요",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044252.1.3",
          "form": "열쇠고리로 쓰시나요?",
          "original_form": "열쇠고리로 쓰시나요?",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044252.1.4",
          "form": "그런건데 거의 덕질용이에요 ㅋㅋㅋ",
          "original_form": "그런건데 거의 덕질용이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044252.1.5",
          "form": "덕질용이 뭐죠?",
          "original_form": "덕질용이 뭐죠?",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044252.1.6",
          "form": "음 영화나 애니메이션에 나온 물건을 따라서 만드는거에요 !",
          "original_form": "음 영화나 애니메이션에 나온 물건을 따라서 만드는거에요 !",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044252.1.7",
          "form": "아하..왜요?",
          "original_form": "아하..왜요?",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044252.1.8",
          "form": "그냥 좋아서요 ㅋㅋㅋ",
          "original_form": "그냥 좋아서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044252.1.9",
          "form": "신기하네요!",
          "original_form": "신기하네요!",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044252.1.10",
          "form": "새로운 취미인데 너무예쁘고 실용성도 있어서 좋아요",
          "original_form": "새로운 취미인데 너무예쁘고 실용성도 있어서 좋아요",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044252.1.11",
          "form": "열쇠고리처럼 쓰면 그렇겠어요!",
          "original_form": "열쇠고리처럼 쓰면 그렇겠어요!",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044252.1.12",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044252.1.13",
          "form": "아크릴이면 투명한건가요?",
          "original_form": "아크릴이면 투명한건가요?",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044252.1.14",
          "form": "네 투명한데 인쇄하는거에요 원하는 이미지를요",
          "original_form": "네 투명한데 인쇄하는거에요 원하는 이미지를요",
          "speaker_id": "1",
          "time": "20210907 22:06"
