      "utterance": [
        {
          "id": "MDRW2100017812.1.1",
          "form": "이삭토스트 짱맛",
          "original_form": "이삭토스트 짱맛",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017812.1.2",
          "form": "점심 뭐먹을예정이예요?",
          "original_form": "점심 뭐먹을예정이예요?",
          "speaker_id": "1",
          "time": "20210827 10:53"
        },
        {
          "id": "MDRW2100017812.1.3",
          "form": "점심 짬뽕밥 머글거임",
          "original_form": "점심 짬뽕밥 머글거임",
          "speaker_id": "2",
          "time": "20210827 11:02"
        },
        {
          "id": "MDRW2100017812.1.4",
          "form": "짱맛있겠다 짬뽕밥",
          "original_form": "짱맛있겠다 짬뽕밥",
          "speaker_id": "1",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100017812.1.5",
          "form": "저녁 먹었어?",
          "original_form": "저녁 먹었어?",
          "speaker_id": "2",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100017812.1.6",
          "form": "웅!!!떡볶이 먹었오ㅎㅎ",
          "original_form": "웅!!!떡볶이 먹었오ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100017812.1.7",
          "form": "뭐야 징그럽게",
          "original_form": "뭐야 징그럽게",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100017812.1.8",
          "form": "사나이답게 다시 대답해봐",
          "original_form": "사나이답게 다시 대답해봐",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100017812.1.9",
          "form": "거 입맛 사로잡는 떡볶이 먹었수다",
          "original_form": "거 입맛 사로잡는 떡볶이 먹었수다",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100017812.1.10",
          "form": "껄껄껄",
          "original_form": "껄껄껄",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100017812.1.11",
          "form": "거 주모가 뜨끈하게 말아줍디까?",
          "original_form": "거 주모가 뜨끈하게 말아줍디까?",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100017812.1.12",
          "form": "매운맛 화끈헌게 속이 뜨뜻해집디다",
          "original_form": "매운맛 화끈헌게 속이 뜨뜻해집디다",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100017812.1.13",
          "form": "거 어느 주모가 해준 뻘건 떡무침이요?",
          "original_form": "거 어느 주모가 해준 뻘건 떡무침이요?",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100017812.1.14",
          "form": "거대궁전 떡무침이 국물 낭낭허이 푸짐하드만",
          "original_form": "거대궁전 떡무침이 국물 낭낭허이 푸짐하드만",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100017812.1.15",
          "form": "어디 떡무침 좋아허시오?",
          "original_form": "어디 떡무침 좋아허시오?",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100017812.1.16",
          "form": "허허 구급방 떡무침이 나의 사냥터 안에 있는지라",
          "original_form": "허허 구급방 떡무침이 나의 사냥터 안에 있는지라",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100017812.1.17",
          "form": "마음이 동할때마다 사냥해온다네",
          "original_form": "마음이 동할때마다 사냥해온다네",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100017812.1.18",
          "form": "거 참 열정 넘치는 사내일세",
          "original_form": "거 참 열정 넘치는 사내일세",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100017812.1.19",
          "form": "요즘 어떤 민족이 직접 가져다 내 곳간까지 들어다 주는데 사냥까지 나가다니",
          "original_form": "요즘 어떤 민족이 직접 가져다 내 곳간까지 들어다 주는데 사냥까지 나가다니",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100017812.1.20",
          "form": "진정한 사내 대장부라면 무릇 나서서 사냥하는것이 도리 아니겠나",
          "original_form": "진정한 사내 대장부라면 무릇 나서서 사냥하는것이 도리 아니겠나",
          "speaker_id": "2",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100017812.1.21",
          "form": "거 시종을 부리면 일삯을 줘야하는데 내 못내 아쉬워서말이네",
          "original_form": "거 시종을 부리면 일삯을 줘야하는데 내 못내 아쉬워서말이네",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100017812.1.22",
          "form": "ㅋㅋㄱㅋㄱㄱㅋㄱㅋㄱㄱㅋ",
          "original_form": "ㅋㅋㄱㅋㄱㄱㅋㄱㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100017812.1.23",
          "form": "이집 잘하네",
          "original_form": "이집 잘하네",
          "speaker_id": "1",
