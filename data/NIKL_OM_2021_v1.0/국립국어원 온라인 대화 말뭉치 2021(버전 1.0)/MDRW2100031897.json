        },
        {
          "id": "MDRW2100031897.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210905 08:17"
        },
        {
          "id": "MDRW2100031897.1.3",
          "form": "육아중이신가요",
          "original_form": "육아중이신가요",
          "speaker_id": "1",
          "time": "20210905 08:17"
        },
        {
          "id": "MDRW2100031897.1.4",
          "form": "아니요~ 육아중이신가요?",
          "original_form": "아니요~ 육아중이신가요?",
          "speaker_id": "2",
          "time": "20210905 08:21"
        },
        {
          "id": "MDRW2100031897.1.5",
          "form": "아니요 아직이요 ㅎㅎ",
          "original_form": "아니요 아직이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:27"
        },
        {
          "id": "MDRW2100031897.1.6",
          "form": "가족 관계가 어떻게 되세요?",
          "original_form": "가족 관계가 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210905 08:27"
        },
        {
          "id": "MDRW2100031897.1.7",
          "form": "부모님과 동생2명이 있어요~ 가족관계가 어떻게 되시나요?",
          "original_form": "부모님과 동생2명이 있어요~ 가족관계가 어떻게 되시나요?",
          "speaker_id": "2",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100031897.1.8",
          "form": "저는 동생 하나인데",
          "original_form": "저는 동생 하나인데",
          "speaker_id": "1",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031897.1.9",
          "form": "결혼해서 독립했어요 ㅎㅎ",
          "original_form": "결혼해서 독립했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031897.1.10",
          "form": "오 이제 조금은 여유로워 지셨겠네요..",
          "original_form": "오 이제 조금은 여유로워 지셨겠네요..",
          "speaker_id": "2",
          "time": "20210905 11:27"
        },
        {
          "id": "MDRW2100031897.1.11",
          "form": "청소를 좋아하시나요?",
          "original_form": "청소를 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 11:28"
        },
        {
          "id": "MDRW2100031897.1.12",
          "form": "살기위해 해야죠 ㅎㅎ",
          "original_form": "살기위해 해야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100031897.1.13",
          "form": "그렇긴한뎈ㅋㅋ 전 집안일 중에 청소를 귀찮아해요..",
          "original_form": "그렇긴한뎈ㅋㅋ 전 집안일 중에 청소를 귀찮아해요..",
          "speaker_id": "2",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100031897.1.14",
          "form": ".",
          "original_form": ".",
          "speaker_id": "2",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100031897.1.15",
          "form": "차라리 설겆이느는 하겠는데ㅋㅋㅋㅋ",
          "original_form": "차라리 설겆이느는 하겠는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100031897.1.16",
          "form": "식기세척기가 있죠ㅎㅎ",
          "original_form": "식기세척기가 있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100031897.1.17",
          "form": "와 부러워요 ㅋㅋㅋ",
          "original_form": "와 부러워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100031897.1.18",
          "form": "식세기 사고싶은데 자리가 정말 없어요ㅋㅋㅋㅋ",
          "original_form": "식세기 사고싶은데 자리가 정말 없어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100031897.1.19",
          "form": "저희도 열심히 테트리스해서 ㅎㅎ",
          "original_form": "저희도 열심히 테트리스해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100031897.1.20",
          "form": "부모님 집이라 엄마의 허락도 필요한까요ㅋㅋㅋㅋ",
          "original_form": "부모님 집이라 엄마의 허락도 필요한까요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100031897.1.21",
          "form": "네 꼭 볼께요 ㅎㅎ",
          "original_form": "네 꼭 볼께요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100031897.1.22",
          "form": "제가 500개를 채워서",
          "original_form": "제가 500개를 채워서",
