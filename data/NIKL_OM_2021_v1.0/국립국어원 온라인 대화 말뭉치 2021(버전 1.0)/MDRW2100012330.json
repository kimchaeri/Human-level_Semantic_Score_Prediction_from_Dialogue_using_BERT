      "utterance": [
        {
          "id": "MDRW2100012330.1.1",
          "form": "저 꿔바로우 처음 먹었을 때는",
          "original_form": "저 꿔바로우 처음 먹었을 때는",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100012330.1.2",
          "form": "잘 못 만들었는지 너무 셔서 못 먹었어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잘 못 만들었는지 너무 셔서 못 먹었어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100012330.1.3",
          "form": "앗ㅠ 그렇군요  꿔바로우가 원래 좀 새콤하긴 한데.. 그 친구는 뭔가 잘못 만들어졌나봐요",
          "original_form": "앗ㅠ 그렇군요  꿔바로우가 원래 좀 새콤하긴 한데.. 그 친구는 뭔가 잘못 만들어졌나봐요",
          "speaker_id": "2",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100012330.1.4",
          "form": "네 긍데 다른 곳에서 먹으니까",
          "original_form": "네 긍데 다른 곳에서 먹으니까",
          "speaker_id": "1",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012330.1.5",
          "form": "환상이더라구요!!!!",
          "original_form": "환상이더라구요!!!!",
          "speaker_id": "1",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012330.1.6",
          "form": "아 그쵸그쵸~~ 쫄깃하고..너무 맛있어요!",
          "original_form": "아 그쵸그쵸~~ 쫄깃하고..너무 맛있어요!",
          "speaker_id": "2",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012330.1.7",
          "form": "그래서 예전에 인터넷에서 파는 냉동 꿔바로우를 사 먹었었는데",
          "original_form": "그래서 예전에 인터넷에서 파는 냉동 꿔바로우를 사 먹었었는데",
          "speaker_id": "2",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012330.1.8",
          "form": "꿔바로우가 아니라 그냥 탕수육이더라고요..?",
          "original_form": "꿔바로우가 아니라 그냥 탕수육이더라고요..?",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012330.1.9",
          "form": "ㅋㅋㅋㅋㅋ아 약간 한끗차이죠",
          "original_form": "ㅋㅋㅋㅋㅋ아 약간 한끗차이죠",
          "speaker_id": "1",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012330.1.10",
          "form": "근데 찹쌀탕수육하고 꿔바로우랑 비슷한거죠?",
          "original_form": "근데 찹쌀탕수육하고 꿔바로우랑 비슷한거죠?",
          "speaker_id": "1",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012330.1.11",
          "form": "약간 비슷한데 꿔바로우가 더 쫄깃하고 새콤한 것 같아요",
          "original_form": "약간 비슷한데 꿔바로우가 더 쫄깃하고 새콤한 것 같아요",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012330.1.12",
          "form": "그 튀김 반죽이 다른 느낌?",
          "original_form": "그 튀김 반죽이 다른 느낌?",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012330.1.13",
          "form": "아하 전 소스 차인줄 알았어요ㅋㅋㅋㅋㅋ",
          "original_form": "아하 전 소스 차인줄 알았어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012330.1.14",
          "form": "아무래도 소스가 많이 차이나죠ㅋㅋㅋ",
          "original_form": "아무래도 소스가 많이 차이나죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012330.1.15",
          "form": "아 먹고싶네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 먹고싶네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012330.1.16",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012330.1.17",
          "form": "근데 꿔바로우는 혼자 먹기에는 좀 양이 많아서",
          "original_form": "근데 꿔바로우는 혼자 먹기에는 좀 양이 많아서",
          "speaker_id": "2",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012330.1.18",
          "form": "자주 먹기 어려워서 아쉬워요",
          "original_form": "자주 먹기 어려워서 아쉬워요",
          "speaker_id": "2",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012330.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012330.1.20",
          "form": "가격도 혼자먹기에",
          "original_form": "가격도 혼자먹기에",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012330.1.21",
          "form": "비싸요",
          "original_form": "비싸요",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012330.1.22",
          "form": "가끔 중국집 보면 1인세트로 탕수육 조금 파는 곳도 있던데",
          "original_form": "가끔 중국집 보면 1인세트로 탕수육 조금 파는 곳도 있던데",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012330.1.23",
          "form": "꿔바로우도 그랬으면 좋겠어요ㅋㅋㅋ",
          "original_form": "꿔바로우도 그랬으면 좋겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012330.1.24",
          "form": "맞아요 좀 저렴하게 ㅠㅠ",
          "original_form": "맞아요 좀 저렴하게 ㅠㅠ",
