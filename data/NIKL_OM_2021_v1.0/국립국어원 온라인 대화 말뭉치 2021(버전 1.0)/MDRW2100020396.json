          "speaker_id": "2",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020396.1.3",
          "form": "해봤었음",
          "original_form": "해봤었음",
          "speaker_id": "2",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020396.1.4",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020396.1.5",
          "form": "카트는 컴퓨터보다",
          "original_form": "카트는 컴퓨터보다",
          "speaker_id": "2",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020396.1.6",
          "form": "뭐 제일 잘해요",
          "original_form": "뭐 제일 잘해요",
          "speaker_id": "1",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020396.1.7",
          "form": "폰으로 잘합니더",
          "original_form": "폰으로 잘합니더",
          "speaker_id": "2",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020396.1.8",
          "form": "저 근데 잘하는건없고",
          "original_form": "저 근데 잘하는건없고",
          "speaker_id": "2",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020396.1.9",
          "form": "그 세대 게임 중에",
          "original_form": "그 세대 게임 중에",
          "speaker_id": "1",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020396.1.10",
          "form": "폰게임그닥",
          "original_form": "폰게임그닥",
          "speaker_id": "1",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020396.1.11",
          "form": "고루고루하는 스타일이라",
          "original_form": "고루고루하는 스타일이라",
          "speaker_id": "2",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020396.1.12",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "1",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.13",
          "form": "옵치도많이했었는데",
          "original_form": "옵치도많이했었는데",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.14",
          "form": "저 메이플도 잘함",
          "original_form": "저 메이플도 잘함",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.15",
          "form": "name3 옵치하잖아요",
          "original_form": "&name3& 옵치하잖아요",
          "speaker_id": "1",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.16",
          "form": "같이 안 했어요?",
          "original_form": "같이 안 했어요?",
          "speaker_id": "1",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.17",
          "form": "걔랑많이함",
          "original_form": "걔랑많이함",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.19",
          "form": "ㅋㅋ개못하지않나요",
          "original_form": "ㅋㅋ개못하지않나요",
          "speaker_id": "1",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.20",
          "form": "name3보다는",
          "original_form": "&name3&보다는",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.21",
          "form": "다른친구랑도 많이했었는데",
          "original_form": "다른친구랑도 많이했었는데",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.22",
          "form": "name3 실력기억안나네",
          "original_form": "&name3& 실력기억안나네",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.23",
          "form": "메이플은 잘하고 말고가 있나요",
          "original_form": "메이플은 잘하고 말고가 있나요",
          "speaker_id": "1",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.24",
          "form": "왜냐면 저도 조밥이라",
          "original_form": "왜냐면 저도 조밥이라",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.25",
          "form": "메이플도 얼마나",
          "original_form": "메이플도 얼마나",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.26",
          "form": "걍 현질 노가다인데ㅋㅋ",
          "original_form": "걍 현질 노가다인데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020396.1.27",
          "form": "점수에 전전긍긍하는 겜인데요",
          "original_form": "점수에 전전긍긍하는 겜인데요",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.28",
          "form": "그래서 리부트 서버로 감",
          "original_form": "그래서 리부트 서버로 감",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.29",
          "form": "전 메이플잘모름",
          "original_form": "전 메이플잘모름",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.30",
          "form": "현질할 자신없음",
          "original_form": "현질할 자신없음",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.31",
          "form": "테일즈위버알아요?",
          "original_form": "테일즈위버알아요?",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.32",
          "form": "또뭐하심",
          "original_form": "또뭐하심",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.33",
          "form": "아 들어만봄",
          "original_form": "아 들어만봄",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.34",
          "form": "그거 했음",
          "original_form": "그거 했음",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.35",
          "form": "사이퍼즈",
          "original_form": "사이퍼즈",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.36",
          "form": "아심?",
          "original_form": "아심?",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.37",
          "form": "오노",
          "original_form": "오노",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.38",
          "form": "남들롤할때 사퍼함 ㅋ",
          "original_form": "남들롤할때 사퍼함 ㅋ",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.39",
          "form": "알긴알죠",
          "original_form": "알긴알죠",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.40",
          "form": "으으",
          "original_form": "으으",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.41",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020396.1.42",
          "form": "ㅋ왜 으으인데여",
          "original_form": "ㅋ왜 으으인데여",
          "speaker_id": "2",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020396.1.43",
