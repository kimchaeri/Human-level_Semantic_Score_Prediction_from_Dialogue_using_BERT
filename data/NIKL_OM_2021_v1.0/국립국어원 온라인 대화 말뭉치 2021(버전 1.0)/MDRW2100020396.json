          "original_form": "오타쿠겜인가요",
          "speaker_id": "2",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020396.1.44",
          "form": "그거 약간",
          "original_form": "그거 약간",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
