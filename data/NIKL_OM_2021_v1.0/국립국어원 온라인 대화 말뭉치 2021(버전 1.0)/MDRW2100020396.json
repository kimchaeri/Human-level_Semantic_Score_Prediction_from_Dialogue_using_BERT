          "original_form": "ㅋㅋㅋ무섭",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020396.1.57",
          "form": "리듬게임",
          "original_form": "리듬게임",
          "speaker_id": "2",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020396.1.58",
          "form": "인라인스케이트타고",
          "original_form": "인라인스케이트타고",
          "speaker_id": "2",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020396.1.59",
          "form": "그거 영업당해서했는데",
          "original_form": "그거 영업당해서했는데",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020396.1.60",
          "form": "저 그것도 좋아함",
          "original_form": "저 그것도 좋아함",
          "speaker_id": "2",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020396.1.61",
          "form": "당일에 친구바름",
          "original_form": "당일에 친구바름",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020396.1.62",
          "form": "이제 섭종해서못하잖아요",
          "original_form": "이제 섭종해서못하잖아요",
