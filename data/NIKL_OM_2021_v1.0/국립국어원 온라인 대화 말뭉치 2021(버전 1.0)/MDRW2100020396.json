          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020396.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020396.1.47",
          "form": "제친구가 영업해서",
          "original_form": "제친구가 영업해서",
          "speaker_id": "2",
          "time": "20210828 02:47"
        },
