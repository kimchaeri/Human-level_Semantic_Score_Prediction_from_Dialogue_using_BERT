      "utterance": [
        {
          "id": "MDRW2100038205.1.1",
          "form": "야식드셨나여??",
          "original_form": "야식드셨나여??",
          "speaker_id": "2",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100038205.1.2",
          "form": "네~~~",
          "original_form": "네~~~",
          "speaker_id": "1",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038205.1.3",
          "form": "육개장 사발면이랑",
          "original_form": "육개장 사발면이랑",
          "speaker_id": "1",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038205.1.4",
          "form": "도시락 먹었어요~",
          "original_form": "도시락 먹었어요~",
          "speaker_id": "1",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038205.1.5",
          "form": "오! 근본조합~맛잘알이시군요!",
          "original_form": "오! 근본조합~맛잘알이시군요!",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100038205.1.6",
          "form": "저는 야밤에 주먹밥만들어먹엇답니당ㅋㅋㅋ",
          "original_form": "저는 야밤에 주먹밥만들어먹엇답니당ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100038205.1.7",
          "form": "주먹밥엔 사발면인데!",
          "original_form": "주먹밥엔 사발면인데!",
          "speaker_id": "1",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038205.1.8",
          "form": "앜ㅋㅋㅋㅋ 그쵸 ㅠㅠ근데집에사발면이없어서! 그냥 치즈올려서먹엇어요 ㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ 그쵸 ㅠㅠ근데집에사발면이없어서! 그냥 치즈올려서먹엇어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100038205.1.9",
          "form": "도시락도 짠데",
          "original_form": "도시락도 짠데",
          "speaker_id": "1",
          "time": "20210902 02:09"
        },
        {
          "id": "MDRW2100038205.1.10",
          "form": "짠 라면까지 먹어서",
          "original_form": "짠 라면까지 먹어서",
          "speaker_id": "1",
          "time": "20210902 02:09"
        },
        {
          "id": "MDRW2100038205.1.11",
          "form": "두시간 내내 물만 들이키는 중이에요ㅠㅠ",
          "original_form": "두시간 내내 물만 들이키는 중이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 02:09"
        },
        {
          "id": "MDRW2100038205.1.12",
          "form": "엌ㅋㅋㅋㅋㅋㅋ 많이짜게드셧군여",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋ 많이짜게드셧군여",
          "speaker_id": "2",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100038205.1.13",
          "form": "점심은드셧나여?",
          "original_form": "점심은드셧나여?",
          "speaker_id": "2",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100038205.1.14",
          "form": "네~~ 야식을 너무 헤비하게 먹어서",
          "original_form": "네~~ 야식을 너무 헤비하게 먹어서",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100038205.1.15",
          "form": "샐러드 가게 오픈하자마자 사왓어요",
          "original_form": "샐러드 가게 오픈하자마자 사왓어요",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100038205.1.16",
          "form": "오왕!! 클린식단하셧군요!! 저는 주먹밥해먹엇어요 ㅎㅎ",
          "original_form": "오왕!! 클린식단하셧군요!! 저는 주먹밥해먹엇어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100038205.1.17",
          "form": "어제 맛있었나요?ㅋㅋㅋㅋ",
          "original_form": "어제 맛있었나요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100038205.1.18",
          "form": "무슨 주먹밥이여?!",
          "original_form": "무슨 주먹밥이여?!",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100038205.1.19",
          "form": "넹ㅋㅋㅋㅋㅋㅋ 아 참치땡초마요주먹밥해먹엇어요~단무지랑양파도다져서요 ㅎㅎ",
          "original_form": "넹ㅋㅋㅋㅋㅋㅋ 아 참치땡초마요주먹밥해먹엇어요~단무지랑양파도다져서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038205.1.20",
          "form": "으악 저는 땡초 못먹어요ㅋㅋㅋㅋㅋ",
          "original_form": "으악 저는 땡초 못먹어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100038205.1.21",
          "form": "절대....",
