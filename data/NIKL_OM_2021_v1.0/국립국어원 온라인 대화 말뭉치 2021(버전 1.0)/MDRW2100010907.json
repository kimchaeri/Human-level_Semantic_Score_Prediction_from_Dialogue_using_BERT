      "utterance": [
        {
          "id": "MDRW2100010907.1.1",
          "form": "고양이 기르고싶다",
          "original_form": "고양이 기르고싶다",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010907.1.2",
          "form": "왜 못기르나요",
          "original_form": "왜 못기르나요",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010907.1.3",
          "form": "자취하는데 집이 너무 좁아",
          "original_form": "자취하는데 집이 너무 좁아",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010907.1.4",
          "form": "최소 투룸은 가서 기르고싶어",
          "original_form": "최소 투룸은 가서 기르고싶어",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010907.1.5",
          "form": "근데 왜 자꾸 반말해?",
          "original_form": "근데 왜 자꾸 반말해?",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010907.1.6",
          "form": "앗 그냥..? 불편했어?",
          "original_form": "앗 그냥..? 불편했어?",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010907.1.7",
          "form": "응 조금 불편하네 서로존대하자",
          "original_form": "응 조금 불편하네 서로존대하자",
          "speaker_id": "2",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010907.1.8",
          "form": "채팅을 여러개하니까 말투를 고정한건데 미안해요",
          "original_form": "채팅을 여러개하니까 말투를 고정한건데 미안해요",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010907.1.9",
          "form": "고양이 어떤 점이 좋아요?",
          "original_form": "고양이 어떤 점이 좋아요?",
          "speaker_id": "2",
          "time": "20210823 22:55"
        },
        {
          "id": "MDRW2100010907.1.10",
          "form": "일단 귀엽기도 하고, 은근하게 곁을 맴돌아서 좋아요",
          "original_form": "일단 귀엽기도 하고, 은근하게 곁을 맴돌아서 좋아요",
          "speaker_id": "1",
          "time": "20210823 22:55"
        },
        {
          "id": "MDRW2100010907.1.11",
          "form": "외모가 귀여워서 호감이긴 해요 ㅎ",
          "original_form": "외모가 귀여워서 호감이긴 해요 ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:56"
        },
        {
          "id": "MDRW2100010907.1.12",
          "form": "고양이 가까이서 본 적은 있나요",
          "original_form": "고양이 가까이서 본 적은 있나요",
          "speaker_id": "2",
          "time": "20210823 22:56"
        },
        {
          "id": "MDRW2100010907.1.13",
          "form": "친구중에 기르는 친구가 꽤 있어서 몇번 만져봤어요",
          "original_form": "친구중에 기르는 친구가 꽤 있어서 몇번 만져봤어요",
          "speaker_id": "1",
          "time": "20210823 22:57"
        },
        {
          "id": "MDRW2100010907.1.14",
          "form": "부들부들하고 너무 귀엽더라구요ㅜㅜ",
          "original_form": "부들부들하고 너무 귀엽더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 22:57"
        },
        {
          "id": "MDRW2100010907.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 22:57"
        },
        {
          "id": "MDRW2100010907.1.16",
          "form": "부들부들하고 따뜻하고 말랑거려요",
          "original_form": "부들부들하고 따뜻하고 말랑거려요",
          "speaker_id": "2",
          "time": "20210823 22:57"
        },
        {
          "id": "MDRW2100010907.1.17",
          "form": "젤리도 만져본적있는데 너무 말캉하고좋았어요ㅜㅜ",
          "original_form": "젤리도 만져본적있는데 너무 말캉하고좋았어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 22:58"
        }
      ]
