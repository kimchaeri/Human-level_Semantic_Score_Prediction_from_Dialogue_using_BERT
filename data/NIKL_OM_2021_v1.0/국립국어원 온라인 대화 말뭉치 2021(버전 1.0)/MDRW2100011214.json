        },
        {
          "id": "MDRW2100011214.1.2",
          "form": "부모님이랑 남동생 한명잇어요!",
          "original_form": "부모님이랑 남동생 한명잇어요!",
          "speaker_id": "2",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100011214.1.3",
          "form": "아하 집안일은 분담하세요?",
          "original_form": "아하 집안일은 분담하세요?",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100011214.1.4",
          "form": "보통 엄마가 다하긴해요",
          "original_form": "보통 엄마가 다하긴해요",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100011214.1.5",
          "form": "독립 생각은 있으세요?",
          "original_form": "독립 생각은 있으세요?",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100011214.1.6",
          "form": "넹 커서 하고싶어요!",
          "original_form": "넹 커서 하고싶어요!",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100011214.1.7",
          "form": "나이가 어떻게되세요?",
          "original_form": "나이가 어떻게되세요?",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100011214.1.8",
          "form": "16살 입니당",
          "original_form": "16살 입니당",
          "speaker_id": "2",
