      "utterance": [
        {
          "id": "MDRW2100045405.1.1",
          "form": "네, 다 남자를 사람으로 안 봐요.",
          "original_form": "네, 다 남자를 사람으로 안 봐요.",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045405.1.2",
          "form": "전 직장동료들이 비혼이 많아서",
          "original_form": "전 직장동료들이 비혼이 많아서",
          "speaker_id": "1",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045405.1.3",
          "form": "ㅇㅏ 진짜 좋네요!",
          "original_form": "ㅇㅏ 진짜 좋네요!",
          "speaker_id": "1",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045405.1.4",
          "form": "좋네요 !!",
          "original_form": "좋네요 !!",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045405.1.5",
          "form": "친구면 더 좋죠!",
          "original_form": "친구면 더 좋죠!",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045405.1.6",
          "form": "전 대부분 애엄마라ㅜ",
          "original_form": "전 대부분 애엄마라ㅜ",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045405.1.7",
          "form": "그거 엄청 스트레스네요",
          "original_form": "그거 엄청 스트레스네요",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045405.1.8",
          "form": "한명남았는데",
          "original_form": "한명남았는데",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045405.1.9",
          "form": "억지로 결혼해요",
          "original_form": "억지로 결혼해요",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045405.1.10",
          "form": "굳이???",
          "original_form": "굳이???",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045405.1.11",
          "form": "저는 정말 친했던 친구 중 한 명을 올 초에 포기했어요.",
          "original_form": "저는 정말 친했던 친구 중 한 명을 올 초에 포기했어요.",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045405.1.12",
          "form": "와 구출할 방법이 없나요?",
          "original_form": "와 구출할 방법이 없나요?",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045405.1.13",
          "form": "ㅋㅋ팔자죠",
          "original_form": "ㅋㅋ팔자죠",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045405.1.14",
          "form": "그 친군 남친이 없던적이...",
          "original_form": "그 친군 남친이 없던적이...",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045405.1.15",
          "form": "대화의 대부분이 백인 남자 연예인 덕질이고 그 배우의 사상을 자꾸 들이미는 식이라",
          "original_form": "대화의 대부분이 백인 남자 연예인 덕질이고 그 배우의 사상을 자꾸 들이미는 식이라",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045405.1.16",
          "form": "아???",
          "original_form": "아???",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045405.1.17",
          "form": "그런\r\n 경우도 있나요??",
          "original_form": "그런\r\n 경우도 있나요??",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045405.1.18",
          "form": "많습니다",
          "original_form": "많습니다",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045405.1.19",
          "form": "서른 초중반들 중에 많아요",
          "original_form": "서른 초중반들 중에 많아요",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045405.1.20",
          "form": "친구분은 대단하시네요",
          "original_form": "친구분은 대단하시네요",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045405.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045405.1.22",
          "form": "성향 차이라고 위로를 ㅜㅋ",
          "original_form": "성향 차이라고 위로를 ㅜㅋ",
          "speaker_id": "2",
          "time": "20210908 02:24"
        }
      ]
