      "utterance": [
        {
          "id": "MDRW2100002974.1.1",
          "form": "와 근데 갑자기 비가 쏟아부어",
          "original_form": "와 근데 갑자기 비가 쏟아부어",
          "speaker_id": "2",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002974.1.2",
          "form": "깜짝 놀랐네~뭐 이렇게 갑자기 내려 ㅎㅎ",
          "original_form": "깜짝 놀랐네~뭐 이렇게 갑자기 내려 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002974.1.3",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002974.1.4",
          "form": "앞이 안보일 정도로 쏴아 ㅡㅡ",
          "original_form": "앞이 안보일 정도로 쏴아 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002974.1.5",
          "form": "밖에 걸어다니던 사람들 깜놀했겠어 ㅋㅋ",
          "original_form": "밖에 걸어다니던 사람들 깜놀했겠어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002974.1.6",
          "form": "우산 쓸 시간도 없었을 듯",
          "original_form": "우산 쓸 시간도 없었을 듯",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002974.1.7",
          "form": "그러게 말이야 ㅎㅎ",
          "original_form": "그러게 말이야 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002974.1.8",
          "form": "신랑도 우산 없잖아",
          "original_form": "신랑도 우산 없잖아",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002974.1.9",
          "form": "다행히 회사에 하나 있어",
          "original_form": "다행히 회사에 하나 있어",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002974.1.10",
          "form": "ㅇㅇ 다행이네",
          "original_form": "ㅇㅇ 다행이네",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002974.1.11",
          "form": "없었으면 큰일날뻔 ㅎㅎ 비가 언제 올지 몰라",
          "original_form": "없었으면 큰일날뻔 ㅎㅎ 비가 언제 올지 몰라",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002974.1.12",
          "form": "ㅋㅋㅋ 진짜 순식간에 내려버리니",
          "original_form": "ㅋㅋㅋ 진짜 순식간에 내려버리니",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002974.1.13",
          "form": "하루에도 몇번씩 비가 오락가락하네",
          "original_form": "하루에도 몇번씩 비가 오락가락하네",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002974.1.14",
          "form": "날씨가 너무 제멋대로야 이상해졌어 ㅎㅎ",
          "original_form": "날씨가 너무 제멋대로야 이상해졌어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002974.1.15",
          "form": "해가 짧아진 것 같아",
          "original_form": "해가 짧아진 것 같아",
          "speaker_id": "1",
