      "utterance": [
        {
          "id": "MDRW2100026536.1.1",
          "form": "종교 있어??",
          "original_form": "종교 있어??",
          "speaker_id": "2",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100026536.1.2",
          "form": "아니! 나는 종교 없는데",
          "original_form": "아니! 나는 종교 없는데",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100026536.1.3",
          "form": "너는 있어??",
          "original_form": "너는 있어??",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100026536.1.4",
          "form": "나도 없어... ㅎㅎ",
          "original_form": "나도 없어... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100026536.1.5",
          "form": "중학교가 기독교 학교라서 교회 가보긴 했어!",
          "original_form": "중학교가 기독교 학교라서 교회 가보긴 했어!",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100026536.1.6",
          "form": "예배 끝나니까 빵 주더라구 ㅋㅋ",
          "original_form": "예배 끝나니까 빵 주더라구 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100026536.1.7",
          "form": "아~ 나도 고등학교 기독교 학교였어",
          "original_form": "아~ 나도 고등학교 기독교 학교였어",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100026536.1.8",
          "form": "우린 맨날 헌급 뜯어거던데",
          "original_form": "우린 맨날 헌급 뜯어거던데",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100026536.1.9",
          "form": "좋은 학교였구나 빵도 주다니!",
          "original_form": "좋은 학교였구나 빵도 주다니!",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100026536.1.10",
          "form": "헉 헌급이라니...",
          "original_form": "헉 헌급이라니...",
          "speaker_id": "2",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100026536.1.11",
          "form": "먹을 거 아예 안 줬어??",
          "original_form": "먹을 거 아예 안 줬어??",
          "speaker_id": "2",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100026536.1.12",
          "form": "먹을거 거의 안준거같아....",
          "original_form": "먹을거 거의 안준거같아....",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100026536.1.13",
          "form": "항상 뜯기기만 했지...ㅎㅎ",
          "original_form": "항상 뜯기기만 했지...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100026536.1.14",
          "form": "그래서 별로 나한테 이미지가 좋지는 않아!",
          "original_form": "그래서 별로 나한테 이미지가 좋지는 않아!",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100026536.1.15",
          "form": "너가 다닌 중학교만 같으면 참 좋을텐데!",
          "original_form": "너가 다닌 중학교만 같으면 참 좋을텐데!",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100026536.1.16",
          "form": "헐 너무하다... ㅠㅠ",
          "original_form": "헐 너무하다... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100026536.1.17",
          "form": "학생들한테 돈을...",
          "original_form": "학생들한테 돈을...",
          "speaker_id": "2",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100026536.1.18",
          "form": "그러니까ㅜㅜㅜ",
          "original_form": "그러니까ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100026536.1.19",
          "form": "좋았던 기억이라고는 찬송가 불렀던거..?",
          "original_form": "좋았던 기억이라고는 찬송가 불렀던거..?",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100026536.1.20",
          "form": "오 찬송가도 불렀구나!",
          "original_form": "오 찬송가도 불렀구나!",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100026536.1.21",
          "form": "재밌었겠다 ㅎㅎ",
          "original_form": "재밌었겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100026536.1.22",
          "form": "응! 생각보다 재밌더라구ㅎㅎ",
          "original_form": "응! 생각보다 재밌더라구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100026536.1.23",
          "form": "그 외엔 별로였지만ㅜㅜㅜ",
          "original_form": "그 외엔 별로였지만ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100026536.1.24",
          "form": "그래도 추억이 됐네 ㅎㅎ",
          "original_form": "그래도 추억이 됐네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100026536.1.25",
          "form": "우리 둘 다 무교니까 무슨 얘기를 해야할 지 모르겠네...",
          "original_form": "우리 둘 다 무교니까 무슨 얘기를 해야할 지 모르겠네...",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100026536.1.26",
          "form": "그러니까...!",
          "original_form": "그러니까...!",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100026536.1.27",
          "form": "한국은 무교가 많은거같아",
          "original_form": "한국은 무교가 많은거같아",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100026536.1.28",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100026536.1.29",
          "form": "은근 기독교도 많더라구",
          "original_form": "은근 기독교도 많더라구",
          "speaker_id": "2",
          "time": "20210903 14:14"
        }
      ]
    }
