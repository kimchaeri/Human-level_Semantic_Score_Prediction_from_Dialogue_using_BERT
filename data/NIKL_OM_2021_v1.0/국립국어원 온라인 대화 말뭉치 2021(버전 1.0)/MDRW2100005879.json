      "utterance": [
        {
          "id": "MDRW2100005879.1.1",
          "form": "하 배고프네요 ㅠㅠ",
          "original_form": "하 배고프네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100005879.1.2",
          "form": "점심은 드셨나요",
          "original_form": "점심은 드셨나요",
          "speaker_id": "1",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100005879.1.3",
          "form": "네 저 기숙사 도시락 먹었어요!",
          "original_form": "네 저 기숙사 도시락 먹었어요!",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100005879.1.4",
          "form": "점심 안드셨나요 ?",
          "original_form": "점심 안드셨나요 ?",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100005879.1.5",
          "form": "기숙사다니시는구나",
          "original_form": "기숙사다니시는구나",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100005879.1.6",
          "form": "점심을 안먹었더니 너무배고프네요ㅠㅠ",
          "original_form": "점심을 안먹었더니 너무배고프네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100005879.1.7",
          "form": "사시는 지역이어떻게되세요?",
          "original_form": "사시는 지역이어떻게되세요?",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100005879.1.8",
          "form": "저 본가는 천안이고 대학은 서울에서 다니고 있어요 !",
          "original_form": "저 본가는 천안이고 대학은 서울에서 다니고 있어요 !",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100005879.1.9",
          "form": "점심 왜 안드셨나요 바쁘셨나요?",
          "original_form": "점심 왜 안드셨나요 바쁘셨나요?",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100005879.1.10",
          "form": "네ㅜㅜ좀",
          "original_form": "네ㅜㅜ좀",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005879.1.11",
          "form": "아 서울어디쪽이세요?",
          "original_form": "아 서울어디쪽이세요?",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005879.1.12",
          "form": "제 취미가 맛집탐방이러",
          "original_form": "제 취미가 맛집탐방이러",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005879.1.13",
          "form": "가까운곳 맛집있으면 추천해두릴게요!",
          "original_form": "가까운곳 맛집있으면 추천해두릴게요!",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005879.1.14",
          "form": "오 저 노원 쪽이에요 ! 완전 구석탱이라 모르실 것 같긴한데 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 저 노원 쪽이에요 ! 완전 구석탱이라 모르실 것 같긴한데 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100005879.1.15",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005879.1.16",
          "form": "저 노원살아요 ㅋㅋㅋ",
          "original_form": "저 노원살아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005879.1.17",
          "form": "과기대 근처에 맛집많지않아요?",
          "original_form": "과기대 근처에 맛집많지않아요?",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005879.1.18",
          "form": "기숙사있는 노원대학은 과기대일거같은뎅",
          "original_form": "기숙사있는 노원대학은 과기대일거같은뎅",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005879.1.19",
          "form": "오 정말 신기하네요 !!!!!!!!!",
          "original_form": "오 정말 신기하네요 !!!!!!!!!",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005879.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005879.1.21",
          "form": "어디지 삽겹살 진짜 맛있더라고요",
          "original_form": "어디지 삽겹살 진짜 맛있더라고요",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005879.1.22",
          "form": "공릉맛집...잠깐만요",
          "original_form": "공릉맛집...잠깐만요",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005879.1.23",
          "form": "지도에서좀 찾아볼게요",
          "original_form": "지도에서좀 찾아볼게요",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005879.1.24",
          "form": "아잌ㅋㅋㅋㅋ 그렇게까짙ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아잌ㅋㅋㅋㅋ 그렇게까짙ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005879.1.25",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005879.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005879.1.27",
          "form": "옛날에 그앞에 삼익아파트? 많이갔었어요 공릉",
          "original_form": "옛날에 그앞에 삼익아파트? 많이갔었어요 공릉",
          "speaker_id": "1",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100005879.1.28",
          "form": "오 그렇군요 !",
          "original_form": "오 그렇군요 !",
          "speaker_id": "2",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100005879.1.29",
          "form": "국수나무 진짜 맛있어요",
          "original_form": "국수나무 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100005879.1.30",
          "form": "제 최애 알밥이 국수나무에요",
          "original_form": "제 최애 알밥이 국수나무에요",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100005879.1.31",
          "form": "아 ㅎㅎ 국수나무 맛있죠!!",
          "original_form": "아 ㅎㅎ 국수나무 맛있죠!!",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005879.1.32",
          "form": "뭔가 동네주민만나니까 신기하네요",
          "original_form": "뭔가 동네주민만나니까 신기하네요",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005879.1.33",
          "form": "천안에서는 꼭먹어야하는 맛집있나요?",
          "original_form": "천안에서는 꼭먹어야하는 맛집있나요?",
          "speaker_id": "1",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100005879.1.34",
          "form": "그러게요 ㅋㅋㅋㅋ 진짜 신기하네요 넓고넓은 서울에서 ㅌㅌㅌㅌㅋㅋㅋㅋ",
          "original_form": "그러게요 ㅋㅋㅋㅋ 진짜 신기하네요 넓고넓은 서울에서 ㅌㅌㅌㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100005879.1.35",
          "form": "천안에 유명하고 전통있는 국수집이 하나 있어요",
          "original_form": "천안에 유명하고 전통있는 국수집이 하나 있어요",
          "speaker_id": "2",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100005879.1.36",
          "form": "최고에요",
          "original_form": "최고에요",
          "speaker_id": "2",
          "time": "20210907 15:36"
        }
