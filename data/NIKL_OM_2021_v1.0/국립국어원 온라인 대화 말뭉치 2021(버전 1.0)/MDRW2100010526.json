      "utterance": [
        {
          "id": "MDRW2100010526.1.1",
          "form": "전은 다드셨나요?ㅎ",
          "original_form": "전은 다드셨나요?ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100010526.1.2",
          "form": "네 넘배불러요",
          "original_form": "네 넘배불러요",
          "speaker_id": "2",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010526.1.3",
          "form": "역시 비올때 저녁은?전이죠",
          "original_form": "역시 비올때 저녁은?전이죠",
          "speaker_id": "2",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010526.1.4",
          "form": "배달 업체가",
          "original_form": "배달 업체가",
          "speaker_id": "2",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010526.1.5",
          "form": "양이 엄청나요",
          "original_form": "양이 엄청나요",
          "speaker_id": "2",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010526.1.6",
          "form": "모듬전?",
          "original_form": "모듬전?",
          "speaker_id": "1",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010526.1.7",
          "form": "진짜 운좋았어요",
          "original_form": "진짜 운좋았어요",
          "speaker_id": "2",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010526.1.8",
          "form": "시키셨어요?",
          "original_form": "시키셨어요?",
          "speaker_id": "1",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010526.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010526.1.10",
          "form": "전 부칠순 없고 시켰는데",
          "original_form": "전 부칠순 없고 시켰는데",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010526.1.11",
          "form": "생선전 산적",
          "original_form": "생선전 산적",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010526.1.12",
          "form": "아...진짜 땡기네요 ㅎ",
          "original_form": "아...진짜 땡기네요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010526.1.13",
          "form": "김치전 호박전",
          "original_form": "김치전 호박전",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010526.1.14",
          "form": "캬~~~ 산적 생선살",
          "original_form": "캬~~~ 산적 생선살",
          "speaker_id": "1",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010526.1.15",
          "form": "두부 동그랑땡 풋고추전",
          "original_form": "두부 동그랑땡 풋고추전",
          "speaker_id": "2",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100010526.1.16",
          "form": "종류가 엄청 났어요",
          "original_form": "종류가 엄청 났어요",
          "speaker_id": "2",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100010526.1.17",
          "form": "김치전 언제먹었는지",
          "original_form": "김치전 언제먹었는지",
          "speaker_id": "1",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100010526.1.18",
          "form": "기억도 안나네요",
          "original_form": "기억도 안나네요",
          "speaker_id": "1",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010526.1.19",
          "form": "진짜 종류별로 많네요",
          "original_form": "진짜 종류별로 많네요",
          "speaker_id": "1",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010526.1.20",
          "form": "네 이렇게 많은건 첨봐요",
          "original_form": "네 이렇게 많은건 첨봐요",
          "speaker_id": "2",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010526.1.21",
          "form": "가격은 얼마예요?",
          "original_form": "가격은 얼마예요?",
          "speaker_id": "1",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010526.1.22",
          "form": "매운거 땡기시죠 ㅜㅜ",
          "original_form": "매운거 땡기시죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010526.1.23",
          "form": "23000원요",
          "original_form": "23000원요",
          "speaker_id": "2",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010526.1.24",
          "form": "싸죠?",
          "original_form": "싸죠?",
          "speaker_id": "2",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010526.1.25",
          "form": "다른데는 3만원 넘더라구요",
          "original_form": "다른데는 3만원 넘더라구요",
          "speaker_id": "2",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010526.1.26",
          "form": "그가격엔 못먹죠 ㅜㅜ",
          "original_form": "그가격엔 못먹죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010526.1.27",
          "form": "와 싸네요",
          "original_form": "와 싸네요",
          "speaker_id": "1",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010526.1.28",
          "form": "네 매콥한거 먹ㅈ고싶네요",
          "original_form": "네 매콥한거 먹ㅈ고싶네요",
          "speaker_id": "1",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010526.1.29",
          "form": "그렇죠 첨생긴데래요",
          "original_form": "그렇죠 첨생긴데래요",
          "speaker_id": "2",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010526.1.30",
          "form": "ㅜㅜ애들땜시 못드시고",
          "original_form": "ㅜㅜ애들땜시 못드시고",
          "speaker_id": "2",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010526.1.31",
          "form": "저는 그런거 시켜먹을 생각을 왜 못했을까요?",
          "original_form": "저는 그런거 시켜먹을 생각을 왜 못했을까요?",
          "speaker_id": "1",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010526.1.32",
          "form": "시켜보세요",
          "original_form": "시켜보세요",
          "speaker_id": "2",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010526.1.33",
          "form": "저도 주말에 신랑오면 도전 ㅎ",
          "original_form": "저도 주말에 신랑오면 도전 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010526.1.34",
          "form": "애들도 먹을수있러요",
          "original_form": "애들도 먹을수있러요",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010526.1.35",
          "form": "안매운거많아요",
          "original_form": "안매운거많아요",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010526.1.36",
          "form": "지금은 시키면 양이...많을듯요",
          "original_form": "지금은 시키면 양이...많을듯요",
          "speaker_id": "1",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010526.1.37",
          "form": "아., .내일 저녁메뉴로도 좋겠네요",
          "original_form": "아., .내일 저녁메뉴로도 좋겠네요",
          "speaker_id": "1",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010526.1.38",
          "form": "굳아이디어!!!!예요",
          "original_form": "굳아이디어!!!!예요",
          "speaker_id": "1",
