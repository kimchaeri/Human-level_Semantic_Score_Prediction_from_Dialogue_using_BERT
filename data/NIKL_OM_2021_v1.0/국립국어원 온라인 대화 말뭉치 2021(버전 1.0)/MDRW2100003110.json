        {
          "id": "MDRW2100003110.1.1",
          "form": "화장 자주 해?",
          "original_form": "화장 자주 해?",
          "speaker_id": "2",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100003110.1.2",
          "form": "가끔 ㅋㅋ기초 제품 추천해줘 ㅋㅋ",
          "original_form": "가끔 ㅋㅋ기초 제품 추천해줘 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003110.1.3",
          "form": "나 꾸준히 쓰는 건 아벤느 시칼파트",
          "original_form": "나 꾸준히 쓰는 건 아벤느 시칼파트",
          "speaker_id": "2",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003110.1.4",
          "form": "그거 좋아?",
          "original_form": "그거 좋아?",
          "speaker_id": "1",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003110.1.5",
          "form": "응 좋은 거 같아",
          "original_form": "응 좋은 거 같아",
          "speaker_id": "2",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003110.1.6",
          "form": "피부 장벽 복구해주는 거라서",
          "original_form": "피부 장벽 복구해주는 거라서",
          "speaker_id": "2",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100003110.1.7",
          "form": "자극 없고",
          "original_form": "자극 없고",
          "speaker_id": "2",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100003110.1.8",
          "form": "나중에 발라봐야지",
          "original_form": "나중에 발라봐야지",
          "speaker_id": "1",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100003110.1.9",
          "form": "응 한번 써봐",
          "original_form": "응 한번 써봐",
          "speaker_id": "2",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100003110.1.10",
          "form": "또 뭐 있어?",
          "original_form": "또 뭐 있어?",
          "speaker_id": "1",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100003110.1.11",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100003110.1.12",
          "form": "핌플패드 꾸준히 쓰고 있어",
          "original_form": "핌플패드 꾸준히 쓰고 있어",
          "speaker_id": "2",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100003110.1.13",
          "form": "핌플패드 나도 써 ㅋㅋ",
          "original_form": "핌플패드 나도 써 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100003110.1.14",
          "form": "어디꺼 써?",
          "original_form": "어디꺼 써?",
          "speaker_id": "2",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100003110.1.15",
          "form": "수박패드 ㅋㅋ",
          "original_form": "수박패드 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100003110.1.16",
          "form": "벨라몬스터꺼",
          "original_form": "벨라몬스터꺼",
          "speaker_id": "1",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100003110.1.17",
          "form": "나는 코스알엑스",
          "original_form": "나는 코스알엑스",
          "speaker_id": "2",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100003110.1.18",
          "form": "스트라덱스 썼었는데 수박패드 세일하길래 ㅋㅋ",
          "original_form": "스트라덱스 썼었는데 수박패드 세일하길래 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:54"
        },
        {
          "id": "MDRW2100003110.1.19",
          "form": "좋아?",
          "original_form": "좋아?",
          "speaker_id": "2",
          "time": "20210820 18:54"
        },
        {
