      "utterance": [
        {
          "id": "MDRW2100029747.1.1",
          "form": "연애하고 계신가요?",
          "original_form": "연애하고 계신가요?",
          "speaker_id": "1",
          "time": "20210903 12:49"
        },
        {
          "id": "MDRW2100029747.1.2",
          "form": "연애요? 당연하죠",
          "original_form": "연애요? 당연하죠",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029747.1.3",
          "form": "당신은요?",
          "original_form": "당신은요?",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029747.1.4",
          "form": "당연?",
          "original_form": "당연?",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029747.1.5",
          "form": "자도 하고 있습니다",
          "original_form": "자도 하고 있습니다",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029747.1.6",
          "form": "요즘 연애 안하는 사람도 있나요?",
          "original_form": "요즘 연애 안하는 사람도 있나요?",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029747.1.7",
          "form": "사귄지 얼마나 되셨어요",
          "original_form": "사귄지 얼마나 되셨어요",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100029747.1.8",
          "form": "연애왕이신가 보네여",
          "original_form": "연애왕이신가 보네여",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100029747.1.9",
          "form": "저는 10개월됐습니다",
          "original_form": "저는 10개월됐습니다",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100029747.1.10",
          "form": "선생님은요",
          "original_form": "선생님은요",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100029747.1.11",
          "form": "저는 63일이요",
          "original_form": "저는 63일이요",
          "speaker_id": "2",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100029747.1.12",
          "form": "오래 만나셨네요 결혼하셔야겠어요",
          "original_form": "오래 만나셨네요 결혼하셔야겠어요",
          "speaker_id": "1",
          "time": "20210903 12:56"
        },
        {
          "id": "MDRW2100029747.1.13",
          "form": "놀리시는 건가요??",
          "original_form": "놀리시는 건가요??",
          "speaker_id": "2",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100029747.1.14",
          "form": "아닌데요?",
          "original_form": "아닌데요?",
          "speaker_id": "1",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100029747.1.15",
          "form": "소개팅으로 만나셨어요?",
          "original_form": "소개팅으로 만나셨어요?",
          "speaker_id": "1",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100029747.1.16",
          "form": "저는 아직 결혼 생각이 없어요",
          "original_form": "저는 아직 결혼 생각이 없어요",
          "speaker_id": "2",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029747.1.17",
          "form": "네 소개팅 했습니다",
          "original_form": "네 소개팅 했습니다",
          "speaker_id": "2",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100029747.1.18",
          "form": "전 자만추입니다",
          "original_form": "전 자만추입니다",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029747.1.19",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "2",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100029747.1.20",
          "form": "소만추세요?",
          "original_form": "소만추세요?",
          "speaker_id": "1",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100029747.1.21",
          "form": "아뇨 저도 자만추입니다.",
          "original_form": "아뇨 저도 자만추입니다.",
          "speaker_id": "2",
          "time": "20210903 13:11"
        },
        {
          "id": "MDRW2100029747.1.22",
          "form": ";;모순되네요",
          "original_form": ";;모순되네요",
          "speaker_id": "1",
          "time": "20210903 13:11"
        },
        {
          "id": "MDRW2100029747.1.23",
          "form": "그래요",
