      "utterance": [
        {
          "id": "MDRW2100017825.1.1",
          "form": "오늘 저녁 뭐 드셨어요?",
          "original_form": "오늘 저녁 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017825.1.2",
          "form": "떡볶이요",
          "original_form": "떡볶이요",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017825.1.3",
          "form": "무슨 떡볶이 먹었어요?",
          "original_form": "무슨 떡볶이 먹었어요?",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017825.1.4",
          "form": "근처 시장에서 사먹었어요",
          "original_form": "근처 시장에서 사먹었어요",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017825.1.5",
          "form": "아 그렇구나 ㅎㅎ",
          "original_form": "아 그렇구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017825.1.6",
          "form": "쌀떡이에요 밀떡이에요?",
          "original_form": "쌀떡이에요 밀떡이에요?",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017825.1.7",
          "form": "밀떡이요",
          "original_form": "밀떡이요",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017825.1.8",
          "form": "우와 ㅎㅎ 소스는요?",
          "original_form": "우와 ㅎㅎ 소스는요?",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017825.1.9",
          "form": "약간 매콤하고 맛있어요",
          "original_form": "약간 매콤하고 맛있어요",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017825.1.10",
          "form": "프랜차이즈랑 좀 다르긴 하네요ㅋㅋ",
          "original_form": "프랜차이즈랑 좀 다르긴 하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017825.1.11",
          "form": "그럼 그냥 고추장소스인가보네요",
          "original_form": "그럼 그냥 고추장소스인가보네요",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017825.1.12",
          "form": "튀김도 사셨어요?",
          "original_form": "튀김도 사셨어요?",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017825.1.13",
          "form": "아뇨 순대 사서 같이 먹었어요",
          "original_form": "아뇨 순대 사서 같이 먹었어요",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017825.1.14",
          "form": "순대 맛있겠다 ㅎㅎ",
          "original_form": "순대 맛있겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017825.1.15",
          "form": "순대는 보통 어디에 찍어 드세요?",
          "original_form": "순대는 보통 어디에 찍어 드세요?",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017825.1.16",
          "form": "전 소금이요. 지역따라 차이 있다고 하던데요",
          "original_form": "전 소금이요. 지역따라 차이 있다고 하던데요",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017825.1.17",
          "form": "오 ㅎㅎ 맞아요! 쌈장, 초고추장 등등 다양하더라구요.",
          "original_form": "오 ㅎㅎ 맞아요! 쌈장, 초고추장 등등 다양하더라구요.",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017825.1.18",
          "form": "그래서 따라해봤는데 맛있었어요 ㅎㅎ",
          "original_form": "그래서 따라해봤는데 맛있었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:43"
        }
      ]
