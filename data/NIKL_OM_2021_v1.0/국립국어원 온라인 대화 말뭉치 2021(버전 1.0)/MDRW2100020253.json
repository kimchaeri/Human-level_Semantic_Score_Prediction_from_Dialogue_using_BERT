      "utterance": [
        {
          "id": "MDRW2100020253.1.1",
          "form": "김밥 좋아하니",
          "original_form": "김밥 좋아하니",
          "speaker_id": "2",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020253.1.2",
          "form": "소풍 갈 때 많이 먹었어",
          "original_form": "소풍 갈 때 많이 먹었어",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020253.1.3",
          "form": "요즘 김밥이 유행이야",
          "original_form": "요즘 김밥이 유행이야",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020253.1.4",
          "form": "김밥은 만들기가 힘들어",
          "original_form": "김밥은 만들기가 힘들어",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020253.1.5",
          "form": "그렇지만 도 아나",
          "original_form": "그렇지만 도 아나",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020253.1.6",
          "form": "어떤 김밥 좋아해",
          "original_form": "어떤 김밥 좋아해",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020253.1.7",
          "form": "돈까스 김밥 맛있어",
          "original_form": "돈까스 김밥 맛있어",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020253.1.8",
          "form": "참치김밥도 맛있어",
          "original_form": "참치김밥도 맛있어",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020253.1.9",
          "form": "야채 김밥도 맛있어",
          "original_form": "야채 김밥도 맛있어",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020253.1.10",
          "form": "김밥 잘 사는 방법 알려줘",
          "original_form": "김밥 잘 사는 방법 알려줘",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020253.1.11",
          "form": "요즘 김밥 맛집들은 웨이팅이 길어",
          "original_form": "요즘 김밥 맛집들은 웨이팅이 길어",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020253.1.12",
          "form": "그렇게까지 먹다니",
          "original_form": "그렇게까지 먹다니",
          "speaker_id": "1",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020253.1.13",
          "form": "집에서 만들기 은근 힘들잖아",
          "original_form": "집에서 만들기 은근 힘들잖아",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020253.1.14",
          "form": "내일 김밥 먹어야겠다",
          "original_form": "내일 김밥 먹어야겠다",
          "speaker_id": "1",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020253.1.15",
          "form": "아 또 그럴까",
          "original_form": "아 또 그럴까",
