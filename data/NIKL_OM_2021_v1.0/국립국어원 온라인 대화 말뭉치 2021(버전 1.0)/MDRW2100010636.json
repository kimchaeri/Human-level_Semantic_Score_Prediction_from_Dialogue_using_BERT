      "utterance": [
        {
          "id": "MDRW2100010636.1.1",
          "form": "아까 4시쯤",
          "original_form": "아까 4시쯤",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010636.1.2",
          "form": "로제떡볶이 시켰는데",
          "original_form": "로제떡볶이 시켰는데",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010636.1.3",
          "form": "뭔가 평소처럼 막",
          "original_form": "뭔가 평소처럼 막",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010636.1.4",
          "form": "맛있다~ 이러면서 먹고 만족하기보단",
          "original_form": "맛있다~ 이러면서 먹고 만족하기보단",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010636.1.5",
          "form": "살려고 먹은 느낌이어서",
          "original_form": "살려고 먹은 느낌이어서",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010636.1.6",
          "form": "그런 느낌 처음이라 당혹스러웠음",
          "original_form": "그런 느낌 처음이라 당혹스러웠음",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010636.1.7",
          "form": "안돼ㅠㅠ",
          "original_form": "안돼ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010636.1.8",
          "form": "병원 한 번 가보자",
          "original_form": "병원 한 번 가보자",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010636.1.9",
          "form": "내일이나 내일모레 병원 가서 상처도 함 보고",
          "original_form": "내일이나 내일모레 병원 가서 상처도 함 보고",
          "speaker_id": "2",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010636.1.10",
          "form": "지금 상태 그대로면 얘기 해봐야겠다",
          "original_form": "지금 상태 그대로면 얘기 해봐야겠다",
          "speaker_id": "2",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010636.1.11",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010636.1.12",
          "form": "갑자기 카페 가고 싶다",
          "original_form": "갑자기 카페 가고 싶다",
          "speaker_id": "2",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010636.1.13",
          "form": "시험 기간 이후로",
          "original_form": "시험 기간 이후로",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010636.1.14",
          "form": "안 간 거 같은데",
          "original_form": "안 간 거 같은데",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010636.1.15",
          "form": "요즘 커피도 안 마심",
          "original_form": "요즘 커피도 안 마심",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010636.1.16",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010636.1.17",
          "form": "음료 제대로 마신 지 오래된 ㄷ.ㅅ",
          "original_form": "음료 제대로 마신 지 오래된 ㄷ.ㅅ",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010636.1.18",
          "form": "빵 먹고 싶다",
          "original_form": "빵 먹고 싶다",
          "speaker_id": "1",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010636.1.19",
          "form": "블루베리 크림 치즈 베이글...",
          "original_form": "블루베리 크림 치즈 베이글...",
          "speaker_id": "1",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010636.1.20",
          "form": "요즘 자꾸 생각남",
          "original_form": "요즘 자꾸 생각남",
          "speaker_id": "1",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010636.1.21",
          "form": "나는 치즈케이크랑 마카롱",
          "original_form": "나는 치즈케이크랑 마카롱",
          "speaker_id": "2",
          "time": "20210824 00:20"
        },
        {
          "id": "MDRW2100010636.1.22",
          "form": "초코쉐이크조",
          "original_form": "초코쉐이크조",
          "speaker_id": "2",
          "time": "20210824 00:20"
        },
        {
          "id": "MDRW2100010636.1.23",
          "form": "그냥 달달한 거 다 때려붓고 싶어",
          "original_form": "그냥 달달한 거 다 때려붓고 싶어",
          "speaker_id": "2",
          "time": "20210824 00:20"
        },
        {
          "id": "MDRW2100010636.1.24",
          "form": "안 먹은지 너무 오래됐어",
          "original_form": "안 먹은지 너무 오래됐어",
          "speaker_id": "1",
          "time": "20210824 07:07"
        },
        {
          "id": "MDRW2100010636.1.25",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100010636.1.26",
          "form": "내일 마라탕 먹자",
          "original_form": "내일 마라탕 먹자",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100010636.1.27",
          "form": "아님 머 먹고 싶은 거 있어?",
          "original_form": "아님 머 먹고 싶은 거 있어?",
          "speaker_id": "1",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100010636.1.28",
          "form": "밥 먹이기 프로젝트-",
          "original_form": "밥 먹이기 프로젝트-{emoji:☆}",
          "speaker_id": "1",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100010636.1.29",
          "form": "오늘은 뭐 좀 먹었어?",
          "original_form": "오늘은 뭐 좀 먹었어?",
          "speaker_id": "1",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100010636.1.30",
          "form": "마라탕 ㅎ",
          "original_form": "마라탕 ㅎ",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100010636.1.31",
          "form": "꿔바로우 먹어봄?",
          "original_form": "꿔바로우 먹어봄?",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100010636.1.32",
          "form": "가끔 먹어",
          "original_form": "가끔 먹어",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100010636.1.33",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100010636.1.34",
          "form": "나도 가끔",
          "original_form": "나도 가끔",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100010636.1.35",
          "form": "좋아해?",
          "original_form": "좋아해?",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100010636.1.36",
          "form": "응 맛있어",
          "original_form": "응 맛있어",
          "speaker_id": "2",
          "time": "20210825 15:46"
        }
      ]
