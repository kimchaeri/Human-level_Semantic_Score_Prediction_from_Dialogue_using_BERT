        },
        {
          "id": "MDRW2100003188.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 02:08"
        },
        {
          "id": "MDRW2100003188.1.3",
          "form": "반려동물을 키워보신적 있으신가요?",
          "original_form": "반려동물을 키워보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 02:21"
        },
        {
          "id": "MDRW2100003188.1.4",
          "form": "아니요 키울자신이 없어서 안키워봤어요",
          "original_form": "아니요 키울자신이 없어서 안키워봤어요",
          "speaker_id": "2",
          "time": "20210821 02:22"
        },
        {
          "id": "MDRW2100003188.1.5",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "2",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003188.1.6",
          "form": "네 저는 햄스터 키우고있어요",
          "original_form": "네 저는 햄스터 키우고있어요",
          "speaker_id": "1",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003188.1.7",
          "form": "와~ 시끄럽지않으세요?",
          "original_form": "와~ 시끄럽지않으세요?",
          "speaker_id": "2",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003188.1.8",
          "form": "다른 방에 둬서 괜찮아요",
          "original_form": "다른 방에 둬서 괜찮아요",
          "speaker_id": "1",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003188.1.9",
          "form": "딱히 우는것도 아니라 시끄럽진 않아요",
          "original_form": "딱히 우는것도 아니라 시끄럽진 않아요",
          "speaker_id": "1",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003188.1.10",
          "form": "챗바퀴 막 굴리지않아요?",
          "original_form": "챗바퀴 막 굴리지않아요?",
          "speaker_id": "2",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003188.1.11",
          "form": "아...다른방이면 좀 낫겠네요",
          "original_form": "아...다른방이면 좀 낫겠네요",
          "speaker_id": "2",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003188.1.12",
          "form": "귀엽긴하죠 저는 눈팅으로만 ㅎㅎ",
          "original_form": "귀엽긴하죠 저는 눈팅으로만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 02:25"
        },
        {
          "id": "MDRW2100003188.1.13",
          "form": "쳇바퀴 소리도 그렇게 크지 않더라구요",
          "original_form": "쳇바퀴 소리도 그렇게 크지 않더라구요",
          "speaker_id": "1",
          "time": "20210821 02:25"
        },
        {
          "id": "MDRW2100003188.1.14",
          "form": "전에도 키워보신적 없으세요?",
          "original_form": "전에도 키워보신적 없으세요?",
          "speaker_id": "1",
          "time": "20210821 02:25"
        },
        {
          "id": "MDRW2100003188.1.15",
          "form": "네 저는 멀 키우기가 귀찮을것같아서 한번도 안해봤어요",
          "original_form": "네 저는 멀 키우기가 귀찮을것같아서 한번도 안해봤어요",
          "speaker_id": "2",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003188.1.16",
          "form": "강아지 키우는 친구보니 거으 애같이 키우더라구요",
          "original_form": "강아지 키우는 친구보니 거으 애같이 키우더라구요",
          "speaker_id": "2",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003188.1.17",
          "form": "와 저도 강아지 키워보고 싶었어요",
          "original_form": "와 저도 강아지 키워보고 싶었어요",
          "speaker_id": "1",
          "time": "20210821 02:27"
        },
        {
          "id": "MDRW2100003188.1.18",
          "form": "아무래도 조금 귀찮긴 하죠",
          "original_form": "아무래도 조금 귀찮긴 하죠",
          "speaker_id": "1",
          "time": "20210821 02:27"
        },
        {
          "id": "MDRW2100003188.1.19",
          "form": "집에오면 그렇게 반겨준다고 그애교에 안키울수가 없다고 그러시더라구요",
          "original_form": "집에오면 그렇게 반겨준다고 그애교에 안키울수가 없다고 그러시더라구요",
          "speaker_id": "2",
          "time": "20210821 02:27"
        },
        {
          "id": "MDRW2100003188.1.20",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003188.1.21",
          "form": "강아지 너무 키워보고 싶은데",
          "original_form": "강아지 너무 키워보고 싶은데",
          "speaker_id": "1",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003188.1.22",
          "form": "부모님이랑 살아서 못키워봤어요",
          "original_form": "부모님이랑 살아서 못키워봤어요",
          "speaker_id": "1",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003188.1.23",
          "form": "저는 식물도 못키워요 제손에 오면 시들어서 가더라구요 ㅎㅎ",
          "original_form": "저는 식물도 못키워요 제손에 오면 시들어서 가더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003188.1.24",
          "form": "행스터는 승락하신거예요?",
          "original_form": "행스터는 승락하신거예요?",
          "speaker_id": "2",
          "time": "20210821 02:29"
        },
        {
          "id": "MDRW2100003188.1.25",
          "form": "네 원래 햄스터도 안좋아하셨는데",
          "original_form": "네 원래 햄스터도 안좋아하셨는데",
          "speaker_id": "1",
          "time": "20210821 02:29"
        },
        {
          "id": "MDRW2100003188.1.26",
          "form": "어릴때부터 졸라서 키우고 있어요ㅋㅋ",
          "original_form": "어릴때부터 졸라서 키우고 있어요ㅋㅋ",
