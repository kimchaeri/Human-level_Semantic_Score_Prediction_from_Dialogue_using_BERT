        {
          "id": "MDRW2100002902.1.1",
          "form": "공연 전시회 좋아하세요?",
          "original_form": "공연 전시회 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002902.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002902.1.3",
          "form": "저도 좋아하는데",
          "original_form": "저도 좋아하는데",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002902.1.4",
          "form": "요즘 코로나때문에...",
          "original_form": "요즘 코로나때문에...",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002902.1.5",
          "form": "코로나",
          "original_form": "코로나",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002902.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002902.1.7",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002902.1.8",
          "form": "예전에 전시회 공연 보러 자주 다녔었는데",
          "original_form": "예전에 전시회 공연 보러 자주 다녔었는데",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002902.1.9",
          "form": "요즘엔 영화관도 못가요",
          "original_form": "요즘엔 영화관도 못가요",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002902.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002902.1.11",
          "form": "빛의 화가 그 전시회 봤었는데",
          "original_form": "빛의 화가 그 전시회 봤었는데",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002902.1.12",
          "form": "누구요? 렘브란트?",
          "original_form": "누구요? 렘브란트?",
          "speaker_id": "1",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002902.1.13",
          "form": "모네요",
          "original_form": "모네요",
          "speaker_id": "2",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002902.1.14",
          "form": "오 미술 전시 좋아하시구나",
          "original_form": "오 미술 전시 좋아하시구나",
          "speaker_id": "1",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002902.1.15",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002902.1.16",
          "form": "전시는 안가리고 좋아하긴해요",
          "original_form": "전시는 안가리고 좋아하긴해요",
          "speaker_id": "2",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002902.1.17",
          "form": "저도 장르는 딱히 가리지 않네요",
          "original_form": "저도 장르는 딱히 가리지 않네요",
          "speaker_id": "1",
          "time": "20210820 16:38"
        }
