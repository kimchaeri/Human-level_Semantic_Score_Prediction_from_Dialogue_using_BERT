      "utterance": [
        {
          "id": "MDRW2100040205.1.1",
          "form": "신랑왔나??",
          "original_form": "신랑왔나??",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100040205.1.2",
          "form": "놀이터에서 만날래?? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "놀이터에서 만날래?? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100040205.1.3",
          "form": "원은 친구만나러 나가서",
          "original_form": "원은 친구만나러 나가서",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100040205.1.4",
          "form": "투가 심심하다해가 나가볼까 생각중 ㅎㅎ",
          "original_form": "투가 심심하다해가 나가볼까 생각중 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100040205.1.5",
          "form": "나 잠깐 잠들었었다",
          "original_form": "나 잠깐 잠들었었다",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040205.1.6",
          "form": "지금 둘째 낮잠",
          "original_form": "지금 둘째 낮잠",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040205.1.7",
          "form": "ㅋㅋㅋㅋㄱ 너무 피곤하네  ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱ 너무 피곤하네  ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040205.1.8",
          "form": "이시간에 낮잠자서 우짜노",
          "original_form": "이시간에 낮잠자서 우짜노",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100040205.1.9",
          "form": "오늘도....퇴근이 늦겠구먼",
          "original_form": "오늘도....퇴근이 늦겠구먼",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100040205.1.10",
          "form": "그라게 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그라게 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100040205.1.11",
          "form": "오늘도 늦었다",
          "original_form": "오늘도 늦었다",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100040205.1.12",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100040205.1.13",
          "form": "그래 ㅋㅋㅋㅋ 낮잠이 너무 늦었네 ㅋㅋ",
          "original_form": "그래 ㅋㅋㅋㅋ 낮잠이 너무 늦었네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100040205.1.14",
          "form": "우리 원은",
          "original_form": "우리 원은",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100040205.1.15",
          "form": "같은반 친구중에 절교했던 친구있었다고",
          "original_form": "같은반 친구중에 절교했던 친구있었다고",
          "speaker_id": "1",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100040205.1.16",
          "form": "내가 얘기 했었나??",
          "original_form": "내가 얘기 했었나??",
          "speaker_id": "1",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100040205.1.17",
          "form": "그 친구랑 이학기 돼서 화해해서",
          "original_form": "그 친구랑 이학기 돼서 화해해서",
          "speaker_id": "1",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100040205.1.18",
          "form": "오늘 만나러 나갔다",
          "original_form": "오늘 만나러 나갔다",
          "speaker_id": "1",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100040205.1.19",
          "form": "아니 도 알지 힐데스에 name3 미소얼집",
          "original_form": "아니 도 알지 힐데스에 &name3& 미소얼집",
          "speaker_id": "1",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100040205.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100040205.1.21",
          "form": "아 name3이알지ㅋㅋ",
          "original_form": "아 &name3&이알지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100040205.1.22",
          "form": "걔랑 절교했었나",
          "original_form": "걔랑 절교했었나",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100040205.1.23",
          "form": "웃기노ㅋㄱㅋ",
          "original_form": "웃기노ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100040205.1.24",
          "form": "어후 말도 마라 정말 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어후 말도 마라 정말 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040205.1.25",
          "form": "1학기때 매주 만나서 한 4번 놀다가",
          "original_form": "1학기때 매주 만나서 한 4번 놀다가",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040205.1.26",
          "form": "그뒤로 name3이가 절교선언해서",
          "original_form": "그뒤로 &name3&이가 절교선언해서",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040205.1.27",
          "form": "한참 절교하다가",
          "original_form": "한참 절교하다가",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040205.1.28",
          "form": "개학해서 name4이가 화해신청해서 다시 논다",
          "original_form": "개학해서 &name4&이가 화해신청해서 다시 논다",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040205.1.29",
          "form": "아구 그렇구나ㅜ",
          "original_form": "아구 그렇구나ㅜ",
          "speaker_id": "2",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100040205.1.30",
          "form": "절교선언이라니ㅜㅜ",
          "original_form": "절교선언이라니ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100040205.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100040205.1.32",
          "form": "그래두 화해해서 다행이네ㅋㅋ",
          "original_form": "그래두 화해해서 다행이네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100040205.1.33",
          "form": "ㄱㅋㅋㄱ 그래 절교라니...",
          "original_form": "ㄱㅋㅋㄱ 그래 절교라니...",
          "speaker_id": "1",
          "time": "20210904 20:51"
        },
        {
          "id": "MDRW2100040205.1.34",
          "form": "4달동안 말 안했다 ㅋㅋㅋㅋㄱ",
          "original_form": "4달동안 말 안했다 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210904 20:51"
        },
        {
          "id": "MDRW2100040205.1.35",
          "form": "대단하다ㅋㅋㅋ와",
          "original_form": "대단하다ㅋㅋㅋ와",
          "speaker_id": "2",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100040205.1.36",
          "form": "장난아니제??",
          "original_form": "장난아니제??",
          "speaker_id": "1",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100040205.1.37",
          "form": "난 name3이가  절교하자 고 했다고 해서",
          "original_form": "난 &name3&이가  절교하자 고 했다고 해서",
          "speaker_id": "1",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100040205.1.38",
          "form": "다음날 되면 얘기할 줄 알았는데",
          "original_form": "다음날 되면 얘기할 줄 알았는데",
          "speaker_id": "1",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100040205.1.39",
          "form": "네달 동안 말을 안 했어",
          "original_form": "네달 동안 말을 안 했어",
          "speaker_id": "1",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100040205.1.40",
          "form": "그렇네ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇네ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100040205.1.41",
          "form": "둘 다 장난아니다ㅋㅋㅋㅋㅋㄱ",
          "original_form": "둘 다 장난아니다ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100040205.1.42",
          "form": "중간에 name4이가 그만하자했는데 계속 말안한다했다대",
          "original_form": "중간에 &name4&이가 그만하자했는데 계속 말안한다했다대",
          "speaker_id": "1",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040205.1.43",
          "form": "와 name3이도 와 ㅋㅋㅋ",
          "original_form": "와 &name3&이도 와 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100040205.1.44",
          "form": "왜그러는지 이유를 얘기안해줘서",
          "original_form": "왜그러는지 이유를 얘기안해줘서",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100040205.1.45",
          "form": "성격장난아닌가보다ㅋ",
          "original_form": "성격장난아닌가보다ㅋ",
