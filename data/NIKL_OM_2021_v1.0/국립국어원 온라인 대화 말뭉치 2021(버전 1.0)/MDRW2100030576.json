      "utterance": [
        {
          "id": "MDRW2100030576.1.1",
          "form": "혹시 자영업자이신가요?",
          "original_form": "혹시 자영업자이신가요?",
          "speaker_id": "2",
          "time": "20210904 09:26"
        },
        {
          "id": "MDRW2100030576.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210904 09:26"
        },
        {
          "id": "MDRW2100030576.1.3",
          "form": "주변에친한언니네가 자영업자요",
          "original_form": "주변에친한언니네가 자영업자요",
          "speaker_id": "1",
          "time": "20210904 09:26"
        },
        {
          "id": "MDRW2100030576.1.4",
          "form": "고용한파어떻게 생각하시나요?",
          "original_form": "고용한파어떻게 생각하시나요?",
          "speaker_id": "2",
          "time": "20210904 09:27"
        },
        {
          "id": "MDRW2100030576.1.5",
          "form": "인건비가 넘비싸서",
          "original_form": "인건비가 넘비싸서",
          "speaker_id": "1",
          "time": "20210904 09:27"
        },
        {
          "id": "MDRW2100030576.1.6",
          "form": "덜팔고 사람안쓴다고하더군요",
          "original_form": "덜팔고 사람안쓴다고하더군요",
          "speaker_id": "1",
          "time": "20210904 09:27"
        },
        {
          "id": "MDRW2100030576.1.7",
          "form": "인건비가 비싸지니 파는음식값도 엄청오르는듯해요",
          "original_form": "인건비가 비싸지니 파는음식값도 엄청오르는듯해요",
          "speaker_id": "2",
          "time": "20210904 09:27"
        },
        {
          "id": "MDRW2100030576.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 09:28"
        },
        {
          "id": "MDRW2100030576.1.9",
          "form": "그러다보니 사람을 안쓰고",
          "original_form": "그러다보니 사람을 안쓰고",
          "speaker_id": "1",
          "time": "20210904 09:28"
        },
        {
          "id": "MDRW2100030576.1.10",
          "form": "인건비가 오른다고 다좋은건아닌듯하더군요",
          "original_form": "인건비가 오른다고 다좋은건아닌듯하더군요",
          "speaker_id": "2",
          "time": "20210904 09:28"
        },
        {
          "id": "MDRW2100030576.1.11",
          "form": "옛날엔 오천원이면 밥한끼 사먹었는데",
          "original_form": "옛날엔 오천원이면 밥한끼 사먹었는데",
          "speaker_id": "1",
          "time": "20210904 09:28"
        },
        {
          "id": "MDRW2100030576.1.12",
          "form": "지금은 오천원짜리 찾기가 어려워요",
          "original_form": "지금은 오천원짜리 찾기가 어려워요",
          "speaker_id": "1",
          "time": "20210904 09:29"
        },
        {
          "id": "MDRW2100030576.1.13",
          "form": "착한음식점이죠",
          "original_form": "착한음식점이죠",
          "speaker_id": "2",
          "time": "20210904 09:29"
        },
        {
          "id": "MDRW2100030576.1.14",
          "form": "그정도가격이면",
          "original_form": "그정도가격이면",
          "speaker_id": "2",
          "time": "20210904 09:29"
        },
        {
          "id": "MDRW2100030576.1.15",
          "form": "수입보다 지출이많아지면 일 안하는게 낫다고 생각하는것같아요",
          "original_form": "수입보다 지출이많아지면 일 안하는게 낫다고 생각하는것같아요",
          "speaker_id": "1",
          "time": "20210904 09:30"
        },
        {
          "id": "MDRW2100030576.1.16",
          "form": "인건비주고나면 남는게없으니",
          "original_form": "인건비주고나면 남는게없으니",
          "speaker_id": "1",
          "time": "20210904 09:30"
        },
        {
          "id": "MDRW2100030576.1.17",
          "form": "요즘시대 자영업자진짜힘들듯하더라구요",
          "original_form": "요즘시대 자영업자진짜힘들듯하더라구요",
          "speaker_id": "2",
          "time": "20210904 09:31"
        },
        {
          "id": "MDRW2100030576.1.18",
          "form": "가장 많이줄일수있는게 인건비인듯",
          "original_form": "가장 많이줄일수있는게 인건비인듯",
          "speaker_id": "1",
          "time": "20210904 09:31"
        }
