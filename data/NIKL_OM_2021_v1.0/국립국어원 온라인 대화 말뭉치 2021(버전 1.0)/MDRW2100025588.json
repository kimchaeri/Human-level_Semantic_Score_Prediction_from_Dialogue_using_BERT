      "utterance": [
        {
          "id": "MDRW2100025588.1.1",
          "form": "요즘 사고싶은게있나요??",
          "original_form": "요즘 사고싶은게있나요??",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025588.1.2",
          "form": "지금은 집에 있는 물건 처분하는게 목적이라 새로운걸 사고 싶지 않았어요",
          "original_form": "지금은 집에 있는 물건 처분하는게 목적이라 새로운걸 사고 싶지 않았어요",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025588.1.3",
          "form": "멀 처분하고싶나요!",
          "original_form": "멀 처분하고싶나요!",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025588.1.4",
          "form": "오래돼서 전 안쓰지만 필요한 분들이 있을까봐 그런것들 올려요",
          "original_form": "오래돼서 전 안쓰지만 필요한 분들이 있을까봐 그런것들 올려요",
          "speaker_id": "2",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025588.1.5",
          "form": "저도 중고거래 마니 이용하는거같아요",
          "original_form": "저도 중고거래 마니 이용하는거같아요",
          "speaker_id": "1",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025588.1.6",
          "form": "예를 들면 카세트테이프 같은 거요",
          "original_form": "예를 들면 카세트테이프 같은 거요",
          "speaker_id": "2",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025588.1.7",
          "form": "그런건  잘팔리나요??",
          "original_form": "그런건  잘팔리나요??",
          "speaker_id": "1",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100025588.1.8",
          "form": "요새 못구하다 보니까 오히리 잘나가요",
          "original_form": "요새 못구하다 보니까 오히리 잘나가요",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025588.1.9",
          "form": "수요가 있더라구요",
          "original_form": "수요가 있더라구요",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025588.1.10",
          "form": "다행이네요!!!ㅋㅋ",
          "original_form": "다행이네요!!!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025588.1.11",
          "form": "저도 책같은거 팔고싶긴한데",
          "original_form": "저도 책같은거 팔고싶긴한데",
          "speaker_id": "1",
          "time": "20210901 14:55"
        },
        {
          "id": "MDRW2100025588.1.12",
          "form": "팔릴련지..",
          "original_form": "팔릴련지..",
          "speaker_id": "1",
          "time": "20210901 14:55"
        },
        {
          "id": "MDRW2100025588.1.13",
          "form": "책도 팔았어요",
          "original_form": "책도 팔았어요",
          "speaker_id": "2",
          "time": "20210901 14:55"
        },
        {
          "id": "MDRW2100025588.1.14",
          "form": "새책 안읽는거 내놨어요",
          "original_form": "새책 안읽는거 내놨어요",
          "speaker_id": "2",
          "time": "20210901 14:55"
        },
        {
          "id": "MDRW2100025588.1.15",
          "form": "그런것도 잘팔리나바요",
          "original_form": "그런것도 잘팔리나바요",
          "speaker_id": "1",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100025588.1.16",
          "form": "잘 팔리는 정도는 아닌데 팔렸어요..ㅎㅎ",
          "original_form": "잘 팔리는 정도는 아닌데 팔렸어요..ㅎㅎ",
