      "utterance": [
        {
          "id": "MDRW2100007178.1.1",
          "form": "사실 그만큼은 못먹어요허세였어요",
          "original_form": "사실 그만큼은 못먹어요허세였어요",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007178.1.2",
          "form": "음료는 뭐 좋아하시나요",
          "original_form": "음료는 뭐 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007178.1.3",
          "form": "그거 김종국이랑 하하도 실패한 챌린지 ㅋㅋ",
          "original_form": "그거 김종국이랑 하하도 실패한 챌린지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007178.1.4",
          "form": "음료는 쿨피스",
          "original_form": "음료는 쿨피스",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007178.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007178.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ쿨피스!! 떡볶이라",
          "original_form": "ㅋㅋㅋㅋㅋㅋ쿨피스!! 떡볶이라",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007178.1.7",
          "form": "커피 ㅋㅋㅋ",
          "original_form": "커피 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007178.1.8",
          "form": "랑 먹어야되는데",
          "original_form": "랑 먹어야되는데",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007178.1.9",
          "form": "엽떡의 단짝",
          "original_form": "엽떡의 단짝",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007178.1.10",
          "form": "신전떡볶이에서주는 쿨피스좋아해요",
          "original_form": "신전떡볶이에서주는 쿨피스좋아해요",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007178.1.11",
          "form": "엽떡 침고이네요",
          "original_form": "엽떡 침고이네요",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007178.1.12",
          "form": "엽떡이 너무 먹고 싶네요 요즘",
          "original_form": "엽떡이 너무 먹고 싶네요 요즘",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007178.1.13",
          "form": "엽떡 몇단계 가능 하세요?",
          "original_form": "엽떡 몇단계 가능 하세요?",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007178.1.14",
          "form": "엽떡류좋아하시면 응급실떡볶이드셔보셨어요?",
          "original_form": "엽떡류좋아하시면 응급실떡볶이드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007178.1.15",
          "form": "응떡은 아직 ㅠㅠㅠ",
          "original_form": "응떡은 아직 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007178.1.16",
          "form": "저 무난하게 2~3단계시켜요",
          "original_form": "저 무난하게 2~3단계시켜요",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007178.1.17",
          "form": "응떡으로 갈아탔어요",
          "original_form": "응떡으로 갈아탔어요",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007178.1.18",
          "form": "장담합니다",
          "original_form": "장담합니다",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007178.1.19",
          "form": "응떡 엽떡중 모가 아 그래요?",
          "original_form": "응떡 엽떡중 모가 아 그래요?",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007178.1.20",
          "form": "다음은 응떡입니다",
          "original_form": "다음은 응떡입니다",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007178.1.21",
          "form": "맵기는 비슷한가요??",
          "original_form": "맵기는 비슷한가요??",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007178.1.22",
          "form": "응떡이 맵기는 비슷한데",
          "original_form": "응떡이 맵기는 비슷한데",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007178.1.23",
          "form": "치즈가진짜진짜맛있고",
          "original_form": "치즈가진짜진짜맛있고",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007178.1.24",
          "form": "전 3은 좀 맵고 2가 맞는데",
          "original_form": "전 3은 좀 맵고 2가 맞는데",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007178.1.25",
          "form": "응떡 2 시키시면딱",
          "original_form": "응떡 2 시키시면딱",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007178.1.26",
          "form": "맞으실거같아요",
          "original_form": "맞으실거같아요",
