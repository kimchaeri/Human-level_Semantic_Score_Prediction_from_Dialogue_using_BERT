      "utterance": [
        {
          "id": "MDRW2100018378.1.1",
          "form": "혹시 지금 연애중이신가요?",
          "original_form": "혹시 지금 연애중이신가요?",
          "speaker_id": "2",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018378.1.2",
          "form": "아뇨ㅠㅠ 연애중이세요?",
          "original_form": "아뇨ㅠㅠ 연애중이세요?",
          "speaker_id": "1",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018378.1.3",
          "form": "저도 아니요ㅋㅋㅋㅋ",
          "original_form": "저도 아니요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018378.1.4",
          "form": "연애안한지 너무 오래됐네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "연애안한지 너무 오래됐네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018378.1.5",
          "form": "마지막 연애가 언제세요?",
          "original_form": "마지막 연애가 언제세요?",
          "speaker_id": "1",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018378.1.6",
          "form": "전 사실 한번도 안해봤어요ㅠㅠ",
          "original_form": "전 사실 한번도 안해봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018378.1.7",
          "form": "앗그렇구나",
          "original_form": "앗그렇구나",
          "speaker_id": "1",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018378.1.8",
          "form": "전 앞으로 안하려구요ㅋㅋㅋㅋㅋㅋㅠ",
          "original_form": "전 앞으로 안하려구요ㅋㅋㅋㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018378.1.9",
          "form": "저도 근데 꼭 하고싶지는 않아요ㅋㅋㅋ",
          "original_form": "저도 근데 꼭 하고싶지는 않아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:42"
        },
        {
          "id": "MDRW2100018378.1.10",
          "form": "한 번쯤 해보면 좋은 경험이 될거라고 생각하지만 꼭 해야겠다는 생각은 없어요",
          "original_form": "한 번쯤 해보면 좋은 경험이 될거라고 생각하지만 꼭 해야겠다는 생각은 없어요",
          "speaker_id": "2",
          "time": "20210827 00:42"
        },
        {
          "id": "MDRW2100018378.1.11",
          "form": "맞아요 연애하는 거보다 다른게 더 재밌어요",
          "original_form": "맞아요 연애하는 거보다 다른게 더 재밌어요",
          "speaker_id": "1",
          "time": "20210827 00:43"
        },
        {
          "id": "MDRW2100018378.1.12",
          "form": "이상형은 어떤 사람이에요?",
          "original_form": "이상형은 어떤 사람이에요?",
          "speaker_id": "1",
          "time": "20210827 00:43"
        },
        {
          "id": "MDRW2100018378.1.13",
          "form": "맞아요. 저는 주변에 비혼주의자인 친구들도 많은 편이라서 친구들과 아직 더 놀고싶어요",
          "original_form": "맞아요. 저는 주변에 비혼주의자인 친구들도 많은 편이라서 친구들과 아직 더 놀고싶어요",
          "speaker_id": "2",
          "time": "20210827 00:43"
        },
        {
          "id": "MDRW2100018378.1.14",
          "form": "저는 당연한말이지만 여성을 존중하는 사람이요.",
          "original_form": "저는 당연한말이지만 여성을 존중하는 사람이요.",
          "speaker_id": "2",
          "time": "20210827 00:44"
        },
        {
          "id": "MDRW2100018378.1.15",
          "form": "아 맞아요 ㅠㅠ 제일 기본인데 기본이 안된 사람들이 많죠",
          "original_form": "아 맞아요 ㅠㅠ 제일 기본인데 기본이 안된 사람들이 많죠",
          "speaker_id": "1",
          "time": "20210827 00:45"
        },
        {
          "id": "MDRW2100018378.1.16",
          "form": "그리고 저한테 원하는 게 많은 사람보다는 해주고싶어하는 게 많은 사람이었으면 좋겠어요.",
          "original_form": "그리고 저한테 원하는 게 많은 사람보다는 해주고싶어하는 게 많은 사람이었으면 좋겠어요.",
          "speaker_id": "2",
          "time": "20210827 00:45"
        },
        {
          "id": "MDRW2100018378.1.17",
          "form": "name1의 이상형은 어떤가요??",
          "original_form": "&name1&의 이상형은 어떤가요??",
          "speaker_id": "2",
          "time": "20210827 00:46"
        },
        {
          "id": "MDRW2100018378.1.18",
          "form": "저도 name2이랑 비슷한데",
          "original_form": "저도 &name2&이랑 비슷한데",
          "speaker_id": "1",
          "time": "20210827 00:46"
        },
        {
          "id": "MDRW2100018378.1.19",
          "form": "잘생기고 키가 크고 돈을 부족함없이 벌며 성격에 모가나지 않았으면 좋겠어요ㅎㅎㅎ",
          "original_form": "잘생기고 키가 크고 돈을 부족함없이 벌며 성격에 모가나지 않았으면 좋겠어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:47"
        },
        {
          "id": "MDRW2100018378.1.20",
          "form": "이상형이니까!",
          "original_form": "이상형이니까!",
          "speaker_id": "1",
          "time": "20210827 00:47"
        },
        {
          "id": "MDRW2100018378.1.21",
          "form": "어후 다 맞는 말이에요. 너무 공감되네요.",
          "original_form": "어후 다 맞는 말이에요. 너무 공감되네요.",
          "speaker_id": "2",
          "time": "20210827 00:48"
        },
        {
          "id": "MDRW2100018378.1.22",
          "form": "현실엔 잘 없다는게 슬프네요ㅠㅠㅠㅋ",
          "original_form": "현실엔 잘 없다는게 슬프네요ㅠㅠㅠㅋ",
          "speaker_id": "1",
          "time": "20210827 00:49"
        }
      ]
