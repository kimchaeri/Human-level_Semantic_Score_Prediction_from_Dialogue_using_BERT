      "utterance": [
        {
          "id": "MDRW2100002240.1.1",
          "form": "봉사...",
          "original_form": "봉사...",
          "speaker_id": "2",
          "time": "20210820 07:51"
        },
        {
          "id": "MDRW2100002240.1.2",
          "form": "종교활동을 해보셨나요",
          "original_form": "종교활동을 해보셨나요",
          "speaker_id": "1",
          "time": "20210820 08:19"
        },
        {
          "id": "MDRW2100002240.1.3",
          "form": "아니요 전 무교에요!",
          "original_form": "아니요 전 무교에요!",
          "speaker_id": "2",
          "time": "20210820 08:26"
        },
        {
          "id": "MDRW2100002240.1.4",
          "form": "어릴 때 교회 조금 다는 거 말고는 ㅎ",
          "original_form": "어릴 때 교회 조금 다는 거 말고는 ㅎ",
          "speaker_id": "2",
          "time": "20210820 08:26"
        },
        {
          "id": "MDRW2100002240.1.5",
          "form": "해보셨나요?",
          "original_form": "해보셨나요?",
          "speaker_id": "2",
          "time": "20210820 08:26"
        },
        {
          "id": "MDRW2100002240.1.6",
          "form": "저도 무교에요 종교를 가지지않는 이유가 있나요?",
          "original_form": "저도 무교에요 종교를 가지지않는 이유가 있나요?",
          "speaker_id": "1",
          "time": "20210820 08:26"
        },
        {
          "id": "MDRW2100002240.1.7",
          "form": "저는 종교를 가지려면 좀 부지런해야된다고 생각하는데",
          "original_form": "저는 종교를 가지려면 좀 부지런해야된다고 생각하는데",
          "speaker_id": "2",
          "time": "20210820 08:27"
        },
        {
          "id": "MDRW2100002240.1.8",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "1",
          "time": "20210820 08:27"
        },
        {
          "id": "MDRW2100002240.1.9",
          "form": "제가 그렇지 못해서요ㅋㅋㅋ ㅋㅋㅋ 매일 나갈 수가 없어요",
          "original_form": "제가 그렇지 못해서요ㅋㅋㅋ ㅋㅋㅋ 매일 나갈 수가 없어요",
          "speaker_id": "2",
          "time": "20210820 08:27"
        },
        {
          "id": "MDRW2100002240.1.10",
          "form": "가족들도 다 무교세요?",
          "original_form": "가족들도 다 무교세요?",
          "speaker_id": "2",
          "time": "20210820 08:27"
        },
        {
          "id": "MDRW2100002240.1.11",
          "form": "네 신이라는 존재를 믿지 않아서ㅎㅎ",
          "original_form": "네 신이라는 존재를 믿지 않아서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 08:28"
        },
        {
          "id": "MDRW2100002240.1.12",
          "form": "약간 눈에 안보이는 허상..? 같은느낌이라",
          "original_form": "약간 눈에 안보이는 허상..? 같은느낌이라",
          "speaker_id": "1",
          "time": "20210820 08:28"
        },
        {
          "id": "MDRW2100002240.1.13",
          "form": "오 저희 가족도",
          "original_form": "오 저희 가족도",
          "speaker_id": "2",
          "time": "20210820 08:28"
        },
        {
          "id": "MDRW2100002240.1.14",
          "form": "믿는 사람이 다 없어요 공감해요",
          "original_form": "믿는 사람이 다 없어요 공감해요",
          "speaker_id": "2",
          "time": "20210820 08:28"
        },
        {
          "id": "MDRW2100002240.1.15",
          "form": "그리고 음.. 약간 강압적인 믿음",
          "original_form": "그리고 음.. 약간 강압적인 믿음",
          "speaker_id": "1",
          "time": "20210820 08:28"
        },
        {
          "id": "MDRW2100002240.1.16",
          "form": "뭔지아시죠?",
          "original_form": "뭔지아시죠?",
          "speaker_id": "1",
          "time": "20210820 08:28"
        },
        {
          "id": "MDRW2100002240.1.17",
          "form": "네 알죠 ㅋㅋㅋㅋ 그리고 전 저 하나 보기에도 바쁜데",
          "original_form": "네 알죠 ㅋㅋㅋㅋ 그리고 전 저 하나 보기에도 바쁜데",
          "speaker_id": "2",
          "time": "20210820 08:29"
        },
        {
          "id": "MDRW2100002240.1.18",
          "form": "종교까지하면 어우..",
          "original_form": "종교까지하면 어우..",
          "speaker_id": "2",
          "time": "20210820 08:29"
        },
        {
          "id": "MDRW2100002240.1.19",
          "form": "아 알아요ㅋㅋ",
          "original_form": "아 알아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:29"
        },
        {
          "id": "MDRW2100002240.1.20",
          "form": "근데 우리나라에 무교도 많지만 종교가진 사람들이 은근 많더라구요",
          "original_form": "근데 우리나라에 무교도 많지만 종교가진 사람들이 은근 많더라구요",
          "speaker_id": "2",
          "time": "20210820 08:30"
        },
