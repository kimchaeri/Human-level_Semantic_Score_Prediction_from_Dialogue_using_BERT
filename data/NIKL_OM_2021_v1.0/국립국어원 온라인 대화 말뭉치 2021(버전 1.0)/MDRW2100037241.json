      "utterance": [
        {
          "id": "MDRW2100037241.1.1",
          "form": "바이럴 마케팅 엄청 했나봐요",
          "original_form": "바이럴 마케팅 엄청 했나봐요",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100037241.1.2",
          "form": "그리고 연예인 모델값",
          "original_form": "그리고 연예인 모델값",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100037241.1.3",
          "form": "그러게요 하다못해 이 가격이면 건기식이라도 돼야지ㅂㄷㅂㄷ",
          "original_form": "그러게요 하다못해 이 가격이면 건기식이라도 돼야지ㅂㄷㅂㄷ",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037241.1.4",
          "form": "동생은 이제 안마시나요 아예?",
          "original_form": "동생은 이제 안마시나요 아예?",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037241.1.5",
          "form": "10개 네이버 광고보고 샀는데",
          "original_form": "10개 네이버 광고보고 샀는데",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037241.1.6",
          "form": "택배상자 뜯자마자",
          "original_form": "택배상자 뜯자마자",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037241.1.7",
          "form": "아 난 이것만 먹고는 못산다 싶더래요",
          "original_form": "아 난 이것만 먹고는 못산다 싶더래요",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037241.1.8",
          "form": "혼자 내가 박스를버려서 그런가 ㅋㅋ이러고",
          "original_form": "혼자 내가 박스를버려서 그런가 ㅋㅋ이러고",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037241.1.9",
          "form": "ㅎㅎ 그렇기는 하죠 자몽주스만 먹고 살라니ㅎㅎ",
          "original_form": "ㅎㅎ 그렇기는 하죠 자몽주스만 먹고 살라니ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037241.1.10",
          "form": "그럼 시도도 안해본 거네요ㅎ",
          "original_form": "그럼 시도도 안해본 거네요ㅎ",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037241.1.11",
          "form": "그래도 덕분인지 때문인지 name2님이 도전하게 됐으니",
          "original_form": "그래도 덕분인지 때문인지 &name2&님이 도전하게 됐으니",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100037241.1.12",
          "form": "운동은 같이 하나요?",
          "original_form": "운동은 같이 하나요?",
          "speaker_id": "1",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037241.1.13",
          "form": "자몽주스만 마심 기력이 없을 것 같은데",
          "original_form": "자몽주스만 마심 기력이 없을 것 같은데",
          "speaker_id": "1",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037241.1.14",
          "form": "그 홈피에는 운동도하라던데 전 하루종일 짜증나고",
          "original_form": "그 홈피에는 운동도하라던데 전 하루종일 짜증나고",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037241.1.15",
          "form": "힘이없달까? 막 그런상태여서",
          "original_form": "힘이없달까? 막 그런상태여서",
          "speaker_id": "2",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100037241.1.16",
          "form": "버스 두정류장 걸었네요 허허",
          "original_form": "버스 두정류장 걸었네요 허허",
          "speaker_id": "2",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100037241.1.17",
          "form": "헉. 극기훈련 수준이네요",
          "original_form": "헉. 극기훈련 수준이네요",
          "speaker_id": "1",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100037241.1.18",
          "form": "그래도 의지가 대단하세요",
          "original_form": "그래도 의지가 대단하세요",
          "speaker_id": "1",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100037241.1.19",
          "form": "근데 후기보니까 잘해야 삼키로빠졌대요",
          "original_form": "근데 후기보니까 잘해야 삼키로빠졌대요",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037241.1.20",
          "form": "일키로정도 빠지는게보통같고",
          "original_form": "일키로정도 빠지는게보통같고",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037241.1.21",
          "form": "이거 원래는 얼마정도 해야 하나요ㄷㅡ",
          "original_form": "이거 원래는 얼마정도 해야 하나요ㄷㅡ",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037241.1.22",
          "form": "이틀 마시고 하루 쉬고 이틀 마시고 하루 쉬고 그런 식인가요;;?",
          "original_form": "이틀 마시고 하루 쉬고 이틀 마시고 하루 쉬고 그런 식인가요;;?",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100037241.1.23",
          "form": "그럼 죽지않을까요",
          "original_form": "그럼 죽지않을까요",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100037241.1.24",
          "form": "프로그램이 3개인데",
          "original_form": "프로그램이 3개인데",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100037241.1.25",
          "form": "10개를 하루다섯개씩 48시간하는 프로그램",
          "original_form": "10개를 하루다섯개씩 48시간하는 프로그램",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100037241.1.26",
          "form": "아니ㅋㅋ사망엔딩",
          "original_form": "아니ㅋㅋ사망엔딩",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037241.1.27",
          "form": "아침저녁 대신하는 프로그램",
          "original_form": "아침저녁 대신하는 프로그램",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037241.1.28",
          "form": "이렇게 3개있어요",
          "original_form": "이렇게 3개있어요",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037241.1.29",
          "form": "아 지속적인 프로그램이 아니군요",
          "original_form": "아 지속적인 프로그램이 아니군요",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037241.1.30",
          "form": "이건 급하게 살뺄사람을 위한 거군요",
          "original_form": "이건 급하게 살뺄사람을 위한 거군요",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037241.1.31",
          "form": "한개 3700원인데 ㅋㅋ 지속하려먼",
          "original_form": "한개 3700원인데 ㅋㅋ 지속하려먼",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037241.1.32",
          "form": "전그돈으로 소고기 사먹고싶네요 ㅋㅋ",
          "original_form": "전그돈으로 소고기 사먹고싶네요 ㅋㅋ",
