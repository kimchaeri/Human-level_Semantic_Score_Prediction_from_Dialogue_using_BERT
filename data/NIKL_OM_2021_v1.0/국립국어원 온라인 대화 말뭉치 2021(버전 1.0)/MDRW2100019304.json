        },
        {
          "id": "MDRW2100019304.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019304.1.3",
          "form": "건강해?",
          "original_form": "건강해?",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019304.1.4",
          "form": "아들은 어때",
          "original_form": "아들은 어때",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019304.1.5",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019304.1.6",
          "form": "설거지 할게 산더미야",
          "original_form": "설거지 할게 산더미야",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019304.1.7",
          "form": "설거지 좀 해",
          "original_form": "설거지 좀 해",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019304.1.8",
          "form": "오케이",
          "original_form": "오케이",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019304.1.9",
          "form": "방바닥도 더러움",
          "original_form": "방바닥도 더러움",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019304.1.10",
          "form": "그러도록 하자",
          "original_form": "그러도록 하자",
          "speaker_id": "1",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100019304.1.11",
          "form": "장 봐야하는데",
          "original_form": "장 봐야하는데",
          "speaker_id": "1",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100019304.1.12",
          "form": "7시에 시간 돼?",
          "original_form": "7시에 시간 돼?",
          "speaker_id": "1",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100019304.1.13",
          "form": "아니 혼자 갔다와",
          "original_form": "아니 혼자 갔다와",
          "speaker_id": "2",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100019304.1.14",
          "form": "너무해",
          "original_form": "너무해",
          "speaker_id": "1",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019304.1.15",
          "form": "아기 밥먹을 시간이야",
          "original_form": "아기 밥먹을 시간이야",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019304.1.16",
          "form": "마트 가면 아기 장난감 사와 딸랑이",
          "original_form": "마트 가면 아기 장난감 사와 딸랑이",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019304.1.17",
          "form": "내일 엄마 오시니까 매운새우깡도 사와",
          "original_form": "내일 엄마 오시니까 매운새우깡도 사와",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019304.1.18",
          "form": "매운 새우깡 나는 별로 안좋아해",
          "original_form": "매운 새우깡 나는 별로 안좋아해",
          "speaker_id": "1",
          "time": "20210827 17:12"
        },
        {
          "id": "MDRW2100019304.1.19",
          "form": "양파링 사올게",
          "original_form": "양파링 사올게",
          "speaker_id": "1",
          "time": "20210827 17:12"
        },
        {
          "id": "MDRW2100019304.1.20",
          "form": "엄마가 좋아하거든",
          "original_form": "엄마가 좋아하거든",
          "speaker_id": "2",
          "time": "20210827 17:12"
        },
        {
          "id": "MDRW2100019304.1.21",
          "form": "엄마의 엄마면 할머니이구나",
          "original_form": "엄마의 엄마면 할머니이구나",
          "speaker_id": "1",
          "time": "20210827 17:12"
        },
        {
          "id": "MDRW2100019304.1.22",
          "form": "내일은 가족끼리 산책을 가자",
          "original_form": "내일은 가족끼리 산책을 가자",
          "speaker_id": "2",
          "time": "20210827 17:12"
        }
      ]
