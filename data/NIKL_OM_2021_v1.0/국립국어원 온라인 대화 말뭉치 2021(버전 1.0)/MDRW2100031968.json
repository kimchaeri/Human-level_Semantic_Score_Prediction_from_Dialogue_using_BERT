      "utterance": [
        {
          "id": "MDRW2100031968.1.1",
          "form": "난 분모자가 넘 좋아",
          "original_form": "난 분모자가 넘 좋아",
          "speaker_id": "1",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031968.1.2",
          "form": "ㅋ 개 전분으로 만든건데 이름이 무슨뜻일까?",
          "original_form": "ㅋ 개 전분으로 만든건데 이름이 무슨뜻일까?",
          "speaker_id": "2",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100031968.1.3",
          "form": "몰라 중국언가",
          "original_form": "몰라 중국언가",
          "speaker_id": "1",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100031968.1.4",
          "form": "그런가보다 참 모르는 음식이 많으니",
          "original_form": "그런가보다 참 모르는 음식이 많으니",
          "speaker_id": "2",
          "time": "20210905 09:54"
        },
        {
          "id": "MDRW2100031968.1.5",
          "form": "치즈떡도 맛있어",
          "original_form": "치즈떡도 맛있어",
          "speaker_id": "1",
          "time": "20210905 09:56"
        },
        {
          "id": "MDRW2100031968.1.6",
          "form": "그 떡덕분에 그나마 먹어줄만함\r\n석관동이 이번엔 좀 실패한듯",
          "original_form": "그 떡덕분에 그나마 먹어줄만함\r\n석관동이 이번엔 좀 실패한듯",
          "speaker_id": "2",
          "time": "20210905 09:59"
        },
        {
          "id": "MDRW2100031968.1.7",
          "form": "근데 기름떡볶이는 석관동이지....!",
          "original_form": "근데 기름떡볶이는 석관동이지....!",
          "speaker_id": "1",
          "time": "20210905 10:01"
        },
        {
          "id": "MDRW2100031968.1.8",
          "form": "맞아 다른 떡볶인 맛있지\r\n저녁엔 기름떡볶이 먹을까?",
          "original_form": "맞아 다른 떡볶인 맛있지\r\n저녁엔 기름떡볶이 먹을까?",
          "speaker_id": "2",
          "time": "20210905 10:02"
        },
        {
          "id": "MDRW2100031968.1.9",
          "form": "오 좋지!!!!",
          "original_form": "오 좋지!!!!",
          "speaker_id": "1",
          "time": "20210905 10:02"
        },
        {
          "id": "MDRW2100031968.1.10",
          "form": "그리고 순대도 맛나다구",
          "original_form": "그리고 순대도 맛나다구",
          "speaker_id": "1",
          "time": "20210905 10:02"
        },
        {
          "id": "MDRW2100031968.1.11",
          "form": "순대는 없어ㅠ 튀김을 먹을까 말까?",
          "original_form": "순대는 없어ㅠ 튀김을 먹을까 말까?",
          "speaker_id": "2",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100031968.1.12",
          "form": "어떤 튀김인데?",
          "original_form": "어떤 튀김인데?",
          "speaker_id": "1",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100031968.1.13",
          "form": "고구마 야채 만두일것같은데 안 뜯어봐서 잘 모름 ㅎ",
          "original_form": "고구마 야채 만두일것같은데 안 뜯어봐서 잘 모름 ㅎ",
          "speaker_id": "2",
          "time": "20210905 10:04"
        },
