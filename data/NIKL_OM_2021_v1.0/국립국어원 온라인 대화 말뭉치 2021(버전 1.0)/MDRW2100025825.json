      "utterance": [
        {
          "id": "MDRW2100025825.1.1",
          "form": "싸이월드를 아시나요",
          "original_form": "싸이월드를 아시나요",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025825.1.2",
          "form": "넵 ㅋㅋㅋ 학창시절에많이했었죠 ㅎㅎ 도토리 모으고",
          "original_form": "넵 ㅋㅋㅋ 학창시절에많이했었죠 ㅎㅎ 도토리 모으고",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025825.1.3",
          "form": "좀 아시네요 ㅎ",
          "original_form": "좀 아시네요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025825.1.4",
          "form": "저도 싸이월드 시대라 ㅋㅋ",
          "original_form": "저도 싸이월드 시대라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025825.1.5",
          "form": "싸이월드 부활한단 얘기에 신이났답니다",
          "original_form": "싸이월드 부활한단 얘기에 신이났답니다",
          "speaker_id": "1",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025825.1.6",
          "form": "진짜요 ? 제주위분들은 흑역사 오픈될까바 싫어하시더라구여 ㅎㅎㅎ",
          "original_form": "진짜요 ? 제주위분들은 흑역사 오픈될까바 싫어하시더라구여 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025825.1.7",
          "form": "그 퍼가요 때문에 ㅎㅎ",
          "original_form": "그 퍼가요 때문에 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025825.1.8",
          "form": "아 ㅎ 무섭긴한데\n뭐 그것도 추억이니 ㅎ",
          "original_form": "아 ㅎ 무섭긴한데\n뭐 그것도 추억이니 ㅎ",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025825.1.9",
          "form": "보고싶은이들도 많구요 ㅎ",
          "original_form": "보고싶은이들도 많구요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025825.1.10",
          "form": "ㅋㅋㅋ아아 생각나는 이들이 있긴하죠 연락은 끊겨찌만요 ㅠㅠ",
          "original_form": "ㅋㅋㅋ아아 생각나는 이들이 있긴하죠 연락은 끊겨찌만요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025825.1.11",
          "form": "미니에서 만나요 ㅋㅋㅋ",
          "original_form": "미니에서 만나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100025825.1.12",
          "form": "ㅋㅋㅋ추억은 추억일때 아름답지않을까하고..ㅎㅎ",
          "original_form": "ㅋㅋㅋ추억은 추억일때 아름답지않을까하고..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025825.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025825.1.14",
          "form": "그냥 궁금해요 다들 어떻게ㅜ사나",
          "original_form": "그냥 궁금해요 다들 어떻게ㅜ사나",
