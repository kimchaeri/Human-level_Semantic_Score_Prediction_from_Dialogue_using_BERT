      "utterance": [
        {
          "id": "MDRW2100027839.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027839.1.2",
          "form": "우리교수님들은 자율에 맡기던데",
          "original_form": "우리교수님들은 자율에 맡기던데",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027839.1.3",
          "form": "대부붕은 자율인데",
          "original_form": "대부붕은 자율인데",
          "speaker_id": "1",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027839.1.4",
          "form": "대부분은...",
          "original_form": "대부분은...",
          "speaker_id": "1",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027839.1.5",
          "form": "가끔 비디오 켜라고 하시는 교수님들이 계셔서...",
          "original_form": "가끔 비디오 켜라고 하시는 교수님들이 계셔서...",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027839.1.6",
          "form": "우리도... 가끔 켜라고 하는 교수님들이",
          "original_form": "우리도... 가끔 켜라고 하는 교수님들이",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027839.1.7",
          "form": "랜덤으로 계시긴 하는데",
          "original_form": "랜덤으로 계시긴 하는데",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027839.1.8",
          "form": "이번엔 가챠 성공한 듯",
          "original_form": "이번엔 가챠 성공한 듯",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027839.1.9",
          "form": "사실 비디오 켜라고 하시지 않아도",
          "original_form": "사실 비디오 켜라고 하시지 않아도",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027839.1.10",
          "form": "줌 수업 하시는 교수님들이 싫다",
          "original_form": "줌 수업 하시는 교수님들이 싫다",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027839.1.11",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "2",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027839.1.12",
          "form": "자체적으로 1.5배속 해주시든가요",
          "original_form": "자체적으로 1.5배속 해주시든가요",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027839.1.13",
          "form": "우리는... 싸강이라고 표시된 수업 아니면",
          "original_form": "우리는... 싸강이라고 표시된 수업 아니면",
          "speaker_id": "2",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027839.1.14",
          "form": "다 현강이야",
          "original_form": "다 현강이야",
          "speaker_id": "2",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027839.1.15",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027839.1.16",
          "form": "우린 교수 재량인데",
          "original_form": "우린 교수 재량인데",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027839.1.17",
          "form": "싸강은 들을 수 있는 학점 제한도 있어",
          "original_form": "싸강은 들을 수 있는 학점 제한도 있어",
          "speaker_id": "2",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027839.1.18",
          "form": "6학점까진가",
          "original_form": "6학점까진가",
          "speaker_id": "2",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027839.1.19",
          "form": "그래서 슈뢰딩거의 싸강",
          "original_form": "그래서 슈뢰딩거의 싸강",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027839.1.20",
          "form": "우리도... 코시국 전에는 그랬었는데...",
          "original_form": "우리도... 코시국 전에는 그랬었는데...",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027839.1.21",
          "form": "지금은 그냥 줌/녹강 교수 선택이라",
          "original_form": "지금은 그냥 줌/녹강 교수 선택이라",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027839.1.22",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027839.1.23",
          "form": "아냐 근데 싸강이면...",
          "original_form": "아냐 근데 싸강이면...",
          "speaker_id": "2",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027839.1.24",
          "form": "맨날 벼락치기해서",
          "original_form": "맨날 벼락치기해서",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027839.1.25",
          "form": "현강이 나은 듯(ㅎㅎ)",
          "original_form": "현강이 나은 듯(ㅎㅎ)",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027839.1.26",
          "form": "웃겨",
          "original_form": "웃겨",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027839.1.27",
          "form": "난 실강이면 딴짓해서",
          "original_form": "난 실강이면 딴짓해서",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027839.1.28",
          "form": "녹강이 좋음...",
          "original_form": "녹강이 좋음...",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027839.1.29",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027839.1.30",
          "form": "1.5배속 소중해",
          "original_form": "1.5배속 소중해",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027839.1.31",
          "form": "수업 시간에 게임하는 사람",
          "original_form": "수업 시간에 게임하는 사람",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027839.1.32",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027839.1.33",
          "form": "아무래도 그렇지",
          "original_form": "아무래도 그렇지",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027839.1.34",
          "form": "녹강은 공부하고 싶을 때 들으면 되지만",
          "original_form": "녹강은 공부하고 싶을 때 들으면 되지만",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027839.1.35",
          "form": "실강은 꼭 게임하고 싶을 때 하니까",
          "original_form": "실강은 꼭 게임하고 싶을 때 하니까",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027839.1.36",
          "form": "어쩔 수 없어",
          "original_form": "어쩔 수 없어",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027839.1.37",
          "form": "공부하고 싶을 때가 있나요?",
          "original_form": "공부하고 싶을 때가 있나요?",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027839.1.38",
          "form": "수업을 들을 결심이 서는 때는 있지",
          "original_form": "수업을 들을 결심이 서는 때는 있지",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027839.1.39",
          "form": "멋진데",
          "original_form": "멋진데",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027839.1.40",
          "form": "그야 우리 전공은 재밋으니까",
          "original_form": "그야 우리 전공은 재밋으니까",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027839.1.41",
          "form": "걍 들으면 돼",
          "original_form": "걍 들으면 돼",
          "speaker_id": "1",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027839.1.42",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027839.1.43",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027839.1.44",
          "form": "우리 전공은... 재미없는데",
          "original_form": "우리 전공은... 재미없는데",
