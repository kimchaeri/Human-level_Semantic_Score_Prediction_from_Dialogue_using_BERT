      "utterance": [
        {
          "id": "MDRW2100012594.1.1",
          "form": "잡지나 이런 거 많이 보세요?",
          "original_form": "잡지나 이런 거 많이 보세요?",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100012594.1.2",
          "form": "아니요 name1은요?",
          "original_form": "아니요 &name1&은요?",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012594.1.3",
          "form": "전 자주 구매하고 봤는데",
          "original_form": "전 자주 구매하고 봤는데",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012594.1.4",
          "form": "한번 보고 안봐서 이젠 안사요ㅋㅋㅋㅋㅋ",
          "original_form": "한번 보고 안봐서 이젠 안사요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012594.1.5",
          "form": "어떤 잡지 보셨어요?",
          "original_form": "어떤 잡지 보셨어요?",
          "speaker_id": "2",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012594.1.6",
          "form": "나일론 쎄씨 이런 거 봤어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나일론 쎄씨 이런 거 봤어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012594.1.7",
          "form": "한번씩 들어봤네요ㅋㅋ",
          "original_form": "한번씩 들어봤네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100012594.1.8",
          "form": "보그도 보셨나요?",
          "original_form": "보그도 보셨나요?",
          "speaker_id": "2",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100012594.1.9",
          "form": "아 보그도 필수죠 !!!!",
          "original_form": "아 보그도 필수죠 !!!!",
          "speaker_id": "1",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100012594.1.10",
          "form": "잡지 보게된 계기가 있으세요?",
          "original_form": "잡지 보게된 계기가 있으세요?",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012594.1.11",
          "form": "그냥 어쩌다보니 보게됐어요 ㅋㅋㅋ ㅋㅋㅋㅋ",
          "original_form": "그냥 어쩌다보니 보게됐어요 ㅋㅋㅋ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012594.1.12",
          "form": "패션쪽이기도 해서요!",
          "original_form": "패션쪽이기도 해서요!",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012594.1.13",
          "form": "오 잡지를 볼 수 밖에 없네요~  디자이너세요?",
          "original_form": "오 잡지를 볼 수 밖에 없네요~  디자이너세요?",
          "speaker_id": "2",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012594.1.14",
          "form": "아니에요 ㅋㅋㅋㅋㅋㅋ 예전에 전공이 그쪽이여서요",
          "original_form": "아니에요 ㅋㅋㅋㅋㅋㅋ 예전에 전공이 그쪽이여서요",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012594.1.15",
          "form": "지금은 아니그요",
          "original_form": "지금은 아니그요",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012594.1.16",
          "form": "베스트 셀러 도서 보신적 있으신가요?",
          "original_form": "베스트 셀러 도서 보신적 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012594.1.17",
          "form": "네 있죠!",
          "original_form": "네 있죠!",
          "speaker_id": "1",
          "time": "20210824 23:22"
        }
