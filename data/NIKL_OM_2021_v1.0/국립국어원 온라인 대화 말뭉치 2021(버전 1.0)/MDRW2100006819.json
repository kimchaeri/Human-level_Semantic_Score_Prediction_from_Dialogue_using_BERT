      "utterance": [
        {
          "id": "MDRW2100006819.1.1",
          "form": "참치 진짜 좋아하시나봐요 ㅋㅋㅋ",
          "original_form": "참치 진짜 좋아하시나봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100006819.1.2",
          "form": "넼ㅋㅋㅋㅋㅋㅋㅋ 참치김밥 특히 너무 좋아요",
          "original_form": "넼ㅋㅋㅋㅋㅋㅋㅋ 참치김밥 특히 너무 좋아요",
          "speaker_id": "1",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100006819.1.3",
          "form": "마요네즈를 좋아해요",
          "original_form": "마요네즈를 좋아해요",
          "speaker_id": "1",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100006819.1.4",
          "form": "맞아요 마요네즈도 맛있어요 ㅋㅋㅋ",
          "original_form": "맞아요 마요네즈도 맛있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100006819.1.5",
          "form": "스팸마요 이런거 좋아하는거 같아요",
          "original_form": "스팸마요 이런거 좋아하는거 같아요",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100006819.1.6",
          "form": "스팸마요도 진짜 맛있어요",
          "original_form": "스팸마요도 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100006819.1.7",
          "form": "혹시 점심은 뭐 드실건가요??",
          "original_form": "혹시 점심은 뭐 드실건가요??",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100006819.1.8",
          "form": "저는 짜파게티 먹을려구용",
          "original_form": "저는 짜파게티 먹을려구용",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100006819.1.9",
          "form": "짜파게티에 계란 올려드세요??",
          "original_form": "짜파게티에 계란 올려드세요??",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100006819.1.10",
          "form": "오 가끔이요!!!",
          "original_form": "오 가끔이요!!!",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006819.1.11",
          "form": "그 후라이 말하는건가용?",
          "original_form": "그 후라이 말하는건가용?",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006819.1.12",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006819.1.13",
          "form": "보통 짜파게티에 후라이 올려서",
          "original_form": "보통 짜파게티에 후라이 올려서",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006819.1.14",
          "form": "오오 너무 맛있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "오오 너무 맛있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006819.1.15",
          "form": "만두 구워서 같이 먹어요 ㅋㅋㅋ",
          "original_form": "만두 구워서 같이 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006819.1.16",
          "form": "저는 치즈도 올려먹어요 ㅋㅋㅋ",
          "original_form": "저는 치즈도 올려먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006819.1.17",
          "form": "헐 치즈는 올려본 적이 없네요...",
          "original_form": "헐 치즈는 올려본 적이 없네요...",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006819.1.18",
          "form": "짜파구리도 드셔 보셨어요??",
          "original_form": "짜파구리도 드셔 보셨어요??",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006819.1.19",
          "form": "가끔 먹어요 !! ㅋㅋㅋ",
