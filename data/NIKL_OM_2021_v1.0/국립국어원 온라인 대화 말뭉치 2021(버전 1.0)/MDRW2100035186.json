      "utterance": [
        {
          "id": "MDRW2100035186.1.1",
          "form": "안녕하세요? 아침은 드셨나요?",
          "original_form": "안녕하세요? 아침은 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100035186.1.2",
          "form": "네네 점심으로 닭칼국수 먹엇어여 ㅎㅎ",
          "original_form": "네네 점심으로 닭칼국수 먹엇어여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100035186.1.3",
          "form": "와 맛있겠다 여름엔 초계탕이 좋지만 이젠 칼국수가 맛있죠",
          "original_form": "와 맛있겠다 여름엔 초계탕이 좋지만 이젠 칼국수가 맛있죠",
          "speaker_id": "1",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100035186.1.4",
          "form": "날이 선선해졌으니까요",
          "original_form": "날이 선선해졌으니까요",
          "speaker_id": "1",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100035186.1.5",
          "form": "비오니 땡기더라구요 ㅎㅎ",
          "original_form": "비오니 땡기더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100035186.1.6",
          "form": "ㄴㅔ 오늘 날씨에딱이에요 ㅋㅋ 외식하셨나요? 아님 직접 만드셨나요?",
          "original_form": "ㄴㅔ 오늘 날씨에딱이에요 ㅋㅋ 외식하셨나요? 아님 직접 만드셨나요?",
          "speaker_id": "1",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100035186.1.7",
          "form": "직접이요 ㅎㄹ",
          "original_form": "직접이요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100035186.1.8",
          "form": "삼계탕 끓임 양이 항상 남아서",
          "original_form": "삼계탕 끓임 양이 항상 남아서",
          "speaker_id": "2",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100035186.1.9",
          "form": "소분해서 칼국수용으로 남겨둬요 ㅎㅎ",
          "original_form": "소분해서 칼국수용으로 남겨둬요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100035186.1.10",
          "form": "와 너무 맛있겠다 ㅠㅠㅠ",
          "original_form": "와 너무 맛있겠다 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 12:59"
        },
        {
          "id": "MDRW2100035186.1.11",
          "form": "저 닭육수를 좋아하거든요,,,",
          "original_form": "저 닭육수를 좋아하거든요,,,",
          "speaker_id": "1",
          "time": "20210829 12:59"
        },
        {
          "id": "MDRW2100035186.1.12",
          "form": "전 그냥 김이랑 계란 햄을 조미금으로 둘둘 만",
          "original_form": "전 그냥 김이랑 계란 햄을 조미금으로 둘둘 만",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100035186.1.13",
          "form": "야매김밥 먹었어욬",
          "original_form": "야매김밥 먹었어욬",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100035186.1.14",
          "form": "김밥은 집김밥이 최고졓ㅎ",
          "original_form": "김밥은 집김밥이 최고졓ㅎ",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100035186.1.15",
          "form": "맛있어요 ㅋㅋ 김밥김 아닌 김이라 눅눅해지는 맛이 좀있지만 것도조아요",
          "original_form": "맛있어요 ㅋㅋ 김밥김 아닌 김이라 눅눅해지는 맛이 좀있지만 것도조아요",
          "speaker_id": "1",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100035186.1.16",
          "form": "ㅎㅎ 담백담백하고 속도 편해여 ㅎㅎ",
          "original_form": "ㅎㅎ 담백담백하고 속도 편해여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100035186.1.17",
          "form": "네 그래서 자주 이렇게 야매로 해먹어요 ㅋㅋ",
          "original_form": "네 그래서 자주 이렇게 야매로 해먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100035186.1.18",
          "form": "집밥이 최고최고 ㅎㅎ",
          "original_form": "집밥이 최고최고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:57"
        },
        {
          "id": "MDRW2100035186.1.19",
          "form": "나가기도 귀찮고 돈스기도 싫으니 결론은 집밥입니다",
          "original_form": "나가기도 귀찮고 돈스기도 싫으니 결론은 집밥입니다",
          "speaker_id": "1",
