      "utterance": [
        {
          "id": "MDRW2100013083.1.1",
          "form": "어디로 가고싶은데?",
          "original_form": "어디로 가고싶은데?",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013083.1.2",
          "form": "열기구처럼 생겨서는 나는거 있잖아",
          "original_form": "열기구처럼 생겨서는 나는거 있잖아",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013083.1.3",
          "form": "그거타러가고싶어",
          "original_form": "그거타러가고싶어",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013083.1.4",
          "form": "플라잉수원 하러가!",
          "original_form": "플라잉수원 하러가!",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013083.1.5",
          "form": "그거 말구 몸뚱이채로 나는거!",
          "original_form": "그거 말구 몸뚱이채로 나는거!",
          "speaker_id": "1",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100013083.1.6",
          "form": "스피드퀴즈처럼 됐네ㅋㅋㅋㅋㅋ",
          "original_form": "스피드퀴즈처럼 됐네ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100013083.1.7",
          "form": "어디든 그거 타러~가고싶어~",
          "original_form": "어디든 그거 타러~가고싶어~",
          "speaker_id": "1",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100013083.1.8",
          "form": "그리고 나중에 수상레저도..ㅜㅜ",
          "original_form": "그리고 나중에 수상레저도..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 12:32"
        },
        {
          "id": "MDRW2100013083.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:49"
        },
        {
          "id": "MDRW2100013083.1.10",
          "form": "나는 무서워서 그런건 못할고가타...",
          "original_form": "나는 무서워서 그런건 못할고가타...",
          "speaker_id": "2",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100013083.1.11",
          "form": "편의점 기프티콘 들어와따",
          "original_form": "편의점 기프티콘 들어와따",
          "speaker_id": "2",
          "time": "20210825 13:07"
        },
        {
          "id": "MDRW2100013083.1.12",
          "form": "그러네!!",
          "original_form": "그러네!!",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013083.1.13",
          "form": "아냐 넌 필참멩버라서",
          "original_form": "아냐 넌 필참멩버라서",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013083.1.14",
          "form": "나랏 같이 떠나야해",
          "original_form": "나랏 같이 떠나야해",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013083.1.15",
          "form": "여행메이트 해",
          "original_form": "여행메이트 해",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013083.1.16",
          "form": "아닛...그런~!",
          "original_form": "아닛...그런~!",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013083.1.17",
          "form": "그럼 가야지",
          "original_form": "그럼 가야지",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013083.1.18",
          "form": "훗 돈 준비해",
          "original_form": "훗 돈 준비해",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013083.1.19",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ운전학원으로 호로록 나갈것같긴하지만",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ운전학원으로 호로록 나갈것같긴하지만",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013083.1.20",
          "form": "지금 못가는만큼 나중에 여행 왕창 가자",
          "original_form": "지금 못가는만큼 나중에 여행 왕창 가자",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013083.1.21",
          "form": "최대한 아껴보게쑴",
          "original_form": "최대한 아껴보게쑴",
