        {
          "id": "MDRW2100041323.1.1",
          "form": "학교때 어떤거 공부하셨어요?",
          "original_form": "학교때 어떤거 공부하셨어요?",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041323.1.2",
          "form": "학생 때 외국어 공부 했어요~",
          "original_form": "학생 때 외국어 공부 했어요~",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041323.1.3",
          "form": "어떤 공부 하셨나요?",
          "original_form": "어떤 공부 하셨나요?",
          "speaker_id": "1",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100041323.1.4",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100041323.1.5",
          "form": "저는 경영이요",
          "original_form": "저는 경영이요",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100041323.1.6",
          "form": "아 경영학! 재미있으셨나요 ㅋㅋㅋ",
          "original_form": "아 경영학! 재미있으셨나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041323.1.7",
          "form": "아ㅋㅋ아뇨ㅋㅋ",
          "original_form": "아ㅋㅋ아뇨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041323.1.8",
          "form": "전공살리셨나요?",
          "original_form": "전공살리셨나요?",
          "speaker_id": "2",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041323.1.9",
          "form": "전공은 그냥 취미로 하고 있고 다른 일 합니다 ㅋㅋ",
          "original_form": "전공은 그냥 취미로 하고 있고 다른 일 합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041323.1.10",
          "form": "전공 살리셨어요?",
          "original_form": "전공 살리셨어요?",
          "speaker_id": "1",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041323.1.11",
          "form": "ㅋㅋ 살리긴한거 같은데 썩ㅋㅋ",
          "original_form": "ㅋㅋ 살리긴한거 같은데 썩ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041323.1.12",
          "form": "남녀공학 나오셨나요?",
          "original_form": "남녀공학 나오셨나요?",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041323.1.13",
          "form": "네 남녀공학이었어요! 남녀공학 이셨어요?",
          "original_form": "네 남녀공학이었어요! 남녀공학 이셨어요?",
          "speaker_id": "1",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041323.1.14",
          "form": "아뇨ㅜㅋㅋ 여중여고여대",
          "original_form": "아뇨ㅜㅋㅋ 여중여고여대",
          "speaker_id": "2",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041323.1.15",
          "form": "아하! 그래도 정말 재밌다고 하던데 그런가요 ㅋㅋㅋ",
          "original_form": "아하! 그래도 정말 재밌다고 하던데 그런가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100041323.1.16",
          "form": "고등학교의 연장?? 같은 느낌ㅋㅋ 강압없고 군기잡는거 없고 자유로웠어요ㅋㅋ",
          "original_form": "고등학교의 연장?? 같은 느낌ㅋㅋ 강압없고 군기잡는거 없고 자유로웠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041323.1.17",
          "form": "졸업때까지 술자리 3번? ㅋㅋ",
          "original_form": "졸업때까지 술자리 3번? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041323.1.18",
          "form": "건전하네요 ㅋㅋㅋㅋㅋ",
          "original_form": "건전하네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
