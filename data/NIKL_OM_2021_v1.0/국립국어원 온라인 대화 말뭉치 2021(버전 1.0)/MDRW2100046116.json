      "utterance": [
        {
          "id": "MDRW2100046116.1.1",
          "form": "글쎄요 고민 중이에요",
          "original_form": "글쎄요 고민 중이에요",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046116.1.2",
          "form": "원래는 사무실 사람들이랑 같이 먹는데",
          "original_form": "원래는 사무실 사람들이랑 같이 먹는데",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046116.1.3",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046116.1.4",
          "form": "메뉴 고르는거 항상 고민이에요",
          "original_form": "메뉴 고르는거 항상 고민이에요",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046116.1.5",
          "form": "제가 어제 모더나를 맞고",
          "original_form": "제가 어제 모더나를 맞고",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046116.1.6",
          "form": "지금 집에서 쉬는 중이라",
          "original_form": "지금 집에서 쉬는 중이라",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046116.1.7",
          "form": "헉 백신 ㅠㅠ",
          "original_form": "헉 백신 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046116.1.8",
          "form": "뭘 먹을지 고민이네요",
          "original_form": "뭘 먹을지 고민이네요",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046116.1.9",
          "form": "저희엄마는 죽 드셧어요",
          "original_form": "저희엄마는 죽 드셧어요",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046116.1.10",
          "form": "요즘은 계속 비가 와서 그런지",
          "original_form": "요즘은 계속 비가 와서 그런지",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046116.1.11",
          "form": "칼국수 수제비를 많이 먹었어요",
          "original_form": "칼국수 수제비를 많이 먹었어요",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046116.1.12",
          "form": "우아 수제비 ㅠㅠㅠ",
          "original_form": "우아 수제비 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046116.1.13",
          "form": "비오는날에 딱이죵",
          "original_form": "비오는날에 딱이죵",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046116.1.14",
          "form": "비가 계속 계속 오다보니까",
          "original_form": "비가 계속 계속 오다보니까",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046116.1.15",
          "form": "일주일에 두 번 정도는 칼국수를 먹은 듯해요",
          "original_form": "일주일에 두 번 정도는 칼국수를 먹은 듯해요",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046116.1.16",
          "form": "저는 파전..",
          "original_form": "저는 파전..",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046116.1.17",
          "form": "어우 파전",
          "original_form": "어우 파전",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046116.1.18",
          "form": "너무 좋죠",
          "original_form": "너무 좋죠",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046116.1.19",
          "form": "맞아요 ㅠㅠ 너무 좋아요 ㅋㅋㅋㅋ 냄새랑 소리",
          "original_form": "맞아요 ㅠㅠ 너무 좋아요 ㅋㅋㅋㅋ 냄새랑 소리",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046116.1.20",
          "form": "소리까지 ㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "소리까지 ㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046116.1.21",
          "form": "아 너무 재밌으시네요",
          "original_form": "아 너무 재밌으시네요",
