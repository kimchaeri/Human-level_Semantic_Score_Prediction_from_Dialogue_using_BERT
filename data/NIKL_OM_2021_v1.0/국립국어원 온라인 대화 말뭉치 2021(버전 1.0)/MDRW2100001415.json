      "utterance": [
        {
          "id": "MDRW2100001415.1.1",
          "form": "안녕하세요 name2씨",
          "original_form": "안녕하세요 &name2&씨",
          "speaker_id": "1",
          "time": "20211015 15:38"
        },
        {
          "id": "MDRW2100001415.1.2",
          "form": "안녕하세요! 저녁 메뉴는 생각하셨나요",
          "original_form": "안녕하세요! 저녁 메뉴는 생각하셨나요",
          "speaker_id": "2",
          "time": "20211015 15:38"
        },
        {
          "id": "MDRW2100001415.1.3",
          "form": "아직이요 뭐먹을까요",
          "original_form": "아직이요 뭐먹을까요",
          "speaker_id": "1",
          "time": "20211015 15:39"
        },
        {
          "id": "MDRW2100001415.1.4",
          "form": "글쎄요 점심에 뭐 드셨나요?",
          "original_form": "글쎄요 점심에 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20211015 15:40"
        },
        {
          "id": "MDRW2100001415.1.5",
          "form": "실패할 수 없는 메뉴를 먹었습니다",
          "original_form": "실패할 수 없는 메뉴를 먹었습니다",
          "speaker_id": "1",
          "time": "20211015 15:41"
        },
        {
          "id": "MDRW2100001415.1.6",
          "form": "돈까스와 쫄면",
          "original_form": "돈까스와 쫄면",
          "speaker_id": "1",
          "time": "20211015 15:41"
        },
        {
          "id": "MDRW2100001415.1.7",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20211015 15:42"
        },
        {
          "id": "MDRW2100001415.1.8",
          "form": "name2씨는요",
          "original_form": "&name2&씨는요",
          "speaker_id": "1",
          "time": "20211015 15:42"
        },
        {
          "id": "MDRW2100001415.1.9",
          "form": "저는 그냥 과자 집어먹다보니",
          "original_form": "저는 그냥 과자 집어먹다보니",
          "speaker_id": "2",
          "time": "20211015 15:42"
        },
        {
          "id": "MDRW2100001415.1.10",
          "form": "배가 찼어요 ㅋㅋ",
          "original_form": "배가 찼어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 15:42"
        },
        {
          "id": "MDRW2100001415.1.11",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "1",
          "time": "20211015 15:42"
        },
        {
          "id": "MDRW2100001415.1.12",
          "form": "돈까스랑 쫄면이면 .. 저녁엔 밥을 드셔야겠네요",
          "original_form": "돈까스랑 쫄면이면 .. 저녁엔 밥을 드셔야겠네요",
          "speaker_id": "2",
          "time": "20211015 15:43"
        },
        {
          "id": "MDRW2100001415.1.13",
          "form": "사실 저녁메뉴는 정해져있었습니다",
          "original_form": "사실 저녁메뉴는 정해져있었습니다",
          "speaker_id": "1",
          "time": "20211015 15:43"
        },
        {
          "id": "MDRW2100001415.1.14",
          "form": "피자 먹을거에여ㅎ",
          "original_form": "피자 먹을거에여ㅎ",
          "speaker_id": "1",
          "time": "20211015 15:43"
        },
        {
          "id": "MDRW2100001415.1.15",
          "form": "아니 세상에",
          "original_form": "아니 세상에",
          "speaker_id": "2",
          "time": "20211015 15:43"
        },
        {
          "id": "MDRW2100001415.1.16",
          "form": "오늘 미피 1+1인데",
          "original_form": "오늘 미피 1+1인데",
          "speaker_id": "2",
          "time": "20211015 15:44"
        },
        {
          "id": "MDRW2100001415.1.17",
          "form": "알고계시나요",
          "original_form": "알고계시나요",
          "speaker_id": "2",
          "time": "20211015 15:44"
        },
        {
          "id": "MDRW2100001415.1.18",
          "form": "아 도미노 먹으려 그랬는디",
          "original_form": "아 도미노 먹으려 그랬는디",
          "speaker_id": "1",
          "time": "20211015 15:44"
        },
        {
          "id": "MDRW2100001415.1.19",
          "form": "그럼 미피 메뉴 추천해주세요",
          "original_form": "그럼 미피 메뉴 추천해주세요",
          "speaker_id": "1",
          "time": "20211015 15:44"
        },
        {
          "id": "MDRW2100001415.1.20",
          "form": "ㅋㅋㅋㅋ1+1은 아마 메뉴가 정해져있을거예요",
          "original_form": "ㅋㅋㅋㅋ1+1은 아마 메뉴가 정해져있을거예요",
          "speaker_id": "2",
          "time": "20211015 15:44"
        },
        {
          "id": "MDRW2100001415.1.21",
