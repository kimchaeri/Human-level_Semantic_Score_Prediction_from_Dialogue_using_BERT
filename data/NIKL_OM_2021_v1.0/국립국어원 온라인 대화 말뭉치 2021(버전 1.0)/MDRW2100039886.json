      "utterance": [
        {
          "id": "MDRW2100039886.1.1",
          "form": "최근에 공연 보신적 있나요?",
          "original_form": "최근에 공연 보신적 있나요?",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039886.1.2",
          "form": "영화봤네요..공연은 못보고",
          "original_form": "영화봤네요..공연은 못보고",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039886.1.3",
          "form": "인질 보셨나요?",
          "original_form": "인질 보셨나요?",
          "speaker_id": "1",
          "time": "20210904 09:43"
        },
        {
          "id": "MDRW2100039886.1.4",
          "form": "아니요.",
          "original_form": "아니요.",
          "speaker_id": "2",
          "time": "20210904 09:43"
        },
        {
          "id": "MDRW2100039886.1.5",
          "form": "재미있다고하던데..보고싶어요",
          "original_form": "재미있다고하던데..보고싶어요",
          "speaker_id": "2",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100039886.1.6",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "2",
          "time": "20210904 09:45"
        },
        {
          "id": "MDRW2100039886.1.7",
          "form": "아니요~ 영화 뭐 모셨나요?",
          "original_form": "아니요~ 영화 뭐 모셨나요?",
          "speaker_id": "1",
          "time": "20210904 09:53"
        },
        {
          "id": "MDRW2100039886.1.8",
          "form": "요즘최근은 모가디슈요",
          "original_form": "요즘최근은 모가디슈요",
          "speaker_id": "2",
          "time": "20210904 09:54"
        },
        {
          "id": "MDRW2100039886.1.9",
          "form": "넘 재미있게봤네요",
          "original_form": "넘 재미있게봤네요",
          "speaker_id": "2",
          "time": "20210904 09:54"
        },
        {
          "id": "MDRW2100039886.1.10",
          "form": "보고나니 아프칸 사태가 영화랑똑같더라구요",
          "original_form": "보고나니 아프칸 사태가 영화랑똑같더라구요",
          "speaker_id": "2",
          "time": "20210904 09:54"
        },
        {
          "id": "MDRW2100039886.1.11",
          "form": "처음들어서 찾아보고 왔는데 줄거리 읽어보니 넘 재미있겠어요",
          "original_form": "처음들어서 찾아보고 왔는데 줄거리 읽어보니 넘 재미있겠어요",
          "speaker_id": "1",
          "time": "20210904 09:54"
        },
        {
          "id": "MDRW2100039886.1.12",
          "form": "전 재미있게봤어요",
          "original_form": "전 재미있게봤어요",
          "speaker_id": "2",
          "time": "20210904 09:55"
        },
        {
          "id": "MDRW2100039886.1.13",
          "form": "시간이 금방가더군요",
          "original_form": "시간이 금방가더군요",
          "speaker_id": "2",
          "time": "20210904 09:55"
        },
        {
          "id": "MDRW2100039886.1.14",
          "form": "저런류의 영화를 좋아하시는군요",
          "original_form": "저런류의 영화를 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210904 09:55"
        },
        {
          "id": "MDRW2100039886.1.15",
          "form": "전 최근에 무지개 물고기 뮤지컬 예약했어요",
          "original_form": "전 최근에 무지개 물고기 뮤지컬 예약했어요",
          "speaker_id": "1",
          "time": "20210904 09:55"
        },
        {
          "id": "MDRW2100039886.1.16",
          "form": "딱히 그런건아닌데 이야기 전개가 잘되더라구요",
          "original_form": "딱히 그런건아닌데 이야기 전개가 잘되더라구요",
          "speaker_id": "2",
          "time": "20210904 09:55"
        },
        {
          "id": "MDRW2100039886.1.17",
          "form": "오..뮤지컬",
          "original_form": "오..뮤지컬",
          "speaker_id": "2",
          "time": "20210904 09:55"
        },
        {
          "id": "MDRW2100039886.1.18",
          "form": "멋지네요",
          "original_form": "멋지네요",
          "speaker_id": "2",
          "time": "20210904 09:56"
        },
        {
          "id": "MDRW2100039886.1.19",
          "form": "뮤지컬본지가 언젠지 기억도 안나네요",
          "original_form": "뮤지컬본지가 언젠지 기억도 안나네요",
          "speaker_id": "2",
          "time": "20210904 09:56"
        },
        {
          "id": "MDRW2100039886.1.20",
          "form": "아이가 보고 싶다고 하더라고요",
          "original_form": "아이가 보고 싶다고 하더라고요",
          "speaker_id": "1",
          "time": "20210904 09:56"
        },
        {
          "id": "MDRW2100039886.1.21",
          "form": "저희집 앞에 현수막이 걸려 있거든요",
          "original_form": "저희집 앞에 현수막이 걸려 있거든요",
          "speaker_id": "1",
          "time": "20210904 09:56"
        },
        {
          "id": "MDRW2100039886.1.22",
          "form": "무지개물고기책 옛날에본것같아요",
          "original_form": "무지개물고기책 옛날에본것같아요",
          "speaker_id": "2",
          "time": "20210904 09:57"
        },
        {
          "id": "MDRW2100039886.1.23",
          "form": "티켓 예약한게 정말 백만년만에 해보는것 같아요",
          "original_form": "티켓 예약한게 정말 백만년만에 해보는것 같아요",
          "speaker_id": "1",
          "time": "20210904 09:57"
        },
        {
          "id": "MDRW2100039886.1.24",
          "form": "그 물고기 유명하죠?ㅎㅎ",
          "original_form": "그 물고기 유명하죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 09:57"
        },
        {
          "id": "MDRW2100039886.1.25",
          "form": "맞아요~ 그 무지개 물고기인것 같아요",
          "original_form": "맞아요~ 그 무지개 물고기인것 같아요",
          "speaker_id": "1",
          "time": "20210904 09:57"
        },
        {
          "id": "MDRW2100039886.1.26",
          "form": "저희집에도 그 책 있거든요",
          "original_form": "저희집에도 그 책 있거든요",
