        {
          "id": "MDRW2100042024.1.1",
          "form": "네 하져",
          "original_form": "네 하져",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100042024.1.2",
          "form": "하시나요?",
          "original_form": "하시나요?",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100042024.1.3",
          "form": "네넹 거의 주로합니당 ㅎㅎ",
          "original_form": "네넹 거의 주로합니당 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042024.1.4",
          "form": "오 좋아하세요?",
          "original_form": "오 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042024.1.5",
          "form": "전 싫어해요",
          "original_form": "전 싫어해요",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042024.1.6",
          "form": "저도 좋아하는건아닌데",
          "original_form": "저도 좋아하는건아닌데",
          "speaker_id": "1",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100042024.1.7",
          "form": "깨끗한걸 좋아해서 자꾸 몸이 움직여욬ㅋㅋ",
          "original_form": "깨끗한걸 좋아해서 자꾸 몸이 움직여욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100042024.1.8",
          "form": "우와ㅋㅋㅋㅋㅋㅋ 움직이는게 대박이네요",
          "original_form": "우와ㅋㅋㅋㅋㅋㅋ 움직이는게 대박이네요",
          "speaker_id": "2",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100042024.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ사람마다 다른거같아용",
          "original_form": "ㅋㅋㅋㅋㅋㅋ사람마다 다른거같아용",
          "speaker_id": "1",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100042024.1.10",
          "form": "전 ㅠ 깨끗한 거 좋지만 더러운.. 하하",
          "original_form": "전 ㅠ 깨끗한 거 좋지만 더러운.. 하하",
          "speaker_id": "2",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100042024.1.11",
          "form": "ㅋㅋㅋㅋ아무래도 귀찮죠 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ아무래도 귀찮죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042024.1.12",
          "form": "ㅠ 네 증말 청소기계 나오먄 좋겠어요",
          "original_form": "ㅠ 네 증말 청소기계 나오먄 좋겠어요",
          "speaker_id": "2",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100042024.1.13",
          "form": "로봇청소기 있으세용?",
          "original_form": "로봇청소기 있으세용?",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100042024.1.14",
          "form": "있는데 거의 안써요...",
          "original_form": "있는데 거의 안써요...",
          "speaker_id": "2",
          "time": "20210906 19:12"
        },
        {
          "id": "MDRW2100042024.1.15",
          "form": "전 요즘 사고싶어서 알아보는중이에엽",
          "original_form": "전 요즘 사고싶어서 알아보는중이에엽",
          "speaker_id": "1",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100042024.1.16",
          "form": "저희 집에 있는건 오래된거라",
          "original_form": "저희 집에 있는건 오래된거라",
          "speaker_id": "2",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100042024.1.17",
          "form": "요새꺼는 좋을 거 같아요",
