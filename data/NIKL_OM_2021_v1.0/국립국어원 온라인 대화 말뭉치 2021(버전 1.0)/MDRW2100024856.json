      "utterance": [
        {
          "id": "MDRW2100024856.1.1",
          "form": "코로나 언제 끝날까요",
          "original_form": "코로나 언제 끝날까요",
          "speaker_id": "2",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100024856.1.2",
          "form": "여행가고싶네요",
          "original_form": "여행가고싶네요",
          "speaker_id": "2",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100024856.1.3",
          "form": "어디 여행가보고 싶으세요?",
          "original_form": "어디 여행가보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210831 19:53"
        },
        {
          "id": "MDRW2100024856.1.4",
          "form": "저 어디든 가고싶어요",
          "original_form": "저 어디든 가고싶어요",
          "speaker_id": "2",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100024856.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100024856.1.6",
          "form": "저는 일본 가보고 싶어요!",
          "original_form": "저는 일본 가보고 싶어요!",
          "speaker_id": "1",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100024856.1.7",
          "form": "일본 좋죠",
          "original_form": "일본 좋죠",
          "speaker_id": "2",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100024856.1.8",
          "form": "전 딱 한번 가봤어요...",
          "original_form": "전 딱 한번 가봤어요...",
          "speaker_id": "2",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100024856.1.9",
          "form": "어디 가보셨어요?!",
          "original_form": "어디 가보셨어요?!",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100024856.1.10",
          "form": "저 오사카요ㅋㅋㅋㅋㅋ",
          "original_form": "저 오사카요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100024856.1.11",
          "form": "어디 가고싶으세요?",
          "original_form": "어디 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100024856.1.12",
          "form": "저는 홋카이도랑 도쿄 가보고 싶어요!",
          "original_form": "저는 홋카이도랑 도쿄 가보고 싶어요!",
          "speaker_id": "1",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100024856.1.13",
          "form": "아 도쿄 저두여",
          "original_form": "아 도쿄 저두여",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100024856.1.14",
          "form": "아니면 전 삿포로요",
          "original_form": "아니면 전 삿포로요",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100024856.1.15",
          "form": "겨울에...",
          "original_form": "겨울에...",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100024856.1.16",
          "form": "맞아요!!! 특히 콘비니 가보고 싶어욤ㅁ",
          "original_form": "맞아요!!! 특히 콘비니 가보고 싶어욤ㅁ",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100024856.1.17",
          "form": "콘비니는 어딘지 모르는데",
          "original_form": "콘비니는 어딘지 모르는데",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100024856.1.18",
          "form": "겨울에 가기에 좋군요?!",
          "original_form": "겨울에 가기에 좋군요?!",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100024856.1.19",
          "form": "편의점(=콘비니)이요ㅋㅋㅋ 삿포로는 낙농업이 발달했다고 해서 특산물도 궁금하네용",
          "original_form": "편의점(=콘비니)이요ㅋㅋㅋ 삿포로는 낙농업이 발달했다고 해서 특산물도 궁금하네용",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100024856.1.20",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 몰랐습니다..",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 몰랐습니다..",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100024856.1.21",
          "form": "편의점에서 저 막.. 그 롤빵 사먹고",
          "original_form": "편의점에서 저 막.. 그 롤빵 사먹고",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100024856.1.22",
          "form": "그랬어요 ㅠㅠㅠ",
          "original_form": "그랬어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100024856.1.23",
          "form": "맞아요 ㅠㅠ 시루이코이비토 ? 그것도 먹어보고 싶어요",
          "original_form": "맞아요 ㅠㅠ 시루이코이비토 ? 그것도 먹어보고 싶어요",
          "speaker_id": "1",
          "time": "20210901 20:29"
        }
      ]
