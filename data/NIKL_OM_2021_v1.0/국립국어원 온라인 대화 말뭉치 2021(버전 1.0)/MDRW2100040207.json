      "utterance": [
        {
          "id": "MDRW2100040207.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100040207.1.2",
          "form": "이제 하려구요",
          "original_form": "이제 하려구요",
          "speaker_id": "1",
          "time": "20210904 14:45"
        },
        {
          "id": "MDRW2100040207.1.3",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "1",
          "time": "20210904 14:46"
        },
        {
          "id": "MDRW2100040207.1.4",
          "form": "네 저는 삼각김밥만들어먹었어요ㅋ",
          "original_form": "네 저는 삼각김밥만들어먹었어요ㅋ",
          "speaker_id": "2",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100040207.1.5",
          "form": "뭐드세요?",
          "original_form": "뭐드세요?",
          "speaker_id": "2",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100040207.1.6",
          "form": "라면이요ㅋㅋ",
          "original_form": "라면이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040207.1.7",
          "form": "두개 같이먹으면 세트네요",
          "original_form": "두개 같이먹으면 세트네요",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040207.1.8",
          "form": "네 그렇네오ㅋㅋ오늘아침에는",
          "original_form": "네 그렇네오ㅋㅋ오늘아침에는",
          "speaker_id": "2",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100040207.1.9",
          "form": "후레이크 먹었어요ㅋ",
          "original_form": "후레이크 먹었어요ㅋ",
          "speaker_id": "2",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100040207.1.10",
          "form": "아침드셨어요?",
          "original_form": "아침드셨어요?",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100040207.1.11",
          "form": "네 점심엔 비빔면먹었어요ㅎㅎ",
          "original_form": "네 점심엔 비빔면먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100040207.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100040207.1.13",
          "form": "비빔면 맛있죠ㅋㅋ",
          "original_form": "비빔면 맛있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:07"
        },
        {
          "id": "MDRW2100040207.1.14",
          "form": "저는 수육먹었어요ㅋㅋㅋㅋ",
          "original_form": "저는 수육먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:08"
        },
        {
          "id": "MDRW2100040207.1.15",
          "form": "오랫만에 수육먹으니 맛있더라구요ㅋㅋ",
          "original_form": "오랫만에 수육먹으니 맛있더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:08"
        },
        {
          "id": "MDRW2100040207.1.16",
          "form": "맛있었겠네요ㅎㅎ",
          "original_form": "맛있었겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100040207.1.17",
          "form": "저는 고등어먹었어요",
          "original_form": "저는 고등어먹었어요",
          "speaker_id": "1",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100040207.1.18",
          "form": "고등어도 맛있죠 ㅋㅋㅋㅋ 좋아하는 음식은 뭔가요?ㅋ",
          "original_form": "고등어도 맛있죠 ㅋㅋㅋㅋ 좋아하는 음식은 뭔가요?ㅋ",
          "speaker_id": "2",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100040207.1.19",
          "form": "요새는 스시 좋아해요",
          "original_form": "요새는 스시 좋아해요",
          "speaker_id": "1",
          "time": "20210905 20:10"
        },
        {
          "id": "MDRW2100040207.1.20",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 20:10"
        },
        {
          "id": "MDRW2100040207.1.21",
          "form": "오 저도 스시 정말 좋아해요 ㅋㅋ",
          "original_form": "오 저도 스시 정말 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100040207.1.22",
          "form": "옛날에는 초밥, 회 못먹었거든요 ㅋ",
          "original_form": "옛날에는 초밥, 회 못먹었거든요 ㅋ",
          "speaker_id": "2",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100040207.1.23",
          "form": "나이가 드니 ㅋㅋㅋㅋ 입맛이 달라지더라구요",
          "original_form": "나이가 드니 ㅋㅋㅋㅋ 입맛이 달라지더라구요",
          "speaker_id": "2",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100040207.1.24",
          "form": "맞아요ㅋㅋ 저도 회 안먹다가 먹게됐어요",
          "original_form": "맞아요ㅋㅋ 저도 회 안먹다가 먹게됐어요",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100040207.1.25",
          "form": "스시 잘하는집가면 정말 맛있어요",
          "original_form": "스시 잘하는집가면 정말 맛있어요",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100040207.1.26",
          "form": "네 그러니깐요 ㅋㅋ 스시 맛있어요 ㅋ",
          "original_form": "네 그러니깐요 ㅋㅋ 스시 맛있어요 ㅋ",
          "speaker_id": "2",
          "time": "20210905 20:19"
        }
      ]
    }
