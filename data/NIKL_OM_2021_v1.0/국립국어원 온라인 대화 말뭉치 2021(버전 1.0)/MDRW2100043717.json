      "utterance": [
        {
          "id": "MDRW2100043717.1.1",
          "form": "킹받는가격",
          "original_form": "킹받는가격",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043717.1.2",
          "form": "그래도 라자냐 꼿 기회되면",
          "original_form": "그래도 라자냐 꼿 기회되면",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043717.1.3",
          "form": "사먹어볼겡ㅅ",
          "original_form": "사먹어볼겡ㅅ",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043717.1.4",
          "form": "설명을 잘해주셔서 넘 맛날거같아요",
          "original_form": "설명을 잘해주셔서 넘 맛날거같아요",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043717.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043717.1.6",
          "form": "기대되네요",
          "original_form": "기대되네요",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043717.1.7",
          "form": "언젠가 드실 기회가 생긴다면",
          "original_form": "언젠가 드실 기회가 생긴다면",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043717.1.8",
          "form": "name2을 기억하면",
          "original_form": "&name2&을 기억하면",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043717.1.9",
          "form": "기억하면서",
          "original_form": "기억하면서",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043717.1.10",
          "form": "드셔주세요",
          "original_form": "드셔주세요",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043717.1.11",
          "form": "알겠습니다~~~ 라자냐하면 딱 떠오를거같아요!",
          "original_form": "알겠습니다~~~ 라자냐하면 딱 떠오를거같아요!",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043717.1.12",
          "form": "전 그전엔 냑간 관심없는 음식이어서",
          "original_form": "전 그전엔 냑간 관심없는 음식이어서",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043717.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043717.1.14",
          "form": "저는 막 소울푸드는 없는데",
          "original_form": "저는 막 소울푸드는 없는데",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043717.1.15",
          "form": "지금은 미역국이 너무 먹고싶어요!",
          "original_form": "지금은 미역국이 너무 먹고싶어요!",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043717.1.16",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043717.1.17",
          "form": "미역국",
          "original_form": "미역국",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043717.1.18",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043717.1.19",
          "form": "name1님은 미역국에",
          "original_form": "&name1&님은 미역국에",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043717.1.20",
          "form": "특별히 넣는 토핑같은게있으신가요",
          "original_form": "특별히 넣는 토핑같은게있으신가요",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043717.1.21",
          "form": "막 쇠고기라던가",
          "original_form": "막 쇠고기라던가",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043717.1.22",
          "form": "조개라던가",
          "original_form": "조개라던가",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043717.1.23",
          "form": "굴같은...",
          "original_form": "굴같은...",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043717.1.24",
          "form": "전 무조갼 소고기입나다..!",
          "original_form": "전 무조갼 소고기입나다..!",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043717.1.25",
          "form": "소고기미역국이 아닌 미역국은 상상할 수 얎어요",
          "original_form": "소고기미역국이 아닌 미역국은 상상할 수 얎어요",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043717.1.26",
          "form": "이야",
          "original_form": "이야",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043717.1.27",
          "form": "그렇습니다...",
          "original_form": "그렇습니다...",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043717.1.28",
          "form": "제 어머니는...",
          "original_form": "제 어머니는...",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043717.1.29",
          "form": "전라도분이셔서그런지",
          "original_form": "전라도분이셔서그런지",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043717.1.30",
          "form": "해산물을 참좋아하셔요",
          "original_form": "해산물을 참좋아하셔요",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043717.1.31",
          "form": "그래서... 종종...제가 싫어하는 굴을...",
          "original_form": "그래서... 종종...제가 싫어하는 굴을...",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043717.1.32",
          "form": "넣어주시는데...",
          "original_form": "넣어주시는데...",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043717.1.33",
          "form": "ㅜㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅜㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043717.1.34",
          "form": "넘 슬퍼서 굴만 빼고 냅다 밥말아먹습니다",
          "original_form": "넘 슬퍼서 굴만 빼고 냅다 밥말아먹습니다",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043717.1.35",
          "form": "아니 이런~!~!",
          "original_form": "아니 이런~!~!",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043717.1.36",
          "form": "굴을 넣으시는운요!!",
          "original_form": "굴을 넣으시는운요!!",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043717.1.37",
          "form": "굴 넣는건 처음 들어요!",
          "original_form": "굴 넣는건 처음 들어요!",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043717.1.38",
          "form": "저는 김장김치랑 먹는 생굴만 좋아해요!",
          "original_form": "저는 김장김치랑 먹는 생굴만 좋아해요!",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043717.1.39",
          "form": "오오오옹옹",
          "original_form": "오오오옹옹",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043717.1.40",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043717.1.41",
          "form": "김장김치랑 먹는 생굴이요?!?!?!?!",
          "original_form": "김장김치랑 먹는 생굴이요?!?!?!?!",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043717.1.42",
          "form": "허거거거ㅓㄱ",
          "original_form": "허거거거ㅓㄱ",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043717.1.43",
          "form": "안비리신가요?!?!",
          "original_form": "안비리신가요?!?!",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043717.1.44",
          "form": "네!! 안비리고 맛있어요!!",
          "original_form": "네!! 안비리고 맛있어요!!",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043717.1.45",
          "form": "ㅇ렇게 얘기하니까 김장김치랑 수육이랑 굴 같이 먹고싶네요!",
          "original_form": "ㅇ렇게 얘기하니까 김장김치랑 수육이랑 굴 같이 먹고싶네요!",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043717.1.46",
          "form": "이야...",
          "original_form": "이야...",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043717.1.47",
          "form": "수육...",
          "original_form": "수육...",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043717.1.48",
          "form": "홍어도",
          "original_form": "홍어도",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043717.1.49",
          "form": "좋아하시는지요",
          "original_form": "좋아하시는지요",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043717.1.50",
          "form": "오.. 아니여..",
          "original_form": "오.. 아니여..",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043717.1.51",
          "form": "홍어는.. 시도조차.. 못해봤어요!",
          "original_form": "홍어는.. 시도조차.. 못해봤어요!",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043717.1.52",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043717.1.53",
          "form": "헐아니요",
          "original_form": "헐아니요",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043717.1.54",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043717.1.55",
          "form": "굴좋아한다고",
          "original_form": "굴좋아한다고",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043717.1.56",
          "form": "홍어를좋아하진않네요 사람들이",
          "original_form": "홍어를좋아하진않네요 사람들이",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043717.1.57",
          "form": "(당연함",
          "original_form": "(당연함",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043717.1.58",
          "form": "홍어는 가까이서 냄새 한번 맡았을 뿐인데..",
          "original_form": "홍어는 가까이서 냄새 한번 맡았을 뿐인데..",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043717.1.59",
          "form": "거의 눈물이 고일 지경이더라구여",
          "original_form": "거의 눈물이 고일 지경이더라구여",
          "speaker_id": "1",
          "time": "20210907 20:31"
        }
