      "utterance": [
        {
          "id": "MDRW2100031622.1.1",
          "form": "지금 냉동실에도",
          "original_form": "지금 냉동실에도",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031622.1.2",
          "form": "얼려두고있어요,,,,",
          "original_form": "얼려두고있어요,,,,",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031622.1.3",
          "form": "낼 떡볶이에 올려먹게,,,",
          "original_form": "낼 떡볶이에 올려먹게,,,",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031622.1.4",
          "form": "세상에... 떡볶이에 치즈...",
          "original_form": "세상에... 떡볶이에 치즈...",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031622.1.5",
          "form": "진짜 배우신분",
          "original_form": "진짜 배우신분",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031622.1.6",
          "form": "하...먹깨비죠,,,,",
          "original_form": "하...먹깨비죠,,,,",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031622.1.7",
          "form": "진짜 피자치즈... 촥촥 뿌려서 먹으면 최고죠",
          "original_form": "진짜 피자치즈... 촥촥 뿌려서 먹으면 최고죠",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031622.1.8",
          "form": "최근에 래핑카우가 1+1 이벤트로 팔아서\n엄청 사서 먹었어요",
          "original_form": "최근에 래핑카우가 1+1 이벤트로 팔아서\n엄청 사서 먹었어요",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031622.1.9",
          "form": "이렇게 단기간에 크림치즈 많이 먹은건 아마 처음일거에요",
          "original_form": "이렇게 단기간에 크림치즈 많이 먹은건 아마 처음일거에요",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031622.1.10",
          "form": "우아아아아앙 아",
          "original_form": "우아아아아앙 아",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031622.1.11",
          "form": "미치겠다 치즈...ㅠㅠ",
          "original_form": "미치겠다 치즈...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031622.1.12",
          "form": "아 밤인데 ㅠㅠㅠ 큰일났닼ㅋㅋ 내일 치즈 무조건 먹을거에요",
          "original_form": "아 밤인데 ㅠㅠㅠ 큰일났닼ㅋㅋ 내일 치즈 무조건 먹을거에요",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031622.1.13",
          "form": "하 저두 낼 꼭 ㅠㅠ",
          "original_form": "하 저두 낼 꼭 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031622.1.14",
          "form": "진짜 떡볶이 꼭!!!",
          "original_form": "진짜 떡볶이 꼭!!!",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031622.1.15",
          "form": "후항...소울메이트 ㅠㅠ",
          "original_form": "후항...소울메이트 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031622.1.16",
          "form": "전떡볶이먹었네욤!",
          "original_form": "전떡볶이먹었네욤!",
          "speaker_id": "2",
          "time": "20210905 11:15"
        },
        {
          "id": "MDRW2100031622.1.17",
          "form": "우왓 드셨군요!!",
          "original_form": "우왓 드셨군요!!",
          "speaker_id": "1",
          "time": "20210905 11:16"
        },
        {
          "id": "MDRW2100031622.1.18",
          "form": "전 아직... 공복이에요.... ㅠ",
          "original_form": "전 아직... 공복이에요.... ㅠ",
          "speaker_id": "1",
          "time": "20210905 11:16"
        },
        {
          "id": "MDRW2100031622.1.19",
          "form": "왜요오 ㅠㅠ챙겨드셔야죠 ㅠㅠ",
          "original_form": "왜요오 ㅠㅠ챙겨드셔야죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 11:16"
        },
        {
          "id": "MDRW2100031622.1.20",
          "form": "뭐랄까.. 깼는데... 집에... 먹을게 이렇게 없었나 하고 있었어요",
          "original_form": "뭐랄까.. 깼는데... 집에... 먹을게 이렇게 없었나 하고 있었어요",
          "speaker_id": "1",
          "time": "20210905 11:21"
        },
        {
          "id": "MDRW2100031622.1.21",
          "form": "분명 많은데 왜 먹을게 없..죠? ㅋㅋㅋ",
          "original_form": "분명 많은데 왜 먹을게 없..죠? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100031622.1.22",
          "form": "어..음..원래 항상내가먹고싶은게 없으면",
          "original_form": "어..음..원래 항상내가먹고싶은게 없으면",
          "speaker_id": "2",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100031622.1.23",
          "form": "먹을게없는거죠",
          "original_form": "먹을게없는거죠",
          "speaker_id": "2",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100031622.1.24",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100031622.1.25",
          "form": "그..그러게요 허허..",
          "original_form": "그..그러게요 허허..",
          "speaker_id": "1",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100031622.1.26",
          "form": "일단은 백향과로 에이드 만들어 먹고있어요",
          "original_form": "일단은 백향과로 에이드 만들어 먹고있어요",
          "speaker_id": "1",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100031622.1.27",
          "form": "우아...금손이시다",
          "original_form": "우아...금손이시다",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100031622.1.28",
          "form": "앗... 아는 분이 청을 담아주셔서",
          "original_form": "앗... 아는 분이 청을 담아주셔서",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100031622.1.29",
          "form": "청을 넣고 얼음이랑 물 부우면 끝이에요 ㅋㅋㅋㅋ",
          "original_form": "청을 넣고 얼음이랑 물 부우면 끝이에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100031622.1.30",
          "form": "제가바로 이동네 요리똥손을 맡고있거든요",
          "original_form": "제가바로 이동네 요리똥손을 맡고있거든요",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100031622.1.31",
          "form": "딱 이정도 수준까진 저도 하니 누구나 다 만들 수 있을거에요",
          "original_form": "딱 이정도 수준까진 저도 하니 누구나 다 만들 수 있을거에요",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100031622.1.32",
          "form": "헐....맛있겠어요 ㅠㅠㅠ",
          "original_form": "헐....맛있겠어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100031622.1.33",
          "form": "달달하고 씨가 씹히는데 그게 또 맛있네요",
          "original_form": "달달하고 씨가 씹히는데 그게 또 맛있네요",
          "speaker_id": "1",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100031622.1.34",
          "form": "패션후르츠랑 같은 친구에요 ㅋㅋ",
          "original_form": "패션후르츠랑 같은 친구에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100031622.1.35",
          "form": "우리나라말로 백향과라고 부른데요",
          "original_form": "우리나라말로 백향과라고 부른데요",
          "speaker_id": "1",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100031622.1.36",
          "form": "향이 굉장히 다양하게 나서 이름이 그렇다고 하는데",
          "original_form": "향이 굉장히 다양하게 나서 이름이 그렇다고 하는데",
          "speaker_id": "1",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100031622.1.37",
          "form": "우아아아아응",
          "original_form": "우아아아아응",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100031622.1.38",
          "form": "좋은정보알아갑니당!",
          "original_form": "좋은정보알아갑니당!",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100031622.1.39",
          "form": "실제로 맛도 달고 새콤달콤해서 저는 잘 먹고 있어요 ㅋㅋ",
          "original_form": "실제로 맛도 달고 새콤달콤해서 저는 잘 먹고 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100031622.1.40",
          "form": "헐...먹고싶어요,,,!ㅋㅋㅋㅋ",
          "original_form": "헐...먹고싶어요,,,!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100031622.1.41",
          "form": "카페에서 가끔 팔기도 하니까 한번정도 시도해보는거 추천이에요!",
          "original_form": "카페에서 가끔 팔기도 하니까 한번정도 시도해보는거 추천이에요!",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100031622.1.42",
          "form": "우아아아아 패션후르츠는 저두 먹어요!종종",
          "original_form": "우아아아아 패션후르츠는 저두 먹어요!종종",
          "speaker_id": "2",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100031622.1.43",
          "form": "아아ㅏ앙 드시는구나아아",
          "original_form": "아아ㅏ앙 드시는구나아아{emoji:♡}",
          "speaker_id": "1",
          "time": "20210905 12:48"
        },
        {
          "id": "MDRW2100031622.1.44",
          "form": "이게 개구리알 같다고 싫다고 하는 사람도 많았어요 ㅠㅠ",
          "original_form": "이게 개구리알 같다고 싫다고 하는 사람도 많았어요 ㅠㅠ",
          "speaker_id": "1",
