        {
          "id": "MDRW2100008626.1.1",
          "form": "집안일은 뭐가 힘들어요",
          "original_form": "집안일은 뭐가 힘들어요",
          "speaker_id": "2",
          "time": "20210822 17:05"
        },
        {
          "id": "MDRW2100008626.1.2",
          "form": "창소요",
          "original_form": "창소요",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008626.1.3",
          "form": "청소",
          "original_form": "청소",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008626.1.4",
          "form": "무슨 청소요?",
          "original_form": "무슨 청소요?",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008626.1.5",
          "form": "화장실이대박이죠",
          "original_form": "화장실이대박이죠",
          "speaker_id": "1",
          "time": "20210823 07:35"
        },
        {
          "id": "MDRW2100008626.1.6",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "2",
          "time": "20210823 09:12"
        },
        {
          "id": "MDRW2100008626.1.7",
          "form": "하고나면 허리아프고",
          "original_form": "하고나면 허리아프고",
          "speaker_id": "2",
          "time": "20210823 09:12"
        },
        {
          "id": "MDRW2100008626.1.8",
          "form": "맞아요 티도 안나거",
          "original_form": "맞아요 티도 안나거",
          "speaker_id": "1",
          "time": "20210823 09:52"
        },
        {
          "id": "MDRW2100008626.1.9",
          "form": "근데 또 미루다보면 엄청 커져서",
          "original_form": "근데 또 미루다보면 엄청 커져서",
          "speaker_id": "2",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100008626.1.10",
          "form": "감당이..",
          "original_form": "감당이..",
          "speaker_id": "2",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100008626.1.11",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100008626.1.12",
          "form": "저는 빨래가 힘들더라고여",
          "original_form": "저는 빨래가 힘들더라고여",
          "speaker_id": "2",
          "time": "20210823 17:28"
        },
        {
          "id": "MDRW2100008626.1.13",
          "form": "것도 맞아요",
          "original_form": "것도 맞아요",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100008626.1.14",
          "form": "집은 마음에드세요?",
          "original_form": "집은 마음에드세요?",
          "speaker_id": "2",
          "time": "20210824 00:43"
