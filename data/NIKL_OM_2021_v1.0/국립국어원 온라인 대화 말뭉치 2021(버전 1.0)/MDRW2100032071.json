      "utterance": [
        {
          "id": "MDRW2100032071.1.1",
          "form": "남이 타 주는 커피 먹고 싶어요",
          "original_form": "남이 타 주는 커피 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.2",
          "form": "얼른 나가서 찾아 드시면 되죠",
          "original_form": "얼른 나가서 찾아 드시면 되죠",
          "speaker_id": "2",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.3",
          "form": "역시 돈 쓰는게 짱이야",
          "original_form": "역시 돈 쓰는게 짱이야",
          "speaker_id": "1",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.4",
          "form": "테이크아웃은 참 저렴해요",
          "original_form": "테이크아웃은 참 저렴해요",
          "speaker_id": "2",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.5",
          "form": "아직 씻지도 않았어요",
          "original_form": "아직 씻지도 않았어요",
          "speaker_id": "1",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.6",
          "form": "선택권이 많아졌죠",
          "original_form": "선택권이 많아졌죠",
          "speaker_id": "2",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.7",
          "form": "테이크아웃이 편하죠",
          "original_form": "테이크아웃이 편하죠",
          "speaker_id": "1",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.8",
          "form": "마스크를 쓰니까 그냥 막 나가도 괜찮아요",
          "original_form": "마스크를 쓰니까 그냥 막 나가도 괜찮아요",
          "speaker_id": "2",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.9",
          "form": "편한 옷차림이 대세 이잖아요",
          "original_form": "편한 옷차림이 대세 이잖아요",
          "speaker_id": "2",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.10",
          "form": "고고",
          "original_form": "고고",
          "speaker_id": "2",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.11",
          "form": "머리에 기름기가 껄껄",
          "original_form": "머리에 기름기가 껄껄",
          "speaker_id": "1",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.12",
          "form": "머리띠하고 똥머리 하고 나가요",
          "original_form": "머리띠하고 똥머리 하고 나가요",
          "speaker_id": "2",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.13",
          "form": "아 저는 아포카토 더 먹고 싶네요",
          "original_form": "아 저는 아포카토 더 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100032071.1.14",
          "form": "집 근처에 커피숍이 있나요",
          "original_form": "집 근처에 커피숍이 있나요",
          "speaker_id": "2",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100032071.1.15",
          "form": "바닐라 아이스크림을 사야겠네요",
          "original_form": "바닐라 아이스크림을 사야겠네요",
          "speaker_id": "2",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100032071.1.16",
          "form": "아이스크림 사 와서 아보카도 해 먹고 싶어요",
          "original_form": "아이스크림 사 와서 아보카도 해 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100032071.1.17",
          "form": "근처에 무인으로 아이스크림 파는 가게가 있거든요",
          "original_form": "근처에 무인으로 아이스크림 파는 가게가 있거든요",
          "speaker_id": "1",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100032071.1.18",
          "form": "그거 괜찮죠  양껏 먹죠",
          "original_form": "그거 괜찮죠  양껏 먹죠",
          "speaker_id": "2",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100032071.1.19",
          "form": "편의점 봄이나 마트보다 훨씬 저렴하게 살 수 있는 것 같아서 좋아요",
          "original_form": "편의점 봄이나 마트보다 훨씬 저렴하게 살 수 있는 것 같아서 좋아요",
          "speaker_id": "1",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100032071.1.20",
          "form": "결제는 현금도 되나요",
          "original_form": "결제는 현금도 되나요",
          "speaker_id": "2",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100032071.1.21",
          "form": "편의점 이벤트 아니면 비싸요",
          "original_form": "편의점 이벤트 아니면 비싸요",
          "speaker_id": "2",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100032071.1.22",
          "form": "현금도 있는 거 같지만 카드만 달랑 들고 가서 결제를 해서 잘 안 해 봐서 모르겠어요",
          "original_form": "현금도 있는 거 같지만 카드만 달랑 들고 가서 결제를 해서 잘 안 해 봐서 모르겠어요",
          "speaker_id": "1",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100032071.1.23",
          "form": "오늘 과연 밖에 나갈 수 있을까요 올리브영 가고 싶어요",
          "original_form": "오늘 과연 밖에 나갈 수 있을까요 올리브영 가고 싶어요",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032071.1.24",
          "form": "씻기가 귀찮아요",
          "original_form": "씻기가 귀찮아요",
          "speaker_id": "1",
