      "utterance": [
        {
          "id": "MDRW2100015446.1.1",
          "form": "드길 1이 진짜 명작이죠",
          "original_form": "드길 1이 진짜 명작이죠",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015446.1.2",
          "form": "귀여운건 3편까지 쭉 귀엽답니다",
          "original_form": "귀여운건 3편까지 쭉 귀엽답니다",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015446.1.3",
          "form": "3편에 투슬리스 결혼해요",
          "original_form": "3편에 투슬리스 결혼해요",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015446.1.4",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015446.1.5",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015446.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015446.1.7",
          "form": "라이트 퓨리라고",
          "original_form": "라이트 퓨리라고",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015446.1.8",
          "form": "흰색 용",
          "original_form": "흰색 용",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015446.1.9",
          "form": "그런 일이 있다니",
          "original_form": "그런 일이 있다니",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015446.1.10",
          "form": "그 있지 예지 닮은 용이랑",
          "original_form": "그 있지 예지 닮은 용이랑",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015446.1.11",
          "form": "아 네네",
          "original_form": "아 네네",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015446.1.12",
          "form": "투슬리스..결혼...글고 애 셋",
          "original_form": "투슬리스..결혼...글고 애 셋",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015446.1.13",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015446.1.14",
          "form": "투슬리스 이자식..",
          "original_form": "투슬리스 이자식..",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015446.1.15",
          "form": "매우 귀엽답니다",
          "original_form": "매우 귀엽답니다",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015446.1.16",
          "form": "귀여운 아가를 셋이나",
          "original_form": "귀여운 아가를 셋이나",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015446.1.17",
          "form": "투슬리스가 한국인보다 낫다..!",
          "original_form": "투슬리스가 한국인보다 낫다..!",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015446.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아 웃기다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아 웃기다",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015446.1.19",
          "form": "또 뭐 좋아해요",
          "original_form": "또 뭐 좋아해요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015446.1.20",
          "form": "투슬리스 인형도 있어요",
          "original_form": "투슬리스 인형도 있어요",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015446.1.21",
          "form": "엣헴",
          "original_form": "엣헴",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015446.1.22",
          "form": "친구가 선물로 줬답니다",
          "original_form": "친구가 선물로 줬답니다",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015446.1.23",
          "form": "또 좋아하는 거라...",
          "original_form": "또 좋아하는 거라...",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015446.1.24",
          "form": "성덕이시네요",
          "original_form": "성덕이시네요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        }
      ]
