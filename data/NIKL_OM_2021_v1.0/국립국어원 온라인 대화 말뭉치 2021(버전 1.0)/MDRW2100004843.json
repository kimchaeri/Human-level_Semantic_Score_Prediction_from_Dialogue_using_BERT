      "utterance": [
        {
          "id": "MDRW2100004843.1.1",
          "form": "종교 없어요!",
          "original_form": "종교 없어요!",
          "speaker_id": "2",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004843.1.2",
          "form": "부모님은 불교요",
          "original_form": "부모님은 불교요",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004843.1.3",
          "form": "친구분 따라서 교회 가보신 적 있다고",
          "original_form": "친구분 따라서 교회 가보신 적 있다고",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004843.1.4",
          "form": "하시지 않았나요?",
          "original_form": "하시지 않았나요?",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004843.1.5",
          "form": "어릴때 그렇죠 ㅎㄹ",
          "original_form": "어릴때 그렇죠 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004843.1.6",
          "form": "제 친구도",
          "original_form": "제 친구도",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004843.1.7",
          "form": "어릴때 친구따라서 교회 가 봤다고 하더라구요",
          "original_form": "어릴때 친구따라서 교회 가 봤다고 하더라구요",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004843.1.8",
          "form": "그때는 어릴때고 교회분들이 먹을 거 나눠주셔서",
          "original_form": "그때는 어릴때고 교회분들이 먹을 거 나눠주셔서",
          "speaker_id": "2",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004843.1.9",
          "form": "좋았데요",
          "original_form": "좋았데요",
          "speaker_id": "2",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004843.1.10",
          "form": "네 그때는 재밌었아오",
          "original_form": "네 그때는 재밌었아오",
          "speaker_id": "1",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004843.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004843.1.12",
          "form": "중학교때",
          "original_form": "중학교때",
          "speaker_id": "2",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004843.1.13",
          "form": "하교하면 교회에서 간식 많이 나눠주셨던거 같아요",
          "original_form": "하교하면 교회에서 간식 많이 나눠주셨던거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004843.1.14",
          "form": "전도가 주목적이시지만...",
          "original_form": "전도가 주목적이시지만...",
          "speaker_id": "2",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004843.1.15",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210821 18:57"
        },
        {
          "id": "MDRW2100004843.1.16",
          "form": "저는 항상 버스정류장에서 붙잡혔던거 같아요",
          "original_form": "저는 항상 버스정류장에서 붙잡혔던거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:59"
        },
        {
          "id": "MDRW2100004843.1.17",
          "form": "도를 아십니까 아녜요?",
          "original_form": "도를 아십니까 아녜요?",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004843.1.18",
          "form": "그건 다행히 아니였어요!!",
          "original_form": "그건 다행히 아니였어요!!",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004843.1.19",
          "form": "근처 교회 다니시던 어르신이였어요",
          "original_form": "근처 교회 다니시던 어르신이였어요",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004843.1.20",
          "form": "계속 오라고 하셔서",
          "original_form": "계속 오라고 하셔서",
          "speaker_id": "2",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004843.1.21",
          "form": "\"가고 싶은데 지금은 바빠서 못 가요, 졸업 후에 갈께요\"라고 말씀드렸던거 같아요",
          "original_form": "\"가고 싶은데 지금은 바빠서 못 가요, 졸업 후에 갈께요\"라고 말씀드렸던거 같아요",
          "speaker_id": "2",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004843.1.22",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004843.1.23",
          "form": "절은 역사나 문학 적으로 유명한 곳이나",
          "original_form": "절은 역사나 문학 적으로 유명한 곳이나",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004843.1.24",
          "form": "부처님 오신 날에 주로 갔던거 같아요!",
          "original_form": "부처님 오신 날에 주로 갔던거 같아요!",
          "speaker_id": "2",
          "time": "20210821 19:21"
        }
      ]
    }
