      "utterance": [
        {
          "id": "MDRW2100005800.1.1",
          "form": "님 머리색 어케됨",
          "original_form": "님 머리색 어케됨",
          "speaker_id": "1",
          "time": "20210907 12:36"
        },
        {
          "id": "MDRW2100005800.1.2",
          "form": "미용과 건강 요즘 잘 챙기는거지",
          "original_form": "미용과 건강 요즘 잘 챙기는거지",
          "speaker_id": "2",
          "time": "20210907 12:36"
        },
        {
          "id": "MDRW2100005800.1.3",
          "form": "파랑 하루이틀만에 빠지고",
          "original_form": "파랑 하루이틀만에 빠지고",
          "speaker_id": "2",
          "time": "20210907 12:37"
        },
        {
          "id": "MDRW2100005800.1.4",
          "form": "요즘은 은발생활중이지",
          "original_form": "요즘은 은발생활중이지",
          "speaker_id": "2",
          "time": "20210907 12:37"
        },
        {
          "id": "MDRW2100005800.1.5",
          "form": "다음에 날 잡고 매직 받으러 갈 예정",
          "original_form": "다음에 날 잡고 매직 받으러 갈 예정",
          "speaker_id": "2",
          "time": "20210907 12:37"
        },
        {
          "id": "MDRW2100005800.1.6",
          "form": "다른색으로 한번더 안입혀?",
          "original_form": "다른색으로 한번더 안입혀?",
          "speaker_id": "1",
          "time": "20210907 12:39"
        },
        {
          "id": "MDRW2100005800.1.7",
          "form": "흠 이번에 재난지원금 받으면 그래볼까?",
          "original_form": "흠 이번에 재난지원금 받으면 그래볼까?",
          "speaker_id": "2",
          "time": "20210907 12:39"
        },
        {
          "id": "MDRW2100005800.1.8",
          "form": "여기서 더 밝게는 머리가 못 버텨줄거고",
          "original_form": "여기서 더 밝게는 머리가 못 버텨줄거고",
          "speaker_id": "2",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100005800.1.9",
          "form": "흰머리까지 가볼까 고민중",
          "original_form": "흰머리까지 가볼까 고민중",
          "speaker_id": "2",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100005800.1.10",
          "form": "아냐.. 두피 지켜",
          "original_form": "아냐.. 두피 지켜",
          "speaker_id": "1",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100005800.1.11",
          "form": "너도 두피 별로 안건강하자너ㅠㅠ",
          "original_form": "너도 두피 별로 안건강하자너ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100005800.1.12",
          "form": "당장 지금도 뒷머리 심각함",
          "original_form": "당장 지금도 뒷머리 심각함",
          "speaker_id": "2",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100005800.1.13",
          "form": "막 뭐 나다보니",
          "original_form": "막 뭐 나다보니",
          "speaker_id": "2",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100005800.1.14",
          "form": "그냥 매직으로 끝내야할듯ㅋㅋ",
          "original_form": "그냥 매직으로 끝내야할듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:45"
        },
        {
          "id": "MDRW2100005800.1.15",
          "form": "갑자기 탈색은왜한겨 ㅋㅋㅋ근데 ㅋㅋㅋㅋㅋ",
          "original_form": "갑자기 탈색은왜한겨 ㅋㅋㅋ근데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:47"
        },
        {
          "id": "MDRW2100005800.1.16",
          "form": "그냥 기분전환겸 한참 하고싶은거 참다가 드디어",
          "original_form": "그냥 기분전환겸 한참 하고싶은거 참다가 드디어",
          "speaker_id": "2",
          "time": "20210907 12:48"
        },
        {
