      "utterance": [
        {
          "id": "MDRW2100030432.1.1",
          "form": "아무튼 시리즈 중에 특히 기억남는거 있으세요?",
          "original_form": "아무튼 시리즈 중에 특히 기억남는거 있으세요?",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030432.1.2",
          "form": "아무튼 술이 재밌었어요ㅎㅎ",
          "original_form": "아무튼 술이 재밌었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100030432.1.3",
          "form": "술을 그다지 좋아하지 않는데도",
          "original_form": "술을 그다지 좋아하지 않는데도",
          "speaker_id": "1",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100030432.1.4",
          "form": "글을 참 맛나게 쓰셨더라구요",
          "original_form": "글을 참 맛나게 쓰셨더라구요",
          "speaker_id": "1",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100030432.1.5",
          "form": "혹시 리디셀렉트 쓰세요?",
          "original_form": "혹시 리디셀렉트 쓰세요?",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030432.1.6",
          "form": "그건 뭔가요?ㅎㅎ",
          "original_form": "그건 뭔가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030432.1.7",
          "form": "이북정기구독제라고 보시면되요",
          "original_form": "이북정기구독제라고 보시면되요",
          "speaker_id": "1",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030432.1.8",
          "form": "스트리밍 사이트같은거죠",
          "original_form": "스트리밍 사이트같은거죠",
          "speaker_id": "1",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030432.1.9",
          "form": "아 밀리의 서재같은거군요",
          "original_form": "아 밀리의 서재같은거군요",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030432.1.10",
          "form": "여하튼 리디셀렉트에 아무튼 시리즈가 많아요",
          "original_form": "여하튼 리디셀렉트에 아무튼 시리즈가 많아요",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030432.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030432.1.12",
          "form": "전 이북리더기가 없어서ㅜㅜ",
          "original_form": "전 이북리더기가 없어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030432.1.13",
          "form": "갠적으론 아무튼 시리즈가 이북으로 읽기에 좋았어요",
          "original_form": "갠적으론 아무튼 시리즈가 이북으로 읽기에 좋았어요",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030432.1.14",
          "form": "너무 복잡하거나 무거운 책은 이북으로 읽기 힘들거든요",
          "original_form": "너무 복잡하거나 무거운 책은 이북으로 읽기 힘들거든요",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030432.1.15",
          "form": "이북리더기 갖고 계시나요?",
          "original_form": "이북리더기 갖고 계시나요?",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030432.1.16",
          "form": "하나 있긴 한데",
          "original_form": "하나 있긴 한데",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030432.1.17",
          "form": "실질적으론 폰으로 더 많이 보는듯요",
          "original_form": "실질적으론 폰으로 더 많이 보는듯요",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030432.1.18",
          "form": "각잡고 읽으려면 리더기가 눈 안아프고 좋긴해요",
          "original_form": "각잡고 읽으려면 리더기가 눈 안아프고 좋긴해요",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030432.1.19",
          "form": "아 폰으로 보시는군요ㅎㅎ와 잘 읽히나요? 눈이랑 손목 아플것같은 느낌",
          "original_form": "아 폰으로 보시는군요ㅎㅎ와 잘 읽히나요? 눈이랑 손목 아플것같은 느낌",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030432.1.20",
          "form": "읽을때는 잘 모르죠...",
          "original_form": "읽을때는 잘 모르죠...",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030432.1.21",
          "form": "사실 아프기 시작하면 이미 좀 늦은건데.... ㅎㅎ",
          "original_form": "사실 아프기 시작하면 이미 좀 늦은건데.... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030432.1.22",
          "form": "그래도 화면밝기를 낮추고 읽는 정도는 합니다.",
          "original_form": "그래도 화면밝기를 낮추고 읽는 정도는 합니다.",
          "speaker_id": "1",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030432.1.23",
          "form": "리디셀렉트에는 책 다양한가요?",
          "original_form": "리디셀렉트에는 책 다양한가요?",
          "speaker_id": "2",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030432.1.24",
          "form": "음... 꽤 많은 편이긴 한데",
          "original_form": "음... 꽤 많은 편이긴 한데",
          "speaker_id": "1",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100030432.1.25",
          "form": "인기작들도 종종 올라와요",
          "original_form": "인기작들도 종종 올라와요",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030432.1.26",
          "form": "좀 빨리 내려가서 그렇지",
          "original_form": "좀 빨리 내려가서 그렇지",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030432.1.27",
          "form": "다양성은 잘 모르겠어요",
          "original_form": "다양성은 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030432.1.28",
          "form": "제가 읽는것만 읽어서...",
          "original_form": "제가 읽는것만 읽어서...",
          "speaker_id": "1",
          "time": "20210904 01:15"
        }
      ]
    }
