      "utterance": [
        {
          "id": "MDRW2100013305.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013305.1.2",
          "form": "재택근무일자리 구하구싶어",
          "original_form": "재택근무일자리 구하구싶어",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013305.1.3",
          "form": "이제 콤퓨타도되는데말이야",
          "original_form": "이제 콤퓨타도되는데말이야",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013305.1.4",
          "form": "머선일이고",
          "original_form": "머선일이고",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013305.1.5",
          "form": "나도 재택하고싶다",
          "original_form": "나도 재택하고싶다",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013305.1.6",
          "form": "잠옷입고",
          "original_form": "잠옷입고",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013305.1.7",
          "form": "누워서 재택근무",
          "original_form": "누워서 재택근무",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013305.1.8",
          "form": "난 재택근무 일자리구하구시풩",
          "original_form": "난 재택근무 일자리구하구시풩",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013305.1.9",
          "form": "근데없어",
          "original_form": "근데없어",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013305.1.10",
          "form": "여기는 사람적어서 그런지",
          "original_form": "여기는 사람적어서 그런지",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013305.1.11",
          "form": "재택도 안시켜줘",
          "original_form": "재택도 안시켜줘",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013305.1.12",
          "form": "일하는거보면 재택해도될거같은데",
          "original_form": "일하는거보면 재택해도될거같은데",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013305.1.13",
          "form": "내생각두 그래",
          "original_form": "내생각두 그래",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013305.1.14",
          "form": "재택은 거의 프리랜서아님?",
          "original_form": "재택은 거의 프리랜서아님?",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013305.1.15",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013305.1.16",
          "form": "프리랜서지",
          "original_form": "프리랜서지",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013305.1.17",
          "form": "역시 사람은 기술을 배워야",
          "original_form": "역시 사람은 기술을 배워야",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013305.1.18",
          "form": "근데 재택일자리 아는게 없어서",
          "original_form": "근데 재택일자리 아는게 없어서",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013305.1.19",
          "form": "대화가 안되는데",
          "original_form": "대화가 안되는데",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013305.1.20",
          "form": "기술이 필요해",
          "original_form": "기술이 필요해",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013305.1.21",
          "form": "그",
          "original_form": "그",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013305.1.22",
          "form": "워드작업",
          "original_form": "워드작업",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013305.1.23",
          "form": "서류같은거",
          "original_form": "서류같은거",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013305.1.24",
          "form": "인형눈붙이는거도 재택이긴하지",
          "original_form": "인형눈붙이는거도 재택이긴하지",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013305.1.25",
          "form": "워드정리해주는거있던데",
          "original_form": "워드정리해주는거있던데",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013305.1.26",
          "form": "요새는 그런거없자나",
          "original_form": "요새는 그런거없자나",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013305.1.27",
          "form": "무슨 옛날얘기를 하구있어",
          "original_form": "무슨 옛날얘기를 하구있어",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013305.1.28",
          "form": "니가했잖앙",
          "original_form": "니가했잖앙",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013305.1.29",
          "form": "요즘은 재택하려면 스펙두 좋아야댈듯",
          "original_form": "요즘은 재택하려면 스펙두 좋아야댈듯",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013305.1.30",
          "form": "자격증필수일듯",
          "original_form": "자격증필수일듯",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013305.1.31",
          "form": "번역해주는것도",
          "original_form": "번역해주는것도",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013305.1.32",
          "form": "재택가능이잖아",
          "original_form": "재택가능이잖아",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013305.1.33",
          "form": "번역은 자리없지않나",
          "original_form": "번역은 자리없지않나",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013305.1.34",
          "form": "진짜 잘해야 줄 듯",
          "original_form": "진짜 잘해야 줄 듯",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013305.1.35",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013305.1.36",
          "form": "아니면 잘 안배우는 외국어나",
          "original_form": "아니면 잘 안배우는 외국어나",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013305.1.37",
          "form": "구글같은거돌리면",
          "original_form": "구글같은거돌리면",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013305.1.38",
          "form": "끝장이지",
          "original_form": "끝장이지",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013305.1.39",
          "form": "불어같은거",
          "original_form": "불어같은거",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013305.1.40",
          "form": "구글 넘 부자연스러워서 안댐",
          "original_form": "구글 넘 부자연스러워서 안댐",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013305.1.41",
          "form": "요새는 재택근무 구하기 어려워",
          "original_form": "요새는 재택근무 구하기 어려워",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013305.1.42",
          "form": "개꿀이면 다 할라하지",
          "original_form": "개꿀이면 다 할라하지",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013305.1.43",
          "form": "그런거는 이미 다",
          "original_form": "그런거는 이미 다",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013305.1.44",
          "form": "하고있을듯",
          "original_form": "하고있을듯",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013305.1.45",
          "form": "손빠르고 능력좋은 사람들",
          "original_form": "손빠르고 능력좋은 사람들",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013305.1.46",
          "form": "글고 진짜 잘하면 일이 걍 찾아오지ㅠ",
          "original_form": "글고 진짜 잘하면 일이 걍 찾아오지ㅠ",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013305.1.47",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013305.1.48",
          "form": "개부럽다",
          "original_form": "개부럽다",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013305.1.49",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013305.1.50",
          "form": "일자리 구하기가",
          "original_form": "일자리 구하기가",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013305.1.51",
          "form": "이러케 힘들다니",
          "original_form": "이러케 힘들다니",
          "speaker_id": "1",
          "time": "20210825 14:05"
        }
      ]
    }
  ]
