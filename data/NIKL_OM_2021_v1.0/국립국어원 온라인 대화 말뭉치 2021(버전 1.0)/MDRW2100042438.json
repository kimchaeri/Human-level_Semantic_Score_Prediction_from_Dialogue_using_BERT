      "utterance": [
        {
          "id": "MDRW2100042438.1.1",
          "form": "요즘 과일값이 비싸더라",
          "original_form": "요즘 과일값이 비싸더라",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042438.1.2",
          "form": "맛있는 과일 찾기가 진짜 하늘의 별따기예요 ㅠㅠ",
          "original_form": "맛있는 과일 찾기가 진짜 하늘의 별따기예요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042438.1.3",
          "form": "그거때문인거같아 비 많이 와서",
          "original_form": "그거때문인거같아 비 많이 와서",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042438.1.4",
          "form": "과일 막 떨어지나요",
          "original_form": "과일 막 떨어지나요",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042438.1.5",
          "form": "아닌가 폭염때문인가",
          "original_form": "아닌가 폭염때문인가",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042438.1.6",
          "form": "둘다일수도 있구 아닐수도 있구",
          "original_form": "둘다일수도 있구 아닐수도 있구",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042438.1.7",
          "form": "날 선선해지니까 슬슬 맛있는 과일 나오기 시작했으면 좋겠다",
          "original_form": "날 선선해지니까 슬슬 맛있는 과일 나오기 시작했으면 좋겠다",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042438.1.8",
          "form": "엄마는 배도 좋아하시니까!",
          "original_form": "엄마는 배도 좋아하시니까!",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042438.1.9",
          "form": "배도 좋아하고 사과도 좋아하고 홍시도 좋아하지",
          "original_form": "배도 좋아하고 사과도 좋아하고 홍시도 좋아하지",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042438.1.10",
          "form": "홍시는 너무 먹기 힘들어서 별룬데",
          "original_form": "홍시는 너무 먹기 힘들어서 별룬데",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042438.1.11",
          "form": "홍시 맛있어 달달하고",
          "original_form": "홍시 맛있어 달달하고",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042438.1.12",
          "form": "곶감도 별로예요 단감만 좋아",
          "original_form": "곶감도 별로예요 단감만 좋아",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042438.1.13",
          "form": "곶감도 맛있는데 뭘 모르는구나",
          "original_form": "곶감도 맛있는데 뭘 모르는구나",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042438.1.14",
          "form": "저는 어린애 입맛이니까",
          "original_form": "저는 어린애 입맛이니까{emoji:…}",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042438.1.15",
          "form": "과일 가리는 것도 편식인가",
          "original_form": "과일 가리는 것도 편식인가",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042438.1.16",
          "form": "그것까지 편식에 넣으면 세상의 어린아이들한테 너무 미안하지 않나요",
          "original_form": "그것까지 편식에 넣으면 세상의 어린아이들한테 너무 미안하지 않나요",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042438.1.17",
          "form": "세상의 우리 딸한테만 미안하겠지",
          "original_form": "세상의 우리 딸한테만 미안하겠지",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042438.1.18",
          "form": "아니 안미안하다",
          "original_form": "아니 안미안하다",
          "speaker_id": "2",
          "time": "20210906 22:20"
        }
      ]
