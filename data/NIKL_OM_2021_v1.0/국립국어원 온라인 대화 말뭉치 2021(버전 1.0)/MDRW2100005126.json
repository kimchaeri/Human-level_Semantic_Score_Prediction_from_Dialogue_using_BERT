      "utterance": [
        {
          "id": "MDRW2100005126.1.1",
          "form": "요새 티비 보는거 있니??",
          "original_form": "요새 티비 보는거 있니??",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005126.1.2",
          "form": "아까 놀면뭐하니 잠깐봤는데 MBC뉴스 앵커도하네?ㅋ",
          "original_form": "아까 놀면뭐하니 잠깐봤는데 MBC뉴스 앵커도하네?ㅋ",
          "speaker_id": "1",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005126.1.3",
          "form": "다음주에 하는건가봐 나도 예고편 봣어",
          "original_form": "다음주에 하는건가봐 나도 예고편 봣어",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005126.1.4",
          "form": "근데 하하랑 정준하도 나오나본데",
          "original_form": "근데 하하랑 정준하도 나오나본데",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005126.1.5",
          "form": "넘 무도멤버 재활용인듯",
          "original_form": "넘 무도멤버 재활용인듯",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005126.1.6",
          "form": "박명수는 안나왓든데",
          "original_form": "박명수는 안나왓든데",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005126.1.7",
          "form": "그려  그나마 ㅋ",
          "original_form": "그려  그나마 ㅋ",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005126.1.8",
          "form": "놀면뭐하니  다나왓으먄 별로였을지도",
          "original_form": "놀면뭐하니  다나왓으먄 별로였을지도",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005126.1.9",
          "form": "정준하 근데 살엄청쪄서 딴사람같더라",
          "original_form": "정준하 근데 살엄청쪄서 딴사람같더라",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005126.1.10",
          "form": "응 그렇긴해",
          "original_form": "응 그렇긴해",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005126.1.11",
          "form": "아 지금은 전참시 오빠가 틀어서 보고있는데 웃기네ㅋㅋ",
          "original_form": "아 지금은 전참시 오빠가 틀어서 보고있는데 웃기네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005126.1.12",
          "form": "그래? 울엄나는 광자매본다",
          "original_form": "그래? 울엄나는 광자매본다",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005126.1.13",
          "form": "그옆에 조그마한애는 유튜브보면서 뭐가 그기 가지고 싶은지",
          "original_form": "그옆에 조그마한애는 유튜브보면서 뭐가 그기 가지고 싶은지",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005126.1.14",
          "form": "다가지고 싶데",
          "original_form": "다가지고 싶데",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005126.1.15",
          "form": "조카?ㅋㅋ",
          "original_form": "조카?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005126.1.16",
          "form": "어 조카지",
          "original_form": "어 조카지",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005126.1.17",
          "form": "난 다시 건축탐구 보는중ㅋ",
          "original_form": "난 다시 건축탐구 보는중ㅋ",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005126.1.18",
          "form": "ㅋㅋㅋ 지금은 난 살림남 보고있짗",
          "original_form": "ㅋㅋㅋ 지금은 난 살림남 보고있짗",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005126.1.19",
          "form": "양궁선수 김제덕 나오네",
          "original_form": "양궁선수 김제덕 나오네",
