      "utterance": [
        {
          "id": "MDRW2100009435.1.1",
          "form": "아놔 답장못봤엉",
          "original_form": "아놔 답장못봤엉",
          "speaker_id": "1",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009435.1.2",
          "form": "구직활동중이신가요..",
          "original_form": "구직활동중이신가요..",
          "speaker_id": "1",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009435.1.3",
          "form": "아뇨....",
          "original_form": "아뇨....",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009435.1.4",
          "form": "저 아직 애기 대학생이라서...",
          "original_form": "저 아직 애기 대학생이라서...",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009435.1.5",
          "form": "토플같은 스펙은요?",
          "original_form": "토플같은 스펙은요?",
          "speaker_id": "1",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009435.1.6",
          "form": "-.- 없는데요 불만이신가요",
          "original_form": "-.- 없는데요 불만이신가요",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009435.1.7",
          "form": "대외활동만 했어요...",
          "original_form": "대외활동만 했어요...",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009435.1.8",
          "form": "온스는..요",
          "original_form": "온스는..요",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009435.1.9",
          "form": "토플같이 스터디하실래요?",
          "original_form": "토플같이 스터디하실래요?",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009435.1.10",
          "form": "저는 동아리하나가 끝이랍니다ㅠㅠ",
          "original_form": "저는 동아리하나가 끝이랍니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009435.1.11",
          "form": "헉 토플 스터디요??",
          "original_form": "헉 토플 스터디요??",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009435.1.12",
          "form": "토익.. 말구요...?!",
          "original_form": "토익.. 말구요...?!",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009435.1.13",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009435.1.14",
          "form": "토익이요!",
          "original_form": "토익이요!",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009435.1.15",
          "form": "저 국장붙었어여!!",
          "original_form": "저 국장붙었어여!!",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009435.1.16",
          "form": "헐 축하해ㄴ다!!",
          "original_form": "헐 축하해ㄴ다!!",
          "speaker_id": "2",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100009435.1.17",
          "form": "국장은 무슨 일 해여~?",
          "original_form": "국장은 무슨 일 해여~?",
          "speaker_id": "2",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100009435.1.18",
          "form": "국가근로라고했어야했는데...",
          "original_form": "국가근로라고했어야했는데...",
          "speaker_id": "1",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100009435.1.19",
          "form": "교개원에서 일해요",
          "original_form": "교개원에서 일해요",
          "speaker_id": "1",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100009435.1.20",
          "form": "교개원 냥이가 돼주실래요?",
          "original_form": "교개원 냥이가 돼주실래요?",
          "speaker_id": "1",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100009435.1.21",
          "form": "아르바이트가 이렇게 반가울줄이야ㅠㅠ",
          "original_form": "아르바이트가 이렇게 반가울줄이야ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100009435.1.22",
          "form": "우왕!! 교개원 일 멋져요!",
          "original_form": "우왕!! 교개원 일 멋져요!",
          "speaker_id": "2",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100009435.1.23",
          "form": "저번에는 그 학교에서 일하지 안ㄹ앗억나?",
          "original_form": "저번에는 그 학교에서 일하지 안ㄹ앗억나?",
          "speaker_id": "2",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100009435.1.24",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100009435.1.25",
          "form": "그 학종지!",
          "original_form": "그 학종지!",
          "speaker_id": "1",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100009435.1.26",
          "form": "아나 아침에 자고있다가 전화와서 개놀랬자나~",
          "original_form": "아나 아침에 자고있다가 전화와서 개놀랬자나~",
          "speaker_id": "1",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100009435.1.27",
          "form": "우오ㅓㅇ 추카래여",
          "original_form": "우오ㅓㅇ 추카래여",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100009435.1.28",
          "form": "필라테스는 언제 그만둬ㅡㅁ",
          "original_form": "필라테스는 언제 그만둬ㅡㅁ",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100009435.1.29",
          "form": "글올ㄹ8ㅆ다!!!",
          "original_form": "글올ㄹ8ㅆ다!!!",
          "speaker_id": "1",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100009435.1.30",
          "form": "enfp인사람들이 잘 맞을 직업..",
          "original_form": "enfp인사람들이 잘 맞을 직업..",
          "speaker_id": "1",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100009435.1.31",
          "form": "글올렸다!? 란.. 뜻일가나?",
          "original_form": "글올렸다!? 란.. 뜻일가나?",
          "speaker_id": "2",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100009435.1.32",
          "form": "아 그려? 그런거.. 좀 힘빠지지",
          "original_form": "아 그려? 그런거.. 좀 힘빠지지",
          "speaker_id": "2",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100009435.1.33",
          "form": "난 사람 맞이하는 일은 너무 힘들어",
          "original_form": "난 사람 맞이하는 일은 너무 힘들어",
