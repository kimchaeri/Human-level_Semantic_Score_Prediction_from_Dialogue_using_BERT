      "utterance": [
        {
          "id": "MDRW2100031253.1.1",
          "form": "나는 종교없는데",
          "original_form": "나는 종교없는데",
          "speaker_id": "2",
          "time": "20210904 19:12"
        },
        {
          "id": "MDRW2100031253.1.2",
          "form": "너는있니",
          "original_form": "너는있니",
          "speaker_id": "2",
          "time": "20210904 19:12"
        },
        {
          "id": "MDRW2100031253.1.3",
          "form": "종교는 없는데 가끔 절은 가보고싶더라",
          "original_form": "종교는 없는데 가끔 절은 가보고싶더라",
          "speaker_id": "1",
          "time": "20210904 19:13"
        },
        {
          "id": "MDRW2100031253.1.4",
          "form": "절? 절은 왜?",
          "original_form": "절? 절은 왜?",
          "speaker_id": "2",
          "time": "20210904 19:15"
        },
        {
          "id": "MDRW2100031253.1.5",
          "form": "절 조용해서 좋아 너무 수학여행스럽나?",
          "original_form": "절 조용해서 좋아 너무 수학여행스럽나?",
          "speaker_id": "1",
          "time": "20210904 19:24"
        },
        {
          "id": "MDRW2100031253.1.6",
          "form": "음.. 부모님이 수학여행 절로 갔을거같아",
          "original_form": "음.. 부모님이 수학여행 절로 갔을거같아",
          "speaker_id": "2",
          "time": "20210904 19:42"
        },
        {
          "id": "MDRW2100031253.1.7",
          "form": "나의 나이가 이렇게 밝혀진다고? 요즘 템플스테이도 한다구!",
          "original_form": "나의 나이가 이렇게 밝혀진다고? 요즘 템플스테이도 한다구!",
          "speaker_id": "1",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100031253.1.8",
          "form": "템플스테이 가고싶다!",
          "original_form": "템플스테이 가고싶다!",
          "speaker_id": "1",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100031253.1.9",
          "form": "그건 또 끌리네",
          "original_form": "그건 또 끌리네",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031253.1.10",
          "form": "나도 무교긴하지만 절은 좋아해",
          "original_form": "나도 무교긴하지만 절은 좋아해",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031253.1.11",
          "form": "템플스테이 가면 어디로 가는게 젤 이쁠까?",
          "original_form": "템플스테이 가면 어디로 가는게 젤 이쁠까?",
          "speaker_id": "1",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031253.1.12",
          "form": "화엄사?",
          "original_form": "화엄사?",
          "speaker_id": "1",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100031253.1.13",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100031253.1.14",
          "form": "절은 잘 모르지만",
          "original_form": "절은 잘 모르지만",
          "speaker_id": "2",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100031253.1.15",
          "form": "..산에있고 크고 근처면 좋지않을까?",
          "original_form": "..산에있고 크고 근처면 좋지않을까?",
          "speaker_id": "2",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100031253.1.16",
          "form": "그럼 전남쪽 송광사도 좋대!",
          "original_form": "그럼 전남쪽 송광사도 좋대!",
          "speaker_id": "1",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031253.1.17",
          "form": "나중에 템플스테이 같이 가자~",
          "original_form": "나중에 템플스테이 같이 가자~",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031253.1.18",
          "form": "좋아 가을이 좋아 봄이 좋아?",
          "original_form": "좋아 가을이 좋아 봄이 좋아?",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031253.1.19",
          "form": "가을이 좋을거같아 봄은 춥고 벌레도 많아..",
          "original_form": "가을이 좋을거같아 봄은 춥고 벌레도 많아..",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031253.1.20",
          "form": "가을 좋아!",
          "original_form": "가을 좋아!",
          "speaker_id": "1",
          "time": "20210904 20:40"
        }
      ]
    }
