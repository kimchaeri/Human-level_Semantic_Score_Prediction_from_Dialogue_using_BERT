      "utterance": [
        {
          "id": "MDRW2100036507.1.1",
          "form": "너는 제습기써본적잇니?",
          "original_form": "너는 제습기써본적잇니?",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036507.1.2",
          "form": "너 그거알아?",
          "original_form": "너 그거알아?",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036507.1.3",
          "form": "아.. 난 없어. 에어컨에서 그 기능을 써.",
          "original_form": "아.. 난 없어. 에어컨에서 그 기능을 써.",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036507.1.4",
          "form": "제습기는 비싸",
          "original_form": "제습기는 비싸",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036507.1.5",
          "form": "아하 난 에어컨으로 부족해서 비싼걸 갖고싶더라",
          "original_form": "아하 난 에어컨으로 부족해서 비싼걸 갖고싶더라",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036507.1.6",
          "form": "제습기를 써면 방 온도가 놓아져",
          "original_form": "제습기를 써면 방 온도가 놓아져",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036507.1.7",
          "form": "높아져",
          "original_form": "높아져",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036507.1.8",
          "form": "그러니까 ㅠ 좀 싸게 내주면 좋겠어",
          "original_form": "그러니까 ㅠ 좀 싸게 내주면 좋겠어",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036507.1.9",
          "form": "어머 그건 몰랐던 사실이네",
          "original_form": "어머 그건 몰랐던 사실이네",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036507.1.10",
          "form": "너는 나한테 뭘 물어보려고했어?",
          "original_form": "너는 나한테 뭘 물어보려고했어?",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036507.1.11",
          "form": "사막같이 돼",
          "original_form": "사막같이 돼",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036507.1.12",
          "form": "아냐. 제습기 흥미로워.",
          "original_form": "아냐. 제습기 흥미로워.",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036507.1.13",
          "form": "가격대는 알아본거야?",
          "original_form": "가격대는 알아본거야?",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036507.1.14",
          "form": "응 한 이십만원하더라...",
          "original_form": "응 한 이십만원하더라...",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036507.1.15",
          "form": "그건 누구한테 사달라고 할 수도 없잖아?",
          "original_form": "그건 누구한테 사달라고 할 수도 없잖아?",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036507.1.16",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036507.1.17",
          "form": "진정한 금값이네",
          "original_form": "진정한 금값이네",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036507.1.18",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036507.1.19",
          "form": "아빠한테 조르는 던 어때?",
          "original_form": "아빠한테 조르는 던 어때?",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036507.1.20",
          "form": "친구 열면 모아서 사달라고 할까?",
          "original_form": "친구 열면 모아서 사달라고 할까?",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100036507.1.21",
          "form": "아니면 동생에게 졸라봐! 추석맞이 용돈을 받자",
          "original_form": "아니면 동생에게 졸라봐! 추석맞이 용돈을 받자",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100036507.1.22",
          "form": "이미 용던을 두배로 받아서",
          "original_form": "이미 용던을 두배로 받아서",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100036507.1.23",
          "form": "그럴 염치가 없어",
          "original_form": "그럴 염치가 없어",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100036507.1.24",
          "form": "열명모아서 좋지",
          "original_form": "열명모아서 좋지",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100036507.1.25",
          "form": "경제적이구나",
          "original_form": "경제적이구나",
          "speaker_id": "1",
          "time": "20210830 23:58"
        }
      ]
    }
  ]
