      "utterance": [
        {
          "id": "MDRW2100007705.1.1",
          "form": "요새 통 못갔네요",
          "original_form": "요새 통 못갔네요",
          "speaker_id": "2",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007705.1.2",
          "form": "코로나로",
          "original_form": "코로나로",
          "speaker_id": "2",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007705.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007705.1.4",
          "form": "오늘은 비그쳐서 계곡에 급 고기 구우러 갈까",
          "original_form": "오늘은 비그쳐서 계곡에 급 고기 구우러 갈까",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007705.1.5",
          "form": "고민되네요",
          "original_form": "고민되네요",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007705.1.6",
          "form": "발시려울듯 해요",
          "original_form": "발시려울듯 해요",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007705.1.7",
          "form": "계곡물이 아주 차겠죠 ㅋㅋ",
          "original_form": "계곡물이 아주 차겠죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007705.1.8",
          "form": "생각맘 해도 좋네여",
          "original_form": "생각맘 해도 좋네여",
          "speaker_id": "1",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007705.1.9",
          "form": "네 여기 계곡 많아서 좋아요",
          "original_form": "네 여기 계곡 많아서 좋아요",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007705.1.10",
          "form": "부러운 동내네요",
          "original_form": "부러운 동내네요",
          "speaker_id": "1",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007705.1.11",
          "form": "시골이 좋긴해요",
          "original_form": "시골이 좋긴해요",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007705.1.12",
          "form": "이럴때 사람없어서 코로나 비켜가요",
          "original_form": "이럴때 사람없어서 코로나 비켜가요",
          "speaker_id": "2",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007705.1.13",
          "form": "벌레 안 많아요?",
          "original_form": "벌레 안 많아요?",
          "speaker_id": "1",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007705.1.14",
          "form": "많죠",
          "original_form": "많죠",
          "speaker_id": "2",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007705.1.15",
          "form": "근데 아파트라 괜찮아요",
          "original_form": "근데 아파트라 괜찮아요",
          "speaker_id": "2",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007705.1.16",
          "form": "계곡은 벌레가 딸만 공격해서 놀러갈때 만반의 준비해요",
          "original_form": "계곡은 벌레가 딸만 공격해서 놀러갈때 만반의 준비해요",
          "speaker_id": "2",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007705.1.17",
          "form": "뿌리거 가야해요",
          "original_form": "뿌리거 가야해요",
          "speaker_id": "1",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007705.1.18",
          "form": "가도 3시간",
          "original_form": "가도 3시간",
          "speaker_id": "2",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007705.1.19",
          "form": "산은 그게 좀 불편해요",
          "original_form": "산은 그게 좀 불편해요",
          "speaker_id": "2",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007705.1.20",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210822 10:00"
        },
        {
          "id": "MDRW2100007705.1.21",
          "form": "바다는 모래가 넘 찝찝하고 놀때 좋은데 돌아 올때 털기 힘들고",
          "original_form": "바다는 모래가 넘 찝찝하고 놀때 좋은데 돌아 올때 털기 힘들고",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007705.1.22",
          "form": "그래도 놀땐 좋아요 ㅎㅎ",
          "original_form": "그래도 놀땐 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007705.1.23",
          "form": "즐거우면 그만이죠",
          "original_form": "즐거우면 그만이죠",
          "speaker_id": "1",
          "time": "20210822 10:02"
        }
      ]
