      "utterance": [
        {
          "id": "MDRW2100040414.1.1",
          "form": "ㅎㅎ 지역이 어디세요?",
          "original_form": "ㅎㅎ 지역이 어디세요?",
          "speaker_id": "2",
          "time": "20210904 18:50"
        },
        {
          "id": "MDRW2100040414.1.2",
          "form": "저는 가을 좋아요",
          "original_form": "저는 가을 좋아요",
          "speaker_id": "2",
          "time": "20210904 18:50"
        },
        {
          "id": "MDRW2100040414.1.3",
          "form": "여기는 강원도요",
          "original_form": "여기는 강원도요",
          "speaker_id": "1",
          "time": "20210904 18:51"
        },
        {
          "id": "MDRW2100040414.1.4",
          "form": "엇! 저도 가을이 제일 좋아요",
          "original_form": "엇! 저도 가을이 제일 좋아요",
          "speaker_id": "1",
          "time": "20210904 18:51"
        },
        {
          "id": "MDRW2100040414.1.5",
          "form": "오 강원도 사시는구나 저는 서울입니당",
          "original_form": "오 강원도 사시는구나 저는 서울입니당",
          "speaker_id": "2",
          "time": "20210904 18:52"
        },
        {
          "id": "MDRW2100040414.1.6",
          "form": "가을은 너무 인기계절이죠 ㅋㅋㅋㅋ",
          "original_form": "가을은 너무 인기계절이죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:53"
        },
        {
          "id": "MDRW2100040414.1.7",
          "form": "여름은 너무 덥고 겨울은 너무 추워요",
          "original_form": "여름은 너무 덥고 겨울은 너무 추워요",
          "speaker_id": "2",
          "time": "20210904 18:53"
        },
        {
          "id": "MDRW2100040414.1.8",
          "form": "봄도 추워요..",
          "original_form": "봄도 추워요..",
          "speaker_id": "2",
          "time": "20210904 18:53"
        },
        {
          "id": "MDRW2100040414.1.9",
          "form": "맞아요! 덥지도 않고 춥지도 않은 때라서 좋아해요",
          "original_form": "맞아요! 덥지도 않고 춥지도 않은 때라서 좋아해요",
          "speaker_id": "1",
          "time": "20210904 18:54"
        },
        {
          "id": "MDRW2100040414.1.10",
          "form": "그리고 선선한 바람도 좋아요",
          "original_form": "그리고 선선한 바람도 좋아요",
          "speaker_id": "1",
          "time": "20210904 18:55"
        },
        {
          "id": "MDRW2100040414.1.11",
          "form": "맞아요 약간 오바지만 폐까지 시원한 느낌 ㅋㅋㅋ",
          "original_form": "맞아요 약간 오바지만 폐까지 시원한 느낌 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:57"
        },
        {
          "id": "MDRW2100040414.1.12",
          "form": "맞아요ㅋㅋㅋㅋㅋ 시원하지만 또 그렇다고 춥지 않은 바람이라 좋아요",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ 시원하지만 또 그렇다고 춥지 않은 바람이라 좋아요",
          "speaker_id": "1",
          "time": "20210904 19:00"
        },
        {
          "id": "MDRW2100040414.1.13",
          "form": "네 ㅋㅋㅋ 기분도 좋구요 무엇보다 하늘이 깨끗하고 높아보이는게 좋아요",
          "original_form": "네 ㅋㅋㅋ 기분도 좋구요 무엇보다 하늘이 깨끗하고 높아보이는게 좋아요",
          "speaker_id": "2",
          "time": "20210904 19:07"
        },
        {
          "id": "MDRW2100040414.1.14",
          "form": "그리고 가을되면 코스모스 피는것두요!",
          "original_form": "그리고 가을되면 코스모스 피는것두요!",
          "speaker_id": "2",
          "time": "20210904 19:07"
        },
        {
          "id": "MDRW2100040414.1.15",
          "form": "맞아요! 하늘도 정말 예쁘죠\r\n코스모스는 잠시 잊고있었는데 이야기들으니 떠오르네요!",
          "original_form": "맞아요! 하늘도 정말 예쁘죠\r\n코스모스는 잠시 잊고있었는데 이야기들으니 떠오르네요!",
          "speaker_id": "1",
          "time": "20210904 19:09"
        },
        {
          "id": "MDRW2100040414.1.16",
          "form": "코스모스 진짜 예쁘지 않나요?",
          "original_form": "코스모스 진짜 예쁘지 않나요?",
          "speaker_id": "2",
          "time": "20210904 19:11"
        },
        {
          "id": "MDRW2100040414.1.17",
          "form": "색깔도 여러가지고",
          "original_form": "색깔도 여러가지고",
          "speaker_id": "2",
          "time": "20210904 19:11"
        },
        {
          "id": "MDRW2100040414.1.18",
          "form": "가시도 없고",
          "original_form": "가시도 없고",
          "speaker_id": "2",
          "time": "20210904 19:11"
        },
        {
          "id": "MDRW2100040414.1.19",
          "form": "관상용으로 딱이에요",
          "original_form": "관상용으로 딱이에요",
          "speaker_id": "2",
          "time": "20210904 19:11"
        },
        {
          "id": "MDRW2100040414.1.20",
          "form": "그쵸!!  맞아요\r\n코스모스 활짝 펼쳐진 곳 가면 너무 예뻐요",
          "original_form": "그쵸!!  맞아요\r\n코스모스 활짝 펼쳐진 곳 가면 너무 예뻐요",
          "speaker_id": "1",
          "time": "20210904 19:14"
        },
        {
          "id": "MDRW2100040414.1.21",
          "form": "저는 그런데는 못가봤는데 어디 아시는데 있나요?",
          "original_form": "저는 그런데는 못가봤는데 어디 아시는데 있나요?",
          "speaker_id": "2",
          "time": "20210904 19:14"
        },
        {
          "id": "MDRW2100040414.1.22",
          "form": "가보고싶어요",
          "original_form": "가보고싶어요",
          "speaker_id": "2",
          "time": "20210904 19:14"
        },
        {
          "id": "MDRW2100040414.1.23",
          "form": "예전에 강릉 경포에 코스모스 쫙 피어있던 곳 보러 갔었어요",
          "original_form": "예전에 강릉 경포에 코스모스 쫙 피어있던 곳 보러 갔었어요",
          "speaker_id": "1",
          "time": "20210904 19:18"
        },
        {
          "id": "MDRW2100040414.1.24",
          "form": "경포 생태저류지 그곳에 코스모스꽃밭이 만들어져 있었어요",
          "original_form": "경포 생태저류지 그곳에 코스모스꽃밭이 만들어져 있었어요",
          "speaker_id": "1",
          "time": "20210904 19:19"
        },
        {
          "id": "MDRW2100040414.1.25",
          "form": "저 지금 캡쳐했어요ㅋㅋㅋㅋ 꼭 가봐야지 감사합니다",
          "original_form": "저 지금 캡쳐했어요ㅋㅋㅋㅋ 꼭 가봐야지 감사합니다",
          "speaker_id": "2",
          "time": "20210904 19:19"
        },
        {
          "id": "MDRW2100040414.1.26",
          "form": "엇! 지금도 계속 하는지 모르겠어서 찾아봤는데",
          "original_form": "엇! 지금도 계속 하는지 모르겠어서 찾아봤는데",
          "speaker_id": "1",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040414.1.27",
          "form": "19년도부터는 안 했나봐요ㅜㅜ",
          "original_form": "19년도부터는 안 했나봐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040414.1.28",
          "form": "아이고 수고스럽게 ㅠ",
          "original_form": "아이고 수고스럽게 ㅠ",
          "speaker_id": "2",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040414.1.29",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040414.1.30",
          "form": "사진으로 즐겨야겠네요 ㅎㅎㅎㅎ",
          "original_form": "사진으로 즐겨야겠네요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040414.1.31",
          "form": "헛걸음 하시면 아쉽잖아요",
          "original_form": "헛걸음 하시면 아쉽잖아요",
          "speaker_id": "1",
          "time": "20210904 19:23"
        }
      ]
