      "utterance": [
        {
          "id": "MDRW2100016343.1.1",
          "form": "즐겨보는 방송있나요",
          "original_form": "즐겨보는 방송있나요",
          "speaker_id": "1",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100016343.1.2",
          "form": "티비는 없고 넷플릭스랑 왓챠 번갈아 보고 있어요.",
          "original_form": "티비는 없고 넷플릭스랑 왓챠 번갈아 보고 있어요.",
          "speaker_id": "2",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016343.1.3",
          "form": "저는 넷플릭스봐요",
          "original_form": "저는 넷플릭스봐요",
          "speaker_id": "1",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016343.1.4",
          "form": "어떤 프로 보시나요",
          "original_form": "어떤 프로 보시나요",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016343.1.5",
          "form": "요즘은 빌리언스에 꽂혀서 세 번 째 보고 있어요.",
          "original_form": "요즘은 빌리언스에 꽂혀서 세 번 째 보고 있어요.",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016343.1.6",
          "form": "어떤 거 보세요?",
          "original_form": "어떤 거 보세요?",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016343.1.7",
          "form": "잼있나요??",
          "original_form": "잼있나요??",
          "speaker_id": "1",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016343.1.8",
          "form": "저는 돌싱글즈하고..상견니요",
          "original_form": "저는 돌싱글즈하고..상견니요",
          "speaker_id": "1",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016343.1.9",
          "form": "네, 엄청 재밌어요 !",
          "original_form": "네, 엄청 재밌어요 !",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016343.1.10",
          "form": "오 .. 그것도 인기가 높더라고요. 다른 거 보느라 아직 보진 않았지만",
          "original_form": "오 .. 그것도 인기가 높더라고요. 다른 거 보느라 아직 보진 않았지만",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016343.1.11",
          "form": "저도 시간날때 빌리언즈 봐야겠어요",
          "original_form": "저도 시간날때 빌리언즈 봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016343.1.12",
          "form": "상견니 남주 너무 잘생겼어요\r\n내용도 너무좋구요",
          "original_form": "상견니 남주 너무 잘생겼어요\r\n내용도 너무좋구요",
          "speaker_id": "1",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016343.1.13",
          "form": "오 대만 드라마네요",
          "original_form": "오 대만 드라마네요",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016343.1.14",
          "form": "분위기도 좋아보여요",
          "original_form": "분위기도 좋아보여요",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016343.1.15",
          "form": "네 대만드라마요 강추입니다 ㅎ",
          "original_form": "네 대만드라마요 강추입니다 ㅎ",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016343.1.16",
          "form": "한동안 빠져봤네요",
          "original_form": "한동안 빠져봤네요",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016343.1.17",
          "form": "리스트에 넣었습니다 ㅎㅎ",
          "original_form": "리스트에 넣었습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016343.1.18",
          "form": "감사해요",
          "original_form": "감사해요",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016343.1.19",
          "form": "네 ㅎㅎ 저도 넣었습니다! 찜!!",
          "original_form": "네 ㅎㅎ 저도 넣었습니다! 찜!!",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016343.1.20",
          "form": "티비방송은 잘안보세요?",
          "original_form": "티비방송은 잘안보세요?",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016343.1.21",
          "form": "어쩌다 보는 편이예요 ㅎㅎ 아, 비밀의 숲 재밌게 봤어요.",
          "original_form": "어쩌다 보는 편이예요 ㅎㅎ 아, 비밀의 숲 재밌게 봤어요.",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016343.1.22",
          "form": "로맨스를 보는 경우는 한국 드라마로는 잘 없는 편이예요.",
          "original_form": "로맨스를 보는 경우는 한국 드라마로는 잘 없는 편이예요.",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016343.1.23",
          "form": "비밀의 숲 재미있다고 들었어요",
          "original_form": "비밀의 숲 재미있다고 들었어요",
          "speaker_id": "1",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016343.1.24",
          "form": "시즌이 이어지고 있어서 계속 기대 중이예요 ㅎㅎ",
          "original_form": "시즌이 이어지고 있어서 계속 기대 중이예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016343.1.25",
          "form": "시즌이 이어지고 있어서 기대 중이예요,",
          "original_form": "시즌이 이어지고 있어서 기대 중이예요,",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016343.1.26",
          "form": "실화 기반이기도 하고요 ㅎㅎ",
          "original_form": "실화 기반이기도 하고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:35"
        }
      ]
