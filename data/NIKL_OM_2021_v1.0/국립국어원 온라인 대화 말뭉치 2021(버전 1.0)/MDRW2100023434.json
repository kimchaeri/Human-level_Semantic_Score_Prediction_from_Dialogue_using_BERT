      "utterance": [
        {
          "id": "MDRW2100023434.1.1",
          "form": "똑똑똑~",
          "original_form": "똑똑똑~",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023434.1.2",
          "form": "오오ㅎ일찍오셨네요 ㅡㅎㅎ",
          "original_form": "오오ㅎ일찍오셨네요 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100023434.1.3",
          "form": "전이제 육퇴하고 왔어요ㅎ",
          "original_form": "전이제 육퇴하고 왔어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100023434.1.4",
          "form": "안피곤하시나요?ㅎㅎ",
          "original_form": "안피곤하시나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100023434.1.5",
          "form": "ㅎㅎ 안피곤해요 ㅎㅎ",
          "original_form": "ㅎㅎ 안피곤해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100023434.1.6",
          "form": "오늘은 교구도 미리미리 다만들구 ㅋ",
          "original_form": "오늘은 교구도 미리미리 다만들구 ㅋ",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100023434.1.7",
          "form": "ㅎㅎ 우와 전 쓰러질뻔ㅎ",
          "original_form": "ㅎㅎ 우와 전 쓰러질뻔ㅎ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100023434.1.8",
          "form": "교구도 만드시고 대단하십니당ㅎ",
          "original_form": "교구도 만드시고 대단하십니당ㅎ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100023434.1.9",
          "form": "어 피곤 하면 주무셔요 ㅜㅜ",
          "original_form": "어 피곤 하면 주무셔요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100023434.1.10",
          "form": "아녀요ㅎ",
          "original_form": "아녀요ㅎ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100023434.1.11",
          "form": "달달한거먹고왔음요ㅎ",
          "original_form": "달달한거먹고왔음요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100023434.1.12",
          "form": "와 부럽네요 ㅋ",
          "original_form": "와 부럽네요 ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100023434.1.13",
          "form": "교구는 오늘은 뭐만들었어요?",
          "original_form": "교구는 오늘은 뭐만들었어요?",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100023434.1.14",
          "form": "책중에 아이스크림이 꽁꽁 이란 책이 있어요 ㅎㅎ",
          "original_form": "책중에 아이스크림이 꽁꽁 이란 책이 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100023434.1.15",
          "form": "아아 ㅡㅎㅎ",
          "original_form": "아아 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100023434.1.16",
          "form": "진짜 근데 부럽네요ㅠ 저희는 다사가지고ㅠ",
          "original_form": "진짜 근데 부럽네요ㅠ 저희는 다사가지고ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100023434.1.17",
          "form": "아 근데 별거 아니긴한데 아이가 좋아하져",
          "original_form": "아 근데 별거 아니긴한데 아이가 좋아하져",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100023434.1.18",
          "form": "책에서 고양이들이 꽁꽁 어는 장면 나오거든요",
          "original_form": "책에서 고양이들이 꽁꽁 어는 장면 나오거든요",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100023434.1.19",
          "form": "ㅎㅎ 저희는 사주니깐ㅎ 자꾸 더 사달라해요ㅎ 요즘진짜 많이 사달라고ㅎ",
          "original_form": "ㅎㅎ 저희는 사주니깐ㅎ 자꾸 더 사달라해요ㅎ 요즘진짜 많이 사달라고ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100023434.1.20",
          "form": "ㅎㅎㅎ 같이 손으로 만드는거 도전해보세오 ㅎㅎ",
          "original_form": "ㅎㅎㅎ 같이 손으로 만드는거 도전해보세오 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100023434.1.21",
          "form": "그리고 이야기 하고",
          "original_form": "그리고 이야기 하고",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100023434.1.22",
          "form": "노래부루고  춤추고 다  놀이이면서 공부 ㅎㅎ",
          "original_form": "노래부루고  춤추고 다  놀이이면서 공부 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100023434.1.23",
          "form": "클레이랑 뭐 비슷한거 하는데ㅎ 다 섞어버려요ㅎㄹ",
          "original_form": "클레이랑 뭐 비슷한거 하는데ㅎ 다 섞어버려요ㅎㄹ",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100023434.1.24",
          "form": "아 아직은 그럴나이에요",
          "original_form": "아 아직은 그럴나이에요",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100023434.1.25",
          "form": "그건 계속놀아주긴해요ㅎ",
          "original_form": "그건 계속놀아주긴해요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100023434.1.26",
          "form": "근데 요즘 많이 삐치고 ㅡㅎ",
          "original_form": "근데 요즘 많이 삐치고 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100023434.1.27",
          "form": "ㅋ 제일 만ㄹ이 삐칠 시기죠",
          "original_form": "ㅋ 제일 만ㄹ이 삐칠 시기죠",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100023434.1.28",
          "form": "엄마한테는 말못하는데ㅎ 저한테는 엄청 화도많이내고ㅎㅎ",
          "original_form": "엄마한테는 말못하는데ㅎ 저한테는 엄청 화도많이내고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:05"
        }
      ]
    }
