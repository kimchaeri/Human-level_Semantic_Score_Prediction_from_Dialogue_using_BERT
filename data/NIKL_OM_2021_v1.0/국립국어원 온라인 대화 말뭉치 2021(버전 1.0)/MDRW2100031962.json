      "utterance": [
        {
          "id": "MDRW2100031962.1.1",
          "form": "우리 시계 사자",
          "original_form": "우리 시계 사자",
          "speaker_id": "1",
          "time": "20210905 09:57"
        },
        {
          "id": "MDRW2100031962.1.2",
          "form": "뭔 시계? 손목 벽 아님 탁상 종류 많아",
          "original_form": "뭔 시계? 손목 벽 아님 탁상 종류 많아",
          "speaker_id": "2",
          "time": "20210905 09:59"
        },
        {
          "id": "MDRW2100031962.1.3",
          "form": "벽 시계 사야할 것 같은데.",
          "original_form": "벽 시계 사야할 것 같은데.",
          "speaker_id": "1",
          "time": "20210905 10:02"
        },
        {
          "id": "MDRW2100031962.1.4",
          "form": "어디에 놓을 건데",
          "original_form": "어디에 놓을 건데",
          "speaker_id": "2",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100031962.1.5",
          "form": "화장실에 놓으려고",
          "original_form": "화장실에 놓으려고",
          "speaker_id": "1",
          "time": "20210905 10:04"
        },
        {
          "id": "MDRW2100031962.1.6",
          "form": "아 화장실용 시계 난 탁상 시계도 사고파",
          "original_form": "아 화장실용 시계 난 탁상 시계도 사고파",
          "speaker_id": "2",
          "time": "20210905 10:05"
        },
        {
          "id": "MDRW2100031962.1.7",
          "form": "탁상 시계는 어디 필요한데?",
          "original_form": "탁상 시계는 어디 필요한데?",
          "speaker_id": "1",
          "time": "20210905 10:06"
        },
        {
          "id": "MDRW2100031962.1.8",
          "form": "내 방레 놓으려고 들고 다니기 귀찮아서",
          "original_form": "내 방레 놓으려고 들고 다니기 귀찮아서",
          "speaker_id": "2",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100031962.1.9",
          "form": "나 오타가 잘 나네 ㅠ",
          "original_form": "나 오타가 잘 나네 ㅠ",
          "speaker_id": "2",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100031962.1.10",
          "form": "좋은 생각이야.",
          "original_form": "좋은 생각이야.",
          "speaker_id": "1",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100031962.1.11",
          "form": "칭찬 고마워 부엉이 시계로 사고 싶네",
          "original_form": "칭찬 고마워 부엉이 시계로 사고 싶네",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031962.1.12",
          "form": "귀엽겠다 ㅎㅎ 난 컵도 사고 싶어. 샐리컵 좋은데 색깔이 내 취향이 아니라",
          "original_form": "귀엽겠다 ㅎㅎ 난 컵도 사고 싶어. 샐리컵 좋은데 색깔이 내 취향이 아니라",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031962.1.13",
          "form": "오 유리 도자기 아님 뭐로 살 거야?",
          "original_form": "오 유리 도자기 아님 뭐로 살 거야?",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031962.1.14",
          "form": "유리는 써본적 없어서 잘 모르겠고 도자기로 사야나?",
          "original_form": "유리는 써본적 없어서 잘 모르겠고 도자기로 사야나?",
          "speaker_id": "1",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031962.1.15",
          "form": "유리 추천한다 내열 유리 컵 좋아",
          "original_form": "유리 추천한다 내열 유리 컵 좋아",
          "speaker_id": "2",
          "time": "20210905 10:32"
        }
      ]
