        },
        {
          "id": "MDRW2100013169.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ방학때도 군말없이 가고~",
          "original_form": "ㅋㅋㅋㅋㅋㅋ방학때도 군말없이 가고~",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013169.1.6",
          "form": "진짜 애들 착해요^^",
          "original_form": "진짜 애들 착해요^^",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013169.1.7",
          "form": "피아노 수업",
          "original_form": "피아노 수업",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013169.1.8",
          "form": "첫찌 원래가던시간에 애들많아서 조정한다던데~",
          "original_form": "첫찌 원래가던시간에 애들많아서 조정한다던데~",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013169.1.9",
          "form": "나는5시30분이야ㅡㅡ",
          "original_form": "나는5시30분이야ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013169.1.10",
          "form": "그시간도 사람많드라",
          "original_form": "그시간도 사람많드라",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013169.1.11",
          "form": "장사잘된데이",
          "original_form": "장사잘된데이",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013169.1.12",
          "form": "느무 많이.받아요ㅠㅠ",
          "original_form": "느무 많이.받아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013169.1.13",
          "form": "혹시나 하교하고 바로 안된다 카면",
          "original_form": "혹시나 하교하고 바로 안된다 카면",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013169.1.14",
          "form": "옮길라고요 ㅋㅋㅋㅋㅋ",
          "original_form": "옮길라고요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013169.1.15",
          "form": "그래 다른시간해라ㅋㅋ",
          "original_form": "그래 다른시간해라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013169.1.16",
          "form": "난 다 오후다ㅡㅡ",
          "original_form": "난 다 오후다ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013169.1.17",
          "form": "공부방도 오후",
          "original_form": "공부방도 오후",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013169.1.18",
          "form": "아녀 ㅋㅋㅋㅋㅋ학원을 ㅋㅋㅋ",
          "original_form": "아녀 ㅋㅋㅋㅋㅋ학원을 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013169.1.19",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013169.1.20",
          "form": "난또 미련없지 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "난또 미련없지 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013169.1.21",
          "form": "어디로갈라고?",
          "original_form": "어디로갈라고?",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013169.1.22",
          "form": "name3요??ㅋㅋㅋ",
          "original_form": "&name3&요??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013169.1.23",
          "form": "거긴 왜 주마다 피아노치는거 영상도 보내주고",
          "original_form": "거긴 왜 주마다 피아노치는거 영상도 보내주고",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013169.1.24",
          "form": "여튼 잘해주는듯요 ㅋㅋㅋㅋ",
          "original_form": "여튼 잘해주는듯요 ㅋㅋㅋㅋ",
