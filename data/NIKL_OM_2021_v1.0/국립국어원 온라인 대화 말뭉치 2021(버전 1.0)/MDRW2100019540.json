      "utterance": [
        {
          "id": "MDRW2100019540.1.1",
          "form": "메타버스에 대해 잘 아시나요?",
          "original_form": "메타버스에 대해 잘 아시나요?",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019540.1.2",
          "form": "잘 모르는데 회사에서 제페토 앱 통해서 뭘 많이해요",
          "original_form": "잘 모르는데 회사에서 제페토 앱 통해서 뭘 많이해요",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019540.1.3",
          "form": "아 그러시구나 전 제페토 앱 안해봣어요",
          "original_form": "아 그러시구나 전 제페토 앱 안해봣어요",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019540.1.4",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019540.1.5",
          "form": "아바타꾸미고 월드맵 돌아다니는게 다에요ㅠㅠ",
          "original_form": "아바타꾸미고 월드맵 돌아다니는게 다에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019540.1.6",
          "form": "월드맵 돌아다니면서 다른 유저들과 대화하고 그러는거예요?",
          "original_form": "월드맵 돌아다니면서 다른 유저들과 대화하고 그러는거예요?",
          "speaker_id": "2",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019540.1.7",
          "form": "네네그런거같아요앉을수도있고",
          "original_form": "네네그런거같아요앉을수도있고",
          "speaker_id": "1",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019540.1.8",
          "form": "사진전송되면 제 아바타 보여드리고싶네요 ㅎㄹ",
          "original_form": "사진전송되면 제 아바타 보여드리고싶네요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019540.1.9",
          "form": "앉을수도 잇어요? ㅋㅋㅋㅋ뭔가 귀엽네요",
          "original_form": "앉을수도 잇어요? ㅋㅋㅋㅋ뭔가 귀엽네요",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019540.1.10",
          "form": "네네 카페에 앉을수도있어요",
          "original_form": "네네 카페에 앉을수도있어요",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019540.1.11",
          "form": "오 신기해요",
          "original_form": "오 신기해요",
          "speaker_id": "2",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019540.1.12",
          "form": "제페토 앱은 유료예요?",
          "original_form": "제페토 앱은 유료예요?",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100019540.1.13",
          "form": "무료에요",
          "original_form": "무료에요",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019540.1.14",
          "form": "코인도 쌓이고",
          "original_form": "코인도 쌓이고",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019540.1.15",
          "form": "보석은 돈주고 사야해요ㅠ",
          "original_form": "보석은 돈주고 사야해요ㅠ",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019540.1.16",
          "form": "보석으로 아바타를 꾸미는 거예요?",
          "original_form": "보석으로 아바타를 꾸미는 거예요?",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019540.1.17",
          "form": "보석으론 더 좋은 제품 살 수 있어요",
          "original_form": "보석으론 더 좋은 제품 살 수 있어요",
          "speaker_id": "1",
          "time": "20210827 21:29"
        },
