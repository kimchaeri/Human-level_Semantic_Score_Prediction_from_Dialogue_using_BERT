      "utterance": [
        {
          "id": "MDRW2100020673.1.1",
          "form": "송중기는 어떤 작품부터 좋아하셨어요?",
          "original_form": "송중기는 어떤 작품부터 좋아하셨어요?",
          "speaker_id": "2",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020673.1.2",
          "form": "뿌리깊은나무요!",
          "original_form": "뿌리깊은나무요!",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020673.1.3",
          "form": "확 눈에 들어왔어요",
          "original_form": "확 눈에 들어왔어요",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020673.1.4",
          "form": "아 뿌나 진짜 재밌었어요",
          "original_form": "아 뿌나 진짜 재밌었어요",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020673.1.5",
          "form": "오! 보셨군욥",
          "original_form": "오! 보셨군욥",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020673.1.6",
          "form": "드라마 좋아하시나봐요!",
          "original_form": "드라마 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020673.1.7",
          "form": "종종 시간 나면 한꺼번에 몰아봐요",
          "original_form": "종종 시간 나면 한꺼번에 몰아봐요",
          "speaker_id": "2",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020673.1.8",
          "form": "오~  최근에 보신건 어떤거에요?",
          "original_form": "오~  최근에 보신건 어떤거에요?",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020673.1.9",
          "form": "최근엔 드라마 자체를 잘 못보고 있어요 ㅠ",
          "original_form": "최근엔 드라마 자체를 잘 못보고 있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020673.1.10",
          "form": "보건교사 안은영이 마지막이었던가 ㅋㅋ",
          "original_form": "보건교사 안은영이 마지막이었던가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020673.1.11",
          "form": "앗ㅠ 바쁘신가봐요",
          "original_form": "앗ㅠ 바쁘신가봐요",
          "speaker_id": "1",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020673.1.12",
          "form": "그거 재밌나요?",
          "original_form": "그거 재밌나요?",
          "speaker_id": "1",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020673.1.13",
          "form": "책은 재밌었는데!",
          "original_form": "책은 재밌었는데!",
          "speaker_id": "1",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020673.1.14",
          "form": "뭔가 특이하고 오묘하더라구요 ㅋㅋㅋㅋ첨엔 좀 그랬는데 보다보니 재밌었어요",
          "original_form": "뭔가 특이하고 오묘하더라구요 ㅋㅋㅋㅋ첨엔 좀 그랬는데 보다보니 재밌었어요",
          "speaker_id": "2",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020673.1.15",
          "form": "오홍ㅋㅋ 배우도 좋았던걸로 기억하는데 연기 괜찮았나요?",
          "original_form": "오홍ㅋㅋ 배우도 좋았던걸로 기억하는데 연기 괜찮았나요?",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020673.1.16",
          "form": "넹 정유미 정말 좋아하는 배우에요ㅎㅎ",
          "original_form": "넹 정유미 정말 좋아하는 배우에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020673.1.17",
          "form": "저두요ㅎㅎ 되게 사랑스러워요",
          "original_form": "저두요ㅎㅎ 되게 사랑스러워요",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020673.1.18",
          "form": "욕을 쓰는 정유미는 좀 적응 안되긴 했는데ㅋㅋㅋㅋ 암튼 사랑스럽더라구요",
          "original_form": "욕을 쓰는 정유미는 좀 적응 안되긴 했는데ㅋㅋㅋㅋ 암튼 사랑스럽더라구요",
          "speaker_id": "2",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020673.1.19",
          "form": "욕하는게 나오는군요ㅋㅋ",
          "original_form": "욕하는게 나오는군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020673.1.20",
          "form": "새롭네요",
          "original_form": "새롭네요",
          "speaker_id": "1",
          "time": "20210828 13:23"
        }
      ]
