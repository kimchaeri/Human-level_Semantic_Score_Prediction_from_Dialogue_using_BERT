      "utterance": [
        {
          "id": "MDRW2100007334.1.1",
          "form": "이제 여름 휴가가기엔 넘 선선해졌어요 ㅠ",
          "original_form": "이제 여름 휴가가기엔 넘 선선해졌어요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007334.1.2",
          "form": "물에 못가요",
          "original_form": "물에 못가요",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007334.1.3",
          "form": "아쉬워요 ㅠㅠ",
          "original_form": "아쉬워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007334.1.4",
          "form": "그래도 산책하기엔 딱 좋은 날씨 같아요",
          "original_form": "그래도 산책하기엔 딱 좋은 날씨 같아요",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007334.1.5",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007334.1.6",
          "form": "운동도요 ㅎㅎ",
          "original_form": "운동도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007334.1.7",
          "form": "네 그치만 운동하기 조금 귀찮은..",
          "original_form": "네 그치만 운동하기 조금 귀찮은..",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007334.1.8",
          "form": "많이 귀찮은..",
          "original_form": "많이 귀찮은..",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007334.1.9",
          "form": "그래도 공기도 깨끗해서 좋은 것 같아요",
          "original_form": "그래도 공기도 깨끗해서 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007334.1.10",
          "form": "이때가 젤좋네요 공기가 맑아서",
          "original_form": "이때가 젤좋네요 공기가 맑아서",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007334.1.11",
          "form": "미세먼지 싫어요 ㅜㅜ",
          "original_form": "미세먼지 싫어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007334.1.12",
          "form": "오늘비는 어땠나요?",
          "original_form": "오늘비는 어땠나요?",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007334.1.13",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007334.1.14",
          "form": "전 비 별로예요",
          "original_form": "전 비 별로예요",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007334.1.15",
          "form": "앗 정말요?",
          "original_form": "앗 정말요?",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007334.1.16",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007334.1.17",
          "form": "전 좋아해요",
          "original_form": "전 좋아해요",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007334.1.18",
          "form": "오 그럴것 같았어요",
          "original_form": "오 그럴것 같았어요",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007334.1.19",
          "form": "시원하고 뭔가 센치할 때도 있고 ㅋㅋ",
          "original_form": "시원하고 뭔가 센치할 때도 있고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007334.1.20",
          "form": "옷 차려입었을 땐 너무 싫지만..",
          "original_form": "옷 차려입었을 땐 너무 싫지만..",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007334.1.21",
          "form": "예전에 비오는날 수학샘이",
          "original_form": "예전에 비오는날 수학샘이",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007334.1.22",
          "form": "미술관가서 차한잔하고 오기 숙제를 내줬는데",
          "original_form": "미술관가서 차한잔하고 오기 숙제를 내줬는데",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007334.1.23",
          "form": "지금와서 보니",
          "original_form": "지금와서 보니",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007334.1.24",
          "form": "이유를 알겠어요",
          "original_form": "이유를 알겠어요",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007334.1.25",
          "form": "와 엄청 낭만적인 숙제에요",
          "original_form": "와 엄청 낭만적인 숙제에요",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007334.1.26",
          "form": "이게 잊을수 없는 추억이 되네요",
          "original_form": "이게 잊을수 없는 추억이 되네요",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007334.1.27",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007334.1.28",
          "form": "정말 그땐 몰랐어요",
          "original_form": "정말 그땐 몰랐어요",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007334.1.29",
          "form": "비오는 날 따뜻한 차 조합까지 너무 좋아요",
          "original_form": "비오는 날 따뜻한 차 조합까지 너무 좋아요",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007334.1.30",
          "form": "몽글몽글한 기분",
          "original_form": "몽글몽글한 기분",
          "speaker_id": "1",
          "time": "20210822 01:41"
        }
      ]
    }
