      "utterance": [
        {
          "id": "MDRW2100008772.1.1",
          "form": "요즘 쇼핑하신거 있으세요?",
          "original_form": "요즘 쇼핑하신거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 18:13"
        },
        {
          "id": "MDRW2100008772.1.2",
          "form": "아까 올만에 후드집업 하나샀어요",
          "original_form": "아까 올만에 후드집업 하나샀어요",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008772.1.3",
          "form": "아 저도 사야하는데ㅎㅎ 분명히 샀는데 또 없더라구요",
          "original_form": "아 저도 사야하는데ㅎㅎ 분명히 샀는데 또 없더라구요",
          "speaker_id": "1",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008772.1.4",
          "form": "매년 느껴요ㅎ",
          "original_form": "매년 느껴요ㅎ",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008772.1.5",
          "form": "작년엔 대체 뭐입고 다녔는지",
          "original_form": "작년엔 대체 뭐입고 다녔는지",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008772.1.6",
          "form": "뭐 사신거 있으세요?",
          "original_form": "뭐 사신거 있으세요?",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008772.1.7",
          "form": "저도 가을옷 몇개사구 런닝화 구경하고 있어요ㅎㅎ",
          "original_form": "저도 가을옷 몇개사구 런닝화 구경하고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008772.1.8",
          "form": "벌써 가을옷 사셨어요?",
          "original_form": "벌써 가을옷 사셨어요?",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008772.1.9",
          "form": "네 비그치면 가을이 짧을거같아서ㅎㅎ",
          "original_form": "네 비그치면 가을이 짧을거같아서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008772.1.10",
          "form": "탁월한 선택이세요ㅎ 저도 긴팔 원피스좀 사고싶은데",
          "original_form": "탁월한 선택이세요ㅎ 저도 긴팔 원피스좀 사고싶은데",
          "speaker_id": "2",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008772.1.11",
          "form": "찾는게 요즘 넘귀찮네요",
          "original_form": "찾는게 요즘 넘귀찮네요",
          "speaker_id": "2",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008772.1.12",
          "form": "맞아요 고르다보면 지쳐요 ㅎㅎ",
          "original_form": "맞아요 고르다보면 지쳐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008772.1.13",
          "form": "쿠폰찾고 여기저기 가격비교하고ㅎ",
          "original_form": "쿠폰찾고 여기저기 가격비교하고ㅎ",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008772.1.14",
          "form": "누가 그냥 코디 해주고 사다주면 좋겠어요",
          "original_form": "누가 그냥 코디 해주고 사다주면 좋겠어요",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008772.1.15",
          "form": "맞아요ㅋㅋㅋㅋ맘에 드는옷 누가 대신 쇼핑해줬으면 ㅎㅎ",
          "original_form": "맞아요ㅋㅋㅋㅋ맘에 드는옷 누가 대신 쇼핑해줬으면 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008772.1.16",
          "form": "요즘은 약속도 못잡고하니 옷보다 먹는걸 주로사네요ㅎ",
          "original_form": "요즘은 약속도 못잡고하니 옷보다 먹는걸 주로사네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008772.1.17",
          "form": "저도 그래요ㅎㅎ",
          "original_form": "저도 그래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:27"
        }
