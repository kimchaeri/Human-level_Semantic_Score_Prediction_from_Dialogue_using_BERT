        {
          "id": "MDRW2100013312.1.1",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013312.1.2",
          "form": "중학교 때로 돌아가고싶어요",
          "original_form": "중학교 때로 돌아가고싶어요",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013312.1.3",
          "form": "전 대학생이요ㅋㅋ",
          "original_form": "전 대학생이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013312.1.4",
          "form": "대학생은 성인이라..아무생각없는미성년자로 돌아가고싶어요",
          "original_form": "대학생은 성인이라..아무생각없는미성년자로 돌아가고싶어요",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013312.1.5",
          "form": "전 대학 때 알바하고 시험 공부하던 게 그리워요",
          "original_form": "전 대학 때 알바하고 시험 공부하던 게 그리워요",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013312.1.6",
          "form": "대학때 알바하셨어요?",
          "original_form": "대학때 알바하셨어요?",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013312.1.7",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013312.1.8",
          "form": "어떤 알바하셨어요?",
          "original_form": "어떤 알바하셨어요?",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013312.1.9",
          "form": "학생회관에서 카페도하고 햄버거집에서도 일했어요",
          "original_form": "학생회관에서 카페도하고 햄버거집에서도 일했어요",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013312.1.10",
          "form": "학생시절을 알차게 보내셨네요",
          "original_form": "학생시절을 알차게 보내셨네요",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013312.1.11",
          "form": "그런가요ㅋㅋ",
          "original_form": "그런가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013312.1.12",
          "form": "동아리도 하셨나요?",
          "original_form": "동아리도 하셨나요?",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013312.1.13",
          "form": "네 춤동아리를 했었어요",
          "original_form": "네 춤동아리를 했었어요",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013312.1.14",
          "form": "우와 춤 잘추세요?",
          "original_form": "우와 춤 잘추세요?",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013312.1.15",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013312.1.16",
          "form": "대학 다니세요",
          "original_form": "대학 다니세요",
