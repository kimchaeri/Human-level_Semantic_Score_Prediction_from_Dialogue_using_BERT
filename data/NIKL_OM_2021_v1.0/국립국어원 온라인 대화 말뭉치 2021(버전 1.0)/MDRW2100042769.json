      "utterance": [
        {
          "id": "MDRW2100042769.1.1",
          "form": "제주도는 날씨어때요?",
          "original_form": "제주도는 날씨어때요?",
          "speaker_id": "1",
          "time": "20210907 08:47"
        },
        {
          "id": "MDRW2100042769.1.2",
          "form": "짐 육지나왔다가 오후비행기로 들어가는데",
          "original_form": "짐 육지나왔다가 오후비행기로 들어가는데",
          "speaker_id": "2",
          "time": "20210907 08:57"
        },
        {
          "id": "MDRW2100042769.1.3",
          "form": "맑은것 같네요",
          "original_form": "맑은것 같네요",
          "speaker_id": "2",
          "time": "20210907 08:57"
        },
        {
          "id": "MDRW2100042769.1.4",
          "form": "아..그렇군요",
          "original_form": "아..그렇군요",
          "speaker_id": "1",
          "time": "20210907 08:58"
        },
        {
          "id": "MDRW2100042769.1.5",
          "form": "제주도 멋지죠",
          "original_form": "제주도 멋지죠",
          "speaker_id": "1",
          "time": "20210907 08:58"
        },
        {
          "id": "MDRW2100042769.1.6",
          "form": "네 거긴어떤가요",
          "original_form": "네 거긴어떤가요",
          "speaker_id": "2",
          "time": "20210907 08:58"
        },
        {
          "id": "MDRW2100042769.1.7",
          "form": "여긴 비와요",
          "original_form": "여긴 비와요",
          "speaker_id": "1",
          "time": "20210907 08:58"
        },
        {
          "id": "MDRW2100042769.1.8",
          "form": "서울은 아침저녁으로 쌀쌀해진 것 같던데",
          "original_form": "서울은 아침저녁으로 쌀쌀해진 것 같던데",
          "speaker_id": "2",
          "time": "20210907 08:58"
        },
        {
          "id": "MDRW2100042769.1.9",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210907 08:58"
        },
        {
          "id": "MDRW2100042769.1.10",
          "form": "대전요",
          "original_form": "대전요",
          "speaker_id": "1",
          "time": "20210907 08:58"
        },
        {
          "id": "MDRW2100042769.1.11",
          "form": "부산인데 어제 비가 좀 오더라구요",
          "original_form": "부산인데 어제 비가 좀 오더라구요",
          "speaker_id": "2",
          "time": "20210907 08:58"
        },
        {
          "id": "MDRW2100042769.1.12",
          "form": "여기도 어제부터계속 비가와요",
          "original_form": "여기도 어제부터계속 비가와요",
          "speaker_id": "1",
          "time": "20210907 08:59"
        },
        {
          "id": "MDRW2100042769.1.13",
          "form": "아 가끔 오면 좋은데",
          "original_form": "아 가끔 오면 좋은데",
          "speaker_id": "2",
          "time": "20210907 08:59"
        },
        {
          "id": "MDRW2100042769.1.14",
          "form": "태풍이오는것같아요",
          "original_form": "태풍이오는것같아요",
          "speaker_id": "1",
          "time": "20210907 08:59"
        },
        {
          "id": "MDRW2100042769.1.15",
          "form": "아맞다 담주쯤에 올 거같네여",
          "original_form": "아맞다 담주쯤에 올 거같네여",
          "speaker_id": "2",
          "time": "20210907 09:00"
        },
        {
          "id": "MDRW2100042769.1.16",
          "form": "맑은날 좋아하시죠",
          "original_form": "맑은날 좋아하시죠",
          "speaker_id": "2",
          "time": "20210907 09:00"
        },
        {
          "id": "MDRW2100042769.1.17",
          "form": "주말은 날씨 좋았는데",
          "original_form": "주말은 날씨 좋았는데",
          "speaker_id": "1",
          "time": "20210907 09:00"
        },
        {
          "id": "MDRW2100042769.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 09:00"
        },
        {
          "id": "MDRW2100042769.1.19",
          "form": "화창하고 가을날씨",
          "original_form": "화창하고 가을날씨",
          "speaker_id": "1",
          "time": "20210907 09:00"
        },
        {
          "id": "MDRW2100042769.1.20",
          "form": "저도 가면 빨래해야해서",
          "original_form": "저도 가면 빨래해야해서",
          "speaker_id": "2",
          "time": "20210907 09:00"
        },
        {
