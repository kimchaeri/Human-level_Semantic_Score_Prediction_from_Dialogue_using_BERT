      "utterance": [
        {
          "id": "MDRW2100023019.1.1",
          "form": "맵고 당면 들어가기도 하고",
          "original_form": "맵고 당면 들어가기도 하고",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023019.1.2",
          "form": "오징어문어낙지류인가요",
          "original_form": "오징어문어낙지류인가요",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023019.1.3",
          "form": "다리가 많은 바다생물인가요?",
          "original_form": "다리가 많은 바다생물인가요?",
          "speaker_id": "2",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023019.1.4",
          "form": "네네ㅋㅋㅋㅋ 맛있는 두족류 친구...",
          "original_form": "네네ㅋㅋㅋㅋ 맛있는 두족류 친구...",
          "speaker_id": "1",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023019.1.5",
          "form": "친구 미안...",
          "original_form": "친구 미안...",
          "speaker_id": "1",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023019.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023019.1.7",
          "form": "와 당면때문에 너무 어려워요",
          "original_form": "와 당면때문에 너무 어려워요",
          "speaker_id": "2",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023019.1.8",
          "form": "뭘까...",
          "original_form": "뭘까...",
          "speaker_id": "2",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023019.1.9",
          "form": "그냥 당면 빼고 생각해보세요!",
          "original_form": "그냥 당면 빼고 생각해보세요!",
          "speaker_id": "1",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023019.1.10",
          "form": "당면넣는집 우리집뿐일수도...!",
          "original_form": "당면넣는집 우리집뿐일수도...!",
          "speaker_id": "1",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023019.1.11",
          "form": "매운낙지볶음 말했었나요??????",
          "original_form": "매운낙지볶음 말했었나요??????",
          "speaker_id": "2",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023019.1.12",
          "form": "저 지금 낙지탈트 붕괴 오고 있어요",
          "original_form": "저 지금 낙지탈트 붕괴 오고 있어요",
          "speaker_id": "2",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023019.1.13",
          "form": "어쩜 좋아",
          "original_form": "어쩜 좋아",
          "speaker_id": "2",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023019.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ낚지볶음 아니에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ낚지볶음 아니에요",
          "speaker_id": "1",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100023019.1.15",
          "form": "오징어인가요?",
          "original_form": "오징어인가요?",
