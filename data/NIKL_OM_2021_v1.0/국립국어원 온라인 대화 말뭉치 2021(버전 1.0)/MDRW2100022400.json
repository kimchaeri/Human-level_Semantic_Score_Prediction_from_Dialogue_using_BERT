      "utterance": [
        {
          "id": "MDRW2100022400.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 13:50"
        },
        {
          "id": "MDRW2100022400.1.2",
          "form": "네 안녕하세요?",
          "original_form": "네 안녕하세요?",
          "speaker_id": "1",
          "time": "20210831 20:45"
        },
        {
          "id": "MDRW2100022400.1.3",
          "form": "배달앱 뭐 쓰세요",
          "original_form": "배달앱 뭐 쓰세요",
          "speaker_id": "2",
          "time": "20210831 20:49"
        },
        {
          "id": "MDRW2100022400.1.4",
          "form": "배민 요기요요",
          "original_form": "배민 요기요요",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100022400.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100022400.1.6",
          "form": "배민이요 ㅋㅋ",
          "original_form": "배민이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100022400.1.7",
          "form": "어떤 메뉴 주로 시켜요?",
          "original_form": "어떤 메뉴 주로 시켜요?",
          "speaker_id": "2",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100022400.1.8",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100022400.1.9",
          "form": "님은요?ㅋㅋ",
          "original_form": "님은요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100022400.1.10",
          "form": "떡볶이요 ㅋㅋㅋ",
          "original_form": "떡볶이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100022400.1.11",
          "form": "어떤 브랜드를 선호하시는지도 궁금해요",
          "original_form": "어떤 브랜드를 선호하시는지도 궁금해요",
          "speaker_id": "1",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100022400.1.12",
          "form": "신전 좋아합니다",
          "original_form": "신전 좋아합니다",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100022400.1.13",
          "form": "치킨은 뭐 좋아하시나요",
          "original_form": "치킨은 뭐 좋아하시나요",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100022400.1.14",
          "form": "아 저도 신전 좋아해요",
          "original_form": "아 저도 신전 좋아해요",
          "speaker_id": "1",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100022400.1.15",
          "form": "전 교촌이요",
          "original_form": "전 교촌이요",
          "speaker_id": "1",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100022400.1.16",
          "form": "신저 몇단게 드세요?",
          "original_form": "신저 몇단게 드세요?",
          "speaker_id": "1",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100022400.1.17",
          "form": "젤 순한거요 ㅋㅋ",
          "original_form": "젤 순한거요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100022400.1.18",
          "form": "교촌이면 허니시리즈??",
          "original_form": "교촌이면 허니시리즈??",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100022400.1.19",
          "form": "전 레드콤보요 ㅋㅋ",
          "original_form": "전 레드콤보요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100022400.1.20",
          "form": "아 그것도 좋죠 ㅋㅋㅋ",
          "original_form": "아 그것도 좋죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100022400.1.21",
          "form": "좋아하는 카페 프랜차이즈 있니요",
          "original_form": "좋아하는 카페 프랜차이즈 있니요",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100022400.1.22",
          "form": "신전 튀김은 뭘 드시나요?",
          "original_form": "신전 튀김은 뭘 드시나요?",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100022400.1.23",
          "form": "딱히 없어요 ㅋㅋ",
          "original_form": "딱히 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100022400.1.24",
          "form": "오뎅튀김을 필수로 하고 있어요",
          "original_form": "오뎅튀김을 필수로 하고 있어요",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100022400.1.25",
          "form": "신전 오뎅 튀김 저렴하고 조아요",
          "original_form": "신전 오뎅 튀김 저렴하고 조아요",
          "speaker_id": "1",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100022400.1.26",
          "form": "저도 꼭 시킵니다",
          "original_form": "저도 꼭 시킵니다",
          "speaker_id": "1",
          "time": "20210831 20:57"
        }
      ]
