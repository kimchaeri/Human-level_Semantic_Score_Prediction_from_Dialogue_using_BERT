      "utterance": [
        {
          "id": "MDRW2100022872.1.1",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022872.1.2",
          "form": "안녕하세요 ~",
          "original_form": "안녕하세요 ~",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022872.1.3",
          "form": "네 여행 좋아해요~",
          "original_form": "네 여행 좋아해요~",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022872.1.4",
          "form": "오오 여행장비같은거 사시나요?",
          "original_form": "오오 여행장비같은거 사시나요?",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022872.1.5",
          "form": "캠핑요???",
          "original_form": "캠핑요???",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022872.1.6",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022872.1.7",
          "form": "저는 글램핑좋아하는데",
          "original_form": "저는 글램핑좋아하는데",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022872.1.8",
          "form": "음 저는 해본적 없는 거 같아요  친구들도 가자고 안해서 ㅋㅋ 좋은가요??",
          "original_form": "음 저는 해본적 없는 거 같아요  친구들도 가자고 안해서 ㅋㅋ 좋은가요??",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100022872.1.9",
          "form": "조금 비싸긴한데 캠핑도구없이도 편히갈수있어서좋아요",
          "original_form": "조금 비싸긴한데 캠핑도구없이도 편히갈수있어서좋아요",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022872.1.10",
          "form": "최근도 갔다 오셨나요?? 캠핑 좋아하면 주말마다 가시거라구요~",
          "original_form": "최근도 갔다 오셨나요?? 캠핑 좋아하면 주말마다 가시거라구요~",
          "speaker_id": "2",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100022872.1.11",
          "form": "최근에는..코로나라서 잘못가요 가고싶은데",
          "original_form": "최근에는..코로나라서 잘못가요 가고싶은데",
          "speaker_id": "1",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022872.1.12",
          "form": "코로나라서 오히려 많이 가시길래 가시는줄 알았어요~",
          "original_form": "코로나라서 오히려 많이 가시길래 가시는줄 알았어요~",
          "speaker_id": "2",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022872.1.13",
          "form": "4단계들어가면서 좀사리기시작했죠...",
          "original_form": "4단계들어가면서 좀사리기시작했죠...",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022872.1.14",
          "form": "많이가야재밌어서요",
          "original_form": "많이가야재밌어서요",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022872.1.15",
          "form": "아 그쵸 ㅋㅋㅋ",
          "original_form": "아 그쵸 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022872.1.16",
          "form": "캠핑가서 계속 먹다 오더라구요 ㅋㅋ 재밌겠다",
          "original_form": "캠핑가서 계속 먹다 오더라구요 ㅋㅋ 재밌겠다",
          "speaker_id": "2",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022872.1.17",
          "form": "거기서 고기구워먹으면 짱이긴하죠",
          "original_form": "거기서 고기구워먹으면 짱이긴하죠",
          "speaker_id": "1",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022872.1.18",
          "form": "불맛 나자나요",
          "original_form": "불맛 나자나요",
          "speaker_id": "2",
          "time": "20210829 19:48"
        },
        {
          "id": "MDRW2100022872.1.19",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100022872.1.20",
          "form": "불맛이 집에서는 더안나요",
          "original_form": "불맛이 집에서는 더안나요",
          "speaker_id": "1",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100022872.1.21",
          "form": "맞아요 불맛 내기도 힘들고 ㅋㅋ 그러고보니 저녁 드셨나요?",
          "original_form": "맞아요 불맛 내기도 힘들고 ㅋㅋ 그러고보니 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100022872.1.22",
          "form": "저는 방금 먹고 뒷정리했습니다. 드셨나요?",
          "original_form": "저는 방금 먹고 뒷정리했습니다. 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100022872.1.23",
          "form": "네 저두 아까먹고 씻었어요 ㅋ 벌써 월욜이 오다니 ㅠㅠ 주말 너무 짧아요",
          "original_form": "네 저두 아까먹고 씻었어요 ㅋ 벌써 월욜이 오다니 ㅠㅠ 주말 너무 짧아요",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100022872.1.24",
          "form": "그러게ㅠ 벌써 시간이 이렇게됬네요 출근하기싫네요ㅠ",
          "original_form": "그러게ㅠ 벌써 시간이 이렇게됬네요 출근하기싫네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100022872.1.25",
          "form": "맞아요 ㅠㅠ 내일도 쉬었으면 좋겠어요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅠㅠ 내일도 쉬었으면 좋겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100022872.1.26",
          "form": "저는 항상...ㅋㅋㅋ",
          "original_form": "저는 항상...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100022872.1.27",
          "form": "아 항상은 저도 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 항상은 저도 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100022872.1.28",
          "form": "ㅋㅋㅋ전부그럴거같아요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ전부그럴거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100022872.1.29",
          "form": "뭐했다고 벌써 11시임박이죠?",
          "original_form": "뭐했다고 벌써 11시임박이죠?",
          "speaker_id": "2",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100022872.1.30",
          "form": "어후 자야할시간이네요ㅠ",
          "original_form": "어후 자야할시간이네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100022872.1.31",
          "form": "몇시쯤 주무세요?",
          "original_form": "몇시쯤 주무세요?",
          "speaker_id": "2",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100022872.1.32",
          "form": "저는 한 1시쯤?",
          "original_form": "저는 한 1시쯤?",
          "speaker_id": "1",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100022872.1.33",
          "form": "항상요? 아침에 몇시 일어나시는데요?  피곤하실거 같은데",
          "original_form": "항상요? 아침에 몇시 일어나시는데요?  피곤하실거 같은데",
          "speaker_id": "2",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100022872.1.34",
          "form": "아침에 한10시쯤 일어나요 오후출근이라서",
          "original_form": "아침에 한10시쯤 일어나요 오후출근이라서",
          "speaker_id": "1",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100022872.1.35",
          "form": "아 늦은 출근이시군여~ 그러면 더 즐기다 자야죠 ㅋ",
          "original_form": "아 늦은 출근이시군여~ 그러면 더 즐기다 자야죠 ㅋ",
          "speaker_id": "2",
          "time": "20210829 23:30"
        },
        {
          "id": "MDRW2100022872.1.36",
          "form": "몇시쯤 주무세요?",
          "original_form": "몇시쯤 주무세요?",
          "speaker_id": "1",
          "time": "20210829 23:36"
        },
        {
          "id": "MDRW2100022872.1.37",
