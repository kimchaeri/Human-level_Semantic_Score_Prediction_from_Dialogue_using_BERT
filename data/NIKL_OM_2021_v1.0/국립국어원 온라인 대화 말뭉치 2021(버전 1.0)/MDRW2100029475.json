      "utterance": [
        {
          "id": "MDRW2100029475.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100029475.1.2",
          "form": "네 안녕하세요!",
          "original_form": "네 안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 09:32"
        },
        {
          "id": "MDRW2100029475.1.3",
          "form": "동호회 하시나요?",
          "original_form": "동호회 하시나요?",
          "speaker_id": "2",
          "time": "20210903 09:32"
        },
        {
          "id": "MDRW2100029475.1.4",
          "form": "게임 동호회 하고 있어요",
          "original_form": "게임 동호회 하고 있어요",
          "speaker_id": "1",
          "time": "20210903 09:34"
        },
        {
          "id": "MDRW2100029475.1.5",
          "form": "코로나 때문에 모이지는 못하지만요",
          "original_form": "코로나 때문에 모이지는 못하지만요",
          "speaker_id": "1",
          "time": "20210903 09:34"
        },
        {
          "id": "MDRW2100029475.1.6",
          "form": "우와 어떤 게임 하세요?",
          "original_form": "우와 어떤 게임 하세요?",
          "speaker_id": "2",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100029475.1.7",
          "form": "하얀고양이 프로젝트라고 한국에서는 섭종한 게임이에요",
          "original_form": "하얀고양이 프로젝트라고 한국에서는 섭종한 게임이에요",
          "speaker_id": "1",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100029475.1.8",
          "form": "아~ 인기가 많이 없었나보네요",
          "original_form": "아~ 인기가 많이 없었나보네요",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029475.1.9",
          "form": "네ㅠ 슬프지만 하는 사람만 했다네요",
          "original_form": "네ㅠ 슬프지만 하는 사람만 했다네요",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029475.1.10",
          "form": "그래서 동호회로 하시는구나ㅜㅜㅜ",
          "original_form": "그래서 동호회로 하시는구나ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029475.1.11",
          "form": "봉사활동은 하시나요??",
          "original_form": "봉사활동은 하시나요??",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029475.1.12",
          "form": "했었는데 코로나 이후로는 못하고 있어요",
          "original_form": "했었는데 코로나 이후로는 못하고 있어요",
          "speaker_id": "1",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100029475.1.13",
          "form": "어떤 봉사 하셨어요?",
          "original_form": "어떤 봉사 하셨어요?",
          "speaker_id": "2",
          "time": "20210903 12:21"
        },
        {
          "id": "MDRW2100029475.1.14",
          "form": "몸이 불편하신 분들 도왔어요",
          "original_form": "몸이 불편하신 분들 도왔어요",
          "speaker_id": "1",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029475.1.15",
          "form": "오~ 멋지시네요!",
          "original_form": "오~ 멋지시네요!",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029475.1.16",
          "form": "매주 다녔었는데 그것도 사실",
          "original_form": "매주 다녔었는데 그것도 사실",
          "speaker_id": "1",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100029475.1.17",
          "form": "봉사시간 채울려고 그랬었네요",
          "original_form": "봉사시간 채울려고 그랬었네요",
          "speaker_id": "1",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100029475.1.18",
          "form": "아~ 봉사시간 채워야 되시나보군요",
          "original_form": "아~ 봉사시간 채워야 되시나보군요",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100029475.1.19",
          "form": "그거 채우는거 너무 귀찮아요...",
          "original_form": "그거 채우는거 너무 귀찮아요...",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100029475.1.20",
          "form": "저는 봉사 채울때 지하철...에서 한 기억밖에 없네요ㅋㅋ",
          "original_form": "저는 봉사 채울때 지하철...에서 한 기억밖에 없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100029475.1.21",
          "form": "저는 주변에 할만한 곳이 더 없었어요",
          "original_form": "저는 주변에 할만한 곳이 더 없었어요",
