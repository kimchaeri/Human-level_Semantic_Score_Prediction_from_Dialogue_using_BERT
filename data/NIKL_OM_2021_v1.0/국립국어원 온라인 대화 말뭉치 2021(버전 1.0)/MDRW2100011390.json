      "utterance": [
        {
          "id": "MDRW2100011390.1.1",
          "form": "가가구님어디가보셨어요",
          "original_form": "가가구님어디가보셨어요",
          "speaker_id": "2",
          "time": "20210824 11:58"
        },
        {
          "id": "MDRW2100011390.1.2",
          "form": "저 해외는 대만 세부 일본 홍콩 가봤어요ㅎㅎ",
          "original_form": "저 해외는 대만 세부 일본 홍콩 가봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:01"
        },
        {
          "id": "MDRW2100011390.1.3",
          "form": "와 많이가셨네요",
          "original_form": "와 많이가셨네요",
          "speaker_id": "2",
          "time": "20210824 12:01"
        },
        {
          "id": "MDRW2100011390.1.4",
          "form": "저는 인도 일본 우즈베키스탄 딱여기 ㅎ",
          "original_form": "저는 인도 일본 우즈베키스탄 딱여기 ㅎ",
          "speaker_id": "2",
          "time": "20210824 12:01"
        },
        {
          "id": "MDRW2100011390.1.5",
          "form": "와 인도 대박 어떠셨나요",
          "original_form": "와 인도 대박 어떠셨나요",
          "speaker_id": "1",
          "time": "20210824 12:03"
        },
        {
          "id": "MDRW2100011390.1.6",
          "form": "관광이 아니고 해외봉사로 간거라 여기저기는 못본거같아요",
          "original_form": "관광이 아니고 해외봉사로 간거라 여기저기는 못본거같아요",
          "speaker_id": "2",
          "time": "20210824 12:04"
        },
        {
          "id": "MDRW2100011390.1.7",
          "form": "아 그러시구나 일본은 어디 가보셨나요",
          "original_form": "아 그러시구나 일본은 어디 가보셨나요",
          "speaker_id": "1",
          "time": "20210824 12:05"
        },
        {
          "id": "MDRW2100011390.1.8",
          "form": "북해도갔었어요 가가구님은요?",
          "original_form": "북해도갔었어요 가가구님은요?",
          "speaker_id": "2",
          "time": "20210824 12:05"
        },
        {
          "id": "MDRW2100011390.1.9",
          "form": "저는 오사카랑 후쿠오카요!",
          "original_form": "저는 오사카랑 후쿠오카요!",
          "speaker_id": "1",
          "time": "20210824 12:06"
        },
        {
          "id": "MDRW2100011390.1.10",
          "form": "오 부럽네요 맛있는거 많은곳이네요",
          "original_form": "오 부럽네요 맛있는거 많은곳이네요",
          "speaker_id": "2",
          "time": "20210824 12:07"
        },
        {
          "id": "MDRW2100011390.1.11",
          "form": "일본 특유의 분위기 너무 좋아요",
          "original_form": "일본 특유의 분위기 너무 좋아요",
          "speaker_id": "1",
          "time": "20210824 12:07"
        },
        {
          "id": "MDRW2100011390.1.12",
          "form": "네 제가 간곳은 따뜻한동네 느낌이 들더라구요",
          "original_form": "네 제가 간곳은 따뜻한동네 느낌이 들더라구요",
          "speaker_id": "2",
          "time": "20210824 12:09"
        },
        {
          "id": "MDRW2100011390.1.13",
          "form": "코로나끝나면 다시 가고싶네요ㅠ",
          "original_form": "코로나끝나면 다시 가고싶네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:22"
        }
      ]
