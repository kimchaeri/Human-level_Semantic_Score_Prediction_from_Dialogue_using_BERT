        },
        {
          "id": "MDRW2100014177.1.2",
          "form": "군대다녀온 친구들과의 썰이 있니?",
          "original_form": "군대다녀온 친구들과의 썰이 있니?",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014177.1.3",
          "form": "남자동기들이 편지써달라구 해서 정말 많이 썼던거같아",
          "original_form": "남자동기들이 편지써달라구 해서 정말 많이 썼던거같아",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014177.1.4",
          "form": "name2이 너는 있니?",
          "original_form": "&name2&이 너는 있니?",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014177.1.5",
          "form": "아니 나는 전혀",
          "original_form": "아니 나는 전혀",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014177.1.6",
          "form": "남자동기들 군대가면 그냥 신경안쓴듯 ㅋㅋㅋ",
          "original_form": "남자동기들 군대가면 그냥 신경안쓴듯 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014177.1.7",
          "form": "와 전혀라니 쿨했네ㅋㅋㅋㅋ",
          "original_form": "와 전혀라니 쿨했네ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014177.1.8",
          "form": "어 정말 나는 군대간 분들과는",
          "original_form": "어 정말 나는 군대간 분들과는",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014177.1.9",
          "form": "편지쓰거나 연락한적이없어",
          "original_form": "편지쓰거나 연락한적이없어",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014177.1.10",
          "form": "아 동생있네 동생에게 편지썼었지",
          "original_form": "아 동생있네 동생에게 편지썼었지",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014177.1.11",
          "form": "그래도 동생한테는 써주고 착한누나구나",
          "original_form": "그래도 동생한테는 써주고 착한누나구나",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014177.1.12",
          "form": "남친도 군대는 면제여서 그럴일이없었네",
          "original_form": "남친도 군대는 면제여서 그럴일이없었네",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014177.1.13",
          "form": "이번에 회사에 군대 다녀온 직원은 갑자기 디스크가 와서 디스크 수술 했대ㅠㅠ",
          "original_form": "이번에 회사에 군대 다녀온 직원은 갑자기 디스크가 와서 디스크 수술 했대ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014177.1.14",
          "form": "헐 어떡하냐   그러고보니 내친구동생도 군대가서 손가락 절단됐었다..수술잘되긴했지만ㅜㅜ",
          "original_form": "헐 어떡하냐   그러고보니 내친구동생도 군대가서 손가락 절단됐었다..수술잘되긴했지만ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014177.1.15",
          "form": "헐 절단이라니ㅜㅜ",
          "original_form": "헐 절단이라니ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014177.1.16",
          "form": "회사분은 수술받은게 좀 잘못된것같았어ㅠㅠ",
          "original_form": "회사분은 수술받은게 좀 잘못된것같았어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014177.1.17",
          "form": "젊으신 분인데 안타까워",
          "original_form": "젊으신 분인데 안타까워",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014177.1.18",
          "form": "군대가 참 그렇다잉.. 고생많은곳ㅜㅜ",
          "original_form": "군대가 참 그렇다잉.. 고생많은곳ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014177.1.19",
          "form": "맞아 그래도 주변분들 다 잘 제대해서 다행이야",
          "original_form": "맞아 그래도 주변분들 다 잘 제대해서 다행이야",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014177.1.20",
          "form": "응 다들 건강하게 잘 돌아왔어",
          "original_form": "응 다들 건강하게 잘 돌아왔어",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014177.1.21",
          "form": "나보고가라하면못가겠다...",
          "original_form": "나보고가라하면못가겠다...",
