      "utterance": [
        {
          "id": "MDRW2100001044.1.1",
          "form": "요즘 날씨 엄청덥잖아",
          "original_form": "요즘 날씨 엄청덥잖아",
          "speaker_id": "2",
          "time": "20210722 16:24"
        },
        {
          "id": "MDRW2100001044.1.2",
          "form": "그치 엄청 더워",
          "original_form": "그치 엄청 더워",
          "speaker_id": "1",
          "time": "20210722 16:25"
        },
        {
          "id": "MDRW2100001044.1.3",
          "form": "밖에 돌아다녀봤어?",
          "original_form": "밖에 돌아다녀봤어?",
          "speaker_id": "1",
          "time": "20210722 16:25"
        },
        {
          "id": "MDRW2100001044.1.4",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210722 16:25"
        },
        {
          "id": "MDRW2100001044.1.5",
          "form": "완전 더워서",
          "original_form": "완전 더워서",
          "speaker_id": "2",
          "time": "20210722 16:25"
        },
        {
          "id": "MDRW2100001044.1.6",
          "form": "숨이 막혀 진짜",
          "original_form": "숨이 막혀 진짜",
          "speaker_id": "2",
          "time": "20210722 16:25"
        },
        {
          "id": "MDRW2100001044.1.7",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210722 16:26"
        },
        {
          "id": "MDRW2100001044.1.8",
          "form": "이번주는 계속 덥대",
          "original_form": "이번주는 계속 덥대",
          "speaker_id": "1",
          "time": "20210722 16:26"
        },
        {
          "id": "MDRW2100001044.1.9",
          "form": "작년 여름 기억나?",
          "original_form": "작년 여름 기억나?",
          "speaker_id": "1",
          "time": "20210722 16:26"
        },
        {
          "id": "MDRW2100001044.1.10",
          "form": "잘 기억안나",
          "original_form": "잘 기억안나",
          "speaker_id": "2",
          "time": "20210722 16:26"
        },
        {
          "id": "MDRW2100001044.1.11",
          "form": "작년에는",
          "original_form": "작년에는",
          "speaker_id": "1",
          "time": "20210722 16:27"
        },
        {
          "id": "MDRW2100001044.1.12",
          "form": "비 엄청 와서",
          "original_form": "비 엄청 와서",
          "speaker_id": "1",
          "time": "20210722 16:27"
        },
        {
          "id": "MDRW2100001044.1.13",
          "form": "별로 덥진않았거든",
          "original_form": "별로 덥진않았거든",
          "speaker_id": "1",
          "time": "20210722 16:27"
        },
        {
          "id": "MDRW2100001044.1.14",
          "form": "비오는것도 싫지만",
          "original_form": "비오는것도 싫지만",
          "speaker_id": "1",
          "time": "20210722 16:27"
        },
        {
          "id": "MDRW2100001044.1.15",
          "form": "더운것도 싫다 ㅠㅠ",
          "original_form": "더운것도 싫다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210722 16:27"
        },
        {
          "id": "MDRW2100001044.1.16",
          "form": "아 진짜 인정...",
          "original_form": "아 진짜 인정...",
          "speaker_id": "2",
          "time": "20210722 16:28"
        },
        {
          "id": "MDRW2100001044.1.17",
          "form": "습한것도 진짜 싫어..",
          "original_form": "습한것도 진짜 싫어..",
          "speaker_id": "2",
          "time": "20210722 16:29"
        },
        {
          "id": "MDRW2100001044.1.18",
          "form": "어떤 계절 좋아해",
          "original_form": "어떤 계절 좋아해",
          "speaker_id": "1",
          "time": "20210722 16:29"
        },
        {
          "id": "MDRW2100001044.1.19",
          "form": "난 겨울!!",
          "original_form": "난 겨울!!",
          "speaker_id": "2",
          "time": "20210722 16:29"
        },
        {
          "id": "MDRW2100001044.1.20",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210722 16:30"
        },
        {
          "id": "MDRW2100001044.1.21",
          "form": "난 추운게 좋거든",
          "original_form": "난 추운게 좋거든",
          "speaker_id": "2",
          "time": "20210722 16:30"
        },
        {
          "id": "MDRW2100001044.1.22",
          "form": "눈 내리는 것도 좋고",
          "original_form": "눈 내리는 것도 좋고",
          "speaker_id": "2",
          "time": "20210722 16:30"
        },
        {
          "id": "MDRW2100001044.1.23",
          "form": "한해가 끝나가는 분위기도 있어서",
          "original_form": "한해가 끝나가는 분위기도 있어서",
          "speaker_id": "2",
          "time": "20210722 16:30"
        },
        {
          "id": "MDRW2100001044.1.24",
          "form": "너무 좋아!",
          "original_form": "너무 좋아!",
          "speaker_id": "2",
          "time": "20210722 16:30"
        },
        {
          "id": "MDRW2100001044.1.25",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210722 16:31"
        },
        {
          "id": "MDRW2100001044.1.26",
          "form": "나도 겨울 좋아해",
          "original_form": "나도 겨울 좋아해",
          "speaker_id": "1",
          "time": "20210722 16:31"
        },
        {
          "id": "MDRW2100001044.1.27",
          "form": "더운것보다",
          "original_form": "더운것보다",
          "speaker_id": "1",
          "time": "20210722 16:31"
        },
        {
          "id": "MDRW2100001044.1.28",
          "form": "추운게 나아",
          "original_form": "추운게 나아",
          "speaker_id": "1",
          "time": "20210722 16:31"
        },
        {
          "id": "MDRW2100001044.1.29",
          "form": "인정 ㅠㅠ",
          "original_form": "인정 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 16:32"
        }
      ]
    }
