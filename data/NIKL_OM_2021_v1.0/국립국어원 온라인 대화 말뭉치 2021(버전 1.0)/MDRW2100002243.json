      "utterance": [
        {
          "id": "MDRW2100002243.1.1",
          "form": "동물 좋아해?",
          "original_form": "동물 좋아해?",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002243.1.2",
          "form": "응 좋아해",
          "original_form": "응 좋아해",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002243.1.3",
          "form": "너는 좋아해?",
          "original_form": "너는 좋아해?",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002243.1.4",
          "form": "좋아하긴 하는데",
          "original_form": "좋아하긴 하는데",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002243.1.5",
          "form": "어떤 동물은 좋아하고 어떤 동물은 안좋아해",
          "original_form": "어떤 동물은 좋아하고 어떤 동물은 안좋아해",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002243.1.6",
          "form": "그래? 어떤 동물을 좋아하는데?",
          "original_form": "그래? 어떤 동물을 좋아하는데?",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002243.1.7",
          "form": "나는 고양이 좋아하고",
          "original_form": "나는 고양이 좋아하고",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002243.1.8",
          "form": "고라니는 안좋아해",
          "original_form": "고라니는 안좋아해",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002243.1.9",
          "form": "고라니라니 정말 생각도 못한 동물인걸",
          "original_form": "고라니라니 정말 생각도 못한 동물인걸",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002243.1.10",
          "form": "고양이는 나도 좋아해",
          "original_form": "고양이는 나도 좋아해",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002243.1.11",
          "form": "ㅋㅋ 고라니 무서워",
          "original_form": "ㅋㅋ 고라니 무서워",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002243.1.12",
          "form": "고양이 키우고 싶다",
          "original_form": "고양이 키우고 싶다",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002243.1.13",
          "form": "너는 고양이가 좋아 강아지가 좋아?",
          "original_form": "너는 고양이가 좋아 강아지가 좋아?",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002243.1.14",
          "form": "나는 어렸을때부터 강아지가 더 좋았어",
          "original_form": "나는 어렸을때부터 강아지가 더 좋았어",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002243.1.15",
          "form": "강아지 귀엽지",
          "original_form": "강아지 귀엽지",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002243.1.16",
          "form": "특별히 좋아하는 종 있어?",
          "original_form": "특별히 좋아하는 종 있어?",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002243.1.17",
          "form": "난 딱히 종은 가리지 않아서 없어",
          "original_form": "난 딱히 종은 가리지 않아서 없어",
          "speaker_id": "2",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002243.1.18",
          "form": "나도 마찬가지야",
          "original_form": "나도 마찬가지야",
          "speaker_id": "1",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002243.1.19",
          "form": "굳이 따지자면 나는 회색 고양이가 좋은거 같아",
          "original_form": "굳이 따지자면 나는 회색 고양이가 좋은거 같아",
          "speaker_id": "1",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002243.1.20",
          "form": "코리안숏헤어라고 부르는 길고양이 중에서 가장 많은 고양이!",
          "original_form": "코리안숏헤어라고 부르는 길고양이 중에서 가장 많은 고양이!",
          "speaker_id": "1",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002243.1.21",
          "form": "아 어떤 고양이인지 알아",
          "original_form": "아 어떤 고양이인지 알아",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002243.1.22",
          "form": "그런 고양이가 좋아",
          "original_form": "그런 고양이가 좋아",
          "speaker_id": "1",
          "time": "20210820 12:28"
        }
      ]
