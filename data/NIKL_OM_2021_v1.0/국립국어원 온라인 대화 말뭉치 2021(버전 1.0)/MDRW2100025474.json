        },
        {
          "id": "MDRW2100025474.1.2",
          "form": "묵비권을 행사한다니까욧!! ㅎ",
          "original_form": "묵비권을 행사한다니까욧!! ㅎ",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025474.1.3",
          "form": "아 우리회사는  지역과 상생합니다ㅎㅎ",
          "original_form": "아 우리회사는  지역과 상생합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100025474.1.4",
          "form": "ㅋㅋ아 앞에가 짤려서",
          "original_form": "ㅋㅋ아 앞에가 짤려서",
          "speaker_id": "2",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025474.1.5",
          "form": "저희도 상생합니다",
          "original_form": "저희도 상생합니다",
          "speaker_id": "2",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025474.1.6",
          "form": "이전할수가 없어요ㅋㅋㅋ",
          "original_form": "이전할수가 없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025474.1.7",
          "form": "ㅎㅎ 우리는 지역상생을 위해 구내식당을 안 만들죠",
          "original_form": "ㅎㅎ 우리는 지역상생을 위해 구내식당을 안 만들죠",
          "speaker_id": "1",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025474.1.8",
          "form": "이 악덕 사업주ㅜ",
          "original_form": "이 악덕 사업주ㅜ",
          "speaker_id": "1",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025474.1.9",
          "form": "훔~~대기업이군요 ㅋㅋ",
          "original_form": "훔~~대기업이군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025474.1.10",
          "form": "구내식당이라니",
          "original_form": "구내식당이라니",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025474.1.11",
          "form": "아뇨ㅎ 그냥 회장이 임대장사하느라ㅎ",
          "original_form": "아뇨ㅎ 그냥 회장이 임대장사하느라ㅎ",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100025474.1.12",
          "form": "말만 번지르해서 열받네요ㅎㅎ",
          "original_form": "말만 번지르해서 열받네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100025474.1.13",
          "form": "name2도 구내식당 없나요",
          "original_form": "&name2&도 구내식당 없나요",
          "speaker_id": "1",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025474.1.14",
          "form": "ㅋㅋ음 있긴한데 여러군데에 나눠서 있어서요",
          "original_form": "ㅋㅋ음 있긴한데 여러군데에 나눠서 있어서요",
          "speaker_id": "2",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100025474.1.15",
          "form": "회사가 일반 회사가 아니라서 ㅎㅎㅎ",
          "original_form": "회사가 일반 회사가 아니라서 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100025474.1.16",
          "form": "아 여러  지점 다 이용 가능한 거죠?",
          "original_form": "아 여러  지점 다 이용 가능한 거죠?",
