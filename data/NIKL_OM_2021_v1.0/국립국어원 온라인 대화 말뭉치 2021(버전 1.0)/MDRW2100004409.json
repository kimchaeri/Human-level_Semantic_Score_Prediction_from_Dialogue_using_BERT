      "utterance": [
        {
          "id": "MDRW2100004409.1.1",
          "form": "안녕! 가족이랑 대화 자주하는편이야?",
          "original_form": "안녕! 가족이랑 대화 자주하는편이야?",
          "speaker_id": "1",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004409.1.2",
          "form": "별로?",
          "original_form": "별로?",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004409.1.3",
          "form": "갑자기 호구조사?",
          "original_form": "갑자기 호구조사?",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004409.1.4",
          "form": "그냥 주제가 가족이라는데 난 가정을 꾸리고싶진않아서^^ ㅋㅋㅋ",
          "original_form": "그냥 주제가 가족이라는데 난 가정을 꾸리고싶진않아서^^ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004409.1.5",
          "form": "진로때문에 이것저것 얘기는 하는듯?",
          "original_form": "진로때문에 이것저것 얘기는 하는듯?",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004409.1.6",
          "form": "돈 벌면 반드시 자취를!",
          "original_form": "돈 벌면 반드시 자취를!",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004409.1.7",
          "form": "나도 자취하면 근처 친구나 비혼룸메를 꼭 두고싶어",
          "original_form": "나도 자취하면 근처 친구나 비혼룸메를 꼭 두고싶어",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004409.1.8",
          "form": "이웃은 좋지만 룸메는 스트레스받을거같아서",
          "original_form": "이웃은 좋지만 룸메는 스트레스받을거같아서",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004409.1.9",
          "form": "난 혼자가 좋아",
          "original_form": "난 혼자가 좋아",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004409.1.10",
          "form": "아니 이게 왜 나쁜말?",
          "original_form": "아니 이게 왜 나쁜말?",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004409.1.11",
          "form": "난 룸메라면 조건이",
          "original_form": "난 룸메라면 조건이",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004409.1.12",
          "form": "거실을 중심으로 두고 방과방이 끝에 위치해야해",
          "original_form": "거실을 중심으로 두고 방과방이 끝에 위치해야해",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004409.1.13",
          "form": "서로 소음 안들리게ㅋㅋ",
          "original_form": "서로 소음 안들리게ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004409.1.14",
          "form": "멀어야되는군",
          "original_form": "멀어야되는군",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004409.1.15",
          "form": "집안일은 어찌 나눌 생각이야?",
          "original_form": "집안일은 어찌 나눌 생각이야?",
          "speaker_id": "2",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004409.1.16",
          "form": "화장실은 2개있지않을까 그정도 사이즈면 그러니 각자고",
          "original_form": "화장실은 2개있지않을까 그정도 사이즈면 그러니 각자고",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004409.1.17",
          "form": "요리도 철저하게 자기꺼후 정돈",
          "original_form": "요리도 철저하게 자기꺼후 정돈",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004409.1.18",
          "form": "걍 모든걸 각자ㅋㅋㅋㅋㅋㅋ 청소기돌리는거정도야 거실 부엌은 금방하니",
          "original_form": "걍 모든걸 각자ㅋㅋㅋㅋㅋㅋ 청소기돌리는거정도야 거실 부엌은 금방하니",
          "speaker_id": "1",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004409.1.19",
          "form": "돈이 꽤 필요하겠는걸",
          "original_form": "돈이 꽤 필요하겠는걸",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004409.1.20",
          "form": "방2개에 화장실 2개",
          "original_form": "방2개에 화장실 2개",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004409.1.21",
          "form": "청소는 그렇지ㅋㅋㅋㅋ",
          "original_form": "청소는 그렇지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004409.1.22",
          "form": "근데 자취자체가 돈이 들수밖에없어ㅋㅋㅋ",
          "original_form": "근데 자취자체가 돈이 들수밖에없어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004409.1.23",
          "form": "난 룸메 저런식으로 원하는게",
          "original_form": "난 룸메 저런식으로 원하는게",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004409.1.24",
          "form": "혼자는 위험하잖아",
          "original_form": "혼자는 위험하잖아",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004409.1.25",
          "form": "기절하면 누가 날 발견해",
          "original_form": "기절하면 누가 날 발견해",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004409.1.26",
          "form": "그렇긴 하지",
          "original_form": "그렇긴 하지",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004409.1.27",
          "form": "아님 우산이라도 갔다줄수도있는거고",
          "original_form": "아님 우산이라도 갔다줄수도있는거고",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004409.1.28",
          "form": "흠 어려운걸",
          "original_form": "흠 어려운걸",
          "speaker_id": "2",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004409.1.29",
          "form": "혼자 살면 배달음식 시키기도 무서울거같긴 해",
          "original_form": "혼자 살면 배달음식 시키기도 무서울거같긴 해",
          "speaker_id": "2",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004409.1.30",
          "form": "근데 나라면 배달비가 아까워서 포장시킬듯",
          "original_form": "근데 나라면 배달비가 아까워서 포장시킬듯",
          "speaker_id": "2",
          "time": "20210821 16:36"
        }
      ]
    }
