        },
        {
          "id": "MDRW2100030827.1.2",
          "form": "아쉽게도(?) 싸이월드 하지않았습니다",
          "original_form": "아쉽게도(?) 싸이월드 하지않았습니다",
          "speaker_id": "2",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030827.1.3",
          "form": "name1는?",
          "original_form": "&name1&는?",
          "speaker_id": "2",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030827.1.4",
          "form": "name1도 아쉽게도 싸이월드 하지 않았습니다",
          "original_form": "&name1&도 아쉽게도 싸이월드 하지 않았습니다",
          "speaker_id": "1",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100030827.1.5",
          "form": "하지만 도토리 선물이 있었다는 것과",
          "original_form": "하지만 도토리 선물이 있었다는 것과",
          "speaker_id": "1",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100030827.1.6",
          "form": "싸이월드 부활 얘기가 정말로 나오던건 생각이 나는군여",
          "original_form": "싸이월드 부활 얘기가 정말로 나오던건 생각이 나는군여",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100030827.1.7",
          "form": "누군가들의 과거의 중2스런 흔적들이",
          "original_form": "누군가들의 과거의 중2스런 흔적들이",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100030827.1.8",
          "form": "세상 밖으로 다시 나올지두",
          "original_form": "세상 밖으로 다시 나올지두",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100030827.1.9",
          "form": "하이파이브",
          "original_form": "하이파이브",
          "speaker_id": "2",
          "time": "20210904 18:57"
        },
        {
          "id": "MDRW2100030827.1.10",
          "form": "ㅋㅋ맞아요 싸이월드 흑역사가 되살아나서",
          "original_form": "ㅋㅋ맞아요 싸이월드 흑역사가 되살아나서",
          "speaker_id": "2",
          "time": "20210904 18:57"
        },
        {
          "id": "MDRW2100030827.1.11",
          "form": "바로 삭제한다는 사람들도ㅋㅋㅋ",
          "original_form": "바로 삭제한다는 사람들도ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:57"
        },
        {
          "id": "MDRW2100030827.1.12",
          "form": "안해서 다행입니다ㅎㅎ 지울게 없네요",
          "original_form": "안해서 다행입니다ㅎㅎ 지울게 없네요",
          "speaker_id": "2",
          "time": "20210904 18:57"
        },
        {
          "id": "MDRW2100030827.1.13",
          "form": "ㅎㅎ네이버 블로그에는 혹시 부끄러운 글같은거",
          "original_form": "ㅎㅎ네이버 블로그에는 혹시 부끄러운 글같은거",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100030827.1.14",
          "form": "남은거업나용",
          "original_form": "남은거업나용",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100030827.1.15",
          "form": "전 싸이소식듣고 네이버블로그 글을 죄다 비공개해버렷어요",
          "original_form": "전 싸이소식듣고 네이버블로그 글을 죄다 비공개해버렷어요",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100030827.1.16",
          "form": "생각나버려서...",
          "original_form": "생각나버려서...",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100030827.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ하 싸이월드 대신 블로그가",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ하 싸이월드 대신 블로그가",
          "speaker_id": "2",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100030827.1.18",
          "form": "흑역사쥬^-^",
          "original_form": "흑역사쥬^-^",
          "speaker_id": "2",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100030827.1.19",
          "form": "비공해놨죠 name3이 제 블로그글 가져오길래",
          "original_form": "비공해놨죠 &name3&이 제 블로그글 가져오길래",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100030827.1.20",
          "form": "그날로 비공 다 걸었습니다",
          "original_form": "그날로 비공 다 걸었습니다",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100030827.1.21",
          "form": "다..비공개겠지...?",
          "original_form": "다..비공개겠지...?",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100030827.1.22",
          "form": "아마도...",
          "original_form": "아마도...",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100030827.1.23",
          "form": "ㅋㅋㅋㅋㅋ걱정되면 함 보고와두...?",
          "original_form": "ㅋㅋㅋㅋㅋ걱정되면 함 보고와두...?",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100030827.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ구래야지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ구래야지",
          "speaker_id": "2",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100030827.1.25",
          "form": "사실 2달전쯤인가 네이버블로그 챌린지한다고",
          "original_form": "사실 2달전쯤인가 네이버블로그 챌린지한다고",
          "speaker_id": "2",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100030827.1.26",
          "form": "일기 썼었거든",
          "original_form": "일기 썼었거든",
          "speaker_id": "2",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100030827.1.27",
          "form": "ㅎ중딩때 쓴만큼의 흑역사는 아니지만 꺌꺌",
          "original_form": "ㅎ중딩때 쓴만큼의 흑역사는 아니지만 꺌꺌",
          "speaker_id": "2",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100030827.1.28",
          "form": "아~맞아 나도 썼는데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아~맞아 나도 썼는데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:50"
        },
        {
          "id": "MDRW2100030827.1.29",
          "form": "그 일기도 한참지나면 부끄러워지겠지...",
          "original_form": "그 일기도 한참지나면 부끄러워지겠지...",
          "speaker_id": "1",
          "time": "20210905 12:50"
        },
        {
          "id": "MDRW2100030827.1.30",
          "form": "싸이도 부활햇는데 다시 사람들 쓰게하려면",
          "original_form": "싸이도 부활햇는데 다시 사람들 쓰게하려면",
          "speaker_id": "1",
          "time": "20210905 12:50"
        },
        {
          "id": "MDRW2100030827.1.31",
          "form": "그런 돈주는 이벤이 잇어야할지도 모르겟군여",
          "original_form": "그런 돈주는 이벤이 잇어야할지도 모르겟군여",
          "speaker_id": "1",
          "time": "20210905 12:50"
        },
        {
          "id": "MDRW2100030827.1.32",
          "form": "ㅇ니 진차????",
          "original_form": "ㅇ니 진차????",
          "speaker_id": "2",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100030827.1.33",
          "form": "우리 서로이웃..인데 왜 몰랐지(머쓱",
          "original_form": "우리 서로이웃..인데 왜 몰랐지(머쓱",
          "speaker_id": "2",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100030827.1.34",
          "form": "ㅋㅋㅋㅋㅋ그때의 갬성으로~~",
          "original_form": "ㅋㅋㅋㅋㅋ그때의 갬성으로~~",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100030827.1.35",
          "form": "싸이월드 부활한 만큼 21세기에 맞게 다시 태어나길~!",
          "original_form": "싸이월드 부활한 만큼 21세기에 맞게 다시 태어나길~!",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100030827.1.36",
          "form": "돈도 좀 뿌리고ㅎㅎ",
          "original_form": "돈도 좀 뿌리고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100030827.1.37",
          "form": "아 왜냐면 나 다른계정에서 일기 썼어서ㅋㅋㅋㅋㅋ",
          "original_form": "아 왜냐면 나 다른계정에서 일기 썼어서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100030827.1.38",
          "form": "아앗 짜식...!",
          "original_form": "아앗 짜식...!",
          "speaker_id": "2",
          "time": "20210905 17:53"
        },
        {
          "id": "MDRW2100030827.1.39",
          "form": "비밀리에 썼을줄이야!!!",
          "original_form": "비밀리에 썼을줄이야!!!",
          "speaker_id": "2",
          "time": "20210905 17:53"
        },
        {
          "id": "MDRW2100030827.1.40",
          "form": "우린 싸이월드 사용안해봤지만 써봤던 사람들은 추억 퐁퐁 솟아나겠다",
