      "utterance": [
        {
          "id": "MDRW2100046486.1.1",
          "form": "되나유?",
          "original_form": "되나유?",
          "speaker_id": "1",
          "time": "20211020 17:27"
        },
        {
          "id": "MDRW2100046486.1.2",
          "form": "낙.낙",
          "original_form": "낙.낙",
          "speaker_id": "1",
          "time": "20211020 17:28"
        },
        {
          "id": "MDRW2100046486.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211020 17:58"
        },
        {
          "id": "MDRW2100046486.1.4",
          "form": "회의하고 와서 답변이 늦었네요!",
          "original_form": "회의하고 와서 답변이 늦었네요!",
          "speaker_id": "2",
          "time": "20211020 17:59"
        },
        {
          "id": "MDRW2100046486.1.5",
          "form": "아 직장인이면 그럴수있죠!",
          "original_form": "아 직장인이면 그럴수있죠!",
          "speaker_id": "1",
          "time": "20211020 17:59"
        },
        {
          "id": "MDRW2100046486.1.6",
          "form": "탄산음료 좋아하세요?",
          "original_form": "탄산음료 좋아하세요?",
          "speaker_id": "2",
          "time": "20211020 17:59"
        },
        {
          "id": "MDRW2100046486.1.7",
          "form": "네 많이좋아합니다",
          "original_form": "네 많이좋아합니다",
          "speaker_id": "1",
          "time": "20211020 17:59"
        },
        {
          "id": "MDRW2100046486.1.8",
          "form": "콜라파신가요?",
          "original_form": "콜라파신가요?",
          "speaker_id": "2",
          "time": "20211020 17:59"
        },
        {
          "id": "MDRW2100046486.1.9",
          "form": "사이다파신가요?",
          "original_form": "사이다파신가요?",
          "speaker_id": "2",
          "time": "20211020 17:59"
        },
        {
          "id": "MDRW2100046486.1.10",
          "form": "사이다파입니다!",
          "original_form": "사이다파입니다!",
          "speaker_id": "1",
          "time": "20211020 17:59"
        },
        {
          "id": "MDRW2100046486.1.11",
          "form": "사이다파시구나 ㅎㅎ",
          "original_form": "사이다파시구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 18:00"
        },
        {
          "id": "MDRW2100046486.1.12",
          "form": "이번에 나온 제로사이다 드셔보셨어요?",
          "original_form": "이번에 나온 제로사이다 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20211020 18:00"
        },
        {
          "id": "MDRW2100046486.1.13",
          "form": "먹어봤는데결국 원래 사이다로 돌아갔습니다 ㅎㅎ",
          "original_form": "먹어봤는데결국 원래 사이다로 돌아갔습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211020 18:01"
        },
        {
          "id": "MDRW2100046486.1.14",
          "form": "역시 원조 못따라가죠 ㅎㅎ",
          "original_form": "역시 원조 못따라가죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 18:01"
        },
        {
          "id": "MDRW2100046486.1.15",
          "form": "전 제로도 맛있더라구요!",
          "original_form": "전 제로도 맛있더라구요!",
          "speaker_id": "2",
          "time": "20211020 18:01"
        },
        {
          "id": "MDRW2100046486.1.16",
          "form": "ㅎㅎ그쵸 뭔가 아싑더라고요",
          "original_form": "ㅎㅎ그쵸 뭔가 아싑더라고요",
          "speaker_id": "1",
          "time": "20211020 18:02"
        },
        {
          "id": "MDRW2100046486.1.17",
          "form": "그래도 몸생각해서 섞어마십니다 ㅎㅎㅎ",
          "original_form": "그래도 몸생각해서 섞어마십니다 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20211020 18:02"
        },
        {
          "id": "MDRW2100046486.1.18",
          "form": "지금도 회의 끝나고 제로 콜라 한사발 마시고 있어요",
          "original_form": "지금도 회의 끝나고 제로 콜라 한사발 마시고 있어요",
          "speaker_id": "2",
          "time": "20211020 18:02"
        },
        {
          "id": "MDRW2100046486.1.19",
          "form": "수요일이라 그런가 너무 피곤해요",
          "original_form": "수요일이라 그런가 너무 피곤해요",
          "speaker_id": "2",
          "time": "20211020 18:02"
        },
        {
          "id": "MDRW2100046486.1.20",
          "form": "ㅋㅋㅋ 한사발씩이나",
          "original_form": "ㅋㅋㅋ 한사발씩이나",
          "speaker_id": "1",
          "time": "20211020 18:03"
        },
        {
          "id": "MDRW2100046486.1.21",
          "form": "회의까지하시면 녹초될만하죠 ㅋㅋㅋ",
          "original_form": "회의까지하시면 녹초될만하죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 18:03"
        },
        {
          "id": "MDRW2100046486.1.22",
          "form": "이번주 시간이 엄청 빨리 가네요",
          "original_form": "이번주 시간이 엄청 빨리 가네요",
          "speaker_id": "2",
          "time": "20211020 18:04"
        },
        {
          "id": "MDRW2100046486.1.23",
          "form": "그쵸 가을이 지나가서 인가 더 시간이 빠르게 느껴지네요",
          "original_form": "그쵸 가을이 지나가서 인가 더 시간이 빠르게 느껴지네요",
          "speaker_id": "1",
          "time": "20211020 18:05"
        }
      ]
