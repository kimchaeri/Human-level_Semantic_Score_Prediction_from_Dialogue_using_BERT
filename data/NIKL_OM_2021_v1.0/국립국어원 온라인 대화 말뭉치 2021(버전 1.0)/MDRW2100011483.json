      "utterance": [
        {
          "id": "MDRW2100011483.1.1",
          "form": "동물 키우시나요?",
          "original_form": "동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011483.1.2",
          "form": "아뇨 .. name1님은 키우시나요?",
          "original_form": "아뇨 .. &name1&님은 키우시나요?",
          "speaker_id": "2",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100011483.1.3",
          "form": "아뇨ㅠㅠ",
          "original_form": "아뇨ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011483.1.4",
          "form": "강아지 좋아하시나요?",
          "original_form": "강아지 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011483.1.5",
          "form": "강아지 좋아해요 ! name1님은요?",
          "original_form": "강아지 좋아해요 ! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011483.1.6",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100011483.1.7",
          "form": "고양이는요?",
          "original_form": "고양이는요?",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100011483.1.8",
          "form": "동물들 다 좋아해요 귀여워요 ..ㅎㅎ",
          "original_form": "동물들 다 좋아해요 귀여워요 ..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100011483.1.9",
          "form": "뱀은요?!",
          "original_form": "뱀은요?!",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100011483.1.10",
          "form": "뱀은 한번도 본적 없는데.. 작은 뱀은 귀엽더라구요",
          "original_form": "뱀은 한번도 본적 없는데.. 작은 뱀은 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210825 00:25"
        },
        {
          "id": "MDRW2100011483.1.11",
          "form": "name1님도 뱀좋아하세요?",
          "original_form": "&name1&님도 뱀좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 00:25"
        },
        {
          "id": "MDRW2100011483.1.12",
          "form": "아니요ㅠㅠ",
          "original_form": "아니요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100011483.1.13",
          "form": "뱀은 너무징그러워요ㅠㅠ",
          "original_form": "뱀은 너무징그러워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100011483.1.14",
          "form": "맞아요 뱀은 호불호가 많이 갈리긴하죠",
          "original_form": "맞아요 뱀은 호불호가 많이 갈리긴하죠",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100011483.1.15",
          "form": "햄스터는 좋아하세요? 예전에 햄스터 키워본적 있어요",
          "original_form": "햄스터는 좋아하세요? 예전에 햄스터 키워본적 있어요",
          "speaker_id": "2",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100011483.1.16",
          "form": "네네 좋아해요!",
          "original_form": "네네 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100011483.1.17",
          "form": "햄스터 너무귀엽지않나요?",
          "original_form": "햄스터 너무귀엽지않나요?",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100011483.1.18",
          "form": "네 너무 귀엽죠ㅠㅠ 보송보송해요",
          "original_form": "네 너무 귀엽죠ㅠㅠ 보송보송해요",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100011483.1.19",
          "form": "맞아요ㅠㅠ 진짜",
          "original_form": "맞아요ㅠㅠ 진짜",
          "speaker_id": "1",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100011483.1.20",
          "form": "전 강아지 키우고싶어요",
          "original_form": "전 강아지 키우고싶어요",
          "speaker_id": "1",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100011483.1.21",
          "form": "맞아요 강아지 너무 귀엽죠 ㅠㅠ 저도 여건만 되면 키우고싶어요!",
          "original_form": "맞아요 강아지 너무 귀엽죠 ㅠㅠ 저도 여건만 되면 키우고싶어요!",
