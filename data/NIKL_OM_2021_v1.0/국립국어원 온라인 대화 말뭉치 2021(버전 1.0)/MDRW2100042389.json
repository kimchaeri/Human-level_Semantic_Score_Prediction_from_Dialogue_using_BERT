      "utterance": [
        {
          "id": "MDRW2100042389.1.1",
          "form": "들어가는 고기가 새고기니",
          "original_form": "들어가는 고기가 새고기니",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042389.1.2",
          "form": "밥이랑 같이 많이 먹어",
          "original_form": "밥이랑 같이 많이 먹어",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042389.1.3",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042389.1.4",
          "form": "아니 다리 4개달린 고기야",
          "original_form": "아니 다리 4개달린 고기야",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042389.1.5",
          "form": "너구리 라면 먹었구나!",
          "original_form": "너구리 라면 먹었구나!",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042389.1.6",
          "form": "참나~ 너구리고기",
          "original_form": "참나~ 너구리고기",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042389.1.7",
          "form": "맛없겠다",
          "original_form": "맛없겠다",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042389.1.8",
          "form": "토끼고기니?",
          "original_form": "토끼고기니?",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042389.1.9",
          "form": "넥슨은 다람쥐를 뿌려라",
          "original_form": "넥슨은 다람쥐를 뿌려라",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042389.1.10",
          "form": "세상에 토끼고기라니",
          "original_form": "세상에 토끼고기라니",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042389.1.11",
          "form": "바람의 나라니",
          "original_form": "바람의 나라니",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042389.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042389.1.13",
          "form": "양고기니?",
          "original_form": "양고기니?",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042389.1.14",
          "form": "아니야!!!!!",
          "original_form": "아니야!!!!!",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042389.1.15",
          "form": "소고기야?",
          "original_form": "소고기야?",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042389.1.16",
          "form": "소고기로 만드는 집도 있을걸",
          "original_form": "소고기로 만드는 집도 있을걸",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042389.1.17",
          "form": "그래? 으으으으음",
          "original_form": "그래? 으으으으음",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042389.1.18",
          "form": "그럼 돼지고기가 주로 쓰이니?",
          "original_form": "그럼 돼지고기가 주로 쓰이니?",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042389.1.19",
          "form": "일반적으로?",
          "original_form": "일반적으로?",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042389.1.20",
          "form": "응 돼지고기가 주로쓰여",
          "original_form": "응 돼지고기가 주로쓰여",
          "speaker_id": "2",
          "time": "20210906 22:00"
