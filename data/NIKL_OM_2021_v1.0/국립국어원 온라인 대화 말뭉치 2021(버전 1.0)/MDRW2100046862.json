        },
        {
          "id": "MDRW2100046862.1.3",
          "form": "혹시 글램핑 좋아하세요?",
          "original_form": "혹시 글램핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20211106 13:32"
        },
        {
          "id": "MDRW2100046862.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211106 14:07"
        },
        {
          "id": "MDRW2100046862.1.5",
          "form": "진짜좋아해요",
          "original_form": "진짜좋아해요",
          "speaker_id": "1",
          "time": "20211106 14:07"
        },
        {
          "id": "MDRW2100046862.1.6",
          "form": "오 헐 ㅋㅋㅋㅋ",
          "original_form": "오 헐 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211106 14:13"
        },
        {
          "id": "MDRW2100046862.1.7",
          "form": "저도 진짜 좋아해요 ㅎㅎ 많이는 못가봤는데!!",
          "original_form": "저도 진짜 좋아해요 ㅎㅎ 많이는 못가봤는데!!",
          "speaker_id": "2",
          "time": "20211106 14:13"
        },
        {
          "id": "MDRW2100046862.1.8",
          "form": "어디가봤어요?",
          "original_form": "어디가봤어요?",
          "speaker_id": "1",
          "time": "20211106 15:07"
        },
        {
          "id": "MDRW2100046862.1.9",
          "form": "저는 가평 쪽만 가봤네요 ㅋㅋㅋ ㅠㅠ",
          "original_form": "저는 가평 쪽만 가봤네요 ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20211106 16:21"
        },
        {
          "id": "MDRW2100046862.1.10",
          "form": "차가 없어서..",
          "original_form": "차가 없어서..",
          "speaker_id": "2",
          "time": "20211106 16:21"
        },
        {
          "id": "MDRW2100046862.1.11",
          "form": "다양한 곳을 못가봤어요 흑",
          "original_form": "다양한 곳을 못가봤어요 흑",
          "speaker_id": "2",
          "time": "20211106 16:21"
        },
        {
          "id": "MDRW2100046862.1.12",
          "form": "재밌겠어요ㅋㅋ",
          "original_form": "재밌겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211106 16:22"
        },
        {
          "id": "MDRW2100046862.1.13",
          "form": "네네 ㅎㅎ 먹고 불멍하는게 다긴 하지만",
          "original_form": "네네 ㅎㅎ 먹고 불멍하는게 다긴 하지만",
          "speaker_id": "2",
          "time": "20211106 16:23"
        },
        {
          "id": "MDRW2100046862.1.14",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211106 16:23"
        },
        {
          "id": "MDRW2100046862.1.15",
          "form": "먹는게최고죠ㅎ",
          "original_form": "먹는게최고죠ㅎ",
          "speaker_id": "1",
          "time": "20211106 16:23"
        },
        {
          "id": "MDRW2100046862.1.16",
          "form": "글램핑 명소 아시나요??",
          "original_form": "글램핑 명소 아시나요??",
          "speaker_id": "2",
          "time": "20211106 16:23"
        },
        {
          "id": "MDRW2100046862.1.17",
          "form": "ㅎㅎ 맞아요 글램핑은 먹으러 가는 맛",
          "original_form": "ㅎㅎ 맞아요 글램핑은 먹으러 가는 맛",
          "speaker_id": "2",
          "time": "20211106 16:24"
        },
        {
          "id": "MDRW2100046862.1.18",
          "form": "안가봤어요ㅋㅋ",
          "original_form": "안가봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211106 16:24"
        },
        {
          "id": "MDRW2100046862.1.19",
          "form": "아아 좋아하신다길래",
          "original_form": "아아 좋아하신다길래",
          "speaker_id": "2",
          "time": "20211106 16:24"
        },
        {
          "id": "MDRW2100046862.1.20",
          "form": "가보신줄 ㅎㅎ ㅋㅋㅋ",
          "original_form": "가보신줄 ㅎㅎ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211106 16:24"
        },
        {
          "id": "MDRW2100046862.1.21",
          "form": "추천받고가보려구요ㅋㅋ",
          "original_form": "추천받고가보려구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211106 16:24"
        },
        {
          "id": "MDRW2100046862.1.22",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20211106 16:25"
        },
        {
          "id": "MDRW2100046862.1.23",
          "form": "일단 처음은 가평이나 남양주로 가보세요!!",
          "original_form": "일단 처음은 가평이나 남양주로 가보세요!!",
          "speaker_id": "2",
          "time": "20211106 16:25"
        },
        {
          "id": "MDRW2100046862.1.24",
          "form": "마트에서 일정 금액 구매하면",
          "original_form": "마트에서 일정 금액 구매하면",
          "speaker_id": "2",
          "time": "20211106 16:25"
        },
        {
          "id": "MDRW2100046862.1.25",
          "form": "네 추천감사해요",
          "original_form": "네 추천감사해요",
          "speaker_id": "1",
          "time": "20211106 16:25"
        },
        {
          "id": "MDRW2100046862.1.26",
          "form": "네네",
