      "utterance": [
        {
          "id": "MDRW2100039696.1.1",
          "form": "한자배우러갔어요?ㅋ",
          "original_form": "한자배우러갔어요?ㅋ",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100039696.1.2",
          "form": "오늘하루종일 바빴다ㅜ",
          "original_form": "오늘하루종일 바빴다ㅜ",
          "speaker_id": "2",
          "time": "20210904 21:58"
        },
        {
          "id": "MDRW2100039696.1.3",
          "form": "ㅋㅋ언니 뭐한다고 바빴어요?ㅋ",
          "original_form": "ㅋㅋ언니 뭐한다고 바빴어요?ㅋ",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100039696.1.4",
          "form": "오늘 이것저것ㅋ",
          "original_form": "오늘 이것저것ㅋ",
          "speaker_id": "2",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100039696.1.5",
          "form": "이제설거지다함ㅋ",
          "original_form": "이제설거지다함ㅋ",
          "speaker_id": "2",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100039696.1.6",
          "form": "아고 언니 고생했네유",
          "original_form": "아고 언니 고생했네유",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100039696.1.7",
          "form": "지금 설거지를하다니ㅜㅋ",
          "original_form": "지금 설거지를하다니ㅜㅋ",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100039696.1.8",
          "form": "ㅋㅇㅋ",
          "original_form": "ㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100039696.1.9",
          "form": "내일은 뭐하노?",
          "original_form": "내일은 뭐하노?",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100039696.1.10",
          "form": "오늘 신랑벌초다녀오고 혼자애들봤거든요",
          "original_form": "오늘 신랑벌초다녀오고 혼자애들봤거든요",
          "speaker_id": "1",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100039696.1.11",
          "form": "내일은 또 집에있다가 점심이나 저녁이나 시댁에밥먹고오려고요. 친정식구들은 바쁨.ㅋ",
          "original_form": "내일은 또 집에있다가 점심이나 저녁이나 시댁에밥먹고오려고요. 친정식구들은 바쁨.ㅋ",
          "speaker_id": "1",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100039696.1.12",
          "form": "언니는 뭐해요?",
          "original_form": "언니는 뭐해요?",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100039696.1.13",
          "form": "난 내일은 별일읍다 신랑ㅈ이 이마트가자해서",
          "original_form": "난 내일은 별일읍다 신랑ㅈ이 이마트가자해서",
          "speaker_id": "2",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100039696.1.14",
          "form": "가서 식물하나 사올라고",
          "original_form": "가서 식물하나 사올라고",
          "speaker_id": "2",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100039696.1.15",
          "form": "요즘 뭘자꾸 키우고싶어하는지ㅡㅡ",
          "original_form": "요즘 뭘자꾸 키우고싶어하는지ㅡㅡ",
          "speaker_id": "2",
          "time": "20210905 00:10"
        },
        {
          "id": "MDRW2100039696.1.16",
          "form": "아아ㅋ식물뭐요?ㅋ",
          "original_form": "아아ㅋ식물뭐요?ㅋ",
          "speaker_id": "1",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100039696.1.17",
          "form": "저는 식물은 못키워서ㅜㅋ",
          "original_form": "저는 식물은 못키워서ㅜㅋ",
          "speaker_id": "1",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100039696.1.18",
          "form": "돈나무인가?ㅋㅋ",
          "original_form": "돈나무인가?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100039696.1.19",
          "form": "일단 내일가서고르는걸로ㅋ",
          "original_form": "일단 내일가서고르는걸로ㅋ",
          "speaker_id": "2",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100039696.1.20",
          "form": "아아 돈나무 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ키우기 쉽잖아요 ㅋㅋ",
          "original_form": "아아 돈나무 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ키우기 쉽잖아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:50"
        },
        {
          "id": "MDRW2100039696.1.21",
          "form": "니 진짜늦게자네ㅋㅋ",
          "original_form": "니 진짜늦게자네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100039696.1.22",
          "form": "우린 이제준비끝",
          "original_form": "우린 이제준비끝",
          "speaker_id": "2",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100039696.1.23",
          "form": "나간다ㅋㅋ",
          "original_form": "나간다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100039696.1.24",
          "form": "어제는 지오랑 같이 낮잠자서ㅋㅋㄱㅋ잠이 안오더라구요",
          "original_form": "어제는 지오랑 같이 낮잠자서ㅋㅋㄱㅋ잠이 안오더라구요",
