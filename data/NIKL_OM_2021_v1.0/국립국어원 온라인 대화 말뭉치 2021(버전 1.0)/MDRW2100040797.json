      "utterance": [
        {
          "id": "MDRW2100040797.1.1",
          "form": "저는 주말에 한끼는 집밥 챙겨먹으려고 노력해요",
          "original_form": "저는 주말에 한끼는 집밥 챙겨먹으려고 노력해요",
          "speaker_id": "2",
          "time": "20210905 07:29"
        },
        {
          "id": "MDRW2100040797.1.2",
          "form": "그쵸~ 요샌 너무시켜먹다보니",
          "original_form": "그쵸~ 요샌 너무시켜먹다보니",
          "speaker_id": "1",
          "time": "20210905 07:29"
        },
        {
          "id": "MDRW2100040797.1.3",
          "form": "집밥이 생각나요ㅠ",
          "original_form": "집밥이 생각나요ㅠ",
          "speaker_id": "1",
          "time": "20210905 07:30"
        },
        {
          "id": "MDRW2100040797.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210905 07:30"
        },
        {
          "id": "MDRW2100040797.1.5",
          "form": "커피좋아하시나요~?",
          "original_form": "커피좋아하시나요~?",
          "speaker_id": "1",
          "time": "20210905 07:30"
        },
        {
          "id": "MDRW2100040797.1.6",
          "form": "제가 카페인에 약해서 원래 커피 잘 안마셨는데",
          "original_form": "제가 카페인에 약해서 원래 커피 잘 안마셨는데",
          "speaker_id": "2",
          "time": "20210905 07:30"
        },
        {
          "id": "MDRW2100040797.1.7",
          "form": "요즘은 2-3일에 한번 정도 마셔요",
          "original_form": "요즘은 2-3일에 한번 정도 마셔요",
          "speaker_id": "2",
          "time": "20210905 07:31"
        },
        {
          "id": "MDRW2100040797.1.8",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 07:32"
        },
        {
          "id": "MDRW2100040797.1.9",
          "form": "네~ 전매일꼭마셔요ㅠㅠ",
          "original_form": "네~ 전매일꼭마셔요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 07:33"
        },
        {
          "id": "MDRW2100040797.1.10",
          "form": "오 그러시구나",
          "original_form": "오 그러시구나",
          "speaker_id": "2",
          "time": "20210905 07:33"
        },
        {
          "id": "MDRW2100040797.1.11",
          "form": "어떤 커피 좋아하세요?",
          "original_form": "어떤 커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 07:33"
        },
        {
          "id": "MDRW2100040797.1.12",
          "form": "전 아메파에여!!",
          "original_form": "전 아메파에여!!",
          "speaker_id": "1",
          "time": "20210905 07:33"
        },
        {
          "id": "MDRW2100040797.1.13",
          "form": "오 산미 있는 것 좋아하세요?",
          "original_form": "오 산미 있는 것 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 07:34"
        },
        {
          "id": "MDRW2100040797.1.14",
          "form": "잘모르지만",
          "original_form": "잘모르지만",
          "speaker_id": "1",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100040797.1.15",
          "form": "거의 가리지않고 다좋아해요 ㅎ",
          "original_form": "거의 가리지않고 다좋아해요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100040797.1.16",
          "form": "ㅎㅎ커피 박애주의자시네요",
          "original_form": "ㅎㅎ커피 박애주의자시네요",
          "speaker_id": "2",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100040797.1.17",
          "form": "그닥 브랜드도 안가려서 ㅎㅎ",
          "original_form": "그닥 브랜드도 안가려서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100040797.1.18",
          "form": "커피인 게 중요하군요",
          "original_form": "커피인 게 중요하군요",
          "speaker_id": "2",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100040797.1.19",
          "form": "맞아요 ㅋㅋ 뭔갸 저를 깨워주는",
          "original_form": "맞아요 ㅋㅋ 뭔갸 저를 깨워주는",
