      "utterance": [
        {
          "id": "MDRW2100010937.1.1",
          "form": "ㅎㅎ 전 묵은지고등어조림 먹고와서",
          "original_form": "ㅎㅎ 전 묵은지고등어조림 먹고와서",
          "speaker_id": "2",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010937.1.2",
          "form": "요기요 나이트쿠폰을 줬던데 넘늦게보는바람에 파바빵을 못샀네요",
          "original_form": "요기요 나이트쿠폰을 줬던데 넘늦게보는바람에 파바빵을 못샀네요",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010937.1.3",
          "form": "오늘만 쓸 수 있나요?ㅎㅎ",
          "original_form": "오늘만 쓸 수 있나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010937.1.4",
          "form": "27일까지 계속주나봐요",
          "original_form": "27일까지 계속주나봐요",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010937.1.5",
          "form": "이번주 금욜까지  5천원할인이라 만원어치사고 3천결제가능요ㅎ",
          "original_form": "이번주 금욜까지  5천원할인이라 만원어치사고 3천결제가능요ㅎ",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010937.1.6",
          "form": "샐러드나 빵사가서 드세요ㅋ요기요 아뒤있으심",
          "original_form": "샐러드나 빵사가서 드세요ㅋ요기요 아뒤있으심",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010937.1.7",
          "form": "허 근데 집 앞 빠바가 공사중이라ㅠ",
          "original_form": "허 근데 집 앞 빠바가 공사중이라ㅠ",
          "speaker_id": "1",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010937.1.8",
          "form": "앗ㅎ 안타깝네요",
          "original_form": "앗ㅎ 안타깝네요",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010937.1.9",
          "form": "일터 근처를 찾아봐야겠어요",
          "original_form": "일터 근처를 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010937.1.10",
          "form": "포장하셔야돼요",
          "original_form": "포장하셔야돼요",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010937.1.11",
          "form": "빵좋아하세요?",
          "original_form": "빵좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010937.1.12",
          "form": "네ㅋㅋ 피자빵 좋아해요",
          "original_form": "네ㅋㅋ 피자빵 좋아해요",
          "speaker_id": "1",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100010937.1.13",
          "form": "피자빵 맛있죠ㅎ",
          "original_form": "피자빵 맛있죠ㅎ",
          "speaker_id": "2",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100010937.1.14",
          "form": "낼 출근늦게하시면 9시 쿠폰주니 만원어치 사서 드세요ㅎㅎ",
          "original_form": "낼 출근늦게하시면 9시 쿠폰주니 만원어치 사서 드세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100010937.1.15",
          "form": "전 마늘빵이나 크림치즈들어간게 맛있는듯",
          "original_form": "전 마늘빵이나 크림치즈들어간게 맛있는듯",
          "speaker_id": "2",
          "time": "20210824 00:48"
        },
        {
          "id": "MDRW2100010937.1.16",
          "form": "네ㅋㅋ 좋은 정보 감사해요",
          "original_form": "네ㅋㅋ 좋은 정보 감사해요",
          "speaker_id": "1",
          "time": "20210824 00:49"
        },
        {
          "id": "MDRW2100010937.1.17",
          "form": "마늘빵 맛있죠!",
          "original_form": "마늘빵 맛있죠!",
          "speaker_id": "1",
          "time": "20210824 00:49"
        },
        {
          "id": "MDRW2100010937.1.18",
          "form": "마늘빵은 뚜레쥬르가 맛있는거같아요",
          "original_form": "마늘빵은 뚜레쥬르가 맛있는거같아요",
          "speaker_id": "2",
          "time": "20210824 00:50"
        },
        {
          "id": "MDRW2100010937.1.19",
          "form": "ㅎㅎ 참고할께요!",
          "original_form": "ㅎㅎ 참고할께요!",
          "speaker_id": "1",
          "time": "20210824 00:51"
        },
        {
          "id": "MDRW2100010937.1.20",
          "form": "늦었는데 언제 주무세요?",
          "original_form": "늦었는데 언제 주무세요?",
          "speaker_id": "1",
          "time": "20210824 00:51"
        },
        {
          "id": "MDRW2100010937.1.21",
          "form": "두시넘어서요ㅋ",
          "original_form": "두시넘어서요ㅋ",
          "speaker_id": "2",
          "time": "20210824 00:51"
        }
      ]
