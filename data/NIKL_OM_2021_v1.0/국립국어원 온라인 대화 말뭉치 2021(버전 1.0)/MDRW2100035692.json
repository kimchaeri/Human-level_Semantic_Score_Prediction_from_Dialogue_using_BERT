      "utterance": [
        {
          "id": "MDRW2100035692.1.1",
          "form": "지금은 엄마랑 살고있는데요",
          "original_form": "지금은 엄마랑 살고있는데요",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035692.1.2",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035692.1.3",
          "form": "슬슬 나가고싶을때도 있고 아닐때도 있고 그렇습니다ㅋㅋ",
          "original_form": "슬슬 나가고싶을때도 있고 아닐때도 있고 그렇습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035692.1.4",
          "form": "저도 그래요ㅋㅋㅋㅋ",
          "original_form": "저도 그래요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035692.1.5",
          "form": "지금이 불만족스럽거나",
          "original_form": "지금이 불만족스럽거나",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035692.1.6",
          "form": "왜냐면 자취하면 진짲필요한 물건이 너무많아요",
          "original_form": "왜냐면 자취하면 진짲필요한 물건이 너무많아요",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035692.1.7",
          "form": "빨리 독립하고싶은건 아니지만ㅎㅎ",
          "original_form": "빨리 독립하고싶은건 아니지만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035692.1.8",
          "form": "이번에",
          "original_form": "이번에",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035692.1.9",
          "form": "오빠가 혼자 독립을 했는데",
          "original_form": "오빠가 혼자 독립을 했는데",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035692.1.10",
          "form": "집에 있는 살림살이를 거덜냈어요",
          "original_form": "집에 있는 살림살이를 거덜냈어요",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035692.1.11",
          "form": "ㅋㅋ그렇게됩니다",
          "original_form": "ㅋㅋ그렇게됩니다",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035692.1.12",
          "form": "그냥 내한몸 빠져나가는게 아니고",
          "original_form": "그냥 내한몸 빠져나가는게 아니고",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035692.1.13",
          "form": "후라이팬도 몇개를 스틸해가지는지ㅋㅋㅋㅋ",
          "original_form": "후라이팬도 몇개를 스틸해가지는지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035692.1.14",
          "form": "집 한채가 통째로 나간다니까요",
          "original_form": "집 한채가 통째로 나간다니까요",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035692.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035692.1.16",
          "form": "냉장고 세탁기 다 사야하고",
          "original_form": "냉장고 세탁기 다 사야하고",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035692.1.17",
          "form": "아휴 돈 많이 들더라구요",
          "original_form": "아휴 돈 많이 들더라구요",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035692.1.18",
          "form": "그져",
          "original_form": "그져",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035692.1.19",
          "form": "휴지나",
          "original_form": "휴지나",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035692.1.20",
          "form": "네네 치약이 이렇게 비쌌는지 요즘에서야 깨달았대요ㅋㅋㅋㅋ",
          "original_form": "네네 치약이 이렇게 비쌌는지 요즘에서야 깨달았대요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035692.1.21",
          "form": "칫솔이랑ㅋㅋ",
          "original_form": "칫솔이랑ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035692.1.22",
          "form": "바닥닦는 수건하나마저도 사야합니다ㅋㅋ",
          "original_form": "바닥닦는 수건하나마저도 사야합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035692.1.23",
          "form": "집안일도 하게되고",
          "original_form": "집안일도 하게되고",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035692.1.24",
          "form": "모든걸 혼자해야하니까ㅠ",
          "original_form": "모든걸 혼자해야하니까ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:30"
        }
      ]
    }
