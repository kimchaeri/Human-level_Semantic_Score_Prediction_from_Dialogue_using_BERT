        {
          "id": "MDRW2100046191.1.1",
          "form": "똥은 못가리나요?",
          "original_form": "똥은 못가리나요?",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046191.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046191.1.3",
          "form": "그럼 어떻게 해요?",
          "original_form": "그럼 어떻게 해요?",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046191.1.4",
          "form": "똥치우기가 힘드시겠네요",
          "original_form": "똥치우기가 힘드시겠네요",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046191.1.5",
          "form": "첨엔 어떡할까싶었는데",
          "original_form": "첨엔 어떡할까싶었는데",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046191.1.6",
          "form": "다행히 똥은 냄새가 안나요",
          "original_form": "다행히 똥은 냄새가 안나요",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046191.1.7",
          "form": "오 정말 좋네요",
          "original_form": "오 정말 좋네요",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046191.1.8",
          "form": "네 그래서 똥같지도않아요",
          "original_form": "네 그래서 똥같지도않아요",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046191.1.9",
          "form": "오줌에선 냄새나는건가요",
          "original_form": "오줌에선 냄새나는건가요",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046191.1.10",
          "form": "네 오줌은 냄새나요",
          "original_form": "네 오줌은 냄새나요",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046191.1.11",
          "form": "그래도 편하시겠네요",
          "original_form": "그래도 편하시겠네요",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046191.1.12",
          "form": "네 화장실을 가리니까",
          "original_form": "네 화장실을 가리니까",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046191.1.13",
          "form": "시끄럽진않나요?",
          "original_form": "시끄럽진않나요?",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046191.1.14",
          "form": "울고 그러는건 아니어서 시끄럽진않아요",
          "original_form": "울고 그러는건 아니어서 시끄럽진않아요",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046191.1.15",
          "form": "키우기 편하시겠네요",
          "original_form": "키우기 편하시겠네요",
