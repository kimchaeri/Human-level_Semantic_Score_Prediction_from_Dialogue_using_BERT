      "utterance": [
        {
          "id": "MDRW2100018133.1.1",
          "form": "수명이 얼마나 되요?",
          "original_form": "수명이 얼마나 되요?",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018133.1.2",
          "form": "평균십년이상요",
          "original_form": "평균십년이상요",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018133.1.3",
          "form": "참고로 울아가는 열두살이요",
          "original_form": "참고로 울아가는 열두살이요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018133.1.4",
          "form": "아고... 할모니네요..",
          "original_form": "아고... 할모니네요..",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018133.1.5",
          "form": "강아지랑 비슷하네요..",
          "original_form": "강아지랑 비슷하네요..",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018133.1.6",
          "form": "할아부지예요",
          "original_form": "할아부지예요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018133.1.7",
          "form": "아픈데는 없으시구유?",
          "original_form": "아픈데는 없으시구유?",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018133.1.8",
          "form": "흰머리도 많으시쥬? ㅋㅋㅋㅋ",
          "original_form": "흰머리도 많으시쥬? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018133.1.9",
          "form": "원래 하애서...ㅋㅋ",
          "original_form": "원래 하애서...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018133.1.10",
          "form": "다행히 건강해요",
          "original_form": "다행히 건강해요",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018133.1.11",
          "form": "밥은 하루에 몇번 먹어요?",
          "original_form": "밥은 하루에 몇번 먹어요?",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018133.1.12",
          "form": "건초는 계속 떨어지지않게 채워주고",
          "original_form": "건초는 계속 떨어지지않게 채워주고",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018133.1.13",
          "form": "사료는 한국자 정도 줘요",
          "original_form": "사료는 한국자 정도 줘요",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018133.1.14",
          "form": "아.. 하루종일 먹는거에요? \n저도 너무 키우고 싶네요.. \n토깽이",
          "original_form": "아.. 하루종일 먹는거에요? \n저도 너무 키우고 싶네요.. \n토깽이",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018133.1.15",
          "form": "대부분 가정분양인가유?",
          "original_form": "대부분 가정분양인가유?",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018133.1.16",
          "form": "가정분양도 있고 농장도 있구요",
          "original_form": "가정분양도 있고 농장도 있구요",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018133.1.17",
          "form": "유기토 입양도하구요",
          "original_form": "유기토 입양도하구요",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018133.1.18",
          "form": "유기토도 있그낭... 첨들어보네..",
          "original_form": "유기토도 있그낭... 첨들어보네..",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018133.1.19",
          "form": "키우다가 털날리고 병원비도 많이드니까 유기하시분들 많터라구요",
          "original_form": "키우다가 털날리고 병원비도 많이드니까 유기하시분들 많터라구요",
          "speaker_id": "2",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018133.1.20",
          "form": "모든 애완동물 다 그런듯.. \n무책임행....",
          "original_form": "모든 애완동물 다 그런듯.. \n무책임행....",
          "speaker_id": "1",
          "time": "20210827 10:19"
        }
      ]
