        },
        {
          "id": "MDRW2100024270.1.2",
          "form": "유튜브 요즘은 잘 안봐?",
          "original_form": "유튜브 요즘은 잘 안봐?",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024270.1.3",
          "form": "축구 컨텐츠를 많이 봐",
          "original_form": "축구 컨텐츠를 많이 봐",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024270.1.4",
          "form": "축구는 봐도봐도 볼 게 그렇게 많아?",
          "original_form": "축구는 봐도봐도 볼 게 그렇게 많아?",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024270.1.5",
          "form": "해외소식이 많더라고",
          "original_form": "해외소식이 많더라고",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024270.1.6",
          "form": "해축은 비시즌이 언제야?",
          "original_form": "해축은 비시즌이 언제야?",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024270.1.7",
          "form": "5월부터 7월까지",
          "original_form": "5월부터 7월까지",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024270.1.8",
          "form": "되게 짧게 쉬는 거 같네",
          "original_form": "되게 짧게 쉬는 거 같네",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024270.1.9",
          "form": "응 한3개월?",
          "original_form": "응 한3개월?",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024270.1.10",
          "form": "우리나라도 그 정도 쉬어?",
          "original_form": "우리나라도 그 정도 쉬어?",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024270.1.11",
          "form": "우리나라도 비슷할걸?",
          "original_form": "우리나라도 비슷할걸?",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024270.1.12",
          "form": "겨울엔 내내 쉴 거 아냐",
