        },
        {
          "id": "MDRW2100043766.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043766.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043766.1.4",
          "form": "오오 딱복이신가요 물복이신가요",
          "original_form": "오오 딱복이신가요 물복이신가요",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043766.1.5",
          "form": "딱복이요!!",
          "original_form": "딱복이요!!",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043766.1.6",
          "form": "꺅 사실 전 둘다입니다",
          "original_form": "꺅 사실 전 둘다입니다",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043766.1.7",
          "form": "와사삭한 딱복",
          "original_form": "와사삭한 딱복",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043766.1.8",
          "form": "아하 ㅎㅎ 둘다 좋긴하죠",
          "original_form": "아하 ㅎㅎ 둘다 좋긴하죠",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043766.1.9",
          "form": "굳이 선택하자면 딱복!!",
          "original_form": "굳이 선택하자면 딱복!!",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043766.1.10",
          "form": "입니다 ㅎㅎ",
          "original_form": "입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043766.1.11",
          "form": "ㅎㅎㅎㅎ 이번에 복숭아에 요거트 넣어먹는게 유행같더라구요",
          "original_form": "ㅎㅎㅎㅎ 이번에 복숭아에 요거트 넣어먹는게 유행같더라구요",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043766.1.12",
          "form": "아 맞아요 저고 봤어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아요 저고 봤어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043766.1.13",
          "form": "그게 맛나나...그시간에 복숭아를 하나 더 목는게 낫지않나 했는데",
          "original_form": "그게 맛나나...그시간에 복숭아를 하나 더 목는게 낫지않나 했는데",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043766.1.14",
          "form": "진짜 맛있대욬ㅋㅋㅋㅋ",
          "original_form": "진짜 맛있대욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043766.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래요????",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래요????",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043766.1.16",
          "form": "그래도 전 복숭아만 먹을거같긴한데",
          "original_form": "그래도 전 복숭아만 먹을거같긴한데",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043766.1.17",
          "form": "먹러볼 기회가 있었으면 좋겠네요",
          "original_form": "먹러볼 기회가 있었으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043766.1.18",
          "form": "다른 과일은 어떤거 좋아하시나요?",
          "original_form": "다른 과일은 어떤거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043766.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ 제가 만들긴 귀찮습니다 ^^",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 제가 만들긴 귀찮습니다 ^^",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043766.1.20",
          "form": "저 그 다음은 딸기...?",
          "original_form": "저 그 다음은 딸기...?",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043766.1.21",
          "form": "겨울되면 귤도 좋아요",
          "original_form": "겨울되면 귤도 좋아요",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043766.1.22",
          "form": "사실 웬만한 과일 다좋아해욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "사실 웬만한 과일 다좋아해욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043766.1.23",
          "form": "ㅋㅋㅋ오홍 그중 최애가 복숭아?",
          "original_form": "ㅋㅋㅋ오홍 그중 최애가 복숭아?",
