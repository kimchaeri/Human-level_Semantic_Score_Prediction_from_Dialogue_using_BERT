      "utterance": [
        {
          "id": "MDRW2100001069.1.1",
          "form": "바디프로필 알아요??",
          "original_form": "바디프로필 알아요??",
          "speaker_id": "1",
          "time": "20210722 23:21"
        },
        {
          "id": "MDRW2100001069.1.2",
          "form": "알죠! 자신의 몸 변화를 찍는거잖아요",
          "original_form": "알죠! 자신의 몸 변화를 찍는거잖아요",
          "speaker_id": "2",
          "time": "20210722 23:21"
        },
        {
          "id": "MDRW2100001069.1.3",
          "form": "저도 나중에 바디프로필 한번 찍어보고싶어요",
          "original_form": "저도 나중에 바디프로필 한번 찍어보고싶어요",
          "speaker_id": "2",
          "time": "20210722 23:22"
        },
        {
          "id": "MDRW2100001069.1.4",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210722 23:22"
        },
        {
          "id": "MDRW2100001069.1.5",
          "form": "근데 그거 악용될수도 있대요!!",
          "original_form": "근데 그거 악용될수도 있대요!!",
          "speaker_id": "1",
          "time": "20210722 23:22"
        },
        {
          "id": "MDRW2100001069.1.6",
          "form": "헉 그래요?",
          "original_form": "헉 그래요?",
          "speaker_id": "2",
          "time": "20210722 23:22"
        },
        {
          "id": "MDRW2100001069.1.7",
          "form": "저도 페북에서 본 거긴 한데...",
          "original_form": "저도 페북에서 본 거긴 한데...",
          "speaker_id": "1",
          "time": "20210722 23:23"
        },
        {
          "id": "MDRW2100001069.1.8",
          "form": "여자 분들 바디프로필을 성적으로...",
          "original_form": "여자 분들 바디프로필을 성적으로...",
          "speaker_id": "1",
          "time": "20210722 23:23"
        },
        {
          "id": "MDRW2100001069.1.9",
          "form": "인터넷에 떠돌아다니지 않게 저만 간직하는걸로 해야겠네요",
          "original_form": "인터넷에 떠돌아다니지 않게 저만 간직하는걸로 해야겠네요",
          "speaker_id": "2",
          "time": "20210722 23:23"
        },
        {
          "id": "MDRW2100001069.1.10",
          "form": "악용한대요!!",
          "original_form": "악용한대요!!",
          "speaker_id": "1",
          "time": "20210722 23:23"
        },
        {
          "id": "MDRW2100001069.1.11",
          "form": "그래야 하는게 안전할거에요!!",
          "original_form": "그래야 하는게 안전할거에요!!",
          "speaker_id": "1",
          "time": "20210722 23:23"
        },
        {
          "id": "MDRW2100001069.1.12",
          "form": "그래도 바디프로필로 자신의 변화를 알 수 있으니 그 점은 좋은 것 같아요",
          "original_form": "그래도 바디프로필로 자신의 변화를 알 수 있으니 그 점은 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210722 23:24"
        },
        {
          "id": "MDRW2100001069.1.13",
          "form": "딱 바디프로필을 찍겠다는 목표의식이 생기니까 몸을 더 건강하게 만들 수도 있고ㅎㅎ",
          "original_form": "딱 바디프로필을 찍겠다는 목표의식이 생기니까 몸을 더 건강하게 만들 수도 있고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210722 23:24"
        },
        {
          "id": "MDRW2100001069.1.14",
          "form": "오오오옹",
          "original_form": "오오오옹",
          "speaker_id": "1",
          "time": "20210722 23:24"
        },
        {
          "id": "MDRW2100001069.1.15",
          "form": "전 예전에 연예인들이나 트레이너들만 찍는건줄 알았는데",
          "original_form": "전 예전에 연예인들이나 트레이너들만 찍는건줄 알았는데",
          "speaker_id": "2",
          "time": "20210722 23:25"
        },
        {
          "id": "MDRW2100001069.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210722 23:26"
        },
        {
          "id": "MDRW2100001069.1.17",
          "form": "일반인들도 잘 찍을 수 있게 해놓은 곳들이 많아서 사람들이 최근에 더 바디 프로필을 많이 찍는 것 같아요",
          "original_form": "일반인들도 잘 찍을 수 있게 해놓은 곳들이 많아서 사람들이 최근에 더 바디 프로필을 많이 찍는 것 같아요",
          "speaker_id": "2",
          "time": "20210722 23:26"
        },
        {
          "id": "MDRW2100001069.1.18",
          "form": "요즈은 학생들도 많이찍잖아요",
          "original_form": "요즈은 학생들도 많이찍잖아요",
          "speaker_id": "1",
          "time": "20210722 23:26"
        },
        {
          "id": "MDRW2100001069.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210722 23:26"
        },
        {
          "id": "MDRW2100001069.1.20",
          "form": "혹시..",
          "original_form": "혹시..",
          "speaker_id": "1",
          "time": "20210722 23:26"
        },
        {
          "id": "MDRW2100001069.1.21",
          "form": "개인적으로 연락하고싶은데..",
          "original_form": "개인적으로 연락하고싶은데..",
          "speaker_id": "1",
          "time": "20210722 23:27"
        },
        {
          "id": "MDRW2100001069.1.22",
          "form": "그건 안될 것 같아요 ",
          "original_form": "그건 안될 것 같아요 ",
          "speaker_id": "2",
          "time": "20210722 23:27"
        },
        {
          "id": "MDRW2100001069.1.23",
          "form": "아.... 넵..",
          "original_form": "아.... 넵..",
