      "utterance": [
        {
          "id": "MDRW2100046278.1.1",
          "form": "안녕하십니까",
          "original_form": "안녕하십니까",
          "speaker_id": "1",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100046278.1.2",
          "form": "안녕하세요 점심 드셨나요??",
          "original_form": "안녕하세요 점심 드셨나요??",
          "speaker_id": "2",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100046278.1.3",
          "form": "1시에 집가서 먹으려고요ㅎ",
          "original_form": "1시에 집가서 먹으려고요ㅎ",
          "speaker_id": "1",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100046278.1.4",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "1",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100046278.1.5",
          "form": "메뉴추천 부탁드립니다",
          "original_form": "메뉴추천 부탁드립니다",
          "speaker_id": "1",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100046278.1.6",
          "form": "저는 짬뽕을 먹었는데 집에서 직접 차려드시나요??",
          "original_form": "저는 짬뽕을 먹었는데 집에서 직접 차려드시나요??",
          "speaker_id": "2",
          "time": "20211016 00:31"
        },
        {
          "id": "MDRW2100046278.1.7",
          "form": "가급적이면 차려먹습니다 간단하게요",
          "original_form": "가급적이면 차려먹습니다 간단하게요",
          "speaker_id": "1",
          "time": "20211016 00:31"
        },
        {
          "id": "MDRW2100046278.1.8",
          "form": "짬뽕 맛있겠네요ㅠ",
          "original_form": "짬뽕 맛있겠네요ㅠ",
          "speaker_id": "1",
          "time": "20211016 00:31"
        },
        {
          "id": "MDRW2100046278.1.9",
          "form": "그럼 김치볶음밥은 어떠신가요??",
          "original_form": "그럼 김치볶음밥은 어떠신가요??",
          "speaker_id": "2",
          "time": "20211016 00:31"
        },
        {
          "id": "MDRW2100046278.1.10",
          "form": "좋죠. 오랜만에 김치볶음밥 먹어야겠어요",
          "original_form": "좋죠. 오랜만에 김치볶음밥 먹어야겠어요",
          "speaker_id": "1",
          "time": "20211016 00:32"
        },
        {
          "id": "MDRW2100046278.1.11",
          "form": "금요일이라 뭘 하든 성공적인 이 기분",
          "original_form": "금요일이라 뭘 하든 성공적인 이 기분",
          "speaker_id": "1",
          "time": "20211016 00:32"
        },
        {
          "id": "MDRW2100046278.1.12",
          "form": "이번 주말에는 날씨가 좋길..",
          "original_form": "이번 주말에는 날씨가 좋길..",
          "speaker_id": "1",
          "time": "20211016 00:32"
        },
        {
          "id": "MDRW2100046278.1.13",
          "form": "이번 주말에 기온이 뚝 떨어진다던데..",
          "original_form": "이번 주말에 기온이 뚝 떨어진다던데..",
          "speaker_id": "2",
          "time": "20211016 00:33"
        },
        {
          "id": "MDRW2100046278.1.14",
          "form": "외출하신다면 따듯하게 입고 나가셔야할것같아요ㅠㅠ",
          "original_form": "외출하신다면 따듯하게 입고 나가셔야할것같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20211016 00:33"
        },
        {
          "id": "MDRW2100046278.1.15",
          "form": "덥지만 않으면 좋겠어요, 특히 비는 더욱..",
          "original_form": "덥지만 않으면 좋겠어요, 특히 비는 더욱..",
          "speaker_id": "1",
          "time": "20211016 00:33"
        },
        {
          "id": "MDRW2100046278.1.16",
          "form": "여기는 지방이라 아직 반팔입고 다니시네요 다들 ㅋㅋ",
          "original_form": "여기는 지방이라 아직 반팔입고 다니시네요 다들 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211016 00:33"
        },
        {
          "id": "MDRW2100046278.1.17",
          "form": "미리 겨울 옷 사놔야 할 듯요",
          "original_form": "미리 겨울 옷 사놔야 할 듯요",
          "speaker_id": "1",
          "time": "20211016 00:34"
        },
        {
          "id": "MDRW2100046278.1.18",
          "form": "헉 저는 오늘 가디건입고 나왔어요!",
          "original_form": "헉 저는 오늘 가디건입고 나왔어요!",
          "speaker_id": "2",
          "time": "20211016 00:34"
        },
        {
          "id": "MDRW2100046278.1.19",
          "form": "이러다 갑자기 추워지지 또..",
          "original_form": "이러다 갑자기 추워지지 또..",
          "speaker_id": "1",
          "time": "20211016 00:34"
        },
        {
          "id": "MDRW2100046278.1.20",
          "form": "그러셨구나 역시 날씨 종잡을 수가 없네요",
          "original_form": "그러셨구나 역시 날씨 종잡을 수가 없네요",
          "speaker_id": "1",
          "time": "20211016 00:35"
        },
        {
          "id": "MDRW2100046278.1.21",
          "form": "겨울 옷 무엇을 사실 예정이신가요??",
          "original_form": "겨울 옷 무엇을 사실 예정이신가요??",
          "speaker_id": "2",
          "time": "20211016 00:35"
        },
        {
          "id": "MDRW2100046278.1.22",
          "form": "맨투맨기모는 너무 이르려나요",
          "original_form": "맨투맨기모는 너무 이르려나요",
          "speaker_id": "1",
          "time": "20211016 00:35"
        },
        {
          "id": "MDRW2100046278.1.23",
          "form": "그냥 일반 맨투맨을 살까나",
          "original_form": "그냥 일반 맨투맨을 살까나",
          "speaker_id": "1",
          "time": "20211016 00:35"
        },
        {
          "id": "MDRW2100046278.1.24",
          "form": "봄가을이 사라져서 휴",
          "original_form": "봄가을이 사라져서 휴",
          "speaker_id": "1",
          "time": "20211016 00:35"
        },
        {
          "id": "MDRW2100046278.1.25",
          "form": "지금 구매하면 딱 맞춰입을 수 있을 것같은데요!!",
          "original_form": "지금 구매하면 딱 맞춰입을 수 있을 것같은데요!!",
          "speaker_id": "2",
          "time": "20211016 00:36"
        },
        {
          "id": "MDRW2100046278.1.26",
          "form": "맨투맨 기모는 겨울에도 입을 수 있으니까요 ㅎㅎ",
          "original_form": "맨투맨 기모는 겨울에도 입을 수 있으니까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211016 00:36"
        },
        {
          "id": "MDRW2100046278.1.27",
          "form": "그쵸? 그럼 기모로 탕탕탕",
          "original_form": "그쵸? 그럼 기모로 탕탕탕",
          "speaker_id": "1",
          "time": "20211016 00:36"
        },
        {
          "id": "MDRW2100046278.1.28",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211016 00:37"
        },
        {
          "id": "MDRW2100046278.1.29",
          "form": "끝났나요! 감사합니다",
          "original_form": "끝났나요! 감사합니다",
          "speaker_id": "1",
          "time": "20211016 00:37"
        },
        {
          "id": "MDRW2100046278.1.30",
          "form": "좋은 하루 보내세요:)",
          "original_form": "좋은 하루 보내세요:)",
          "speaker_id": "2",
          "time": "20211016 00:37"
        }
      ]
