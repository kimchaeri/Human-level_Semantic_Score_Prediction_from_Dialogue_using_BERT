      "utterance": [
        {
          "id": "MDRW2100018489.1.1",
          "form": "pc게임 즐겨하세요?",
          "original_form": "pc게임 즐겨하세요?",
          "speaker_id": "1",
          "time": "20210827 05:29"
        },
        {
          "id": "MDRW2100018489.1.2",
          "form": "아니면 모바일게임 하세요??",
          "original_form": "아니면 모바일게임 하세요??",
          "speaker_id": "1",
          "time": "20210827 05:29"
        },
        {
          "id": "MDRW2100018489.1.3",
          "form": "답장이 늦어 죄송합니다ㅠㅠ 저는 심즈해요!",
          "original_form": "답장이 늦어 죄송합니다ㅠㅠ 저는 심즈해요!",
          "speaker_id": "2",
          "time": "20210828 03:57"
        },
        {
          "id": "MDRW2100018489.1.4",
          "form": "심즈라는 게임 아시나요?",
          "original_form": "심즈라는 게임 아시나요?",
          "speaker_id": "2",
          "time": "20210828 03:57"
        },
        {
          "id": "MDRW2100018489.1.5",
          "form": "알아요ㅋㅋㅋ유명하잖아요~",
          "original_form": "알아요ㅋㅋㅋ유명하잖아요~",
          "speaker_id": "1",
          "time": "20210828 03:57"
        },
        {
          "id": "MDRW2100018489.1.6",
          "form": "저도 심즈 3인가 4?",
          "original_form": "저도 심즈 3인가 4?",
          "speaker_id": "1",
          "time": "20210828 03:57"
        },
        {
          "id": "MDRW2100018489.1.7",
          "form": "해보셨어요??",
          "original_form": "해보셨어요??",
          "speaker_id": "2",
          "time": "20210828 03:57"
        },
        {
          "id": "MDRW2100018489.1.8",
          "form": "한.....7년전에 사봤어요",
          "original_form": "한.....7년전에 사봤어요",
          "speaker_id": "1",
          "time": "20210828 03:57"
        },
        {
          "id": "MDRW2100018489.1.9",
          "form": "친구가 추천해줘서ㅋㅋㅋ",
          "original_form": "친구가 추천해줘서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 03:58"
        },
        {
          "id": "MDRW2100018489.1.10",
          "form": "심즈모바일도 있는데 재밌어요!ㅎㅎ",
          "original_form": "심즈모바일도 있는데 재밌어요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 03:58"
        },
        {
          "id": "MDRW2100018489.1.11",
          "form": "하시는 게임 있으세요?",
          "original_form": "하시는 게임 있으세요?",
          "speaker_id": "2",
          "time": "20210828 03:58"
        },
        {
          "id": "MDRW2100018489.1.12",
          "form": "모바일이 있는줄은 몰랐네요",
          "original_form": "모바일이 있는줄은 몰랐네요",
          "speaker_id": "1",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100018489.1.13",
          "form": "프렌즈타운이라고",
          "original_form": "프렌즈타운이라고",
          "speaker_id": "1",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100018489.1.14",
          "form": "애니팡같은거 해요ㅋㅋㅋㅋ",
          "original_form": "애니팡같은거 해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100018489.1.15",
          "form": "오 뭔가 귀엽고 아기자기 할 것 같아요ㅋㅋㅋㅋ",
          "original_form": "오 뭔가 귀엽고 아기자기 할 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100018489.1.16",
          "form": "이것도 과금 유도 심한편인가요?ㅠㅠ",
          "original_form": "이것도 과금 유도 심한편인가요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100018489.1.17",
          "form": "음....아마도요ㅋㅋㅋㅋㅋ",
          "original_form": "음....아마도요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:45"
        },
        {
          "id": "MDRW2100018489.1.18",
          "form": "아무래도 하트 충전하느라? 근데저는",
          "original_form": "아무래도 하트 충전하느라? 근데저는",
          "speaker_id": "1",
          "time": "20210829 06:45"
        },
        {
          "id": "MDRW2100018489.1.19",
          "form": "한번도안했어요",
          "original_form": "한번도안했어요",
          "speaker_id": "1",
          "time": "20210829 06:45"
        },
        {
          "id": "MDRW2100018489.1.20",
          "form": "심즈도 과금있어요?",
          "original_form": "심즈도 과금있어요?",
          "speaker_id": "1",
          "time": "20210829 06:45"
        },
        {
          "id": "MDRW2100018489.1.21",
          "form": "무과금러이시군요ㅋㅋㅋ",
          "original_form": "무과금러이시군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:46"
        },
        {
          "id": "MDRW2100018489.1.22",
          "form": "이거 과금있어요ㅠㅠ 그래서 게임에 돈 한번도 안썼는데",
          "original_form": "이거 과금있어요ㅠㅠ 그래서 게임에 돈 한번도 안썼는데",
          "speaker_id": "2",
          "time": "20210829 06:46"
        },
        {
          "id": "MDRW2100018489.1.23",
          "form": "처음으로 돈 썼어요",
          "original_form": "처음으로 돈 썼어요",
          "speaker_id": "2",
          "time": "20210829 06:46"
        },
        {
          "id": "MDRW2100018489.1.24",
          "form": "심즈는 과금할게있어요?????",
          "original_form": "심즈는 과금할게있어요?????",
          "speaker_id": "1",
          "time": "20210829 06:48"
        },
        {
          "id": "MDRW2100018489.1.25",
          "form": "아이템 사는 건가요?!",
          "original_form": "아이템 사는 건가요?!",
          "speaker_id": "1",
          "time": "20210829 06:48"
        },
        {
          "id": "MDRW2100018489.1.26",
          "form": "넹넹ㅋㅋㅋ 그리고 시간이 흐르면서 진행되는 게임인데",
          "original_form": "넹넹ㅋㅋㅋ 그리고 시간이 흐르면서 진행되는 게임인데",
          "speaker_id": "2",
          "time": "20210829 06:48"
        },
        {
          "id": "MDRW2100018489.1.27",
          "form": "레벨업 시켜요 결제해서ㅋㅋ",
          "original_form": "레벨업 시켜요 결제해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:49"
        },
        {
          "id": "MDRW2100018489.1.28",
          "form": "ㅋㅋㅋㅋ앜ㅋㅋㅋㅋ컴으로할때 배속기능있던거 같은데",
          "original_form": "ㅋㅋㅋㅋ앜ㅋㅋㅋㅋ컴으로할때 배속기능있던거 같은데",
          "speaker_id": "1",
          "time": "20210829 06:50"
        },
        {
          "id": "MDRW2100018489.1.29",
          "form": "아닌가...?!",
          "original_form": "아닌가...?!",
          "speaker_id": "1",
          "time": "20210829 06:50"
        }
      ]
    }
