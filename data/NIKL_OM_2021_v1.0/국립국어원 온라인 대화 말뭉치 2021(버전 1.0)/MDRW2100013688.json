      "utterance": [
        {
          "id": "MDRW2100013688.1.1",
          "form": "혹시..팩트나 파데 쓰세요?",
          "original_form": "혹시..팩트나 파데 쓰세요?",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013688.1.2",
          "form": "요즘은 그냥 선크림만 발라요 name1님은요?",
          "original_form": "요즘은 그냥 선크림만 발라요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013688.1.3",
          "form": "저도요\r\n근데가끔 화장하고싶은데.. 블리팩트가 좋더라구요",
          "original_form": "저도요\r\n근데가끔 화장하고싶은데.. 블리팩트가 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013688.1.4",
          "form": "피부가 좋았으면좋겠네요 피부관리하세요?",
          "original_form": "피부가 좋았으면좋겠네요 피부관리하세요?",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013688.1.5",
          "form": "아뇨 저는 글렀어요.. 태생이 게으름뱅이인가봐요..ㅠ 노력도 안하면서 피부 좋았으면 좋겠다고 맨날 생각만해요",
          "original_form": "아뇨 저는 글렀어요.. 태생이 게으름뱅이인가봐요..ㅠ 노력도 안하면서 피부 좋았으면 좋겠다고 맨날 생각만해요",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013688.1.6",
          "form": "블리팩트 나중을 위해서 기억해 둬야겠어요 감사합니다",
          "original_form": "블리팩트 나중을 위해서 기억해 둬야겠어요 감사합니다",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013688.1.7",
          "form": "저도 그래요 팩만 어쩌다가..",
          "original_form": "저도 그래요 팩만 어쩌다가..",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013688.1.8",
          "form": "피부관리받거싶은데 피부가 얇아진다고 하더라구요",
          "original_form": "피부관리받거싶은데 피부가 얇아진다고 하더라구요",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013688.1.9",
          "form": "헉 그래요? 몰랐어요 조심해야겠네요",
          "original_form": "헉 그래요? 몰랐어요 조심해야겠네요",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013688.1.10",
          "form": "저는 피부가 얇아서 피부과도 못다니겠네요 ㅜ",
          "original_form": "저는 피부가 얇아서 피부과도 못다니겠네요 ㅜ",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013688.1.11",
          "form": "피부도 그렇고 뭔가 건강을 위해서 노력하고 싶은데 쉽지않네요?",
          "original_form": "피부도 그렇고 뭔가 건강을 위해서 노력하고 싶은데 쉽지않네요?",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013688.1.12",
          "form": "물음표는 오타에요!!",
          "original_form": "물음표는 오타에요!!",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013688.1.13",
          "form": "아네 ㅎ",
          "original_form": "아네 ㅎ",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013688.1.14",
          "form": "물이라도 많이 마셔요 우리ㅠ",
          "original_form": "물이라도 많이 마셔요 우리ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013688.1.15",
          "form": "코로나라 집에만있으니 운동도힘들구요",
          "original_form": "코로나라 집에만있으니 운동도힘들구요",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013688.1.16",
          "form": "그나마 그게 제일 쉬운것 같아요",
          "original_form": "그나마 그게 제일 쉬운것 같아요",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013688.1.17",
          "form": "네 물이라도 노력해요 ㅜ",
          "original_form": "네 물이라도 노력해요 ㅜ",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013688.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013688.1.19",
          "form": "집에서 전 가끔 자전거타",
          "original_form": "집에서 전 가끔 자전거타",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013688.1.20",
          "form": "타요",
          "original_form": "타요",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013688.1.21",
          "form": "어 저두요!",
          "original_form": "어 저두요!",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
