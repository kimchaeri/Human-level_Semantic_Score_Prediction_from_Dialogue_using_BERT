        },
        {
          "id": "MDRW2100013688.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013688.1.25",
          "form": "그래요? 그나마 괜찮죠 ㅋㅋ",
          "original_form": "그래요? 그나마 괜찮죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013688.1.26",
          "form": "국민자전건가봐요",
          "original_form": "국민자전건가봐요",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013688.1.27",
