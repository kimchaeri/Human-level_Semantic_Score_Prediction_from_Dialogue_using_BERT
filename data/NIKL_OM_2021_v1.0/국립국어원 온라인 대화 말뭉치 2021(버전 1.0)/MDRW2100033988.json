      "utterance": [
        {
          "id": "MDRW2100033988.1.1",
          "form": "산드라 블록이다 브래드피트랑 찍은 영화로 2000만달러",
          "original_form": "산드라 블록이다 브래드피트랑 찍은 영화로 2000만달러",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100033988.1.2",
          "form": "아 산드라블록 그럼 브래드랑 똑같이 받았네",
          "original_form": "아 산드라블록 그럼 브래드랑 똑같이 받았네",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100033988.1.3",
          "form": "공동9위 2000만달러가 너무 많았는지 8위가 없고 바로 7위다",
          "original_form": "공동9위 2000만달러가 너무 많았는지 8위가 없고 바로 7위다",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100033988.1.4",
          "form": "ㅋㅋㅋ 7위 당연히 남자겠지",
          "original_form": "ㅋㅋㅋ 7위 당연히 남자겠지",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100033988.1.5",
          "form": "7위 줄리아 로버츠 2500만달러",
          "original_form": "7위 줄리아 로버츠 2500만달러",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100033988.1.6",
          "form": "영화 찍은거 있어?",
          "original_form": "영화 찍은거 있어?",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100033988.1.7",
          "form": "덴젤워싱턴이랑 넷플릭스 영화 찍었네",
          "original_form": "덴젤워싱턴이랑 넷플릭스 영화 찍었네",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100033988.1.8",
          "form": "공개일은 미정이란다",
          "original_form": "공개일은 미정이란다",
          "speaker_id": "2",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100033988.1.9",
          "form": "넷플릭스 역시나 돈이 많군",
          "original_form": "넷플릭스 역시나 돈이 많군",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100033988.1.10",
          "form": "그렇다 1위도 이럼 넷플릭스겠다",
          "original_form": "그렇다 1위도 이럼 넷플릭스겠다",
          "speaker_id": "2",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100033988.1.11",
          "form": "그럴듯 요즘 시국은",
          "original_form": "그럴듯 요즘 시국은",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100033988.1.12",
          "form": "공동 7위 제니퍼 로렌스 이 영화 역시 넷플릭스",
          "original_form": "공동 7위 제니퍼 로렌스 이 영화 역시 넷플릭스",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100033988.1.13",
          "form": "이 위로는 거의 넷플릭스 일듯",
          "original_form": "이 위로는 거의 넷플릭스 일듯",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100033988.1.14",
          "form": "5위는 마크 월버그",
          "original_form": "5위는 마크 월버그",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100033988.1.15",
          "form": "누군지 모르겠다ㅋ",
          "original_form": "누군지 모르겠다ㅋ",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100033988.1.16",
          "form": "3000만달러 여기도 또 넷플락스다",
          "original_form": "3000만달러 여기도 또 넷플락스다",
