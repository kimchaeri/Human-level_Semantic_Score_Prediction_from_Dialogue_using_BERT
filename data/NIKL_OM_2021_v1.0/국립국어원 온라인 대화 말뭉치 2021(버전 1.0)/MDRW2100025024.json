      "utterance": [
        {
          "id": "MDRW2100025024.1.1",
          "form": "같이볶아도상관없는데",
          "original_form": "같이볶아도상관없는데",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100025024.1.2",
          "form": "간할 때 다진마늘!",
          "original_form": "간할 때 다진마늘!",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100025024.1.3",
          "form": "설탕!",
          "original_form": "설탕!",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100025024.1.4",
          "form": "소슴은 안 넣는것같아요",
          "original_form": "소슴은 안 넣는것같아요",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100025024.1.5",
          "form": "그리고 고춧가루조금..?",
          "original_form": "그리고 고춧가루조금..?",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100025024.1.6",
          "form": "치킨스톡 하나 사셔서",
          "original_form": "치킨스톡 하나 사셔서",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100025024.1.7",
          "form": "그렇게 했는데도 와 이건 입에 넣을수가없다",
          "original_form": "그렇게 했는데도 와 이건 입에 넣을수가없다",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100025024.1.8",
          "form": "치킨스톡 넣어주세용",
          "original_form": "치킨스톡 넣어주세용",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100025024.1.9",
          "form": "싶으명 다시다 넣어요..",
          "original_form": "싶으명 다시다 넣어요..",
          "speaker_id": "1",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100025024.1.10",
          "form": "아니면 참치액",
          "original_form": "아니면 참치액",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100025024.1.11",
          "form": "치킨스톡 있긴 한데",
          "original_form": "치킨스톡 있긴 한데",
          "speaker_id": "1",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100025024.1.12",
          "form": "다시다랑 차이가 있나요..?",
          "original_form": "다시다랑 차이가 있나요..?",
          "speaker_id": "1",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100025024.1.13",
          "form": "음 좀더 닭고기맛이나요",
          "original_form": "음 좀더 닭고기맛이나요",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100025024.1.14",
          "form": "소고기다시다넣으시나요?",
          "original_form": "소고기다시다넣으시나요?",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100025024.1.15",
          "form": "네넹 소고기요",
          "original_form": "네넹 소고기요",
          "speaker_id": "1",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100025024.1.16",
          "form": "음 그럼 ㄷ볶으시다가",
          "original_form": "음 그럼 ㄷ볶으시다가",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100025024.1.17",
          "form": "김치찌개에 닭고기는 잘 안 넣지 않나요?ㅠㅠ",
          "original_form": "김치찌개에 닭고기는 잘 안 넣지 않나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025024.1.18",
          "form": "물넣기전에 치킨스톡 한스푼넣어보세요 ㅋㄱㅋㅋㄱ",
          "original_form": "물넣기전에 치킨스톡 한스푼넣어보세요 ㅋㄱㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025024.1.19",
          "form": "넣은채로 살짝더볶음다음에",
          "original_form": "넣은채로 살짝더볶음다음에",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025024.1.20",
          "form": "재료가잠길때까지 넣고",
          "original_form": "재료가잠길때까지 넣고",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025024.1.21",
          "form": "한참끓이시면돼요",
          "original_form": "한참끓이시면돼요",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025024.1.22",
          "form": "하 나중에 해볼게요 제발 성공하고싶다",
          "original_form": "하 나중에 해볼게요 제발 성공하고싶다",
          "speaker_id": "1",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025024.1.23",
          "form": "그리고 간장이랑",
          "original_form": "그리고 간장이랑",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025024.1.24",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025024.1.25",
          "form": "까나리나 멸치중에",
          "original_form": "까나리나 멸치중에",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100025024.1.26",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100025024.1.27",
          "form": "넣고삭힌액상물을",
          "original_form": "넣고삭힌액상물을",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100025024.1.28",
          "form": "유추해서 넣어주세요",
          "original_form": "유추해서 넣어주세요",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100025024.1.29",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
