      "utterance": [
        {
          "id": "MDRW2100022930.1.1",
          "form": "전 어제 오늘 둘다",
          "original_form": "전 어제 오늘 둘다",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022930.1.2",
          "form": "운동을 못했는데",
          "original_form": "운동을 못했는데",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022930.1.3",
          "form": "하셨나요?",
          "original_form": "하셨나요?",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022930.1.4",
          "form": "어제햇고.. 오늘은 이따 어두워지면 런닝하러 갈려구요..",
          "original_form": "어제햇고.. 오늘은 이따 어두워지면 런닝하러 갈려구요..",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022930.1.5",
          "form": "뛰는 거 좋죠",
          "original_form": "뛰는 거 좋죠",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100022930.1.6",
          "form": "약 어느정도 뛰세요?",
          "original_form": "약 어느정도 뛰세요?",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100022930.1.7",
          "form": "1.5키로도 버거워요..\r\n런닝쉰지 6년?",
          "original_form": "1.5키로도 버거워요..\r\n런닝쉰지 6년?",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022930.1.8",
          "form": "주기적으로 못뛰어서 안느네요..",
          "original_form": "주기적으로 못뛰어서 안느네요..",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022930.1.9",
          "form": "와 운동되게",
          "original_form": "와 운동되게",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022930.1.10",
          "form": "꾸준히 잘하시네요",
          "original_form": "꾸준히 잘하시네요",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022930.1.11",
          "form": "멋있어요ㅋㅋ",
          "original_form": "멋있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022930.1.12",
          "form": "ㅎㅎ 보통 3키로는 다 뛸껄요?",
          "original_form": "ㅎㅎ 보통 3키로는 다 뛸껄요?",
          "speaker_id": "1",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022930.1.13",
          "form": "어릴때 10키로씩 주기적으로 뛰었는데.. \r\n체중도 불고해서..",
          "original_form": "어릴때 10키로씩 주기적으로 뛰었는데.. \r\n체중도 불고해서..",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022930.1.14",
          "form": "와우 10키로ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와우 10키로ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022930.1.15",
          "form": "저도 운동해야하는데",
          "original_form": "저도 운동해야하는데",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022930.1.16",
          "form": "진짜 왜이리 하기 싫은지 ㅠㅠ",
          "original_form": "진짜 왜이리 하기 싫은지 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022930.1.17",
          "form": "전 운동안하면 아파서.. ㅋㅋㅋ",
          "original_form": "전 운동안하면 아파서.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022930.1.18",
          "form": "이상하게 쉬면 마디마디가 아파요.. ㅋㅋ",
          "original_form": "이상하게 쉬면 마디마디가 아파요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022930.1.19",
          "form": "몸에 운동이 베였나봐요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "몸에 운동이 베였나봐요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:57"
        },
        {
          "id": "MDRW2100022930.1.20",
          "form": "그렇다고 하더라구요.. \r\n운동하다가 않하면 근육이 수축해서 밸런스가 깨져서 아프다고 하더라구요..",
          "original_form": "그렇다고 하더라구요.. \r\n운동하다가 않하면 근육이 수축해서 밸런스가 깨져서 아프다고 하더라구요..",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100022930.1.21",
          "form": "해도 아프고 안해도 아프면..그냥 아예 안하는게..ㅋㅋㅋ",
          "original_form": "해도 아프고 안해도 아프면..그냥 아예 안하는게..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100022930.1.22",
          "form": "아 짐짜요?",
          "original_form": "아 짐짜요?",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100022930.1.23",
          "form": "그래도 해야 나중에",
          "original_form": "그래도 해야 나중에",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100022930.1.24",
          "form": "편한 거 같아요",
          "original_form": "편한 거 같아요",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100022930.1.25",
          "form": "오늘 밖에 운동하러 나온 사람 엄청 많네요..\r\n오랜만에.비안와서 그런지",
          "original_form": "오늘 밖에 운동하러 나온 사람 엄청 많네요..\r\n오랜만에.비안와서 그런지",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100022930.1.26",
          "form": "날씨가 좀",
          "original_form": "날씨가 좀",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100022930.1.27",
          "form": "시원해서 그런가봐요",
          "original_form": "시원해서 그런가봐요",
          "speaker_id": "2",
          "time": "20210829 22:12"
        }
      ]
    }
