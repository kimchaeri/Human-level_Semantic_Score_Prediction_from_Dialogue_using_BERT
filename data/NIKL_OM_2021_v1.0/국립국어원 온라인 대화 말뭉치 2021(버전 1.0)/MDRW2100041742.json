      "utterance": [
        {
          "id": "MDRW2100041742.1.1",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041742.1.2",
          "form": "전 운동을 좋아해서요",
          "original_form": "전 운동을 좋아해서요",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041742.1.3",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041742.1.4",
          "form": "어떤거 하세요?!",
          "original_form": "어떤거 하세요?!",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041742.1.5",
          "form": "특히 수영을 오래했죠",
          "original_form": "특히 수영을 오래했죠",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041742.1.6",
          "form": "거의 20년 가까이 했어요",
          "original_form": "거의 20년 가까이 했어요",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041742.1.7",
          "form": "그러다가 작년부터 수영장을 안가고 있네요",
          "original_form": "그러다가 작년부터 수영장을 안가고 있네요",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041742.1.8",
          "form": "와...!! 저 수영 아예 못하는데...",
          "original_form": "와...!! 저 수영 아예 못하는데...",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041742.1.9",
          "form": "수영하고 싶어 참 답답해요",
          "original_form": "수영하고 싶어 참 답답해요",
          "speaker_id": "2",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041742.1.10",
          "form": "물에도 못떠요",
          "original_form": "물에도 못떠요",
          "speaker_id": "1",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041742.1.11",
          "form": "아ㅜㅜ 코로나때문에요?? ㅜㅜㅜ",
          "original_form": "아ㅜㅜ 코로나때문에요?? ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041742.1.12",
          "form": "물에도 못떠요? 난감하군요",
          "original_form": "물에도 못떠요? 난감하군요",
          "speaker_id": "2",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041742.1.13",
          "form": "네 코로나로 다중이용시설을 안가고 있어요",
          "original_form": "네 코로나로 다중이용시설을 안가고 있어요",
          "speaker_id": "2",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041742.1.14",
          "form": "악 ㅋㅋㅋㅋ그쵸 ㅋㅋㅋ저 물에빠지면 걍 죽는거에요 ㅋㅋㅋㅋㅋ",
          "original_form": "악 ㅋㅋㅋㅋ그쵸 ㅋㅋㅋ저 물에빠지면 걍 죽는거에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041742.1.15",
          "form": "물에 뜨기만 해도 수영은 절반은 한건데요",
          "original_form": "물에 뜨기만 해도 수영은 절반은 한건데요",
          "speaker_id": "2",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041742.1.16",
          "form": "물에 어떻게 뜰 수 있단건지 ㅋㅋㅋ이해가 안가요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "물에 어떻게 뜰 수 있단건지 ㅋㅋㅋ이해가 안가요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041742.1.17",
          "form": "몸에 힘을 빼면 떠요",
          "original_form": "몸에 힘을 빼면 떠요",
          "speaker_id": "2",
          "time": "20210906 15:23"
        },
        {
          "id": "MDRW2100041742.1.18",
          "form": "긴장해서 몸이 굳어있으면 가라앉아요",
          "original_form": "긴장해서 몸이 굳어있으면 가라앉아요",
          "speaker_id": "2",
          "time": "20210906 15:23"
        },
        {
          "id": "MDRW2100041742.1.19",
          "form": "가라앉는거같아도 숨 좀 못쉰다고 생각하고 힘을 빼야하고 있을랑가요 ㅜㅜ 가라앉는다 싶으면 무조건 몸에 힘이 들어가는거같아요",
          "original_form": "가라앉는거같아도 숨 좀 못쉰다고 생각하고 힘을 빼야하고 있을랑가요 ㅜㅜ 가라앉는다 싶으면 무조건 몸에 힘이 들어가는거같아요",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041742.1.20",
          "form": "근데 수영은 진짜 수영장이 없으면 하기 힘들겠네요 ㅜㅠㅠ",
          "original_form": "근데 수영은 진짜 수영장이 없으면 하기 힘들겠네요 ㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041742.1.21",
          "form": "정말 가고싶으시겠어요",
          "original_form": "정말 가고싶으시겠어요",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041742.1.22",
          "form": "일단 무조건 긴장 풀어야해요",
          "original_form": "일단 무조건 긴장 풀어야해요",
          "speaker_id": "2",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041742.1.23",
          "form": "안가라앉는다고 생각해야 해요",
          "original_form": "안가라앉는다고 생각해야 해요",
          "speaker_id": "2",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041742.1.24",
          "form": "나는 해파리다",
          "original_form": "나는 해파리다",
          "speaker_id": "2",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041742.1.25",
          "form": "이렇게 생각하고 무념무상으로 둥둥 떠있으면 돼요",
          "original_form": "이렇게 생각하고 무념무상으로 둥둥 떠있으면 돼요",
          "speaker_id": "2",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041742.1.26",
          "form": "우리 집 근처엔 수영장이 큰게 2개나 있어서요 다니긴 좋았어요",
          "original_form": "우리 집 근처엔 수영장이 큰게 2개나 있어서요 다니긴 좋았어요",
          "speaker_id": "2",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041742.1.27",
          "form": "악ㅋㅋㅋㅋㅋ 나는 해파리다~~ 미역이다~~",
          "original_form": "악ㅋㅋㅋㅋㅋ 나는 해파리다~~ 미역이다~~",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041742.1.28",
          "form": "저희동네는 수영장가려면 마을버스 한번 타야해요 ㅋㅋㅋ쿠 ㅜㅜ",
          "original_form": "저희동네는 수영장가려면 마을버스 한번 타야해요 ㅋㅋㅋ쿠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041742.1.29",
          "form": "접근성이 좋아야 꾸준히 운동할 수 있는데요",
          "original_form": "접근성이 좋아야 꾸준히 운동할 수 있는데요",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041742.1.30",
          "form": "그러게요 ㅋㅋㅋ저 배드민턴이라도 좀 치고 싶은데 이건 또...같이 칠수 있는 사람이 필요하니 ㅜㅜ ㅋㅋㅋㅋ",
          "original_form": "그러게요 ㅋㅋㅋ저 배드민턴이라도 좀 치고 싶은데 이건 또...같이 칠수 있는 사람이 필요하니 ㅜㅜ ㅋㅋㅋㅋ",
