        {
          "id": "MDRW2100020411.1.2",
          "form": "또게임",
          "original_form": "또게임",
          "speaker_id": "1",
          "time": "20210830 11:34"
        },
        {
          "id": "MDRW2100020411.1.3",
          "form": "주말동안 무슨게임하셨나요",
          "original_form": "주말동안 무슨게임하셨나요",
          "speaker_id": "1",
          "time": "20210830 11:34"
        },
        {
          "id": "MDRW2100020411.1.4",
          "form": "갓오브워했습니다",
          "original_form": "갓오브워했습니다",
          "speaker_id": "2",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100020411.1.5",
          "form": "재밌나요",
          "original_form": "재밌나요",
          "speaker_id": "1",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100020411.1.6",
          "form": "전 와일드 리프트했습니다",
          "original_form": "전 와일드 리프트했습니다",
          "speaker_id": "1",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100020411.1.7",
          "form": "칼바람 존잼",
          "original_form": "칼바람 존잼",
          "speaker_id": "1",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100020411.1.8",
          "form": "그게 먼데요",
          "original_form": "그게 먼데요",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100020411.1.9",
          "form": "나중에",
          "original_form": "나중에",
          "speaker_id": "1",
          "time": "20210830 15:04"
        },
        {
          "id": "MDRW2100020411.1.10",
          "form": "유튜브에 찾아서 보세요",
          "original_form": "유튜브에 찾아서 보세요",
          "speaker_id": "1",
          "time": "20210830 15:04"
        },
        {
          "id": "MDRW2100020411.1.11",
          "form": "만능유튜브",
          "original_form": "만능유튜브",
          "speaker_id": "1",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100020411.1.12",
          "form": "글쿤요",
          "original_form": "글쿤요",
          "speaker_id": "2",
          "time": "20210830 15:24"
        },
        {
          "id": "MDRW2100020411.1.13",
          "form": "전 이거 다 깨고 스파이더맨할거",
          "original_form": "전 이거 다 깨고 스파이더맨할거",
          "speaker_id": "2",
          "time": "20210830 15:24"
        },
        {
          "id": "MDRW2100020411.1.14",
          "form": "그건 무슨겜인가요",
          "original_form": "그건 무슨겜인가요",
          "speaker_id": "1",
          "time": "20210830 17:11"
        },
        {
          "id": "MDRW2100020411.1.15",
          "form": "코믹스 스토리대로",
          "original_form": "코믹스 스토리대로",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100020411.1.16",
          "form": "스파이더맨 겜이에요",
          "original_form": "스파이더맨 겜이에요",
          "speaker_id": "2",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100020411.1.17",
          "form": "돈내고 사야하는 게임인가요",
          "original_form": "돈내고 사야하는 게임인가요",
          "speaker_id": "1",
          "time": "20210831 10:40"
