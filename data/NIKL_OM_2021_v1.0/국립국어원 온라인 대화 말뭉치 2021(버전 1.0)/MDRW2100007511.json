      "utterance": [
        {
          "id": "MDRW2100007511.1.1",
          "form": "혹시 취업하신 분이신가요",
          "original_form": "혹시 취업하신 분이신가요",
          "speaker_id": "1",
          "time": "20210822 02:43"
        },
        {
          "id": "MDRW2100007511.1.2",
          "form": "예민한 질문이었다",
          "original_form": "예민한 질문이었다",
          "speaker_id": "1",
          "time": "20210822 02:43"
        },
        {
          "id": "MDRW2100007511.1.3",
          "form": "아뇨 아뇨",
          "original_form": "아뇨 아뇨",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.4",
          "form": "괜찮아요",
          "original_form": "괜찮아요",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.5",
          "form": "코로나로 쉬고있어요",
          "original_form": "코로나로 쉬고있어요",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.6",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.7",
          "form": "웹툰님은요?",
          "original_form": "웹툰님은요?",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.8",
          "form": "헉 코로나로 쉬신다고요?!",
          "original_form": "헉 코로나로 쉬신다고요?!",
          "speaker_id": "1",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.9",
          "form": "앗 저는 취준생이라",
          "original_form": "앗 저는 취준생이라",
          "speaker_id": "1",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.10",
          "form": "흑흡",
          "original_form": "흑흡",
          "speaker_id": "1",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.11",
          "form": "코로나로 안된다고 정신승리하고있어요",
          "original_form": "코로나로 안된다고 정신승리하고있어요",
          "speaker_id": "1",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.12",
          "form": "에이 아니에요",
          "original_form": "에이 아니에요",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.13",
          "form": "꼭 좋은 곳 들어가실거에요",
          "original_form": "꼭 좋은 곳 들어가실거에요",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.14",
          "form": "알바 같은 건 해봤나요?",
          "original_form": "알바 같은 건 해봤나요?",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007511.1.15",
          "form": "앗 네넵",
          "original_form": "앗 네넵",
          "speaker_id": "1",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007511.1.16",
          "form": "알바는 하는데 제가 원하는 직종이랑 상관은 없어서",
          "original_form": "알바는 하는데 제가 원하는 직종이랑 상관은 없어서",
          "speaker_id": "1",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007511.1.17",
          "form": "하 고민입니다",
          "original_form": "하 고민입니다",
          "speaker_id": "1",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007511.1.18",
          "form": "어디 직종을 생각하고있나요?",
          "original_form": "어디 직종을 생각하고있나요?",
          "speaker_id": "2",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007511.1.19",
          "form": "웹툰이랑 관련된 직종으로 생각하고 있어요",
          "original_form": "웹툰이랑 관련된 직종으로 생각하고 있어요",
          "speaker_id": "1",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007511.1.20",
          "form": "오 작가? 어시?음 스토리작가?",
          "original_form": "오 작가? 어시?음 스토리작가?",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007511.1.21",
          "form": "웹툰PD라고 그걸로 준비중인데",
          "original_form": "웹툰PD라고 그걸로 준비중인데",
          "speaker_id": "1",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007511.1.22",
          "form": "엇 작가 어시도 넣고 있어요",
          "original_form": "엇 작가 어시도 넣고 있어요",
          "speaker_id": "1",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007511.1.23",
          "form": "웹툰pd는",
          "original_form": "웹툰pd는",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007511.1.24",
          "form": "어떤걸하나요?",
          "original_form": "어떤걸하나요?",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007511.1.25",
          "form": "작가들 괸리?",
          "original_form": "작가들 괸리?",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007511.1.26",
          "form": "이런거하나요?",
          "original_form": "이런거하나요?",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007511.1.27",
          "form": "회사 들어가서 작가들 컨택한다음에",
          "original_form": "회사 들어가서 작가들 컨택한다음에",
          "speaker_id": "1",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007511.1.28",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007511.1.29",
          "form": "연재 주기 관리하고 그런거 한대요",
          "original_form": "연재 주기 관리하고 그런거 한대요",
          "speaker_id": "1",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007511.1.30",
          "form": "오 흥미롭다",
          "original_form": "오 흥미롭다",
          "speaker_id": "2",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007511.1.31",
          "form": "그리고 작가님들 그림 들어오면 연출적으로 잘 아는 사람이면 피드백 줄 수도 있다고 하더라고요",
          "original_form": "그리고 작가님들 그림 들어오면 연출적으로 잘 아는 사람이면 피드백 줄 수도 있다고 하더라고요",
          "speaker_id": "1",
          "time": "20210822 02:47"
        }
      ]
    }
