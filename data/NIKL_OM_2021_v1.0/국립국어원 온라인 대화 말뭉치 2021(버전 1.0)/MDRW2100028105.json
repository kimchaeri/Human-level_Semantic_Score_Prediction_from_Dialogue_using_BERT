      "utterance": [
        {
          "id": "MDRW2100028105.1.1",
          "form": "바질새우맛있음",
          "original_form": "바질새우맛있음",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028105.1.2",
          "form": "먹고 있다는 건 아니고 그냥 맛있다고",
          "original_form": "먹고 있다는 건 아니고 그냥 맛있다고",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028105.1.3",
          "form": "안 먹어봤는데",
          "original_form": "안 먹어봤는데",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028105.1.4",
          "form": "뭐라",
          "original_form": "뭐라",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028105.1.5",
          "form": "걍 새우에 바질 버무린 건가",
          "original_form": "걍 새우에 바질 버무린 건가",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028105.1.6",
          "form": "그런 편임",
          "original_form": "그런 편임",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028105.1.7",
          "form": "바질 소스",
          "original_form": "바질 소스",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028105.1.8",
          "form": "의미를 모르겠군",
          "original_form": "의미를 모르겠군",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028105.1.9",
          "form": "하지만 새우는 내가 까지만 않으면",
          "original_form": "하지만 새우는 내가 까지만 않으면",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028105.1.10",
          "form": "대체로 좋아함",
          "original_form": "대체로 좋아함",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028105.1.11",
          "form": "아 당연히 깐새우지",
          "original_form": "아 당연히 깐새우지",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028105.1.12",
          "form": "근데 그거 나도임",
          "original_form": "근데 그거 나도임",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028105.1.13",
          "form": "ㄹㅇ 내가 까야하면 손이 안 감",
          "original_form": "ㄹㅇ 내가 까야하면 손이 안 감",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028105.1.14",
          "form": "너무 번거로워",
          "original_form": "너무 번거로워",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028105.1.15",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028105.1.16",
          "form": "아예 요리과정에서 까야 하는 거면 괜찮은데",
          "original_form": "아예 요리과정에서 까야 하는 거면 괜찮은데",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028105.1.17",
          "form": "칵테일새우 이런 거 짱",
          "original_form": "칵테일새우 이런 거 짱",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028105.1.18",
          "form": "먹는 사람이 까야하면 안 먹게됨",
          "original_form": "먹는 사람이 까야하면 안 먹게됨",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028105.1.19",
          "form": "ㄹㅇ 양념 묻은 새우 까야하는 거",
          "original_form": "ㄹㅇ 양념 묻은 새우 까야하는 거",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028105.1.20",
          "form": "최악",
          "original_form": "최악",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028105.1.21",
          "form": "으",
          "original_form": "으",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028105.1.22",
          "form": "바질새우 와인 안주로 딱임",
          "original_form": "바질새우 와인 안주로 딱임",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028105.1.23",
          "form": "맛잇겠다",
          "original_form": "맛잇겠다",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028105.1.24",
          "form": "와인 잘 안 마시지만",
          "original_form": "와인 잘 안 마시지만",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028105.1.25",
          "form": "제일 좋아하는 술 종류가 뭔가요",
          "original_form": "제일 좋아하는 술 종류가 뭔가요",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028105.1.26",
          "form": "술 잘 안 마셔서",
          "original_form": "술 잘 안 마셔서",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028105.1.27",
          "form": "그건 알지만",
          "original_form": "그건 알지만",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028105.1.28",
          "form": "제일 자주 마시는 건 아무래도 맥주",
          "original_form": "제일 자주 마시는 건 아무래도 맥주",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028105.1.29",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028105.1.30",
          "form": "다른 건 안 마신지 너무 오래돼서 모르겠음",
          "original_form": "다른 건 안 마신지 너무 오래돼서 모르겠음",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028105.1.31",
          "form": "선호하는 브랜드가 있으신지",
          "original_form": "선호하는 브랜드가 있으신지",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028105.1.32",
          "form": "요즘은 칭따오 생맥 자주 마시는 듯?",
          "original_form": "요즘은 칭따오 생맥 자주 마시는 듯?",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028105.1.33",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028105.1.34",
          "form": "가볍고 좋아",
          "original_form": "가볍고 좋아",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028105.1.35",
          "form": "님은 뭐 마시시나요",
          "original_form": "님은 뭐 마시시나요",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028105.1.36",
          "form": "제일 좋아하는 종류는...",
          "original_form": "제일 좋아하는 종류는...",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028105.1.37",
          "form": "아무래도 소주",
          "original_form": "아무래도 소주",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028105.1.38",
          "form": "맥주 브랜드는... 요즘은 하이네켄",
          "original_form": "맥주 브랜드는... 요즘은 하이네켄",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028105.1.39",
          "form": "그렇구만...",
          "original_form": "그렇구만...",
          "speaker_id": "1",
          "time": "20210902 17:41"
        }
      ]
    }
