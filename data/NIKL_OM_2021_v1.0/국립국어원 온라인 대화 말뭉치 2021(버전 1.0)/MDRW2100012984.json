      "utterance": [
        {
          "id": "MDRW2100012984.1.1",
          "form": "메타버스에 대해 아세요?",
          "original_form": "메타버스에 대해 아세요?",
          "speaker_id": "1",
          "time": "20210825 10:00"
        },
        {
          "id": "MDRW2100012984.1.2",
          "form": "그냥 관심있는정도..? 제페토하는건 봤어요",
          "original_form": "그냥 관심있는정도..? 제페토하는건 봤어요",
          "speaker_id": "2",
          "time": "20210825 10:00"
        },
        {
          "id": "MDRW2100012984.1.3",
          "form": "제페토는 뭐죠??",
          "original_form": "제페토는 뭐죠??",
          "speaker_id": "1",
          "time": "20210825 10:01"
        },
        {
          "id": "MDRW2100012984.1.4",
          "form": "아바타로 하는 가상 세계더라구요ㅎㅎ",
          "original_form": "아바타로 하는 가상 세계더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 10:01"
        },
        {
          "id": "MDRW2100012984.1.5",
          "form": "은행도 지점낸다해서 신기해서 봤어요ㅋㅋ",
          "original_form": "은행도 지점낸다해서 신기해서 봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:02"
        },
        {
          "id": "MDRW2100012984.1.6",
          "form": "방금 제페토 찾아봤는데 그냥 게임 같은데요~",
          "original_form": "방금 제페토 찾아봤는데 그냥 게임 같은데요~",
          "speaker_id": "1",
          "time": "20210825 10:03"
        },
        {
          "id": "MDRW2100012984.1.7",
          "form": "근데 게임도 있구 소통하는 개념이 더 크더라구요ㅋㅋ",
          "original_form": "근데 게임도 있구 소통하는 개념이 더 크더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:03"
        },
        {
          "id": "MDRW2100012984.1.8",
          "form": "겨울에 네이버가 메타버스 관련주라고만 듣고 메타버스에 대해 잠시 찾아봤거든요.",
          "original_form": "겨울에 네이버가 메타버스 관련주라고만 듣고 메타버스에 대해 잠시 찾아봤거든요.",
          "speaker_id": "1",
          "time": "20210825 10:04"
        },
        {
          "id": "MDRW2100012984.1.9",
          "form": "아 네 제페토가 네이버꺼에요ㅋㅋ",
          "original_form": "아 네 제페토가 네이버꺼에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:05"
        },
        {
          "id": "MDRW2100012984.1.10",
          "form": "찾아보니 그렇더라고요. 감을 잡기 위해 저도 그 게임 해볼까봐요 ㅋㅋ",
          "original_form": "찾아보니 그렇더라고요. 감을 잡기 위해 저도 그 게임 해볼까봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:54"
        },
        {
          "id": "MDRW2100012984.1.11",
          "form": "예전 싸이같기도 하구ㅎㅎ 재밌어요ㅋㅋ",
          "original_form": "예전 싸이같기도 하구ㅎㅎ 재밌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:55"
        },
        {
          "id": "MDRW2100012984.1.12",
          "form": "해보셨군요?",
          "original_form": "해보셨군요?",
          "speaker_id": "1",
          "time": "20210825 10:56"
        },
        {
          "id": "MDRW2100012984.1.13",
          "form": "네 학생들이 주변에 있어서.. 거기서 만나자는거에요ㅋㅋㅋ신기했죠",
          "original_form": "네 학생들이 주변에 있어서.. 거기서 만나자는거에요ㅋㅋㅋ신기했죠",
          "speaker_id": "2",
          "time": "20210825 10:57"
        },
        {
          "id": "MDRW2100012984.1.14",
          "form": "실제로 만나는거랑 비슷한 느낌인가요?? 아예 감이 없네요",
          "original_form": "실제로 만나는거랑 비슷한 느낌인가요?? 아예 감이 없네요",
          "speaker_id": "1",
          "time": "20210825 10:58"
        },
        {
          "id": "MDRW2100012984.1.15",
          "form": "네 게임같아요ㅎㅎ",
          "original_form": "네 게임같아요ㅎㅎ",
