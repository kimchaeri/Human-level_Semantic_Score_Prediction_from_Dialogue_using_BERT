      "utterance": [
        {
          "id": "MDRW2100032378.1.1",
          "form": "요즘 올영세일 하더라",
          "original_form": "요즘 올영세일 하더라",
          "speaker_id": "2",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032378.1.2",
          "form": "그래서 피지오겔 크림 삼",
          "original_form": "그래서 피지오겔 크림 삼",
          "speaker_id": "2",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032378.1.3",
          "form": "보습력은 그게 제일이지ㅋㅋ",
          "original_form": "보습력은 그게 제일이지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100032378.1.4",
          "form": "뭔가 피부가 거칠어져서 기초를 더 써야하나 싶은데",
          "original_form": "뭔가 피부가 거칠어져서 기초를 더 써야하나 싶은데",
          "speaker_id": "1",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100032378.1.5",
          "form": "뭘 써야 할지 모르겠어",
          "original_form": "뭘 써야 할지 모르겠어",
          "speaker_id": "1",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100032378.1.6",
          "form": "요즘은 로션하나 바르는것도 벅차",
          "original_form": "요즘은 로션하나 바르는것도 벅차",
          "speaker_id": "1",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100032378.1.7",
          "form": "나도 에센스 추가해야 하는데 뭐 사야할지 모르겠어",
          "original_form": "나도 에센스 추가해야 하는데 뭐 사야할지 모르겠어",
          "speaker_id": "2",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100032378.1.8",
          "form": "급한대로 엄마 설화수  에센스 쓰는데 한방의 냄새가 강력해서 내꺼 빨리 사고싶다",
          "original_form": "급한대로 엄마 설화수  에센스 쓰는데 한방의 냄새가 강력해서 내꺼 빨리 사고싶다",
          "speaker_id": "2",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032378.1.9",
          "form": "어릴때처럼 막 살수가 없는데 기초바르기도 귀찮아...ㅋㅋㅋ",
          "original_form": "어릴때처럼 막 살수가 없는데 기초바르기도 귀찮아...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032378.1.10",
          "form": "그래도 설화수 좋더라 너무 비싸서 그렇지...",
          "original_form": "그래도 설화수 좋더라 너무 비싸서 그렇지...",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032378.1.11",
          "form": "ㅇㅇ 그래서 화해들어가서 적당한 에센스 없나 보고 있는데 뭐 살지 못정했어",
          "original_form": "ㅇㅇ 그래서 화해들어가서 적당한 에센스 없나 보고 있는데 뭐 살지 못정했어",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032378.1.12",
          "form": "진짜 피부 관리좀 해야하는데...",
          "original_form": "진짜 피부 관리좀 해야하는데...",
          "speaker_id": "1",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032378.1.13",
          "form": "갈수록 관리비용이 늘어난다ㅋㅋ",
          "original_form": "갈수록 관리비용이 늘어난다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032378.1.14",
          "form": "참 너는 백신 맞았어?",
          "original_form": "참 너는 백신 맞았어?",
          "speaker_id": "1",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032378.1.15",
          "form": "응 1차  화이자로 접종함",
          "original_form": "응 1차  화이자로 접종함",
          "speaker_id": "2",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032378.1.16",
          "form": "괜찮았어?",
          "original_form": "괜찮았어?",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032378.1.17",
          "form": "난 2차 못 맞을거 같다ㅠ",
          "original_form": "난 2차 못 맞을거 같다ㅠ",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032378.1.18",
          "form": "근육통처럼 뻐근한게 3일정도 갔어",
          "original_form": "근육통처럼 뻐근한게 3일정도 갔어",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032378.1.19",
          "form": "너는 왜? 휴유증있었어?",
          "original_form": "너는 왜? 휴유증있었어?",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032378.1.20",
          "form": "생리주기랑 안 맞고 일도 바쁘고",
          "original_form": "생리주기랑 안 맞고 일도 바쁘고",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032378.1.21",
          "form": "연기 안 됐으면 딱 맞는 일정이었는데ㅠ",
          "original_form": "연기 안 됐으면 딱 맞는 일정이었는데ㅠ",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032378.1.22",
          "form": "아하 근데 그거 날짜 변경가능하지 않아?",
          "original_form": "아하 근데 그거 날짜 변경가능하지 않아?",
          "speaker_id": "2",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032378.1.23",
          "form": "전화해서 변경하라는데 통화가 안됨ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전화해서 변경하라는데 통화가 안됨ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100032378.1.24",
          "form": "ㅋㅋㅋㅋㅋ전국민 수강신청이자 피켓팅ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ전국민 수강신청이자 피켓팅ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100032378.1.25",
          "form": "아님 사이트에서 변경 안되나?",
          "original_form": "아님 사이트에서 변경 안되나?",
          "speaker_id": "2",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100032378.1.26",
          "form": "절대안됨ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "절대안됨ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100032378.1.27",
          "form": "취소도 안됨ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "취소도 안됨ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100032378.1.28",
          "form": "되는게 없어",
          "original_form": "되는게 없어",
          "speaker_id": "1",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100032378.1.29",
          "form": "우와 빡빡하다",
          "original_form": "우와 빡빡하다",
          "speaker_id": "2",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100032378.1.30",
          "form": "정부에서 날 버렸어ㅋㅋㅋㅋㅋㅋ",
          "original_form": "정부에서 날 버렸어ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100032378.1.31",
          "form": "주위의 모두가 맞아서 집단면역만을 기다려야지....",
          "original_form": "주위의 모두가 맞아서 집단면역만을 기다려야지....",
          "speaker_id": "1",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100032378.1.32",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100032378.1.33",
          "form": "아님  잔여백신을 노려봄이 어떨지",
          "original_form": "아님  잔여백신을 노려봄이 어떨지",
          "speaker_id": "2",
          "time": "20210905 20:08"
        },
        {
          "id": "MDRW2100032378.1.34",
          "form": "그것도 1차 맞기전인 사람만 가능함ㅠ",
          "original_form": "그것도 1차 맞기전인 사람만 가능함ㅠ",
          "speaker_id": "1",
          "time": "20210905 20:09"
        },
        {
          "id": "MDRW2100032378.1.35",
          "form": "진짜 방법이 없어",
          "original_form": "진짜 방법이 없어",
          "speaker_id": "1",
          "time": "20210905 20:10"
        },
        {
          "id": "MDRW2100032378.1.36",
          "form": "아 그렇구나ㅠ",
          "original_form": "아 그렇구나ㅠ",
          "speaker_id": "2",
          "time": "20210905 20:10"
        },
        {
          "id": "MDRW2100032378.1.37",
          "form": "진짜 기적적으로 통화가 되는 수 밖에  없구나",
          "original_form": "진짜 기적적으로 통화가 되는 수 밖에  없구나",
          "speaker_id": "2",
          "time": "20210905 20:10"
        },
        {
          "id": "MDRW2100032378.1.38",
          "form": "내 주위가 모두 면역자가 되는수밖에ㅋㅋ",
          "original_form": "내 주위가 모두 면역자가 되는수밖에ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:13"
        },
        {
          "id": "MDRW2100032378.1.39",
          "form": "그래도 너는 3일만 피곤했던거면 괜찮은 편인거같네 다행이다",
          "original_form": "그래도 너는 3일만 피곤했던거면 괜찮은 편인거같네 다행이다",
          "speaker_id": "1",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100032378.1.40",
          "form": "ㅋㅋㅋㅋ 비자발적 백신 거부자가 되었구나",
          "original_form": "ㅋㅋㅋㅋ 비자발적 백신 거부자가 되었구나",
          "speaker_id": "2",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100032378.1.41",
          "form": "응 화이자가 좀 더 쎄다고 해서 걱정했는데 다행이야",
          "original_form": "응 화이자가 좀 더 쎄다고 해서 걱정했는데 다행이야",
          "speaker_id": "2",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100032378.1.42",
          "form": "근데 2차가 더 휴유증이 크다고 해서 걱정반 두려움 반임",
          "original_form": "근데 2차가 더 휴유증이 크다고 해서 걱정반 두려움 반임",
          "speaker_id": "2",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100032378.1.43",
          "form": "1차는 거의 무난하던데 2차는 진짜 복불복인거 같더라",
          "original_form": "1차는 거의 무난하던데 2차는 진짜 복불복인거 같더라",
          "speaker_id": "1",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100032378.1.44",
          "form": "우리집에는 아직 2차까지 접종한 사람이 없어서 걱정이야",
          "original_form": "우리집에는 아직 2차까지 접종한 사람이 없어서 걱정이야",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100032378.1.45",
          "form": "친구들 중에서 2차까지 접종한 애들은 별이상없었다는데 요즘 뉴스에서 하도 난리라",
          "original_form": "친구들 중에서 2차까지 접종한 애들은 별이상없었다는데 요즘 뉴스에서 하도 난리라",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032378.1.46",
          "form": "컨디션 조절 잘하면 괜찮을거야",
          "original_form": "컨디션 조절 잘하면 괜찮을거야",
          "speaker_id": "1",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100032378.1.47",
          "form": "근데 우리집도 부모님이 걱정이네",
          "original_form": "근데 우리집도 부모님이 걱정이네",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100032378.1.48",
          "form": "두분이서 같은 날에 접종만 안하시면 될것이여",
          "original_form": "두분이서 같은 날에 접종만 안하시면 될것이여",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
