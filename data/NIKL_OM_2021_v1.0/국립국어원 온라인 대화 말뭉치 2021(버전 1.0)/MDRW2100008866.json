      "utterance": [
        {
          "id": "MDRW2100008866.1.1",
          "form": "쇼핑 자주 하세요?",
          "original_form": "쇼핑 자주 하세요?",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008866.1.2",
          "form": "인터넷 쇼핑 자주해요",
          "original_form": "인터넷 쇼핑 자주해요",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008866.1.3",
          "form": "오늘도 4박스 받았습니다 ㅋㅋㅋ",
          "original_form": "오늘도 4박스 받았습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008866.1.4",
          "form": "아ㅋ 저와 비슷하세요",
          "original_form": "아ㅋ 저와 비슷하세요",
          "speaker_id": "1",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008866.1.5",
          "form": "뭐 사셨나요?",
          "original_form": "뭐 사셨나요?",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008866.1.6",
          "form": "전 구두요ㅋ",
          "original_form": "전 구두요ㅋ",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008866.1.7",
          "form": "필요없는 관상용",
          "original_form": "필요없는 관상용",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008866.1.8",
          "form": "오!! 본인을 위한 소비",
          "original_form": "오!! 본인을 위한 소비",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008866.1.9",
          "form": "전 라면 샀어요",
          "original_form": "전 라면 샀어요",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008866.1.10",
          "form": "ㅎㅎ 그것도 맛나요",
          "original_form": "ㅎㅎ 그것도 맛나요",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008866.1.11",
          "form": "9월부터 라면값이 오른다고 해서 미리 좀 쟁였어요",
          "original_form": "9월부터 라면값이 오른다고 해서 미리 좀 쟁였어요",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008866.1.12",
          "form": "전 구두를 산게 언제적인지...",
          "original_form": "전 구두를 산게 언제적인지...",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008866.1.13",
          "form": "사고싶은거 있으세요?",
          "original_form": "사고싶은거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008866.1.14",
          "form": "돈만 있으면 뭔들 못 사겠어요",
          "original_form": "돈만 있으면 뭔들 못 사겠어요",
          "speaker_id": "2",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008866.1.15",
          "form": "정답이에요",
          "original_form": "정답이에요",
          "speaker_id": "1",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008866.1.16",
          "form": "애플망고 가방 전복 등등",
          "original_form": "애플망고 가방 전복 등등",
          "speaker_id": "2",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008866.1.17",
          "form": "전복ㅋㄱ",
          "original_form": "전복ㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008866.1.18",
          "form": "진귀한것들을 사고 싶습니다",
          "original_form": "진귀한것들을 사고 싶습니다",
          "speaker_id": "2",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008866.1.19",
          "form": "전복이 엄청 싸게 팔아서 사먹었는데 또 사려고 보니 너무 비싼거예요 ㅠ",
          "original_form": "전복이 엄청 싸게 팔아서 사먹었는데 또 사려고 보니 너무 비싼거예요 ㅠ",
          "speaker_id": "2",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008866.1.20",
          "form": "사도사도 끝도없는",
          "original_form": "사도사도 끝도없는",
          "speaker_id": "1",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008866.1.21",
          "form": "옆방 샘들은 가방을 자꾸자꾸 사던데 저도 돈만 있으면 사고파요",
          "original_form": "옆방 샘들은 가방을 자꾸자꾸 사던데 저도 돈만 있으면 사고파요",
          "speaker_id": "2",
          "time": "20210822 19:56"
        },
        {
          "id": "MDRW2100008866.1.22",
          "form": "근데 가방은 이제 에코백이 젤 편하더라고요",
          "original_form": "근데 가방은 이제 에코백이 젤 편하더라고요",
          "speaker_id": "1",
          "time": "20210822 19:57"
        },
        {
          "id": "MDRW2100008866.1.23",
          "form": "맞아요 무거우면 손목 아프죠",
          "original_form": "맞아요 무거우면 손목 아프죠",
          "speaker_id": "2",
          "time": "20210822 19:59"
        },
        {
          "id": "MDRW2100008866.1.24",
          "form": "저도 제가 안들껄 아는데 사고 싶어요",
          "original_form": "저도 제가 안들껄 아는데 사고 싶어요",
          "speaker_id": "2",
          "time": "20210822 19:59"
        },
        {
          "id": "MDRW2100008866.1.25",
          "form": "전 전시장사서 채우려고요",
          "original_form": "전 전시장사서 채우려고요",
          "speaker_id": "1",
          "time": "20210822 20:02"
        },
        {
          "id": "MDRW2100008866.1.26",
          "form": "지정신 아니지요ㅠ",
          "original_form": "지정신 아니지요ㅠ",
          "speaker_id": "1",
          "time": "20210822 20:02"
        },
        {
          "id": "MDRW2100008866.1.27",
          "form": "전시장요?? ㅋㅋㅋㅋ",
          "original_form": "전시장요?? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:03"
        },
        {
          "id": "MDRW2100008866.1.28",
          "form": "알아보고있어요ㅋ",
          "original_form": "알아보고있어요ㅋ",
          "speaker_id": "1",
          "time": "20210822 20:03"
        },
        {
          "id": "MDRW2100008866.1.29",
          "form": "그냥 관상용 전시",
          "original_form": "그냥 관상용 전시",
          "speaker_id": "1",
          "time": "20210822 20:03"
        },
        {
          "id": "MDRW2100008866.1.30",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "1",
          "time": "20210822 20:03"
        },
        {
          "id": "MDRW2100008866.1.31",
          "form": "아~ 그래도 보고 있으면 흐믓할것 같아요 ㅎ",
          "original_form": "아~ 그래도 보고 있으면 흐믓할것 같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 20:03"
        },
        {
          "id": "MDRW2100008866.1.32",
          "form": "모하는건지 싶다가도",
          "original_form": "모하는건지 싶다가도",
          "speaker_id": "1",
          "time": "20210822 20:06"
        },
        {
          "id": "MDRW2100008866.1.33",
          "form": "보고있으면 행복",
          "original_form": "보고있으면 행복",
          "speaker_id": "1",
          "time": "20210822 20:07"
        },
        {
          "id": "MDRW2100008866.1.34",
          "form": "맞아요~ 행복을 돈으로 산다고 생각하면 되요 ㅋㅋ",
          "original_form": "맞아요~ 행복을 돈으로 산다고 생각하면 되요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:08"
        }
      ]
