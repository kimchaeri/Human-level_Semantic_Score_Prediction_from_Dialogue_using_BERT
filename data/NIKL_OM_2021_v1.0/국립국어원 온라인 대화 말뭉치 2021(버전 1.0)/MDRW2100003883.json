        {
          "id": "MDRW2100003883.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003883.1.2",
          "form": "여행 좋아하는데 최근에는 잘 못갔어요",
          "original_form": "여행 좋아하는데 최근에는 잘 못갔어요",
          "speaker_id": "2",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100003883.1.3",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "2",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100003883.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 13:29"
        },
        {
          "id": "MDRW2100003883.1.5",
          "form": "국내여행? 해외여행?",
          "original_form": "국내여행? 해외여행?",
          "speaker_id": "2",
          "time": "20210821 13:29"
        },
        {
          "id": "MDRW2100003883.1.6",
          "form": "해외요",
          "original_form": "해외요",
          "speaker_id": "1",
          "time": "20210821 13:30"
        },
        {
          "id": "MDRW2100003883.1.7",
          "form": "코로나때문에 가진 못하지만",
          "original_form": "코로나때문에 가진 못하지만",
          "speaker_id": "1",
          "time": "20210821 13:30"
        },
        {
          "id": "MDRW2100003883.1.8",
          "form": "해외 어디 가보셨나요",
          "original_form": "해외 어디 가보셨나요",
          "speaker_id": "2",
          "time": "20210821 13:31"
        },
        {
          "id": "MDRW2100003883.1.9",
          "form": "호즈요",
          "original_form": "호즈요",
          "speaker_id": "1",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100003883.1.10",
          "form": "ㅎ주",
          "original_form": "ㅎ주",
          "speaker_id": "1",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100003883.1.11",
          "form": "호주?",
          "original_form": "호주?",
          "speaker_id": "2",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100003883.1.12",
          "form": "호주갔을 때 뭐가 가장 좋았나요",
          "original_form": "호주갔을 때 뭐가 가장 좋았나요",
          "speaker_id": "2",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100003883.1.13",
          "form": "오페라 하우스 앞 식당 공연이요",
          "original_form": "오페라 하우스 앞 식당 공연이요",
          "speaker_id": "1",
          "time": "20210821 13:33"
        },
        {
          "id": "MDRW2100003883.1.14",
          "form": "어떤 공연을 보셨나요",
          "original_form": "어떤 공연을 보셨나요",
          "speaker_id": "2",
          "time": "20210821 13:33"
        },
        {
          "id": "MDRW2100003883.1.15",
          "form": "클래식인데 길거리공연도 좋다라구요",
          "original_form": "클래식인데 길거리공연도 좋다라구요",
          "speaker_id": "1",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100003883.1.16",
          "form": "동물같은것도 보셨나요?",
          "original_form": "동물같은것도 보셨나요?",
          "speaker_id": "2",
          "time": "20210821 13:35"
        },
        {
          "id": "MDRW2100003883.1.17",
          "form": "코알라나 캥거루요",
          "original_form": "코알라나 캥거루요",
          "speaker_id": "2",
          "time": "20210821 13:35"
        },
        {
          "id": "MDRW2100003883.1.18",
          "form": "네 눈앞에서 봤어여",
          "original_form": "네 눈앞에서 봤어여",
          "speaker_id": "1",
