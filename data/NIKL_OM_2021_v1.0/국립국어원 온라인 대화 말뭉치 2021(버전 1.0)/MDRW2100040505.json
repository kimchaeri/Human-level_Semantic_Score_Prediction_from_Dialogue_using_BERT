      "utterance": [
        {
          "id": "MDRW2100040505.1.1",
          "form": "칫솔을 새로 샀나",
          "original_form": "칫솔을 새로 샀나",
          "speaker_id": "2",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100040505.1.2",
          "form": "웅웅 있던거 온유가 다 갉아먹어서 ㅋㅋ",
          "original_form": "웅웅 있던거 온유가 다 갉아먹어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100040505.1.3",
          "form": "잘했다",
          "original_form": "잘했다",
          "speaker_id": "2",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100040505.1.4",
          "form": "칫솔 모양이 좀 특이함",
          "original_form": "칫솔 모양이 좀 특이함",
          "speaker_id": "1",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100040505.1.5",
          "form": "어린이 칫솔이야",
          "original_form": "어린이 칫솔이야",
          "speaker_id": "2",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100040505.1.6",
          "form": "아니 어른도 쓸수있는데 어금니 칫솔이라고 작게생김",
          "original_form": "아니 어른도 쓸수있는데 어금니 칫솔이라고 작게생김",
          "speaker_id": "1",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100040505.1.7",
          "form": "강아지가 써도 되나",
          "original_form": "강아지가 써도 되나",
          "speaker_id": "2",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100040505.1.8",
          "form": "웅웅 작아서 강아지 용으로 많이 쓴대",
          "original_form": "웅웅 작아서 강아지 용으로 많이 쓴대",
          "speaker_id": "1",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100040505.1.9",
          "form": "신기하네",
          "original_form": "신기하네",
          "speaker_id": "2",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100040505.1.10",
          "form": "웅웅 그래서 써봤는데 괜찮은듯",
          "original_form": "웅웅 그래서 써봤는데 괜찮은듯",
          "speaker_id": "1",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100040505.1.11",
          "form": "양치 잘 시켜 줘라",
          "original_form": "양치 잘 시켜 줘라",
          "speaker_id": "2",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100040505.1.12",
          "form": "웅웅 그래야지",
          "original_form": "웅웅 그래야지",
          "speaker_id": "1",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100040505.1.13",
          "form": "애들 치아가 깨끗해지겠다",
          "original_form": "애들 치아가 깨끗해지겠다",
          "speaker_id": "2",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100040505.1.14",
          "form": "강아지들 아직 안자나?",
          "original_form": "강아지들 아직 안자나?",
