      "utterance": [
        {
          "id": "MDRW2100027068.1.1",
          "form": "군대는 다녀오셨나요?",
          "original_form": "군대는 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100027068.1.2",
          "form": "미필자입니다",
          "original_form": "미필자입니다",
          "speaker_id": "2",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100027068.1.3",
          "form": "네 전 육군 출신",
          "original_form": "네 전 육군 출신",
          "speaker_id": "1",
          "time": "20210902 09:06"
        },
        {
          "id": "MDRW2100027068.1.4",
          "form": "군대 갈 생각하면 잠을 못 자요",
          "original_form": "군대 갈 생각하면 잠을 못 자요",
          "speaker_id": "2",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100027068.1.5",
          "form": "음 여자분 아니신가요?",
          "original_form": "음 여자분 아니신가요?",
          "speaker_id": "1",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100027068.1.6",
          "form": "아 어떻게 아셨어요 전 군대는 못 갈 것 같아요",
          "original_form": "아 어떻게 아셨어요 전 군대는 못 갈 것 같아요",
          "speaker_id": "2",
          "time": "20210902 09:11"
        },
        {
          "id": "MDRW2100027068.1.7",
          "form": "30대 여 라고 나와요.... 저는 티오디 했어요",
          "original_form": "30대 여 라고 나와요.... 저는 티오디 했어요",
          "speaker_id": "1",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100027068.1.8",
          "form": "티오디가 뭔가요",
          "original_form": "티오디가 뭔가요",
          "speaker_id": "2",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100027068.1.9",
          "form": "군대 메뉴 중에 뭘 가장 좋아하셨어요",
          "original_form": "군대 메뉴 중에 뭘 가장 좋아하셨어요",
          "speaker_id": "2",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100027068.1.10",
          "form": "흠 야간에 보는거요 그 안 보이는거",
          "original_form": "흠 야간에 보는거요 그 안 보이는거",
          "speaker_id": "1",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100027068.1.11",
          "form": "아 네 전 군대리아는 보았는데 뭘 좋아하셨어요",
          "original_form": "아 네 전 군대리아는 보았는데 뭘 좋아하셨어요",
          "speaker_id": "2",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027068.1.12",
          "form": "저는 간짬뽕이랑 맛다시요ㅋ 근데 꿀땅콩을 젤 많이 먹은듯요",
          "original_form": "저는 간짬뽕이랑 맛다시요ㅋ 근데 꿀땅콩을 젤 많이 먹은듯요",
          "speaker_id": "1",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027068.1.13",
          "form": "네예ㅎ 꿀땅콩도 군대에서 먹나보네요",
          "original_form": "네예ㅎ 꿀땅콩도 군대에서 먹나보네요",
          "speaker_id": "2",
          "time": "20210902 09:17"
        },
        {
          "id": "MDRW2100027068.1.14",
          "form": "네 뭐 싸제음식도 맛이먹지요",
          "original_form": "네 뭐 싸제음식도 맛이먹지요",
          "speaker_id": "1",
