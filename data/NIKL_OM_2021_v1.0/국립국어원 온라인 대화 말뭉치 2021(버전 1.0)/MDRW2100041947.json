      "utterance": [
        {
          "id": "MDRW2100041947.1.1",
          "form": "저도 얼른 움직여야 김치볶음밥 먹는데....ㅜㅜ",
          "original_form": "저도 얼른 움직여야 김치볶음밥 먹는데....ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041947.1.2",
          "form": "가장 좋아하는 배달 음식은 뭔가요?-?",
          "original_form": "가장 좋아하는 배달 음식은 뭔가요?-?",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041947.1.3",
          "form": "김치볶음밥도 맛있죠",
          "original_form": "김치볶음밥도 맛있죠",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041947.1.4",
          "form": "당연히 치킨이죠",
          "original_form": "당연히 치킨이죠",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041947.1.5",
          "form": "배달음식은ㅋㅋㅋ",
          "original_form": "배달음식은ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041947.1.6",
          "form": "그다음에는 중국집 짜장면 탕수워",
          "original_form": "그다음에는 중국집 짜장면 탕수워",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041947.1.7",
          "form": "육",
          "original_form": "육",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041947.1.8",
          "form": "근데 요즘은 진짜 다 배달이 되다보니까..",
          "original_form": "근데 요즘은 진짜 다 배달이 되다보니까..",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041947.1.9",
          "form": "저는 떡볶이랑 치킨이요",
          "original_form": "저는 떡볶이랑 치킨이요",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041947.1.10",
          "form": "요즘배달주문 장사잘될꺼에요",
          "original_form": "요즘배달주문 장사잘될꺼에요",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041947.1.11",
          "form": "배달라이더되게 바쁘던데요",
          "original_form": "배달라이더되게 바쁘던데요",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041947.1.12",
          "form": "밤늦게까지 배달하고",
          "original_form": "밤늦게까지 배달하고",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041947.1.13",
          "form": "맞아요 진짜!",
          "original_form": "맞아요 진짜!",
          "speaker_id": "2",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041947.1.14",
          "form": "온갖거 다 배달하고",
          "original_form": "온갖거 다 배달하고",
          "speaker_id": "2",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041947.1.15",
          "form": "주변에 자전거타고 배달하는거 봤어요",
          "original_form": "주변에 자전거타고 배달하는거 봤어요",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100041947.1.16",
          "form": "배달라이더많더라구요",
          "original_form": "배달라이더많더라구요",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100041947.1.17",
          "form": "그만큼사람들이 주문을많이해서",
          "original_form": "그만큼사람들이 주문을많이해서",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100041947.1.18",
          "form": "배달음식 문화가 진짜 발달한고 같아요",
          "original_form": "배달음식 문화가 진짜 발달한고 같아요",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041947.1.19",
          "form": "음료부터 술안주까지 다 배달하니까",
          "original_form": "음료부터 술안주까지 다 배달하니까",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041947.1.20",
          "form": "그니깐요!!배달라이더 하는 사람 꽤 있더라구요",
          "original_form": "그니깐요!!배달라이더 하는 사람 꽤 있더라구요",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100041947.1.21",
          "form": "배달의 민족~~",
          "original_form": "배달의 민족~~",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041947.1.22",
          "form": "요기요도있어요",
          "original_form": "요기요도있어요",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041947.1.23",
          "form": "어느배달사용해요?",
          "original_form": "어느배달사용해요?",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041947.1.24",
          "form": "저는 배민이용",
          "original_form": "저는 배민이용",
          "speaker_id": "2",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041947.1.25",
          "form": "역시 배민이 인기많네요",
          "original_form": "역시 배민이 인기많네요",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041947.1.26",
          "form": "자전거로 배민하는사람은 되게 어리던데",
          "original_form": "자전거로 배민하는사람은 되게 어리던데",
          "speaker_id": "1",
          "time": "20210906 18:26"
        }
      ]
