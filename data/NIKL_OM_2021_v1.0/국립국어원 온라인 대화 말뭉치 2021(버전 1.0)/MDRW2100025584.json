      "utterance": [
        {
          "id": "MDRW2100025584.1.1",
          "form": "최근에는 멸균우유 산 거 같네요",
          "original_form": "최근에는 멸균우유 산 거 같네요",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025584.1.2",
          "form": "오늘 왔음",
          "original_form": "오늘 왔음",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025584.1.3",
          "form": "오 우유 좋지",
          "original_form": "오 우유 좋지",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025584.1.4",
          "form": "나도 두유 사야하는데",
          "original_form": "나도 두유 사야하는데",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025584.1.5",
          "form": "생각난김에 주문해야겠다",
          "original_form": "생각난김에 주문해야겠다",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025584.1.6",
          "form": "매트리스 사고싶다",
          "original_form": "매트리스 사고싶다",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025584.1.7",
          "form": "다리 달린걸로",
          "original_form": "다리 달린걸로",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025584.1.8",
          "form": "바닥이 너무 불편하네요",
          "original_form": "바닥이 너무 불편하네요",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025584.1.9",
          "form": "다리달린 매트리스?",
          "original_form": "다리달린 매트리스?",
          "speaker_id": "1",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100025584.1.10",
          "form": "그런것도 있나ㅋㅋㅋ신기하네",
          "original_form": "그런것도 있나ㅋㅋㅋ신기하네",
          "speaker_id": "1",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100025584.1.11",
          "form": "나는 다린 없지만 매트리스 쓰는 중",
          "original_form": "나는 다린 없지만 매트리스 쓰는 중",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025584.1.12",
          "form": "역시 두꺼워야 좋더라",
          "original_form": "역시 두꺼워야 좋더라",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025584.1.13",
          "form": "청소할 때 힘들긴한데",
          "original_form": "청소할 때 힘들긴한데",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025584.1.14",
          "form": "침대같이 일체형 으로 다리 달린거임",
          "original_form": "침대같이 일체형 으로 다리 달린거임",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025584.1.15",
          "form": "그럭저럭 쓰는 중",
          "original_form": "그럭저럭 쓰는 중",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025584.1.16",
          "form": "글군",
          "original_form": "글군",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025584.1.17",
          "form": "방금",
          "original_form": "방금",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025584.1.18",
          "form": "머랭쿠키 샀다",
          "original_form": "머랭쿠키 샀다",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025584.1.19",
          "form": "오 멋져",
          "original_form": "오 멋져",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025584.1.20",
          "form": "나도 두유 사야해...",
          "original_form": "나도 두유 사야해...",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025584.1.21",
          "form": "맞다 가을이라 니트가디건 하나 골라둠",
          "original_form": "맞다 가을이라 니트가디건 하나 골라둠",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025584.1.22",
          "form": "근데 배송비 있어서 하나 더 사야해서 사이트 둘러보는 중",
          "original_form": "근데 배송비 있어서 하나 더 사야해서 사이트 둘러보는 중",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025584.1.23",
          "form": "니트가디건 좋군",
          "original_form": "니트가디건 좋군",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025584.1.24",
          "form": "나도 가을옷 하나 사고시파",
          "original_form": "나도 가을옷 하나 사고시파",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025584.1.25",
          "form": "사자사자",
          "original_form": "사자사자",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025584.1.26",
          "form": "그리고 긴바지가 몇 년전에 산거 밖에 없어서ㅋㅋㅋ것도 하나 사야함",
          "original_form": "그리고 긴바지가 몇 년전에 산거 밖에 없어서ㅋㅋㅋ것도 하나 사야함",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025584.1.27",
          "form": "내 기준 세상에서 제일 급함",
          "original_form": "내 기준 세상에서 제일 급함",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025584.1.28",
          "form": "근데 바지는 온오프 전부 성공한 적이 거의 없어서",
          "original_form": "근데 바지는 온오프 전부 성공한 적이 거의 없어서",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100025584.1.29",
          "form": "잘 고르려고 겁나게 고민 중임",
          "original_form": "잘 고르려고 겁나게 고민 중임",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100025584.1.30",
          "form": "바지는 성공한 데가 있긴 한데",
          "original_form": "바지는 성공한 데가 있긴 한데",
          "speaker_id": "2",
          "time": "20210901 16:07"
        },
        {
          "id": "MDRW2100025584.1.31",
          "form": "청바지 회사라서",
          "original_form": "청바지 회사라서",
          "speaker_id": "2",
          "time": "20210901 16:07"
        },
        {
          "id": "MDRW2100025584.1.32",
          "form": "슬랙스를 사고싶음",
          "original_form": "슬랙스를 사고싶음",
          "speaker_id": "2",
          "time": "20210901 16:07"
        },
        {
          "id": "MDRW2100025584.1.33",
          "form": "오 나는 지금 청바지 보는 중",
          "original_form": "오 나는 지금 청바지 보는 중",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025584.1.34",
          "form": "청반바지는 입어봤는데 청바지는 성인되고 입어본 적 거의 없는거 같아",
          "original_form": "청반바지는 입어봤는데 청바지는 성인되고 입어본 적 거의 없는거 같아",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025584.1.35",
          "form": "슬랙스도 좋지",
          "original_form": "슬랙스도 좋지",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025584.1.36",
          "form": "그것도 알아봐야겠다",
          "original_form": "그것도 알아봐야겠다",
          "speaker_id": "1",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100025584.1.37",
          "form": "가디건도 하나 샀으면 좋겠네",
          "original_form": "가디건도 하나 샀으면 좋겠네",
          "speaker_id": "2",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100025584.1.38",
          "form": "사자사자",
          "original_form": "사자사자",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100025584.1.39",
          "form": "가디건 베이지로 샀는데 사이즈가 낭낭해서 편해보이더라",
          "original_form": "가디건 베이지로 샀는데 사이즈가 낭낭해서 편해보이더라",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100025584.1.40",
          "form": "에잉 운동화도 사고싶고요",
          "original_form": "에잉 운동화도 사고싶고요",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100025584.1.41",
          "form": "운동화도 좋지 단 2개 있는데 하나는 구멍났고",
          "original_form": "운동화도 좋지 단 2개 있는데 하나는 구멍났고",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100025584.1.42",
          "form": "하나는 빨아뒀음",
          "original_form": "하나는 빨아뒀음",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100025584.1.43",
          "form": "발목 안아픈걸로 사고싶네",
          "original_form": "발목 안아픈걸로 사고싶네",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100025584.1.44",
          "form": "그리고 집이 사고싶다 나 혼자 살고싶네^^",
          "original_form": "그리고 집이 사고싶다 나 혼자 살고싶네^^",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100025584.1.45",
          "form": "그러면 발목 잡아주는 신발을 찾아봐야겠네",
          "original_form": "그러면 발목 잡아주는 신발을 찾아봐야겠네",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100025584.1.46",
          "form": "집...좋지...나는 땅문서도 왔으면...ㅎㅎㅎㅎㅎ",
          "original_form": "집...좋지...나는 땅문서도 왔으면...ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100025584.1.47",
          "form": "땅 좋더라 몇억정도만 사두면 좋겠던데 그만큼 돈이 없네 ㅎㅎ",
          "original_form": "땅 좋더라 몇억정도만 사두면 좋겠던데 그만큼 돈이 없네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100025584.1.48",
          "form": "그러게 이번주 로또 1등이 얼른 나한테 와야 하는데",
          "original_form": "그러게 이번주 로또 1등이 얼른 나한테 와야 하는데",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100025584.1.49",
          "form": "또 집나감",
          "original_form": "또 집나감",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100025584.1.50",
          "form": "진짜 모니터 중고로 하나 살까",
          "original_form": "진짜 모니터 중고로 하나 살까",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100025584.1.51",
          "form": "중고 모니터도 괜찮지",
          "original_form": "중고 모니터도 괜찮지",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100025584.1.52",
          "form": "화질만 좋으면 이즈 뭔들 아니겠어",
          "original_form": "화질만 좋으면 이즈 뭔들 아니겠어",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100025584.1.53",
          "form": "대단히 화질 좋을 필요도 없고",
          "original_form": "대단히 화질 좋을 필요도 없고",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100025584.1.54",
          "form": "당근마켓 가면 싸게 사던데",
          "original_form": "당근마켓 가면 싸게 사던데",
