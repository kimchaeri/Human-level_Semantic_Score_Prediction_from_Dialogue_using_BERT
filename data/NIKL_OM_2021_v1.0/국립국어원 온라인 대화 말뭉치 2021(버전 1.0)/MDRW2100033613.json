        {
          "id": "MDRW2100033613.1.1",
          "form": "name2님은 중고거래하세요?",
          "original_form": "&name2&님은 중고거래하세요?",
          "speaker_id": "1",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100033613.1.2",
          "form": "아뇨. 중고는 안해봤어요",
          "original_form": "아뇨. 중고는 안해봤어요",
          "speaker_id": "2",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100033613.1.3",
          "form": "그냥 기프티스타에 쿠폰정도만 팔아봤어요",
          "original_form": "그냥 기프티스타에 쿠폰정도만 팔아봤어요",
          "speaker_id": "2",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100033613.1.4",
          "form": "저도 당근마켓에 판매만 해요",
          "original_form": "저도 당근마켓에 판매만 해요",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100033613.1.5",
          "form": "요즘 당근에 gs25 마감상품도 팔더라구요",
          "original_form": "요즘 당근에 gs25 마감상품도 팔더라구요",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100033613.1.6",
          "form": "마감상품이 어떤거에요?",
          "original_form": "마감상품이 어떤거에요?",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100033613.1.7",
          "form": "설마 기한지난건 아닐꺼고 기한임박한거요?",
          "original_form": "설마 기한지난건 아닐꺼고 기한임박한거요?",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100033613.1.8",
          "form": "네  그런거 할인해서 팔던데요",
          "original_form": "네  그런거 할인해서 팔던데요",
          "speaker_id": "1",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100033613.1.9",
          "form": "연계상품인지",
          "original_form": "연계상품인지",
          "speaker_id": "1",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100033613.1.10",
          "form": "아~~~ 싸게 사면좋겠네요",
          "original_form": "아~~~ 싸게 사면좋겠네요",
          "speaker_id": "2",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100033613.1.11",
          "form": "전 유통기한은 별로신경안써서요",
          "original_form": "전 유통기한은 별로신경안써서요",
          "speaker_id": "2",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100033613.1.12",
          "form": "나중에 저녁쯤 함들어가보세요",
          "original_form": "나중에 저녁쯤 함들어가보세요",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100033613.1.13",
          "form": "어찌사는건지를 근데 모르겠더라구요",
          "original_form": "어찌사는건지를 근데 모르겠더라구요",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100033613.1.14",
          "form": "ㅎ 당근이 없어요",
          "original_form": "ㅎ 당근이 없어요",
          "speaker_id": "2",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100033613.1.15",
          "form": "진상들많데서 그냥 가입안했어요",
          "original_form": "진상들많데서 그냥 가입안했어요",
          "speaker_id": "2",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100033613.1.16",
          "form": "아 당근마켓 안하시는군요",
          "original_form": "아 당근마켓 안하시는군요",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100033613.1.17",
          "form": "전 아직 진상은 못봤어요",
          "original_form": "전 아직 진상은 못봤어요",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100033613.1.18",
          "form": "소수인데 그사람들이 글올려서 그런듯해요. 제가듣는 당근은 그사람들이말하는게 전부인듯 느껴져요",
          "original_form": "소수인데 그사람들이 글올려서 그런듯해요. 제가듣는 당근은 그사람들이말하는게 전부인듯 느껴져요",
          "speaker_id": "2",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100033613.1.19",
          "form": "저도 그런글많이 봤어요",
          "original_form": "저도 그런글많이 봤어요",
          "speaker_id": "1",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100033613.1.20",
          "form": "연락처달라해서 줬더니 유부남이 막 연락온대요",
          "original_form": "연락처달라해서 줬더니 유부남이 막 연락온대요",
          "speaker_id": "1",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100033613.1.21",
          "form": "헐~~~대박. 미쳤네요",
          "original_form": "헐~~~대박. 미쳤네요",
          "speaker_id": "2",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100033613.1.22",
          "form": "그래서 전번바꾸고 그랬다고 ㅡㅡ",
          "original_form": "그래서 전번바꾸고 그랬다고 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210824 17:26"
        },
        {
          "id": "MDRW2100033613.1.23",
          "form": "차단해도 당근으로 계속톡오고",
          "original_form": "차단해도 당근으로 계속톡오고",
