      "utterance": [
        {
          "id": "MDRW2100037387.1.1",
          "form": "요새 일기예보 이상하지안ㅅ아?",
          "original_form": "요새 일기예보 이상하지안ㅅ아?",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.2",
          "form": "뭐 매일 틀리는 거 같아",
          "original_form": "뭐 매일 틀리는 거 같아",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.3",
          "form": "응 진짜!!!",
          "original_form": "응 진짜!!!",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.4",
          "form": "오늘도 분명 오전부터 비가온댔거든",
          "original_form": "오늘도 분명 오전부터 비가온댔거든",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.5",
          "form": "오늘 하루종일 비온다고하더니",
          "original_form": "오늘 하루종일 비온다고하더니",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.6",
          "form": "근데 더웠어",
          "original_form": "근데 더웠어",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.7",
          "form": "니가 본거랑 내가 본거랑 또 다르구나",
          "original_form": "니가 본거랑 내가 본거랑 또 다르구나",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.8",
          "form": "그리고 비온다는 시간이 점점 늦춰지더니 아까 갑자기 쏟아지더라",
          "original_form": "그리고 비온다는 시간이 점점 늦춰지더니 아까 갑자기 쏟아지더라",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.9",
          "form": "진짜 우산을 머리에 이식하고싶어",
          "original_form": "진짜 우산을 머리에 이식하고싶어",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.10",
          "form": "우리 또 다른시간에 일기예보를 봤나봐",
          "original_form": "우리 또 다른시간에 일기예보를 봤나봐",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.11",
          "form": "비가 엄청 많이왔었지???",
          "original_form": "비가 엄청 많이왔었지???",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.12",
          "form": "아 그거참 신박하다",
          "original_form": "아 그거참 신박하다",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.13",
          "form": "응! 소리가 엄청났어",
          "original_form": "응! 소리가 엄청났어",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.14",
          "form": "좀 많이 시원한 소리였어",
          "original_form": "좀 많이 시원한 소리였어",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.15",
          "form": "나는 직접 보지는 못했지만",
          "original_form": "나는 직접 보지는 못했지만",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037387.1.16",
          "form": "길에 물웅덩이가 많더라",
          "original_form": "길에 물웅덩이가 많더라",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100037387.1.17",
          "form": "지금은 그쳤나봐. 시원하다 밤공기가.",
          "original_form": "지금은 그쳤나봐. 시원하다 밤공기가.",
          "speaker_id": "2",
          "time": "20210831 23:07"
        }
      ]
