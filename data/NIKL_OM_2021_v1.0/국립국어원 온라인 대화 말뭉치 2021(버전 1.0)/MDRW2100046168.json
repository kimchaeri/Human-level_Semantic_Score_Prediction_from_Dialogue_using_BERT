          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046168.1.9",
          "form": "오 샀어요?",
          "original_form": "오 샀어요?",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046168.1.10",
          "form": "판매아니구요?",
          "original_form": "판매아니구요?",
