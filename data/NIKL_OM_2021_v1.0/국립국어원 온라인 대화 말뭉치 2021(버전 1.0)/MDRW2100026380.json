        {
          "id": "MDRW2100026380.1.9",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026380.1.10",
          "form": "국내판은 방영하던거 잠깐잠깐 봤었는데",
