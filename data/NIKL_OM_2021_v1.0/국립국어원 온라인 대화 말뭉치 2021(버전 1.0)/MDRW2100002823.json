      "utterance": [
        {
          "id": "MDRW2100002823.1.1",
          "form": "이번 올림픽때 봤던 경기 있어요?",
          "original_form": "이번 올림픽때 봤던 경기 있어요?",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002823.1.2",
          "form": "배구",
          "original_form": "배구",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002823.1.3",
          "form": "넘재밌으염",
          "original_form": "넘재밌으염",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002823.1.4",
          "form": "식빵언니 너무 멋있졍",
          "original_form": "식빵언니 너무 멋있졍{emoji:♡}{emoji:♡}",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002823.1.5",
          "form": "멋졍~~",
          "original_form": "멋졍~~",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002823.1.6",
          "form": "양궁도",
          "original_form": "양궁도",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002823.1.7",
          "form": "저는 양궁 너무 심장 쫄깃쫄깃하게 봤어영",
          "original_form": "저는 양궁 너무 심장 쫄깃쫄깃하게 봤어영",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002823.1.8",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002823.1.9",
          "form": "어린애들이",
          "original_form": "어린애들이",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002823.1.10",
          "form": "어캐그리 잘하던지..",
          "original_form": "어캐그리 잘하던지..",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002823.1.11",
          "form": "실제로 경기장 가서\r\n직관해본  스포츠 있나여?ㅋㅋ",
          "original_form": "실제로 경기장 가서\r\n직관해본  스포츠 있나여?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002823.1.12",
          "form": "진짜 꼬맹이때",
          "original_form": "진짜 꼬맹이때",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002823.1.13",
          "form": "야구장",
          "original_form": "야구장",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002823.1.14",
          "form": "한번가봤어여",
          "original_form": "한번가봤어여",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002823.1.15",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002823.1.16",
          "form": "오 야구장ㅋㅋ",
          "original_form": "오 야구장ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002823.1.17",
          "form": "어디 응원하셨나여",
          "original_form": "어디 응원하셨나여",
          "speaker_id": "1",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002823.1.18",
          "form": "기억도 안나옄ㅋㅋㅋㅋㅋㅋ",
          "original_form": "기억도 안나옄ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002823.1.19",
          "form": "야구팀 있어여??",
          "original_form": "야구팀 있어여??",
          "speaker_id": "2",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002823.1.20",
          "form": "전없어서 ㅋㅋ",
          "original_form": "전없어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002823.1.21",
          "form": "응원하는팀이",
          "original_form": "응원하는팀이",
          "speaker_id": "2",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002823.1.22",
          "form": "저는 엘지트윈스요",
          "original_form": "저는 엘지트윈스요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210820 16:40"
        },
        {
          "id": "MDRW2100002823.1.23",
          "form": "코로나 때문에",
          "original_form": "코로나 때문에",
          "speaker_id": "1",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002823.1.24",
          "form": "직관 가본지도 엄청 오래 됐네염 ㅜㅜ",
          "original_form": "직관 가본지도 엄청 오래 됐네염 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002823.1.25",
          "form": "야구좋아하셨구낭",
          "original_form": "야구좋아하셨구낭",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002823.1.26",
          "form": "난 잘몰라서",
          "original_form": "난 잘몰라서",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002823.1.27",
          "form": "그래도 가면 재밌긴할듯 ㅋ",
          "original_form": "그래도 가면 재밌긴할듯 ㅋ",
          "speaker_id": "2",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002823.1.28",
          "form": "남편따라 다니기 시작했는데",
          "original_form": "남편따라 다니기 시작했는데",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002823.1.29",
          "form": "응원하며 스트레스 풉니다ㅋㅋ",
          "original_form": "응원하며 스트레스 풉니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002823.1.30",
          "form": "경기는 볼 줄 멀라여ㅋㅋㅋ",
          "original_form": "경기는 볼 줄 멀라여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002823.1.31",
          "form": "걍 응원가만 떼창 하다 옵니닼ㅋㅋㅋ",
          "original_form": "걍 응원가만 떼창 하다 옵니닼ㅋㅋㅋ",
