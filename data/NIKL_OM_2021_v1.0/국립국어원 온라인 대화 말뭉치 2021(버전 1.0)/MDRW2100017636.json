      "utterance": [
        {
          "id": "MDRW2100017636.1.1",
          "form": "영화 좋아하시나요",
          "original_form": "영화 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017636.1.2",
          "form": "네 좋아하죠!",
          "original_form": "네 좋아하죠!",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017636.1.3",
          "form": "어떤 영화보셨나요최근",
          "original_form": "어떤 영화보셨나요최근",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017636.1.4",
          "form": "블랙 위도우요!",
          "original_form": "블랙 위도우요!",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017636.1.5",
          "form": "오 저도봤어요!",
          "original_form": "오 저도봤어요!",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100017636.1.6",
          "form": "영화관에서 직접 직관했는데",
          "original_form": "영화관에서 직접 직관했는데",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100017636.1.7",
          "form": "재밌더라고요!",
          "original_form": "재밌더라고요!",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100017636.1.8",
          "form": "저도, 마블빠라ㅎㅎ",
          "original_form": "저도, 마블빠라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100017636.1.9",
          "form": "마블 영화 나오면 무조건 보는 타입이라 저도 ㅋㅋ",
          "original_form": "마블 영화 나오면 무조건 보는 타입이라 저도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100017636.1.10",
          "form": "이번에 11월달에 신작 나온다는데 나오면 볼려구요",
          "original_form": "이번에 11월달에 신작 나온다는데 나오면 볼려구요",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100017636.1.11",
          "form": "그 샹치맞나요??",
          "original_form": "그 샹치맞나요??",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100017636.1.12",
          "form": "아하 맞아용",
          "original_form": "아하 맞아용",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100017636.1.13",
          "form": "9월 1일 개봉이구나",
          "original_form": "9월 1일 개봉이구나",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100017636.1.14",
          "form": "아아 이게 아니구나",
          "original_form": "아아 이게 아니구나",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100017636.1.15",
          "form": "제가 말한건 이터널스 시놉시스?에용",
          "original_form": "제가 말한건 이터널스 시놉시스?에용",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100017636.1.16",
          "form": "이터널스!",
          "original_form": "이터널스!",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100017636.1.17",
          "form": "헉 동석이형 나오는거ㅎㅎ",
          "original_form": "헉 동석이형 나오는거ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100017636.1.18",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100017636.1.19",
          "form": "이번에 이터널스 엄청 기대중이에요..",
          "original_form": "이번에 이터널스 엄청 기대중이에요..",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100017636.1.20",
