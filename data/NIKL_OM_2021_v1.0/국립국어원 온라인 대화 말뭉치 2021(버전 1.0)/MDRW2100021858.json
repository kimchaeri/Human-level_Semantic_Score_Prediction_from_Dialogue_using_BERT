      "utterance": [
        {
          "id": "MDRW2100021858.1.1",
          "form": "금손분들 부럽 부럽 ㅎㅎ",
          "original_form": "금손분들 부럽 부럽 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021858.1.2",
          "form": "전 뭔가 살짝 부족해요ㅋㅋ",
          "original_form": "전 뭔가 살짝 부족해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021858.1.3",
          "form": "ㅎㅎㅎ 살짝 부족란게 어디에요",
          "original_form": "ㅎㅎㅎ 살짝 부족란게 어디에요",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021858.1.4",
          "form": "전 미용쪽은 진짜 똥손 ㅠㅠㅋ",
          "original_form": "전 미용쪽은 진짜 똥손 ㅠㅠㅋ",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021858.1.5",
          "form": "아??? ㅋㅋ 화장 잘하세요???",
          "original_form": "아??? ㅋㅋ 화장 잘하세요???",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021858.1.6",
          "form": "아뇨 그냥 정말  욕안먹을정도먼해요",
          "original_form": "아뇨 그냥 정말  욕안먹을정도먼해요",
          "speaker_id": "1",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021858.1.7",
          "form": "ㅋㅋ저도 파데만 겨우 발라요ㅋㅋ",
          "original_form": "ㅋㅋ저도 파데만 겨우 발라요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100021858.1.8",
          "form": "소질오 없고 ㅋㅋㅋ",
          "original_form": "소질오 없고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100021858.1.9",
          "form": "그래서 저 그거 했어요 반영구 ㅋ",
          "original_form": "그래서 저 그거 했어요 반영구 ㅋ",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100021858.1.10",
          "form": "아 그럼 반성공이죠! 안해도 준비된 얼굴!!",
          "original_form": "아 그럼 반성공이죠! 안해도 준비된 얼굴!!",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021858.1.11",
          "form": "눈썹이랑  아이라이너만요 ㅋ",
          "original_form": "눈썹이랑  아이라이너만요 ㅋ",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021858.1.12",
          "form": "그럼 진짜 준비되어계시네요! 전 출근하면 바로 뷰러ㅋㅋ",
          "original_form": "그럼 진짜 준비되어계시네요! 전 출근하면 바로 뷰러ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021858.1.13",
          "form": "근데 웃긴게 엄마가 하자해서 ㅋㅋ \r\n엄마가 더멋쟁이 이심",
          "original_form": "근데 웃긴게 엄마가 하자해서 ㅋㅋ \r\n엄마가 더멋쟁이 이심",
          "speaker_id": "1",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100021858.1.14",
          "form": "저는 안한다고 버티가 했는데 신세계라 2번 햇네요 ㅋㅋㅋ",
          "original_form": "저는 안한다고 버티가 했는데 신세계라 2번 햇네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100021858.1.15",
          "form": "아 완전 멋쟁이모녀신데요! 보톡스도 젊어서 맞아야한대요",
          "original_form": "아 완전 멋쟁이모녀신데요! 보톡스도 젊어서 맞아야한대요",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100021858.1.16",
          "form": "부모님의 은혜!!",
          "original_form": "부모님의 은혜!!",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100021858.1.17",
          "form": "넘길게요!",
          "original_form": "넘길게요!",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021858.1.18",
          "form": "안되네요ㅋㅋ 저도 하고는 싶은데 귀차니즘이 이겨요 늘...",
          "original_form": "안되네요ㅋㅋ 저도 하고는 싶은데 귀차니즘이 이겨요 늘...",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021858.1.19",
          "form": "ㅎㅎ 엄마가 멋재이니 혜택은 많네요",
          "original_form": "ㅎㅎ 엄마가 멋재이니 혜택은 많네요",
