      "utterance": [
        {
          "id": "MDRW2100014180.1.1",
          "form": "오늘 저녁은 치킨이다",
          "original_form": "오늘 저녁은 치킨이다",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014180.1.2",
          "form": "아들은 아직 오지않고....",
          "original_form": "아들은 아직 오지않고....",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014180.1.3",
          "form": "딸은 티비를 보고...",
          "original_form": "딸은 티비를 보고...",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014180.1.4",
          "form": "아직도 안왔아요??ㅋㅋㅋㅋㅋ",
          "original_form": "아직도 안왔아요??ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014180.1.5",
          "form": "태권도 최고네 진짜 ㅋㅋㅋㅋ",
          "original_form": "태권도 최고네 진짜 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014180.1.6",
          "form": "우린 뭐 오뎅탕으로 시마이 ㅋㅋㅋㅋㅋ7시되면 씻으러가기러 ㅋㅋㅋㅋ",
          "original_form": "우린 뭐 오뎅탕으로 시마이 ㅋㅋㅋㅋㅋ7시되면 씻으러가기러 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014180.1.7",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014180.1.8",
          "form": "아니 태권도 갔다가",
          "original_form": "아니 태권도 갔다가",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014180.1.9",
          "form": "놀이터에 놀러 갔지",
          "original_form": "놀이터에 놀러 갔지",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014180.1.10",
          "form": "이제 와서 씻고",
          "original_form": "이제 와서 씻고",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014180.1.11",
          "form": "밥 먹을 준비 중",
          "original_form": "밥 먹을 준비 중",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014180.1.12",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014180.1.13",
          "form": "신나게 놀아서 밥맛은 좋겠군여 ㅋㅋㅋㅋㅋ",
          "original_form": "신나게 놀아서 밥맛은 좋겠군여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014180.1.14",
          "form": "신나게 놀아도 밥은 안 먹음",
          "original_form": "신나게 놀아도 밥은 안 먹음",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014180.1.15",
          "form": "맛있게 먹고 오늘은 일찍 육퇴합시당 ㅋㅋㅋㅋ",
          "original_form": "맛있게 먹고 오늘은 일찍 육퇴합시당 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014180.1.16",
          "form": "우리 투는 요새 맨날 친구집",
          "original_form": "우리 투는 요새 맨날 친구집",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014180.1.17",
          "form": "가자고 난리",
          "original_form": "가자고 난리",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014180.1.18",
          "form": "아니면 친구를 데려오던지",
          "original_form": "아니면 친구를 데려오던지",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014180.1.19",
          "form": "힘들다 다섯 살",
          "original_form": "힘들다 다섯 살",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014180.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ우리 하긴 둘찌는",
          "original_form": "ㅋㅋㅋㅋㅋㅋ우리 하긴 둘찌는",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014180.1.21",
          "form": "친구들이랑 잘 안놀려주니깐 ㅋㅋㅋㅋㅋ",
          "original_form": "친구들이랑 잘 안놀려주니깐 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014180.1.22",
          "form": "다 첫째위주임 ㅋㅋㅋㅋㅋ",
          "original_form": "다 첫째위주임 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014180.1.23",
          "form": "그렇지 우리 투도 오빠 친구들이랑만 놀다가",
          "original_form": "그렇지 우리 투도 오빠 친구들이랑만 놀다가",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014180.1.24",
          "form": "유치원 가고 본인 친구한테 재미들림",
          "original_form": "유치원 가고 본인 친구한테 재미들림",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014180.1.25",
          "form": "한번 서로 집을 왕래하니",
          "original_form": "한번 서로 집을 왕래하니",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014180.1.26",
          "form": "계속 서로 집 가자고 난리",
          "original_form": "계속 서로 집 가자고 난리",
          "speaker_id": "2",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014180.1.27",
          "form": "굴쵸~그 맛을 알면 맨날 우리집 오라고 ㅋㅋㅋㅋ",
          "original_form": "굴쵸~그 맛을 알면 맨날 우리집 오라고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014180.1.28",
          "form": "애들이 참았다가 다 폭발했네 ㅋㅋㅋㅋ",
          "original_form": "애들이 참았다가 다 폭발했네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014180.1.29",
          "form": "그러이 지금 폭발했음",
          "original_form": "그러이 지금 폭발했음",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014180.1.30",
          "form": "하원 하면 전쟁 임",
          "original_form": "하원 하면 전쟁 임",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014180.1.31",
          "form": "언니 무섭겠구만요 ㅋㅋㅋㅋ",
          "original_form": "언니 무섭겠구만요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014180.1.32",
          "form": "어 이제 원은 알아서 노는데",
          "original_form": "어 이제 원은 알아서 노는데",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014180.1.33",
          "form": "투 때문에  피곤하군",
          "original_form": "투 때문에  피곤하군",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014180.1.34",
          "form": "요새 왜 이래 늦게 자노??",
          "original_form": "요새 왜 이래 늦게 자노??",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014180.1.35",
          "form": "7시30분에 애들 자섴;;",
          "original_form": "7시30분에 애들 자섴;;",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014180.1.36",
          "form": "오늘 타이밍이 안맞았어요ㅋㅋㅋㅋㅋ",
          "original_form": "오늘 타이밍이 안맞았어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014180.1.37",
          "form": "진짜 일찍 자 네",
          "original_form": "진짜 일찍 자 네",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014180.1.38",
          "form": "언제 잘려고??",
          "original_form": "언제 잘려고??",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014180.1.39",
          "form": "9시30분쯤요??ㅋㅋㅋㅋㅋ",
          "original_form": "9시30분쯤요??ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014180.1.40",
          "form": "요즘 새벽이슬 맞고 다니는 수준임 ㅋㅋㅋ",
          "original_form": "요즘 새벽이슬 맞고 다니는 수준임 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014180.1.41",
          "form": "장난없네",
          "original_form": "장난없네",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014180.1.42",
          "form": "내일부터 개학이라 일찍 일어나야 되는군",
          "original_form": "내일부터 개학이라 일찍 일어나야 되는군",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014180.1.43",
          "form": "그래서 오늘은 개학 전야제로다가 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 오늘은 개학 전야제로다가 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:53"
        }
      ]
