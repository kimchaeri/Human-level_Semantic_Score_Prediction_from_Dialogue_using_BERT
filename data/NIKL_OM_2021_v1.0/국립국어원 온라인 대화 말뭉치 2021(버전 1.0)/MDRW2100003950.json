      "utterance": [
        {
          "id": "MDRW2100003950.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 14:07"
        },
        {
          "id": "MDRW2100003950.1.2",
          "form": "요즘 노래 뭐 듣나요",
          "original_form": "요즘 노래 뭐 듣나요",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003950.1.3",
          "form": "특정노래 듣는건 없구",
          "original_form": "특정노래 듣는건 없구",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003950.1.4",
          "form": "스포티파이에서 랜덤 노래..?",
          "original_form": "스포티파이에서 랜덤 노래..?",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003950.1.5",
          "form": "주로 듣는것같아요",
          "original_form": "주로 듣는것같아요",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003950.1.6",
          "form": "오 스포티파이!!",
          "original_form": "오 스포티파이!!",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003950.1.7",
          "form": "저 유튜브프리미엄 쓰는데",
          "original_form": "저 유튜브프리미엄 쓰는데",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003950.1.8",
          "form": "갈아탈만한가요?",
          "original_form": "갈아탈만한가요?",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003950.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003950.1.10",
          "form": "저는 막귀라 잘 모르겠는데",
          "original_form": "저는 막귀라 잘 모르겠는데",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003950.1.11",
          "form": "음질이 확실히 더 좋다고 하더라구요",
          "original_form": "음질이 확실히 더 좋다고 하더라구요",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003950.1.12",
          "form": "저는 제 취향 노래 추천 잘 해줘서",
          "original_form": "저는 제 취향 노래 추천 잘 해줘서",
          "speaker_id": "1",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003950.1.13",
          "form": "잘쓰고있는것같아요",
          "original_form": "잘쓰고있는것같아요",
          "speaker_id": "1",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003950.1.14",
          "form": "아 음질이 어플에 따라서 바뀐단 것두",
          "original_form": "아 음질이 어플에 따라서 바뀐단 것두",
          "speaker_id": "2",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003950.1.15",
          "form": "처음 알았어요ㅋㅋ",
          "original_form": "처음 알았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003950.1.16",
          "form": "만원초반대죠?",
          "original_form": "만원초반대죠?",
          "speaker_id": "2",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003950.1.17",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003950.1.18",
          "form": "가격이 좀 있긴한데",
          "original_form": "가격이 좀 있긴한데",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003950.1.19",
          "form": "해외 음악도 많아서 전 좋더라구요",
          "original_form": "해외 음악도 많아서 전 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003950.1.20",
          "form": "일할때는 한국음악보다 외국음악 틀어놓는게 집중이 잘되서",
          "original_form": "일할때는 한국음악보다 외국음악 틀어놓는게 집중이 잘되서",
          "speaker_id": "1",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003950.1.21",
          "form": "아 역시",
          "original_form": "아 역시",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003950.1.22",
          "form": "외국음악 많이들으면 스포티 많이 쓰더라구요",
          "original_form": "외국음악 많이들으면 스포티 많이 쓰더라구요",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003950.1.23",
          "form": "전 국내인디음악만 들어서 ㅜㅜ",
          "original_form": "전 국내인디음악만 들어서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003950.1.24",
          "form": "ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003950.1.25",
          "form": "그러면 유튜브 프리미엄 그대로 쓰는것도 나쁘지않을것 같아요..ㅎㅎ",
          "original_form": "그러면 유튜브 프리미엄 그대로 쓰는것도 나쁘지않을것 같아요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003950.1.26",
          "form": "그쵸?ㅋㅋㅋㅋ나중에 취향이 좀 더 고급스러워지면",
          "original_form": "그쵸?ㅋㅋㅋㅋ나중에 취향이 좀 더 고급스러워지면",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003950.1.27",
          "form": "스포티파이로 갈아탈게요",
          "original_form": "스포티파이로 갈아탈게요",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003950.1.28",
          "form": "둘다쓰기엔 돈이 ㅜ",
          "original_form": "둘다쓰기엔 돈이 ㅜ",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003950.1.29",
          "form": "ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003950.1.30",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003950.1.31",
          "form": "구독료로 나가는돈들 은근히 많아서",
          "original_form": "구독료로 나가는돈들 은근히 많아서",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003950.1.32",
          "form": "절약 잘해야되요..ㅠㅠ",
          "original_form": "절약 잘해야되요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003950.1.33",
          "form": "ㅋㅋㅋ스트리밍시대라고들하니까",
          "original_form": "ㅋㅋㅋ스트리밍시대라고들하니까",
          "speaker_id": "2",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003950.1.34",
          "form": "또 뭐 구독하는거 있으세요?",
          "original_form": "또 뭐 구독하는거 있으세요?",
          "speaker_id": "2",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003950.1.35",
          "form": "저는 ebs마스터?",
          "original_form": "저는 ebs마스터?",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003950.1.36",
          "form": "구독은 하고있는데 잘 안보고있네요",
          "original_form": "구독은 하고있는데 잘 안보고있네요",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003950.1.37",
          "form": "그리고 넷플릭스는 필수죠!",
          "original_form": "그리고 넷플릭스는 필수죠!",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003950.1.38",
          "form": "은근 교육적이네요",
          "original_form": "은근 교육적이네요",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003950.1.39",
          "form": "저 넷플릭스는 4인팟으로 봐요ㅋㅋ절약!!!",
          "original_form": "저 넷플릭스는 4인팟으로 봐요ㅋㅋ절약!!!",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003950.1.40",
          "form": "토스프라임두",
          "original_form": "토스프라임두",
          "speaker_id": "2",
          "time": "20210821 14:24"
        },
        {
          "id": "MDRW2100003950.1.41",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003950.1.42",
          "form": "토스프라임은 처음들어봐요",
          "original_form": "토스프라임은 처음들어봐요",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003950.1.43",
          "form": "토스?",
          "original_form": "토스?",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003950.1.44",
          "form": "그냥 저 카드를 토스로만 써서",
          "original_form": "그냥 저 카드를 토스로만 써서",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003950.1.45",
          "form": "그거랑 연관된건가요?",
          "original_form": "그거랑 연관된건가요?",
          "speaker_id": "1",
          "time": "20210821 14:26"
