      "utterance": [
        {
          "id": "MDRW2100021386.1.1",
          "form": "운동 하세요?",
          "original_form": "운동 하세요?",
          "speaker_id": "2",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100021386.1.2",
          "form": "아녀... 회사일이 너무빡세서 다닐 기운이 안나여 허허허",
          "original_form": "아녀... 회사일이 너무빡세서 다닐 기운이 안나여 허허허",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021386.1.3",
          "form": "님은요? 어떠세여",
          "original_form": "님은요? 어떠세여",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021386.1.4",
          "form": "저는 홈트 조금씩 하고 있어요",
          "original_form": "저는 홈트 조금씩 하고 있어요",
          "speaker_id": "2",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100021386.1.5",
          "form": "홈트~  부지런하시네요",
          "original_form": "홈트~  부지런하시네요",
          "speaker_id": "1",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100021386.1.6",
          "form": "저는 옆에서 봐줘야 하는 타입이라 ㅋㅋㅋ",
          "original_form": "저는 옆에서 봐줘야 하는 타입이라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100021386.1.7",
          "form": "코로나가 좀 줄어들어야 센터에 등록을 할텐데",
          "original_form": "코로나가 좀 줄어들어야 센터에 등록을 할텐데",
          "speaker_id": "1",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100021386.1.8",
          "form": "아직은 모여서 운동하기 힘들죠ㅠ",
          "original_form": "아직은 모여서 운동하기 힘들죠ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100021386.1.9",
          "form": "그래서 그냥 먹는 것만 관리하려구요",
          "original_form": "그래서 그냥 먹는 것만 관리하려구요",
          "speaker_id": "1",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100021386.1.10",
          "form": "와 식단 관리가 더 힘드실텐데",
          "original_form": "와 식단 관리가 더 힘드실텐데",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100021386.1.11",
          "form": "한끼정도 셀러드를 먹으면 좀 건강해지지 않을까요 ㅋㅋㅋ",
          "original_form": "한끼정도 셀러드를 먹으면 좀 건강해지지 않을까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100021386.1.12",
          "form": "풀때기만 먹고 몸을 움직일 수는 없어요... 인간은 초식동물이 아니라구요",
          "original_form": "풀때기만 먹고 몸을 움직일 수는 없어요... 인간은 초식동물이 아니라구요",
          "speaker_id": "2",
          "time": "20210830 12:31"
        },
        {
          "id": "MDRW2100021386.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋ 풀ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋ 풀ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:44"
        },
        {
          "id": "MDRW2100021386.1.14",
          "form": "확고하시네요",
          "original_form": "확고하시네요",
          "speaker_id": "1",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100021386.1.15",
          "form": "육식파시군요",
          "original_form": "육식파시군요",
          "speaker_id": "1",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100021386.1.16",
          "form": "정확히는 잡식파입니다 막상 고기 먹을때 풀 없으면 못먹어서ㅋㅋ",
          "original_form": "정확히는 잡식파입니다 막상 고기 먹을때 풀 없으면 못먹어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100021386.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 모든지 적당히가 좋긴하죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 모든지 적당히가 좋긴하죠",
          "speaker_id": "1",
          "time": "20210830 14:30"
        },
        {
          "id": "MDRW2100021386.1.18",
          "form": "저는 원체 잘 안먹어서 식비도 많이 안나가고 그냥 좋게 챙겨먹으려구요",
          "original_form": "저는 원체 잘 안먹어서 식비도 많이 안나가고 그냥 좋게 챙겨먹으려구요",
          "speaker_id": "1",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100021386.1.19",
          "form": "건강해지겠죠 ㅋㅋㅋ",
          "original_form": "건강해지겠죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100021386.1.20",
          "form": "잘 안먹으신다니요ㅠ 활동량보다 좀 더 먹어야지 현상 유지가 좋을텐데요",
          "original_form": "잘 안먹으신다니요ㅠ 활동량보다 좀 더 먹어야지 현상 유지가 좋을텐데요",
          "speaker_id": "2",
          "time": "20210831 11:11"
        },
        {
          "id": "MDRW2100021386.1.21",
          "form": "욕심도 없어서 그냥 먹으면 끝이거든요",
          "original_form": "욕심도 없어서 그냥 먹으면 끝이거든요",
