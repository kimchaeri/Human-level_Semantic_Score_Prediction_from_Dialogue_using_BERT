      "utterance": [
        {
          "id": "MDRW2100039778.1.1",
          "form": "구매 예정 있으세요?",
          "original_form": "구매 예정 있으세요?",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039778.1.2",
          "form": "전 태블릿이나 노트북 사고싶어요!",
          "original_form": "전 태블릿이나 노트북 사고싶어요!",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039778.1.3",
          "form": "오 어느 브랜드 생각 중이세요?",
          "original_form": "오 어느 브랜드 생각 중이세요?",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039778.1.4",
          "form": "태블릿은 삼성 갤럭시탭 s7 생각중이고",
          "original_form": "태블릿은 삼성 갤럭시탭 s7 생각중이고",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039778.1.5",
          "form": "노트북은 잘 모르겠어요!",
          "original_form": "노트북은 잘 모르겠어요!",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039778.1.6",
          "form": "가벼운 건 엘지 좋은 것 같아요 ㅎㅎ 매장 갔는데 정말 가볍더라구요",
          "original_form": "가벼운 건 엘지 좋은 것 같아요 ㅎㅎ 매장 갔는데 정말 가볍더라구요",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039778.1.7",
          "form": "맞아요 워낙 가벼워서",
          "original_form": "맞아요 워낙 가벼워서",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039778.1.8",
          "form": "주변에 노트북 들고 다니는 사람들",
          "original_form": "주변에 노트북 들고 다니는 사람들",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039778.1.9",
          "form": "절반 이상 엘지 그램을 쓰더라구요",
          "original_form": "절반 이상 엘지 그램을 쓰더라구요",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039778.1.10",
          "form": "저는 무게보단 성능파라 좀 망설여져요ㅋㅋㅋ",
          "original_form": "저는 무게보단 성능파라 좀 망설여져요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039778.1.11",
          "form": "성능파시면 잘 고려하시고 구매해야겠네요",
          "original_form": "성능파시면 잘 고려하시고 구매해야겠네요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039778.1.12",
          "form": "게이밍 노트북이 성능은 최고던데",
          "original_form": "게이밍 노트북이 성능은 최고던데",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039778.1.13",
          "form": "너무 비싸더라구요ㅠㅠ",
          "original_form": "너무 비싸더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039778.1.14",
          "form": "게이밍 붙으면 좋더라구요 의자 마우스 키보드 등등",
          "original_form": "게이밍 붙으면 좋더라구요 의자 마우스 키보드 등등",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039778.1.15",
          "form": "게이밍 의자는 너무 편해보여서 꼭 사고 싶어요ㅋㅋㅋ",
          "original_form": "게이밍 의자는 너무 편해보여서 꼭 사고 싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039778.1.16",
          "form": "손목이 안좋아서 마우스는 버티컬로 샀지만",
          "original_form": "손목이 안좋아서 마우스는 버티컬로 샀지만",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039778.1.17",
          "form": "버티컬 마우스 손목에 좋나요?",
          "original_form": "버티컬 마우스 손목에 좋나요?",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039778.1.18",
          "form": "이미 망한 손목을 복구해주진 않지만",
          "original_form": "이미 망한 손목을 복구해주진 않지만",
          "speaker_id": "1",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039778.1.19",
          "form": "악화되는건 막아주는 것 같아요!",
          "original_form": "악화되는건 막아주는 것 같아요!",
          "speaker_id": "1",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039778.1.20",
          "form": "그래서 이왕이면 손목 괜찮을때 투자 좀 해두라고들 하더라구요!",
          "original_form": "그래서 이왕이면 손목 괜찮을때 투자 좀 해두라고들 하더라구요!",
          "speaker_id": "1",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039778.1.21",
          "form": "오 찜해놔야 겠어요",
          "original_form": "오 찜해놔야 겠어요",
          "speaker_id": "2",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039778.1.22",
          "form": "예전엔 비쌌다던데 요즘은 앵커나 코시같이 저렴한 브랜드도 많더라구요",
          "original_form": "예전엔 비쌌다던데 요즘은 앵커나 코시같이 저렴한 브랜드도 많더라구요",
          "speaker_id": "1",
          "time": "20210903 22:01"
        }
      ]
    }
