      "utterance": [
        {
          "id": "MDRW2100028028.1.1",
          "form": "게임 좋아하세여?",
          "original_form": "게임 좋아하세여?",
          "speaker_id": "1",
          "time": "20210906 11:32"
        },
        {
          "id": "MDRW2100028028.1.2",
          "form": "네! 완전 좋아해요! 근데 귀찮아하는..",
          "original_form": "네! 완전 좋아해요! 근데 귀찮아하는..",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100028028.1.3",
          "form": "무슨 게임 하세요?",
          "original_form": "무슨 게임 하세요?",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100028028.1.4",
          "form": "각종 리듬게임이요!!",
          "original_form": "각종 리듬게임이요!!",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100028028.1.5",
          "form": "오... 아이돌 곡 나오는건가요?",
          "original_form": "오... 아이돌 곡 나오는건가요?",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100028028.1.6",
          "form": "광고 많던데요",
          "original_form": "광고 많던데요",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100028028.1.7",
          "form": "앗 그것도 해요! 사실 최근에 다시 깔았어요ㅋㅋ",
          "original_form": "앗 그것도 해요! 사실 최근에 다시 깔았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100028028.1.8",
          "form": "잘 하시나요? 리듬게임 잘 못해서 별로 안하거든요.",
          "original_form": "잘 하시나요? 리듬게임 잘 못해서 별로 안하거든요.",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100028028.1.9",
          "form": "그냥 보통 정도만 하는 것 같아요! 리듬게임도 잠깐만 하다가 빨리 질려서ㅜㅜ",
          "original_form": "그냥 보통 정도만 하는 것 같아요! 리듬게임도 잠깐만 하다가 빨리 질려서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100028028.1.10",
          "form": "좀 그렇긴 하져ㅋㅋ 오래 하는 사람들도 많더라구요.",
          "original_form": "좀 그렇긴 하져ㅋㅋ 오래 하는 사람들도 많더라구요.",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100028028.1.11",
          "form": "진짜요! 게임 오래하는 분들 너무 신기해요",
          "original_form": "진짜요! 게임 오래하는 분들 너무 신기해요",
          "speaker_id": "2",
          "time": "20210907 11:46"
        },
        {
          "id": "MDRW2100028028.1.12",
          "form": "요새는 재밌는 게임이 새로 안나오는거 같아요",
          "original_form": "요새는 재밌는 게임이 새로 안나오는거 같아요",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100028028.1.13",
          "form": "맞아요 이거 진짜 인정합니다... 다 거기서 거기더라구요",
          "original_form": "맞아요 이거 진짜 인정합니다... 다 거기서 거기더라구요",
          "speaker_id": "2",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100028028.1.14",
          "form": "하던 게임만 계속 하고 있어여ㅋㅋ",
          "original_form": "하던 게임만 계속 하고 있어여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100028028.1.15",
          "form": "진짜요ㅋㅋ 새게임 시작해도 결국 다시 돌아오는..",
          "original_form": "진짜요ㅋㅋ 새게임 시작해도 결국 다시 돌아오는..",
          "speaker_id": "2",
          "time": "20210907 12:23"
        }
      ]
