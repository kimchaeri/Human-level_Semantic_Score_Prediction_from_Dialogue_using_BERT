      "utterance": [
        {
          "id": "MDRW2100031490.1.1",
          "form": "너무어릴때는 딱히 뭐안해준듯..",
          "original_form": "너무어릴때는 딱히 뭐안해준듯..",
          "speaker_id": "2",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100031490.1.2",
          "form": "내가 먹는거밖에 생각안남 크로플 이생각뿐",
          "original_form": "내가 먹는거밖에 생각안남 크로플 이생각뿐",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100031490.1.3",
          "form": "그냥 언니꺼 해주고그랬던거같어",
          "original_form": "그냥 언니꺼 해주고그랬던거같어",
          "speaker_id": "2",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100031490.1.4",
          "form": "산모쪽??",
          "original_form": "산모쪽??",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031490.1.5",
          "form": "새들러크로플 짱맛",
          "original_form": "새들러크로플 짱맛",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031490.1.6",
          "form": "먹을꺼해줘 고고",
          "original_form": "먹을꺼해줘 고고",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031490.1.7",
          "form": "잘먹어야디",
          "original_form": "잘먹어야디",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031490.1.8",
          "form": "음 그건 명절마다 소소하게 챙겻는디",
          "original_form": "음 그건 명절마다 소소하게 챙겻는디",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031490.1.9",
          "form": "그래? 크로플 사줄까",
          "original_form": "그래? 크로플 사줄까",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031490.1.10",
          "form": "그래도될둣",
          "original_form": "그래도될둣",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031490.1.11",
          "form": "밀가루 디저트 좋아하긴하던데 치즈랑",
          "original_form": "밀가루 디저트 좋아하긴하던데 치즈랑",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031490.1.12",
          "form": "굿굿 투홈에서 질러야겠다",
          "original_form": "굿굿 투홈에서 질러야겠다",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031490.1.13",
          "form": "애본다고 본인챙기기 힘들시기라..",
          "original_form": "애본다고 본인챙기기 힘들시기라..",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031490.1.14",
          "form": "고마워할듯",
          "original_form": "고마워할듯",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031490.1.15",
          "form": "음 근데 애기꺼 하나쯩은 챙겨야겠다",
          "original_form": "음 근데 애기꺼 하나쯩은 챙겨야겠다",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031490.1.16",
          "form": "내복정도.,..",
          "original_form": "내복정도.,..",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031490.1.17",
          "form": "웅웅 그러고보니",
          "original_form": "웅웅 그러고보니",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031490.1.18",
          "form": "아는동생 애기때",
          "original_form": "아는동생 애기때",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031490.1.19",
          "form": "꿍방지템 사줫는데 엄청좋아햇엇음",
          "original_form": "꿍방지템 사줫는데 엄청좋아햇엇음",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031490.1.20",
          "form": "꿍방지템?",
          "original_form": "꿍방지템?",
          "speaker_id": "1",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100031490.1.21",
          "form": "애들 앉아잇다가 뒤로 넘어가거든..",
          "original_form": "애들 앉아잇다가 뒤로 넘어가거든..",
          "speaker_id": "2",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100031490.1.22",
          "form": "머리부딧히는거 방지하는거야",
          "original_form": "머리부딧히는거 방지하는거야",
          "speaker_id": "2",
          "time": "20210904 22:06"
        }
