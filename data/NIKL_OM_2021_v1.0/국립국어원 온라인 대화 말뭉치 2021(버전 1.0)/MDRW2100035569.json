      "utterance": [
        {
          "id": "MDRW2100035569.1.1",
          "form": "안녕하세용 즐겨보시는 티비 프로그램 있으신가요?",
          "original_form": "안녕하세용 즐겨보시는 티비 프로그램 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100035569.1.2",
          "form": "저 방금 집사부일체 봤어요",
          "original_form": "저 방금 집사부일체 봤어요",
          "speaker_id": "2",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100035569.1.3",
          "form": "양궁선수들 멋지더라고요!",
          "original_form": "양궁선수들 멋지더라고요!",
          "speaker_id": "2",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100035569.1.4",
          "form": "아하 집사부일체에 선수분들 나오셨나보네요",
          "original_form": "아하 집사부일체에 선수분들 나오셨나보네요",
          "speaker_id": "1",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100035569.1.5",
          "form": "저는 1박2일 봤어용!ㅎㅎ",
          "original_form": "저는 1박2일 봤어용!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100035569.1.6",
          "form": "오 오늘은 뭔내용이였어요?",
          "original_form": "오 오늘은 뭔내용이였어요?",
          "speaker_id": "2",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100035569.1.7",
          "form": "저는 초반에 가끔봤어요 딘딘이 까나리 마시던 시절에",
          "original_form": "저는 초반에 가끔봤어요 딘딘이 까나리 마시던 시절에",
          "speaker_id": "2",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100035569.1.8",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100035569.1.9",
          "form": "오늘은 물놀이하고 초계국수 먹고",
          "original_form": "오늘은 물놀이하고 초계국수 먹고",
          "speaker_id": "1",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100035569.1.10",
          "form": "문세윤 개그맨이",
          "original_form": "문세윤 개그맨이",
          "speaker_id": "1",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100035569.1.11",
          "form": "신곡 음원 낸거 아세용?",
          "original_form": "신곡 음원 낸거 아세용?",
          "speaker_id": "1",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100035569.1.12",
          "form": "오 ㅋㅋ초계국수 맛있겠어요",
          "original_form": "오 ㅋㅋ초계국수 맛있겠어요",
          "speaker_id": "2",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100035569.1.13",
          "form": "아 그 수줍은 뚱뚱이",
          "original_form": "아 그 수줍은 뚱뚱이",
          "speaker_id": "2",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100035569.1.14",
          "form": "네네!ㅋㅋㅋ",
          "original_form": "네네!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100035569.1.15",
          "form": "부끄뚱 무대했어용",
          "original_form": "부끄뚱 무대했어용",
          "speaker_id": "1",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100035569.1.16",
          "form": "귀엽던데요ㅋㅋ",
          "original_form": "귀엽던데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100035569.1.17",
          "form": "가사가 귀여워요",
          "original_form": "가사가 귀여워요",
          "speaker_id": "2",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100035569.1.18",
          "form": "핑크머리가 은근 잘어울리더라구욬ㅋㅋ",
          "original_form": "핑크머리가 은근 잘어울리더라구욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100035569.1.19",
          "form": "혹시 심야괴담회 프로는 보세용?",
          "original_form": "혹시 심야괴담회 프로는 보세용?",
          "speaker_id": "1",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100035569.1.20",
          "form": "ㅋㅋ문세윤씨 은근 춤도잘추고",
          "original_form": "ㅋㅋ문세윤씨 은근 춤도잘추고",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100035569.1.21",
          "form": "만능 엔터테이너 같아용",
          "original_form": "만능 엔터테이너 같아용",
          "speaker_id": "1",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100035569.1.22",
          "form": "무서워가지고ㅠ 안봤어요",
          "original_form": "무서워가지고ㅠ 안봤어요",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100035569.1.23",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100035569.1.24",
          "form": "무서운이야기 하죠?",
          "original_form": "무서운이야기 하죠?",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100035569.1.25",
          "form": "저도 누구 옆에있을때만 봐요ㅋㅋㅋ",
          "original_form": "저도 누구 옆에있을때만 봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100035569.1.26",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210829 20:37"
        }
      ]
    }
  ]
