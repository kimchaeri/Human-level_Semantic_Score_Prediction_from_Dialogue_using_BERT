        },
        {
          "id": "MDRW2100013220.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013220.1.3",
          "form": "쇼핑 자주하시나요? 전 온라인으로 쇼핑자주해요.",
          "original_form": "쇼핑 자주하시나요? 전 온라인으로 쇼핑자주해요.",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013220.1.4",
          "form": "저는 가끔해요 ㅎ",
          "original_form": "저는 가끔해요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013220.1.5",
          "form": "저도 하게되면 온라인으로해요",
          "original_form": "저도 하게되면 온라인으로해요",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013220.1.6",
          "form": "장보는것도 온라인이 좋더라구요",
          "original_form": "장보는것도 온라인이 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013220.1.7",
          "form": "온라인 배송 도 빠르고 할인도 되니 좋아요",
          "original_form": "온라인 배송 도 빠르고 할인도 되니 좋아요",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100013220.1.8",
          "form": "그리고 견물생심이라고",
          "original_form": "그리고 견물생심이라고",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100013220.1.9",
          "form": "보면 사고싶어져요",
          "original_form": "보면 사고싶어져요",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013220.1.10",
          "form": "살계획이 없었는데 괜히 보면 싸다고 담고 ㅋ",
          "original_form": "살계획이 없었는데 괜히 보면 싸다고 담고 ㅋ",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013220.1.11",
          "form": "오프라인은아무래도. 그런것 같아요. 1+1 행사",
          "original_form": "오프라인은아무래도. 그런것 같아요. 1+1 행사",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013220.1.12",
          "form": "충동구매 자제를 위해서도 온라인이 좋은거같아요",
          "original_form": "충동구매 자제를 위해서도 온라인이 좋은거같아요",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013220.1.13",
          "form": "집앞까지 갖다주니 좋고 ㅎ",
          "original_form": "집앞까지 갖다주니 좋고 ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013220.1.14",
          "form": "그렇죠. 너무편해요\n  택배기사님들한테는. 미안한데 배송빨라 좋아요",
          "original_form": "그렇죠. 너무편해요\n  택배기사님들한테는. 미안한데 배송빨라 좋아요",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013220.1.15",
          "form": "주로 어느사이트 이용하세요?",
          "original_form": "주로 어느사이트 이용하세요?",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013220.1.16",
          "form": "택배기사님께는 진짜 죄송해요 ㅎ",
          "original_form": "택배기사님께는 진짜 죄송해요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013220.1.17",
          "form": "최근에 구매한거 있나요?",
          "original_form": "최근에 구매한거 있나요?",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013220.1.18",
          "form": "저 어제 도착한거있어요",
          "original_form": "저 어제 도착한거있어요",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013220.1.19",
          "form": "워치4요 ㅋ",
          "original_form": "워치4요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013220.1.20",
          "form": "와우..워치 예쁘더라구요",
          "original_form": "와우..워치 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013220.1.21",
          "form": "갤럭시워치4",
          "original_form": "갤럭시워치4",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013220.1.22",
          "form": "사이즈 40할까 44할까",
          "original_form": "사이즈 40할까 44할까",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100013220.1.23",
          "form": "엄청고민하다가 카드할인 쿠폰선착종료되고 ㅜㅠ",
          "original_form": "엄청고민하다가 카드할인 쿠폰선착종료되고 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100013220.1.24",
          "form": "아..이런.안타깝네요",
          "original_form": "아..이런.안타깝네요",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013220.1.25",
          "form": "14만원에 살수있었는데 16만원에 40 구매했네요",
          "original_form": "14만원에 살수있었는데 16만원에 40 구매했네요",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013220.1.26",
          "form": "근데 검정으로 했더니",
          "original_form": "근데 검정으로 했더니",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013220.1.27",
          "form": "고민은 배송만 미루는게 아니었네요",
          "original_form": "고민은 배송만 미루는게 아니었네요",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013220.1.28",
          "form": "이쁜지는 모르겠어요 ㅋ",
          "original_form": "이쁜지는 모르겠어요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013220.1.29",
          "form": "할인 쿠폰까지 날라가버렸군요",
          "original_form": "할인 쿠폰까지 날라가버렸군요",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013220.1.30",
          "form": "사전예약 선착이다보니",
          "original_form": "사전예약 선착이다보니",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100013220.1.31",
