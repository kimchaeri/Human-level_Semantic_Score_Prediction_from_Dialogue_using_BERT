      "utterance": [
        {
          "id": "MDRW2100010278.1.1",
          "form": "맞아요!! 망고도 좋고 망고스틴이 너무 맛있었어요ㅎㅎ",
          "original_form": "맞아요!! 망고도 좋고 망고스틴이 너무 맛있었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:20"
        },
        {
          "id": "MDRW2100010278.1.2",
          "form": "저도 둘다 좋아해요ㅎㅎ",
          "original_form": "저도 둘다 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100010278.1.3",
          "form": "한국에선 비싸서 왕창 먹을 순 없더라구요ㅠㅠ",
          "original_form": "한국에선 비싸서 왕창 먹을 순 없더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100010278.1.4",
          "form": "맞아요ㅠ 체리도 완전 맛있는데 넘비싸요ㅠ",
          "original_form": "맞아요ㅠ 체리도 완전 맛있는데 넘비싸요ㅠ",
          "speaker_id": "1",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010278.1.5",
          "form": "체리 진짜 맛있죠 ㅜㅠ 다른과일은 또 뭐좋아하세요?",
          "original_form": "체리 진짜 맛있죠 ㅜㅠ 다른과일은 또 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010278.1.6",
          "form": "사과는 매일먹고 포도도 좋아요!",
          "original_form": "사과는 매일먹고 포도도 좋아요!",
          "speaker_id": "1",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100010278.1.7",
          "form": "파는곳이 별로없는데 자두에이드도 좋아해용",
          "original_form": "파는곳이 별로없는데 자두에이드도 좋아해용",
          "speaker_id": "1",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100010278.1.8",
          "form": "자두에이드 상큼하니 맛있죠!!",
          "original_form": "자두에이드 상큼하니 맛있죠!!",
          "speaker_id": "2",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010278.1.9",
          "form": "여름이라 맛있는 과일이 많아서 좋아요ㅎㅎ",
          "original_form": "여름이라 맛있는 과일이 많아서 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010278.1.10",
          "form": "맞아요!! 사실 과일이면 거의다좋은것 같아요ㅋㅋㅋ",
          "original_form": "맞아요!! 사실 과일이면 거의다좋은것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010278.1.11",
          "form": "그쵸 과일은 다 맛있어요~",
          "original_form": "그쵸 과일은 다 맛있어요~",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010278.1.12",
          "form": "가을오면 배랑 홍시 많이 먹어야겠어요",
          "original_form": "가을오면 배랑 홍시 많이 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010278.1.13",
          "form": "홍시! 완전 좋아여",
          "original_form": "홍시! 완전 좋아여",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010278.1.14",
          "form": "곶감으로도 맛있구",
          "original_form": "곶감으로도 맛있구",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010278.1.15",
          "form": "오 맞아요 곶감도 진짜 맛있죠",
          "original_form": "오 맞아요 곶감도 진짜 맛있죠",
          "speaker_id": "2",
          "time": "20210823 16:38"
        },
        {
          "id": "MDRW2100010278.1.16",
          "form": "은은한단맛이 좋아용",
          "original_form": "은은한단맛이 좋아용",
          "speaker_id": "1",
          "time": "20210823 16:38"
        },
        {
          "id": "MDRW2100010278.1.17",
          "form": "쫀득해서 더 맛있어요 ..",
          "original_form": "쫀득해서 더 맛있어요 ..",
          "speaker_id": "2",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010278.1.18",
          "form": "제일 좋아하는 과일 하나 고른다면 어떤건가요?",
          "original_form": "제일 좋아하는 과일 하나 고른다면 어떤건가요?",
          "speaker_id": "2",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010278.1.19",
          "form": "저는 망고스틴!",
          "original_form": "저는 망고스틴!",
          "speaker_id": "1",
          "time": "20210823 16:49"
        }
      ]
