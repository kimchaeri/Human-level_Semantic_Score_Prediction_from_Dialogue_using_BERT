      "utterance": [
        {
          "id": "MDRW2100041954.1.1",
          "form": "저희 단대 교직원분들",
          "original_form": "저희 단대 교직원분들",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041954.1.2",
          "form": "정말 바빠보이시더라구요,,,",
          "original_form": "정말 바빠보이시더라구요,,,",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041954.1.3",
          "form": "그럼 name1님은 교직원 자리 계속 알아보고 계시는건가요?",
          "original_form": "그럼 &name1&님은 교직원 자리 계속 알아보고 계시는건가요?",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041954.1.4",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041954.1.5",
          "form": "팀장만아니면 즐거웠어요",
          "original_form": "팀장만아니면 즐거웠어요",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.6",
          "form": "우와아아아",
          "original_form": "우와아아아",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.7",
          "form": "근로했던친구들이랑도 아직잘만나고있고ㅠ",
          "original_form": "근로했던친구들이랑도 아직잘만나고있고ㅠ",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.8",
          "form": "즐거웠어요ㅠ",
          "original_form": "즐거웠어요ㅠ",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.9",
          "form": "교직원 정말 정직원만 되면",
          "original_form": "교직원 정말 정직원만 되면",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.10",
          "form": "우와 ㅠㅠ 진짜 훈훈해요",
          "original_form": "우와 ㅠㅠ 진짜 훈훈해요",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.11",
          "form": "저희 학교도",
          "original_form": "저희 학교도",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.12",
          "form": "정직원만 되면 복지가 좋다는 이야기릉",
          "original_form": "정직원만 되면 복지가 좋다는 이야기릉",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.13",
          "form": "들었어요",
          "original_form": "들었어요",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.14",
          "form": "그래서 졸업생들도 많이 노린다는데,,,",
          "original_form": "그래서 졸업생들도 많이 노린다는데,,,",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.15",
          "form": "그쵸!!",
          "original_form": "그쵸!!",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.16",
          "form": "근데 잘 안뽑힌대요 ,,",
          "original_form": "근데 잘 안뽑힌대요 ,,",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041954.1.17",
          "form": "그리고 좀 이상한게",
          "original_form": "그리고 좀 이상한게",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041954.1.18",
          "form": "보통 자교 나왔으면",
          "original_form": "보통 자교 나왔으면",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041954.1.19",
          "form": "지금학교에 계신 정직원 과장차장팀장급들 대부분 학교졸업생이더라고요",
          "original_form": "지금학교에 계신 정직원 과장차장팀장급들 대부분 학교졸업생이더라고요",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041954.1.20",
          "form": "애교심 있게 잘할거란 생각으로 더 많이 뽑아야하지 않나? 생각했는데",
          "original_form": "애교심 있게 잘할거란 생각으로 더 많이 뽑아야하지 않나? 생각했는데",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041954.1.21",
          "form": "헉 맞네요?",
          "original_form": "헉 맞네요?",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041954.1.22",
          "form": "저희학교는 이상하게",
          "original_form": "저희학교는 이상하게",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041954.1.23",
          "form": "그시절엔 좀 취업이 쉬웠겠죠,,,ㅎㅎ",
          "original_form": "그시절엔 좀 취업이 쉬웠겠죠,,,ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041954.1.24",
          "form": "별로 자교출신이...",
          "original_form": "별로 자교출신이...",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041954.1.25",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041954.1.26",
          "form": "맞네요...ㅎ",
          "original_form": "맞네요...ㅎ",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041954.1.27",
          "form": "(숙연",
          "original_form": "(숙연",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041954.1.28",
          "form": "혹시남자정직원들이많나요?!",
          "original_form": "혹시남자정직원들이많나요?!",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041954.1.29",
          "form": "앗 그거까진 모르겠는데",
          "original_form": "앗 그거까진 모르겠는데",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041954.1.30",
          "form": "저희학교는 좀 적지않앗어요,,",
          "original_form": "저희학교는 좀 적지않앗어요,,",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041954.1.31",
          "form": "여자분들이 많은 것 같긴 해요!!!",
          "original_form": "여자분들이 많은 것 같긴 해요!!!",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041954.1.32",
          "form": "하 제가 소상히 알고있는건 또 아니어서... 흠...",
          "original_form": "하 제가 소상히 알고있는건 또 아니어서... 흠...",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041954.1.33",
          "form": "근데 진짜 저도 교직원 되면 너무 좋을거같아요",
          "original_form": "근데 진짜 저도 교직원 되면 너무 좋을거같아요",
          "speaker_id": "2",
          "time": "20210906 18:04"
        }
      ]
