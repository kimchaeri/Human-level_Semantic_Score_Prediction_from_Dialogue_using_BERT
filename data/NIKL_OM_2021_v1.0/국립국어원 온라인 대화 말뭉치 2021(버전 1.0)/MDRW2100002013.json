      "utterance": [
        {
          "id": "MDRW2100002013.1.1",
          "form": "정모해요?",
          "original_form": "정모해요?",
          "speaker_id": "1",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002013.1.2",
          "form": "네 정모합니다",
          "original_form": "네 정모합니다",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002013.1.3",
          "form": "약속시간 지켜주세요",
          "original_form": "약속시간 지켜주세요",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002013.1.4",
          "form": "언제죠?",
          "original_form": "언제죠?",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002013.1.5",
          "form": "게시글 확인부탁드립니다ㅎㅎ",
          "original_form": "게시글 확인부탁드립니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002013.1.6",
          "form": "어디 공지죠?",
          "original_form": "어디 공지죠?",
          "speaker_id": "1",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100002013.1.7",
          "form": "커뮤 제일 상단에 공지글 있어요",
          "original_form": "커뮤 제일 상단에 공지글 있어요",
          "speaker_id": "2",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100002013.1.8",
          "form": "네 준비물은요?",
          "original_form": "네 준비물은요?",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100002013.1.9",
          "form": "회비 20만원이랑 단체복, 브로마이드요",
          "original_form": "회비 20만원이랑 단체복, 브로마이드요",
          "speaker_id": "2",
          "time": "20210819 22:58"
        },
        {
          "id": "MDRW2100002013.1.10",
          "form": "핑크 단체복 맞죠?",
          "original_form": "핑크 단체복 맞죠?",
          "speaker_id": "1",
          "time": "20210819 23:00"
        },
        {
          "id": "MDRW2100002013.1.11",
          "form": "네네 맞습니다. 입기 싫으시면 챙겨서 와주셔야해요. 단체사진에는 입고 찍으셔야해서",
          "original_form": "네네 맞습니다. 입기 싫으시면 챙겨서 와주셔야해요. 단체사진에는 입고 찍으셔야해서",
          "speaker_id": "2",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100002013.1.12",
          "form": "우리 구호 잊지 않았죠?",
          "original_form": "우리 구호 잊지 않았죠?",
          "speaker_id": "1",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100002013.1.13",
          "form": "당연하죠. 먼저 선창해주시면 제가 후창 할게요",
          "original_form": "당연하죠. 먼저 선창해주시면 제가 후창 할게요",
          "speaker_id": "2",
          "time": "20210819 23:02"
        },
        {
          "id": "MDRW2100002013.1.14",
          "form": "연습차 해보죠",
          "original_form": "연습차 해보죠",
          "speaker_id": "2",
          "time": "20210819 23:02"
        },
        {
          "id": "MDRW2100002013.1.15",
          "form": "선창 해주세요",
          "original_form": "선창 해주세요",
          "speaker_id": "2",
          "time": "20210819 23:02"
        },
        {
          "id": "MDRW2100002013.1.16",
          "form": "방장님이 선창하셔야 우리 회원이 하죠...",
          "original_form": "방장님이 선창하셔야 우리 회원이 하죠...",
          "speaker_id": "1",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100002013.1.17",
          "form": "저번에 왜 반장만 선창하냐는 항의가 들어와서 회원분들 선창으로 바뀌신거 아시면서.. 저 곤란하게 하시려고 그러시네 ㅎㅎ",
          "original_form": "저번에 왜 반장만 선창하냐는 항의가 들어와서 회원분들 선창으로 바뀌신거 아시면서.. 저 곤란하게 하시려고 그러시네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:06"
        }
      ]
