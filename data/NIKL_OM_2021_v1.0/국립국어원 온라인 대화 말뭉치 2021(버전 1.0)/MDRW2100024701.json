      "utterance": [
        {
          "id": "MDRW2100024701.1.1",
          "form": "요즘 인기있는 게임 뭐가있죠?",
          "original_form": "요즘 인기있는 게임 뭐가있죠?",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024701.1.2",
          "form": "카트라이더요",
          "original_form": "카트라이더요",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024701.1.3",
          "form": "모바일요?",
          "original_form": "모바일요?",
          "speaker_id": "1",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024701.1.4",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024701.1.5",
          "form": "피시도해요",
          "original_form": "피시도해요",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024701.1.6",
          "form": "번갈아서하고있어요",
          "original_form": "번갈아서하고있어요",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024701.1.7",
          "form": "카트 귀여운 차 많았능데 요즘도 그러려나요 ㅎㅎ",
          "original_form": "카트 귀여운 차 많았능데 요즘도 그러려나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024701.1.8",
          "form": "저 카트잘해요!!ㅎㅎ",
          "original_form": "저 카트잘해요!!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:48"
        },
        {
          "id": "MDRW2100024701.1.9",
          "form": "ㅋㅋㅋㅋ 매번 1등하시나요 저는 예전에 모바일로 했을때 자꾸져서 화딱지나던데ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 매번 1등하시나요 저는 예전에 모바일로 했을때 자꾸져서 화딱지나던데ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024701.1.10",
          "form": "아이템전에서만요 좀해욬ㅋㅋㅋ",
          "original_form": "아이템전에서만요 좀해욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 07:51"
        },
        {
          "id": "MDRW2100024701.1.11",
          "form": "재밌어요 ㅋㅋㅋ 저는 아이템전을 안해봤어요 ㅋㅋㅋ",
          "original_form": "재밌어요 ㅋㅋㅋ 저는 아이템전을 안해봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100024701.1.12",
          "form": "아이템전도 재미있어요ㅋㅋㄱㅋ",
          "original_form": "아이템전도 재미있어요ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100024701.1.13",
          "form": "아이템 뭐가있나요? 뭐 끌어당기기 이런것도 있으려나",
          "original_form": "아이템 뭐가있나요? 뭐 끌어당기기 이런것도 있으려나",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100024701.1.14",
          "form": "그건 자석이네요",
          "original_form": "그건 자석이네요",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100024701.1.15",
          "form": "저도 가끔씩만해서 아이템 새로나온게 많더라구요",
          "original_form": "저도 가끔씩만해서 아이템 새로나온게 많더라구요",
          "speaker_id": "2",
