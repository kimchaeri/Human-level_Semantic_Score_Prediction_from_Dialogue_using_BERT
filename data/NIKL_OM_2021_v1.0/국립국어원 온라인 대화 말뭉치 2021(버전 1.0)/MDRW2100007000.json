      "utterance": [
        {
          "id": "MDRW2100007000.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 11:59"
        },
        {
          "id": "MDRW2100007000.1.2",
          "form": "안녕하세요 학교 다니세요?",
          "original_form": "안녕하세요 학교 다니세요?",
          "speaker_id": "2",
          "time": "20210822 11:59"
        },
        {
          "id": "MDRW2100007000.1.3",
          "form": "네네 고등학생이예요!",
          "original_form": "네네 고등학생이예요!",
          "speaker_id": "1",
          "time": "20210822 11:59"
        },
        {
          "id": "MDRW2100007000.1.4",
          "form": "저도 고등학생이에요!",
          "original_form": "저도 고등학생이에요!",
          "speaker_id": "2",
          "time": "20210822 12:00"
        },
        {
          "id": "MDRW2100007000.1.5",
          "form": "요즘 학교개학하고 뒤숭숭해요 ㅎ",
          "original_form": "요즘 학교개학하고 뒤숭숭해요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007000.1.6",
          "form": "헐헐 왜요 ㅜ",
          "original_form": "헐헐 왜요 ㅜ",
          "speaker_id": "1",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007000.1.7",
          "form": "이제 개학했으니 다시 시험공부도 해야하고 수행평가도 봐야하고 바빠질 것 같아서요ㅠ ㅋㄱ",
          "original_form": "이제 개학했으니 다시 시험공부도 해야하고 수행평가도 봐야하고 바빠질 것 같아서요ㅠ ㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007000.1.8",
          "form": "아...그렇죠 ㅠ",
          "original_form": "아...그렇죠 ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007000.1.9",
          "form": "공부하기 너무",
          "original_form": "공부하기 너무",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007000.1.10",
          "form": "그래도 학교급식이 맛있어서 그건 좋아요 ㅎ",
          "original_form": "그래도 학교급식이 맛있어서 그건 좋아요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007000.1.11",
          "form": "빡센거같아요",
          "original_form": "빡센거같아요",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007000.1.12",
          "form": "특히 세특 챙기기",
          "original_form": "특히 세특 챙기기",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007000.1.13",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007000.1.14",
          "form": "헐 부럽다",
          "original_form": "헐 부럽다",
          "speaker_id": "1",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007000.1.15",
          "form": "저희학교 급식 진짜 노맛이예요",
          "original_form": "저희학교 급식 진짜 노맛이예요",
          "speaker_id": "1",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007000.1.16",
          "form": "원래 매점이라도 있엇는데",
          "original_form": "원래 매점이라도 있엇는데",
          "speaker_id": "1",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007000.1.17",
          "form": "코로나 이후로 닫았어요 ㅠ",
          "original_form": "코로나 이후로 닫았어요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007000.1.18",
          "form": "아이고 ㅠ 급식 맛없으면 진짜 싫은데 ㅠ",
          "original_form": "아이고 ㅠ 급식 맛없으면 진짜 싫은데 ㅠ",
          "speaker_id": "2",
          "time": "20210822 12:05"
        },
        {
          "id": "MDRW2100007000.1.19",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:05"
        },
        {
          "id": "MDRW2100007000.1.20",
          "form": "학교 친구들은 어때요?",
          "original_form": "학교 친구들은 어때요?",
          "speaker_id": "2",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100007000.1.21",
          "form": "전 좋아요 ㅎㅎ",
          "original_form": "전 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100007000.1.22",
          "form": "다 착하고 열심히 하고",
          "original_form": "다 착하고 열심히 하고",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100007000.1.23",
          "form": "싸우거나 부딪침도 딱히 없었구요",
          "original_form": "싸우거나 부딪침도 딱히 없었구요",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100007000.1.24",
          "form": "학급 분위기 좋으면 좋죠 단합도 잘되고!",
          "original_form": "학급 분위기 좋으면 좋죠 단합도 잘되고!",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100007000.1.25",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100007000.1.26",
          "form": "단합할 일이 잘 없단게 슬프지만 ㅋㅋㅜ",
          "original_form": "단합할 일이 잘 없단게 슬프지만 ㅋㅋㅜ",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100007000.1.27",
          "form": "장기자랑 이런거 하면",
          "original_form": "장기자랑 이런거 하면",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100007000.1.28",
          "form": "준비하는 과정이 또 재밌잖아요",
          "original_form": "준비하는 과정이 또 재밌잖아요",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100007000.1.29",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100007000.1.30",
          "form": "근데 지금은 학교축제 할지도 불투명하고 그렇죠",
          "original_form": "근데 지금은 학교축제 할지도 불투명하고 그렇죠",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100007000.1.31",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100007000.1.32",
          "form": "학급에서 작게라도 뭐 하는게 너무 좋은것같아요",
          "original_form": "학급에서 작게라도 뭐 하는게 너무 좋은것같아요",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100007000.1.33",
          "form": "아 ㅠㅠ",
          "original_form": "아 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100007000.1.34",
          "form": "name2 학교생활 어떠세요?",
          "original_form": "&name2& 학교생활 어떠세요?",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100007000.1.35",
          "form": "저는 학교가 모처럼 좋은데 코로나때문에 추억이 없어서 너무 아쉽더라고요 ㅠ",
          "original_form": "저는 학교가 모처럼 좋은데 코로나때문에 추억이 없어서 너무 아쉽더라고요 ㅠ",
