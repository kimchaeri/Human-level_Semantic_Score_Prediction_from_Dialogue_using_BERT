      "utterance": [
        {
          "id": "MDRW2100041917.1.1",
          "form": "오늘 저녁 메뉴 먼가요?",
          "original_form": "오늘 저녁 메뉴 먼가요?",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041917.1.2",
          "form": "오늘 저녁은 김치볶음밥이요!",
          "original_form": "오늘 저녁은 김치볶음밥이요!",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041917.1.3",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041917.1.4",
          "form": "오오!!!저는 치킨 먹을꺼에요",
          "original_form": "오오!!!저는 치킨 먹을꺼에요",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041917.1.5",
          "form": "배고파요ㅠㅠ",
          "original_form": "배고파요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041917.1.6",
          "form": "저도 메뉴만 정하고 아직!!",
          "original_form": "저도 메뉴만 정하고 아직!!",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041917.1.7",
          "form": "뮤슨 치킨이요??",
          "original_form": "뮤슨 치킨이요??",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041917.1.8",
          "form": "후라이드 맛 먹을것 같아요",
          "original_form": "후라이드 맛 먹을것 같아요",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041917.1.9",
          "form": "후라이드 좋죠!",
          "original_form": "후라이드 좋죠!",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041917.1.10",
          "form": "저는 양념반후라이드반 좋아해용",
          "original_form": "저는 양념반후라이드반 좋아해용",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041917.1.11",
          "form": "그것도 맛있죠ㅎㅎ",
          "original_form": "그것도 맛있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041917.1.12",
          "form": "이번 치킨은 홈플치킨이라서",
          "original_form": "이번 치킨은 홈플치킨이라서",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041917.1.13",
          "form": "후라이드먹고",
          "original_form": "후라이드먹고",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041917.1.14",
          "form": "소스는 KFC꺼 먹을려구요",
          "original_form": "소스는 KFC꺼 먹을려구요",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041917.1.15",
          "form": "헉 맛있겠다",
          "original_form": "헉 맛있겠다",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041917.1.16",
          "form": "홈플치킨 싸고 좋죠~~",
          "original_form": "홈플치킨 싸고 좋죠~~",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041917.1.17",
          "form": "넹 그것두 먹구",
          "original_form": "넹 그것두 먹구",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041917.1.18",
          "form": "주말에는 KFC에서 햄버거랑치킨먹었어요",
          "original_form": "주말에는 KFC에서 햄버거랑치킨먹었어요",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041917.1.19",
          "form": "저는 어제 돈까스에 크림수프!",
          "original_form": "저는 어제 돈까스에 크림수프!",
          "speaker_id": "2",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041917.1.20",
          "form": "돈까스도 맛있죠!!!",
          "original_form": "돈까스도 맛있죠!!!",
          "speaker_id": "1",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041917.1.21",
          "form": "그것두 소스가 중요하거든요",
          "original_form": "그것두 소스가 중요하거든요",
          "speaker_id": "1",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041917.1.22",
          "form": "맞아요~!!! 근디 마땅한 게 없어서 케찹...ㅠㅠ",
          "original_form": "맞아요~!!! 근디 마땅한 게 없어서 케찹...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041917.1.23",
          "form": "앜ㅋㅋ그것두 맛있어요",
          "original_form": "앜ㅋㅋ그것두 맛있어요",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041917.1.24",
          "form": "집밥먹을때 미니 돈까스에",
          "original_form": "집밥먹을때 미니 돈까스에",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041917.1.25",
          "form": "케찹뿌리자나요ㅋㅋㅋ",
          "original_form": "케찹뿌리자나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041917.1.26",
          "form": "저 그렇게 먹어요",
          "original_form": "저 그렇게 먹어요",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041917.1.27",
          "form": "좋죠좋죠!",
          "original_form": "좋죠좋죠!",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041917.1.28",
          "form": "빨리집에가서 치킨먹고싶네요",
          "original_form": "빨리집에가서 치킨먹고싶네요",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041917.1.29",
          "form": "너무 배고파요",
          "original_form": "너무 배고파요",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041917.1.30",
          "form": "너무 배고파요",
          "original_form": "너무 배고파요",
