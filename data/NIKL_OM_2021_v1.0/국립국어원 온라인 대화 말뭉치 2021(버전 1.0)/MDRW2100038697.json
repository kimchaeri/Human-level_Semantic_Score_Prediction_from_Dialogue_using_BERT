        },
        {
          "id": "MDRW2100038697.1.2",
          "form": "배트맨도 마블인데",
          "original_form": "배트맨도 마블인데",
          "speaker_id": "2",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100038697.1.3",
          "form": "원더우먼하고 수퍼맨 이런쪽도 마블인데요",
          "original_form": "원더우먼하고 수퍼맨 이런쪽도 마블인데요",
          "speaker_id": "2",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100038697.1.4",
          "form": "그렇군요 ㅋㄱ",
          "original_form": "그렇군요 ㅋㄱ",
          "speaker_id": "1",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100038697.1.5",
          "form": "영화사가 다른데 계약해서 따로나오는거에요",
          "original_form": "영화사가 다른데 계약해서 따로나오는거에요",
          "speaker_id": "2",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100038697.1.6",
          "form": "블랙위도우 죽었다는  말을 툭들어서",
          "original_form": "블랙위도우 죽었다는  말을 툭들어서",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100038697.1.7",
          "form": "20세기 폭스사였던가",
          "original_form": "20세기 폭스사였던가",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100038697.1.8",
          "form": "저는 좀 허무했던기억이 ㅋㅋ",
          "original_form": "저는 좀 허무했던기억이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100038697.1.9",
          "form": "네죽었어요ㅜㅜㅜㅜ",
          "original_form": "네죽었어요ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100038697.1.10",
          "form": "2편안나오는이유ㅜㅡㅜ",
          "original_form": "2편안나오는이유ㅜㅡㅜ",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100038697.1.11",
          "form": "스포를 이렇게당하다니ㅜㅜ했다말",
          "original_form": "스포를 이렇게당하다니ㅜㅜ했다말",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100038697.1.12",
          "form": "했다만",
          "original_form": "했다만",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100038697.1.13",
          "form": "저는 마블영화 딱히 챙겨보지 않아서요 ㅋㅋ",
          "original_form": "저는 마블영화 딱히 챙겨보지 않아서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100038697.1.14",
          "form": "에 아직 안보셨군요",
          "original_form": "에 아직 안보셨군요",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100038697.1.15",
          "form": "미안합니다 저는 바로바로 챙겨보는 스탈이라 ㅋ",
          "original_form": "미안합니다 저는 바로바로 챙겨보는 스탈이라 ㅋ",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100038697.1.16",
          "form": "그래두 재밌을꺼에요 날잡아보시믄",
          "original_form": "그래두 재밌을꺼에요 날잡아보시믄",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100038697.1.17",
          "form": "언젠가 몸과마음의 여유가 생기면 마라톤처럼봐도 되고요 ㅋㅋ",
          "original_form": "언젠가 몸과마음의 여유가 생기면 마라톤처럼봐도 되고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100038697.1.18",
          "form": "근데 양조위가 유명한가요",
          "original_form": "근데 양조위가 유명한가요",
          "speaker_id": "1",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100038697.1.19",
          "form": "양조위 얼굴보시면",
          "original_form": "양조위 얼굴보시면",
          "speaker_id": "2",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100038697.1.20",
          "form": "아 이사람이구나 아실꺼에요",
          "original_form": "아 이사람이구나 아실꺼에요",
          "speaker_id": "2",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100038697.1.21",
          "form": "색계에서 남주인공",
          "original_form": "색계에서 남주인공",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100038697.1.22",
          "form": "몇살인가요 40대?",
          "original_form": "몇살인가요 40대?",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100038697.1.23",
          "form": "아유 울부모님 급이죠",
          "original_form": "아유 울부모님 급이죠",
          "speaker_id": "2",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100038697.1.24",
          "form": "홍콩사람이라고하셨으니 영어는 쏼라쏼라 하셨겠네요",
