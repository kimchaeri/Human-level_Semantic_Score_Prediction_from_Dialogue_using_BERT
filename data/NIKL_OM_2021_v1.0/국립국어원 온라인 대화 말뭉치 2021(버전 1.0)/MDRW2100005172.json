      "utterance": [
        {
          "id": "MDRW2100005172.1.1",
          "form": "서울가면 피티끊을거야?",
          "original_form": "서울가면 피티끊을거야?",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005172.1.2",
          "form": "아니 피티 너무비싸서",
          "original_form": "아니 피티 너무비싸서",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005172.1.3",
          "form": "피티안할 것 같어",
          "original_form": "피티안할 것 같어",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005172.1.4",
          "form": "그럼 걍 헬스장?",
          "original_form": "그럼 걍 헬스장?",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005172.1.5",
          "form": "응 그냥 헬스장가고",
          "original_form": "응 그냥 헬스장가고",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005172.1.6",
          "form": "자전거 탈거야",
          "original_form": "자전거 탈거야",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005172.1.7",
          "form": "거기는 한강이랑 먼가?",
          "original_form": "거기는 한강이랑 먼가?",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005172.1.8",
          "form": "별로안멀어",
          "original_form": "별로안멀어",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005172.1.9",
          "form": "한10분?",
          "original_form": "한10분?",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005172.1.10",
          "form": "오 그럼 거기 가서 운동하면 좋겠네",
          "original_form": "오 그럼 거기 가서 운동하면 좋겠네",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005172.1.11",
          "form": "런닝해",
          "original_form": "런닝해",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005172.1.12",
          "form": "자전거 타려고",
          "original_form": "자전거 타려고",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005172.1.13",
          "form": "런닝은 음....",
          "original_form": "런닝은 음....",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005172.1.14",
          "form": "좀빠지면??",
          "original_form": "좀빠지면??",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005172.1.15",
          "form": "난 필테 어케 할까",
          "original_form": "난 필테 어케 할까",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005172.1.16",
          "form": "정해줘봐",
          "original_form": "정해줘봐",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005172.1.17",
          "form": "가까운곳에 등록해봐",
          "original_form": "가까운곳에 등록해봐",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005172.1.18",
          "form": "이곳저곳 해봐야지",
          "original_form": "이곳저곳 해봐야지",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005172.1.19",
          "form": "아님 한 타임 남겨놓고 딴데 등록하다가 갈까..",
          "original_form": "아님 한 타임 남겨놓고 딴데 등록하다가 갈까..",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005172.1.20",
          "form": "어차피 거의 끝난거 아니야?",
          "original_form": "어차피 거의 끝난거 아니야?",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005172.1.21",
          "form": "아니 마지막 타임 수업 들으면 더 안끊을거냐고 물어볼테니",
          "original_form": "아니 마지막 타임 수업 들으면 더 안끊을거냐고 물어볼테니",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005172.1.22",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005172.1.23",
          "form": "다듣는게 좋지않겠어??",
          "original_form": "다듣는게 좋지않겠어??",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005172.1.24",
          "form": "비싼걸",
          "original_form": "비싼걸",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005172.1.25",
          "form": "어짜피 이번 해에만 들으면 되니까",
          "original_form": "어짜피 이번 해에만 들으면 되니까",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005172.1.26",
          "form": "걍 다 들어?",
          "original_form": "걍 다 들어?",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005172.1.27",
          "form": "그냥 듣는게 좋을 것 같아",
          "original_form": "그냥 듣는게 좋을 것 같아",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005172.1.28",
          "form": "그랭.. 지금 필테는 5:1인데",
          "original_form": "그랭.. 지금 필테는 5:1인데",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005172.1.29",
          "form": "여긴 4:1임",
          "original_form": "여긴 4:1임",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005172.1.30",
          "form": "오 더 좋은거 아냐??",
          "original_form": "오 더 좋은거 아냐??",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005172.1.31",
          "form": "그치 그래서 거기 쌤 괜찮으면 4명이니까 가족끼리 해도 좋을듯",
          "original_form": "그치 그래서 거기 쌤 괜찮으면 4명이니까 가족끼리 해도 좋을듯",
          "speaker_id": "1",
          "time": "20210821 21:16"
        }
      ]
    }
