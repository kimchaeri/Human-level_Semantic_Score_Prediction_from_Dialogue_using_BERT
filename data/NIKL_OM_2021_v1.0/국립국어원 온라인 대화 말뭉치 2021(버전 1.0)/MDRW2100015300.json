      "utterance": [
        {
          "id": "MDRW2100015300.1.1",
          "form": "역시 결혼이야기하면 무거워져요ㅎㄹ",
          "original_form": "역시 결혼이야기하면 무거워져요ㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015300.1.2",
          "form": "어쩔수없는듯ㅎㅎㅎ",
          "original_form": "어쩔수없는듯ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015300.1.3",
          "form": "상대편 집안이 저희집에서 부모님보시기엔 많이 맘에 안드셨나봐요",
          "original_form": "상대편 집안이 저희집에서 부모님보시기엔 많이 맘에 안드셨나봐요",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015300.1.4",
          "form": "상대방이 이혼가정에",
          "original_form": "상대방이 이혼가정에",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015300.1.5",
          "form": "재혼하셨는데",
          "original_form": "재혼하셨는데",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015300.1.6",
          "form": "다른 나라분하고 하셔가지고",
          "original_form": "다른 나라분하고 하셔가지고",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015300.1.7",
          "form": "근데 그게 왠지 맞는것 같기도해요",
          "original_form": "근데 그게 왠지 맞는것 같기도해요",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015300.1.8",
          "form": "어른들이 틀린말이없는게",
          "original_form": "어른들이 틀린말이없는게",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015300.1.9",
          "form": "저희도그랬어요",
          "original_form": "저희도그랬어요",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015300.1.10",
          "form": "결국 이혼하게됫구요ㅎ",
          "original_form": "결국 이혼하게됫구요ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015300.1.11",
          "form": "아 반대가 있으셨어요?",
          "original_form": "아 반대가 있으셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015300.1.12",
          "form": "name2님은 어떤이유로?",
          "original_form": "&name2&님은 어떤이유로?",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015300.1.13",
          "form": "아내쪽에는 사별을 했어요 ㅡㅎ",
          "original_form": "아내쪽에는 사별을 했어요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015300.1.14",
          "form": "뭐 거의 이혼상태였었고 아버님이 혼자키웠던것같애요ㅎ",
          "original_form": "뭐 거의 이혼상태였었고 아버님이 혼자키웠던것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015300.1.15",
          "form": "비슷하네요",
          "original_form": "비슷하네요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015300.1.16",
          "form": "이래저래 저희 아버지가 알아봤는데 그래서 홀어머니 밑에서 자라서 반대를 많이랬어요",
          "original_form": "이래저래 저희 아버지가 알아봤는데 그래서 홀어머니 밑에서 자라서 반대를 많이랬어요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015300.1.17",
          "form": "말이 이상한데ㅎㅎ 아버님이 돌아가셬ㅅ어요ㅎ",
          "original_form": "말이 이상한데ㅎㅎ 아버님이 돌아가셬ㅅ어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015300.1.18",
          "form": "왜그런지르모르겟는데 결혼 후",
          "original_form": "왜그런지르모르겟는데 결혼 후",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015300.1.19",
          "form": "조금 아내가 안하무인인게 많았어요",
          "original_form": "조금 아내가 안하무인인게 많았어요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015300.1.20",
          "form": "우리 아버님이 장난친거가지고 심하게 반응하고 아이도 안보여주고",
          "original_form": "우리 아버님이 장난친거가지고 심하게 반응하고 아이도 안보여주고",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015300.1.21",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015300.1.22",
          "form": "너무 힘들더라구요.. 예의가 없어서ㅠㅠ",
          "original_form": "너무 힘들더라구요.. 예의가 없어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015300.1.23",
          "form": "부모님한테 예의없게 하면",
          "original_form": "부모님한테 예의없게 하면",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015300.1.24",
          "form": "정이 확 떨어지던데",
          "original_form": "정이 확 떨어지던데",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015300.1.25",
          "form": "네네ㅎ",
          "original_form": "네네ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015300.1.26",
          "form": "저희 아버님 생신인데",
          "original_form": "저희 아버님 생신인데",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015300.1.27",
          "form": "딸 보여주기 싫다고해서 엄청싸웠네요",
          "original_form": "딸 보여주기 싫다고해서 엄청싸웠네요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015300.1.28",
          "form": "자기 딸이래요",
          "original_form": "자기 딸이래요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015300.1.29",
          "form": "굳이 그렇게까지 할 이유는 없는데",
          "original_form": "굳이 그렇게까지 할 이유는 없는데",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015300.1.30",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015300.1.31",
          "form": "정이 확 떨어져요ㅎ",
          "original_form": "정이 확 떨어져요ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015300.1.32",
          "form": "제친구도 결혼하고 아이 있는데",
          "original_form": "제친구도 결혼하고 아이 있는데",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015300.1.33",
          "form": "근데 부모님이 아프니깐 더 그래요",
          "original_form": "근데 부모님이 아프니깐 더 그래요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015300.1.34",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015300.1.35",
          "form": "비슷해요. 아이 안보여주겠다고",
          "original_form": "비슷해요. 아이 안보여주겠다고",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015300.1.36",
          "form": "자기 싫어한다고",
          "original_form": "자기 싫어한다고",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015300.1.37",
          "form": "복수한다고",
          "original_form": "복수한다고",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015300.1.38",
          "form": "지금은 친구가 애기는 데리고 가는데",
          "original_form": "지금은 친구가 애기는 데리고 가는데",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015300.1.39",
          "form": "와이프는 시댁 안간지 오래됐대요",
          "original_form": "와이프는 시댁 안간지 오래됐대요",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015300.1.40",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015300.1.41",
          "form": "그래도 잘참고 살고계시네여",
          "original_form": "그래도 잘참고 살고계시네여",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015300.1.42",
          "form": "왜 그런지 모르겟어요 저희부모님은 아내가 모난거 알아서 집에도 안오고 아이도 안보는데 말이죠ㅠ",
          "original_form": "왜 그런지 모르겟어요 저희부모님은 아내가 모난거 알아서 집에도 안오고 아이도 안보는데 말이죠ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015300.1.43",
          "form": "제친구도 제가보기엔....",
          "original_form": "제친구도 제가보기엔....",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015300.1.44",
          "form": "친구가 지쳐서 손 놓으면",
          "original_form": "친구가 지쳐서 손 놓으면",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015300.1.45",
          "form": "끝일듯해요",
          "original_form": "끝일듯해요",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015300.1.46",
          "form": "엄청 참고있는거 같더라구요",
          "original_form": "엄청 참고있는거 같더라구요",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015300.1.47",
          "form": "저도 못참고 헤어지는거애요",
          "original_form": "저도 못참고 헤어지는거애요",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015300.1.48",
          "form": "아마 아이때문일꺼에요",
          "original_form": "아마 아이때문일꺼에요",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015300.1.49",
          "form": "근데 정작 본인은 힘들어요ㅠ",
          "original_form": "근데 정작 본인은 힘들어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015300.1.50",
          "form": "그런경우라면 부부간 잠자리도 문제일꺼에요ㅠ",
          "original_form": "그런경우라면 부부간 잠자리도 문제일꺼에요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015300.1.51",
          "form": "친구가 육아도 엄청 도와주는데",
          "original_form": "친구가 육아도 엄청 도와주는데",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015300.1.52",
          "form": "그거가지고도 엄청 불만이많대요",
          "original_form": "그거가지고도 엄청 불만이많대요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015300.1.53",
          "form": "저랑 왜이렇게 비슷한지ㅎ",
          "original_form": "저랑 왜이렇게 비슷한지ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015300.1.54",
          "form": "저도 정말 많이 도와주는데 ...",
          "original_form": "저도 정말 많이 도와주는데 ...",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015300.1.55",
          "form": "참... 당연한거라 생각하더라구요",
          "original_form": "참... 당연한거라 생각하더라구요",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015300.1.56",
          "form": "제친구가 할머니 돌아가셨는데",
          "original_form": "제친구가 할머니 돌아가셨는데",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015300.1.57",
          "form": "자긴 안가겠다고해서",
          "original_form": "자긴 안가겠다고해서",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015300.1.58",
          "form": "화가나서 저한테 하소연하더라구요",
          "original_form": "화가나서 저한테 하소연하더라구요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015300.1.59",
          "form": "아무리 그래도 이건 아니지 않냐고",
          "original_form": "아무리 그래도 이건 아니지 않냐고",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015300.1.60",
          "form": "아무리그래도 그건아니네요",
          "original_form": "아무리그래도 그건아니네요",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015300.1.61",
          "form": "저희 아내도 그렇게는 안할꺼애요",
          "original_form": "저희 아내도 그렇게는 안할꺼애요",
          "speaker_id": "2",
          "time": "20210825 23:04"
        }
