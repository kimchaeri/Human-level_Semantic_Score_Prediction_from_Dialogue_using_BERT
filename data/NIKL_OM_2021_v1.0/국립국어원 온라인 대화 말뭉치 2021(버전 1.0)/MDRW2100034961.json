      "utterance": [
        {
          "id": "MDRW2100034961.1.1",
          "form": "오종현이랑 김군나온다",
          "original_form": "오종현이랑 김군나온다",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100034961.1.2",
          "form": "어디에?",
          "original_form": "어디에?",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100034961.1.3",
          "form": "집이지 티비 볼려고 거실에 앉아있음",
          "original_form": "집이지 티비 볼려고 거실에 앉아있음",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100034961.1.4",
          "form": "집이지ㅋ  연합뉴스 보고 있으시네ㅋ",
          "original_form": "집이지ㅋ  연합뉴스 보고 있으시네ㅋ",
          "speaker_id": "1",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100034961.1.5",
          "form": "ㅋㅋ나혼자산다 재방할려나봐",
          "original_form": "ㅋㅋ나혼자산다 재방할려나봐",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100034961.1.6",
          "form": "재방을 몇번을 하는지",
          "original_form": "재방을 몇번을 하는지",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100034961.1.7",
          "form": "채널 돌아가면서 여기저기하는거지",
          "original_form": "채널 돌아가면서 여기저기하는거지",
          "speaker_id": "2",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100034961.1.8",
          "form": "채널이 많으니 일주일에도 몇번을 하는듯",
          "original_form": "채널이 많으니 일주일에도 몇번을 하는듯",
          "speaker_id": "1",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100034961.1.9",
          "form": "엉 그래서 구지 본방사수 안해도 된다",
          "original_form": "엉 그래서 구지 본방사수 안해도 된다",
          "speaker_id": "2",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100034961.1.10",
          "form": "이만갑은로 채널 돌렸네",
          "original_form": "이만갑은로 채널 돌렸네",
          "speaker_id": "1",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100034961.1.11",
          "form": "ㅋㅋ명품진품은 아직도 방송하네",
          "original_form": "ㅋㅋ명품진품은 아직도 방송하네",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100034961.1.12",
          "form": "이제 끝내야 되는거 아니가?",
          "original_form": "이제 끝내야 되는거 아니가?",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100034961.1.13",
          "form": "그러게 엄청 오래하네",
          "original_form": "그러게 엄청 오래하네",
          "speaker_id": "2",
          "time": "20210829 11:24"
        }
