        {
          "id": "MDRW2100026958.1.34",
          "form": "그런일은없을거라하는걸로ㅠ",
          "original_form": "그런일은없을거라하는걸로ㅠ",
          "speaker_id": "2",
          "time": "20210902 01:59"
        },
        {
          "id": "MDRW2100026958.1.35",
          "form": "글고저슬슬 잘거같아서",
