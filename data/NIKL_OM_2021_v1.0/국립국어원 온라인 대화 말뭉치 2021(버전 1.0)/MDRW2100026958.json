          "time": "20210902 01:57"
        },
        {
          "id": "MDRW2100026958.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:57"
        },
        {
          "id": "MDRW2100026958.1.20",
          "form": "스네이프.....세기의 사랑이죠",
          "original_form": "스네이프.....세기의 사랑이죠",
          "speaker_id": "1",
          "time": "20210902 01:57"
        },
        {
          "id": "MDRW2100026958.1.21",
          "form": "name3도 가끔 name4보는책 본대요",
          "original_form": "&name3&도 가끔 &name4&보는책 본대요",
          "speaker_id": "2",
          "time": "20210902 01:57"
        },
