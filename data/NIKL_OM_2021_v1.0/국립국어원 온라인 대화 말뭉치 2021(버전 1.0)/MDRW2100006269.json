      "utterance": [
        {
          "id": "MDRW2100006269.1.1",
          "form": "나는 아무거나 다 좋아",
          "original_form": "나는 아무거나 다 좋아",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006269.1.2",
          "form": "맨날 그소리시져",
          "original_form": "맨날 그소리시져",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006269.1.3",
          "form": "니네들이 먹고 싶은 걸로 시켜",
          "original_form": "니네들이 먹고 싶은 걸로 시켜",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006269.1.4",
          "form": "안드실거면서",
          "original_form": "안드실거면서",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006269.1.5",
          "form": "너무 씻기 불편하지만 않으면 돼",
          "original_form": "너무 씻기 불편하지만 않으면 돼",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006269.1.6",
          "form": "부드러운게 잘 없네요",
          "original_form": "부드러운게 잘 없네요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006269.1.7",
          "form": "그래도 다 먹긴 해",
          "original_form": "그래도 다 먹긴 해",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006269.1.8",
          "form": "그러면 다행이고요",
          "original_form": "그러면 다행이고요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006269.1.9",
          "form": "고기도 괜찮으니까 먹고 싶은 걸로 시켜",
          "original_form": "고기도 괜찮으니까 먹고 싶은 걸로 시켜",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006269.1.10",
          "form": "소고기 좀 뜯어볼까요",
          "original_form": "소고기 좀 뜯어볼까요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006269.1.11",
          "form": "돼지고기로 할까요",
          "original_form": "돼지고기로 할까요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006269.1.12",
          "form": "한우로 먹어볼까",
          "original_form": "한우로 먹어볼까",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006269.1.13",
          "form": "오이 한우",
          "original_form": "오이 한우",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006269.1.14",
          "form": "돼지고기도 상관없어",
          "original_form": "돼지고기도 상관없어",
