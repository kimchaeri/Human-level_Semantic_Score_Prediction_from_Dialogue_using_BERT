      "utterance": [
        {
          "id": "MDRW2100038298.1.1",
          "form": "격기3반은 격투기 웹툰인데",
          "original_form": "격기3반은 격투기 웹툰인데",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100038298.1.2",
          "form": "",
          "original_form": "{censored}",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100038298.1.3",
          "form": "여캐들이 멋있게 나오는 작품이 좋아요ㅎㅎ",
          "original_form": "여캐들이 멋있게 나오는 작품이 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100038298.1.4",
          "form": "갯백도 보시나요?",
          "original_form": "갯백도 보시나요?",
          "speaker_id": "2",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100038298.1.5",
          "form": "초반만 참으면 그 이후에 작가가 정신차려요 ㅋㅋㅋ",
          "original_form": "초반만 참으면 그 이후에 작가가 정신차려요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100038298.1.6",
          "form": "겟백은 한 3화 보다 말앗는데 어떤가요?",
          "original_form": "겟백은 한 3화 보다 말앗는데 어떤가요?",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100038298.1.7",
          "form": "재밌어요 캐릭터 서사도 튼튼하고",
          "original_form": "재밌어요 캐릭터 서사도 튼튼하고",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100038298.1.8",
          "form": "저도 함 봐야겠네유",
          "original_form": "저도 함 봐야겠네유",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038298.1.9",
          "form": "여성서사 그거랑 럭키언럭키도 보다가 하차햇어욬ㅋㅋ 왤케 하차한게많은지",
          "original_form": "여성서사 그거랑 럭키언럭키도 보다가 하차햇어욬ㅋㅋ 왤케 하차한게많은지",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038298.1.10",
          "form": "여성 느와르는 많이 없잖아요",
          "original_form": "여성 느와르는 많이 없잖아요",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038298.1.11",
          "form": "저는 재밌게 봤어요",
          "original_form": "저는 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038298.1.12",
          "form": "맞아요 혹시 극락왕생 보시나요?",
          "original_form": "맞아요 혹시 극락왕생 보시나요?",
          "speaker_id": "1",
          "time": "20210902 12:49"
        },
        {
          "id": "MDRW2100038298.1.13",
          "form": "보려고요! 아직 안 봤어요ㅎㅎ",
          "original_form": "보려고요! 아직 안 봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100038298.1.14",
          "form": "전 그거 되게 열심히 보고잇어요! 지금 4권 출판되엇더라구요 ㅎㅎ",
          "original_form": "전 그거 되게 열심히 보고잇어요! 지금 4권 출판되엇더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100038298.1.15",
          "form": "장르가 어떻게 되나요?",
          "original_form": "장르가 어떻게 되나요?",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100038298.1.16",
          "form": "불교..모험..? 정도의 느낌이에요",
          "original_form": "불교..모험..? 정도의 느낌이에요",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100038298.1.17",
          "form": "한국형 판타지.. 성장..",
          "original_form": "한국형 판타지.. 성장..",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100038298.1.18",
          "form": "판타지랑 성장 좋아해요ㅎㅎ",
          "original_form": "판타지랑 성장 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100038298.1.19",
          "form": "빨리 정주행 해야겠네요ㅎㅎ",
          "original_form": "빨리 정주행 해야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100038298.1.20",
          "form": "저두요 ㅎㅎㅎ 그리고 엄청 감동적이에요 억지 눈물 이런건 아니고",
          "original_form": "저두요 ㅎㅎㅎ 그리고 엄청 감동적이에요 억지 눈물 이런건 아니고",
          "speaker_id": "1",
          "time": "20210903 08:27"
        },
        {
          "id": "MDRW2100038298.1.21",
          "form": "그리고 엄청 담백한데 감동적이에요",
          "original_form": "그리고 엄청 담백한데 감동적이에요",
