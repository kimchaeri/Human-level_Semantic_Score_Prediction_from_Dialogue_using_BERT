      "utterance": [
        {
          "id": "MDRW2100022553.1.1",
          "form": "동물 뭐 좋아하세요?",
          "original_form": "동물 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022553.1.2",
          "form": "저는 강아지나 요즘 쿼카도 좋아해요 ㅋㅋ !",
          "original_form": "저는 강아지나 요즘 쿼카도 좋아해요 ㅋㅋ !",
          "speaker_id": "2",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022553.1.3",
          "form": "쿼카 귀여운데..ㅋㅋㅋ \r\n울나라에서도 키울 수 있어요?",
          "original_form": "쿼카 귀여운데..ㅋㅋㅋ \r\n울나라에서도 키울 수 있어요?",
          "speaker_id": "1",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022553.1.4",
          "form": "아니요 ㅋㅋㅋ ! 아마 한국에 없지않을까요 ㅠㅠㅠ",
          "original_form": "아니요 ㅋㅋㅋ ! 아마 한국에 없지않을까요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022553.1.5",
          "form": "저희집엔 인형이 있답니다",
          "original_form": "저희집엔 인형이 있답니다",
          "speaker_id": "2",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022553.1.6",
          "form": "아.. 인형으로 대리 만족? ㅋㅋㅋ",
          "original_form": "아.. 인형으로 대리 만족? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022553.1.7",
          "form": "진짜 키우는건 엄서여?",
          "original_form": "진짜 키우는건 엄서여?",
          "speaker_id": "1",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022553.1.8",
          "form": "네 ㅠㅠ 혹시 키우는거 있으세요?",
          "original_form": "네 ㅠㅠ 혹시 키우는거 있으세요?",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022553.1.9",
          "form": "저도 키우는건... \r\n다 키울 수 있다고 하면 뭐 키워보고싶은세요? 용이라던가..",
          "original_form": "저도 키우는건... \r\n다 키울 수 있다고 하면 뭐 키워보고싶은세요? 용이라던가..",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022553.1.10",
          "form": "용ㅋㅋㅋ 아 왤케 뜬금없죠 ㅋㅋㅋ",
          "original_form": "용ㅋㅋㅋ 아 왤케 뜬금없죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022553.1.11",
          "form": "전 그러면 유니콘 할래요 ㅎㅎㅎ",
          "original_form": "전 그러면 유니콘 할래요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022553.1.12",
          "form": "타고 날라다닐꺼에요 ㅎㅎ",
          "original_form": "타고 날라다닐꺼에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022553.1.13",
          "form": "저도.. 뿔도 팔면 재테크도 되고.. ㅋㅋㅋ",
          "original_form": "저도.. 뿔도 팔면 재테크도 되고.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022553.1.14",
          "form": "그냥 현실적으로 사자나 한마리 키우고 싶네요.. ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 현실적으로 사자나 한마리 키우고 싶네요.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022553.1.15",
          "form": "사자도 좋죠 ㅋㅋ 근데 일단 저를 안잡아먹는다는 가정 하에 키우겠어요 ,,",
          "original_form": "사자도 좋죠 ㅋㅋ 근데 일단 저를 안잡아먹는다는 가정 하에 키우겠어요 ,,",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022553.1.16",
          "form": "파충류는 가능? ㅋㅋㅋㅋ",
          "original_form": "파충류는 가능? ㅋㅋㅋㅋ",
