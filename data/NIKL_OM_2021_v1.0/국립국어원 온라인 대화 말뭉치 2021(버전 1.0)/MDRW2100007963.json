      "utterance": [
        {
          "id": "MDRW2100007963.1.1",
          "form": "점심 뭐 드세요?",
          "original_form": "점심 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007963.1.2",
          "form": "곰탕먹으려구요ㅎㅎ",
          "original_form": "곰탕먹으려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007963.1.3",
          "form": "뭐드세요?",
          "original_form": "뭐드세요?",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007963.1.4",
          "form": "제육볶음으로 정했어요!",
          "original_form": "제육볶음으로 정했어요!",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007963.1.5",
          "form": "맛있는거 드시네요ㅎㅎ",
          "original_form": "맛있는거 드시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007963.1.6",
          "form": "ㅎㅎ 그러게요 먹고 일해야해서",
          "original_form": "ㅎㅎ 그러게요 먹고 일해야해서",
          "speaker_id": "1",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007963.1.7",
          "form": "저도 일해야해서ㅎㅎ든든한걸루",
          "original_form": "저도 일해야해서ㅎㅎ든든한걸루",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007963.1.8",
          "form": "주말인데요?ㅋㅋ",
          "original_form": "주말인데요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007963.1.9",
          "form": "낮밤이 없어서 ㅠㅠ",
          "original_form": "낮밤이 없어서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100007963.1.10",
          "form": "커피가 유일한 낙이에요ㅎ",
          "original_form": "커피가 유일한 낙이에요ㅎ",
          "speaker_id": "2",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100007963.1.11",
          "form": "커피는 뭐 좋아하세요",
          "original_form": "커피는 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100007963.1.12",
          "form": "프라푸치노 좋아해요 커피 좋아하세요?",
          "original_form": "프라푸치노 좋아해요 커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100007963.1.13",
          "form": "저는 아아요!",
          "original_form": "저는 아아요!",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100007963.1.14",
          "form": "아아는 써서 못마시겠던데ㅠ",
          "original_form": "아아는 써서 못마시겠던데ㅠ",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100007963.1.15",
          "form": "ㅋㅋ시럽 넣으세요",
          "original_form": "ㅋㅋ시럽 넣으세요",
          "speaker_id": "1",
          "time": "20210822 13:52"
