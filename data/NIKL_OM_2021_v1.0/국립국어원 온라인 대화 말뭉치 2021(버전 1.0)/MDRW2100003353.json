      "utterance": [
        {
          "id": "MDRW2100003353.1.1",
          "form": "요즘 운동하시는거 있나요?",
          "original_form": "요즘 운동하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210820 22:03"
        },
        {
          "id": "MDRW2100003353.1.2",
          "form": "제자리 걷기하고 있어요",
          "original_form": "제자리 걷기하고 있어요",
          "speaker_id": "2",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003353.1.3",
          "form": "걷기운동 좋죠 전 요즘 숨쉬기운동만 해요ㅎ",
          "original_form": "걷기운동 좋죠 전 요즘 숨쉬기운동만 해요ㅎ",
          "speaker_id": "1",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003353.1.4",
          "form": "근데 요즘 바빠서 바빠서 팔로 만보했어요",
          "original_form": "근데 요즘 바빠서 바빠서 팔로 만보했어요",
          "speaker_id": "2",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003353.1.5",
          "form": "팔운동도 운동이니까요ㅎ",
          "original_form": "팔운동도 운동이니까요ㅎ",
          "speaker_id": "1",
          "time": "20210820 22:06"
        },
        {
          "id": "MDRW2100003353.1.6",
          "form": "요즘 번지피지오배워보고싶은데 근처에 없네요",
          "original_form": "요즘 번지피지오배워보고싶은데 근처에 없네요",
          "speaker_id": "1",
          "time": "20210820 22:06"
        },
        {
          "id": "MDRW2100003353.1.7",
          "form": "팔목에 무리갈까봐 걱정이네요",
          "original_form": "팔목에 무리갈까봐 걱정이네요",
          "speaker_id": "2",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003353.1.8",
          "form": "그건 뭐예요?",
          "original_form": "그건 뭐예요?",
          "speaker_id": "2",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003353.1.9",
          "form": "해방타운에나온건데 번지점프하는거처럼 장비차고 필라테스 접목된",
          "original_form": "해방타운에나온건데 번지점프하는거처럼 장비차고 필라테스 접목된",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003353.1.10",
          "form": "재밌겠더라구요",
          "original_form": "재밌겠더라구요",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003353.1.11",
          "form": "아 완전 힘들겠어요",
          "original_form": "아 완전 힘들겠어요",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003353.1.12",
          "form": "코어힘도 기르고 좋겠던데 가격이 비쌀듯해요",
          "original_form": "코어힘도 기르고 좋겠던데 가격이 비쌀듯해요",
          "speaker_id": "1",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003353.1.13",
          "form": "엄청 비쌀거 같은데요",
          "original_form": "엄청 비쌀거 같은데요",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003353.1.14",
          "form": "전 수영 배우고 싶은데",
          "original_form": "전 수영 배우고 싶은데",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003353.1.15",
          "form": "1회 몇만원할거같아요",
          "original_form": "1회 몇만원할거같아요",
          "speaker_id": "1",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003353.1.16",
          "form": "수영 재밌다는데 전 물이 무서워서",
          "original_form": "수영 재밌다는데 전 물이 무서워서",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003353.1.17",
          "form": "그렇겠지요 근데 재미는 있을듯해요",
          "original_form": "그렇겠지요 근데 재미는 있을듯해요",
          "speaker_id": "2",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003353.1.18",
          "form": "전 물에 얼굴도 못담궈요ㅎ 잠수불가",
          "original_form": "전 물에 얼굴도 못담궈요ㅎ 잠수불가",
          "speaker_id": "1",
          "time": "20210820 22:12"
        },
        {
          "id": "MDRW2100003353.1.19",
          "form": "잠수는 해서 5발자국 나가긴 하는데 ㅋㅋ 제자리래요",
          "original_form": "잠수는 해서 5발자국 나가긴 하는데 ㅋㅋ 제자리래요",
          "speaker_id": "2",
          "time": "20210820 22:13"
        },
        {
          "id": "MDRW2100003353.1.20",
          "form": "접영 배우고 싶긴해요",
          "original_form": "접영 배우고 싶긴해요",
          "speaker_id": "1",
          "time": "20210820 22:13"
        },
        {
          "id": "MDRW2100003353.1.21",
          "form": "멋있죠 ㅋㅋ그냥 수영할수 있으면 전 만족하는걸로",
          "original_form": "멋있죠 ㅋㅋ그냥 수영할수 있으면 전 만족하는걸로",
          "speaker_id": "2",
          "time": "20210820 22:14"
        },
        {
          "id": "MDRW2100003353.1.22",
          "form": "남자가 수영잘하면 멋있더라구요ㅎㅅㅈ",
          "original_form": "남자가 수영잘하면 멋있더라구요ㅎㅅㅈ",
          "speaker_id": "1",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100003353.1.23",
          "form": "운동하는 잘하는 남자는 다 멋있는거 같아요",
          "original_form": "운동하는 잘하는 남자는 다 멋있는거 같아요",
          "speaker_id": "2",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003353.1.24",
          "form": "동감이요ㅎ",
          "original_form": "동감이요ㅎ",
          "speaker_id": "1",
          "time": "20210820 22:21"
        }
      ]
    }
  ]
