      "utterance": [
        {
          "id": "MDRW2100007078.1.1",
          "form": "name1님은 요즘 읽으시는 책이 있나요??",
          "original_form": "&name1&님은 요즘 읽으시는 책이 있나요??",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007078.1.2",
          "form": "저는 요즘 데미안 읽고 있어요! 재밌네요",
          "original_form": "저는 요즘 데미안 읽고 있어요! 재밌네요",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007078.1.3",
          "form": "오 저도 데미안 읽다가.. 미루다가 아직 다 못 봤네요",
          "original_form": "오 저도 데미안 읽다가.. 미루다가 아직 다 못 봤네요",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007078.1.4",
          "form": "문학 작품 좋아하세요?",
          "original_form": "문학 작품 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007078.1.5",
          "form": "아녀 그건 아닌데 요즘 좀 읽어야겠단 생각이 들어서요ㅋㅋ",
          "original_form": "아녀 그건 아닌데 요즘 좀 읽어야겠단 생각이 들어서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007078.1.6",
          "form": "저는 한강 작가의 소년이 온다 읽고 있어요!",
          "original_form": "저는 한강 작가의 소년이 온다 읽고 있어요!",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007078.1.7",
          "form": "옛날에 중고 서점에서 사봤는데 이제 읽기 시작했어요 ㅋㅋ",
          "original_form": "옛날에 중고 서점에서 사봤는데 이제 읽기 시작했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007078.1.8",
          "form": "오 정말요! 전 채식주의자만 읽어봤는데",
          "original_form": "오 정말요! 전 채식주의자만 읽어봤는데",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007078.1.9",
          "form": "채식주의자로 유명해지셔서 이름 보고 책을 골랐어요 ㅋㅋ",
          "original_form": "채식주의자로 유명해지셔서 이름 보고 책을 골랐어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007078.1.10",
          "form": "정작 채식주의자는 안 읽어봤지만..",
          "original_form": "정작 채식주의자는 안 읽어봤지만..",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007078.1.11",
          "form": "오 정말요? 한번 읽어보시면 놀라실거에요",
          "original_form": "오 정말요? 한번 읽어보시면 놀라실거에요",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007078.1.12",
          "form": "생각보다 어둡더라구요..",
          "original_form": "생각보다 어둡더라구요..",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007078.1.13",
          "form": "헉.. 그런 내용이었구나",
          "original_form": "헉.. 그런 내용이었구나",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007078.1.14",
          "form": "소년이 온다도 어두운 내용이더라구요",
          "original_form": "소년이 온다도 어두운 내용이더라구요",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007078.1.15",
          "form": "주로 보시는 도서 장르가 있으세요??",
          "original_form": "주로 보시는 도서 장르가 있으세요??",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007078.1.16",
          "form": "전 딱히 가리는건 없어요!",
          "original_form": "전 딱히 가리는건 없어요!",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007078.1.17",
          "form": "굳이 꼽자면 판타지? 많이보네요",
          "original_form": "굳이 꼽자면 판타지? 많이보네요",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007078.1.18",
          "form": "오 저도 옛날에 브레이킹던 시리즈 되게 좋아했어요",
          "original_form": "오 저도 옛날에 브레이킹던 시리즈 되게 좋아했어요",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007078.1.19",
          "form": "그런 판타지류가 약간 상상하기도 좋은",
          "original_form": "그런 판타지류가 약간 상상하기도 좋은",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007078.1.20",
          "form": "브레이킹던 보셨나요??",
          "original_form": "브레이킹던 보셨나요??",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007078.1.21",
          "form": "트와일라잇 시리즈! 저는 2번째 편까지만 봤어요",
          "original_form": "트와일라잇 시리즈! 저는 2번째 편까지만 봤어요",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007078.1.22",
          "form": "아 맞다 그거 트와일라잇이었죠",
          "original_form": "아 맞다 그거 트와일라잇이었죠",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007078.1.23",
          "form": "처음 그게 생각이 안나서 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "처음 그게 생각이 안나서 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007078.1.24",
          "form": "저도 사실 끝까지는 못 봤어요",
          "original_form": "저도 사실 끝까지는 못 봤어요",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007078.1.25",
          "form": "책이 너무 두꺼워서 별로 엄두가 안나더라구요ㅋㅋ",
          "original_form": "책이 너무 두꺼워서 별로 엄두가 안나더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:58"
        }
      ]
