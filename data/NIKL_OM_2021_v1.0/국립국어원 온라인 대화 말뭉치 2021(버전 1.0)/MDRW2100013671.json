      "utterance": [
        {
          "id": "MDRW2100013671.1.1",
          "form": "정모해보신 적 있으신가요?",
          "original_form": "정모해보신 적 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013671.1.2",
          "form": "아니요 딱히 정모는 해본 적이 없어요",
          "original_form": "아니요 딱히 정모는 해본 적이 없어요",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013671.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013671.1.4",
          "form": "저도 정모는 경험해보지 못했어요",
          "original_form": "저도 정모는 경험해보지 못했어요",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013671.1.5",
          "form": "ㅋㅋ ㅋㅋ 전 딱",
          "original_form": "ㅋㅋ ㅋㅋ 전 딱",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100013671.1.6",
          "form": "한두먕씩은 만나봤어요",
          "original_form": "한두먕씩은 만나봤어요",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100013671.1.7",
          "form": "어떤 종류의 모임이었나요? 게임인가요?",
          "original_form": "어떤 종류의 모임이었나요? 게임인가요?",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100013671.1.8",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100013671.1.9",
          "form": "저 덕질하몀서",
          "original_form": "저 덕질하몀서",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100013671.1.10",
          "form": "만난거였어요",
          "original_form": "만난거였어요",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100013671.1.11",
          "form": "같은 작품을 좋아하는 분들과 만나셨군요",
          "original_form": "같은 작품을 좋아하는 분들과 만나셨군요",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100013671.1.12",
          "form": "네 ㅎ 그래서",
          "original_form": "네 ㅎ 그래서",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100013671.1.13",
          "form": "그나마 편했어요",
          "original_form": "그나마 편했어요",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100013671.1.14",
          "form": "저도 해보고 싶은데 저는 커뮤니티에서 친목을 다지지 않아서요ㅋㅋ",
          "original_form": "저도 해보고 싶은데 저는 커뮤니티에서 친목을 다지지 않아서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100013671.1.15",
          "form": "ㅋㅋ ㅋㅋㅋ 저도 그냥",
          "original_form": "ㅋㅋ ㅋㅋㅋ 저도 그냥",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100013671.1.16",
          "form": "일회성 만남이였어요",
          "original_form": "일회성 만남이였어요",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100013671.1.17",
          "form": "이상하게 온라인 익명이지만 말 걸기가 쉽지 않더라구요ㅠ",
          "original_form": "이상하게 온라인 익명이지만 말 걸기가 쉽지 않더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100013671.1.18",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100013671.1.19",
          "form": "ㅋㅋ 그것도 어려워요",
          "original_form": "ㅋㅋ 그것도 어려워요",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100013671.1.20",
          "form": "해보고 싶긴 한데 갑자기 말걸면 당황하실 것 같아서 내적 친밀감만 쌓았어요ㅋㅋ",
          "original_form": "해보고 싶긴 한데 갑자기 말걸면 당황하실 것 같아서 내적 친밀감만 쌓았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100013671.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
