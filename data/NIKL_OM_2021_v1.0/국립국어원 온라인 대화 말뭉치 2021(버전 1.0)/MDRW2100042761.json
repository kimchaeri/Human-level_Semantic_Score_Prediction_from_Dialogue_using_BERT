        {
          "id": "MDRW2100042761.1.1",
          "form": "ㅋㅋㅋ우유싫어하는구나ㅋ",
          "original_form": "ㅋㅋㅋ우유싫어하는구나ㅋ",
          "speaker_id": "2",
          "time": "20210907 08:38"
        },
        {
          "id": "MDRW2100042761.1.2",
          "form": "학교에서 밥 먹는 게  글에 싫단다ㅠ",
          "original_form": "학교에서 밥 먹는 게  글에 싫단다ㅠ",
          "speaker_id": "1",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042761.1.3",
          "form": "급식에서 우유 나오면",
          "original_form": "급식에서 우유 나오면",
          "speaker_id": "1",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042761.1.4",
          "form": "조금만 먹고 버린다는데;",
          "original_form": "조금만 먹고 버린다는데;",
          "speaker_id": "1",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042761.1.5",
          "form": "아 그렇구나ㅜ",
          "original_form": "아 그렇구나ㅜ",
          "speaker_id": "2",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042761.1.6",
          "form": "배고프겠다",
          "original_form": "배고프겠다",
          "speaker_id": "2",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042761.1.7",
          "form": "집에오면",
          "original_form": "집에오면",
          "speaker_id": "2",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042761.1.8",
          "form": "근데 또 집에 오면 잘 안 먹어",
          "original_form": "근데 또 집에 오면 잘 안 먹어",
          "speaker_id": "1",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042761.1.9",
          "form": "식욕이 없는 아이;",
          "original_form": "식욕이 없는 아이;",
          "speaker_id": "1",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042761.1.10",
          "form": "내가 식욕이없어야되는데ㅋㅋ",
          "original_form": "내가 식욕이없어야되는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042761.1.11",
          "form": "나는 진짜 이제는",
          "original_form": "나는 진짜 이제는",
          "speaker_id": "1",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042761.1.12",
