      "utterance": [
        {
          "id": "MDRW2100006180.1.1",
          "form": "울적한가요?",
          "original_form": "울적한가요?",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006180.1.2",
          "form": "가을이 오니까",
          "original_form": "가을이 오니까",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006180.1.3",
          "form": "옛사랑 생각나나요?",
          "original_form": "옛사랑 생각나나요?",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006180.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ음....",
          "original_form": "ㅋㅋㅋㅋㅋㅋ음....",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006180.1.5",
          "form": "그냥 가을 바람이\r\n좀 그런게 있지 않나요?",
          "original_form": "그냥 가을 바람이\r\n좀 그런게 있지 않나요?",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006180.1.6",
          "form": "가을바람 가을밤공기 이런게",
          "original_form": "가을바람 가을밤공기 이런게",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006180.1.7",
          "form": "사람을 센치하게하는ㅋㅋ",
          "original_form": "사람을 센치하게하는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006180.1.8",
          "form": "뭐요?",
          "original_form": "뭐요?",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006180.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006180.1.10",
          "form": "모르시나보네",
          "original_form": "모르시나보네",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006180.1.11",
          "form": "애송이같으니라구ㅋㅋㅋㅋ요ㅋㅋㅋ",
          "original_form": "애송이같으니라구ㅋㅋㅋㅋ요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006180.1.12",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006180.1.13",
          "form": "애들은",
          "original_form": "애들은",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006180.1.14",
          "form": "엄마가 이렇게 가을타는걸 아나요?",
          "original_form": "엄마가 이렇게 가을타는걸 아나요?",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006180.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006180.1.16",
          "form": "아니... 거 가족은 건들지 맙시다",
          "original_form": "아니... 거 가족은 건들지 맙시다",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006180.1.17",
          "form": "ㅋㅋ 크고나면 알지도 모르겠네요",
          "original_form": "ㅋㅋ 크고나면 알지도 모르겠네요",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006180.1.18",
          "form": "그때 우리엄마 가을 탔었나보다",
          "original_form": "그때 우리엄마 가을 탔었나보다",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006180.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100006180.1.20",
          "form": "그런거 비슷하게 닮던데",
          "original_form": "그런거 비슷하게 닮던데",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006180.1.21",
          "form": "계절타는거",
          "original_form": "계절타는거",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006180.1.22",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006180.1.23",
          "form": "우리 아빠가 탔으려나",
          "original_form": "우리 아빠가 탔으려나",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006180.1.24",
          "form": "우리 엄마가 탔으려나",
          "original_form": "우리 엄마가 탔으려나",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006180.1.25",
          "form": "그래도 가을 좋아요",
          "original_form": "그래도 가을 좋아요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006180.1.26",
          "form": "아빠가 가능성 높지 않을까요?",
          "original_form": "아빠가 가능성 높지 않을까요?",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006180.1.27",
          "form": "가을은 남자의 계절",
          "original_form": "가을은 남자의 계절",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006180.1.28",
          "form": "ㅋㅋ무슨 소리시죠?",
          "original_form": "ㅋㅋ무슨 소리시죠?",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006180.1.29",
          "form": "헐크로 변하려고 가을타시는건",
          "original_form": "헐크로 변하려고 가을타시는건",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006180.1.30",
          "form": "가을여자 모르세요? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "가을여자 모르세요? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006180.1.31",
          "form": "죄송....",
          "original_form": "죄송....",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006180.1.32",
          "form": "name2님 오늘 공격적이시네여ㅋㅋ  호호",
          "original_form": "&name2&님 오늘 공격적이시네여ㅋㅋ  호호",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006180.1.33",
          "form": "장난이자나요",
          "original_form": "장난이자나요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006180.1.34",
          "form": "겨울에 만납시다",
          "original_form": "겨울에 만납시다",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006180.1.35",
          "form": "눈뭉치에다가 돌 넣어서",
          "original_form": "눈뭉치에다가 돌 넣어서",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100006180.1.36",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100006180.1.37",
          "form": "던질랑께ㅋㅋ",
          "original_form": "던질랑께ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100006180.1.38",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100006180.1.39",
          "form": "그거 맞으면 죽는건데",
          "original_form": "그거 맞으면 죽는건데",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100006180.1.40",
          "form": "무서운분이시네요",
          "original_form": "무서운분이시네요",
