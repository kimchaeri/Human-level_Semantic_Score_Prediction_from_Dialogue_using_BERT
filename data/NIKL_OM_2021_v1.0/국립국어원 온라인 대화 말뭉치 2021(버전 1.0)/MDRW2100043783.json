      "utterance": [
        {
          "id": "MDRW2100043783.1.1",
          "form": "핫케이크 맛있겠어요",
          "original_form": "핫케이크 맛있겠어요",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043783.1.2",
          "form": "달달하니~",
          "original_form": "달달하니~",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043783.1.3",
          "form": "name1님은 오늘 뭐 드셨어요??",
          "original_form": "&name1&님은 오늘 뭐 드셨어요??",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043783.1.4",
          "form": "저는 어묵탕이요",
          "original_form": "저는 어묵탕이요",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043783.1.5",
          "form": "쌀쌀하고 비와서",
          "original_form": "쌀쌀하고 비와서",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043783.1.6",
          "form": "어묵탕도 좋네요 만들어드셨나요?",
          "original_form": "어묵탕도 좋네요 만들어드셨나요?",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043783.1.7",
          "form": "네 집에서 간단히요ㅎㅎ",
          "original_form": "네 집에서 간단히요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043783.1.8",
          "form": "name1님은 요리를 잘하시나요??",
          "original_form": "&name1&님은 요리를 잘하시나요??",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043783.1.9",
          "form": "아뇨 그냥 맛있게 먹는 걸 좋아해서",
          "original_form": "아뇨 그냥 맛있게 먹는 걸 좋아해서",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043783.1.10",
          "form": "이것저것 시도해봐요",
          "original_form": "이것저것 시도해봐요",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043783.1.11",
          "form": "와아 최근엔 어떤 음식 시도해보셨어요?",
          "original_form": "와아 최근엔 어떤 음식 시도해보셨어요?",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043783.1.12",
          "form": "최근엔 큐브파운드 케이크 만들어봤고",
          "original_form": "최근엔 큐브파운드 케이크 만들어봤고",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043783.1.13",
          "form": "요즘 코울슬로 샐러드 자주 만들어요",
          "original_form": "요즘 코울슬로 샐러드 자주 만들어요",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043783.1.14",
          "form": "아닛... 솜씨가 좋으신데요?",
          "original_form": "아닛... 솜씨가 좋으신데요?",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043783.1.15",
          "form": "코우슬로 샐러드!",
          "original_form": "코우슬로 샐러드!",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043783.1.16",
          "form": "예전엔 맛있다고 느끼지 못했는데 최근에 되게 맛있다고 느끼고 있어요",
          "original_form": "예전엔 맛있다고 느끼지 못했는데 최근에 되게 맛있다고 느끼고 있어요",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043783.1.17",
          "form": "양배추가 많아서",
          "original_form": "양배추가 많아서",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043783.1.18",
          "form": "소비하느라 만들었는데 맛있더라고요",
          "original_form": "소비하느라 만들었는데 맛있더라고요",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043783.1.19",
          "form": "여름에 자주 먹었어요",
          "original_form": "여름에 자주 먹었어요",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043783.1.20",
          "form": "샐러드에 또 어떤걸 같이 드셨어요?",
          "original_form": "샐러드에 또 어떤걸 같이 드셨어요?",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043783.1.21",
          "form": "샐러드만 먹기는 배가 너무 고팠을 거 같아요",
          "original_form": "샐러드만 먹기는 배가 너무 고팠을 거 같아요",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043783.1.22",
          "form": "아 식빵 한쪽 곁들이기도 하고",
          "original_form": "아 식빵 한쪽 곁들이기도 하고",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043783.1.23",
          "form": "치킨 한두조각이랑 먹기도 했어요",
          "original_form": "치킨 한두조각이랑 먹기도 했어요",
          "speaker_id": "1",
          "time": "20210907 20:34"
        }
      ]
