      "utterance": [
        {
          "id": "MDRW2100028086.1.1",
          "form": "오늘 청소하셨나요?",
          "original_form": "오늘 청소하셨나요?",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028086.1.2",
          "form": "다음주에",
          "original_form": "다음주에",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028086.1.3",
          "form": "이사가는지라",
          "original_form": "이사가는지라",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028086.1.4",
          "form": "요즘 열심히",
          "original_form": "요즘 열심히",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028086.1.5",
          "form": "청소하고 있습니다",
          "original_form": "청소하고 있습니다",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028086.1.6",
          "form": "물론 오늘 아침에도 했죠",
          "original_form": "물론 오늘 아침에도 했죠",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028086.1.7",
          "form": "헐 아침에 청소를 하다니",
          "original_form": "헐 아침에 청소를 하다니",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028086.1.8",
          "form": "일단",
          "original_form": "일단",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028086.1.9",
          "form": "이사 가면",
          "original_form": "이사 가면",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028086.1.10",
          "form": "로봇 청소기부터",
          "original_form": "로봇 청소기부터",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028086.1.11",
          "form": "살 생각입니다",
          "original_form": "살 생각입니다",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028086.1.12",
          "form": "12평이니까",
          "original_form": "12평이니까",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028086.1.13",
          "form": "솔직히 지금 집은",
          "original_form": "솔직히 지금 집은",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028086.1.14",
          "form": "실평수 4.5평이라",
          "original_form": "실평수 4.5평이라",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028086.1.15",
          "form": "핸드 청소기랑",
          "original_form": "핸드 청소기랑",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028086.1.16",
          "form": "일회용 행주로 했는데",
          "original_form": "일회용 행주로 했는데",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028086.1.17",
          "form": "아니 바닥 훔치는 청소 도구가 나쁜 뜻으로 쓰이는건 알지만 지금은 문맥이 맞잖아...",
          "original_form": "아니 바닥 훔치는 청소 도구가 나쁜 뜻으로 쓰이는건 알지만 지금은 문맥이 맞잖아...",
          "speaker_id": "1",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028086.1.18",
          "form": "저도 몇 번 지적당했어요",
          "original_form": "저도 몇 번 지적당했어요",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028086.1.19",
          "form": "설거지하셨나요?",
          "original_form": "설거지하셨나요?",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028086.1.20",
          "form": "식기세척기도 사세요",
          "original_form": "식기세척기도 사세요",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028086.1.21",
          "form": "확실히",
          "original_form": "확실히",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028086.1.22",
          "form": "자취하면서 제일 귀찮은게",
          "original_form": "자취하면서 제일 귀찮은게",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028086.1.23",
          "form": "씻는거랑 설거지...",
          "original_form": "씻는거랑 설거지...",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028086.1.24",
          "form": "청소는 며칠 안해도 막 티가 안나는데",
          "original_form": "청소는 며칠 안해도 막 티가 안나는데",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028086.1.25",
          "form": "설거지는 이틀만 안해도",
          "original_form": "설거지는 이틀만 안해도",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028086.1.26",
          "form": "집에서 아무 것도 안 먹으면 설거지를 안 할 수 있어요",
          "original_form": "집에서 아무 것도 안 먹으면 설거지를 안 할 수 있어요",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028086.1.27",
          "form": "자기 주장을 하잖아요",
          "original_form": "자기 주장을 하잖아요",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028086.1.28",
          "form": "배달 음식시켜 먹으세요",
          "original_form": "배달 음식시켜 먹으세요",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028086.1.29",
          "form": "의외로 배달 음식은",
          "original_form": "의외로 배달 음식은",
          "speaker_id": "1",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028086.1.30",
          "form": "2~3주에 한 번 정도만 먹는지라;;",
          "original_form": "2~3주에 한 번 정도만 먹는지라;;",
          "speaker_id": "1",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028086.1.31",
          "form": "자취하면서 해먹다니 훌륭하시네요",
          "original_form": "자취하면서 해먹다니 훌륭하시네요",
          "speaker_id": "2",
          "time": "20210902 18:47"
        }
      ]
    }
