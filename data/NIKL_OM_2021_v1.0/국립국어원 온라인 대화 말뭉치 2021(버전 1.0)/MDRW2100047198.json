      "utterance": [
        {
          "id": "MDRW2100047198.1.1",
          "form": "name2님 좋았던 국내여행은 어디였어요?",
          "original_form": "&name2&님 좋았던 국내여행은 어디였어요?",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100047198.1.2",
          "form": "저 서울이요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 서울이요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:26"
        },
        {
          "id": "MDRW2100047198.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 08:26"
        },
        {
          "id": "MDRW2100047198.1.4",
          "form": "저도 서울 좋아요ㅠㅠ",
          "original_form": "저도 서울 좋아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100047198.1.5",
          "form": "안간지 진짜 오래됐네용",
          "original_form": "안간지 진짜 오래됐네용",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100047198.1.6",
          "form": "전 부산도 좋았어요",
          "original_form": "전 부산도 좋았어요",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100047198.1.7",
          "form": "전 부산은 안가봤어요ㅋㅋ",
          "original_form": "전 부산은 안가봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100047198.1.8",
          "form": "좋도라구요!",
          "original_form": "좋도라구요!",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100047198.1.9",
          "form": "전 바다 좋아해서 그런가바여ㅎㅎ",
          "original_form": "전 바다 좋아해서 그런가바여ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100047198.1.10",
          "form": "ㅎ 부산 가보고 싶어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅎ 부산 가보고 싶어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100047198.1.11",
          "form": "여름이 진짜 좋은데 아니어도 좋은 거 같아요",
          "original_form": "여름이 진짜 좋은데 아니어도 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047198.1.12",
          "form": "맛있는게 넘 많더라구오",
          "original_form": "맛있는게 넘 많더라구오",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047198.1.13",
          "form": "ㅋㅋ 헉 뭐드샸어뇨?",
          "original_form": "ㅋㅋ 헉 뭐드샸어뇨?",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047198.1.14",
          "form": "국밥도 먹구 곱창 이런거 먹었어요",
          "original_form": "국밥도 먹구 곱창 이런거 먹었어요",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100047198.1.15",
          "form": "회도 먹고!",
          "original_form": "회도 먹고!",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100047198.1.16",
          "form": "ㅎ ㅎㅎ 헐",
          "original_form": "ㅎ ㅎㅎ 헐",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047198.1.17",
          "form": "맛있겠네요 ㅠㅠ",
          "original_form": "맛있겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047198.1.18",
          "form": "담에 여행가면 드셔보세욬ㅋㅋㅋ",
          "original_form": "담에 여행가면 드셔보세욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100047198.1.19",
          "form": "엄청 맛있는 돼지껍데기 집도 있대요 부산에",
          "original_form": "엄청 맛있는 돼지껍데기 집도 있대요 부산에",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100047198.1.20",
          "form": "ㅎ 우와.. 감사합니다!!",
          "original_form": "ㅎ 우와.. 감사합니다!!",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047198.1.21",
          "form": "전 여행가면 소품샵 가는 것도 좋아해요ㅎㄹ",
          "original_form": "전 여행가면 소품샵 가는 것도 좋아해요ㅎㄹ",
          "speaker_id": "1",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100047198.1.22",
          "form": "아 좋아요",
          "original_form": "아 좋아요",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100047198.1.23",
          "form": "아기자기한것들 ㅜㅠ",
