      "utterance": [
        {
          "id": "MDRW2100033420.1.1",
          "form": "안녕하세요! 독서 좋아하세요?",
          "original_form": "안녕하세요! 독서 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 07:52"
        },
        {
          "id": "MDRW2100033420.1.2",
          "form": "네네 요즘은 잘 못 읽었어요",
          "original_form": "네네 요즘은 잘 못 읽었어요",
          "speaker_id": "2",
          "time": "20210907 07:52"
        },
        {
          "id": "MDRW2100033420.1.3",
          "form": "자주 보세요?",
          "original_form": "자주 보세요?",
          "speaker_id": "2",
          "time": "20210907 07:53"
        },
        {
          "id": "MDRW2100033420.1.4",
          "form": "앗 저도 잘 못 읽었어요ㅋㅋㅋ 어떤 분야 좋아하세요?",
          "original_form": "앗 저도 잘 못 읽었어요ㅋㅋㅋ 어떤 분야 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 07:53"
        },
        {
          "id": "MDRW2100033420.1.5",
          "form": "저는 에세이 자기계발서요",
          "original_form": "저는 에세이 자기계발서요",
          "speaker_id": "2",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100033420.1.6",
          "form": "최근엔 소설 한강, 채식주의자 읽었어요",
          "original_form": "최근엔 소설 한강, 채식주의자 읽었어요",
          "speaker_id": "2",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100033420.1.7",
          "form": "와 둘 다 되게 유명해서 내용이 궁금했는데 재밌게 읽으셨나요?",
          "original_form": "와 둘 다 되게 유명해서 내용이 궁금했는데 재밌게 읽으셨나요?",
          "speaker_id": "1",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100033420.1.8",
          "form": "한강 작가가 좀 호불호가 있는거같아요",
          "original_form": "한강 작가가 좀 호불호가 있는거같아요",
          "speaker_id": "2",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100033420.1.9",
          "form": "채식주의자도 상 받지않았다면 안봤울거에요 좀 잔인하고 기괴해요ㅠㅠ",
          "original_form": "채식주의자도 상 받지않았다면 안봤울거에요 좀 잔인하고 기괴해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100033420.1.10",
          "form": "아 잔인하고 기괴해요? 따뜻한 느낌인 줄 알았는데 아니었네요ㅜㅜ",
          "original_form": "아 잔인하고 기괴해요? 따뜻한 느낌인 줄 알았는데 아니었네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 07:58"
        },
        {
          "id": "MDRW2100033420.1.11",
          "form": "요즘 읽고 싶은 책 있으신가요?",
          "original_form": "요즘 읽고 싶은 책 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 07:58"
        },
        {
          "id": "MDRW2100033420.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 08:04"
        },
        {
          "id": "MDRW2100033420.1.13",
          "form": "아 달러구트 꿈 백화점 궁금해요",
          "original_form": "아 달러구트 꿈 백화점 궁금해요",
          "speaker_id": "2",
          "time": "20210907 08:04"
        },
        {
          "id": "MDRW2100033420.1.14",
          "form": "아 그것도 진짜 재밌을 것 같던데",
          "original_form": "아 그것도 진짜 재밌을 것 같던데",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100033420.1.15",
          "form": "이번 주말에 도서관에서 책 빌려봐야겠어요",
          "original_form": "이번 주말에 도서관에서 책 빌려봐야겠어요",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100033420.1.16",
          "form": "도서관 이용하시나요?",
          "original_form": "도서관 이용하시나요?",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100033420.1.17",
          "form": "아 코로나 이후로는 안갔어요",
          "original_form": "아 코로나 이후로는 안갔어요",
          "speaker_id": "2",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100033420.1.18",
          "form": "저는 밀리의 서재 앱으로 봅니다",
          "original_form": "저는 밀리의 서재 앱으로 봅니다",
          "speaker_id": "2",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100033420.1.19",
          "form": "직접 빌려보세요? 멋지다",
          "original_form": "직접 빌려보세요? 멋지다",
          "speaker_id": "2",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100033420.1.20",
          "form": "와 그 앱 되게 궁금했는데",
          "original_form": "와 그 앱 되게 궁금했는데",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100033420.1.21",
          "form": "써보니까 어떠신가요?",
          "original_form": "써보니까 어떠신가요?",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100033420.1.22",
          "form": "좋은데 종이책 잀는거랑 달라요ㅠㅠ",
          "original_form": "좋은데 종이책 잀는거랑 달라요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100033420.1.23",
          "form": "난독이 온달까 ㅋㅋㅋㅋ",
          "original_form": "난독이 온달까 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100033420.1.24",
          "form": "대충보게 되어요",
          "original_form": "대충보게 되어요",
          "speaker_id": "2",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100033420.1.25",
          "form": "그쵸ㅜㅜ 아무래도 모바일이 그런 게 아쉬운 것 같아요",
          "original_form": "그쵸ㅜㅜ 아무래도 모바일이 그런 게 아쉬운 것 같아요",
          "speaker_id": "1",
          "time": "20210907 08:06"
        }
      ]
