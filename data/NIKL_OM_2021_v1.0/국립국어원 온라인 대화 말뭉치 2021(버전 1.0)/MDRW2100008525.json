      "utterance": [
        {
          "id": "MDRW2100008525.1.1",
          "form": "침대 테이블 살려고",
          "original_form": "침대 테이블 살려고",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008525.1.2",
          "form": "어디꺼 살라고?",
          "original_form": "어디꺼 살라고?",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008525.1.3",
          "form": "그냥 산껄로 책상 살려니 내년에 이사가면 공간 없을꺼 같아서",
          "original_form": "그냥 산껄로 책상 살려니 내년에 이사가면 공간 없을꺼 같아서",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008525.1.4",
          "form": "어디로 이사가는데?",
          "original_form": "어디로 이사가는데?",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008525.1.5",
          "form": "책상 겸 공간활용하게",
          "original_form": "책상 겸 공간활용하게",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008525.1.6",
          "form": "address으로 이사가지 싶다",
          "original_form": "&address&으로 이사가지 싶다",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008525.1.7",
          "form": "요새 그런 제품 많드라",
          "original_form": "요새 그런 제품 많드라",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008525.1.8",
          "form": "집  구했어?",
          "original_form": "집  구했어?",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008525.1.9",
          "form": "엉 다리에 바퀴달려서 움직이기도 편하고",
          "original_form": "엉 다리에 바퀴달려서 움직이기도 편하고",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008525.1.10",
          "form": "이사가면 사야 될 제품들이 많겠다",
          "original_form": "이사가면 사야 될 제품들이 많겠다",
          "speaker_id": "1",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008525.1.11",
          "form": "아니 집은 아직 돈만 있으면 금방이지  지금집도 삼일만에 결정하고 이사했는데뭐",
          "original_form": "아니 집은 아직 돈만 있으면 금방이지  지금집도 삼일만에 결정하고 이사했는데뭐",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008525.1.12",
          "form": "우리도 이사가거나 리모델링 해야 되는데...  그래서 지금 물건을 못 사겠다",
          "original_form": "우리도 이사가거나 리모델링 해야 되는데...  그래서 지금 물건을 못 사겠다",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008525.1.13",
          "form": "가전제품 다 바꿀듯하다 언니가",
          "original_form": "가전제품 다 바꿀듯하다 언니가",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008525.1.14",
          "form": "이사가면 가전제품 다 바꺼야  함",
          "original_form": "이사가면 가전제품 다 바꺼야  함",
          "speaker_id": "1",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008525.1.15",
          "form": "공간때문에 뭐 사기도 힘들다 정말",
          "original_form": "공간때문에 뭐 사기도 힘들다 정말",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008525.1.16",
          "form": "더 큰집으로 갈거 아니가?",
          "original_form": "더 큰집으로 갈거 아니가?",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008525.1.17",
          "form": "아니다 더 작은집으로 가서리 책상도 못사는거다",
          "original_form": "아니다 더 작은집으로 가서리 책상도 못사는거다",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008525.1.18",
          "form": "address의 오래된아파트 거기서 재건축을 노릴꺼란다",
          "original_form": "&address&의 오래된아파트 거기서 재건축을 노릴꺼란다",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008525.1.19",
          "form": "아 그런거야?  그럼 물건 사기도 그렇겠다",
          "original_form": "아 그런거야?  그럼 물건 사기도 그렇겠다",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008525.1.20",
          "form": "올겨울 가습기 사야할거같다",
          "original_form": "올겨울 가습기 사야할거같다",
          "speaker_id": "2",
