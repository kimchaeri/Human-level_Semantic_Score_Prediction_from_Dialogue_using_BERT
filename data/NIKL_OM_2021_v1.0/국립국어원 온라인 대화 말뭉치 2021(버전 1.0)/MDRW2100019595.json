      "utterance": [
        {
          "id": "MDRW2100019595.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019595.1.2",
          "form": "최근에 영화본 것 있어요?",
          "original_form": "최근에 영화본 것 있어요?",
          "speaker_id": "1",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019595.1.3",
          "form": "블랙위도우 봤어요",
          "original_form": "블랙위도우 봤어요",
          "speaker_id": "2",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019595.1.4",
          "form": "최근에 영화보신거 있으신가요?",
          "original_form": "최근에 영화보신거 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019595.1.5",
          "form": "저도 블랙위도 봤어요!",
          "original_form": "저도 블랙위도 봤어요!",
          "speaker_id": "1",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019595.1.6",
          "form": "가장 최근에 본 건 인질, 씽크홀 이 두개요~",
          "original_form": "가장 최근에 본 건 인질, 씽크홀 이 두개요~",
          "speaker_id": "1",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019595.1.7",
          "form": "인질 , 씽크홀 둘중 어느것이 더 재밌나요?",
          "original_form": "인질 , 씽크홀 둘중 어느것이 더 재밌나요?",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019595.1.8",
          "form": "저는 씽크홀이 훨씬 더 나았어요.",
          "original_form": "저는 씽크홀이 훨씬 더 나았어요.",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019595.1.9",
          "form": "실제로 일어날 법한 재난 블록버스터여서 생동감 있었거든요",
          "original_form": "실제로 일어날 법한 재난 블록버스터여서 생동감 있었거든요",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019595.1.10",
          "form": "재난 블록버스터 엑시트는 재밌게 봤었는데",
          "original_form": "재난 블록버스터 엑시트는 재밌게 봤었는데",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019595.1.11",
          "form": "보셨었나요?",
          "original_form": "보셨었나요?",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019595.1.12",
          "form": "엑시트가 그 조정석 배우 나오는 거 말씀이시죠?",
          "original_form": "엑시트가 그 조정석 배우 나오는 거 말씀이시죠?",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019595.1.13",
          "form": "네! 그영화예요",
          "original_form": "네! 그영화예요",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019595.1.14",
          "form": "저 그거 봤는데 진짜 재난교육용으로 써도 될 것 같더라구요",
          "original_form": "저 그거 봤는데 진짜 재난교육용으로 써도 될 것 같더라구요",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019595.1.15",
          "form": "따 따따~",
          "original_form": "따 따따~",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019595.1.16",
          "form": "그거랑 비교해서 재미가 비슷한가요?",
          "original_form": "그거랑 비교해서 재미가 비슷한가요?",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019595.1.17",
          "form": "씽크홀이요",
          "original_form": "씽크홀이요",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019595.1.18",
          "form": "차승원 배우님이 너무 웃겨서 ㅋㅋㅋ 중간 중간에 피식 많이 했어요",
          "original_form": "차승원 배우님이 너무 웃겨서 ㅋㅋㅋ 중간 중간에 피식 많이 했어요",
          "speaker_id": "1",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019595.1.19",
          "form": "한번 기회되면 봐야겠어요ㅋㅋㅋ 알려주셔서 감사해요",
          "original_form": "한번 기회되면 봐야겠어요ㅋㅋㅋ 알려주셔서 감사해요",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019595.1.20",
          "form": "근데 엑시트는 뭔가 장면도 빠르고 긴장감도 있잖아요?",
          "original_form": "근데 엑시트는 뭔가 장면도 빠르고 긴장감도 있잖아요?",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019595.1.21",
          "form": "근데 씽크홀은 장소가 한정되어 있어서 전개가 빠르진 않아요",
          "original_form": "근데 씽크홀은 장소가 한정되어 있어서 전개가 빠르진 않아요",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019595.1.22",
          "form": "아 그런 차이가 있군요 지루할수도 있나요?",
          "original_form": "아 그런 차이가 있군요 지루할수도 있나요?",
          "speaker_id": "2",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100019595.1.23",
          "form": "딱히 지루하진 않았는데 긴장감 넘치진 않더라구요 ㅋㅋ",
          "original_form": "딱히 지루하진 않았는데 긴장감 넘치진 않더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019595.1.24",
          "form": "원래 재난은 막 생명위험이 느껴질 정도로 아슬아슬해야되는데 뭔가 안정적이였어요",
          "original_form": "원래 재난은 막 생명위험이 느껴질 정도로 아슬아슬해야되는데 뭔가 안정적이였어요",
          "speaker_id": "1",
          "time": "20210827 21:06"
        }
      ]
