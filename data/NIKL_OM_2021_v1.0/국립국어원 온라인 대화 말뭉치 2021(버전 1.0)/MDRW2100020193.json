      "utterance": [
        {
          "id": "MDRW2100020193.1.1",
          "form": "우리들은 혈액이다",
          "original_form": "우리들은 혈액이다",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020193.1.2",
          "form": "막힘없이 흘러라 맞나요 ㅋㅋㅋㅋ",
          "original_form": "막힘없이 흘러라 맞나요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020193.1.3",
          "form": "혈류가 되어...?!",
          "original_form": "혈류가 되어...?!",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020193.1.4",
          "form": "아 아닌가 ㅎ",
          "original_form": "아 아닌가 ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020193.1.5",
          "form": "본지가 오래돼서 기억이 안나네요ㅋㅋㅋ 잼게봤었는데..",
          "original_form": "본지가 오래돼서 기억이 안나네요ㅋㅋㅋ 잼게봤었는데..",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020193.1.6",
          "form": "어쨌거나 피와 관련된 무언가였어요 ㅋㅋㅋ",
          "original_form": "어쨌거나 피와 관련된 무언가였어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020193.1.7",
          "form": "스포츠물에 빠져서 한창 애니 많이 봤었거든요",
          "original_form": "스포츠물에 빠져서 한창 애니 많이 봤었거든요",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020193.1.8",
          "form": "저도요저도요",
          "original_form": "저도요저도요",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020193.1.9",
          "form": "애니가 극적인게 많잖아요",
          "original_form": "애니가 극적인게 많잖아요",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020193.1.10",
          "form": "제친구가 하이큐 아오바죠사이에 빠져서",
          "original_form": "제친구가 하이큐 아오바죠사이에 빠져서",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020193.1.11",
          "form": "자꾸 보라는거에요 그래서 보게됬었어요 ㅋㅋㅋ",
          "original_form": "자꾸 보라는거에요 그래서 보게됬었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020193.1.12",
          "form": "그 민트색 체육복 ㅋㅋ",
          "original_form": "그 민트색 체육복 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020193.1.13",
          "form": "아 금방 검색해보고 왔어요ㅋㅋㅋ민트색 유니폼",
          "original_form": "아 금방 검색해보고 왔어요ㅋㅋㅋ민트색 유니폼",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020193.1.14",
          "form": "여기 주장 잘생겼었어요 나름",
          "original_form": "여기 주장 잘생겼었어요 나름",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020193.1.15",
          "form": "오이카와...?",
          "original_form": "오이카와...?",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020193.1.16",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020193.1.17",
          "form": "맞아요맞아요 ㅋㅋㅋ 인기많아요 ㅋㅋㅋ",
          "original_form": "맞아요맞아요 ㅋㅋㅋ 인기많아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020193.1.18",
          "form": "진짜 재밌게 봤는데 결말이 기억안나네요ㅋㅋㅋㅠ",
          "original_form": "진짜 재밌게 봤는데 결말이 기억안나네요ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020193.1.19",
          "form": "근데 이친구가 이전에는 크게 휘두르며를 추천하는 바람에 야구도..봤죠...",
          "original_form": "근데 이친구가 이전에는 크게 휘두르며를 추천하는 바람에 야구도..봤죠...",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020193.1.20",
          "form": "하 저 야구도 많이 봤는데 너무 많이봐서 제목이 다 섞였네요..",
          "original_form": "하 저 야구도 많이 봤는데 너무 많이봐서 제목이 다 섞였네요..",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020193.1.21",
          "form": "기억에 남는건 다이아몬드 어쩌구랑..",
          "original_form": "기억에 남는건 다이아몬드 어쩌구랑..",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020193.1.22",
          "form": "전 크게휘두르며만 봤어요 ㅋㅋ",
          "original_form": "전 크게휘두르며만 봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020193.1.23",
          "form": "크게 휘두르며는 누가 나오나요..?",
          "original_form": "크게 휘두르며는 누가 나오나요..?",
          "speaker_id": "2",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100020193.1.24",
          "form": "친구는 다이아몬드 에이스 가장 좋아하더라구요 ㅋㅋ",
          "original_form": "친구는 다이아몬드 에이스 가장 좋아하더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100020193.1.25",
          "form": "웬만하면 제가 봤을거 같은데ㅋㅋㅋㅋㅋ",
          "original_form": "웬만하면 제가 봤을거 같은데ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:16"
        }
      ]
    }
