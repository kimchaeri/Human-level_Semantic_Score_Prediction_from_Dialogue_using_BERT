      "utterance": [
        {
          "id": "MDRW2100021837.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021837.1.2",
          "form": "네 안녕하세여",
          "original_form": "네 안녕하세여",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021837.1.3",
          "form": "여행계획있으세요?",
          "original_form": "여행계획있으세요?",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021837.1.4",
          "form": "제주도 해변에 많이 가보셨나요?",
          "original_form": "제주도 해변에 많이 가보셨나요?",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100021837.1.5",
          "form": "제주살이 시작한게 10일밖에 안되었는데",
          "original_form": "제주살이 시작한게 10일밖에 안되었는데",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021837.1.6",
          "form": "날씨가 막 좋아져거",
          "original_form": "날씨가 막 좋아져거",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021837.1.7",
          "form": "여기저기 다니고 있어요",
          "original_form": "여기저기 다니고 있어요",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021837.1.8",
          "form": "애월함덕",
          "original_form": "애월함덕",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021837.1.9",
          "form": "월요일은 서귀포로 내려가려구요",
          "original_form": "월요일은 서귀포로 내려가려구요",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021837.1.10",
          "form": "우와 멋있다 한두달 계시나요?",
          "original_form": "우와 멋있다 한두달 계시나요?",
          "speaker_id": "1",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100021837.1.11",
          "form": "일 그만두고",
          "original_form": "일 그만두고",
          "speaker_id": "2",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100021837.1.12",
          "form": "10월까진 있으려구요",
          "original_form": "10월까진 있으려구요",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100021837.1.13",
          "form": "코로나때문에 제주에있는게",
          "original_form": "코로나때문에 제주에있는게",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100021837.1.14",
          "form": "우와 우와 짱멋있다",
          "original_form": "우와 우와 짱멋있다",
          "speaker_id": "1",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100021837.1.15",
          "form": "덜 답답할 것 같아요",
          "original_form": "덜 답답할 것 같아요",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100021837.1.16",
          "form": "해외도못나가구",
          "original_form": "해외도못나가구",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100021837.1.17",
          "form": "날씨도 딱 좋네요 안덥구 가을이구",
          "original_form": "날씨도 딱 좋네요 안덥구 가을이구",
          "speaker_id": "1",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100021837.1.18",
          "form": "두달이면 맛집다 가보구 체험도 해보기 좋겠어요",
          "original_form": "두달이면 맛집다 가보구 체험도 해보기 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100021837.1.19",
          "form": "제주 오신적 있우세요",
          "original_form": "제주 오신적 있우세요",
          "speaker_id": "2",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021837.1.20",
          "form": "여행지 추천해주세요",
          "original_form": "여행지 추천해주세요",
          "speaker_id": "2",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021837.1.21",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021837.1.22",
          "form": "전 제주해변이 어떻게 생겼는지도 몰라요",
          "original_form": "전 제주해변이 어떻게 생겼는지도 몰라요",
          "speaker_id": "1",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021837.1.23",
          "form": "현무암 바다인가요 아님 백사장인가요?",
          "original_form": "현무암 바다인가요 아님 백사장인가요?",
          "speaker_id": "1",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021837.1.24",
          "form": "거의 까만색 현무암이구",
          "original_form": "거의 까만색 현무암이구",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021837.1.25",
          "form": "우도라고 제주에서 배타고 들어가면",
          "original_form": "우도라고 제주에서 배타고 들어가면",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021837.1.26",
          "form": "산호껍질이 부셔져서 백사장이된 이쁜 해변이 있어요",
          "original_form": "산호껍질이 부셔져서 백사장이된 이쁜 해변이 있어요",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021837.1.27",
          "form": "꼭 한번 가보시면 좋겠네요",
          "original_form": "꼭 한번 가보시면 좋겠네요",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021837.1.28",
          "form": "그럼 name1 사시는 지역 관광명소는 어디예여",
          "original_form": "그럼 &name1& 사시는 지역 관광명소는 어디예여",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021837.1.29",
          "form": "우와 싱기하다 언젠가 꼭 가볼게요",
          "original_form": "우와 싱기하다 언젠가 꼭 가볼게요",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021837.1.30",
          "form": "전 경기도에 살아서,, 움,,,,",
          "original_form": "전 경기도에 살아서,, 움,,,,",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021837.1.31",
          "form": "음제일 최근에 간 근교 관광코스는 인천차이나타운이요 ㅋㅋ",
          "original_form": "음제일 최근에 간 근교 관광코스는 인천차이나타운이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100021837.1.32",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021837.1.33",
          "form": "월병드셨나요",
          "original_form": "월병드셨나요",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021837.1.34",
          "form": "ㅎㄹ",
          "original_form": "ㅎㄹ",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021837.1.35",
          "form": "월병은 안먹구 일본식 가옥서 커피먹고 짜장면 먹었어요",
          "original_form": "월병은 안먹구 일본식 가옥서 커피먹고 짜장면 먹었어요",
