      "utterance": [
        {
          "id": "MDRW2100037851.1.1",
          "form": "반식욕좋아하는데",
          "original_form": "반식욕좋아하는데",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100037851.1.2",
          "form": "제가 적당히를몰라요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "제가 적당히를몰라요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.3",
          "form": "name1님이 다 해보셨을 것 같은데",
          "original_form": "&name1&님이 다 해보셨을 것 같은데",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.4",
          "form": "제가 이것저것 권하는 게 실례는 아닐지ㅠ",
          "original_form": "제가 이것저것 권하는 게 실례는 아닐지ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.5",
          "form": "한시간넘게 반식욕해버려서",
          "original_form": "한시간넘게 반식욕해버려서",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.6",
          "form": "제가 체질문제도있는것같아요",
          "original_form": "제가 체질문제도있는것같아요",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.7",
          "form": "아 뭔지 알아요 저도 그런 성향 있거든요",
          "original_form": "아 뭔지 알아요 저도 그런 성향 있거든요",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.8",
          "form": "네네 ㅠ",
          "original_form": "네네 ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.9",
          "form": "가만히있어도 머리에 열이 몰리고",
          "original_form": "가만히있어도 머리에 열이 몰리고",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.10",
          "form": "배탈도 자주 나시나요?",
          "original_form": "배탈도 자주 나시나요?",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.11",
          "form": "네네네!",
          "original_form": "네네네!",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.12",
          "form": "안빠지더라구여",
          "original_form": "안빠지더라구여",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.13",
          "form": "네네ㅜㅜㅜ",
          "original_form": "네네ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.14",
          "form": "아 그럼 진짜 열이 머리로 가나보네요",
          "original_form": "아 그럼 진짜 열이 머리로 가나보네요",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.15",
          "form": "속이차고 머리는뜨거운거 ㅜ",
          "original_form": "속이차고 머리는뜨거운거 ㅜ",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100037851.1.16",
          "form": "이럴 때는 정말 어떻게 하는게 좋을까요....",
          "original_form": "이럴 때는 정말 어떻게 하는게 좋을까요....",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100037851.1.17",
          "form": "네 평소에도 얼굴이랑머리에 막 뜨끈뜨끈해요 갑자기 열확오를때도있고",
          "original_form": "네 평소에도 얼굴이랑머리에 막 뜨끈뜨끈해요 갑자기 열확오를때도있고",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100037851.1.18",
          "form": "여드름 같은 질환은 없으신가요?",
          "original_form": "여드름 같은 질환은 없으신가요?",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100037851.1.19",
          "form": "한약도먹어봤는데 잘모르겠더라구여..",
          "original_form": "한약도먹어봤는데 잘모르겠더라구여..",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100037851.1.20",
          "form": "저도 그랬어서 여드름이 진짜 심했어요ㅠ",
          "original_form": "저도 그랬어서 여드름이 진짜 심했어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100037851.1.21",
          "form": "여드름은 이제 등드름 오져요",
          "original_form": "여드름은 이제 등드름 오져요",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100037851.1.22",
          "form": "아 얼굴에는 안나고요?",
          "original_form": "아 얼굴에는 안나고요?",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100037851.1.23",
          "form": "네네ㅠ",
          "original_form": "네네ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100037851.1.24",
          "form": "가드름도자주나고",
          "original_form": "가드름도자주나고",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100037851.1.25",
          "form": "얼굴은 어릴때 코랑입주위에",
          "original_form": "얼굴은 어릴때 코랑입주위에",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100037851.1.26",
          "form": "네네 ㅠㅠ",
          "original_form": "네네 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100037851.1.27",
          "form": "아 두피염이 내려오면 이마랑 그 이마라인까지내려와요",
          "original_form": "아 두피염이 내려오면 이마랑 그 이마라인까지내려와요",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100037851.1.28",
          "form": "아휴 듣는 제가 다 속상해요",
          "original_form": "아휴 듣는 제가 다 속상해요",
          "speaker_id": "2",
          "time": "20210901 19:45"
        }
      ]
    }
