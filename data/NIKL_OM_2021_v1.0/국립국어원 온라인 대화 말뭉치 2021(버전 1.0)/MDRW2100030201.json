      "utterance": [
        {
          "id": "MDRW2100030201.1.1",
          "form": "알죠알죠",
          "original_form": "알죠알죠",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030201.1.2",
          "form": "제가 요즘 그래요",
          "original_form": "제가 요즘 그래요",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030201.1.3",
          "form": "요즘 핫한게 디피인데",
          "original_form": "요즘 핫한게 디피인데",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030201.1.4",
          "form": "넷플릭스",
          "original_form": "넷플릭스",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030201.1.5",
          "form": "못보겠어요 뭔가 보기도 전에 지쳐서 ㅠ",
          "original_form": "못보겠어요 뭔가 보기도 전에 지쳐서 ㅠ",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030201.1.6",
          "form": "저 디피 시작도 안했어요",
          "original_form": "저 디피 시작도 안했어요",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030201.1.7",
          "form": "표지부터 지쳐서",
          "original_form": "표지부터 지쳐서",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030201.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030201.1.9",
          "form": "ㅋㅋㅋㅋㅋ그러니까욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그러니까욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030201.1.10",
          "form": "그러다가 주변이",
          "original_form": "그러다가 주변이",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030201.1.11",
          "form": "내용 대충 들으니까 보면 쳐질거 같더라고요",
          "original_form": "내용 대충 들으니까 보면 쳐질거 같더라고요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030201.1.12",
          "form": "너무 닥달해서",
          "original_form": "너무 닥달해서",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030201.1.13",
          "form": "오늘 20분봤는데",
          "original_form": "오늘 20분봤는데",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030201.1.14",
          "form": "어우..... 포기했어요",
          "original_form": "어우..... 포기했어요",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030201.1.15",
          "form": "너무 현실 재현도 재현이고",
          "original_form": "너무 현실 재현도 재현이고",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030201.1.16",
          "form": "욕이랑때리늨거",
          "original_form": "욕이랑때리늨거",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030201.1.17",
          "form": "너무 보기싫어서",
          "original_form": "너무 보기싫어서",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030201.1.18",
          "form": "인내심 안녕해서 포가",
          "original_form": "인내심 안녕해서 포가",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030201.1.19",
          "form": "기",
          "original_form": "기",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030201.1.20",
          "form": "군대 다녀온 사람들이 특히 좋아하더만요",
          "original_form": "군대 다녀온 사람들이 특히 좋아하더만요",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030201.1.21",
          "form": "차라리 배경 예쁜 드라마를 보겠어요",
          "original_form": "차라리 배경 예쁜 드라마를 보겠어요",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030201.1.22",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030201.1.23",
          "form": "그렇게생각해요",
          "original_form": "그렇게생각해요",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030201.1.24",
          "form": "차라이 사랑이야기보겟다 했는데",
          "original_form": "차라이 사랑이야기보겟다 했는데",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030201.1.25",
          "form": "홍천기도 뭐... 판타지더라구요",
          "original_form": "홍천기도 뭐... 판타지더라구요",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030201.1.26",
          "form": "그것도 1화 꾸역꾸역보고 포기",
          "original_form": "그것도 1화 꾸역꾸역보고 포기",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030201.1.27",
          "form": "홍천기가 김유정나오는건가요??",
          "original_form": "홍천기가 김유정나오는건가요??",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030201.1.28",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030201.1.29",
          "form": "아 그거도 넷플에 있나보네요",
          "original_form": "아 그거도 넷플에 있나보네요",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030201.1.30",
          "form": "아 아뇨",
          "original_form": "아 아뇨",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030201.1.31",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030201.1.32",
          "form": "저 판타지 별로 안좋아해서",
          "original_form": "저 판타지 별로 안좋아해서",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030201.1.33",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030201.1.34",
          "form": "그냥 간만에 드라마 추가해볼까해서 티비로 봤죠",
          "original_form": "그냥 간만에 드라마 추가해볼까해서 티비로 봤죠",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030201.1.35",
          "form": "사극이잖아요!!!",
          "original_form": "사극이잖아요!!!",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030201.1.36",
          "form": "한복구경할라고 ㅠ",
          "original_form": "한복구경할라고 ㅠ",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030201.1.37",
          "form": "하지만 저도 판타지 별로라..",
          "original_form": "하지만 저도 판타지 별로라..",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030201.1.38",
          "form": "ㅋㅋㅋㅋ 아하 근데 예고봤는데 거적데기 입은 김유정만 봤어요",
          "original_form": "ㅋㅋㅋㅋ 아하 근데 예고봤는데 거적데기 입은 김유정만 봤어요",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030201.1.39",
          "form": "거적데깈ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "거적데깈ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.40",
          "form": "전이쁜 유정이만 봐가지곸ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전이쁜 유정이만 봐가지곸ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.41",
          "form": "약간 그 예전에 무슨 드라마 같은 느낌이었는데",
          "original_form": "약간 그 예전에 무슨 드라마 같은 느낌이었는데",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.42",
          "form": "하지만 안보겠습니다..ㅎ",
          "original_form": "하지만 안보겠습니다..ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.43",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.44",
          "form": "비슷한 드라마가 있었던 느낌",
          "original_form": "비슷한 드라마가 있었던 느낌",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.45",
          "form": "아 그랬던가욬ㅋㅋㅋ",
          "original_form": "아 그랬던가욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.46",
          "form": "네 ㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.47",
          "form": "뭔내용인지도 몰라서",
          "original_form": "뭔내용인지도 몰라서",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.48",
          "form": "예고만 봤는데도",
          "original_form": "예고만 봤는데도",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.49",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.50",
          "form": "요즘 그런게",
          "original_form": "요즘 그런게",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030201.1.51",
          "form": "유행인거같아요",
          "original_form": "유행인거같아요",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030201.1.52",
          "form": "판타지사극",
          "original_form": "판타지사극",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030201.1.53",
          "form": "예전에 이런 드라마 봤던거 같은데 하고 지나갔어요",
          "original_form": "예전에 이런 드라마 봤던거 같은데 하고 지나갔어요",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030201.1.54",
          "form": "ㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요",
