      "utterance": [
        {
          "id": "MDRW2100021789.1.1",
          "form": "음 다이소 발길 끊은 사람에게..ㅋㅋㅋ",
          "original_form": "음 다이소 발길 끊은 사람에게..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021789.1.2",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021789.1.3",
          "form": "왠지 전에 다이소 단골이였을 거 같아서요 ㅎㅎ",
          "original_form": "왠지 전에 다이소 단골이였을 거 같아서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021789.1.4",
          "form": "근데 보통 문구류를 사러갔던거라",
          "original_form": "근데 보통 문구류를 사러갔던거라",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021789.1.5",
          "form": "추천까진 할게 못되어요 ㅋㅋㅋㅋ",
          "original_form": "추천까진 할게 못되어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021789.1.6",
          "form": "쓸데없이 파티용품 사기도하고",
          "original_form": "쓸데없이 파티용품 사기도하고",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021789.1.7",
          "form": "쓸데없니 벚꽃시즌 물통을 사기도하고",
          "original_form": "쓸데없니 벚꽃시즌 물통을 사기도하고",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021789.1.8",
          "form": "아 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021789.1.9",
          "form": "다이소는 뭔가 급하게 살게 있을 때 유용하더라구요",
          "original_form": "다이소는 뭔가 급하게 살게 있을 때 유용하더라구요",
          "speaker_id": "1",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021789.1.10",
          "form": "특히 선물 포장할 때 ㅋㅋ",
          "original_form": "특히 선물 포장할 때 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021789.1.11",
          "form": "아 맞아요 그럴때 갔어요ㅋㅋㅋ",
          "original_form": "아 맞아요 그럴때 갔어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021789.1.12",
          "form": "아님 주방 소모품 같은거",
          "original_form": "아님 주방 소모품 같은거",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021789.1.13",
          "form": "비닐장갑이나 비닐봉지나 그런거 사러갔어요 종종",
          "original_form": "비닐장갑이나 비닐봉지나 그런거 사러갔어요 종종",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021789.1.14",
          "form": "어머 맞아요 ㅋㅋㅋ",
          "original_form": "어머 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021789.1.15",
          "form": "거기 비닐백 진짜 잘쓰고 있어요",
          "original_form": "거기 비닐백 진짜 잘쓰고 있어요",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021789.1.16",
          "form": "양 많더라구요 ㅋㅋㅌ",
          "original_form": "양 많더라구요 ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021789.1.17",
          "form": "책처럼 생긴 비닐백 뭔지 아세요?",
          "original_form": "책처럼 생긴 비닐백 뭔지 아세요?",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021789.1.18",
          "form": "아네네",
          "original_form": "아네네",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021789.1.19",
          "form": "비닐백 포장지가 책처럼 되어있는거 저 그거",
          "original_form": "비닐백 포장지가 책처럼 되어있는거 저 그거",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021789.1.20",
          "form": "사이즈별로 다 샀잖아요 ㅠㅠㅠㅠ",
          "original_form": "사이즈별로 다 샀잖아요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021789.1.21",
          "form": "누구 아이디어인지 모르지만 대박 편해요 그거",
          "original_form": "누구 아이디어인지 모르지만 대박 편해요 그거",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021789.1.22",
          "form": "신기한것은 다 사보기 때문에 ㅋㅋㅋㅋㅋ",
          "original_form": "신기한것은 다 사보기 때문에 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021789.1.23",
          "form": "근데 막상 중이랑 소밖에 안써욬ㅋㅋㅋ",
          "original_form": "근데 막상 중이랑 소밖에 안써욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021789.1.24",
          "form": "대 사이즈는 언제 써보죠..",
          "original_form": "대 사이즈는 언제 써보죠..",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021789.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021789.1.26",
          "form": "모든 쓰임은 중이 젤 좋더라구요",
          "original_form": "모든 쓰임은 중이 젤 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100021789.1.27",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021789.1.28",
          "form": "제일 빡칠 때가...",
          "original_form": "제일 빡칠 때가...",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021789.1.29",
          "form": "소짜리 꺼냈는데 안들어가지거나 안묶일 때...",
          "original_form": "소짜리 꺼냈는데 안들어가지거나 안묶일 때...",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021789.1.30",
          "form": "비닐 하나 버렸따...ㅠㅠ",
          "original_form": "비닐 하나 버렸따...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021789.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021789.1.32",
          "form": "저는 소가 제일 안 써지던데",
          "original_form": "저는 소가 제일 안 써지던데",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021789.1.33",
          "form": "어디 쓰는건지 모르겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "어디 쓰는건지 모르겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021789.1.34",
          "form": "소짜리 ㅋㅋ 저그거 채소 담기 좋던데요",
          "original_form": "소짜리 ㅋㅋ 저그거 채소 담기 좋던데요",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021789.1.35",
          "form": "양파 하나 꺼내서 반만 쓰고 반개 넣으면 딱 맞아요",
          "original_form": "양파 하나 꺼내서 반만 쓰고 반개 넣으면 딱 맞아요",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021789.1.36",
          "form": "오호 알뜰하셔라 자취생인가요",
          "original_form": "오호 알뜰하셔라 자취생인가요",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021789.1.37",
          "form": "어머 그럼요",
          "original_form": "어머 그럼요",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021789.1.38",
          "form": "자취생이라 양파 하나라도 알뜰히 써야해요",
          "original_form": "자취생이라 양파 하나라도 알뜰히 써야해요",
          "speaker_id": "1",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021789.1.39",
          "form": "다이소에 그 물건 좋더라구요",
          "original_form": "다이소에 그 물건 좋더라구요",
