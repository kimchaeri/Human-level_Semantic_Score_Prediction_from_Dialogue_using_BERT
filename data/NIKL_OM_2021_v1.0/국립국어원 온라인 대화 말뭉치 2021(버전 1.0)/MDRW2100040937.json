        },
        {
          "id": "MDRW2100040937.1.8",
          "form": "잘했음",
          "original_form": "잘했음",
          "speaker_id": "1",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100040937.1.9",
          "form": "열은안남?",
          "original_form": "열은안남?",
          "speaker_id": "1",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100040937.1.10",
          "form": "열은나는데",
          "original_form": "열은나는데",
          "speaker_id": "2",
          "time": "20210905 15:02"
        },
        {
          "id": "MDRW2100040937.1.11",
          "form": "살아는 있음",
          "original_form": "살아는 있음",
          "speaker_id": "2",
