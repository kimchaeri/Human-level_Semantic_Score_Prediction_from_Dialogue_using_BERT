          "speaker_id": "1",
          "time": "20210905 15:06"
        },
        {
          "id": "MDRW2100040937.1.14",
          "form": "아 ㄱㅊㄱㅊ",
          "original_form": "아 ㄱㅊㄱㅊ",
          "speaker_id": "2",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100040937.1.15",
          "form": "지금 라일락 듣고 있음",
          "original_form": "지금 라일락 듣고 있음",
          "speaker_id": "2",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100040937.1.16",
          "form": "꽃이 피는 날 굿바이 할 거임 ㄱㅊ",
          "original_form": "꽃이 피는 날 굿바이 할 거임 ㄱㅊ",
          "speaker_id": "2",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100040937.1.17",
