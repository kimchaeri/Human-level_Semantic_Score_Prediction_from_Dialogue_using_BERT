      "utterance": [
        {
          "id": "MDRW2100017733.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017733.1.2",
          "form": "연애 중이세요?",
          "original_form": "연애 중이세요?",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017733.1.3",
          "form": "아니요! name1 님은요?",
          "original_form": "아니요! &name1& 님은요?",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017733.1.4",
          "form": "저도요...",
          "original_form": "저도요...",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017733.1.5",
          "form": "전에 연애했을 땐 데이트 어떠셨어요?",
          "original_form": "전에 연애했을 땐 데이트 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017733.1.6",
          "form": "학생 때라서 맛있는 거 먹고 여기저기 구경다녔어요!",
          "original_form": "학생 때라서 맛있는 거 먹고 여기저기 구경다녔어요!",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017733.1.7",
          "form": "다 비슷비슷하네요 ㅎㅎ",
          "original_form": "다 비슷비슷하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017733.1.8",
          "form": "이상형 있어요?",
          "original_form": "이상형 있어요?",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017733.1.9",
          "form": "예의 바른 사람 좋아해요~",
          "original_form": "예의 바른 사람 좋아해요~",
          "speaker_id": "2",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017733.1.10",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017733.1.11",
          "form": "저도 착한 사람 좋아해요 ㅎㅎ",
          "original_form": "저도 착한 사람 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017733.1.12",
          "form": "그리고 기본 상식과 개념 있는 사람도요",
          "original_form": "그리고 기본 상식과 개념 있는 사람도요",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017733.1.13",
          "form": "정말 중요한 거 같아요!",
          "original_form": "정말 중요한 거 같아요!",
          "speaker_id": "2",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017733.1.14",
          "form": "요즘에 정말 몰상식한 사람이 많아요",
          "original_form": "요즘에 정말 몰상식한 사람이 많아요",
          "speaker_id": "2",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017733.1.15",
          "form": "그런 사람이랑 결혼하면 매일 싸울 것 같아요 ㅎㅎ",
          "original_form": "그런 사람이랑 결혼하면 매일 싸울 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017733.1.16",
          "form": "끔찍하네요... 그런 사람이랑은 절대 연애도 결혼도 싫어요ㅠㅠ",
          "original_form": "끔찍하네요... 그런 사람이랑은 절대 연애도 결혼도 싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017733.1.17",
          "form": "맞아요 ㅜㅜ 부인이 맘충 소리 들으면 슬플 것 같아요.",
          "original_form": "맞아요 ㅜㅜ 부인이 맘충 소리 들으면 슬플 것 같아요.",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017733.1.18",
          "form": "기본 상식 지키는 거 중요하죠~ 결혼은 언제쯤 하고 싶으세요?",
          "original_form": "기본 상식 지키는 거 중요하죠~ 결혼은 언제쯤 하고 싶으세요?",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017733.1.19",
          "form": "저는 결혼 생각은 아직 없구요",
          "original_form": "저는 결혼 생각은 아직 없구요",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017733.1.20",
          "form": "독신으로 살 것 같아요~",
          "original_form": "독신으로 살 것 같아요~",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017733.1.21",
          "form": "비혼도 괜찮죠~ 저도 혼자 사는 게 잘 맞는 사람은 굳이 결혼 안 해도 된다고 생각해요~",
          "original_form": "비혼도 괜찮죠~ 저도 혼자 사는 게 잘 맞는 사람은 굳이 결혼 안 해도 된다고 생각해요~",
          "speaker_id": "2",
          "time": "20210826 21:10"
        }
      ]
