      "utterance": [
        {
          "id": "MDRW2100036902.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ 그래 집은 어후...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ 그래 집은 어후...",
          "speaker_id": "2",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036902.1.2",
          "form": "무조건 싸운다",
          "original_form": "무조건 싸운다",
          "speaker_id": "2",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036902.1.3",
          "form": "112동은 가끔 가는거 같더라",
          "original_form": "112동은 가끔 가는거 같더라",
          "speaker_id": "2",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036902.1.4",
          "form": "name3이 따라서",
          "original_form": "&name3&이 따라서",
          "speaker_id": "2",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036902.1.5",
          "form": "맞아요 자주 가는 거 같던데요ㅋㅋㅋ",
          "original_form": "맞아요 자주 가는 거 같던데요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036902.1.6",
          "form": "말없이 조용한 것 같으면서",
          "original_form": "말없이 조용한 것 같으면서",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036902.1.7",
          "form": "112동은 항상 열려있음 ㅋㅋㅋㅋㅋ",
          "original_form": "112동은 항상 열려있음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036902.1.8",
          "form": "지 할 말은 다 하는 듯",
          "original_form": "지 할 말은 다 하는 듯",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036902.1.9",
          "form": "항상 바쁘다 거기는",
          "original_form": "항상 바쁘다 거기는",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036902.1.10",
          "form": "애도 봐야 되고 언니들도 가르쳐야 되고",
          "original_form": "애도 봐야 되고 언니들도 가르쳐야 되고",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036902.1.11",
          "form": "그 엄마부터 도 맘에 안 듬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 엄마부터 도 맘에 안 듬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036902.1.12",
          "form": "스스로 하는 사람이 없어",
          "original_form": "스스로 하는 사람이 없어",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036902.1.13",
          "form": "그래 그 엄마도  윽시 낭.창하제??",
          "original_form": "그래 그 엄마도  윽시 낭.창하제??",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036902.1.14",
          "form": "나 전에 한 번 꽂혀서",
          "original_form": "나 전에 한 번 꽂혀서",
          "speaker_id": "1",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036902.1.15",
          "form": "그 뒤로는 쭉 싫어요ㅋㅋㅋㅋ",
          "original_form": "그 뒤로는 쭉 싫어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036902.1.16",
          "form": "그 집 아빠도 똑같음 ㅋㅋㅋ",
          "original_form": "그 집 아빠도 똑같음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036902.1.17",
          "form": "ㅋㅋㅋㅋㅋ 아 아빠는 못봤는데",
          "original_form": "ㅋㅋㅋㅋㅋ 아 아빠는 못봤는데",
          "speaker_id": "2",
          "time": "20210831 14:51"
        },
        {
          "id": "MDRW2100036902.1.18",
          "form": "엄마는 별로인게 보임",
          "original_form": "엄마는 별로인게 보임",
          "speaker_id": "2",
          "time": "20210831 14:51"
        },
        {
          "id": "MDRW2100036902.1.19",
          "form": "ㅋㅋㅋㅋㅋ 그래 문구점에서도 짜증나게 했잖아",
          "original_form": "ㅋㅋㅋㅋㅋ 그래 문구점에서도 짜증나게 했잖아",
          "speaker_id": "2",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100036902.1.20",
          "form": "얼마전에 name4 머리보고 비웃어서",
          "original_form": "얼마전에 &name4& 머리보고 비웃어서",
          "speaker_id": "2",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100036902.1.21",
          "form": "더 짜증남",
          "original_form": "더 짜증남",
          "speaker_id": "2",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100036902.1.22",
          "form": "지는 얼굴이 그따위면서",
          "original_form": "지는 얼굴이 그따위면서",
          "speaker_id": "2",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100036902.1.23",
          "form": "어디 우리아들 머리를 비웃노",
          "original_form": "어디 우리아들 머리를 비웃노",
          "speaker_id": "2",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100036902.1.24",
          "form": "그래요 언니 못 생겨도 귀여운 애가 있고",
          "original_form": "그래요 언니 못 생겨도 귀여운 애가 있고",
          "speaker_id": "1",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100036902.1.25",
          "form": "걔는 아님ㅋㅋㅋㅋㅋㅋ",
          "original_form": "걔는 아님ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100036902.1.26",
          "form": "학교 다닐 때도 왜 그런 애들이 이간질 엄청 하잖아요ㅋㅋㅋㅋ",
          "original_form": "학교 다닐 때도 왜 그런 애들이 이간질 엄청 하잖아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100036902.1.27",
          "form": "눈에 딱 보여요 ㅋㅋㅋㅋ",
          "original_form": "눈에 딱 보여요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100036902.1.28",
          "form": "그래 맞다 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "그래 맞다 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100036902.1.29",
          "form": "딱 이간질하게 생겼음",
          "original_form": "딱 이간질하게 생겼음",
          "speaker_id": "2",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100036902.1.30",
          "form": "눈 찢어져가지고",
          "original_form": "눈 찢어져가지고",
          "speaker_id": "2",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100036902.1.31",
          "form": "이마이 어른들 못났다ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이마이 어른들 못났다ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100036902.1.32",
          "form": "아 웃겨ㅋㅋㅋㅋㅋ",
          "original_form": "아 웃겨ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100036902.1.33",
          "form": "애지만 정 안 가는 애들 있음ㅋㅋㅋㅋ",
          "original_form": "애지만 정 안 가는 애들 있음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:51"
        },
        {
          "id": "MDRW2100036902.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그래 애 하나가지고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그래 애 하나가지고",
          "speaker_id": "2",
          "time": "20210831 15:51"
        },
        {
