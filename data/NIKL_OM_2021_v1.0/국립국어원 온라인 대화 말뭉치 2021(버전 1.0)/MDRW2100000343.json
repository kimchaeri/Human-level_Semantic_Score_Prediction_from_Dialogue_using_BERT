      "utterance": [
        {
          "id": "MDRW2100000343.1.1",
          "form": "저녁 뭐 먹을거야",
          "original_form": "저녁 뭐 먹을거야",
          "speaker_id": "1",
          "time": "20210819 15:18"
        },
        {
          "id": "MDRW2100000343.1.2",
          "form": "나 샤브샤브",
          "original_form": "나 샤브샤브",
          "speaker_id": "2",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000343.1.3",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "2",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000343.1.4",
          "form": "난 고민중인데",
          "original_form": "난 고민중인데",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000343.1.5",
          "form": "곱창먹고싶기도하고",
          "original_form": "곱창먹고싶기도하고",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000343.1.6",
          "form": "곱창맛있지",
          "original_form": "곱창맛있지",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000343.1.7",
          "form": "또?",
          "original_form": "또?",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000343.1.8",
          "form": "음... 어제 두개 사서는 하나만먹고 아껴놨던 마카롱?",
          "original_form": "음... 어제 두개 사서는 하나만먹고 아껴놨던 마카롱?",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000343.1.9",
          "form": "곱창 먹고싶다",
          "original_form": "곱창 먹고싶다",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000343.1.10",
          "form": "ㅋㅋㅋ마카롱이랑 곱창",
          "original_form": "ㅋㅋㅋ마카롱이랑 곱창",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000343.1.11",
          "form": "갭이 너무 큰데?",
          "original_form": "갭이 너무 큰데?",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000343.1.12",
          "form": "곱창 어서 먹어",
          "original_form": "곱창 어서 먹어",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000343.1.13",
          "form": "맛집좀",
          "original_form": "맛집좀",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000343.1.14",
          "form": "우리집주변엔 맛집없어ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우리집주변엔 맛집없어ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000343.1.15",
          "form": "원래 인터넷에나오는 맛집은 우리동네에 없는법이잖아...",
          "original_form": "원래 인터넷에나오는 맛집은 우리동네에 없는법이잖아...",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000343.1.16",
          "form": "칼국수 맛집 있긴하다",
          "original_form": "칼국수 맛집 있긴하다",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000343.1.17",
          "form": "ㅋㅋ칼국수좋지",
          "original_form": "ㅋㅋ칼국수좋지",
          "speaker_id": "2",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000343.1.18",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000343.1.19",
          "form": "곱창이 더 맛있음",
          "original_form": "곱창이 더 맛있음",
          "speaker_id": "2",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000343.1.20",
          "form": "ㄱㄱ",
          "original_form": "ㄱㄱ",
          "speaker_id": "2",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000343.1.21",
          "form": "곱창 진짜 맨날먹고싶은데 비쌈",
          "original_form": "곱창 진짜 맨날먹고싶은데 비쌈",
          "speaker_id": "1",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000343.1.22",
          "form": "샤브샤브 안먹은지도 오래됐어",
          "original_form": "샤브샤브 안먹은지도 오래됐어",
          "speaker_id": "1",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000343.1.23",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000343.1.24",
          "form": "나 원래 샤브안좋아했는데",
          "original_form": "나 원래 샤브안좋아했는데",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000343.1.25",
          "form": "친구가 좋아해서 따라다니다가 좋아짐ㅋㅋ",
          "original_form": "친구가 좋아해서 따라다니다가 좋아짐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000343.1.26",
          "form": "1인샤브!",
          "original_form": "1인샤브!",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000343.1.27",
          "form": "야무지게 잘 먹고다니네ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "야무지게 잘 먹고다니네ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100000343.1.28",
          "form": "샤브샤브먹는다고해서 저녁에 약속있나했는데 혼자먹으러가는거였구나 ㅋㅋㅋ",
          "original_form": "샤브샤브먹는다고해서 저녁에 약속있나했는데 혼자먹으러가는거였구나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000343.1.29",
          "form": "맛있겠당 우리집주변에도 1인샤브집 있나모르겠네",
          "original_form": "맛있겠당 우리집주변에도 1인샤브집 있나모르겠네",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100000343.1.30",
          "form": "ㅋㅋㅋ1인샤브가 딱 내 속도 지켜가며 먹고",
          "original_form": "ㅋㅋㅋ1인샤브가 딱 내 속도 지켜가며 먹고",
          "speaker_id": "2",
          "time": "20210820 10:51"
        },
        {
          "id": "MDRW2100000343.1.31",
          "form": "그래서좋은듯",
          "original_form": "그래서좋은듯",
          "speaker_id": "2",
          "time": "20210820 10:51"
        },
        {
          "id": "MDRW2100000343.1.32",
          "form": "나 어제 너랑 대화하고",
          "original_form": "나 어제 너랑 대화하고",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000343.1.33",
          "form": "1인샤브먹으러갔다 ㅋㅋㅋㅋㅋ ㄹㅇ임",
          "original_form": "1인샤브먹으러갔다 ㅋㅋㅋㅋㅋ ㄹㅇ임",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000343.1.34",
          "form": "ㅋㅋㅋㅋ용케 바로 찾으러갔네",
          "original_form": "ㅋㅋㅋㅋ용케 바로 찾으러갔네",
          "speaker_id": "2",
