        },
        {
          "id": "MDRW2100010465.1.2",
          "form": "네 게임 좋아하죠",
          "original_form": "네 게임 좋아하죠",
          "speaker_id": "2",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010465.1.3",
          "form": "어떤 게임하세요?",
          "original_form": "어떤 게임하세요?",
          "speaker_id": "1",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010465.1.4",
          "form": "카트라이더하고있어요",
          "original_form": "카트라이더하고있어요",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010465.1.5",
          "form": "추억의 게임이네요 ㅎㅎ 전 크아는 나름 잘했는데 카트는 좀 어렵더라구요..",
          "original_form": "추억의 게임이네요 ㅎㅎ 전 크아는 나름 잘했는데 카트는 좀 어렵더라구요..",
          "speaker_id": "1",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010465.1.6",
          "form": "저도 크아 알아욪!!!크아도재있죵",
          "original_form": "저도 크아 알아욪!!!크아도재있죵",
          "speaker_id": "2",
          "time": "20210823 18:54"
        },
        {
          "id": "MDRW2100010465.1.7",
          "form": "카트 말고 다른 운전 게임이나 아케이드는 안하세요?",
          "original_form": "카트 말고 다른 운전 게임이나 아케이드는 안하세요?",
          "speaker_id": "1",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100010465.1.8",
          "form": "지금은 다른 게임은 안하고 있어요",
          "original_form": "지금은 다른 게임은 안하고 있어요",
          "speaker_id": "2",
          "time": "20210823 20:38"
        },
        {
          "id": "MDRW2100010465.1.9",
          "form": "전 게임에 쉽게 중독되는 편이라 게임을 자주 안해요. ㅎㅎ",
          "original_form": "전 게임에 쉽게 중독되는 편이라 게임을 자주 안해요. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 08:52"
        },
        {
          "id": "MDRW2100010465.1.10",
          "form": "메이플 좋아했었어요",
          "original_form": "메이플 좋아했었어요",
          "speaker_id": "1",
          "time": "20210824 08:52"
        },
        {
          "id": "MDRW2100010465.1.11",
          "form": "앜ㅋㅋ저도 메이플한적이있어요",
          "original_form": "앜ㅋㅋ저도 메이플한적이있어요",
          "speaker_id": "2",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100010465.1.12",
          "form": "아주 옛날에",
          "original_form": "아주 옛날에",
          "speaker_id": "2",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100010465.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100010465.1.14",
          "form": "추억의 게임이죠 ㅎㅎ",
          "original_form": "추억의 게임이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100010465.1.15",
          "form": "아직까지 인기많은거보먄.. 넥슨이 대단하긴하네요",
          "original_form": "아직까지 인기많은거보먄.. 넥슨이 대단하긴하네요",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100010465.1.16",
          "form": "넥슥게임이 되게 유명하죠",
          "original_form": "넥슥게임이 되게 유명하죠",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100010465.1.17",
          "form": "폰 게임도 잘 안하시나요?",
          "original_form": "폰 게임도 잘 안하시나요?",
