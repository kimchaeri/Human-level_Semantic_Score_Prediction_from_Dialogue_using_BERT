      "utterance": [
        {
          "id": "MDRW2100034693.1.1",
          "form": "음 단백질파우더요?",
          "original_form": "음 단백질파우더요?",
          "speaker_id": "2",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034693.1.2",
          "form": "오 드실만하던가요?",
          "original_form": "오 드실만하던가요?",
          "speaker_id": "1",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100034693.1.3",
          "form": "전 맛잇엇어요ㅎㅎ",
          "original_form": "전 맛잇엇어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100034693.1.4",
          "form": "그러셨군요 운동복운 어디것 입으셨나요",
          "original_form": "그러셨군요 운동복운 어디것 입으셨나요",
          "speaker_id": "1",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100034693.1.5",
          "form": "전 그냥 헬스장꺼나",
          "original_form": "전 그냥 헬스장꺼나",
          "speaker_id": "2",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100034693.1.6",
          "form": "안다르? 레깅스요",
          "original_form": "안다르? 레깅스요",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100034693.1.7",
          "form": "아 안다르..저도 사봤어요!",
          "original_form": "아 안다르..저도 사봤어요!",
          "speaker_id": "1",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100034693.1.8",
          "form": "막 입기좋더라구요ㅎ",
          "original_form": "막 입기좋더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100034693.1.9",
          "form": "단백질파우더는 어디서 사셨나요?",
          "original_form": "단백질파우더는 어디서 사셨나요?",
          "speaker_id": "1",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100034693.1.10",
          "form": "전 보통 인터넷구매요ㅎ",
          "original_form": "전 보통 인터넷구매요ㅎ",
          "speaker_id": "2",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100034693.1.11",
          "form": "그렇군요 그럼 끼니대신 드시는걸로 사는거죠,",
          "original_form": "그렇군요 그럼 끼니대신 드시는걸로 사는거죠,",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100034693.1.12",
          "form": "아뇨ㅎ",
          "original_form": "아뇨ㅎ",
          "speaker_id": "2",
          "time": "20210828 20:32"
        },
        {
          "id": "MDRW2100034693.1.13",
          "form": "저는 물종류로는 허기가 안채워져서..",
          "original_form": "저는 물종류로는 허기가 안채워져서..",
          "speaker_id": "2",
          "time": "20210828 20:32"
        },
        {
          "id": "MDRW2100034693.1.14",
          "form": "닭가슴살 많이 먹엇어요ㅋㅋ어차피 안먹고빼면 파워요요",
          "original_form": "닭가슴살 많이 먹엇어요ㅋㅋ어차피 안먹고빼면 파워요요",
          "speaker_id": "2",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100034693.1.15",
          "form": "닭 가 슴 살은 어디제품 구매하셨나요",
          "original_form": "닭 가 슴 살은 어디제품 구매하셨나요",
          "speaker_id": "1",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034693.1.16",
          "form": "음 생닭삶아먹다 귀찮아서 저염훈제요ㅋ",
          "original_form": "음 생닭삶아먹다 귀찮아서 저염훈제요ㅋ",
          "speaker_id": "2",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100034693.1.17",
          "form": "가격 저렴한거로요ㅎ",
          "original_form": "가격 저렴한거로요ㅎ",
          "speaker_id": "2",
          "time": "20210828 20:38"
        }
      ]
