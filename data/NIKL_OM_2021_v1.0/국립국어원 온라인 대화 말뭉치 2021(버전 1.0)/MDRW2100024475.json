      "utterance": [
        {
          "id": "MDRW2100024475.1.1",
          "form": "게임 자주하시나요?",
          "original_form": "게임 자주하시나요?",
          "speaker_id": "1",
          "time": "20210831 11:04"
        },
        {
          "id": "MDRW2100024475.1.2",
          "form": "겜 여러개돌려요 도파민 뿜뿜",
          "original_form": "겜 여러개돌려요 도파민 뿜뿜",
          "speaker_id": "2",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100024475.1.3",
          "form": "name1님은여",
          "original_form": "&name1&님은여",
          "speaker_id": "2",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100024475.1.4",
          "form": "전 피쉬돔이랑 스도쿠요ㅎㅎㅎ",
          "original_form": "전 피쉬돔이랑 스도쿠요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100024475.1.5",
          "form": "피쉬돔 레벨 100하고 그만두려했는데",
          "original_form": "피쉬돔 레벨 100하고 그만두려했는데",
          "speaker_id": "1",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100024475.1.6",
          "form": "머리좋아지겠군요",
          "original_form": "머리좋아지겠군요",
          "speaker_id": "2",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100024475.1.7",
          "form": "2700넘었네요 ..",
          "original_form": "2700넘었네요 ..",
          "speaker_id": "1",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100024475.1.8",
          "form": "겜 유익한거하시네요",
          "original_form": "겜 유익한거하시네요",
          "speaker_id": "2",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100024475.1.9",
          "form": "피쉬돔은..안 유익해요",
          "original_form": "피쉬돔은..안 유익해요",
          "speaker_id": "1",
          "time": "20210831 11:07"
        },
        {
          "id": "MDRW2100024475.1.10",
          "form": "애니팡이랑 똑같아요ㅋ",
          "original_form": "애니팡이랑 똑같아요ㅋ",
          "speaker_id": "1",
          "time": "20210831 11:07"
        },
        {
          "id": "MDRW2100024475.1.11",
          "form": "고기낚는거아닙니까",
          "original_form": "고기낚는거아닙니까",
          "speaker_id": "2",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100024475.1.12",
          "form": "재밌겠군요",
          "original_form": "재밌겠군요",
          "speaker_id": "2",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100024475.1.13",
          "form": "전혀요",
          "original_form": "전혀요",
          "speaker_id": "1",
          "time": "20210831 11:09"
        },
        {
          "id": "MDRW2100024475.1.14",
          "form": "그냥 세개맞추기에요",
          "original_form": "그냥 세개맞추기에요",
          "speaker_id": "1",
          "time": "20210831 11:09"
        },
        {
          "id": "MDRW2100024475.1.15",
          "form": "근데 왜하세요",
          "original_form": "근데 왜하세요",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100024475.1.16",
          "form": "모르겠어요..",
          "original_form": "모르겠어요..",
