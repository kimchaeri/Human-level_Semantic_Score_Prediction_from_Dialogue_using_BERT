      "utterance": [
        {
          "id": "MDRW2100047482.1.1",
          "form": "안녕하세요 저녁 뭐 드세요?",
          "original_form": "안녕하세요 저녁 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100047482.1.2",
          "form": "저 아직이요ㅋㅋㅋ",
          "original_form": "저 아직이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100047482.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100047482.1.4",
          "form": "저는 부추전먹고 있어요",
          "original_form": "저는 부추전먹고 있어요",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100047482.1.5",
          "form": "뭐 드실려구요?",
          "original_form": "뭐 드실려구요?",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100047482.1.6",
          "form": "오 부추전맛있겠네요",
          "original_form": "오 부추전맛있겠네요",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100047482.1.7",
          "form": "전 아직고민중이예요",
          "original_form": "전 아직고민중이예요",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100047482.1.8",
          "form": "식사때 뭐 먹을지가 참 고민이죠?",
          "original_form": "식사때 뭐 먹을지가 참 고민이죠?",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100047482.1.9",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100047482.1.10",
          "form": "맛집좋아하세요?",
          "original_form": "맛집좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100047482.1.11",
          "form": "네 아시는 맛집 있으세요?",
          "original_form": "네 아시는 맛집 있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100047482.1.12",
          "form": "단골이라기보단",
          "original_form": "단골이라기보단",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100047482.1.13",
          "form": "어디가면 맛집찾아보고가는편이예요ㅋㅋㅋ",
          "original_form": "어디가면 맛집찾아보고가는편이예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100047482.1.14",
          "form": "그렇죠~어디서 맛집 찾으시나요?",
          "original_form": "그렇죠~어디서 맛집 찾으시나요?",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100047482.1.15",
          "form": "ㅋㅋㅋ전 여기저기봅니다 다양한곳들을",
          "original_form": "ㅋㅋㅋ전 여기저기봅니다 다양한곳들을",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100047482.1.16",
          "form": "대부분 성공하셨나요?",
          "original_form": "대부분 성공하셨나요?",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100047482.1.17",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047482.1.18",
          "form": "음식이이쁘거나 인테리어가이쁘면좋아서",
          "original_form": "음식이이쁘거나 인테리어가이쁘면좋아서",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047482.1.19",
          "form": "맛은 좀 없어도돼요ㅋㅋㅋ",
          "original_form": "맛은 좀 없어도돼요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047482.1.20",
          "form": "아~저는 대부분 실패를 해서ㅜ인터넷 후기는 못 믿겠더라구요ㅜ",
          "original_form": "아~저는 대부분 실패를 해서ㅜ인터넷 후기는 못 믿겠더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047482.1.21",
          "form": "저는 맛을 봐서~ㅎ",
          "original_form": "저는 맛을 봐서~ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:19"
        }
      ]
    }
