      "utterance": [
        {
          "id": "MDRW2100008793.1.1",
          "form": "지금 고민 중이에요",
          "original_form": "지금 고민 중이에요",
          "speaker_id": "2",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008793.1.2",
          "form": "뭘 먹어야 휴일 마지막 만찬을 즐길 수 있을까 싶어요",
          "original_form": "뭘 먹어야 휴일 마지막 만찬을 즐길 수 있을까 싶어요",
          "speaker_id": "2",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008793.1.3",
          "form": "같이 고민해볼까요",
          "original_form": "같이 고민해볼까요",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008793.1.4",
          "form": "뭐 좋아해요",
          "original_form": "뭐 좋아해요",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008793.1.5",
          "form": "저는 한식이요 ㅋㅋ",
          "original_form": "저는 한식이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008793.1.6",
          "form": "나이 먹으면서 한식 더더더 좋아졌어요",
          "original_form": "나이 먹으면서 한식 더더더 좋아졌어요",
          "speaker_id": "2",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008793.1.7",
          "form": "오 국 찌개 이런거 필수?",
          "original_form": "오 국 찌개 이런거 필수?",
          "speaker_id": "1",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008793.1.8",
          "form": "그거까진 아닌가요",
          "original_form": "그거까진 아닌가요",
          "speaker_id": "1",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008793.1.9",
          "form": "필수는 아닌데 파스타 피자 이런 것만 먹으면 속이 안 좋더라구요",
          "original_form": "필수는 아닌데 파스타 피자 이런 것만 먹으면 속이 안 좋더라구요",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008793.1.10",
          "form": "밥이 소화가 잘 되는 것 같아요",
          "original_form": "밥이 소화가 잘 되는 것 같아요",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008793.1.11",
          "form": "음 저도 파스타 싫어요",
          "original_form": "음 저도 파스타 싫어요",
          "speaker_id": "1",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008793.1.12",
          "form": "약간 니글거려서",
          "original_form": "약간 니글거려서",
          "speaker_id": "1",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008793.1.13",
          "form": "그럼 뭐 좋어하시나요!!",
          "original_form": "그럼 뭐 좋어하시나요!!",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008793.1.14",
          "form": "저도 한식좋아해요 음 한식은 한식인디..",
          "original_form": "저도 한식좋아해요 음 한식은 한식인디..",
          "speaker_id": "1",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008793.1.15",
          "form": "밥이랑 분홍햄소시지..같이 먹는 거 좋아해요",
          "original_form": "밥이랑 분홍햄소시지..같이 먹는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008793.1.16",
          "form": "구워서 케찹에 콕? 인가요",
          "original_form": "구워서 케찹에 콕? 인가요",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008793.1.17",
          "form": "아니요 저는 본연의 맛을 즐깁니다",
          "original_form": "아니요 저는 본연의 맛을 즐깁니다",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008793.1.18",
          "form": "계란물만 묻혀서 기름에 자글자글",
          "original_form": "계란물만 묻혀서 기름에 자글자글",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008793.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008793.1.20",
          "form": "케찹은 안 찍어드시는군요",
          "original_form": "케찹은 안 찍어드시는군요",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008793.1.21",
          "form": "계란말이도 케찹없이 드시나요",
          "original_form": "계란말이도 케찹없이 드시나요",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008793.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008793.1.23",
          "form": "전 약간 고유의 맛을 좋아해요",
          "original_form": "전 약간 고유의 맛을 좋아해요",
          "speaker_id": "1",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008793.1.24",
          "form": "근데 감튀는 찍어먹어요",
          "original_form": "근데 감튀는 찍어먹어요",
          "speaker_id": "1",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008793.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008793.1.26",
          "form": "오 저는 반대로 감튀를 잘 안 찍어요",
          "original_form": "오 저는 반대로 감튀를 잘 안 찍어요",
          "speaker_id": "2",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008793.1.27",
          "form": "헉 우리 완전 반대네요",
          "original_form": "헉 우리 완전 반대네요",
