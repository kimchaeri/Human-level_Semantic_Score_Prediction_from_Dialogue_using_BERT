      "utterance": [
        {
          "id": "MDRW2100024440.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210831 09:33"
        },
        {
          "id": "MDRW2100024440.1.2",
          "form": "좋아하는 스포츠 있으세요?",
          "original_form": "좋아하는 스포츠 있으세요?",
          "speaker_id": "1",
          "time": "20210831 09:33"
        },
        {
          "id": "MDRW2100024440.1.3",
          "form": "네, 안녕하세요",
          "original_form": "네, 안녕하세요",
          "speaker_id": "2",
          "time": "20210831 09:34"
        },
        {
          "id": "MDRW2100024440.1.4",
          "form": "스포츠는 딱히 관심없는데 올림픽에서 배구는 좀봤어요~",
          "original_form": "스포츠는 딱히 관심없는데 올림픽에서 배구는 좀봤어요~",
          "speaker_id": "2",
          "time": "20210831 09:35"
        },
        {
          "id": "MDRW2100024440.1.5",
          "form": "배구 룰 아세요?",
          "original_form": "배구 룰 아세요?",
          "speaker_id": "1",
          "time": "20210831 09:35"
        },
        {
          "id": "MDRW2100024440.1.6",
          "form": "저도 재밌게 보긴했는데",
          "original_form": "저도 재밌게 보긴했는데",
          "speaker_id": "1",
          "time": "20210831 09:35"
        },
        {
          "id": "MDRW2100024440.1.7",
          "form": "룰이 어려워서요ㅠ",
          "original_form": "룰이 어려워서요ㅠ",
          "speaker_id": "1",
          "time": "20210831 09:35"
        },
        {
          "id": "MDRW2100024440.1.8",
          "form": "룰모르고 보고있어요ㅎㅎ",
          "original_form": "룰모르고 보고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 09:36"
        },
        {
          "id": "MDRW2100024440.1.9",
          "form": "축구는 그나마? 룰몰라도 잘몰라도 볼만해요",
          "original_form": "축구는 그나마? 룰몰라도 잘몰라도 볼만해요",
          "speaker_id": "2",
          "time": "20210831 09:36"
        },
        {
          "id": "MDRW2100024440.1.10",
          "form": "축구 직관도 한 적 있으세요?",
          "original_form": "축구 직관도 한 적 있으세요?",
          "speaker_id": "1",
          "time": "20210831 09:37"
        },
        {
          "id": "MDRW2100024440.1.11",
          "form": "축구는 없고요 그 예전에 연예인야구프로그램 지나가다 우연히 잠깐 직관한적있어요",
          "original_form": "축구는 없고요 그 예전에 연예인야구프로그램 지나가다 우연히 잠깐 직관한적있어요",
          "speaker_id": "2",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100024440.1.12",
          "form": "축구는 예전에 티비로 봐서요ㅎ",
          "original_form": "축구는 예전에 티비로 봐서요ㅎ",
          "speaker_id": "2",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100024440.1.13",
          "form": "저도 직관한 적은 없고",
          "original_form": "저도 직관한 적은 없고",
          "speaker_id": "1",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100024440.1.14",
          "form": "중계보는건 좋아해요",
          "original_form": "중계보는건 좋아해요",
          "speaker_id": "1",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100024440.1.15",
          "form": "동계스포츠",
          "original_form": "동계스포츠",
          "speaker_id": "1",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100024440.1.16",
          "form": "아~~ 저도 올림픽이나 가끔봐요 예전에 잘모르지만 피겨스케이팅도 보긴했어요",
          "original_form": "아~~ 저도 올림픽이나 가끔봐요 예전에 잘모르지만 피겨스케이팅도 보긴했어요",
          "speaker_id": "2",
          "time": "20210831 09:52"
        },
        {
          "id": "MDRW2100024440.1.17",
          "form": "김연아 선수 모르는 사람이 없죠ㅎㅎ",
          "original_form": "김연아 선수 모르는 사람이 없죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:56"
        },
        {
          "id": "MDRW2100024440.1.18",
          "form": "그쵸~~",
          "original_form": "그쵸~~",
          "speaker_id": "2",
          "time": "20210831 11:28"
        },
        {
          "id": "MDRW2100024440.1.19",
          "form": "스포츠는 혼자 빠져들기는 좀 힘든거같아요",
          "original_form": "스포츠는 혼자 빠져들기는 좀 힘든거같아요",
          "speaker_id": "1",
          "time": "20210901 06:16"
        },
        {
          "id": "MDRW2100024440.1.20",
          "form": "누가 직관데려가주거난",
          "original_form": "누가 직관데려가주거난",
          "speaker_id": "1",
          "time": "20210901 06:17"
        },
        {
          "id": "MDRW2100024440.1.21",
          "form": "옆에서 룰설명을 해줘야 재밌는데",
          "original_form": "옆에서 룰설명을 해줘야 재밌는데",
          "speaker_id": "1",
          "time": "20210901 06:17"
        },
        {
          "id": "MDRW2100024440.1.22",
          "form": "아,그런것같아요~ 같이즐기면 재미있을거같아요",
          "original_form": "아,그런것같아요~ 같이즐기면 재미있을거같아요",
          "speaker_id": "2",
          "time": "20210901 08:11"
        },
        {
          "id": "MDRW2100024440.1.23",
          "form": "카페나 모임 찾아서 참여하면 더 접근하기 쉬울거같아요~",
          "original_form": "카페나 모임 찾아서 참여하면 더 접근하기 쉬울거같아요~",
          "speaker_id": "2",
          "time": "20210901 08:11"
        },
        {
          "id": "MDRW2100024440.1.24",
          "form": "기회되면 야구장 직관 꼭 가보고싶어요",
          "original_form": "기회되면 야구장 직관 꼭 가보고싶어요",
          "speaker_id": "1",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100024440.1.25",
          "form": "그 에너지 느끼러",
          "original_form": "그 에너지 느끼러",
          "speaker_id": "1",
          "time": "20210901 10:49"
        }
      ]
    }
