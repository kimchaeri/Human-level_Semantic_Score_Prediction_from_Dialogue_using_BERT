        {
          "id": "MDRW2100032660.1.1",
          "form": "음료좋아하세요?!",
          "original_form": "음료좋아하세요?!",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032660.1.2",
          "form": "커피만 조금?",
          "original_form": "커피만 조금?",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032660.1.3",
          "form": "물은많이드세요?",
          "original_form": "물은많이드세요?",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032660.1.4",
          "form": "아니요 물을 많이 마셔야 하는데 잘 안되네요",
          "original_form": "아니요 물을 많이 마셔야 하는데 잘 안되네요",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032660.1.5",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032660.1.6",
          "form": "저는 물 정말 많이마셔요",
          "original_form": "저는 물 정말 많이마셔요",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032660.1.7",
          "form": "얼마나 ??",
          "original_form": "얼마나 ??",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032660.1.8",
          "form": "하루에 3리터정도?!",
          "original_form": "하루에 3리터정도?!",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032660.1.9",
          "form": "정말 많이 마시네요",
          "original_form": "정말 많이 마시네요",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032660.1.10",
          "form": "네 완전 하마ㅋㅋ",
          "original_form": "네 완전 하마ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032660.1.11",
          "form": "물 많이 마시는 건 좋은거에요 ㅎㅎ",
          "original_form": "물 많이 마시는 건 좋은거에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032660.1.12",
          "form": "아근데 화장실을 많이가게돼요ㅠ",
          "original_form": "아근데 화장실을 많이가게돼요ㅠ",
