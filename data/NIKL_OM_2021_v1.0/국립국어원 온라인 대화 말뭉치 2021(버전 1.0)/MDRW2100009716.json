      "utterance": [
        {
          "id": "MDRW2100009716.1.1",
          "form": "오늘은 조금씩 내리네요",
          "original_form": "오늘은 조금씩 내리네요",
          "speaker_id": "2",
          "time": "20210823 08:40"
        },
        {
          "id": "MDRW2100009716.1.2",
          "form": "비가오나요?",
          "original_form": "비가오나요?",
          "speaker_id": "2",
          "time": "20210823 08:40"
        },
        {
          "id": "MDRW2100009716.1.3",
          "form": "네 ...",
          "original_form": "네 ...",
          "speaker_id": "1",
          "time": "20210823 08:40"
        },
        {
          "id": "MDRW2100009716.1.4",
          "form": "여긴 아주 많이와요",
          "original_form": "여긴 아주 많이와요",
          "speaker_id": "1",
          "time": "20210823 08:40"
        },
        {
          "id": "MDRW2100009716.1.5",
          "form": "많이 습하겠어요",
          "original_form": "많이 습하겠어요",
          "speaker_id": "2",
          "time": "20210823 08:40"
        },
        {
          "id": "MDRW2100009716.1.6",
          "form": "네 비 올줄 몰랐는데",
          "original_form": "네 비 올줄 몰랐는데",
          "speaker_id": "1",
          "time": "20210823 08:41"
        },
        {
          "id": "MDRW2100009716.1.7",
          "form": "아침에 비가...!!",
          "original_form": "아침에 비가...!!",
          "speaker_id": "1",
          "time": "20210823 08:41"
        },
        {
          "id": "MDRW2100009716.1.8",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210823 08:41"
        },
        {
          "id": "MDRW2100009716.1.9",
          "form": "어제는 좀 괜찮았는데",
          "original_form": "어제는 좀 괜찮았는데",
          "speaker_id": "2",
          "time": "20210823 08:41"
        },
        {
          "id": "MDRW2100009716.1.10",
          "form": "태풍 온다는 말 있더라구요",
          "original_form": "태풍 온다는 말 있더라구요",
          "speaker_id": "2",
          "time": "20210823 08:41"
        },
        {
          "id": "MDRW2100009716.1.11",
          "form": "태풍 정말 싫어요",
          "original_form": "태풍 정말 싫어요",
          "speaker_id": "1",
          "time": "20210823 08:41"
        },
        {
          "id": "MDRW2100009716.1.12",
          "form": "저두요ㅠ",
          "original_form": "저두요ㅠ",
          "speaker_id": "2",
          "time": "20210823 08:42"
        },
        {
          "id": "MDRW2100009716.1.13",
          "form": "저희는 어제도 많이 오다가 오후쯤 괜찮아졌거든요",
          "original_form": "저희는 어제도 많이 오다가 오후쯤 괜찮아졌거든요",
          "speaker_id": "1",
          "time": "20210823 08:42"
        },
        {
          "id": "MDRW2100009716.1.14",
          "form": "피해도 심하고 그래서 싫어요",
          "original_form": "피해도 심하고 그래서 싫어요",
          "speaker_id": "2",
          "time": "20210823 08:42"
        },
        {
          "id": "MDRW2100009716.1.15",
          "form": "엇 어제도 왔었구나",
          "original_form": "엇 어제도 왔었구나",
          "speaker_id": "2",
          "time": "20210823 08:42"
        },
        {
          "id": "MDRW2100009716.1.16",
          "form": "아마 이번주 내내 올 거 같아요",
          "original_form": "아마 이번주 내내 올 거 같아요",
          "speaker_id": "2",
          "time": "20210823 08:42"
        },
        {
          "id": "MDRW2100009716.1.17",
          "form": "아 안돼요..",
          "original_form": "아 안돼요..",
          "speaker_id": "1",
          "time": "20210823 12:16"
        },
        {
          "id": "MDRW2100009716.1.18",
          "form": "그래도 지금은 아주아주 조금 오고 있어요",
          "original_form": "그래도 지금은 아주아주 조금 오고 있어요",
          "speaker_id": "1",
          "time": "20210823 12:16"
        },
        {
          "id": "MDRW2100009716.1.19",
          "form": "저희는 조금 오다가 엄청 퍼붓는 중..",
          "original_form": "저희는 조금 오다가 엄청 퍼붓는 중..",
          "speaker_id": "2",
          "time": "20210823 12:19"
        },
        {
          "id": "MDRW2100009716.1.20",
          "form": "나갔다가 다 젖었어요ㅋㅋ",
          "original_form": "나갔다가 다 젖었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009716.1.21",
          "form": "비오면 우산이 다 무소용이에요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "비오면 우산이 다 무소용이에요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009716.1.22",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009716.1.23",
          "form": "우산은 더 젖을 거 덜 젖게만 해줄 뿐",
          "original_form": "우산은 더 젖을 거 덜 젖게만 해줄 뿐",
          "speaker_id": "2",
          "time": "20210823 12:21"
        }
      ]
