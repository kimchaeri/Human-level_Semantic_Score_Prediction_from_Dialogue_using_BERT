      "utterance": [
        {
          "id": "MDRW2100010838.1.1",
          "form": "넷플릭스 보시나요?",
          "original_form": "넷플릭스 보시나요?",
          "speaker_id": "1",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010838.1.2",
          "form": "넷플릭스 안보고 전 웨이브 이용해요",
          "original_form": "넷플릭스 안보고 전 웨이브 이용해요",
          "speaker_id": "2",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010838.1.3",
          "form": "넷플 뭐 보세요?",
          "original_form": "넷플 뭐 보세요?",
          "speaker_id": "2",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010838.1.4",
          "form": "한국 예능인 노는언니나 맛있는 녀석들 주로 봐요",
          "original_form": "한국 예능인 노는언니나 맛있는 녀석들 주로 봐요",
          "speaker_id": "1",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010838.1.5",
          "form": "웨이브에서는 뭐 보세요?",
          "original_form": "웨이브에서는 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010838.1.6",
          "form": "전 런닝맨이나 애로부부 이런 거",
          "original_form": "전 런닝맨이나 애로부부 이런 거",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010838.1.7",
          "form": "자주봐요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "자주봐요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010838.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010838.1.9",
          "form": "다른 플랫폼보다 웨이브 쓰는 이유가 있으세요?",
          "original_form": "다른 플랫폼보다 웨이브 쓰는 이유가 있으세요?",
          "speaker_id": "1",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010838.1.10",
          "form": "웨이브가 한국예능 이런 거 많아서여ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "웨이브가 한국예능 이런 거 많아서여ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010838.1.11",
          "form": "왜 넷플릭스 사용하세요?",
          "original_form": "왜 넷플릭스 사용하세요?",
          "speaker_id": "2",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010838.1.12",
          "form": "넷플 자체제작 영상 보고 싶어서요!",
          "original_form": "넷플 자체제작 영상 보고 싶어서요!",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010838.1.13",
          "form": "넷플 들어오고나서 자체제작 드라마나 영화 엄청 홍보했잖아요 그래서 저도 모르게 결제를 ㅋㅋ",
          "original_form": "넷플 들어오고나서 자체제작 드라마나 영화 엄청 홍보했잖아요 그래서 저도 모르게 결제를 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010838.1.14",
          "form": "아 그건 좋은 거 같아요",
          "original_form": "아 그건 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010838.1.15",
          "form": "킹덤도 넷플에만 있지 않나요?",
          "original_form": "킹덤도 넷플에만 있지 않나요?",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010838.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010838.1.17",
          "form": "하지만 좀비물 안 좋아해서 안 봤어요 ㅋㅋ",
          "original_form": "하지만 좀비물 안 좋아해서 안 봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010838.1.18",
          "form": "재밌겠네요 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "재밌겠네요 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010838.1.19",
          "form": "아하 안보셨군요ㅋㅋㅋㅋㅋ!",
          "original_form": "아하 안보셨군요ㅋㅋㅋㅋㅋ!",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010838.1.20",
          "form": "좀비물이 조선시대에 나오는 게 흥미롭긴한데",
          "original_form": "좀비물이 조선시대에 나오는 게 흥미롭긴한데",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010838.1.21",
          "form": "저에게 유일한 좀비물은 부산행이 끝이에요 ㅎ",
          "original_form": "저에게 유일한 좀비물은 부산행이 끝이에요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010838.1.22",
          "form": "전 좀비물 좋아해서 부산행도 재밌게 봤어요!",
          "original_form": "전 좀비물 좋아해서 부산행도 재밌게 봤어요!",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010838.1.23",
          "form": "또 좋아하는 좀비물 있으세요?",
          "original_form": "또 좋아하는 좀비물 있으세요?",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010838.1.24",
          "form": "월드워z 이것도 좋아해요ㅋㅋㅋ",
          "original_form": "월드워z 이것도 좋아해요ㅋㅋㅋ",
