      "utterance": [
        {
          "id": "MDRW2100022849.1.1",
          "form": "티라미수 너무 맛있죠ㅠㅠ",
          "original_form": "티라미수 너무 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100022849.1.2",
          "form": "네 부드럽고 달콤한 행복이쥬",
          "original_form": "네 부드럽고 달콤한 행복이쥬",
          "speaker_id": "2",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100022849.1.3",
          "form": "직접 만들어 드셨나요?",
          "original_form": "직접 만들어 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 18:30"
        },
        {
          "id": "MDRW2100022849.1.4",
          "form": "아뇨 이마트트레이더스에 가서 사왔어요",
          "original_form": "아뇨 이마트트레이더스에 가서 사왔어요",
          "speaker_id": "2",
          "time": "20210829 18:31"
        },
        {
          "id": "MDRW2100022849.1.5",
          "form": "이마트 트레이더스 빵 꽤 잘 나오더라고요! 빵 코너 가면 냄새가 너무 유혹적이에요ㅋㅋㅋㅋ",
          "original_form": "이마트 트레이더스 빵 꽤 잘 나오더라고요! 빵 코너 가면 냄새가 너무 유혹적이에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100022849.1.6",
          "form": "맞아요 많이 줏어 담고픔거 겨우 참고 와요",
          "original_form": "맞아요 많이 줏어 담고픔거 겨우 참고 와요",
          "speaker_id": "2",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022849.1.7",
          "form": "저는 에그타르트 한번 먹봤는데 에그타르트도 맛있었어요~",
          "original_form": "저는 에그타르트 한번 먹봤는데 에그타르트도 맛있었어요~",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022849.1.8",
          "form": "아하 그거도 맛나보였어요 ㅎ",
          "original_form": "아하 그거도 맛나보였어요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 19:58"
        },
        {
          "id": "MDRW2100022849.1.9",
          "form": "빵이랑 어떤 음료 같이 드세요?",
          "original_form": "빵이랑 어떤 음료 같이 드세요?",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022849.1.10",
          "form": "전 아이스 아메리카노요",
          "original_form": "전 아이스 아메리카노요",
          "speaker_id": "2",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022849.1.11",
          "form": "아아가 진리인듯요 \r\n사시사철 그거만 마셔요",
          "original_form": "아아가 진리인듯요 \r\n사시사철 그거만 마셔요",
          "speaker_id": "2",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022849.1.12",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022849.1.13",
          "form": "저도 아아요! 카페가면 고민도 안 하고 아아 마셔요",
          "original_form": "저도 아아요! 카페가면 고민도 안 하고 아아 마셔요",
          "speaker_id": "1",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100022849.1.14",
          "form": "가끔 집에서 먹을 때는 우유랑 먹을 때도 있어요. 우유 좋아하시나요?",
          "original_form": "가끔 집에서 먹을 때는 우유랑 먹을 때도 있어요. 우유 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100022849.1.15",
          "form": "우유도 좋아해요..근데 우유도 살이 돼는거 같아서 가급젓 피하는 중이예요 ㅠ",
          "original_form": "우유도 좋아해요..근데 우유도 살이 돼는거 같아서 가급젓 피하는 중이예요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022849.1.16",
          "form": "유유도 지방 때문인지 칼로리가 꽤 되더라고요ㅠㅠ 커피는 칼로리가 낮아서 다행이에요",
          "original_form": "유유도 지방 때문인지 칼로리가 꽤 되더라고요ㅠㅠ 커피는 칼로리가 낮아서 다행이에요",
          "speaker_id": "1",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022849.1.17",
          "form": "그쳐? 우유도 꾸준히 먹음 살이 돼는게 느껴져요 ㅎ",
          "original_form": "그쳐? 우유도 꾸준히 먹음 살이 돼는게 느껴져요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:11"
        }
      ]
