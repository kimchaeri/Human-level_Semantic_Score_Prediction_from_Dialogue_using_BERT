        {
          "id": "MDRW2100040503.1.1",
          "form": "무슨색샀어요?",
          "original_form": "무슨색샀어요?",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100040503.1.2",
          "form": "검은색 샀어요",
          "original_form": "검은색 샀어요",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100040503.1.3",
          "form": "무난한색을샀군요",
          "original_form": "무난한색을샀군요",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100040503.1.4",
          "form": "아이보리색이 참 맘에 들었지만 때타는 게 감당이 안될거같아",
          "original_form": "아이보리색이 참 맘에 들었지만 때타는 게 감당이 안될거같아",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100040503.1.5",
          "form": "단정하겄어용",
          "original_form": "단정하겄어용",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100040503.1.6",
          "form": "검은색을 살수밖에 없었어요",
          "original_form": "검은색을 살수밖에 없었어요",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100040503.1.7",
          "form": "맞아요소매, 주머니감당안돼요",
          "original_form": "맞아요소매, 주머니감당안돼요",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100040503.1.8",
          "form": "좋은선택이예요",
          "original_form": "좋은선택이예요",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040503.1.9",
          "form": "ㅋㅋㅋㅋㅋ아쉬워요",
          "original_form": "ㅋㅋㅋㅋㅋ아쉬워요",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040503.1.10",
          "form": "ㅋㄱㅋㅋㄲㄱㄲ",
          "original_form": "ㅋㄱㅋㅋㄲㄱㄲ",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040503.1.11",
          "form": "입다가 너무좋으면",
          "original_form": "입다가 너무좋으면",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040503.1.12",
          "form": "다른색을또사는편이예요저는",
          "original_form": "다른색을또사는편이예요저는",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040503.1.13",
          "form": "같은디자인",
          "original_form": "같은디자인",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040503.1.14",
          "form": "저두 그래요",
          "original_form": "저두 그래요",
          "speaker_id": "2",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100040503.1.15",
          "form": "근데 그래도 아이보리는 감당안되겠조?",
          "original_form": "근데 그래도 아이보리는 감당안되겠조?",
          "speaker_id": "2",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040503.1.16",
          "form": "입어보다맘에들ㅡ면",
          "original_form": "입어보다맘에들ㅡ면",
          "speaker_id": "1",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040503.1.17",
          "form": "질러요걍",
          "original_form": "질러요걍",
          "speaker_id": "1",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040503.1.18",
          "form": "ㄱㅋㅈㅋㅈㄱㅈㄱㅊㄱ",
          "original_form": "ㄱㅋㅈㅋㅈㄱㅈㄱㅊㄱ",
          "speaker_id": "1",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040503.1.19",
          "form": "세탁망넣어서돌리면",
          "original_form": "세탁망넣어서돌리면",
          "speaker_id": "1",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040503.1.20",
          "form": "일도아니예요",
          "original_form": "일도아니예요",
          "speaker_id": "1",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040503.1.21",
          "form": "ㄱㅋㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱ",
          "original_form": "ㄱㅋㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱ",
          "speaker_id": "1",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040503.1.22",
          "form": "갑자기호의적",
          "original_form": "갑자기호의적",
          "speaker_id": "1",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040503.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040503.1.24",
          "form": "좋아여 사실 너무 제스타일이라 조만간 다른색 살거같아여",
          "original_form": "좋아여 사실 너무 제스타일이라 조만간 다른색 살거같아여",
          "speaker_id": "2",
          "time": "20210904 22:08"
        },
        {
          "id": "MDRW2100040503.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ아이보리말고도또있었나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아이보리말고도또있었나요?",
          "speaker_id": "1",
          "time": "20210904 22:08"
        },
        {
          "id": "MDRW2100040503.1.26",
