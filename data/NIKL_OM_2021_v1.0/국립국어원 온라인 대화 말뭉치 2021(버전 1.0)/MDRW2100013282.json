      "utterance": [
        {
          "id": "MDRW2100013282.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013282.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013282.1.3",
          "form": "실례가 아니라면 직업이 어떻게되시나요?",
          "original_form": "실례가 아니라면 직업이 어떻게되시나요?",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013282.1.4",
          "form": "저는 사무직에있어요",
          "original_form": "저는 사무직에있어요",
          "speaker_id": "1",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013282.1.5",
          "form": "헉..부럽따..",
          "original_form": "헉..부럽따..",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013282.1.6",
          "form": "직장인..",
          "original_form": "직장인..",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013282.1.7",
          "form": "전 작년에 졸업해서",
          "original_form": "전 작년에 졸업해서",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013282.1.8",
          "form": "아직 백수에요ㅠㅠ",
          "original_form": "아직 백수에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013282.1.9",
          "form": "취준생이시구나",
          "original_form": "취준생이시구나",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013282.1.10",
          "form": "코로나 핑계대곤 있지만",
          "original_form": "코로나 핑계대곤 있지만",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013282.1.11",
          "form": "주변 친구들은 다 취직했쬬..",
          "original_form": "주변 친구들은 다 취직했쬬..",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013282.1.12",
          "form": "마음이 조급해지시겠어요",
          "original_form": "마음이 조급해지시겠어요",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013282.1.13",
          "form": "네네ㅠㅠ 괜히 주변눈치 보이구ㅠㅠ",
          "original_form": "네네ㅠㅠ 괜히 주변눈치 보이구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013282.1.14",
          "form": "그래도 너무조급해하지마세요",
          "original_form": "그래도 너무조급해하지마세요",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013282.1.15",
          "form": "누구보다 멋진인생을 사실거에요",
          "original_form": "누구보다 멋진인생을 사실거에요",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013282.1.16",
          "form": "힝 좋은말씀 감사합니다 :D",
          "original_form": "힝 좋은말씀 감사합니다 :D",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013282.1.17",
          "form": "사무직이면",
          "original_form": "사무직이면",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013282.1.18",
          "form": "주로 엑셀 쓰시는건가요?",
          "original_form": "주로 엑셀 쓰시는건가요?",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013282.1.19",
          "form": "엑셀도 주로 사용하는 편이고 저는 사무직이긴 한데ㅈ영업도 뛰는편이에요",
          "original_form": "엑셀도 주로 사용하는 편이고 저는 사무직이긴 한데ㅈ영업도 뛰는편이에요",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013282.1.20",
          "form": "와아 멋있어요!",
          "original_form": "와아 멋있어요!",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013282.1.21",
          "form": "어릴때 꿈이 딱",
          "original_form": "어릴때 꿈이 딱",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013282.1.22",
          "form": "사무실에서 먼가 다루다",
          "original_form": "사무실에서 먼가 다루다",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013282.1.23",
          "form": "현장 가고 그런거였는데ㅋ큐",
          "original_form": "현장 가고 그런거였는데ㅋ큐",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013282.1.24",
          "form": "회사에선 상사분 많이 무섭나요..?",
          "original_form": "회사에선 상사분 많이 무섭나요..?",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013282.1.25",
          "form": "제가 잘 혼나는편이라서",
          "original_form": "제가 잘 혼나는편이라서",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013282.1.26",
          "form": "그게 제일 걱정이네요ㅠㅠㅠ",
          "original_form": "그게 제일 걱정이네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013282.1.27",
          "form": "앗 답이 늦었습니다",
          "original_form": "앗 답이 늦었습니다",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013282.1.28",
          "form": "어 저는 좀 소규모 회사라 눈치보이긴하는데 신입때는 실수하는것이 다연한고",
          "original_form": "어 저는 좀 소규모 회사라 눈치보이긴하는데 신입때는 실수하는것이 다연한고",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013282.1.29",
          "form": "어.. 너무 그렇게 걱정하지마세요",
          "original_form": "어.. 너무 그렇게 걱정하지마세요",
          "speaker_id": "1",
          "time": "20210825 14:06"
        }
      ]
    }
