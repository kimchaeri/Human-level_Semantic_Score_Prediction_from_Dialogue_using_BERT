      "utterance": [
        {
          "id": "MDRW2100019768.1.1",
          "form": "캠핑 가서 어떤 음식을 드세요?",
          "original_form": "캠핑 가서 어떤 음식을 드세요?",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100019768.1.2",
          "form": "주로 고기와 비빔면 같은거 먹어요 ㅎㅎ",
          "original_form": "주로 고기와 비빔면 같은거 먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100019768.1.3",
          "form": "name2님은 캠핑 가보셨어요?",
          "original_form": "&name2&님은 캠핑 가보셨어요?",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100019768.1.4",
          "form": "넵 남편따라 두어번 가봤어요 ㅎ",
          "original_form": "넵 남편따라 두어번 가봤어요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100019768.1.5",
          "form": "ㅎ 가보셨어요? 좋으셨어요?",
          "original_form": "ㅎ 가보셨어요? 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019768.1.6",
          "form": "name2님은 주로 뭐드세요?",
          "original_form": "&name2&님은 주로 뭐드세요?",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019768.1.7",
          "form": "캠핑은 무조건 고기죠 고기 고기 고기",
          "original_form": "캠핑은 무조건 고기죠 고기 고기 고기",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019768.1.8",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100019768.1.9",
          "form": "고기는 언제나 진리죠 ㅎ",
          "original_form": "고기는 언제나 진리죠 ㅎ",
          "speaker_id": "1",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100019768.1.10",
          "form": "저녁은 주로 뭐해 드세요?",
          "original_form": "저녁은 주로 뭐해 드세요?",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100019768.1.11",
          "form": "고긴 저녁에 먹어야죠 ㅎ",
          "original_form": "고긴 저녁에 먹어야죠 ㅎ",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100019768.1.12",
          "form": "아닝 고긴 언제나 진린데 ㅋㅋㅋㅋㅋ",
          "original_form": "아닝 고긴 언제나 진린데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100019768.1.13",
          "form": "그럼 낮에는 뭐 드세요?",
          "original_form": "그럼 낮에는 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100019768.1.14",
          "form": "피칭하고 간단히 먹느라 라면 먹거나 둘째날부턴 점심에도 고기죠 ㅎ",
          "original_form": "피칭하고 간단히 먹느라 라면 먹거나 둘째날부턴 점심에도 고기죠 ㅎ",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019768.1.15",
          "form": "종류별로 다양한 고기 ㅎ",
          "original_form": "종류별로 다양한 고기 ㅎ",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019768.1.16",
          "form": "아 다양한 고기를 드시는군요ㅋㅋㅋ",
          "original_form": "아 다양한 고기를 드시는군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019768.1.17",
          "form": "거의 돼지고긴데 ㅎㅎㅎㅎ 돼지고기를 종류별로 먹어요 ㅎㅎㅎㅎ",
          "original_form": "거의 돼지고긴데 ㅎㅎㅎㅎ 돼지고기를 종류별로 먹어요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019768.1.18",
          "form": "삼겹살이 제일 맛있지않나요?",
          "original_form": "삼겹살이 제일 맛있지않나요?",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019768.1.19",
          "form": "요즘엔 삼겹살과 목살이나 다리살로 보쌈같은거 먹어요",
          "original_form": "요즘엔 삼겹살과 목살이나 다리살로 보쌈같은거 먹어요",
