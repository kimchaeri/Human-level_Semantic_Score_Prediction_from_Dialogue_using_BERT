      "utterance": [
        {
          "id": "MDRW2100034307.1.1",
          "form": "화장품 중요하죠 ㅜㅜ",
          "original_form": "화장품 중요하죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034307.1.2",
          "form": "진짜 유분기  없는거쓰면",
          "original_form": "진짜 유분기  없는거쓰면",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034307.1.3",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034307.1.4",
          "form": "막 피부 갈라지는거같고 그러더라구요ㅠ",
          "original_form": "막 피부 갈라지는거같고 그러더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034307.1.5",
          "form": "로션 어떤거쓰세요?",
          "original_form": "로션 어떤거쓰세요?",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034307.1.6",
          "form": "저는 로션이랑 스킨은",
          "original_form": "저는 로션이랑 스킨은",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100034307.1.7",
          "form": "사실 화장품 제형만 바꾼 상술이래서 안쓰구",
          "original_form": "사실 화장품 제형만 바꾼 상술이래서 안쓰구",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100034307.1.8",
          "form": "에센스랑 크림만 써요",
          "original_form": "에센스랑 크림만 써요",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100034307.1.9",
          "form": "아 진짜요? 상술이였군요ㅠ",
          "original_form": "아 진짜요? 상술이였군요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034307.1.10",
          "form": "어떤게 좋나요?",
          "original_form": "어떤게 좋나요?",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034307.1.11",
          "form": "음 저는그때그때 다른데",
          "original_form": "음 저는그때그때 다른데",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034307.1.12",
          "form": "고정적으로 쓰는건",
          "original_form": "고정적으로 쓰는건",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034307.1.13",
          "form": "차앤박 프로폴리스 앰플써요",
          "original_form": "차앤박 프로폴리스 앰플써요",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034307.1.14",
          "form": "오호! 나중에 한번 써봐야겠네요",
          "original_form": "오호! 나중에 한번 써봐야겠네요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034307.1.15",
          "form": "넵 상품 설명보면",
          "original_form": "넵 상품 설명보면",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034307.1.16",
          "form": "피부과에서 관리받는 느낌을 준대서 그말에 혹해 삿어요 유명하기도 하구 ㅋㅋㅋ",
          "original_form": "피부과에서 관리받는 느낌을 준대서 그말에 혹해 삿어요 유명하기도 하구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034307.1.17",
          "form": "오~말만들어도 좋아보이네요",
          "original_form": "오~말만들어도 좋아보이네요",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034307.1.18",
          "form": "그쵸 ㅋㅋ혹해요",
          "original_form": "그쵸 ㅋㅋ혹해요",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034307.1.19",
          "form": "선크림도 바르시죠?",
          "original_form": "선크림도 바르시죠?",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034307.1.20",
          "form": "아 그건 안발라요 ㅜㅜ",
          "original_form": "아 그건 안발라요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034307.1.21",
          "form": "필수라고들 하는데",
          "original_form": "필수라고들 하는데",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034307.1.22",
          "form": "전에 한번 발랏다가 모공이 금새 커지는거보고 충격받아서..  못바르겟더라구요 ㅠㅠ",
          "original_form": "전에 한번 발랏다가 모공이 금새 커지는거보고 충격받아서..  못바르겟더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034307.1.23",
          "form": "선크림바르면 모공 커지나요??",
          "original_form": "선크림바르면 모공 커지나요??",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100034307.1.24",
          "form": "충격적이네요ㅠ",
          "original_form": "충격적이네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100034307.1.25",
          "form": "그런 말은 못들엇는데 이상하게 저는 그러더라구여 ㅠㅠ",
          "original_form": "그런 말은 못들엇는데 이상하게 저는 그러더라구여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100034307.1.26",
          "form": "기름기많은 제품이엇는지...",
          "original_form": "기름기많은 제품이엇는지...",
          "speaker_id": "1",
          "time": "20210827 23:18"
        }
      ]
    }
  ]
