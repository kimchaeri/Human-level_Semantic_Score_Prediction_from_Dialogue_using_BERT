      "utterance": [
        {
          "id": "MDRW2100036074.1.1",
          "form": "저는 지금 후식 먹고 있어요!",
          "original_form": "저는 지금 후식 먹고 있어요!",
          "speaker_id": "2",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036074.1.2",
          "form": "먹는게 젤 즐거워요",
          "original_form": "먹는게 젤 즐거워요",
          "speaker_id": "1",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036074.1.3",
          "form": "맞아용ㅋㅋ",
          "original_form": "맞아용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036074.1.4",
          "form": "좋아하는 과일 있으세요?",
          "original_form": "좋아하는 과일 있으세요?",
          "speaker_id": "2",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100036074.1.5",
          "form": "전 먹기편한 귤 바나나 이런거요",
          "original_form": "전 먹기편한 귤 바나나 이런거요",
          "speaker_id": "1",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100036074.1.6",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100036074.1.7",
          "form": "귤! 곧 귤을 먹을 수 있을때네요",
          "original_form": "귤! 곧 귤을 먹을 수 있을때네요",
          "speaker_id": "2",
          "time": "20210830 15:55"
        },
        {
          "id": "MDRW2100036074.1.8",
          "form": "네네 한박스 금방 먹어요",
          "original_form": "네네 한박스 금방 먹어요",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100036074.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036074.1.10",
          "form": "왜 하루에 4개만 먹으라는건지 모르겠어요",
          "original_form": "왜 하루에 4개만 먹으라는건지 모르겠어요",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036074.1.11",
          "form": "먹는 즐거움 대단해요",
          "original_form": "먹는 즐거움 대단해요",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100036074.1.12",
          "form": "진짜요 과일은 없어서 못먹죠!",
          "original_form": "진짜요 과일은 없어서 못먹죠!",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036074.1.13",
          "form": "과일도 비싸더라구요",
          "original_form": "과일도 비싸더라구요",
          "speaker_id": "1",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100036074.1.14",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100036074.1.15",
          "form": "날씨 영향도 많이 받구요ㅠ",
          "original_form": "날씨 영향도 많이 받구요ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100036074.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100036074.1.17",
