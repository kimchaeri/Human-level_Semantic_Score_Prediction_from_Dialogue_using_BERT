      "utterance": [
        {
          "id": "MDRW2100042349.1.1",
          "form": "저는이제아기재우러가볼게요",
          "original_form": "저는이제아기재우러가볼게요",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042349.1.2",
          "form": "내일 또 좋은대화 나누어요",
          "original_form": "내일 또 좋은대화 나누어요",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042349.1.3",
          "form": "굿밤하세요!",
          "original_form": "굿밤하세요!",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042349.1.4",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042349.1.5",
          "form": "오늘 운동 하셨나요???",
          "original_form": "오늘 운동 하셨나요???",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100042349.1.6",
          "form": "오늘은 못했어요",
          "original_form": "오늘은 못했어요",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100042349.1.7",
          "form": "비와서ㅠㅠ",
          "original_form": "비와서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100042349.1.8",
          "form": "ㅜㅜㅋㅋ 저는 오늘 딱 그날이 터져서ㅜ",
          "original_form": "ㅜㅜㅋㅋ 저는 오늘 딱 그날이 터져서ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100042349.1.9",
          "form": "아이고ㅠㅠ",
          "original_form": "아이고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100042349.1.10",
          "form": "생리끝나고가 다이어트황금기더라구요",
          "original_form": "생리끝나고가 다이어트황금기더라구요",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100042349.1.11",
          "form": "생리끝나면 정체기가딱풀려요",
          "original_form": "생리끝나면 정체기가딱풀려요",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100042349.1.12",
          "form": "황금기 누려야하는데 그땐 운동안하고 폭식",
          "original_form": "황금기 누려야하는데 그땐 운동안하고 폭식",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100042349.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100042349.1.14",
          "form": "생리통 있으세요?",
          "original_form": "생리통 있으세요?",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100042349.1.15",
          "form": "생리통은출산하고나니 많이줄었어요",
          "original_form": "생리통은출산하고나니 많이줄었어요",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100042349.1.16",
          "form": "아 출산에 장점이!!!",
          "original_form": "아 출산에 장점이!!!",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100042349.1.17",
          "form": "글트라구요^^",
          "original_form": "글트라구요^^",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100042349.1.18",
          "form": "남자를 구해서 애를\n낳아야하나ㅋㅋ싶고요",
          "original_form": "남자를 구해서 애를\n낳아야하나ㅋㅋ싶고요",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100042349.1.19",
          "form": "아이고ㅋㅋㅋㅋㅋ",
          "original_form": "아이고ㅋㅋㅋㅋㅋ",
