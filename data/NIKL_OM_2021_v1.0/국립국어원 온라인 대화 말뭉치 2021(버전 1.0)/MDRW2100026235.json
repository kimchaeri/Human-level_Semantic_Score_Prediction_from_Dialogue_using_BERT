      "utterance": [
        {
          "id": "MDRW2100026235.1.1",
          "form": "어떤 음식 좋아하시나요?",
          "original_form": "어떤 음식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026235.1.2",
          "form": "저는 국물있는 탕 좋아해요",
          "original_form": "저는 국물있는 탕 좋아해요",
          "speaker_id": "2",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100026235.1.3",
          "form": "칼칼한거",
          "original_form": "칼칼한거",
          "speaker_id": "2",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100026235.1.4",
          "form": "오늘 같은 날에는 국물있는 탕이 참 좋죠!!",
          "original_form": "오늘 같은 날에는 국물있는 탕이 참 좋죠!!",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026235.1.5",
          "form": "그러게요ㅎㅎ 저녁 뭐드셨나요",
          "original_form": "그러게요ㅎㅎ 저녁 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026235.1.6",
          "form": "저는 양념게장이 좀 있어서 양념게장에 밥 먹었습니다 ㅎㅎ",
          "original_form": "저는 양념게장이 좀 있어서 양념게장에 밥 먹었습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026235.1.7",
          "form": "와 대박이네요",
          "original_form": "와 대박이네요",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026235.1.8",
          "form": "어디서사셨나요",
          "original_form": "어디서사셨나요",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026235.1.9",
          "form": "엄마께서 직접 만들어주셨어요 ㅎㅎ",
          "original_form": "엄마께서 직접 만들어주셨어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026235.1.10",
          "form": "양념게장은 사먹을때마다 늘 실패라서 ㅠㅠ",
          "original_form": "양념게장은 사먹을때마다 늘 실패라서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026235.1.11",
          "form": "맞아요 양념이 중요한데 입맛에 안맞으면 실패확률이 높을거같아요",
          "original_form": "맞아요 양념이 중요한데 입맛에 안맞으면 실패확률이 높을거같아요",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026235.1.12",
          "form": "어머니가 요리를 잘 하시나봐요!",
          "original_form": "어머니가 요리를 잘 하시나봐요!",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026235.1.13",
          "form": "게장은 사서먹는거아니면  집에서 먹어본적 없는거같아요ㅋㅋ",
          "original_form": "게장은 사서먹는거아니면  집에서 먹어본적 없는거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026235.1.14",
          "form": "오 게장 괜찮은 곳이 있나요? 저도 한 번 사서 맛있는 곳을 찾아보고 싶어요!!",
          "original_form": "오 게장 괜찮은 곳이 있나요? 저도 한 번 사서 맛있는 곳을 찾아보고 싶어요!!",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026235.1.15",
          "form": "간장게장은 개인적으로 파는게 더 맛있더라구요!",
          "original_form": "간장게장은 개인적으로 파는게 더 맛있더라구요!",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026235.1.16",
          "form": "게장안먹은지 오래됐네요ㅎㅎ",
          "original_form": "게장안먹은지 오래됐네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026235.1.17",
          "form": "간장게장 배부르게 먹고싶은데 한마리가 너무 비싼거같아요ㅠㅠ",
          "original_form": "간장게장 배부르게 먹고싶은데 한마리가 너무 비싼거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026235.1.18",
          "form": "그쵸 ㅠㅠㅠ 가격이 너무 사악해요",
          "original_form": "그쵸 ㅠㅠㅠ 가격이 너무 사악해요",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026235.1.19",
          "form": "맛은 너무 좋은데",
          "original_form": "맛은 너무 좋은데",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026235.1.20",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026235.1.21",
          "form": "꽃게도좋은데",
          "original_form": "꽃게도좋은데",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026235.1.22",
          "form": "돌게라고 작은게도 맛있어서 좀 가성비생각하면",
          "original_form": "돌게라고 작은게도 맛있어서 좀 가성비생각하면",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026235.1.23",
          "form": "돌게장먹으러가기도해요 ㅎㅎ",
          "original_form": "돌게장먹으러가기도해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026235.1.24",
          "form": "아 돌게도 괜찮죠!!",
          "original_form": "아 돌게도 괜찮죠!!",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026235.1.25",
          "form": "꽃게 된장국도 참 맛있는데",
          "original_form": "꽃게 된장국도 참 맛있는데",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026235.1.26",
          "form": "갑자기 생각나네요",
          "original_form": "갑자기 생각나네요",
          "speaker_id": "1",
          "time": "20210901 21:20"
        }
      ]
    }
  ]
