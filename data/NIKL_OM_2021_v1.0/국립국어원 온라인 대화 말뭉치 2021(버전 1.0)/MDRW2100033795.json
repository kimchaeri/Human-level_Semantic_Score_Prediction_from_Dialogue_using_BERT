        },
        {
          "id": "MDRW2100033795.1.2",
          "form": "안녕하세요 점심 드셨나요??",
          "original_form": "안녕하세요 점심 드셨나요??",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100033795.1.3",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100033795.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100033795.1.5",
          "form": "저는 이제 먹으러 갑니다!!!ㅎㅎㅎㅎ",
          "original_form": "저는 이제 먹으러 갑니다!!!ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100033795.1.6",
          "form": "뭐드셨나요?!!",
          "original_form": "뭐드셨나요?!!",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100033795.1.7",
          "form": "전치킨이요ㅋㅋㅋ",
          "original_form": "전치킨이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100033795.1.8",
          "form": "왤케늦게드세요?ㅠ",
          "original_form": "왤케늦게드세요?ㅠ",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100033795.1.9",
          "form": "앗 운동하고 먹느라고ㅠㅠㅠ",
          "original_form": "앗 운동하고 먹느라고ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100033795.1.10",
          "form": "아나 멋진 점심 하셨는걸요..?",
          "original_form": "아나 멋진 점심 하셨는걸요..?",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100033795.1.11",
          "form": "이길 수 ×",
          "original_form": "이길 수 ×",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100033795.1.12",
          "form": "ㅋㅋㅋㅋㅋ ㅇ",
          "original_form": "ㅋㅋㅋㅋㅋ ㅇ",
          "speaker_id": "1",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100033795.1.13",
          "form": "와우 운동 마니하시나봐요?",
          "original_form": "와우 운동 마니하시나봐요?",
          "speaker_id": "1",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100033795.1.14",
          "form": "아뇨 저 이번주 첨나왔는데요?!",
          "original_form": "아뇨 저 이번주 첨나왔는데요?!",
          "speaker_id": "2",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100033795.1.15",
          "form": "머쓱",
          "original_form": "머쓱",
          "speaker_id": "2",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100033795.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100033795.1.17",
          "form": "근데 저 마라탕이 너무 먹고싶어요",
          "original_form": "근데 저 마라탕이 너무 먹고싶어요",
          "speaker_id": "2",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100033795.1.18",
          "form": "눈물나",
          "original_form": "눈물나",
          "speaker_id": "2",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100033795.1.19",
          "form": "ㅋㅋㅋ 무슨운동하시나요?",
          "original_form": "ㅋㅋㅋ 무슨운동하시나요?",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100033795.1.20",
          "form": "매운거잘드세요?",
          "original_form": "매운거잘드세요?",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100033795.1.21",
          "form": "저 헬스요",
          "original_form": "저 헬스요",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100033795.1.22",
          "form": "아뇨 맵기 1로 신라면 맵기로 먹으면 넘 맛있더라고요",
          "original_form": "아뇨 맵기 1로 신라면 맵기로 먹으면 넘 맛있더라고요",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100033795.1.23",
          "form": "쓰읍..다이어트하기전에 딱 먹었는데 너무 먹고싶어요",
          "original_form": "쓰읍..다이어트하기전에 딱 먹었는데 너무 먹고싶어요",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100033795.1.24",
          "form": "아 남자분이신가요? 아닌가",
          "original_form": "아 남자분이신가요? 아닌가",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100033795.1.25",
          "form": "ㅋㅋㅋ 1봉정돈",
          "original_form": "ㅋㅋㅋ 1봉정돈",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100033795.1.26",
          "form": "괜찮지않나요?",
          "original_form": "괜찮지않나요?",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100033795.1.27",
          "form": "아뇨 저 여자",
          "original_form": "아뇨 저 여자",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100033795.1.28",
          "form": "괜찮아요 근데 많이먹으려면(?)",
          "original_form": "괜찮아요 근데 많이먹으려면(?)",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100033795.1.29",
          "form": "1이 딱 적당하더라고요",
          "original_form": "1이 딱 적당하더라고요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100033795.1.30",
          "form": "떡볶이 좋아하시나요",
          "original_form": "떡볶이 좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100033795.1.31",
          "form": "남자분이세요??",
          "original_form": "남자분이세요??",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100033795.1.32",
          "form": "그럼 제육볶음 이야기 준비되어있습니다.",
          "original_form": "그럼 제육볶음 이야기 준비되어있습니다.",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100033795.1.33",
          "form": "여자예욬ㅋㅋㅋ",
          "original_form": "여자예욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100033795.1.34",
          "form": "떡볶이좋아한답니다",
          "original_form": "떡볶이좋아한답니다",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100033795.1.35",
          "form": "대세는 로제죠",
          "original_form": "대세는 로제죠",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100033795.1.36",
          "form": "ㅇㄴ 쓰읍 군침도네요",
          "original_form": "ㅇㄴ 쓰읍 군침도네요",
