        {
          "id": "MDRW2100039901.1.1",
          "form": "사극이라서 다들 안한다고 했나 ㅋㅋ",
          "original_form": "사극이라서 다들 안한다고 했나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039901.1.2",
          "form": "내용도 재미없고 방송사도 별로예요ㅋ",
          "original_form": "내용도 재미없고 방송사도 별로예요ㅋ",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039901.1.3",
          "form": "엉 나도 느낌이 그렇다",
          "original_form": "엉 나도 느낌이 그렇다",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039901.1.4",
          "form": "그쵸 여주도 좀 약하고",
          "original_form": "그쵸 여주도 좀 약하고",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039901.1.5",
          "form": "나래 박차오르다 가제인데 이것도 잇어",
          "original_form": "나래 박차오르다 가제인데 이것도 잇어",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039901.1.6",
          "form": "나래? 이름이겠죠?",
          "original_form": "나래? 이름이겠죠?",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039901.1.7",
          "form": "몰락한 전직 태권도 국가대표를 주인공으로 한 액션 코믹 드라마라네",
          "original_form": "몰락한 전직 태권도 국가대표를 주인공으로 한 액션 코믹 드라마라네",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039901.1.8",
          "form": "음 식 상 하네 이 드라마도ㅋ",
          "original_form": "음 식 상 하네 이 드라마도ㅋ",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039901.1.9",
          "form": "나래라니 여주인공이네",
          "original_form": "나래라니 여주인공이네",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039901.1.10",
          "form": "그러게요 여주가 누구예요?",
          "original_form": "그러게요 여주가 누구예요?",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039901.1.11",
          "form": "스테파니리  모델이 주인공",
          "original_form": "스테파니리  모델이 주인공",
          "speaker_id": "2",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039901.1.12",
          "form": "아 약하네 여주가. 거의 조연했는데",
          "original_form": "아 약하네 여주가. 거의 조연했는데",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100039901.1.13",
          "form": "황후의 품격에서 오헬로 조연했던",
          "original_form": "황후의 품격에서 오헬로 조연했던",
          "speaker_id": "2",
          "time": "20210903 22:31"
        }
