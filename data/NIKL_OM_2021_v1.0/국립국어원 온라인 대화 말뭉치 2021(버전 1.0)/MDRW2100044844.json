      "utterance": [
        {
          "id": "MDRW2100044844.1.1",
          "form": "진짜 내가 어지간해선",
          "original_form": "진짜 내가 어지간해선",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044844.1.2",
          "form": "별의 별 게임 운영사를 다 겪어봐서",
          "original_form": "별의 별 게임 운영사를 다 겪어봐서",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044844.1.3",
          "form": "말 안얹는데",
          "original_form": "말 안얹는데",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044844.1.4",
          "form": "얘넨 심각해",
          "original_form": "얘넨 심각해",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044844.1.5",
          "form": "ㅇㅇ인디겜식 운영정도는",
          "original_form": "ㅇㅇ인디겜식 운영정도는",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044844.1.6",
          "form": "걍 눈감아주거든",
          "original_form": "걍 눈감아주거든",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044844.1.7",
          "form": "열정만 보이면",
          "original_form": "열정만 보이면",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044844.1.8",
          "form": "그치 ㅋㅋㅋ 게임 첨하는 것도 아니고...",
          "original_form": "그치 ㅋㅋㅋ 게임 첨하는 것도 아니고...",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044844.1.9",
          "form": "근데 얘넨 하고싶긴 한거임?",
          "original_form": "근데 얘넨 하고싶긴 한거임?",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044844.1.10",
          "form": "그니까 그게 젤 의문임",
          "original_form": "그니까 그게 젤 의문임",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044844.1.11",
          "form": "게임 운영 하고싶긴 한건가?",
          "original_form": "게임 운영 하고싶긴 한건가?",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044844.1.12",
          "form": "걍 냅다 일 벌여서",
          "original_form": "걍 냅다 일 벌여서",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044844.1.13",
          "form": "1억 받아놓고",
          "original_form": "1억 받아놓고",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044844.1.14",
          "form": "수틀려서 다 때려치고싶은데",
          "original_form": "수틀려서 다 때려치고싶은데",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044844.1.15",
          "form": "1억 받은거 수습할 방법은 없어서",
          "original_form": "1억 받은거 수습할 방법은 없어서",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044844.1.16",
          "form": "과제하다 말았는데 제출 시간 다되어가는거보고 이거라도 내야 점수의 형태라도ㅂㅏㄷ겠지 하고 미완성 과제 냅다 제출하는 대학생임?",
          "original_form": "과제하다 말았는데 제출 시간 다되어가는거보고 이거라도 내야 점수의 형태라도ㅂㅏㄷ겠지 하고 미완성 과제 냅다 제출하는 대학생임?",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044844.1.17",
          "form": "ㅇㅇ 대충 기워서 미완작 낸거같음 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 대충 기워서 미완작 낸거같음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044844.1.18",
          "form": "근데 그거 외엔 상황 설명할 길이없어",
          "original_form": "근데 그거 외엔 상황 설명할 길이없어",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044844.1.19",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㄹㅇ 걍",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㄹㅇ 걍",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044844.1.20",
          "form": "퀄이",
          "original_form": "퀄이",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044844.1.21",
          "form": "만들다 말았어",
          "original_form": "만들다 말았어",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044844.1.22",
          "form": "아니 제작만화 보면",
          "original_form": "아니 제작만화 보면",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044844.1.23",
          "form": "아예 생각 없이 낸건 아닌거같은데",
          "original_form": "아예 생각 없이 낸건 아닌거같은데",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044844.1.24",
          "form": "어쩌다 이렇게 됐니...",
          "original_form": "어쩌다 이렇게 됐니...",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044844.1.25",
          "form": "그때까진",
          "original_form": "그때까진",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044844.1.26",
          "form": "그랬겠지ㅎ",
          "original_form": "그랬겠지ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044844.1.27",
          "form": "솔직히 말하자면",
          "original_form": "솔직히 말하자면",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044844.1.28",
          "form": "난 약간 뭐랄까",
          "original_form": "난 약간 뭐랄까",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044844.1.29",
          "form": "커미션으로 소액대출 댕겨쓰는",
          "original_form": "커미션으로 소액대출 댕겨쓰는",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044844.1.30",
          "form": "트위터 그림러의 향이",
          "original_form": "트위터 그림러의 향이",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044844.1.31",
          "form": "느껴져.....",
          "original_form": "느껴져.....",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044844.1.32",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044844.1.33",
          "form": "진짜 비유 찰떡이다",
          "original_form": "진짜 비유 찰떡이다",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044844.1.34",
          "form": "아니 비유가 아니라 ㄹㅇ 그계열같아......",
          "original_form": "아니 비유가 아니라 ㄹㅇ 그계열같아......",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044844.1.35",
          "form": "그림체부터",
          "original_form": "그림체부터",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044844.1.36",
          "form": "하는짓까지",
          "original_form": "하는짓까지",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044844.1.37",
          "form": "그림체는 략간 나의 트라우마로 인한",
          "original_form": "그림체는 략간 나의 트라우마로 인한",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044844.1.38",
          "form": "편견이 섞였지만",
          "original_form": "편견이 섞였지만",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044844.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 근제",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 근제",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044844.1.40",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044844.1.41",
          "form": "그림체도 트위터 동인느낌 나긴 해",
          "original_form": "그림체도 트위터 동인느낌 나긴 해",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044844.1.42",
          "form": "ㅇㅇㅎ",
          "original_form": "ㅇㅇㅎ",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044844.1.43",
          "form": "여튼 진짜",
          "original_form": "여튼 진짜",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044844.1.44",
          "form": "늘 하는 말이지만",
          "original_form": "늘 하는 말이지만",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044844.1.45",
          "form": "1억씩이나 받아놓고",
          "original_form": "1억씩이나 받아놓고",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044844.1.46",
          "form": "이따위로 나를줄은",
          "original_form": "이따위로 나를줄은",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044844.1.47",
          "form": "몰랐네",
          "original_form": "몰랐네",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044844.1.48",
          "form": "그니까 ㅋㅋㅋㅋ 한두푼도 아니고...",
          "original_form": "그니까 ㅋㅋㅋㅋ 한두푼도 아니고...",
          "speaker_id": "1",
          "time": "20210908 00:05"
        }
