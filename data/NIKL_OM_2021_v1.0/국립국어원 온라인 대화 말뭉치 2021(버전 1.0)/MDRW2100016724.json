      "utterance": [
        {
          "id": "MDRW2100016724.1.1",
          "form": "털많이빠지면 힘들것같아서",
          "original_form": "털많이빠지면 힘들것같아서",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016724.1.2",
          "form": "크기보다 털빠짐이 중요한거같아요",
          "original_form": "크기보다 털빠짐이 중요한거같아요",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016724.1.3",
          "form": "너무 크지만않으면 상관없어요 ㅎㅎ",
          "original_form": "너무 크지만않으면 상관없어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016724.1.4",
          "form": "아 맞아요 털 많이빠지는 강아지들은 처음볼때 무슨 피부 벗겨지는줄 알았어요ㅋㅋ",
          "original_form": "아 맞아요 털 많이빠지는 강아지들은 처음볼때 무슨 피부 벗겨지는줄 알았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016724.1.5",
          "form": "확실히 털빠짐으로는 푸들이 좋은 것 같아요!",
          "original_form": "확실히 털빠짐으로는 푸들이 좋은 것 같아요!",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016724.1.6",
          "form": "푸들은 똘똘하고 털도안빠진다고 들었어요 ㅎㅎ",
          "original_form": "푸들은 똘똘하고 털도안빠진다고 들었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016724.1.7",
          "form": "댕댕이 몇살이에요ㅜㅜ?",
          "original_form": "댕댕이 몇살이에요ㅜㅜ?",
          "speaker_id": "2",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016724.1.8",
          "form": "다섯살이요! 똑똑한데 너무 똑똑해서 사고도 많이 쳐요ㅠㅠ",
          "original_form": "다섯살이요! 똑똑한데 너무 똑똑해서 사고도 많이 쳐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016724.1.9",
          "form": "앗 애기네요 ㅎㅎ",
          "original_form": "앗 애기네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016724.1.10",
          "form": "귀엽겟당",
          "original_form": "귀엽겟당",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016724.1.11",
          "form": "강아지나이로는 철들법도 한데 걍 사고뭉치예요ㅋㅋ",
          "original_form": "강아지나이로는 철들법도 한데 걍 사고뭉치예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016724.1.12",
          "form": "저번주엔 비와서 산책 못간다고 엄청 삐치고...",
          "original_form": "저번주엔 비와서 산책 못간다고 엄청 삐치고...",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016724.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016724.1.14",
          "form": "똑똑해서ㅋㅋ",
          "original_form": "똑똑해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016724.1.15",
          "form": "다아나봐유",
          "original_form": "다아나봐유",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016724.1.16",
          "form": "첫 반려견인가요?",
          "original_form": "첫 반려견인가요?",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016724.1.17",
          "form": "다행히 큰 문제는 없이 잘 크네요ㅠㅠ",
          "original_form": "다행히 큰 문제는 없이 잘 크네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100016724.1.18",
          "form": "ㅎㅎ다행이에요",
          "original_form": "ㅎㅎ다행이에요",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016724.1.19",
          "form": "햄스터나 고슴도치같은 소동물 키워보신 적 있으세요??",
          "original_form": "햄스터나 고슴도치같은 소동물 키워보신 적 있으세요??",
          "speaker_id": "1",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016724.1.20",
          "form": "없어요ㅋㅋ",
          "original_form": "없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016724.1.21",
          "form": "다람쥐는 많이 귀여움",
          "original_form": "다람쥐는 많이 귀여움",
          "speaker_id": "2",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016724.1.22",
          "form": "ㅎㅎㅎ다람쥐도 넘 귀엽죠 키우기는 되게 어려울것같지만...",
          "original_form": "ㅎㅎㅎ다람쥐도 넘 귀엽죠 키우기는 되게 어려울것같지만...",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016724.1.23",
          "form": "ㅋㅋ제 남친은 다람쥐 키웟엇어요",
          "original_form": "ㅋㅋ제 남친은 다람쥐 키웟엇어요",
          "speaker_id": "2",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016724.1.24",
          "form": "강아지말고 다른 동물 키워보셧나요?",
          "original_form": "강아지말고 다른 동물 키워보셧나요?",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016724.1.25",
          "form": "몇년전에 햄스터 키웠어요! 다람쥐 진짜 특이하네요ㅎㅎㅎㅎ",
          "original_form": "몇년전에 햄스터 키웠어요! 다람쥐 진짜 특이하네요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:39"
        }
      ]
    }
