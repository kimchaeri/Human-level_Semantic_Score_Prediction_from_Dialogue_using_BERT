      "utterance": [
        {
          "id": "MDRW2100023919.1.1",
          "form": "군대 이야기 들으신적 있나요",
          "original_form": "군대 이야기 들으신적 있나요",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100023919.1.2",
          "form": "음 아니요",
          "original_form": "음 아니요",
          "speaker_id": "2",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100023919.1.3",
          "form": "들으신것 있으세요",
          "original_form": "들으신것 있으세요",
          "speaker_id": "2",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100023919.1.4",
          "form": "저는 남편에게요 ㅎㅎ 친구나 가족에게 안들으시나요?",
          "original_form": "저는 남편에게요 ㅎㅎ 친구나 가족에게 안들으시나요?",
          "speaker_id": "1",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100023919.1.5",
          "form": "ㅎㅎ 남편분은 어디 나오셨나요?",
          "original_form": "ㅎㅎ 남편분은 어디 나오셨나요?",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100023919.1.6",
          "form": "의무소방이라고 아시나요?",
          "original_form": "의무소방이라고 아시나요?",
          "speaker_id": "1",
          "time": "20210831 06:06"
        },
        {
          "id": "MDRW2100023919.1.7",
          "form": "앗 그건 소방서에서 일하시는건가요",
          "original_form": "앗 그건 소방서에서 일하시는건가요",
          "speaker_id": "2",
          "time": "20210831 09:35"
        },
        {
          "id": "MDRW2100023919.1.8",
          "form": "정말 군대는 보직이 많네요 ㅋㅋ",
          "original_form": "정말 군대는 보직이 많네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:35"
        },
        {
          "id": "MDRW2100023919.1.9",
          "form": "네 소방서요~^^ 결혼하셨나요",
          "original_form": "네 소방서요~^^ 결혼하셨나요",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100023919.1.10",
          "form": "아니요 ㅎㅎ",
          "original_form": "아니요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100023919.1.11",
          "form": "저희도 곧 미국처럼될까요?",
          "original_form": "저희도 곧 미국처럼될까요?",
          "speaker_id": "2",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100023919.1.12",
          "form": "지원병제랄까",
          "original_form": "지원병제랄까",
          "speaker_id": "2",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100023919.1.13",
          "form": "그럼 좋을텐데..분단이라는 특수상황이 ㅠㅠ",
          "original_form": "그럼 좋을텐데..분단이라는 특수상황이 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100023919.1.14",
          "form": "음 그건 그렇네요ㅜ",
          "original_form": "음 그건 그렇네요ㅜ",
          "speaker_id": "2",
          "time": "20210831 11:51"
        },
        {
          "id": "MDRW2100023919.1.15",
          "form": "개인적으로는",
          "original_form": "개인적으로는",
          "speaker_id": "2",
          "time": "20210831 11:51"
        },
        {
          "id": "MDRW2100023919.1.16",
          "form": "군대에서 핸드폰 사용가능하게한건",
          "original_form": "군대에서 핸드폰 사용가능하게한건",
          "speaker_id": "2",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100023919.1.17",
          "form": "잘한결정같아요ㅎ",
          "original_form": "잘한결정같아요ㅎ",
          "speaker_id": "2",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100023919.1.18",
          "form": "오 그럼 이제 모든 부대가 가능한건가요? 저는 잘 몰랐어요",
          "original_form": "오 그럼 이제 모든 부대가 가능한건가요? 저는 잘 몰랐어요",
          "speaker_id": "1",
          "time": "20210831 11:54"
        },
        {
          "id": "MDRW2100023919.1.19",
          "form": "저는 그런걸로 알고있어요",
          "original_form": "저는 그런걸로 알고있어요",
          "speaker_id": "2",
          "time": "20210831 11:55"
        },
        {
          "id": "MDRW2100023919.1.20",
          "form": "계속 24시간 쓰는건아니고",
          "original_form": "계속 24시간 쓰는건아니고",
          "speaker_id": "2",
          "time": "20210831 11:55"
        },
        {
          "id": "MDRW2100023919.1.21",
          "form": "학교처럼 걷어갔다가 6시인가 일과 끝나면 주는식이었던거같아요",
          "original_form": "학교처럼 걷어갔다가 6시인가 일과 끝나면 주는식이었던거같아요",
