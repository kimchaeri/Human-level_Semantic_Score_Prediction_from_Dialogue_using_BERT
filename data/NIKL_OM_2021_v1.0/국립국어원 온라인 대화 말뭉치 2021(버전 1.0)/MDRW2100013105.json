      "utterance": [
        {
          "id": "MDRW2100013105.1.1",
          "form": "요즘 무슨 책 읽어",
          "original_form": "요즘 무슨 책 읽어",
          "speaker_id": "2",
          "time": "20210825 12:33"
        },
        {
          "id": "MDRW2100013105.1.2",
          "form": "나 주식투자 입문서로 유명한것들",
          "original_form": "나 주식투자 입문서로 유명한것들",
          "speaker_id": "1",
          "time": "20210825 12:36"
        },
        {
          "id": "MDRW2100013105.1.3",
          "form": "도장깨기하는중",
          "original_form": "도장깨기하는중",
          "speaker_id": "1",
          "time": "20210825 12:36"
        },
        {
          "id": "MDRW2100013105.1.4",
          "form": "넌 뮤ㅓ읽어",
          "original_form": "넌 뮤ㅓ읽어",
          "speaker_id": "1",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013105.1.5",
          "form": "난 소설만 읽는 중",
          "original_form": "난 소설만 읽는 중",
          "speaker_id": "2",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013105.1.6",
          "form": "재밌어",
          "original_form": "재밌어",
          "speaker_id": "2",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013105.1.7",
          "form": "나 진짜 겨울방학때는",
          "original_form": "나 진짜 겨울방학때는",
          "speaker_id": "2",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013105.1.8",
          "form": "이과계열 책 읽을꺼야",
          "original_form": "이과계열 책 읽을꺼야",
          "speaker_id": "2",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013105.1.9",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013105.1.10",
          "form": "좋네",
          "original_form": "좋네",
          "speaker_id": "1",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013105.1.11",
          "form": "나도 인문학 읽어야지 그때",
          "original_form": "나도 인문학 읽어야지 그때",
          "speaker_id": "1",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013105.1.12",
          "form": "좋다좋다",
          "original_form": "좋다좋다",
          "speaker_id": "2",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100013105.1.13",
          "form": "사실 사놨던 정세랑작가님 소설 읽다가",
          "original_form": "사실 사놨던 정세랑작가님 소설 읽다가",
          "speaker_id": "1",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100013105.1.14",
          "form": "인물이 너무많이나와서..ㅋㅋㅋㅋ",
          "original_form": "인물이 너무많이나와서..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100013105.1.15",
          "form": "진도가 안나가서 주식책으로 바꾼거임",
          "original_form": "진도가 안나가서 주식책으로 바꾼거임",
          "speaker_id": "1",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100013105.1.16",
          "form": "인물 많으면",
          "original_form": "인물 많으면",
          "speaker_id": "2",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100013105.1.17",
          "form": "읽기 쉽지 않지",
          "original_form": "읽기 쉽지 않지",
          "speaker_id": "2",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100013105.1.18",
          "form": "내가 읽은 책 중 역대급은 토지였음",
          "original_form": "내가 읽은 책 중 역대급은 토지였음",
          "speaker_id": "2",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100013105.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ토지...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ토지...",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100013105.1.20",
          "form": "그래도 읽엇구나",
          "original_form": "그래도 읽엇구나",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100013105.1.21",
          "form": "대단해",
          "original_form": "대단해",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100013105.1.22",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100013105.1.23",
          "form": "1권 30페이지 읽으면서",
          "original_form": "1권 30페이지 읽으면서",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100013105.1.24",
          "form": "뒤에 인물 소개만 몇번씩 읽다가",
          "original_form": "뒤에 인물 소개만 몇번씩 읽다가",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100013105.1.25",
          "form": "포기했어",
          "original_form": "포기했어",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100013105.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100013105.1.27",
          "form": "피프티피플 읽는데",
          "original_form": "피프티피플 읽는데",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100013105.1.28",
          "form": "이게진짜 제목그대로 50명의 이야기임",
          "original_form": "이게진짜 제목그대로 50명의 이야기임",
          "speaker_id": "1",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100013105.1.29",
          "form": "응 그건 뭐야?",
          "original_form": "응 그건 뭐야?",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100013105.1.30",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100013105.1.31",
          "form": "근데 옴니버스처럼 따로따로인 이야기가 아니라",
          "original_form": "근데 옴니버스처럼 따로따로인 이야기가 아니라",
          "speaker_id": "1",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100013105.1.32",
          "form": "다 연결되는 이야기임",
          "original_form": "다 연결되는 이야기임",
          "speaker_id": "1",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100013105.1.33",
          "form": "ㅎㅎ...한 5명까지 읽다 포기",
          "original_form": "ㅎㅎ...한 5명까지 읽다 포기",
          "speaker_id": "1",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100013105.1.34",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 15:20"
        }
      ]
    }
