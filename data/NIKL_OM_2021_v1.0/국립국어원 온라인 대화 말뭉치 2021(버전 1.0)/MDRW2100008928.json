      "utterance": [
        {
          "id": "MDRW2100008928.1.1",
          "form": "ㅋㅋㅋㅋ 비슷하게들 먹고사는가봐요",
          "original_form": "ㅋㅋㅋㅋ 비슷하게들 먹고사는가봐요",
          "speaker_id": "2",
          "time": "20210822 19:43"
        },
        {
          "id": "MDRW2100008928.1.2",
          "form": "그쵸 아 전 사실 트레이너님께도 그러고있어요",
          "original_form": "그쵸 아 전 사실 트레이너님께도 그러고있어요",
          "speaker_id": "1",
          "time": "20210822 19:43"
        },
        {
          "id": "MDRW2100008928.1.3",
          "form": "앗 트레이너님 제2의 양육자설",
          "original_form": "앗 트레이너님 제2의 양육자설",
          "speaker_id": "2",
          "time": "20210822 19:43"
        },
        {
          "id": "MDRW2100008928.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:44"
        },
        {
          "id": "MDRW2100008928.1.5",
          "form": "건강식단위주로 보내시나요",
          "original_form": "건강식단위주로 보내시나요",
          "speaker_id": "2",
          "time": "20210822 19:44"
        },
        {
          "id": "MDRW2100008928.1.6",
          "form": "트레이너님 죄송합니다...몰래먹은 맥주가 1박스는 될겁니다..",
          "original_form": "트레이너님 죄송합니다...몰래먹은 맥주가 1박스는 될겁니다..",
          "speaker_id": "1",
          "time": "20210822 19:44"
        },
        {
          "id": "MDRW2100008928.1.7",
          "form": "ㅋㅋ맥주 좋아하세요?",
          "original_form": "ㅋㅋ맥주 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100008928.1.8",
          "form": "네 저녁에 한잔씩 하는거 참지 못하겠어요ㅋㅋ",
          "original_form": "네 저녁에 한잔씩 하는거 참지 못하겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:46"
        },
        {
          "id": "MDRW2100008928.1.9",
          "form": "여름엔 특히 그렇죠",
          "original_form": "여름엔 특히 그렇죠",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008928.1.10",
          "form": "반주하는데 술은 빼고 찍어보내죠",
          "original_form": "반주하는데 술은 빼고 찍어보내죠",
          "speaker_id": "1",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008928.1.11",
          "form": "거짓말은 아니고 우연히 생략되는군요",
          "original_form": "거짓말은 아니고 우연히 생략되는군요",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008928.1.12",
          "form": "사진앵글상 희한하게 캔이 사라지는",
          "original_form": "사진앵글상 희한하게 캔이 사라지는",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008928.1.13",
          "form": "오 맞아요~~~~",
          "original_form": "오 맞아요~~~~",
          "speaker_id": "1",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008928.1.14",
          "form": "그렇죠~~~",
          "original_form": "그렇죠~~~",
          "speaker_id": "1",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008928.1.15",
          "form": "정확하세요",
          "original_form": "정확하세요",
          "speaker_id": "1",
          "time": "20210822 19:48"
        },
        {
          "id": "MDRW2100008928.1.16",
          "form": "ㅋㅋㅋㅋ 음식사진의 묘미죠",
          "original_form": "ㅋㅋㅋㅋ 음식사진의 묘미죠",
          "speaker_id": "2",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008928.1.17",
          "form": "채소는 가깝게 기름진친구는 작게 술은 가려지게..",
          "original_form": "채소는 가깝게 기름진친구는 작게 술은 가려지게..",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008928.1.18",
          "form": "지난번에 에쎈에스에서 보고 딸기보드카 샀는데 맛있더라구요",
          "original_form": "지난번에 에쎈에스에서 보고 딸기보드카 샀는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008928.1.19",
          "form": "ㅋㅋㅋ와 딸기보드카",
          "original_form": "ㅋㅋㅋ와 딸기보드카",
          "speaker_id": "1",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008928.1.20",
          "form": "딸기즙맛이나나요",
          "original_form": "딸기즙맛이나나요",
          "speaker_id": "1",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008928.1.21",
          "form": "네 진짜 딸기가 50% 들어있어서",
          "original_form": "네 진짜 딸기가 50% 들어있어서",
          "speaker_id": "2",
          "time": "20210822 19:52"
        },
        {
          "id": "MDRW2100008928.1.22",
          "form": "딸기맛이 나요! 냉장보관 해야해요",
          "original_form": "딸기맛이 나요! 냉장보관 해야해요",
          "speaker_id": "2",
          "time": "20210822 19:52"
        },
        {
          "id": "MDRW2100008928.1.23",
          "form": "오 그렇겠네요 딸기가 상하니까요",
