      "utterance": [
        {
          "id": "MDRW2100027611.1.1",
          "form": "옥수수케이크 먹고싶다",
          "original_form": "옥수수케이크 먹고싶다",
          "speaker_id": "2",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027611.1.2",
          "form": "옥수수케익은 고소하고 달콤해",
          "original_form": "옥수수케익은 고소하고 달콤해",
          "speaker_id": "1",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027611.1.3",
          "form": "고구마보다 달콤하고 고소해",
          "original_form": "고구마보다 달콤하고 고소해",
          "speaker_id": "2",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027611.1.4",
          "form": "노란색이라 더 맛있게 보여",
          "original_form": "노란색이라 더 맛있게 보여",
          "speaker_id": "1",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027611.1.5",
          "form": "파는곳이 있을까",
          "original_form": "파는곳이 있을까",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027611.1.6",
          "form": "한국에 있을거야 맛집",
          "original_form": "한국에 있을거야 맛집",
          "speaker_id": "1",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027611.1.7",
          "form": "레시피가 궁금해",
          "original_form": "레시피가 궁금해",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027611.1.8",
          "form": "한국은 당근케익이 좋더라",
          "original_form": "한국은 당근케익이 좋더라",
          "speaker_id": "1",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027611.1.9",
          "form": "아이디어가 참신해",
          "original_form": "아이디어가 참신해",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027611.1.10",
          "form": "레시피 누가 만들었을까",
          "original_form": "레시피 누가 만들었을까",
          "speaker_id": "1",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027611.1.11",
          "form": "촉촉하고 부드러위",
          "original_form": "촉촉하고 부드러위",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027611.1.12",
          "form": "당근은 안좋아하는데",
          "original_form": "당근은 안좋아하는데",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027611.1.13",
          "form": "케이크는 너무 신선해",
          "original_form": "케이크는 너무 신선해",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027611.1.14",
          "form": "떡케이크도 선물로 좋아",
          "original_form": "떡케이크도 선물로 좋아",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027611.1.15",
          "form": "가끔 케이크 사먹고싶어",
          "original_form": "가끔 케이크 사먹고싶어",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027611.1.16",
