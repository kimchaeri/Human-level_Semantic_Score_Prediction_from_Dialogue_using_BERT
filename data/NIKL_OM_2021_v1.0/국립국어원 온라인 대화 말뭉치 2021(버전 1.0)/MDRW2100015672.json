      "utterance": [
        {
          "id": "MDRW2100015672.1.1",
          "form": "내일 점심 메뉴 정하셧나요?",
          "original_form": "내일 점심 메뉴 정하셧나요?",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015672.1.2",
          "form": "내일 점심은 아마도 그냥 집에서 집밥먹을거같아요!",
          "original_form": "내일 점심은 아마도 그냥 집에서 집밥먹을거같아요!",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015672.1.3",
          "form": "저는 즉석 떡볶이 먹기로 했어요 ㅎㅎ",
          "original_form": "저는 즉석 떡볶이 먹기로 했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:22"
        },
        {
          "id": "MDRW2100015672.1.4",
          "form": "오 즉석 떡볶이 좋네요!ㅎㅎ",
          "original_form": "오 즉석 떡볶이 좋네요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100015672.1.5",
          "form": "요즘에 편의점에서 그런거 잘 나오더라구요!",
          "original_form": "요즘에 편의점에서 그런거 잘 나오더라구요!",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100015672.1.6",
          "form": "헉 편의점에서도 즉석떡볶이가 나오나요??",
          "original_form": "헉 편의점에서도 즉석떡볶이가 나오나요??",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100015672.1.7",
          "form": "네 편의점에서도 나오는거같아요!",
          "original_form": "네 편의점에서도 나오는거같아요!",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100015672.1.8",
          "form": "편의점에 맛있는거 많던데요!?ㅋㅋㅋ",
          "original_form": "편의점에 맛있는거 많던데요!?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100015672.1.9",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100015672.1.10",
          "form": "편의점 음식 안먹은지 너무 오래됐네용",
          "original_form": "편의점 음식 안먹은지 너무 오래됐네용",
          "speaker_id": "2",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100015672.1.11",
          "form": "저는 좀 자주 먹었던거같아요!",
          "original_form": "저는 좀 자주 먹었던거같아요!",
          "speaker_id": "1",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100015672.1.12",
          "form": "근데 떡볶이 좋아하시나보네요?",
          "original_form": "근데 떡볶이 좋아하시나보네요?",
          "speaker_id": "1",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100015672.1.13",
          "form": "넵 저는 사실 모든 음식을 가리지않고 먹어요 ㅎㅎ",
          "original_form": "넵 저는 사실 모든 음식을 가리지않고 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100015672.1.14",
          "form": "아하 그러시군요!",
          "original_form": "아하 그러시군요!",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100015672.1.15",
          "form": "저도 그런거같아요!",
          "original_form": "저도 그런거같아요!",
          "speaker_id": "1",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100015672.1.16",
          "form": "피자좋아하세요?",
          "original_form": "피자좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100015672.1.17",
          "form": "헉 오늘 떡볶이랑 피자먹고왔어요 ㅋㅋㅋ",
          "original_form": "헉 오늘 떡볶이랑 피자먹고왔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100015672.1.18",
          "form": "우와!ㅋㅋㅋ저 피자 엄청 좋아해요!",
          "original_form": "우와!ㅋㅋㅋ저 피자 엄청 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100015672.1.19",
          "form": "어디피자를 젤 좋아하시나요??",
          "original_form": "어디피자를 젤 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100015672.1.20",
          "form": "피자는 그냥 다 좋아하는거같아요!ㅋㅋㅋ",
          "original_form": "피자는 그냥 다 좋아하는거같아요!ㅋㅋㅋ",
