      "utterance": [
        {
          "id": "MDRW2100033011.1.1",
          "form": "집콕 어떻게 보내시나요",
          "original_form": "집콕 어떻게 보내시나요",
          "speaker_id": "1",
          "time": "20210906 13:45"
        },
        {
          "id": "MDRW2100033011.1.2",
          "form": "주제가 참다양해졌네요!",
          "original_form": "주제가 참다양해졌네요!",
          "speaker_id": "2",
          "time": "20210906 13:46"
        },
        {
          "id": "MDRW2100033011.1.3",
          "form": "넷플이나 독서가 전부에요",
          "original_form": "넷플이나 독서가 전부에요",
          "speaker_id": "2",
          "time": "20210906 13:47"
        },
        {
          "id": "MDRW2100033011.1.4",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:48"
        },
        {
          "id": "MDRW2100033011.1.5",
          "form": "요즘 전 이사온지 얼마안되서",
          "original_form": "요즘 전 이사온지 얼마안되서",
          "speaker_id": "1",
          "time": "20210906 13:48"
        },
        {
          "id": "MDRW2100033011.1.6",
          "form": "어린 자녀가 있으면 많은 아이디어가 필요하겠죠",
          "original_form": "어린 자녀가 있으면 많은 아이디어가 필요하겠죠",
          "speaker_id": "2",
          "time": "20210906 13:48"
        },
        {
          "id": "MDRW2100033011.1.7",
          "form": "포스터 같은거 조명 집꾸미는데 관심많아요",
          "original_form": "포스터 같은거 조명 집꾸미는데 관심많아요",
          "speaker_id": "1",
          "time": "20210906 13:48"
        },
        {
          "id": "MDRW2100033011.1.8",
          "form": "애기들은 넘 안타까워요",
          "original_form": "애기들은 넘 안타까워요",
          "speaker_id": "1",
          "time": "20210906 13:49"
        },
        {
          "id": "MDRW2100033011.1.9",
          "form": "맞아요 오늘의집 구경하면 재밌어요",
          "original_form": "맞아요 오늘의집 구경하면 재밌어요",
          "speaker_id": "2",
          "time": "20210906 13:49"
        },
        {
          "id": "MDRW2100033011.1.10",
          "form": "그러다 오늘의 지름으로",
          "original_form": "그러다 오늘의 지름으로",
          "speaker_id": "1",
          "time": "20210906 13:49"
        },
        {
          "id": "MDRW2100033011.1.11",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:49"
        },
        {
          "id": "MDRW2100033011.1.12",
          "form": "집이젤 안전하니 아늑하게!",
          "original_form": "집이젤 안전하니 아늑하게!",
          "speaker_id": "2",
          "time": "20210906 13:49"
        },
        {
          "id": "MDRW2100033011.1.13",
          "form": "홈루덴스는 처음듣네요",
          "original_form": "홈루덴스는 처음듣네요",
          "speaker_id": "2",
          "time": "20210906 13:50"
        },
        {
          "id": "MDRW2100033011.1.14",
          "form": "트렌드 따라가기 힘듭니다ㅋ",
          "original_form": "트렌드 따라가기 힘듭니다ㅋ",
          "speaker_id": "2",
          "time": "20210906 13:50"
        },
        {
          "id": "MDRW2100033011.1.15",
          "form": "밀레니얼세대가 집에서 놀고먹고 다 하는 경우가 많대료",
          "original_form": "밀레니얼세대가 집에서 놀고먹고 다 하는 경우가 많대료",
          "speaker_id": "1",
          "time": "20210906 13:50"
        },
        {
          "id": "MDRW2100033011.1.16",
          "form": "따라갈필요있나요",
          "original_form": "따라갈필요있나요",
          "speaker_id": "1",
          "time": "20210906 13:50"
        },
        {
          "id": "MDRW2100033011.1.17",
          "form": "그만큼 힘든시기에요 버티는것만해도 중박ㅜㅜㄱ",
          "original_form": "그만큼 힘든시기에요 버티는것만해도 중박ㅜㅜㄱ",
          "speaker_id": "2",
          "time": "20210906 13:51"
        },
        {
          "id": "MDRW2100033011.1.18",
          "form": "보석십자수 하는 친구도 있구",
          "original_form": "보석십자수 하는 친구도 있구",
          "speaker_id": "1",
          "time": "20210906 13:51"
        },
        {
          "id": "MDRW2100033011.1.19",
          "form": "저는 반신욕 좋아하는데 욕조가 없네요 ㅠ",
          "original_form": "저는 반신욕 좋아하는데 욕조가 없네요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 13:52"
        },
        {
          "id": "MDRW2100033011.1.20",
          "form": "색칠하는 친구도 있구요ㅎ 취미도 다 비용이고 공간이 필요해요",
          "original_form": "색칠하는 친구도 있구요ㅎ 취미도 다 비용이고 공간이 필요해요",
          "speaker_id": "2",
          "time": "20210906 13:52"
        },
        {
          "id": "MDRW2100033011.1.21",
          "form": "저는 뜨게질 해보고싶어요",
          "original_form": "저는 뜨게질 해보고싶어요",
          "speaker_id": "2",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100033011.1.22",
          "form": "전 손으로 하는건 어렵더라구요",
          "original_form": "전 손으로 하는건 어렵더라구요",
          "speaker_id": "1",
          "time": "20210906 13:55"
        },
        {
          "id": "MDRW2100033011.1.23",
          "form": "어머니도 제 방 책 신경도 안쓰셨는데",
          "original_form": "어머니도 제 방 책 신경도 안쓰셨는데",
          "speaker_id": "1",
          "time": "20210906 13:55"
        },
        {
          "id": "MDRW2100033011.1.24",
          "form": "코로나로 집에.오래계시니까 이번에 내려가니까 책정리하라고 ㅠ",
          "original_form": "코로나로 집에.오래계시니까 이번에 내려가니까 책정리하라고 ㅠ",
          "speaker_id": "1",
          "time": "20210906 13:55"
        }
      ]
    }
  ]
