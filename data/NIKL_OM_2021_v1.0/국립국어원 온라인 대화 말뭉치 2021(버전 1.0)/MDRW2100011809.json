      "utterance": [
        {
          "id": "MDRW2100011809.1.1",
          "form": "ㅋㅋ좀비는 죽이면되니까 할만하죠",
          "original_form": "ㅋㅋ좀비는 죽이면되니까 할만하죠",
          "speaker_id": "2",
          "time": "20210824 16:22"
        },
        {
          "id": "MDRW2100011809.1.2",
          "form": "넷플에 킹덤3나왔는데 아직 못봫어요ㅜ",
          "original_form": "넷플에 킹덤3나왔는데 아직 못봫어요ㅜ",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011809.1.3",
          "form": "킹덤2까지는 재밌게봤는데",
          "original_form": "킹덤2까지는 재밌게봤는데",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011809.1.4",
          "form": "텀이 길어지니까",
          "original_form": "텀이 길어지니까",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011809.1.5",
          "form": "저는 살짝 흥미를 잃었네요ㅋㅋ",
          "original_form": "저는 살짝 흥미를 잃었네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011809.1.6",
          "form": "넷플릭스나 유튜브로 짧게보는게 요즘은 젤 무난한 휴식인거같아요ㅋㅋ",
          "original_form": "넷플릭스나 유튜브로 짧게보는게 요즘은 젤 무난한 휴식인거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011809.1.7",
          "form": "얼른 퇴근하고 집에서 보고싶네요ㅠㅠ",
          "original_form": "얼른 퇴근하고 집에서 보고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011809.1.8",
          "form": "아 그렇죠 근데 뒷시즌 나오면 저는 첨부터 다시봐요 까먹거든요 ㅎㅎ",
          "original_form": "아 그렇죠 근데 뒷시즌 나오면 저는 첨부터 다시봐요 까먹거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011809.1.9",
          "form": "ㅋㅋㅋ저두요",
          "original_form": "ㅋㅋㅋ저두요",
          "speaker_id": "2",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100011809.1.10",
          "form": "정주행 해야져",
          "original_form": "정주행 해야져",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011809.1.11",
          "form": "넷플말고 왓챠보고있어서",
          "original_form": "넷플말고 왓챠보고있어서",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011809.1.12",
          "form": "3을 못보고있어요 ㅋㅋ",
          "original_form": "3을 못보고있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011809.1.13",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011809.1.14",
          "form": "ㅋㅋㅋ저 나쁜말한거없는디",
          "original_form": "ㅋㅋㅋ저 나쁜말한거없는디",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011809.1.15",
          "form": "힝구",
          "original_form": "힝구",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011809.1.16",
          "form": "왓챠도 넷플처럼 자체제작드라마 있나요?",
          "original_form": "왓챠도 넷플처럼 자체제작드라마 있나요?",
          "speaker_id": "2",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011809.1.17",
          "form": "저도 별얘기없는데 걸러지는거 많아요",
          "original_form": "저도 별얘기없는데 걸러지는거 많아요",
          "speaker_id": "1",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011809.1.18",
          "form": "체르노빌은 왓챠 오리지널 같던데요",
          "original_form": "체르노빌은 왓챠 오리지널 같던데요",
          "speaker_id": "1",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011809.1.19",
          "form": "영화 자주보는분들은 둘다 번갈아가면서 써야겠어요ㅎㅎ",
          "original_form": "영화 자주보는분들은 둘다 번갈아가면서 써야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011809.1.20",
          "form": "ㅋㅋ플랫폼이 늘어나요ㅜ",
          "original_form": "ㅋㅋ플랫폼이 늘어나요ㅜ",
          "speaker_id": "1",
          "time": "20210824 17:35"
        },
        {
          "id": "MDRW2100011809.1.21",
          "form": "디즈니도 이제 저런 서비스 운영한다고 하던데",
          "original_form": "디즈니도 이제 저런 서비스 운영한다고 하던데",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011809.1.22",
          "form": "다들 난리에요ㅋㅋ",
          "original_form": "다들 난리에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011809.1.23",
          "form": "티빙 아마존프라임 등등 ㅋㅋ",
          "original_form": "티빙 아마존프라임 등등 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011809.1.24",
          "form": "퇴근길 즐겁게 신곡 들으며 갑니다~",
          "original_form": "퇴근길 즐겁게 신곡 들으며 갑니다~",
          "speaker_id": "2",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011809.1.25",
          "form": "오 퇴근길ㅜㅜ저두 ㅜㅜ부럽당 !",
          "original_form": "오 퇴근길ㅜㅜ저두 ㅜㅜ부럽당 !",
          "speaker_id": "1",
          "time": "20210824 18:03"
        }
      ]
    }
