      "utterance": [
        {
          "id": "MDRW2100041433.1.1",
          "form": "메리골드라는 차를 살까 해요",
          "original_form": "메리골드라는 차를 살까 해요",
          "speaker_id": "2",
          "time": "20210905 23:33"
        },
        {
          "id": "MDRW2100041433.1.2",
          "form": "시력에 좋다고 하더라고요",
          "original_form": "시력에 좋다고 하더라고요",
          "speaker_id": "2",
          "time": "20210905 23:33"
        },
        {
          "id": "MDRW2100041433.1.3",
          "form": "눈에 좋은 음식은 당근 당근 당근 당근 당근 당",
          "original_form": "눈에 좋은 음식은 당근 당근 당근 당근 당근 당",
          "speaker_id": "1",
          "time": "20210905 23:33"
        },
        {
          "id": "MDRW2100041433.1.4",
          "form": "당근 싫어",
          "original_form": "당근 싫어",
          "speaker_id": "2",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.5",
          "form": "메리골드도 좋고 결명자 짱이에요",
          "original_form": "메리골드도 좋고 결명자 짱이에요",
          "speaker_id": "1",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.6",
          "form": "그러신 것 같아서 당근 당근 당근",
          "original_form": "그러신 것 같아서 당근 당근 당근",
          "speaker_id": "1",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.7",
          "form": "결명자는 많이 먹어 봤어요 어렸을 때 많이 먹어 봤어요",
          "original_form": "결명자는 많이 먹어 봤어요 어렸을 때 많이 먹어 봤어요",
          "speaker_id": "2",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.8",
          "form": "당근 케이크는 맛있죠",
          "original_form": "당근 케이크는 맛있죠",
          "speaker_id": "2",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.9",
          "form": "결명자 맛이 참 묘해요",
          "original_form": "결명자 맛이 참 묘해요",
          "speaker_id": "1",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.10",
          "form": "저 편의점에서 당근케이크 먹어봤는데요",
          "original_form": "저 편의점에서 당근케이크 먹어봤는데요",
          "speaker_id": "1",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.11",
          "form": "케이크로 먹을까 봐용",
          "original_form": "케이크로 먹을까 봐용",
          "speaker_id": "2",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.12",
          "form": "진짜 맛있었어요",
          "original_form": "진짜 맛있었어요",
          "speaker_id": "1",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.13",
          "form": "편의점에서 당근 케이크도 파나요 처음 들었네요 정보 감사합니다",
          "original_form": "편의점에서 당근 케이크도 파나요 처음 들었네요 정보 감사합니다",
          "speaker_id": "2",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.14",
          "form": "저희 언니네 당근케이크가 안 팔려서 결국 저한테 왔거든요",
          "original_form": "저희 언니네 당근케이크가 안 팔려서 결국 저한테 왔거든요",
          "speaker_id": "1",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.15",
          "form": "언진짜 맛있게 먹었어요",
          "original_form": "언진짜 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.16",
          "form": "이마트 24에서 파는 건가요",
          "original_form": "이마트 24에서 파는 건가요",
          "speaker_id": "2",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041433.1.17",
          "form": "꼭 가봐야 되겠네요",
          "original_form": "꼭 가봐야 되겠네요",
          "speaker_id": "2",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100041433.1.18",
          "form": "이마트 24에서 샀는데 어 저는 너무 좋았었어요 당근 맛은 그렇게 많지 않았고",
          "original_form": "이마트 24에서 샀는데 어 저는 너무 좋았었어요 당근 맛은 그렇게 많지 않았고",
          "speaker_id": "1",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100041433.1.19",
          "form": "맛이 정확하게 기억이 나진 않는데 진짜 맛있었던 기억이 나요",
          "original_form": "맛이 정확하게 기억이 나진 않는데 진짜 맛있었던 기억이 나요",
          "speaker_id": "1",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100041433.1.20",
          "form": "혹시 집에 샌드위치 기계 있으신가요",
          "original_form": "혹시 집에 샌드위치 기계 있으신가요",
          "speaker_id": "2",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100041433.1.21",
          "form": "살까 말까 고민 중입니다",
          "original_form": "살까 말까 고민 중입니다",
          "speaker_id": "2",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100041433.1.22",
          "form": "샌드위치 기계는 없고 와플 펜",
          "original_form": "샌드위치 기계는 없고 와플 펜",
          "speaker_id": "1",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100041433.1.23",
          "form": "토스트 팬은 있어요",
          "original_form": "토스트 팬은 있어요",
          "speaker_id": "1",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100041433.1.24",
          "form": "근데 정말 안 씁니다",
          "original_form": "근데 정말 안 씁니다",
          "speaker_id": "1",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100041433.1.25",
          "form": "회사에서 인센티브로 받았었어요",
          "original_form": "회사에서 인센티브로 받았었어요",
          "speaker_id": "1",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100041433.1.26",
          "form": "요즘에는 와플 팬에 그 판만 갈면 샌드위치 기계가 되고 급한 만 갈면 붕어빵 기기가 되고 그릴 기계가 되고 그렇던데요",
          "original_form": "요즘에는 와플 팬에 그 판만 갈면 샌드위치 기계가 되고 급한 만 갈면 붕어빵 기기가 되고 그릴 기계가 되고 그렇던데요",
          "speaker_id": "2",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100041433.1.27",
          "form": "생각처럼 그렇지 않구요 판에 들러붙어서 들러붙어서 씻을 때 진짜 힘들어요",
          "original_form": "생각처럼 그렇지 않구요 판에 들러붙어서 들러붙어서 씻을 때 진짜 힘들어요",
          "speaker_id": "1",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100041433.1.28",
          "form": "오일을 발라도 생각처럼 그렇게 깔끔하게 나오지 않아요",
          "original_form": "오일을 발라도 생각처럼 그렇게 깔끔하게 나오지 않아요",
          "speaker_id": "1",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100041433.1.29",
          "form": "혹시 안 좋은 기계라서 그런 건 아닐까요",
          "original_form": "혹시 안 좋은 기계라서 그런 건 아닐까요",
          "speaker_id": "2",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100041433.1.30",
          "form": "오일을 잔뜩 써라고는 하던데",
          "original_form": "오일을 잔뜩 써라고는 하던데",
          "speaker_id": "1",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100041433.1.31",
          "form": "그걸로 뭐 해서 드셔 보셨나요",
          "original_form": "그걸로 뭐 해서 드셔 보셨나요",
          "speaker_id": "2",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100041433.1.32",
          "form": "떡도 구워서 먹어봤구요",
          "original_form": "떡도 구워서 먹어봤구요",
          "speaker_id": "1",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100041433.1.33",
          "form": "근데 떡이 들러붙더라구요",
          "original_form": "근데 떡이 들러붙더라구요",
          "speaker_id": "1",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100041433.1.34",
          "form": "떡은 엄청 들러붙을 것만 같네요",
          "original_form": "떡은 엄청 들러붙을 것만 같네요",
          "speaker_id": "2",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100041433.1.35",
          "form": "그리고 그 와플 펜으로 식빵을 구워 먹었어요",
          "original_form": "그리고 그 와플 펜으로 식빵을 구워 먹었어요",
          "speaker_id": "1",
          "time": "20210905 23:37"
        },
        {
          "id": "MDRW2100041433.1.36",
          "form": "확실히 모양이 이쁘더라구요",
          "original_form": "확실히 모양이 이쁘더라구요",
