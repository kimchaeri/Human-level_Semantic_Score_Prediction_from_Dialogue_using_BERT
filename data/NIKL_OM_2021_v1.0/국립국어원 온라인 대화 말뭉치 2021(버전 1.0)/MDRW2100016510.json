      "utterance": [
        {
          "id": "MDRW2100016510.1.1",
          "form": "오늘도 여유로운 시간대이신가요?ㅎㅎ",
          "original_form": "오늘도 여유로운 시간대이신가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 10:14"
        },
        {
          "id": "MDRW2100016510.1.2",
          "form": "ㅎㅎ 아침부터 달려서",
          "original_form": "ㅎㅎ 아침부터 달려서",
          "speaker_id": "1",
          "time": "20210827 10:16"
        },
        {
          "id": "MDRW2100016510.1.3",
          "form": "좀 피곤하네요 ㅎㅎ",
          "original_form": "좀 피곤하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:16"
        },
        {
          "id": "MDRW2100016510.1.4",
          "form": "지금 뭐하시나요?",
          "original_form": "지금 뭐하시나요?",
          "speaker_id": "1",
          "time": "20210827 10:16"
        },
        {
          "id": "MDRW2100016510.1.5",
          "form": "일하세요?",
          "original_form": "일하세요?",
          "speaker_id": "1",
          "time": "20210827 10:16"
        },
        {
          "id": "MDRW2100016510.1.6",
          "form": "오늘은 공모주를 쉬었더니 시간이 아침에 여유있네요 ㅎㅎ",
          "original_form": "오늘은 공모주를 쉬었더니 시간이 아침에 여유있네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:17"
        },
        {
          "id": "MDRW2100016510.1.7",
          "form": "오늘 청약했으면 팔고 하느라 정신 없었을텐데 ㅋㅋ",
          "original_form": "오늘 청약했으면 팔고 하느라 정신 없었을텐데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:17"
        },
        {
          "id": "MDRW2100016510.1.8",
          "form": "알바 쉬는중요",
          "original_form": "알바 쉬는중요",
          "speaker_id": "1",
          "time": "20210827 10:18"
        },
        {
          "id": "MDRW2100016510.1.9",
          "form": "아 맞다!! 알바하셨지ㅎㅎ",
          "original_form": "아 맞다!! 알바하셨지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100016510.1.10",
          "form": "메모도 중요하겠어요",
          "original_form": "메모도 중요하겠어요",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100016510.1.11",
          "form": "아니얘요",
          "original_form": "아니얘요",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100016510.1.12",
          "form": "저도 기억 하나도 못해요 ㅎㅎ",
          "original_form": "저도 기억 하나도 못해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100016510.1.13",
          "form": "오전에 자깐 공모주 팔고 사는건데요 뭐",
          "original_form": "오전에 자깐 공모주 팔고 사는건데요 뭐",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100016510.1.14",
          "form": "지금 뭐하고계셔요?",
          "original_form": "지금 뭐하고계셔요?",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100016510.1.15",
          "form": "네 점심 먹었어요ㅎ",
          "original_form": "네 점심 먹었어요ㅎ",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100016510.1.16",
          "form": "회사에서 오늘은 샌드위치가 나왔네요",
          "original_form": "회사에서 오늘은 샌드위치가 나왔네요",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100016510.1.17",
          "form": "아 샌드위치요 ㅎㅎ",
          "original_form": "아 샌드위치요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100016510.1.18",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100016510.1.19",
          "form": "밥 좋아하시눈분은 좀 ㅋㅋ싫겠어요",
          "original_form": "밥 좋아하시눈분은 좀 ㅋㅋ싫겠어요",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100016510.1.20",
          "form": "그냥 남이 주는건 감사히 잘 먹습니다ㅋㅋ",
          "original_form": "그냥 남이 주는건 감사히 잘 먹습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100016510.1.21",
          "form": "점심이 회사생활 꽃인데 ㅋㅋ",
          "original_form": "점심이 회사생활 꽃인데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100016510.1.22",
          "form": "그렇죠 남이주면 다 맛나요 ㅎㅎ",
          "original_form": "그렇죠 남이주면 다 맛나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100016510.1.23",
          "form": "ㅎㅎ 메뉴가 바뀌는 것만으로도 행복이에요",
          "original_form": "ㅎㅎ 메뉴가 바뀌는 것만으로도 행복이에요",
          "speaker_id": "2",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100016510.1.24",
          "form": "그럼요 항상 회사다닐때 점심메뉴 고르기",
          "original_form": "그럼요 항상 회사다닐때 점심메뉴 고르기",
          "speaker_id": "1",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100016510.1.25",
          "form": "고문이였어요",
          "original_form": "고문이였어요",
          "speaker_id": "1",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100016510.1.26",
          "form": "누가 주고 바꿔 주기까지 하면 일하나 없어지고 좋은거죠",
          "original_form": "누가 주고 바꿔 주기까지 하면 일하나 없어지고 좋은거죠",
          "speaker_id": "1",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100016510.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100016510.1.28",
          "form": "설거지 안하고ㅋㅋㅋㅋ",
          "original_form": "설거지 안하고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100016510.1.29",
          "form": "힘들어도 자기 만족하며 직장 다니려고요",
          "original_form": "힘들어도 자기 만족하며 직장 다니려고요",
          "speaker_id": "2",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100016510.1.30",
          "form": "그럼요 회사 다니는게 최고예요",
          "original_form": "그럼요 회사 다니는게 최고예요",
          "speaker_id": "1",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100016510.1.31",
          "form": "만족한다니 넘 좋네요",
          "original_form": "만족한다니 넘 좋네요",
          "speaker_id": "1",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100016510.1.32",
          "form": "뭐든 불만보다 만족 하는게 좋죠 ㅎㅎ",
          "original_form": "뭐든 불만보다 만족 하는게 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100016510.1.33",
          "form": "설거지 안하는게 최고 좋죠",
          "original_form": "설거지 안하는게 최고 좋죠",
          "speaker_id": "1",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100016510.1.34",
          "form": "회사밥의 장점이 그거네요",
          "original_form": "회사밥의 장점이 그거네요",
          "speaker_id": "1",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100016510.1.35",
          "form": "요새 주말 알바 알아보는데 영없네요",
          "original_form": "요새 주말 알바 알아보는데 영없네요",
          "speaker_id": "1",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100016510.1.36",
          "form": "예전에 토일 홈쇼핑 상담 알바 했는데",
          "original_form": "예전에 토일 홈쇼핑 상담 알바 했는데",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100016510.1.37",
          "form": "너무 기빨려서 관뒀어요 ㅡㅡ",
          "original_form": "너무 기빨려서 관뒀어요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100016510.1.38",
          "form": "거기 저만 알바지",
          "original_form": "거기 저만 알바지",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100016510.1.39",
          "form": "다들 투잡이시더라고요",
          "original_form": "다들 투잡이시더라고요",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100016510.1.40",
          "form": "진짜 대단했어요",
          "original_form": "진짜 대단했어요",
          "speaker_id": "1",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100016510.1.41",
          "form": "와 대단하시다",
          "original_form": "와 대단하시다",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100016510.1.42",
          "form": "존경스럽네요",
          "original_form": "존경스럽네요",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100016510.1.43",
          "form": "그러니까요 다들 생활력이 보 통아니셨어요",
          "original_form": "그러니까요 다들 생활력이 보 통아니셨어요",
          "speaker_id": "1",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100016510.1.44",
          "form": "전 정말 멘탈이 약해서 주말만 하는것도 일찍 끝나는데 좀 다니다 말았네요",
          "original_form": "전 정말 멘탈이 약해서 주말만 하는것도 일찍 끝나는데 좀 다니다 말았네요",
          "speaker_id": "1",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100016510.1.45",
          "form": "회사가 연령대가 다양해서요",
          "original_form": "회사가 연령대가 다양해서요",
