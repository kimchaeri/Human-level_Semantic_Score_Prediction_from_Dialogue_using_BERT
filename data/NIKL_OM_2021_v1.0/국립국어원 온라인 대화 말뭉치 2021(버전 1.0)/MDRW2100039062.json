      "utterance": [
        {
          "id": "MDRW2100039062.1.1",
          "form": "미숫가루인데 존맛이에요",
          "original_form": "미숫가루인데 존맛이에요",
          "speaker_id": "1",
          "time": "20210903 08:40"
        },
        {
          "id": "MDRW2100039062.1.2",
          "form": "미숫가루!",
          "original_form": "미숫가루!",
          "speaker_id": "2",
          "time": "20210903 08:42"
        },
        {
          "id": "MDRW2100039062.1.3",
          "form": "너무 좋은데요",
          "original_form": "너무 좋은데요",
          "speaker_id": "2",
          "time": "20210903 08:42"
        },
        {
          "id": "MDRW2100039062.1.4",
          "form": "전 그거랑 쌍화차도 ㅋㅋㅋ 퀄 좋다생각하는데 이건 호불호가 갈릴거같네요..",
          "original_form": "전 그거랑 쌍화차도 ㅋㅋㅋ 퀄 좋다생각하는데 이건 호불호가 갈릴거같네요..",
          "speaker_id": "1",
          "time": "20210903 08:43"
        },
        {
          "id": "MDRW2100039062.1.5",
          "form": "쌍화차는 한번도 못먹어봤어요",
          "original_form": "쌍화차는 한번도 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210903 08:44"
        },
        {
          "id": "MDRW2100039062.1.6",
          "form": "무슨 맛인가요?",
          "original_form": "무슨 맛인가요?",
          "speaker_id": "2",
          "time": "20210903 08:44"
        },
        {
          "id": "MDRW2100039062.1.7",
          "form": "말린 대추가 꽤 들어잇어요",
          "original_form": "말린 대추가 꽤 들어잇어요",
          "speaker_id": "1",
          "time": "20210903 08:44"
        },
        {
          "id": "MDRW2100039062.1.8",
          "form": "계피맛이 상하고.. 약간 한약향나고.. 맵고 달달한 맛입니다",
          "original_form": "계피맛이 상하고.. 약간 한약향나고.. 맵고 달달한 맛입니다",
          "speaker_id": "1",
          "time": "20210903 08:44"
        },
        {
          "id": "MDRW2100039062.1.9",
          "form": "계피맛 잘견디면 먹을수 있겠네요",
          "original_form": "계피맛 잘견디면 먹을수 있겠네요",
          "speaker_id": "2",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100039062.1.10",
          "form": "저 계피 좋아하서 괜찮을듯요",
          "original_form": "저 계피 좋아하서 괜찮을듯요",
          "speaker_id": "2",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100039062.1.11",
          "form": "몸 으슬으슬하거나 추운날에 한잔하면 몸이 괜히 뜨끈해지는 것 같아요",
          "original_form": "몸 으슬으슬하거나 추운날에 한잔하면 몸이 괜히 뜨끈해지는 것 같아요",
          "speaker_id": "1",
          "time": "20210903 08:47"
        },
        {
          "id": "MDRW2100039062.1.12",
          "form": "이제 그 시즌이 되겠군요",
          "original_form": "이제 그 시즌이 되겠군요",
          "speaker_id": "2",
          "time": "20210903 08:49"
        },
        {
          "id": "MDRW2100039062.1.13",
          "form": "그 시즌에 추천합니다 ㅋㅋㅋ",
          "original_form": "그 시즌에 추천합니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:50"
        },
        {
          "id": "MDRW2100039062.1.14",
          "form": "전 그거랑 화이트초콜ㄹ릿도 맛잇엇어요!",
          "original_form": "전 그거랑 화이트초콜ㄹ릿도 맛잇엇어요!",
          "speaker_id": "1",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100039062.1.15",
          "form": "네ㅋㅋ 그때 나가면 꼭 마셔볼게요",
          "original_form": "네ㅋㅋ 그때 나가면 꼭 마셔볼게요",
          "speaker_id": "2",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100039062.1.16",
          "form": "화이트초콜릿!! 대환장하죠",
          "original_form": "화이트초콜릿!! 대환장하죠",
          "speaker_id": "2",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100039062.1.17",
          "form": "우유맛과 초코의 적절한 조화..",
          "original_form": "우유맛과 초코의 적절한 조화..",
          "speaker_id": "1",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100039062.1.18",
          "form": "아 환장하시나요 저도요 ㅋㅋㅋㅋ",
          "original_form": "아 환장하시나요 저도요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100039062.1.19",
          "form": "일반 초코보다 화이트 더 좋아해요",
          "original_form": "일반 초코보다 화이트 더 좋아해요",
          "speaker_id": "2",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100039062.1.20",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210903 08:52"
        },
        {
          "id": "MDRW2100039062.1.21",
          "form": "우유랑 맛이 자연스레 이어져서 화이트가 더 좋더라구요",
          "original_form": "우유랑 맛이 자연스레 이어져서 화이트가 더 좋더라구요",
          "speaker_id": "1",
          "time": "20210903 08:52"
        }
      ]
