        },
        {
          "id": "MDRW2100032661.1.2",
          "form": "학교생활 재밌엇니",
          "original_form": "학교생활 재밌엇니",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032661.1.3",
          "form": "...노잼이었던것 같음ㅋㅋㅋㅋㅋ",
          "original_form": "...노잼이었던것 같음ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032661.1.4",
          "form": "재밌었니?...",
          "original_form": "재밌었니?...",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032661.1.5",
          "form": "ㅋㅋㅋㅋㅋ대학교때 나름뭐...근데 좀 확 놀껄그랫어",
          "original_form": "ㅋㅋㅋㅋㅋ대학교때 나름뭐...근데 좀 확 놀껄그랫어",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032661.1.6",
          "form": "아 ㅋㅋㅋ고딩때만 생각햇네",
          "original_form": "아 ㅋㅋㅋ고딩때만 생각햇네",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032661.1.7",
          "form": "대학교때...맞다 좀더 확실하게 놀았어야했는데",
          "original_form": "대학교때...맞다 좀더 확실하게 놀았어야했는데",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032661.1.8",
          "form": "너무 이도저도 아니엇어",
          "original_form": "너무 이도저도 아니엇어",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032661.1.9",
          "form": "마자 동아리는 머해봣서",
          "original_form": "마자 동아리는 머해봣서",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032661.1.10",
          "form": "무슨 취업용 동아리 가입했었는데",
          "original_form": "무슨 취업용 동아리 가입했었는데",
          "speaker_id": "2",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032661.1.11",
          "form": "거기서 친해진 사람이 하나도 없어...",
          "original_form": "거기서 친해진 사람이 하나도 없어...",
          "speaker_id": "2",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032661.1.12",
          "form": "앗...얼마나햇어",
          "original_form": "앗...얼마나햇어",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032661.1.13",
          "form": "1년?...잘 모르겟다 ㅋㅋㅋ",
          "original_form": "1년?...잘 모르겟다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032661.1.14",
          "form": "가입만 하고 잘 안나갔었나봄...",
          "original_form": "가입만 하고 잘 안나갔었나봄...",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032661.1.15",
          "form": "넌 동아리 안함?",
          "original_form": "넌 동아리 안함?",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032661.1.16",
          "form": "ㅋㅋㅋㅋㅋ그렇군 나는 안햇서",
          "original_form": "ㅋㅋㅋㅋㅋ그렇군 나는 안햇서",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032661.1.17",
          "form": "먼가 단기 동아리? 프로젝트동아리같응겅함ㅋㅋㅋㅋ",
          "original_form": "먼가 단기 동아리? 프로젝트동아리같응겅함ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032661.1.18",
          "form": "ㅋㅋㅋㅋ프로젝트? 그...",
          "original_form": "ㅋㅋㅋㅋ프로젝트? 그...",
          "speaker_id": "2",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032661.1.19",
          "form": "싱가포?",
          "original_form": "싱가포?",
          "speaker_id": "2",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032661.1.20",
          "form": "중동문화동아맄ㅋㅋㅋㅋㅋㅋ",
          "original_form": "중동문화동아맄ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032661.1.21",
          "form": "????? 갑분중동? ㅋㅋㅋㅋ",
          "original_form": "????? 갑분중동? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032661.1.22",
          "form": "아 그것도 그렇게치면 맞긴하겟더",
          "original_form": "아 그것도 그렇게치면 맞긴하겟더",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032661.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032661.1.24",
          "form": "어쩌다가 중동문화 가입함ㅋㅋㅋㅋ",
          "original_form": "어쩌다가 중동문화 가입함ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032661.1.25",
          "form": "중동아시아지역 어쩌구 문화관련 조사하고 공붘ㅋㅋㅋㅋ",
          "original_form": "중동아시아지역 어쩌구 문화관련 조사하고 공붘ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032661.1.26",
          "form": "ㅋㅋㅋㅋ중동지역 곤부 ㄷㄷ하넼ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ중동지역 곤부 ㄷㄷ하넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032661.1.27",
          "form": "그래도 먼가 지원금 나와서 밥사먹고 ㅋㅋㅋㅋㅋ필기구사고 알찻다",
          "original_form": "그래도 먼가 지원금 나와서 밥사먹고 ㅋㅋㅋㅋㅋ필기구사고 알찻다",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032661.1.28",
          "form": "ㅋㅋㅋ아 지원금 그럼 가야지",
          "original_form": "ㅋㅋㅋ아 지원금 그럼 가야지",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032661.1.29",
          "form": "ㅋㅋㅋㅋ대학교애 그런거 진짜많은데",
          "original_form": "ㅋㅋㅋㅋ대학교애 그런거 진짜많은데",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032661.1.30",
          "form": "알차게 타먹엇어야햇어..",
          "original_form": "알차게 타먹엇어야햇어..",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032661.1.31",
          "form": "그니께...",
          "original_form": "그니께...",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032661.1.32",
          "form": "분명 알차게 타먹고 다니는 사람들 잇엇을거야",
          "original_form": "분명 알차게 타먹고 다니는 사람들 잇엇을거야",
