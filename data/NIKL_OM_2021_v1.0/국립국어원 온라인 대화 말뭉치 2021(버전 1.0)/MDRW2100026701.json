      "utterance": [
        {
          "id": "MDRW2100026701.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026701.1.2",
          "form": "여행 어디가고싶으세요",
          "original_form": "여행 어디가고싶으세요",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026701.1.3",
          "form": "그러게요 원래도 큰 생각 없었지만 요새는 더한 것 같아요 코로나 때문에ㅠㅠ",
          "original_form": "그러게요 원래도 큰 생각 없었지만 요새는 더한 것 같아요 코로나 때문에ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026701.1.4",
          "form": "국내 여행 좀 잘 다녀보고 싶네요",
          "original_form": "국내 여행 좀 잘 다녀보고 싶네요",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026701.1.5",
          "form": "한복 입고!",
          "original_form": "한복 입고!",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026701.1.6",
          "form": "와 ㅋㅋㅋ 한복은 언제나 옳죠!",
          "original_form": "와 ㅋㅋㅋ 한복은 언제나 옳죠!",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026701.1.7",
          "form": "그저 어떤 한복을 입을지가 고민이되는",
          "original_form": "그저 어떤 한복을 입을지가 고민이되는",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026701.1.8",
          "form": "국내여행 어디가고싶으세요?",
          "original_form": "국내여행 어디가고싶으세요?",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026701.1.9",
          "form": "맞아요 한복은 입을 때마다 사고 싶고 다 입고 싶더라구요",
          "original_form": "맞아요 한복은 입을 때마다 사고 싶고 다 입고 싶더라구요",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026701.1.10",
          "form": "어디가 좋을까요 사실 잘 몰라요 ㅋㅋㅋㅋㅋㅋ 바다가 보고 싶어요",
          "original_form": "어디가 좋을까요 사실 잘 몰라요 ㅋㅋㅋㅋㅋㅋ 바다가 보고 싶어요",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026701.1.11",
          "form": "동해를 안 간 지가 오래됐어요...",
          "original_form": "동해를 안 간 지가 오래됐어요...",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026701.1.12",
          "form": "음 요즘 너무 한복이 잘나와서 그냥 쏙 맘에드는거 사시면되죠!",
          "original_form": "음 요즘 너무 한복이 잘나와서 그냥 쏙 맘에드는거 사시면되죠!",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026701.1.13",
          "form": "동해 ㅋㅋㅋㅋ 저도 안간지 오래된거같아요",
          "original_form": "동해 ㅋㅋㅋㅋ 저도 안간지 오래된거같아요",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026701.1.14",
          "form": "맞아요 잘 나오죠 생활한복도 많이 나오구",
          "original_form": "맞아요 잘 나오죠 생활한복도 많이 나오구",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026701.1.15",
          "form": "바다 너무 보고 싶네요 새파란 것두 새까만 것두 좋아하는데",
          "original_form": "바다 너무 보고 싶네요 새파란 것두 새까만 것두 좋아하는데",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026701.1.16",
          "form": "요샌 뭐 멀리 가질 못하니ㅠㅠ",
          "original_form": "요샌 뭐 멀리 가질 못하니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026701.1.17",
          "form": "ㅠㅜ 근처에 바다가없으신곳에 사시나봐요 ㅠㅠㅠ",
          "original_form": "ㅠㅜ 근처에 바다가없으신곳에 사시나봐요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026701.1.18",
          "form": "네ㅠㅠ 아주 멀진 않은데 차 타고 가야 해요ㅠㅠ",
          "original_form": "네ㅠㅠ 아주 멀진 않은데 차 타고 가야 해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026701.1.19",
          "form": "어딜 가든 민폐일 것 같아서 참구...",
          "original_form": "어딜 가든 민폐일 것 같아서 참구...",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026701.1.20",
          "form": "에휴 해외도 가고 싶네요",
          "original_form": "에휴 해외도 가고 싶네요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026701.1.21",
          "form": "아 그래두... 기분전환정도로 바다는 보러가셔요 ㅠㅠ",
          "original_form": "아 그래두... 기분전환정도로 바다는 보러가셔요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026701.1.22",
          "form": "ㅠㅠ그래도 다들 고생하시니까요",
          "original_form": "ㅠㅠ그래도 다들 고생하시니까요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026701.1.23",
          "form": "더 퍼지면 미안하고 그럴 것 같아서",
          "original_form": "더 퍼지면 미안하고 그럴 것 같아서",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026701.1.24",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026701.1.25",
          "form": "그것두 그렇죠 ㅠ",
          "original_form": "그것두 그렇죠 ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026701.1.26",
          "form": "저 빼고 다 여행 가는 느낌",
          "original_form": "저 빼고 다 여행 가는 느낌",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026701.1.27",
          "form": "이에요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이에요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026701.1.28",
          "form": "맞아요..다들 그냥 가요..",
          "original_form": "맞아요..다들 그냥 가요..",
          "speaker_id": "1",
          "time": "20210901 23:53"
        }
      ]
    }
