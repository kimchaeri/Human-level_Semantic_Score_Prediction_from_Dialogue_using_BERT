      "utterance": [
        {
          "id": "MDRW2100043110.1.1",
          "form": "이직계획 있으시죠?",
          "original_form": "이직계획 있으시죠?",
          "speaker_id": "1",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043110.1.2",
          "form": "학업 때문에 퇴사할 계획은 있습니다",
          "original_form": "학업 때문에 퇴사할 계획은 있습니다",
          "speaker_id": "2",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043110.1.3",
          "form": "일과 병행하고 싶으니까 이직 맞네 ㅋㅋㅋ",
          "original_form": "일과 병행하고 싶으니까 이직 맞네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043110.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ퇴사는 언제쯤 하시게요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ퇴사는 언제쯤 하시게요?",
          "speaker_id": "1",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043110.1.5",
          "form": "일이 적성에 안맞으시나요",
          "original_form": "일이 적성에 안맞으시나요",
          "speaker_id": "1",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043110.1.6",
          "form": "적성에 맞다 안 맞다 판단하기는 어려운 것 같아요",
          "original_form": "적성에 맞다 안 맞다 판단하기는 어려운 것 같아요",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043110.1.7",
          "form": "익숙한 곳이라 업무 처리에 어려움은 못 느껴서...",
          "original_form": "익숙한 곳이라 업무 처리에 어려움은 못 느껴서...",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043110.1.8",
          "form": "퇴사는 계약기간 종료되면 자연스럽게 할 것임",
          "original_form": "퇴사는 계약기간 종료되면 자연스럽게 할 것임",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043110.1.9",
          "form": "그럼 연장안하고 쭉가는건가",
          "original_form": "그럼 연장안하고 쭉가는건가",
          "speaker_id": "1",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100043110.1.10",
          "form": "일할때 제일 ㅂ빡치는게 먼가요",
          "original_form": "일할때 제일 ㅂ빡치는게 먼가요",
          "speaker_id": "1",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100043110.1.11",
          "form": "새로운 업무 생겨서 교육 받으러 갑니다...",
          "original_form": "새로운 업무 생겨서 교육 받으러 갑니다...",
          "speaker_id": "2",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043110.1.12",
          "form": "지금요??",
          "original_form": "지금요??",
          "speaker_id": "1",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043110.1.13",
          "form": "아이고야...끝나고오세용",
          "original_form": "아이고야...끝나고오세용",
          "speaker_id": "1",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043110.1.14",
          "form": "끝났습니다...",
          "original_form": "끝났습니다...",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100043110.1.15",
          "form": "엑셀 정말 싫네요 ..",
          "original_form": "엑셀 정말 싫네요 ..",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100043110.1.16",
          "form": "새로운 업무가 추가되었습니다^^",
          "original_form": "새로운 업무가 추가되었습니다^^",
          "speaker_id": "2",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043110.1.17",
          "form": "고생이많아요",
          "original_form": "고생이많아요",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043110.1.18",
          "form": "역시 회사는 죄악이에..",
          "original_form": "역시 회사는 죄악이에..",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043110.1.19",
          "form": "그래도 이력서에 쓸 말 하나 추가되었어요",
          "original_form": "그래도 이력서에 쓸 말 하나 추가되었어요",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043110.1.20",
          "form": "긍정",
          "original_form": "긍정",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043110.1.21",
          "form": "이직준비빡시게하시네요",
          "original_form": "이직준비빡시게하시네요",
          "speaker_id": "1",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043110.1.22",
          "form": "이제 신규선생님 명함 제작 및 업무용 피씨 구매 진행해봅니다...",
          "original_form": "이제 신규선생님 명함 제작 및 업무용 피씨 구매 진행해봅니다...",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043110.1.23",
          "form": "이력이 중구난방이라 걱정이에요",
          "original_form": "이력이 중구난방이라 걱정이에요",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043110.1.24",
          "form": "머하는 사람이죠?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "머하는 사람이죠?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043110.1.25",
          "form": "뭐긴뭐야 대한민국 흔한 사무직이지",
          "original_form": "뭐긴뭐야 대한민국 흔한 사무직이지",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043110.1.26",
          "form": "이름발은 좋은데 하는 업무가 이상해여",
          "original_form": "이름발은 좋은데 하는 업무가 이상해여",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043110.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043110.1.28",
          "form": "그걸 자소서에 잘 풀면 이제 다재다능한사람이 되는거지",
          "original_form": "그걸 자소서에 잘 풀면 이제 다재다능한사람이 되는거지",
          "speaker_id": "1",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100043110.1.29",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210907 16:01"
        },
        {
          "id": "MDRW2100043110.1.30",
          "form": "학업을 통해 급여를 높이고 싶습니다",
          "original_form": "학업을 통해 급여를 높이고 싶습니다",
          "speaker_id": "2",
          "time": "20210907 16:01"
        },
        {
          "id": "MDRW2100043110.1.31",
          "form": "교수님도 말씀하시더라고요??? 얼른 와서 공부하고 급여 높이라고 ㅋㅋㅋㅋㅋ",
          "original_form": "교수님도 말씀하시더라고요??? 얼른 와서 공부하고 급여 높이라고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:01"
        },
        {
          "id": "MDRW2100043110.1.32",
          "form": "멋진걸....",
          "original_form": "멋진걸....",
          "speaker_id": "1",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100043110.1.33",
          "form": "저도 이직으로 스펙을 높여보려고요",
          "original_form": "저도 이직으로 스펙을 높여보려고요",
          "speaker_id": "1",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043110.1.34",
          "form": "어디로 이직 원하시나요",
          "original_form": "어디로 이직 원하시나요",
