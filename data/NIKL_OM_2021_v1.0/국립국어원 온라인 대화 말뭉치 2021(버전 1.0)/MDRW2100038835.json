      "utterance": [
        {
          "id": "MDRW2100038835.1.1",
          "form": "야식 먹을까 해요",
          "original_form": "야식 먹을까 해요",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100038835.1.2",
          "form": "야식 너무 좋죠~",
          "original_form": "야식 너무 좋죠~",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100038835.1.3",
          "form": "어떤 거 좋아하세요?",
          "original_form": "어떤 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100038835.1.4",
          "form": "저 라면 잘 못끓이긴 하는데",
          "original_form": "저 라면 잘 못끓이긴 하는데",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038835.1.5",
          "form": "라면 좋아해요 ㅋ",
          "original_form": "라면 좋아해요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038835.1.6",
          "form": "라면은 사랑이죠ㅋㅋㅋㅋ",
          "original_form": "라면은 사랑이죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038835.1.7",
          "form": "라면의 김치 딱 이네요",
          "original_form": "라면의 김치 딱 이네요",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038835.1.8",
          "form": "우아 너무 좋아요 ㅠㅠㅜ",
          "original_form": "우아 너무 좋아요 ㅠㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038835.1.9",
          "form": "열무김치가 있어서",
          "original_form": "열무김치가 있어서",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038835.1.10",
          "form": "같이먹을려구용!!",
          "original_form": "같이먹을려구용!!",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038835.1.11",
          "form": "열무김치면 뭐라도 맛있죠ㅋㅋㅌ",
          "original_form": "열무김치면 뭐라도 맛있죠ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038835.1.12",
          "form": "배우신 분ㅋㅌ",
          "original_form": "배우신 분ㅋㅌ",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038835.1.13",
          "form": "라면에 계란 넣는거 좋아하세요??",
          "original_form": "라면에 계란 넣는거 좋아하세요??",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038835.1.14",
          "form": "전 안 넣는 거 좋아해요ㅋㅋ",
          "original_form": "전 안 넣는 거 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038835.1.15",
          "form": "라는 그대로의 맛",
          "original_form": "라는 그대로의 맛",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038835.1.16",
          "form": "와 저도 이상하게 계란 넣으면 국물이 탁하드라구요 ㅋㅋ",
          "original_form": "와 저도 이상하게 계란 넣으면 국물이 탁하드라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038835.1.17",
          "form": "배우신 분ㅋㅋㅋㅋ",
          "original_form": "배우신 분ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038835.1.18",
          "form": "국물 탁한 거 저도 엄청 싫어합니다ㅋㅋㅋ",
          "original_form": "국물 탁한 거 저도 엄청 싫어합니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038835.1.19",
          "form": "무슨 라면 좋아하세용???",
          "original_form": "무슨 라면 좋아하세용???",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038835.1.20",
          "form": "라면은 다 좋아합니다ㅋㅋㅋㅋ",
          "original_form": "라면은 다 좋아합니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100038835.1.21",
          "form": "맛있는 라면 너구리",
