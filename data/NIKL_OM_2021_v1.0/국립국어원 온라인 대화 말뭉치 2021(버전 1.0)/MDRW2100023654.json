      "utterance": [
        {
          "id": "MDRW2100023654.1.1",
          "form": "근래에 여행하신 적 있으세요?",
          "original_form": "근래에 여행하신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100023654.1.2",
          "form": "전 2차백신 맞고나면 호캉스정돈 가려구요",
          "original_form": "전 2차백신 맞고나면 호캉스정돈 가려구요",
          "speaker_id": "1",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100023654.1.3",
          "form": "아니용ㅠㅠ 무서워서 집밖에도 잘 안나가려고 하고있어서요ㅠㅠ",
          "original_form": "아니용ㅠㅠ 무서워서 집밖에도 잘 안나가려고 하고있어서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100023654.1.4",
          "form": "하긴 무섭긴하죠",
          "original_form": "하긴 무섭긴하죠",
          "speaker_id": "1",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100023654.1.5",
          "form": "근데 어차피 재택근무하는것도 아니라 지금은 밖에서 밥만 안먹고 걍 나갈땐 나가고 있어요",
          "original_form": "근데 어차피 재택근무하는것도 아니라 지금은 밖에서 밥만 안먹고 걍 나갈땐 나가고 있어요",
          "speaker_id": "1",
          "time": "20210830 16:06"
        },
        {
          "id": "MDRW2100023654.1.6",
          "form": "저는 재택근무해서 더 조심스럽네용ㅠㅠㅋㅋㅋ",
          "original_form": "저는 재택근무해서 더 조심스럽네용ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:07"
        },
        {
          "id": "MDRW2100023654.1.7",
          "form": "호캉스는 어디로 가실거에용?",
          "original_form": "호캉스는 어디로 가실거에용?",
          "speaker_id": "2",
          "time": "20210830 16:07"
        },
        {
          "id": "MDRW2100023654.1.8",
          "form": "도심?",
          "original_form": "도심?",
          "speaker_id": "2",
          "time": "20210830 16:07"
        },
        {
          "id": "MDRW2100023654.1.9",
          "form": "사실 10월 중순에 어차피 부산 출장이 잡혀버렸어요",
          "original_form": "사실 10월 중순에 어차피 부산 출장이 잡혀버렸어요",
          "speaker_id": "1",
          "time": "20210830 16:07"
        },
        {
          "id": "MDRW2100023654.1.10",
          "form": "오 부산!",
          "original_form": "오 부산!",
          "speaker_id": "2",
          "time": "20210830 16:08"
        },
        {
          "id": "MDRW2100023654.1.11",
          "form": "간김에 이틀정도 더 쉬려구요",
          "original_form": "간김에 이틀정도 더 쉬려구요",
          "speaker_id": "1",
          "time": "20210830 16:08"
        },
        {
          "id": "MDRW2100023654.1.12",
          "form": "부산에 바다도 가실건가요?",
          "original_form": "부산에 바다도 가실건가요?",
          "speaker_id": "2",
          "time": "20210830 16:08"
        },
        {
          "id": "MDRW2100023654.1.13",
          "form": "발정도는 담글거에요",
          "original_form": "발정도는 담글거에요",
          "speaker_id": "1",
          "time": "20210830 16:10"
        },
        {
          "id": "MDRW2100023654.1.14",
          "form": "해수욕하기엔 추울것 같고요",
          "original_form": "해수욕하기엔 추울것 같고요",
          "speaker_id": "1",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100023654.1.15",
          "form": "아 아직은 좀 그렇죠?ㅠㅠ",
          "original_form": "아 아직은 좀 그렇죠?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100023654.1.16",
          "form": "부산가시면 밀면 꼭 드시고 오세용ㅎㅎ",
          "original_form": "부산가시면 밀면 꼭 드시고 오세용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100023654.1.17",
          "form": "호텔을 오션뷰로 열심히 고르고 있어요",
          "original_form": "호텔을 오션뷰로 열심히 고르고 있어요",
          "speaker_id": "1",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100023654.1.18",
          "form": "밀면이요? 서울 밀면은 솔찌 그냥 그랬어서...ㅎㅎ",
          "original_form": "밀면이요? 서울 밀면은 솔찌 그냥 그랬어서...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100023654.1.19",
          "form": "전 돼지국밥이랑 낙곱새를 기대하고 있어요",
          "original_form": "전 돼지국밥이랑 낙곱새를 기대하고 있어요",
          "speaker_id": "1",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100023654.1.20",
          "form": "부산 밀면이용!",
          "original_form": "부산 밀면이용!",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100023654.1.21",
          "form": "낙곱새도 맛있죠~ㅎㅎ",
          "original_form": "낙곱새도 맛있죠~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100023654.1.22",
          "form": "맛있는거 잔뜩 먹어야죠",
          "original_form": "맛있는거 잔뜩 먹어야죠",
          "speaker_id": "1",
          "time": "20210830 16:13"
        }
      ]
    }
