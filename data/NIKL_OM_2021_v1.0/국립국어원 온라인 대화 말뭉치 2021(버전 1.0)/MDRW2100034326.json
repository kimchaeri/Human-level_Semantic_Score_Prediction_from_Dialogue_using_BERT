      "utterance": [
        {
          "id": "MDRW2100034326.1.1",
          "form": "쇼핑 자주하세요?",
          "original_form": "쇼핑 자주하세요?",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034326.1.2",
          "form": "필요한 거 있으면 그때그때 사는 편이에요.",
          "original_form": "필요한 거 있으면 그때그때 사는 편이에요.",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034326.1.3",
          "form": "최근에는 뭐 사셨어요?",
          "original_form": "최근에는 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034326.1.4",
          "form": "가장 최근에는 가방 하나 샀어요",
          "original_form": "가장 최근에는 가방 하나 샀어요",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034326.1.5",
          "form": "백팩? 핸드백? 아니면 에코백?ㅋㅋ",
          "original_form": "백팩? 핸드백? 아니면 에코백?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100034326.1.6",
          "form": "핸드백이요",
          "original_form": "핸드백이요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034326.1.7",
          "form": "작은 백인데 비건가죽으로 만들었대요",
          "original_form": "작은 백인데 비건가죽으로 만들었대요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034326.1.8",
          "form": "레몬색이라 포인트로 들기에도 좋고요",
          "original_form": "레몬색이라 포인트로 들기에도 좋고요",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100034326.1.9",
          "form": "실제 가죽이랑 비슷한 질감인가요?",
          "original_form": "실제 가죽이랑 비슷한 질감인가요?",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100034326.1.10",
          "form": "네 오히려 더 부드러워서 저는 좋더라고요",
          "original_form": "네 오히려 더 부드러워서 저는 좋더라고요",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100034326.1.11",
          "form": "제가 가죽백을 딱히 따지진 않아서",
          "original_form": "제가 가죽백을 딱히 따지진 않아서",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100034326.1.12",
          "form": "파인애플 같은 걸로 만들던데. 궁금하더라구요",
          "original_form": "파인애플 같은 걸로 만들던데. 궁금하더라구요",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100034326.1.13",
          "form": "아 진짜요? 어떻게 만드는 줄은 몰랐어요ㅎㅎ",
          "original_form": "아 진짜요? 어떻게 만드는 줄은 몰랐어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100034326.1.14",
          "form": "원료가 몇 가지 있다고 들었는데 파인애플밖에 기억이 안 나네요ㅋㅋ",
          "original_form": "원료가 몇 가지 있다고 들었는데 파인애플밖에 기억이 안 나네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100034326.1.15",
          "form": "어쨌든 비건가죽백 구입 만족해요",
          "original_form": "어쨌든 비건가죽백 구입 만족해요",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100034326.1.16",
          "form": "제 주변에서도 보고 다들 하나씩 사겠다며",
          "original_form": "제 주변에서도 보고 다들 하나씩 사겠다며",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100034326.1.17",
          "form": "가격도 저렴하거든요ㅎㅎ",
          "original_form": "가격도 저렴하거든요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100034326.1.18",
          "form": "저도 궁금하네요 ㅎㅎ",
          "original_form": "저도 궁금하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100034326.1.19",
          "form": "Jwpei라는 브랜드예요",
          "original_form": "Jwpei라는 브랜드예요",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100034326.1.20",
          "form": "관심 있으시면 한번찾아보셔요ㅎㅅㄴ",
          "original_form": "관심 있으시면 한번찾아보셔요ㅎㅅㄴ",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100034326.1.21",
          "form": "찾아봐야겠어요ㅋㅋ",
          "original_form": "찾아봐야겠어요ㅋㅋ",
