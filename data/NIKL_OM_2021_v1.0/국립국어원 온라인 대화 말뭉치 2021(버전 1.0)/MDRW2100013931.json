      "utterance": [
        {
          "id": "MDRW2100013931.1.1",
          "form": "제일 좋아하는 음료 있나요",
          "original_form": "제일 좋아하는 음료 있나요",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013931.1.2",
          "form": "우유 좋아해서 자주 마셔요~ 웰치스도 좋아하구요",
          "original_form": "우유 좋아해서 자주 마셔요~ 웰치스도 좋아하구요",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013931.1.3",
          "form": "오 저도요. 건강 때문에 저지방 우유마셔요",
          "original_form": "오 저도요. 건강 때문에 저지방 우유마셔요",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013931.1.4",
          "form": "웰치스는 포도맛...? 이겟죠...?",
          "original_form": "웰치스는 포도맛...? 이겟죠...?",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013931.1.5",
          "form": "건강 중요하죠 ㅎㅎ 네 포도맛이요!",
          "original_form": "건강 중요하죠 ㅎㅎ 네 포도맛이요!",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013931.1.6",
          "form": "name1은 뭐 좋아하세요?",
          "original_form": "&name1&은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013931.1.7",
          "form": "pc방가서 웰치스 마실때가 좋았는데... 저는 코코아랑 데자와요",
          "original_form": "pc방가서 웰치스 마실때가 좋았는데... 저는 코코아랑 데자와요",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013931.1.8",
          "form": "데자와 아세요?",
          "original_form": "데자와 아세요?",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013931.1.9",
          "form": "아 네네 ㅋㅋ 마셔본적은 없지만",
          "original_form": "아 네네 ㅋㅋ 마셔본적은 없지만",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013931.1.10",
          "form": "워낙 호불호 갈리기로 유명한 음료죠 ㅋㅋ",
          "original_form": "워낙 호불호 갈리기로 유명한 음료죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013931.1.11",
          "form": "프랜차이즈 카페 뭐 좋아하세요",
          "original_form": "프랜차이즈 카페 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013931.1.12",
          "form": "저는 이디야요~ 가격이 싸서 자주 가요",
          "original_form": "저는 이디야요~ 가격이 싸서 자주 가요",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013931.1.13",
          "form": "이디야에 토피넛 라떼 좋아하는데... 겨울에 최겁니다",
          "original_form": "이디야에 토피넛 라떼 좋아하는데... 겨울에 최겁니다",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013931.1.14",
          "form": "아 맞아요 ㅋㅋ 달고 맛있어요 저는 플랫치노류도 다 좋아해요",
          "original_form": "아 맞아요 ㅋㅋ 달고 맛있어요 저는 플랫치노류도 다 좋아해요",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013931.1.15",
          "form": "쿠앤크쉐이크인가 그것도 맛나던데요",
          "original_form": "쿠앤크쉐이크인가 그것도 맛나던데요",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013931.1.16",
          "form": "플랫치노도 좋고 ㅋㅋㅋ",
          "original_form": "플랫치노도 좋고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013931.1.17",
          "form": "네 그것도 맛있죠 ㅋㅋㅋ name1은 어디 자주가세요?",
          "original_form": "네 그것도 맛있죠 ㅋㅋㅋ &name1&은 어디 자주가세요?",
          "speaker_id": "2",
