        },
        {
          "id": "MDRW2100013630.1.2",
          "form": "안녕하세요 ~!",
          "original_form": "안녕하세요 ~!",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013630.1.3",
          "form": "최근에 뭐 구입하셨어요?",
          "original_form": "최근에 뭐 구입하셨어요?",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013630.1.4",
          "form": "최근에요",
          "original_form": "최근에요",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013630.1.5",
          "form": "음 두부를 샀었어요",
          "original_form": "음 두부를 샀었어요",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013630.1.6",
          "form": "오 두부로 뭐 해드셨어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 두부로 뭐 해드셨어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013630.1.7",
          "form": "두부로 마파두부 해먹었어요",
          "original_form": "두부로 마파두부 해먹었어요",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013630.1.8",
          "form": "마파두부 소스가 싸더라구요",
          "original_form": "마파두부 소스가 싸더라구요",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013630.1.9",
          "form": "오 어디소스 사먹으셨나요",
          "original_form": "오 어디소스 사먹으셨나요",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013630.1.10",
          "form": "풀무원이였던거 같아요",
          "original_form": "풀무원이였던거 같아요",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013630.1.11",
          "form": "이마트에서 샀거든요",
          "original_form": "이마트에서 샀거든요",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013630.1.12",
          "form": "아하 저도 한번 사서",
          "original_form": "아하 저도 한번 사서",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013630.1.13",
          "form": "해먹어야겠어오 ~!",
          "original_form": "해먹어야겠어오 ~!",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013630.1.14",
          "form": "넹 사시는 김에 파랑 돼지고기도 사가세여",
          "original_form": "넹 사시는 김에 파랑 돼지고기도 사가세여",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013630.1.15",
          "form": "만드는데 어렵진 않았죠??!",
          "original_form": "만드는데 어렵진 않았죠??!",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013630.1.16",
          "form": "넹 아주 쉬워여",
          "original_form": "넹 아주 쉬워여",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013630.1.17",
          "form": "그냥 기름에 고기랑 파 둘둘 볶아주시고",
          "original_form": "그냥 기름에 고기랑 파 둘둘 볶아주시고",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013630.1.18",
          "form": "오우!! 그럼 도전해봐야죠",
          "original_form": "오우!! 그럼 도전해봐야죠",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013630.1.19",
          "form": "두부랑 소스 넣어서 끓이시면 되여",
          "original_form": "두부랑 소스 넣어서 끓이시면 되여",
