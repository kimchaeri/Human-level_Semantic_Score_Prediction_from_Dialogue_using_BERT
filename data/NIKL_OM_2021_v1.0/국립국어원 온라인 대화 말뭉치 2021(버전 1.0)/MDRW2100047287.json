      "utterance": [
        {
          "id": "MDRW2100047287.1.1",
          "form": "중고거래 종종 하나요?",
          "original_form": "중고거래 종종 하나요?",
          "speaker_id": "2",
          "time": "20210827 07:35"
        },
        {
          "id": "MDRW2100047287.1.2",
          "form": "은 자주 아니지만 가끔요",
          "original_form": "은 자주 아니지만 가끔요",
          "speaker_id": "1",
          "time": "20210827 07:35"
        },
        {
          "id": "MDRW2100047287.1.3",
          "form": "이게 귀차니즘 때문에",
          "original_form": "이게 귀차니즘 때문에",
          "speaker_id": "1",
          "time": "20210827 07:36"
        },
        {
          "id": "MDRW2100047287.1.4",
          "form": "잘안해요",
          "original_form": "잘안해요",
          "speaker_id": "1",
          "time": "20210827 07:36"
        },
        {
          "id": "MDRW2100047287.1.5",
          "form": "몰아서 해요",
          "original_form": "몰아서 해요",
          "speaker_id": "1",
          "time": "20210827 07:36"
        },
        {
          "id": "MDRW2100047287.1.6",
          "form": "잘하세요?",
          "original_form": "잘하세요?",
          "speaker_id": "1",
          "time": "20210827 07:36"
        },
        {
          "id": "MDRW2100047287.1.7",
          "form": "요새 당근거래에 빠져서요 ㅎㅎ",
          "original_form": "요새 당근거래에 빠져서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100047287.1.8",
          "form": "오 뭐 파셨어요",
          "original_form": "오 뭐 파셨어요",
          "speaker_id": "1",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100047287.1.9",
          "form": "저도 요새 첨해봤어요",
          "original_form": "저도 요새 첨해봤어요",
          "speaker_id": "1",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100047287.1.10",
          "form": "바로 팔려서 좋더라고요",
          "original_form": "바로 팔려서 좋더라고요",
          "speaker_id": "1",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100047287.1.11",
          "form": "원래 중고 거래 안했는데 코로나 터지고 작년에 가입해서 올해 열심히 팔고 있어요",
          "original_form": "원래 중고 거래 안했는데 코로나 터지고 작년에 가입해서 올해 열심히 팔고 있어요",
          "speaker_id": "2",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100047287.1.12",
          "form": "저도 사긴 안하는대 ㅋㅋ요번에 열심히 팔았어요 ㅋㅋ",
          "original_form": "저도 사긴 안하는대 ㅋㅋ요번에 열심히 팔았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100047287.1.13",
          "form": "주로 뭐파세요?",
          "original_form": "주로 뭐파세요?",
          "speaker_id": "1",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100047287.1.14",
          "form": "전 장난감 팔았어요",
          "original_form": "전 장난감 팔았어요",
          "speaker_id": "1",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100047287.1.15",
          "form": "저는 초여름에 선풍기 팔았어요 집에 새것만 5개 있어서요 ㅎㅎ",
          "original_form": "저는 초여름에 선풍기 팔았어요 집에 새것만 5개 있어서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100047287.1.16",
          "form": "어머 잘팔렸겠어요",
          "original_form": "어머 잘팔렸겠어요",
          "speaker_id": "1",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100047287.1.17",
          "form": "좀 수입이 좋으셨겠어요",
          "original_form": "좀 수입이 좋으셨겠어요",
          "speaker_id": "1",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100047287.1.18",
          "form": "전 장난감 너무 싸게 팔아서 ㅜㅜ알마 못벌었어요",
          "original_form": "전 장난감 너무 싸게 팔아서 ㅜㅜ알마 못벌었어요",
          "speaker_id": "1",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100047287.1.19",
          "form": "저도 인형같은거 정리한다고 내놨었는데 확실히 가전제품 쪽이 돈이 되더라고요",
          "original_form": "저도 인형같은거 정리한다고 내놨었는데 확실히 가전제품 쪽이 돈이 되더라고요",
          "speaker_id": "2",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100047287.1.20",
          "form": "그럼요 장난감은 그냥 주는거죠 ㅋㅋ",
          "original_form": "그럼요 장난감은 그냥 주는거죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100047287.1.21",
          "form": "가전쪽이 돈돼요",
          "original_form": "가전쪽이 돈돼요",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100047287.1.22",
          "form": "와서 에누리 해달라하셔서",
          "original_form": "와서 에누리 해달라하셔서",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100047287.1.23",
          "form": "좀 그랬어요",
          "original_form": "좀 그랬어요",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100047287.1.24",
          "form": "싸게 올렸는데 깍아달래요? 너무했나",
          "original_form": "싸게 올렸는데 깍아달래요? 너무했나",
          "speaker_id": "2",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100047287.1.25",
          "form": "다",
          "original_form": "다",
          "speaker_id": "2",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100047287.1.26",
          "form": "네 ㅜㅜ근데 원래 그렇대요",
          "original_form": "네 ㅜㅜ근데 원래 그렇대요",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100047287.1.27",
          "form": "아닌가요?",
          "original_form": "아닌가요?",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100047287.1.28",
          "form": "근데 사진이 참 중요한거 같아요",
          "original_form": "근데 사진이 참 중요한거 같아요",
          "speaker_id": "1",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100047287.1.29",
          "form": "전 사진 엉망이라 잘 안팔려요 ㅋㅋ",
          "original_form": "전 사진 엉망이라 잘 안팔려요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100047287.1.30",
          "form": "친구가 막 사진보고 한숨을 ㅋㅋ",
          "original_form": "친구가 막 사진보고 한숨을 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:08"
        },
        {
          "id": "MDRW2100047287.1.31",
          "form": "사진 잘찍으세요?",
          "original_form": "사진 잘찍으세요?",
          "speaker_id": "1",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100047287.1.32",
          "form": "전 사진보다 글로 그럴싸하게 ㅎㅎ",
          "original_form": "전 사진보다 글로 그럴싸하게 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:32"
        },
        {
          "id": "MDRW2100047287.1.33",
          "form": "근데 사진도 중요하대요 전 중간실력일듯요",
          "original_form": "근데 사진도 중요하대요 전 중간실력일듯요",
          "speaker_id": "2",
          "time": "20210827 08:33"
        },
        {
          "id": "MDRW2100047287.1.34",
          "form": "오 능력자",
          "original_form": "오 능력자",
          "speaker_id": "1",
          "time": "20210827 08:33"
        },
        {
          "id": "MDRW2100047287.1.35",
          "form": "전글도 잘못써요 ㅋㅋ",
          "original_form": "전글도 잘못써요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:33"
        },
        {
          "id": "MDRW2100047287.1.36",
          "form": "팔리는게 신기해요 ㅎㅎ",
          "original_form": "팔리는게 신기해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:33"
        },
        {
          "id": "MDRW2100047287.1.37",
          "form": "사진고 중간 딱 팔기 좋은데요 ㅎㅎ",
          "original_form": "사진고 중간 딱 팔기 좋은데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:33"
        },
        {
          "id": "MDRW2100047287.1.38",
          "form": "근데 싼건 잘 나가고 비싼건 잘 안나가서 거래한 거에 비해 돈은 벌로 안돼요/\n선풍기는 여름특수 누린거구요 ㅎㅎ",
          "original_form": "근데 싼건 잘 나가고 비싼건 잘 안나가서 거래한 거에 비해 돈은 벌로 안돼요/\n선풍기는 여름특수 누린거구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:37"
        },
        {
          "id": "MDRW2100047287.1.39",
          "form": "그렇죠 저도 그래요",
          "original_form": "그렇죠 저도 그래요",
          "speaker_id": "1",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100047287.1.40",
          "form": "오늘까지 보고 확내리고 안됨 ㅜㅜ",
          "original_form": "오늘까지 보고 확내리고 안됨 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100047287.1.41",
          "form": "버리겠어요",
          "original_form": "버리겠어요",
          "speaker_id": "1",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100047287.1.42",
          "form": "중고 거래가 큰돈은 안돼요 ㅡㅡ",
          "original_form": "중고 거래가 큰돈은 안돼요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100047287.1.43",
          "form": "선풍기는 여름이죠 시기 맞춰서 잘파셨네요 ㅎㅎ",
          "original_form": "선풍기는 여름이죠 시기 맞춰서 잘파셨네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100047287.1.44",
          "form": "네.. 선풍기 덕분에 몇만원 단위로 벌었지.. 보통은 몇천원이에요..ㅎ 아.. 산건 아이패드 샀네요.. 판것보다 산게 더 비싼..ㅋ",
          "original_form": "네.. 선풍기 덕분에 몇만원 단위로 벌었지.. 보통은 몇천원이에요..ㅎ 아.. 산건 아이패드 샀네요.. 판것보다 산게 더 비싼..ㅋ",
          "speaker_id": "2",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100047287.1.45",
          "form": "헉 몇배로 사셨네요",
          "original_form": "헉 몇배로 사셨네요",
          "speaker_id": "1",
          "time": "20210827 08:50"
        },
        {
          "id": "MDRW2100047287.1.46",
          "form": "그래도 몇만원 와 좋은데요 ㅎㅎ",
          "original_form": "그래도 몇만원 와 좋은데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:51"
        },
        {
          "id": "MDRW2100047287.1.47",
          "form": "아이패드 잘 쓰고계세요?",
          "original_form": "아이패드 잘 쓰고계세요?",
          "speaker_id": "1",
          "time": "20210827 08:51"
        },
        {
          "id": "MDRW2100047287.1.48",
          "form": "쓰고는 있는데.. 어째 당근하면서 오히려 큰 돈을 쓰게 되더라구요..ㅋ",
          "original_form": "쓰고는 있는데.. 어째 당근하면서 오히려 큰 돈을 쓰게 되더라구요..ㅋ",
          "speaker_id": "2",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100047287.1.49",
          "form": "팔기는 열심히 파는데 ..천원 이천원짜리 파는거라..ㅋㅋㅋ",
          "original_form": "팔기는 열심히 파는데 ..천원 이천원짜리 파는거라..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100047287.1.50",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100047287.1.51",
          "form": "맨날 싼거 ㅋㅋ",
          "original_form": "맨날 싼거 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100047287.1.52",
          "form": "팔면 더 비싼거 사게되는거 같아요 ㅋㅋ",
          "original_form": "팔면 더 비싼거 사게되는거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100047287.1.53",
          "form": "팔고 사고 ㅋㅋ",
          "original_form": "팔고 사고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100047287.1.54",
          "form": "그니까요.. 팔기만 하면 좋은데.. 그럴려고 맘먹고 들어가는데 또 보면  사고 싶고..ㅋ",
          "original_form": "그니까요.. 팔기만 하면 좋은데.. 그럴려고 맘먹고 들어가는데 또 보면  사고 싶고..ㅋ",
          "speaker_id": "2",
          "time": "20210827 09:08"
        },
        {
          "id": "MDRW2100047287.1.55",
          "form": "그간 본능입니다",
          "original_form": "그간 본능입니다",
          "speaker_id": "1",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100047287.1.56",
          "form": "그니까요..눈의 욕망..ㅋㅋㅋ 보면 되게 좋은 걸 저렴하게 파는 거 같아서 막 사고 싶은데 자자헤는게 보통 힘든게 아니에요",
          "original_form": "그니까요..눈의 욕망..ㅋㅋㅋ 보면 되게 좋은 걸 저렴하게 파는 거 같아서 막 사고 싶은데 자자헤는게 보통 힘든게 아니에요",
          "speaker_id": "2",
          "time": "20210827 09:12"
        },
        {
