        {
          "id": "MDRW2100044076.1.1",
          "form": "이제.가을이.된것 같죠?",
          "original_form": "이제.가을이.된것 같죠?",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044076.1.2",
          "form": "네..좀 썰렁한듯",
          "original_form": "네..좀 썰렁한듯",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044076.1.3",
          "form": "근데 오늘 낮엔 후덥지근 하더라고요",
          "original_form": "근데 오늘 낮엔 후덥지근 하더라고요",
