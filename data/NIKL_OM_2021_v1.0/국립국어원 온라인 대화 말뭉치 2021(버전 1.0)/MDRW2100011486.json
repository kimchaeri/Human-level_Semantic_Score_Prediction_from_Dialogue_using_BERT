      "utterance": [
        {
          "id": "MDRW2100011486.1.1",
          "form": "친족이면 되지 않을까",
          "original_form": "친족이면 되지 않을까",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100011486.1.2",
          "form": "근데친척이그냥영주권",
          "original_form": "근데친척이그냥영주권",
          "speaker_id": "1",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100011486.1.3",
          "form": "만있으면",
          "original_form": "만있으면",
          "speaker_id": "1",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100011486.1.4",
          "form": "초대못하고",
          "original_form": "초대못하고",
          "speaker_id": "1",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100011486.1.5",
          "form": "시민권까지있어야할걸",
          "original_form": "시민권까지있어야할걸",
          "speaker_id": "1",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100011486.1.6",
          "form": "영주권초대이런거찾으면",
          "original_form": "영주권초대이런거찾으면",
          "speaker_id": "1",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100011486.1.7",
          "form": "나올걸",
          "original_form": "나올걸",
          "speaker_id": "1",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100011486.1.8",
          "form": "아 그것 참 빡빡하네 이민의 나라에서",
          "original_form": "아 그것 참 빡빡하네 이민의 나라에서",
          "speaker_id": "2",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100011486.1.9",
          "form": "그니까 나도 엄마 초대하려고 알아보다가",
          "original_form": "그니까 나도 엄마 초대하려고 알아보다가",
          "speaker_id": "1",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100011486.1.10",
          "form": "영주권만있으면초대못한다는거알았음",
          "original_form": "영주권만있으면초대못한다는거알았음",
          "speaker_id": "1",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100011486.1.11",
          "form": "에헤이 참",
          "original_form": "에헤이 참",
          "speaker_id": "2",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100011486.1.12",
          "form": "은근미국이이민이런거",
          "original_form": "은근미국이이민이런거",
          "speaker_id": "1",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100011486.1.13",
          "form": "확실히거르는듯",
          "original_form": "확실히거르는듯",
          "speaker_id": "1",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100011486.1.14",
          "form": "그니까 지들도 이민 오면서 시작했으면서",
          "original_form": "그니까 지들도 이민 오면서 시작했으면서",
          "speaker_id": "2",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100011486.1.15",
          "form": "이왕 가는 거면 직계 가족들이랑 같이 있고 싶었는데",
          "original_form": "이왕 가는 거면 직계 가족들이랑 같이 있고 싶었는데",
          "speaker_id": "2",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100011486.1.16",
          "form": "아깝네",
          "original_form": "아깝네",
          "speaker_id": "2",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100011486.1.17",
          "form": "어머님부르고싶으면",
          "original_form": "어머님부르고싶으면",
          "speaker_id": "1",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100011486.1.18",
          "form": "컬리지이런거",
          "original_form": "컬리지이런거",
          "speaker_id": "1",
          "time": "20210824 13:15"
        },
        {
          "id": "MDRW2100011486.1.19",
          "form": "다니시게하면될듯",
          "original_form": "다니시게하면될듯",
          "speaker_id": "1",
          "time": "20210824 13:15"
        },
        {
          "id": "MDRW2100011486.1.20",
          "form": "쉽게갈수있는컬리지많아서",
          "original_form": "쉽게갈수있는컬리지많아서",
          "speaker_id": "1",
          "time": "20210824 13:15"
        },
        {
          "id": "MDRW2100011486.1.21",
          "form": "어옹 그것도 좋은 방법이네",
          "original_form": "어옹 그것도 좋은 방법이네",
          "speaker_id": "2",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100011486.1.22",
          "form": "근데거의아버님안간다고하시면",
          "original_form": "근데거의아버님안간다고하시면",
          "speaker_id": "1",
          "time": "20210824 13:17"
        },
        {
          "id": "MDRW2100011486.1.23",
          "form": "어머님도안가려고하실걸",
          "original_form": "어머님도안가려고하실걸",
