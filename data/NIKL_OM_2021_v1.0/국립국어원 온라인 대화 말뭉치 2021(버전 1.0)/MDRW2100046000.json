      "utterance": [
        {
          "id": "MDRW2100046000.1.1",
          "form": "쇼핑은 해도해도 끝이 없는거같아요",
          "original_form": "쇼핑은 해도해도 끝이 없는거같아요",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100046000.1.2",
          "form": "매일 몬가를 사게되요",
          "original_form": "매일 몬가를 사게되요",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046000.1.3",
          "form": "그렇죠 구매를 해도 또 필요물품이 있죠",
          "original_form": "그렇죠 구매를 해도 또 필요물품이 있죠",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046000.1.4",
          "form": "네 정말 끊임없이 필요한게 생겨요",
          "original_form": "네 정말 끊임없이 필요한게 생겨요",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100046000.1.5",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100046000.1.6",
          "form": "끊을수 없는 삶인것 같아요",
          "original_form": "끊을수 없는 삶인것 같아요",
          "speaker_id": "2",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100046000.1.7",
          "form": "오늘은 또 클렌징오일을 사야할거같아요ㅋ",
          "original_form": "오늘은 또 클렌징오일을 사야할거같아요ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100046000.1.8",
          "form": "전 탈모샴푸 구매 고려중이예요",
          "original_form": "전 탈모샴푸 구매 고려중이예요",
          "speaker_id": "2",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100046000.1.9",
          "form": "근데 그거 효과잇나요",
          "original_form": "근데 그거 효과잇나요",
          "speaker_id": "1",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046000.1.10",
          "form": "항상 궁금",
          "original_form": "항상 궁금",
          "speaker_id": "1",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046000.1.11",
          "form": "전 개인적으로 사용후 덜빠지는것 같아요",
          "original_form": "전 개인적으로 사용후 덜빠지는것 같아요",
          "speaker_id": "2",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100046000.1.12",
          "form": "감을때 빗을때 진짜 마니빠지긴해요",
          "original_form": "감을때 빗을때 진짜 마니빠지긴해요",
          "speaker_id": "1",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100046000.1.13",
          "form": "써봐야겟네요",
          "original_form": "써봐야겟네요",
          "speaker_id": "1",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100046000.1.14",
          "form": "구매한 물품은 모두 만족하시나요",
          "original_form": "구매한 물품은 모두 만족하시나요",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046000.1.15",
          "form": "뭐 그냥그냥 쓰는 편이요\r\n근데 클렌징오일 산게 맘에들어서 이번에 그거 또사려고요",
          "original_form": "뭐 그냥그냥 쓰는 편이요\r\n근데 클렌징오일 산게 맘에들어서 이번에 그거 또사려고요",
          "speaker_id": "1",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100046000.1.16",
          "form": "저도 맘에 드는 제품은 계속 사서 쓰죠",
