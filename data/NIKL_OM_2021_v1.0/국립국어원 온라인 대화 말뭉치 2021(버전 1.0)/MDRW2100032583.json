      "utterance": [
        {
          "id": "MDRW2100032583.1.1",
          "form": "작년에 애들이 보러가는거 봤을때",
          "original_form": "작년에 애들이 보러가는거 봤을때",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032583.1.2",
          "form": "부러웠음...",
          "original_form": "부러웠음...",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032583.1.3",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032583.1.4",
          "form": "보러가자",
          "original_form": "보러가자",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032583.1.5",
          "form": "응 가을에 유명한것 같던데",
          "original_form": "응 가을에 유명한것 같던데",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032583.1.6",
          "form": "안되겠네",
          "original_form": "안되겠네",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032583.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032583.1.8",
          "form": "넌 여행갈때",
          "original_form": "넌 여행갈때",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.9",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.10",
          "form": "계획 다 세우고 가?",
          "original_form": "계획 다 세우고 가?",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.11",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.12",
          "form": "헐 걍 즉흥적으로 가?",
          "original_form": "헐 걍 즉흥적으로 가?",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.13",
          "form": "계획대로 안될껄 알기때문에",
          "original_form": "계획대로 안될껄 알기때문에",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.14",
          "form": "큰 틀만 세우고",
          "original_form": "큰 틀만 세우고",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.15",
          "form": "오진짜?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오진짜?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.16",
          "form": "의외다",
          "original_form": "의외다",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.17",
          "form": "응ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "응ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.18",
          "form": "근데 계획짜고 가면",
          "original_form": "근데 계획짜고 가면",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.19",
          "form": "스트레스를 너무 많이 받음",
          "original_form": "스트레스를 너무 많이 받음",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.20",
          "form": "한국이면 그나마 나은데",
          "original_form": "한국이면 그나마 나은데",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.21",
          "form": "외국은 진짜..",
          "original_form": "외국은 진짜..",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032583.1.22",
          "form": "아아",
