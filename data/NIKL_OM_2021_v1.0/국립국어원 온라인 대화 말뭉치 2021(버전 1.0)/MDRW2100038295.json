      "utterance": [
        {
          "id": "MDRW2100038295.1.1",
          "form": "역시 킹덤인가요ㅎㅎ",
          "original_form": "역시 킹덤인가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100038295.1.2",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100038295.1.3",
          "form": "스위트홈",
          "original_form": "스위트홈",
          "speaker_id": "1",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100038295.1.4",
          "form": "키워드 제출하고 나니 아찔하네요",
          "original_form": "키워드 제출하고 나니 아찔하네요",
          "speaker_id": "1",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100038295.1.5",
          "form": "배우 누구요?;",
          "original_form": "배우 누구요?;",
          "speaker_id": "1",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100038295.1.6",
          "form": "ㅋㅋ 이도현이요",
          "original_form": "ㅋㅋ 이도현이요",
          "speaker_id": "2",
          "time": "20210902 08:56"
        },
        {
          "id": "MDRW2100038295.1.7",
          "form": "오 찾아보고 왔어요",
          "original_form": "오 찾아보고 왔어요",
          "speaker_id": "1",
          "time": "20210902 08:57"
        },
        {
          "id": "MDRW2100038295.1.8",
          "form": "잘생겼네요,,, 동갑인데",
          "original_form": "잘생겼네요,,, 동갑인데",
          "speaker_id": "1",
          "time": "20210902 08:58"
        },
        {
          "id": "MDRW2100038295.1.9",
          "form": "앜ㅋㅋ 동갑이셨군요",
          "original_form": "앜ㅋㅋ 동갑이셨군요",
          "speaker_id": "2",
          "time": "20210902 08:58"
        },
        {
          "id": "MDRW2100038295.1.10",
          "form": "ㅋㅋㅋㅋㅋ알 수 없는 부러움!!!",
          "original_form": "ㅋㅋㅋㅋㅋ알 수 없는 부러움!!!",
          "speaker_id": "1",
          "time": "20210902 08:58"
        },
        {
          "id": "MDRW2100038295.1.11",
          "form": "이도현 개인적으로 안경쓴 게",
          "original_form": "이도현 개인적으로 안경쓴 게",
          "speaker_id": "1",
          "time": "20210902 08:58"
        },
        {
          "id": "MDRW2100038295.1.12",
          "form": "더 매력적이네요 스위트홈에서?",
          "original_form": "더 매력적이네요 스위트홈에서?",
          "speaker_id": "1",
          "time": "20210902 08:58"
        },
        {
          "id": "MDRW2100038295.1.13",
          "form": "맞아요 안경 너무 잘받아요",
          "original_form": "맞아요 안경 너무 잘받아요",
          "speaker_id": "2",
          "time": "20210902 08:59"
        },
        {
          "id": "MDRW2100038295.1.14",
          "form": "약간 초췌한 게",
          "original_form": "약간 초췌한 게",
          "speaker_id": "1",
          "time": "20210902 08:59"
        },
        {
          "id": "MDRW2100038295.1.15",
          "form": "진짜 잘 어울리는 거 같아요",
          "original_form": "진짜 잘 어울리는 거 같아요",
          "speaker_id": "1",
          "time": "20210902 08:59"
        },
        {
          "id": "MDRW2100038295.1.16",
          "form": "초췌한게 어울린다니ㅋㅋ 맞는거 같아요",
          "original_form": "초췌한게 어울린다니ㅋㅋ 맞는거 같아요",
          "speaker_id": "2",
          "time": "20210902 09:00"
        },
        {
          "id": "MDRW2100038295.1.17",
          "form": "근데 다른 드라마에서 보면",
          "original_form": "근데 다른 드라마에서 보면",
          "speaker_id": "2",
          "time": "20210902 09:00"
        },
        {
          "id": "MDRW2100038295.1.18",
          "form": "밝은 것도 어울려요",
          "original_form": "밝은 것도 어울려요",
          "speaker_id": "2",
          "time": "20210902 09:00"
        },
        {
