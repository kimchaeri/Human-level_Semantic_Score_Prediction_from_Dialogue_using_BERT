      "utterance": [
        {
          "id": "MDRW2100003566.1.1",
          "form": "넷플릭스 구독하세요?",
          "original_form": "넷플릭스 구독하세요?",
          "speaker_id": "2",
          "time": "20210821 02:00"
        },
        {
          "id": "MDRW2100003566.1.2",
          "form": "아니요 전 주로 티비시청해요ㅎ",
          "original_form": "아니요 전 주로 티비시청해요ㅎ",
          "speaker_id": "1",
          "time": "20210821 02:00"
        },
        {
          "id": "MDRW2100003566.1.3",
          "form": "넷플릭스 많이 보던데 좋나요?",
          "original_form": "넷플릭스 많이 보던데 좋나요?",
          "speaker_id": "1",
          "time": "20210821 02:01"
        },
        {
          "id": "MDRW2100003566.1.4",
          "form": "네 저는 잘 이용하고 있어요",
          "original_form": "네 저는 잘 이용하고 있어요",
          "speaker_id": "2",
          "time": "20210821 02:01"
        },
        {
          "id": "MDRW2100003566.1.5",
          "form": "4인팟이라 금액 부담도 적고",
          "original_form": "4인팟이라 금액 부담도 적고",
          "speaker_id": "2",
          "time": "20210821 02:01"
        },
        {
          "id": "MDRW2100003566.1.6",
          "form": "주로 뭐보세요?",
          "original_form": "주로 뭐보세요?",
          "speaker_id": "1",
          "time": "20210821 02:01"
        },
        {
          "id": "MDRW2100003566.1.7",
          "form": "저는 주로 미국 시트콤이요",
          "original_form": "저는 주로 미국 시트콤이요",
          "speaker_id": "2",
          "time": "20210821 02:01"
        },
        {
          "id": "MDRW2100003566.1.8",
          "form": "프렌즈나 모던패밀리 등등",
          "original_form": "프렌즈나 모던패밀리 등등",
          "speaker_id": "2",
          "time": "20210821 02:01"
        },
        {
          "id": "MDRW2100003566.1.9",
          "form": "4명이서 공유하니 비용도 세이브되겠네요",
          "original_form": "4명이서 공유하니 비용도 세이브되겠네요",
          "speaker_id": "1",
          "time": "20210821 02:02"
        },
        {
          "id": "MDRW2100003566.1.10",
          "form": "밥 먹을 때 틀어놓으면 좋더라구요",
          "original_form": "밥 먹을 때 틀어놓으면 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 02:02"
        },
        {
          "id": "MDRW2100003566.1.11",
          "form": "영어공부도 되겠어요ㅎㅎ",
          "original_form": "영어공부도 되겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 02:02"
        },
        {
          "id": "MDRW2100003566.1.12",
          "form": "여러번 보다 보면 도움이 되겠죠",
          "original_form": "여러번 보다 보면 도움이 되겠죠",
          "speaker_id": "2",
          "time": "20210821 02:03"
        },
        {
          "id": "MDRW2100003566.1.13",
          "form": "프렌즈는 옛날도 했던 프로같은데 요즘도 있나보네요",
          "original_form": "프렌즈는 옛날도 했던 프로같은데 요즘도 있나보네요",
          "speaker_id": "1",
          "time": "20210821 02:03"
        },
        {
          "id": "MDRW2100003566.1.14",
          "form": "티비 무슨 프로그램 보세여?",
          "original_form": "티비 무슨 프로그램 보세여?",
          "speaker_id": "2",
          "time": "20210821 02:03"
        },
        {
          "id": "MDRW2100003566.1.15",
          "form": "네 프렌즈는 지금봐도 재밌더라구요",
          "original_form": "네 프렌즈는 지금봐도 재밌더라구요",
          "speaker_id": "2",
          "time": "20210821 02:04"
        },
        {
          "id": "MDRW2100003566.1.16",
          "form": "주로 예능봐요ㅎ 아까 나혼자산다보는데 오상욱선수나오더라구요",
          "original_form": "주로 예능봐요ㅎ 아까 나혼자산다보는데 오상욱선수나오더라구요",
          "speaker_id": "1",
          "time": "20210821 02:04"
        },
        {
          "id": "MDRW2100003566.1.17",
          "form": "아 기사 본 것 같아요",
          "original_form": "아 기사 본 것 같아요",
          "speaker_id": "2",
          "time": "20210821 02:04"
        },
        {
          "id": "MDRW2100003566.1.18",
          "form": "나혼자 산다에 김연경 선수도 나온다고 하더라구요",
          "original_form": "나혼자 산다에 김연경 선수도 나온다고 하더라구요",
          "speaker_id": "2",
          "time": "20210821 02:04"
        },
        {
          "id": "MDRW2100003566.1.19",
          "form": "유퀴즈도 재밌고 놀면뭐하니도  재밌어요",
          "original_form": "유퀴즈도 재밌고 놀면뭐하니도  재밌어요",
          "speaker_id": "1",
          "time": "20210821 02:04"
        },
        {
          "id": "MDRW2100003566.1.20",
          "form": "다움주인가 다다음주인가에요",
          "original_form": "다움주인가 다다음주인가에요",
          "speaker_id": "2",
          "time": "20210821 02:04"
        },
        {
          "id": "MDRW2100003566.1.21",
          "form": "아 다음주는 오상욱선수 2부하는거같아요",
          "original_form": "아 다음주는 오상욱선수 2부하는거같아요",
          "speaker_id": "1",
          "time": "20210821 02:05"
        },
        {
          "id": "MDRW2100003566.1.22",
          "form": "유퀴즈는 저도 재밌게 보고 있어요",
          "original_form": "유퀴즈는 저도 재밌게 보고 있어요",
          "speaker_id": "2",
          "time": "20210821 02:05"
        },
        {
          "id": "MDRW2100003566.1.23",
          "form": "김연경선수는 다담주에 나오나보네요",
          "original_form": "김연경선수는 다담주에 나오나보네요",
          "speaker_id": "1",
          "time": "20210821 02:05"
        },
        {
          "id": "MDRW2100003566.1.24",
          "form": "네 저는 그때 챙겨보려구요",
          "original_form": "네 저는 그때 챙겨보려구요",
          "speaker_id": "2",
          "time": "20210821 02:05"
        }
      ]
    }
