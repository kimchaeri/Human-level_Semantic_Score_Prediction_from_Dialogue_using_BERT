      "utterance": [
        {
          "id": "MDRW2100003983.1.1",
          "form": "코스트코 소고기 베이크 먹었어요",
          "original_form": "코스트코 소고기 베이크 먹었어요",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003983.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003983.1.3",
          "form": "오 맛있겠어요  전 이제 먹는중이요",
          "original_form": "오 맛있겠어요  전 이제 먹는중이요",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003983.1.4",
          "form": "감자고로케랑 곱창볶음^^",
          "original_form": "감자고로케랑 곱창볶음^^",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003983.1.5",
          "form": "헉 대박..",
          "original_form": "헉 대박..",
          "speaker_id": "2",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003983.1.6",
          "form": "집에서 드시는거에요? 곱창볶음이라니...",
          "original_form": "집에서 드시는거에요? 곱창볶음이라니...",
          "speaker_id": "2",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003983.1.7",
          "form": "네 ㅋ 정원샵에서 안주야 곱창볶음을 샀는데 양이진짜 적네요",
          "original_form": "네 ㅋ 정원샵에서 안주야 곱창볶음을 샀는데 양이진짜 적네요",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003983.1.8",
          "form": "냉장 냉동음식들은 양 다 적더라구요",
          "original_form": "냉장 냉동음식들은 양 다 적더라구요",
          "speaker_id": "2",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003983.1.9",
          "form": "저도 한 번 시도해보고 다신 시도 안해요 ㅠ",
          "original_form": "저도 한 번 시도해보고 다신 시도 안해요 ㅠ",
          "speaker_id": "2",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003983.1.10",
          "form": "그래도 쿠폰쓰고사서 저렴하거샀네요ㅎ 맛은 괜찮은데요",
          "original_form": "그래도 쿠폰쓰고사서 저렴하거샀네요ㅎ 맛은 괜찮은데요",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003983.1.11",
          "form": "코스트코 소고기가 그렇게 싸다던데 맛은 어때요?",
          "original_form": "코스트코 소고기가 그렇게 싸다던데 맛은 어때요?",
          "speaker_id": "1",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003983.1.12",
          "form": "맛은 괜찮나요? 제가 먹었던건 맛도 별로였어요",
          "original_form": "맛은 괜찮나요? 제가 먹었던건 맛도 별로였어요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003983.1.13",
          "form": "고기만 따로는 많이 안사봐서 잘 모르겠어요",
          "original_form": "고기만 따로는 많이 안사봐서 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003983.1.14",
          "form": "소고기 베이크는 맛있어요 치즈도 제법 많고",
          "original_form": "소고기 베이크는 맛있어요 치즈도 제법 많고",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003983.1.15",
          "form": "아 살치살 팩으로 사면 진짜싼거같던데",
          "original_form": "아 살치살 팩으로 사면 진짜싼거같던데",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003983.1.16",
          "form": "다음에 시도해봐야겠네요",
          "original_form": "다음에 시도해봐야겠네요",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003983.1.17",
          "form": "나쁘진 않다던데요?",
          "original_form": "나쁘진 않다던데요?",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003983.1.18",
          "form": "고기는 기억이 안나는데 사고서 후회한건 없는거 같아요",
          "original_form": "고기는 기억이 안나는데 사고서 후회한건 없는거 같아요",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003983.1.19",
          "form": "고기좋아하는데 먹고싶네요 미국산이여도 괜찮나봐요",
          "original_form": "고기좋아하는데 먹고싶네요 미국산이여도 괜찮나봐요",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003983.1.20",
          "form": "맛에는 차이가 없다 들었어요",
          "original_form": "맛에는 차이가 없다 들었어요",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003983.1.21",
          "form": "전 한우가 더 맛있는건 아니라고 들어서..",
          "original_form": "전 한우가 더 맛있는건 아니라고 들어서..",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003983.1.22",
          "form": "근데 코코는 뭐든 혼자 살면 사기 힘든 양ㅠㅠ",
          "original_form": "근데 코코는 뭐든 혼자 살면 사기 힘든 양ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003983.1.23",
          "form": "등급좋은건 질좋대요 미국산도",
          "original_form": "등급좋은건 질좋대요 미국산도",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003983.1.24",
          "form": "그래서 코스트코는 못가봤어요",
          "original_form": "그래서 코스트코는 못가봤어요",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003983.1.25",
          "form": "저도 혼자 살 때는 못갔어요",
          "original_form": "저도 혼자 살 때는 못갔어요",
          "speaker_id": "2",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100003983.1.26",
          "form": "엄마따라만..",
          "original_form": "엄마따라만..",
          "speaker_id": "2",
          "time": "20210821 14:39"
        }
      ]
    }
