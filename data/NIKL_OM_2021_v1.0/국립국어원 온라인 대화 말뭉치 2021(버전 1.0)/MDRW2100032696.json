      "utterance": [
        {
          "id": "MDRW2100032696.1.1",
          "form": "연애는 하시나요?",
          "original_form": "연애는 하시나요?",
          "speaker_id": "1",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032696.1.2",
          "form": "네네 장거리연애 중이에요ㅠ",
          "original_form": "네네 장거리연애 중이에요ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032696.1.3",
          "form": "외국에있어서 코로나땜에 못본지 2년 다되어가요 ...",
          "original_form": "외국에있어서 코로나땜에 못본지 2년 다되어가요 ...",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032696.1.4",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "1",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100032696.1.5",
          "form": "어디에 사시는데요??",
          "original_form": "어디에 사시는데요??",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032696.1.6",
          "form": "븍유럽에있어요ㅠㅠ",
          "original_form": "븍유럽에있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 07:01"
        },
        {
          "id": "MDRW2100032696.1.7",
          "form": "허루ㅠㅜㅜ뷱유럽 어디요??",
          "original_form": "허루ㅠㅜㅜ뷱유럽 어디요??",
          "speaker_id": "1",
          "time": "20210906 08:45"
        },
        {
          "id": "MDRW2100032696.1.8",
          "form": "노르웨이에 있습니다ㅠㅠ",
          "original_form": "노르웨이에 있습니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 11:39"
        },
        {
          "id": "MDRW2100032696.1.9",
          "form": "너무 보고싶지만 상황은 지겨워요ㅠㅠ",
          "original_form": "너무 보고싶지만 상황은 지겨워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 11:39"
        },
        {
          "id": "MDRW2100032696.1.10",
          "form": "에구ㅜㅜㅜㅜ한국사람이에요??",
          "original_form": "에구ㅜㅜㅜㅜ한국사람이에요??",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100032696.1.11",
          "form": "네네 한국인 유학 갔어요",
          "original_form": "네네 한국인 유학 갔어요",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100032696.1.12",
          "form": "에고 언제가신거예요?",
          "original_form": "에고 언제가신거예요?",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100032696.1.13",
          "form": "2020년도 1월 1일에 갔어요 ㅋㅋㅋ",
          "original_form": "2020년도 1월 1일에 갔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100032696.1.14",
          "form": "그러곤 코로나가 터졌죠",
          "original_form": "그러곤 코로나가 터졌죠",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100032696.1.15",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100032696.1.16",
          "form": "진짜 딱 그때였는데",
          "original_form": "진짜 딱 그때였는데",
          "speaker_id": "1",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100032696.1.17",
          "form": "19년 12월인가",
          "original_form": "19년 12월인가",
          "speaker_id": "1",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100032696.1.18",
          "form": "보거싶겠어요ㅠㅜ",
          "original_form": "보거싶겠어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100032696.1.19",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100032696.1.20",
          "form": "언제나 보려나 ...",
          "original_form": "언제나 보려나 ...",
          "speaker_id": "2",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100032696.1.21",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100032696.1.22",
          "form": "예방접종하면 볼수있지않나요?",
          "original_form": "예방접종하면 볼수있지않나요?",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100032696.1.23",
          "form": "해외 격리가 사라질까요? 우선은 이번주 내로 둘ㄷ 맞아소 다행이에요",
          "original_form": "해외 격리가 사라질까요? 우선은 이번주 내로 둘ㄷ 맞아소 다행이에요",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100032696.1.24",
          "form": "이번주 내에 2차까지 다 접종이에요??",
          "original_form": "이번주 내에 2차까지 다 접종이에요??",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100032696.1.25",
          "form": "2주 격리는 없다그러지 않아요??예방접종하면?",
          "original_form": "2주 격리는 없다그러지 않아요??예방접종하면?",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100032696.1.26",
          "form": "아정말요?",
          "original_form": "아정말요?",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100032696.1.27",
          "form": "백신맞으면 2주 격리없어요?",
          "original_form": "백신맞으면 2주 격리없어요?",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100032696.1.28",
          "form": "그렇게 알고 있어요",
          "original_form": "그렇게 알고 있어요",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100032696.1.29",
          "form": "우와.... 그럼 너무 조은데....",
          "original_form": "우와.... 그럼 너무 조은데....",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100032696.1.30",
          "form": "저 유럽가여해요ㅠㅠ 남친도보고",
          "original_form": "저 유럽가여해요ㅠㅠ 남친도보고",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100032696.1.31",
          "form": "배트남에 조카도 보러가여해요",
          "original_form": "배트남에 조카도 보러가여해요",
