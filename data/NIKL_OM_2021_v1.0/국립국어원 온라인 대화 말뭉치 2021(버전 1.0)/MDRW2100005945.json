      "utterance": [
        {
          "id": "MDRW2100005945.1.1",
          "form": "이번 추석때 고향 가시나요?",
          "original_form": "이번 추석때 고향 가시나요?",
          "speaker_id": "1",
          "time": "20210907 17:04"
        },
        {
          "id": "MDRW2100005945.1.2",
          "form": "사실 고향이라고 하기에도",
          "original_form": "사실 고향이라고 하기에도",
          "speaker_id": "2",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100005945.1.3",
          "form": "민망한게",
          "original_form": "민망한게",
          "speaker_id": "2",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100005945.1.4",
          "form": "자취 시작한지 2년밖에 안되서요...",
          "original_form": "자취 시작한지 2년밖에 안되서요...",
          "speaker_id": "2",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100005945.1.5",
          "form": "부모님이 가까이 계신가요?",
          "original_form": "부모님이 가까이 계신가요?",
          "speaker_id": "1",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100005945.1.6",
          "form": "본가(?)가 인천인지라",
          "original_form": "본가(?)가 인천인지라",
          "speaker_id": "2",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100005945.1.7",
          "form": "조금 아쉬워도 자취를 안해도 되던 상황이었던지라",
          "original_form": "조금 아쉬워도 자취를 안해도 되던 상황이었던지라",
          "speaker_id": "2",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100005945.1.8",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100005945.1.9",
          "form": "이번 추석땐 부모님 집에 가는거죠?",
          "original_form": "이번 추석땐 부모님 집에 가는거죠?",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100005945.1.10",
          "form": "예",
          "original_form": "예",
          "speaker_id": "2",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100005945.1.11",
          "form": "물론 갈 예정입니다",
          "original_form": "물론 갈 예정입니다",
          "speaker_id": "2",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100005945.1.12",
          "form": "저도 따로 사는 딸이 이번에 온다고 해서 기대중이에요",
          "original_form": "저도 따로 사는 딸이 이번에 온다고 해서 기대중이에요",
          "speaker_id": "1",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100005945.1.13",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005945.1.14",
          "form": "멀리 계신(?)가요?",
          "original_form": "멀리 계신(?)가요?",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005945.1.15",
          "form": "어감이 좀 이상한데",
          "original_form": "어감이 좀 이상한데",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005945.1.16",
          "form": "물리적으로 자주 만나기 힘드신가요",
          "original_form": "물리적으로 자주 만나기 힘드신가요",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005945.1.17",
          "form": "보통은 주말마다 왔는데",
          "original_form": "보통은 주말마다 왔는데",
          "speaker_id": "1",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100005945.1.18",
          "form": "최근 거리두기로 6시후 2인이상 못일때",
          "original_form": "최근 거리두기로 6시후 2인이상 못일때",
          "speaker_id": "1",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100005945.1.19",
          "form": "계속 못왔어요",
          "original_form": "계속 못왔어요",
          "speaker_id": "1",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100005945.1.20",
          "form": "딸가족이 올떄 같이 오니까 인원초과거든요",
          "original_form": "딸가족이 올떄 같이 오니까 인원초과거든요",
          "speaker_id": "1",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100005945.1.21",
          "form": "아아...",
          "original_form": "아아...",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100005945.1.22",
          "form": "그래도 이번 추석은",
          "original_form": "그래도 이번 추석은",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100005945.1.23",
          "form": "인원제한이 풀려서 다행이네요",
          "original_form": "인원제한이 풀려서 다행이네요",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100005945.1.24",
          "form": "네 그래서 기대돼요",
          "original_form": "네 그래서 기대돼요",
          "speaker_id": "1",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100005945.1.25",
          "form": "다만 여전히 친정식구들 못만나서 아쉽네요",
          "original_form": "다만 여전히 친정식구들 못만나서 아쉽네요",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100005945.1.26",
          "form": "이래나 저래나 그래도 명절인데",
          "original_form": "이래나 저래나 그래도 명절인데",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100005945.1.27",
          "form": "참...",
          "original_form": "참...",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100005945.1.28",
          "form": "코로나라는게",
          "original_form": "코로나라는게",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100005945.1.29",
          "form": "작년부터 못가고 있어서 슬퍼요",
          "original_form": "작년부터 못가고 있어서 슬퍼요",
