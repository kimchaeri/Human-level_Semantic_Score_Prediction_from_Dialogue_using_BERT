      "utterance": [
        {
          "id": "MDRW2100033113.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100033113.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100033113.1.3",
          "form": "학교 다니세요?",
          "original_form": "학교 다니세요?",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100033113.1.4",
          "form": "네 다니죠 ㅎㅎ",
          "original_form": "네 다니죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100033113.1.5",
          "form": "음 그럼 어떤 과목 좋아하세요?",
          "original_form": "음 그럼 어떤 과목 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100033113.1.6",
          "form": "수학을 제일 좋아합니다",
          "original_form": "수학을 제일 좋아합니다",
          "speaker_id": "1",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100033113.1.7",
          "form": "국어는.. 젤 별루에요",
          "original_form": "국어는.. 젤 별루에요",
          "speaker_id": "1",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100033113.1.8",
          "form": "지금 중학생인데 고등학교 가면 국어를 어떻게 할지가 제일 고민이네요",
          "original_form": "지금 중학생인데 고등학교 가면 국어를 어떻게 할지가 제일 고민이네요",
          "speaker_id": "1",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100033113.1.9",
          "form": "아ㅜㅜ 국어",
          "original_form": "아ㅜㅜ 국어",
          "speaker_id": "2",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100033113.1.10",
          "form": "한국말이여도 어렵죠..",
          "original_form": "한국말이여도 어렵죠..",
          "speaker_id": "2",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100033113.1.11",
          "form": "그런데 요즘 온라인이다 보니까 수업에 집중도 안 되고.. 어떡할지 모르겠네요",
          "original_form": "그런데 요즘 온라인이다 보니까 수업에 집중도 안 되고.. 어떡할지 모르겠네요",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100033113.1.12",
          "form": "아이고.. 요즘은 학교가 온라인이니까 그렇겠네요",
          "original_form": "아이고.. 요즘은 학교가 온라인이니까 그렇겠네요",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033113.1.13",
          "form": "혹시 고등학생이신가요.",
          "original_form": "혹시 고등학생이신가요.",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033113.1.14",
          "form": "넵 고등학교.",
          "original_form": "넵 고등학교.",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033113.1.15",
          "form": "고등학교가 좀 다르긴 하죠",
          "original_form": "고등학교가 좀 다르긴 하죠",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033113.1.16",
          "form": "저한테 약간의 팁이라도 주실수 있나요?",
          "original_form": "저한테 약간의 팁이라도 주실수 있나요?",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033113.1.17",
          "form": "음 그러면",
          "original_form": "음 그러면",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033113.1.18",
          "form": "일단 책 많이 읽기.",
          "original_form": "일단 책 많이 읽기.",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033113.1.19",
          "form": "음.. 제가 책읽기를 잘 안하는데 노력해봐야겠네요",
          "original_form": "음.. 제가 책읽기를 잘 안하는데 노력해봐야겠네요",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033113.1.20",
          "form": "네 ㅋㅋㅋㅋ 나중에 오면 책 읽을 걸 하고 막 후회해여",
          "original_form": "네 ㅋㅋㅋㅋ 나중에 오면 책 읽을 걸 하고 막 후회해여",
          "speaker_id": "2",
          "time": "20210906 16:50"
        }
