      "utterance": [
        {
          "id": "MDRW2100018257.1.1",
          "form": "저희집은 부대찌개요",
          "original_form": "저희집은 부대찌개요",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018257.1.2",
          "form": "와 부대찌개",
          "original_form": "와 부대찌개",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018257.1.3",
          "form": "맛있었겠다",
          "original_form": "맛있었겠다",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018257.1.4",
          "form": "네 ㅎㅎ 싸고 편리하고 ㅎㅎ",
          "original_form": "네 ㅎㅎ 싸고 편리하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018257.1.5",
          "form": "직접 만드셨어요?",
          "original_form": "직접 만드셨어요?",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018257.1.6",
          "form": "담꾹이라고",
          "original_form": "담꾹이라고",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018257.1.7",
          "form": "밀키트로요 ㅎㅎ",
          "original_form": "밀키트로요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018257.1.8",
          "form": "엇 저희동네도 있는데",
          "original_form": "엇 저희동네도 있는데",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018257.1.9",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018257.1.10",
          "form": "먹을만래요",
          "original_form": "먹을만래요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018257.1.11",
          "form": "양은요?",
          "original_form": "양은요?",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018257.1.12",
          "form": "첨엔 적어보였는데",
          "original_form": "첨엔 적어보였는데",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018257.1.13",
          "form": "물넣고 끓이니까",
          "original_form": "물넣고 끓이니까",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018257.1.14",
          "form": "적당해요",
          "original_form": "적당해요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018257.1.15",
          "form": "오 얼마에요?",
          "original_form": "오 얼마에요?",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018257.1.16",
          "form": "9900원이요",
          "original_form": "9900원이요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018257.1.17",
          "form": "추천? 비추천?",
          "original_form": "추천? 비추천?",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018257.1.18",
          "form": "무난해요",
          "original_form": "무난해요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018257.1.19",
          "form": "저도 언젠가 사먹어봐야겠어요",
          "original_form": "저도 언젠가 사먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018257.1.20",
          "form": "편하긴 해요 ㅎㄹ",
          "original_form": "편하긴 해요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018257.1.21",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018257.1.22",
          "form": "저는 옛날통닭 먹었어요",
          "original_form": "저는 옛날통닭 먹었어요",
          "speaker_id": "1",
          "time": "20210826 23:27"
        }
      ]
