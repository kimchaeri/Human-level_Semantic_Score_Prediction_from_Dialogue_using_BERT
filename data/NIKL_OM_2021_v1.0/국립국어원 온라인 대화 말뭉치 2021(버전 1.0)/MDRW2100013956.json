      "utterance": [
        {
          "id": "MDRW2100013956.1.1",
          "form": "조금씩 거드는 정도만 해요",
          "original_form": "조금씩 거드는 정도만 해요",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013956.1.2",
          "form": "저도 뭐",
          "original_form": "저도 뭐",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100013956.1.3",
          "form": "어떤 집안일을 주로 거드세요?",
          "original_form": "어떤 집안일을 주로 거드세요?",
          "speaker_id": "2",
          "time": "20210826 01:10"
        },
        {
          "id": "MDRW2100013956.1.4",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210826 06:37"
        },
        {
          "id": "MDRW2100013956.1.5",
          "form": "저는 빨래를 거들어요",
          "original_form": "저는 빨래를 거들어요",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100013956.1.6",
          "form": "화장실전담이요",
          "original_form": "화장실전담이요",
          "speaker_id": "1",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100013956.1.7",
          "form": "청소하는 꿀팁이 있으신가요?",
          "original_form": "청소하는 꿀팁이 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100013956.1.8",
          "form": "락스조",
          "original_form": "락스조",
          "speaker_id": "1",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100013956.1.9",
          "form": "어느 정도 주기로 청소하세요?",
          "original_form": "어느 정도 주기로 청소하세요?",
          "speaker_id": "2",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100013956.1.10",
          "form": "일주일에한번씩",
          "original_form": "일주일에한번씩",
          "speaker_id": "1",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100013956.1.11",
          "form": "주기적으로 하시나봐요",
          "original_form": "주기적으로 하시나봐요",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100013956.1.12",
          "form": "네 곰팡이생길까보ㅓ",
          "original_form": "네 곰팡이생길까보ㅓ",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100013956.1.13",
          "form": "곰팡이 잘 관리해야하죠..",
          "original_form": "곰팡이 잘 관리해야하죠..",
