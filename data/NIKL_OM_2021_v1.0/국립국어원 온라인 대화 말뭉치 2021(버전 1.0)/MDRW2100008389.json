      "utterance": [
        {
          "id": "MDRW2100008389.1.1",
          "form": "휴 힘들었다.",
          "original_form": "휴 힘들었다.",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008389.1.2",
          "form": "혹시 질병(?) 있으신가요",
          "original_form": "혹시 질병(?) 있으신가요",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008389.1.3",
          "form": "저 눈이 안좋아서 안경써요",
          "original_form": "저 눈이 안좋아서 안경써요",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008389.1.4",
          "form": "헉 저도요",
          "original_form": "헉 저도요",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008389.1.5",
          "form": "마이너스세요?",
          "original_form": "마이너스세요?",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008389.1.6",
          "form": "전 난시가 넘 심해요",
          "original_form": "전 난시가 넘 심해요",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008389.1.7",
          "form": "그건 모르겠는데 한뼘거리 이상은 잘 안보여요",
          "original_form": "그건 모르겠는데 한뼘거리 이상은 잘 안보여요",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008389.1.8",
          "form": "헉 저처럼 많이 나쁘시군요",
          "original_form": "헉 저처럼 많이 나쁘시군요",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008389.1.9",
          "form": "혹시 집순이지만 홈트같은거 하시나요",
          "original_form": "혹시 집순이지만 홈트같은거 하시나요",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008389.1.10",
          "form": "저 달리기해요!",
          "original_form": "저 달리기해요!",
          "speaker_id": "2",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008389.1.11",
          "form": "홀 런데이 맞나요",
          "original_form": "홀 런데이 맞나요",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008389.1.12",
          "form": "헐 아세요!? 맞아요!!",
          "original_form": "헐 아세요!? 맞아요!!",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008389.1.13",
          "form": "이번주까지 7주차예요",
          "original_form": "이번주까지 7주차예요",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008389.1.14",
          "form": "헐 잠만",
          "original_form": "헐 잠만",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008389.1.15",
          "form": "좀 대단한 분이셨네요",
          "original_form": "좀 대단한 분이셨네요",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008389.1.16",
          "form": "그거 어케 7주나 하세요?!!",
          "original_form": "그거 어케 7주나 하세요?!!",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008389.1.17",
          "form": "주변에서 하는 사람이 있어서 나도 해야겠다고 생각하고 그냥 시작해버렸어요 ㅋㅋ",
          "original_form": "주변에서 하는 사람이 있어서 나도 해야겠다고 생각하고 그냥 시작해버렸어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008389.1.18",
          "form": "초보자 8주차 코스있거든요",
          "original_form": "초보자 8주차 코스있거든요",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008389.1.19",
          "form": "저 궁금한게 그거 코스를 따로 만들 수 있는 건가요 아니면",
          "original_form": "저 궁금한게 그거 코스를 따로 만들 수 있는 건가요 아니면",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008389.1.20",
          "form": "저도 추천 많이 해주더라고요 주변사람들이",
          "original_form": "저도 추천 많이 해주더라고요 주변사람들이",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008389.1.21",
          "form": "근데 뭔가",
          "original_form": "근데 뭔가",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008389.1.22",
          "form": "저도 추천합니다!",
          "original_form": "저도 추천합니다!",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008389.1.23",
          "form": "프로들만 한다는 느낌에 장벽이 좀 있어요",
          "original_form": "프로들만 한다는 느낌에 장벽이 좀 있어요",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008389.1.24",
          "form": "노노 초보자 코스 그대로 따라하면 30분 쉬지않고 달릴 수 있게된대요",
          "original_form": "노노 초보자 코스 그대로 따라하면 30분 쉬지않고 달릴 수 있게된대요",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008389.1.25",
          "form": "홀",
          "original_form": "홀",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008389.1.26",
          "form": "넘 좋네요",
          "original_form": "넘 좋네요",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008389.1.27",
          "form": "7주차에 좀 달라진 점이 있던가요",
          "original_form": "7주차에 좀 달라진 점이 있던가요",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008389.1.28",
          "form": "저도 처음엔 대충 1분 달리기 3분 걷기로 시작했거든요?",
          "original_form": "저도 처음엔 대충 1분 달리기 3분 걷기로 시작했거든요?",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008389.1.29",
          "form": "7주차에서는 이제 10분 달린 다음 3분 걷고 15분 달리기 성공했어요",
          "original_form": "7주차에서는 이제 10분 달린 다음 3분 걷고 15분 달리기 성공했어요",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008389.1.30",
          "form": "홀 체력 엄청 좋아지셨다!!!!!",
          "original_form": "홀 체력 엄청 좋아지셨다!!!!!",
          "speaker_id": "1",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008389.1.31",
          "form": "체력 는거 느끼시겠어요",
          "original_form": "체력 는거 느끼시겠어요",
          "speaker_id": "1",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008389.1.32",
          "form": "ㄹㅇ 영업당한다",
          "original_form": "ㄹㅇ 영업당한다",
          "speaker_id": "1",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008389.1.33",
          "form": "네!! 다음주에는 드디어 마지막 8주차예요",
          "original_form": "네!! 다음주에는 드디어 마지막 8주차예요",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008389.1.34",
          "form": "30분 쉬지않고 달리기 할 예정이요 ㄷㄷ",
          "original_form": "30분 쉬지않고 달리기 할 예정이요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008389.1.35",
          "form": "와 대박이다...!!!!",
          "original_form": "와 대박이다...!!!!",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008389.1.36",
          "form": "오 8주로 코스가 짜여있나요?!",
          "original_form": "오 8주로 코스가 짜여있나요?!",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008389.1.37",
          "form": "꽤나 프로패셔널",
          "original_form": "꽤나 프로패셔널",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008389.1.38",
          "form": "처음엔 1분 달리고... 그 다음엔 2분 30초 달리고... 이렇게 아주 천천히 단계가 올라가요",
          "original_form": "처음엔 1분 달리고... 그 다음엔 2분 30초 달리고... 이렇게 아주 천천히 단계가 올라가요",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008389.1.39",
          "form": "초보자 8주코스라서 이번에 종강하고 바로 시작해서 거의 끝나가네요 ㅋㅋ",
          "original_form": "초보자 8주코스라서 이번에 종강하고 바로 시작해서 거의 끝나가네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008389.1.40",
          "form": "헐 저번 학기보다 이번학기에 더 멋져진거군요",
          "original_form": "헐 저번 학기보다 이번학기에 더 멋져진거군요",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008389.1.41",
          "form": "쓰읍..자극됩니다",
          "original_form": "쓰읍..자극됩니다",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008389.1.42",
          "form": "열심히 살아야지",
          "original_form": "열심히 살아야지",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008389.1.43",
          "form": "응원혀요!!!!",
          "original_form": "응원혀요!!!!",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008389.1.44",
          "form": "런데이하면 친추하고 싶네요 ㅎㅎ",
          "original_form": "런데이하면 친추하고 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008389.1.45",
          "form": "친구가 러닝 시작하면 실시간으로 응원도 할 수 있거든요 기록보고 자극도 받고!",
          "original_form": "친구가 러닝 시작하면 실시간으로 응원도 할 수 있거든요 기록보고 자극도 받고!",
