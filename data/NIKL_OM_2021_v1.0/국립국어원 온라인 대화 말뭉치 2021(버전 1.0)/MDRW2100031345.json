      "utterance": [
        {
          "id": "MDRW2100031345.1.1",
          "form": "네 다시요",
          "original_form": "네 다시요",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031345.1.2",
          "form": "진로 소맥 잔 샀어요",
          "original_form": "진로 소맥 잔 샀어요",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031345.1.3",
          "form": "술을 자주 드시나요",
          "original_form": "술을 자주 드시나요",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031345.1.4",
          "form": "아뇨 잔이 예뻐서 샀어요",
          "original_form": "아뇨 잔이 예뻐서 샀어요",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031345.1.5",
          "form": "얼마나",
          "original_form": "얼마나",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031345.1.6",
          "form": "두꺼비가 그려져 있는데 너무 귀여워요",
          "original_form": "두꺼비가 그려져 있는데 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031345.1.7",
          "form": "두꺼비가 귀여워요",
          "original_form": "두꺼비가 귀여워요",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031345.1.8",
          "form": "예 개구린 지 두껍인지 귀여워요",
          "original_form": "예 개구린 지 두껍인지 귀여워요",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031345.1.9",
          "form": "저는 별로",
          "original_form": "저는 별로",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031345.1.10",
          "form": "계속 보다 면 정될 걸요",
          "original_form": "계속 보다 면 정될 걸요",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031345.1.11",
          "form": "딱 히히",
          "original_form": "딱 히히",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031345.1.12",
          "form": "그잔에 먹으면 왠지 맛도 좋아지는 것 같아요",
          "original_form": "그잔에 먹으면 왠지 맛도 좋아지는 것 같아요",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031345.1.13",
          "form": "그냥 느낌이 거에요",
          "original_form": "그냥 느낌이 거에요",
          "speaker_id": "2",
          "time": "20210904 20:41"
