      "utterance": [
        {
          "id": "MDRW2100008189.1.1",
          "form": "딱히 없는듯",
          "original_form": "딱히 없는듯",
          "speaker_id": "2",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008189.1.2",
          "form": "요즘 저는 이것저것 보고 있는데",
          "original_form": "요즘 저는 이것저것 보고 있는데",
          "speaker_id": "1",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008189.1.3",
          "form": "늘...이것저것 보지만",
          "original_form": "늘...이것저것 보지만",
          "speaker_id": "1",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008189.1.4",
          "form": "저는 뭔가 앞뒤가 딱딱맞아서 헉 그게 그거였구니 할 때 전율을 느끼는 거 같아요",
          "original_form": "저는 뭔가 앞뒤가 딱딱맞아서 헉 그게 그거였구니 할 때 전율을 느끼는 거 같아요",
          "speaker_id": "1",
          "time": "20210822 14:00"
        },
        {
          "id": "MDRW2100008189.1.5",
          "form": "그런소설 요즘 없음ㅠ",
          "original_form": "그런소설 요즘 없음ㅠ",
          "speaker_id": "2",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100008189.1.6",
          "form": "맞아요... ㅠㅠㅠㅠ 재미있는 소설은 다 재밌게 볼때쯤 유료연재로 납치당해서...",
          "original_form": "맞아요... ㅠㅠㅠㅠ 재미있는 소설은 다 재밌게 볼때쯤 유료연재로 납치당해서...",
          "speaker_id": "1",
          "time": "20210822 14:03"
        },
        {
          "id": "MDRW2100008189.1.7",
          "form": "그렇기도 하고 내용도 비슷해서",
          "original_form": "그렇기도 하고 내용도 비슷해서",
          "speaker_id": "2",
          "time": "20210822 14:03"
        },
        {
          "id": "MDRW2100008189.1.8",
          "form": "헷갈려",
          "original_form": "헷갈려",
          "speaker_id": "2",
          "time": "20210822 14:04"
        },
        {
          "id": "MDRW2100008189.1.9",
          "form": "소설 발굴하기 힘들다",
          "original_form": "소설 발굴하기 힘들다",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100008189.1.10",
          "form": "내용뿐만아니라 제목도 비슷해서",
          "original_form": "내용뿐만아니라 제목도 비슷해서",
          "speaker_id": "1",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008189.1.11",
          "form": "이제는 유연 넘어간거 나중에 출판되면 사서 봐야지 ㅎㅎ 하고 기다리는 것도",
          "original_form": "이제는 유연 넘어간거 나중에 출판되면 사서 봐야지 ㅎㅎ 하고 기다리는 것도",
          "speaker_id": "1",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008189.1.12",
          "form": "못하겠어요 ㅋㅋㅋㅋㅋ그 제목이 그 제목이라...",
          "original_form": "못하겠어요 ㅋㅋㅋㅋㅋ그 제목이 그 제목이라...",
          "speaker_id": "1",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008189.1.13",
          "form": "이게 바로 나오면 좋은데 몇달 걸리면 잊어버려",
          "original_form": "이게 바로 나오면 좋은데 몇달 걸리면 잊어버려",
          "speaker_id": "2",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008189.1.14",
          "form": "소설공지 해도 본문지워지면 애가 뭐였는지 모르겠고",
          "original_form": "소설공지 해도 본문지워지면 애가 뭐였는지 모르겠고",
          "speaker_id": "2",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008189.1.15",
          "form": "맞아요 솔직히 본문 다 지우지 말고 1~2화 프롤로그 정도는",
          "original_form": "맞아요 솔직히 본문 다 지우지 말고 1~2화 프롤로그 정도는",
          "speaker_id": "1",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008189.1.16",
          "form": "남겨주면 좋겠는데",
          "original_form": "남겨주면 좋겠는데",
          "speaker_id": "1",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008189.1.17",
          "form": "예전엔 그래도 남기는 추세였는데 요즘은 출판사 요구가 있는지",
          "original_form": "예전엔 그래도 남기는 추세였는데 요즘은 출판사 요구가 있는지",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008189.1.18",
          "form": "ㅇㅈ 물어보까",
          "original_form": "ㅇㅈ 물어보까",
          "speaker_id": "2",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008189.1.19",
          "form": "1편도 안남기고 싹 긁어가더라구요",
          "original_form": "1편도 안남기고 싹 긁어가더라구요",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008189.1.20",
          "form": "ㅋㅋㅋㅋㄱ 아직 대화중인데",
          "original_form": "ㅋㅋㅋㅋㄱ 아직 대화중인데",
          "speaker_id": "2",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008189.1.21",
          "form": "물어바욬ㅋㅋㅋㅋㅋ",
          "original_form": "물어바욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008189.1.22",
          "form": "질문중 아니 넘나 궁금",
          "original_form": "질문중 아니 넘나 궁금",
          "speaker_id": "2",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008189.1.23",
          "form": "저도요 왜 1편도 안남겨서 따라가고싶은 독자도 떨궈내는거얏",
          "original_form": "저도요 왜 1편도 안남겨서 따라가고싶은 독자도 떨궈내는거얏",
          "speaker_id": "1",
          "time": "20210822 14:11"
        },
        {
          "id": "MDRW2100008189.1.24",
          "form": "기본적으로 습작만 요청한다고하네",
          "original_form": "기본적으로 습작만 요청한다고하네",
          "speaker_id": "2",
          "time": "20210822 14:11"
        },
        {
          "id": "MDRW2100008189.1.25",
          "form": "대답어려운거면 주제 바꾸자했는데",
          "original_form": "대답어려운거면 주제 바꾸자했는데",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008189.1.26",
          "form": "바꾸심 ㅎ",
          "original_form": "바꾸심 ㅎ",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008189.1.27",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "1",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008189.1.28",
          "form": "주륵...아무튼 따라가기 어려운 작품들이 많아가지고 ㅎ",
          "original_form": "주륵...아무튼 따라가기 어려운 작품들이 많아가지고 ㅎ",
          "speaker_id": "1",
          "time": "20210822 14:14"
        },
        {
          "id": "MDRW2100008189.1.29",
          "form": "그런거 다 놓치다보면",
          "original_form": "그런거 다 놓치다보면",
          "speaker_id": "1",
          "time": "20210822 14:14"
        },
        {
          "id": "MDRW2100008189.1.30",
          "form": "인정...그래서 문피아를 봄",
          "original_form": "인정...그래서 문피아를 봄",
          "speaker_id": "2",
          "time": "20210822 14:14"
        },
        {
          "id": "MDRW2100008189.1.31",
          "form": "제 손안에 들어온거 얼마없어요",
          "original_form": "제 손안에 들어온거 얼마없어요",
          "speaker_id": "1",
          "time": "20210822 14:14"
        },
        {
          "id": "MDRW2100008189.1.32",
          "form": "거기서 그대로 유연가니",
          "original_form": "거기서 그대로 유연가니",
          "speaker_id": "2",
          "time": "20210822 14:14"
        },
        {
          "id": "MDRW2100008189.1.33",
          "form": "전 근데 유연은 편당결제 시스템이",
          "original_form": "전 근데 유연은 편당결제 시스템이",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008189.1.34",
          "form": "저한테 부담돼서",
          "original_form": "저한테 부담돼서",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008189.1.35",
          "form": "전 이북으로 엮어서 권당 결제가 낫거든요...",
          "original_form": "전 이북으로 엮어서 권당 결제가 낫거든요...",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008189.1.36",
          "form": "그래서 유연 못따라감여 ㅠ",
          "original_form": "그래서 유연 못따라감여 ㅠ",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008189.1.37",
          "form": "맞아 그게 좋아",
          "original_form": "맞아 그게 좋아",
          "speaker_id": "2",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008189.1.38",
          "form": "걍 마음을 비우고 보는자",
          "original_form": "걍 마음을 비우고 보는자",
          "speaker_id": "2",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008189.1.39",
          "form": "저도 걍..",
          "original_form": "저도 걍..",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008189.1.40",
          "form": "오는 작품 안막고...가는 작품 안막고..",
          "original_form": "오는 작품 안막고...가는 작품 안막고..",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008189.1.41",
          "form": "그냥 흘러가는 대로 보는 것 같아요",
          "original_form": "그냥 흘러가는 대로 보는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008189.1.42",
          "form": "제목이나 그런게 인상깊고 꼭 보고싶은거는 기다려서 결국 사지만요ㅠㅋㅋㅋ",
          "original_form": "제목이나 그런게 인상깊고 꼭 보고싶은거는 기다려서 결국 사지만요ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:17"
        },
        {
          "id": "MDRW2100008189.1.43",
          "form": "영죽 ㅋㅋ",
          "original_form": "영죽 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:17"
        },
        {
          "id": "MDRW2100008189.1.44",
          "form": "영죽 ㅇㅈㅋㅋㅋㅋㅋㅋㅋㅋ 재밌어욧",
          "original_form": "영죽 ㅇㅈㅋㅋㅋㅋㅋㅋㅋㅋ 재밌어욧",
          "speaker_id": "1",
          "time": "20210822 14:19"
        },
        {
          "id": "MDRW2100008189.1.45",
          "form": "외전 더 내달라...",
          "original_form": "외전 더 내달라...",
          "speaker_id": "1",
          "time": "20210822 14:19"
        },
        {
          "id": "MDRW2100008189.1.46",
          "form": "더 달라",
          "original_form": "더 달라",
          "speaker_id": "2",
          "time": "20210822 14:20"
        }
      ]
