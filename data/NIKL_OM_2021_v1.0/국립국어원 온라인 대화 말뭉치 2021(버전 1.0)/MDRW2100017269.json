      "utterance": [
        {
          "id": "MDRW2100017269.1.1",
          "form": "저랑은 좀 안맞았어요",
          "original_form": "저랑은 좀 안맞았어요",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017269.1.2",
          "form": "그래도 대소변 자동",
          "original_form": "그래도 대소변 자동",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017269.1.3",
          "form": "항상 조용",
          "original_form": "항상 조용",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017269.1.4",
          "form": "그건 좋더라구요",
          "original_form": "그건 좋더라구요",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017269.1.5",
          "form": "이런건 좋았어요",
          "original_form": "이런건 좋았어요",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017269.1.6",
          "form": "훈련시킬 필요가없으니까",
          "original_form": "훈련시킬 필요가없으니까",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017269.1.7",
          "form": "목욕도 필요없고",
          "original_form": "목욕도 필요없고",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017269.1.8",
          "form": "항상 깔끔하고",
          "original_form": "항상 깔끔하고",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017269.1.9",
          "form": "산책도 필요없고",
          "original_form": "산책도 필요없고",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017269.1.10",
          "form": "하지만 전 개가 더 좋아요",
          "original_form": "하지만 전 개가 더 좋아요",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017269.1.11",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017269.1.12",
          "form": "저도 그래요ㅎ",
          "original_form": "저도 그래요ㅎ",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017269.1.13",
          "form": "강아지가 더 손이 많이 가요 ㅎㅎ",
          "original_form": "강아지가 더 손이 많이 가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017269.1.14",
          "form": "전 강아지도 고양이만큼 예뻐보여서",
          "original_form": "전 강아지도 고양이만큼 예뻐보여서",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017269.1.15",
          "form": "맞아요 신경쓸게많아요",
          "original_form": "맞아요 신경쓸게많아요",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017269.1.16",
          "form": "강아지파에요",
          "original_form": "강아지파에요",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017269.1.17",
          "form": "근데 고양이 키워도",
          "original_form": "근데 고양이 키워도",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017269.1.18",
          "form": "그 성질 다 받아주려면",
          "original_form": "그 성질 다 받아주려면",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017269.1.19",
          "form": "정신적으로 소비가 큰 것 같아요 ㅎㅎ",
          "original_form": "정신적으로 소비가 큰 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017269.1.20",
          "form": "근데 고양이 나름이긴해요",
          "original_form": "근데 고양이 나름이긴해요",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017269.1.21",
          "form": "전 진짜 너무너무 힘들었어요",
          "original_form": "전 진짜 너무너무 힘들었어요",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017269.1.22",
          "form": "그렇겠죠...?",
          "original_form": "그렇겠죠...?",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017269.1.23",
          "form": "저희누나 키우는 애는",
          "original_form": "저희누나 키우는 애는",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017269.1.24",
          "form": "사람만 싫어하지 애가 덤덤해서",
          "original_form": "사람만 싫어하지 애가 덤덤해서",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017269.1.25",
          "form": "제가 본 유일한 고양이가 걔라서...",
          "original_form": "제가 본 유일한 고양이가 걔라서...",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017269.1.26",
          "form": "아앗..하필..",
          "original_form": "아앗..하필..",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017269.1.27",
          "form": "아아 덤덤했구나",
          "original_form": "아아 덤덤했구나",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017269.1.28",
          "form": "근데 개도 비슷해요",
          "original_form": "근데 개도 비슷해요",
          "speaker_id": "1",
          "time": "20210826 18:26"
        }
      ]
    }
  ]
