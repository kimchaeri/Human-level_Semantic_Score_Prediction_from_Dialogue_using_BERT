      "utterance": [
        {
          "id": "MDRW2100024210.1.1",
          "form": "차는 좋아하는거 있으세요?",
          "original_form": "차는 좋아하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024210.1.2",
          "form": "저는 요즘 민트차 위주로 마시는데 뱃속 꾸룩꾸룩할때 마시면 좋다고 해서 마셨거든요?",
          "original_form": "저는 요즘 민트차 위주로 마시는데 뱃속 꾸룩꾸룩할때 마시면 좋다고 해서 마셨거든요?",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024210.1.3",
          "form": "근데 진짜 효과봤어요",
          "original_form": "근데 진짜 효과봤어요",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024210.1.4",
          "form": "오ㅋㅋㅋ확실히 좋나요?",
          "original_form": "오ㅋㅋㅋ확실히 좋나요?",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024210.1.5",
          "form": "네 안먹을때랑 달라요",
          "original_form": "네 안먹을때랑 달라요",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024210.1.6",
          "form": "건강에 좋은 차 또 추천해주세요ㅋㅋ",
          "original_form": "건강에 좋은 차 또 추천해주세요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024210.1.7",
          "form": "ㅋㅋㅋㅋ 손발 차시면 대추차라도 드시라고 하고 싶네요",
          "original_form": "ㅋㅋㅋㅋ 손발 차시면 대추차라도 드시라고 하고 싶네요",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024210.1.8",
          "form": "카페가서 한 번 마셔보고, 좋으면 저도 마셔야겠어요",
          "original_form": "카페가서 한 번 마셔보고, 좋으면 저도 마셔야겠어요",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024210.1.9",
          "form": "도라지랑 대추 좋대요",
          "original_form": "도라지랑 대추 좋대요",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024210.1.10",
          "form": "꿀도라지차는 겨울마다 자주 마셔요",
          "original_form": "꿀도라지차는 겨울마다 자주 마셔요",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024210.1.11",
          "form": "기관지에 좋더라구요",
          "original_form": "기관지에 좋더라구요",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024210.1.12",
          "form": "손발도 차서 따뜻한걸 마셔야되는데 맨날 차가운거만 먹게되네요",
          "original_form": "손발도 차서 따뜻한걸 마셔야되는데 맨날 차가운거만 먹게되네요",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024210.1.13",
          "form": "맞아요 도라지는 목에 좋은거 같긴 해요",
          "original_form": "맞아요 도라지는 목에 좋은거 같긴 해요",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024210.1.14",
          "form": "아이스크림 좋아하세요?",
          "original_form": "아이스크림 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024210.1.15",
          "form": "아이스크림 너무 좋아요ㅋㅋ",
          "original_form": "아이스크림 너무 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024210.1.16",
          "form": "달달한건 다 좋아해요",
          "original_form": "달달한건 다 좋아해요",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024210.1.17",
          "form": "저도요 ㅋㅋ 치즈 들어간거 다 좋아요",
          "original_form": "저도요 ㅋㅋ 치즈 들어간거 다 좋아요",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024210.1.18",
          "form": "요즘 특이한 아이스크림 많아서 좋아요",
          "original_form": "요즘 특이한 아이스크림 많아서 좋아요",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024210.1.19",
          "form": "아이스크림은 그냥 빙과류도 좋고 다 좋아요",
          "original_form": "아이스크림은 그냥 빙과류도 좋고 다 좋아요",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024210.1.20",
          "form": "여러가지 콜라보된 아이스크림도 좋고요",
          "original_form": "여러가지 콜라보된 아이스크림도 좋고요",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024210.1.21",
          "form": "요즘 뭐 특이한거 있나요?",
          "original_form": "요즘 뭐 특이한거 있나요?",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024210.1.22",
          "form": "빵빠레카스테라 맛있더라구요ㅋㅋ",
          "original_form": "빵빠레카스테라 맛있더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024210.1.23",
          "form": "빵빠레 카스테라 맛있을거 같아요 처음들었어요",
          "original_form": "빵빠레 카스테라 맛있을거 같아요 처음들었어요",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024210.1.24",
          "form": "빵빠레 진짜 맛있는데 올해는 한번도 못먹었네요",
          "original_form": "빵빠레 진짜 맛있는데 올해는 한번도 못먹었네요",
          "speaker_id": "1",
          "time": "20210830 23:20"
        }
      ]
