      "utterance": [
        {
          "id": "MDRW2100008880.1.1",
          "form": "다이어트 해본 적 있으세요?",
          "original_form": "다이어트 해본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008880.1.2",
          "form": "네ㅋㅋ지금 다이어트중이에요...",
          "original_form": "네ㅋㅋ지금 다이어트중이에요...",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008880.1.3",
          "form": "저도 해야하는데..!ㅠㅠ 잘 돼가세요?",
          "original_form": "저도 해야하는데..!ㅠㅠ 잘 돼가세요?",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008880.1.4",
          "form": "음..일단 몇개월동안.. 4키로?빠졌어요ㅎ",
          "original_form": "음..일단 몇개월동안.. 4키로?빠졌어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008880.1.5",
          "form": "작년부터 점점 찌더니 8키로가 쪘는데ㅜ 반 줄였네요ㅜ",
          "original_form": "작년부터 점점 찌더니 8키로가 쪘는데ㅜ 반 줄였네요ㅜ",
          "speaker_id": "2",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008880.1.6",
          "form": "와 축하드려요!! 운동으로 빼셨어요?",
          "original_form": "와 축하드려요!! 운동으로 빼셨어요?",
          "speaker_id": "1",
          "time": "20210822 19:40"
        },
        {
          "id": "MDRW2100008880.1.7",
          "form": "고맙습니다ㅋㅋㅋ음..일단 운동보다는 군것질을 끊으니까 운동보다 더 효과가 있더라구요",
          "original_form": "고맙습니다ㅋㅋㅋ음..일단 운동보다는 군것질을 끊으니까 운동보다 더 효과가 있더라구요",
          "speaker_id": "2",
          "time": "20210822 19:44"
        },
        {
          "id": "MDRW2100008880.1.8",
          "form": "운동은 다리운동 복근운동 정도...",
          "original_form": "운동은 다리운동 복근운동 정도...",
          "speaker_id": "2",
          "time": "20210822 19:44"
        },
        {
          "id": "MDRW2100008880.1.9",
          "form": "아 정말 다이어트하려면 단 거는 금지인가봐요",
          "original_form": "아 정말 다이어트하려면 단 거는 금지인가봐요",
          "speaker_id": "1",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008880.1.10",
          "form": "저도 운동하면 상체는 안하고 하체랑 복근만 해요ㅋㅋㅋ",
          "original_form": "저도 운동하면 상체는 안하고 하체랑 복근만 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008880.1.11",
          "form": "상체는 뭐... 알아서 빠지겠지...하고 안하게 돼요ㅋㅋㅋ",
          "original_form": "상체는 뭐... 알아서 빠지겠지...하고 안하게 돼요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:52"
        },
        {
          "id": "MDRW2100008880.1.12",
          "form": "ㅋㅋㅋ맞아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ맞아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:55"
        },
        {
          "id": "MDRW2100008880.1.13",
          "form": "그리고 하체랑 복근해도 상체가 안 힘든 건 아니라서..",
          "original_form": "그리고 하체랑 복근해도 상체가 안 힘든 건 아니라서..",
          "speaker_id": "1",
          "time": "20210822 19:56"
        },
        {
          "id": "MDRW2100008880.1.14",
          "form": "ㅋㅋㅋ복근도 결국엔 상체죠 뭐!",
          "original_form": "ㅋㅋㅋ복근도 결국엔 상체죠 뭐!",
          "speaker_id": "2",
          "time": "20210822 19:57"
        },
        {
          "id": "MDRW2100008880.1.15",
          "form": "전 일단 8키로 찐 거 빼고...더 뺄 수 있으면 딱 채워서 총 10키로 빼고싶어요",
          "original_form": "전 일단 8키로 찐 거 빼고...더 뺄 수 있으면 딱 채워서 총 10키로 빼고싶어요",
          "speaker_id": "2",
          "time": "20210822 19:58"
        },
        {
          "id": "MDRW2100008880.1.16",
          "form": "매우 어렵겠지만요ㅜ",
          "original_form": "매우 어렵겠지만요ㅜ",
          "speaker_id": "2",
          "time": "20210822 19:58"
        },
        {
          "id": "MDRW2100008880.1.17",
          "form": "맞아요 결국 다 같이 빠질거에요ㅋㅋㅋㅋ",
          "original_form": "맞아요 결국 다 같이 빠질거에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:05"
        },
        {
          "id": "MDRW2100008880.1.18",
          "form": "그래도 이대로만 하면 꼭 원하는 결과 얻으실거에요!!",
