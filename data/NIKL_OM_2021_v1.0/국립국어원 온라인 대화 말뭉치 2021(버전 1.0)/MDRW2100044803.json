        },
        {
          "id": "MDRW2100044803.1.2",
          "form": "녹두꽃 한예리 나오고 한거 기억나네요",
          "original_form": "녹두꽃 한예리 나오고 한거 기억나네요",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044803.1.3",
          "form": "녹두곷",
          "original_form": "녹두곷",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044803.1.4",
          "form": "드라마 제대로 보지는 않았지만",
          "original_form": "드라마 제대로 보지는 않았지만",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044803.1.5",
          "form": "48부작",
          "original_form": "48부작",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044803.1.6",
          "form": "꽤 길었네요?",
          "original_form": "꽤 길었네요?",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044803.1.7",
          "form": "맞아 하예리가 여주",
          "original_form": "맞아 하예리가 여주",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044803.1.8",
          "form": "시청률 좀 나온거로 기억나요",
          "original_form": "시청률 좀 나온거로 기억나요",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044803.1.9",
          "form": "윤시윤도 나오고",
          "original_form": "윤시윤도 나오고",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044803.1.10",
          "form": "아 그래요?  윤시윤도 나왔네요",
          "original_form": "아 그래요?  윤시윤도 나왔네요",
          "speaker_id": "1",
          "time": "20210907 23:51"
