      "utterance": [
        {
          "id": "MDRW2100029519.1.1",
          "form": "존마시죠",
          "original_form": "존마시죠",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.2",
          "form": "존맛",
          "original_form": "존맛",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.3",
          "form": "녹차맛이 최고입닏ㆍ",
          "original_form": "녹차맛이 최고입닏ㆍ",
          "speaker_id": "1",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.4",
          "form": "입니다",
          "original_form": "입니다",
          "speaker_id": "1",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.5",
          "form": "그건 찬음료죠?",
          "original_form": "그건 찬음료죠?",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.6",
          "form": "녹차는 진리죠",
          "original_form": "녹차는 진리죠",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.7",
          "form": "얼음을 갈았어요",
          "original_form": "얼음을 갈았어요",
          "speaker_id": "1",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.8",
          "form": "말차도 맛나죠",
          "original_form": "말차도 맛나죠",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.9",
          "form": "갈갈",
          "original_form": "갈갈",
          "speaker_id": "1",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.10",
          "form": "녹차랑 비슷하던데요",
          "original_form": "녹차랑 비슷하던데요",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.11",
          "form": "말차랑 녹차랑 다름이 무엇",
          "original_form": "말차랑 녹차랑 다름이 무엇",
          "speaker_id": "1",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.12",
          "form": "다른건가요 같은건가요",
          "original_form": "다른건가요 같은건가요",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029519.1.13",
          "form": "종자가 다르다는데 구분 모답니다",
          "original_form": "종자가 다르다는데 구분 모답니다",
          "speaker_id": "1",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100029519.1.14",
          "form": "역시 맛이똑같았어요",
          "original_form": "역시 맛이똑같았어요",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100029519.1.15",
          "form": "달달한 음료도 좋죠",
          "original_form": "달달한 음료도 좋죠",
          "speaker_id": "1",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100029519.1.16",
          "form": "오늘은 버블티가 먹고싶네요",
          "original_form": "오늘은 버블티가 먹고싶네요",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100029519.1.17",
          "form": "초콜릿이나 카라멜맛",
          "original_form": "초콜릿이나 카라멜맛",
          "speaker_id": "1",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100029519.1.18",
          "form": "전과일",
          "original_form": "전과일",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100029519.1.19",
          "form": "사하라 존맛",
          "original_form": "사하라 존맛",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100029519.1.20",
          "form": "버블티는 아마스빈이죠",
          "original_form": "버블티는 아마스빈이죠",
          "speaker_id": "1",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100029519.1.21",
          "form": "저는 타로맛좋아해요",
          "original_form": "저는 타로맛좋아해요",
          "speaker_id": "1",
          "time": "20210903 09:56"
        }
      ]
