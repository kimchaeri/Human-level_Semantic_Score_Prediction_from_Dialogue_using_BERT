      "utterance": [
        {
          "id": "MDRW2100026412.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026412.1.2",
          "form": "주부세요?",
          "original_form": "주부세요?",
          "speaker_id": "1",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026412.1.3",
          "form": "아니요 아직 결혼 안했어요~",
          "original_form": "아니요 아직 결혼 안했어요~",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026412.1.4",
          "form": "그치만 집안일도 많이하고 부모님하고 같이 살아서 구매하는거나 이런거도 제가 많이해요",
          "original_form": "그치만 집안일도 많이하고 부모님하고 같이 살아서 구매하는거나 이런거도 제가 많이해요",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026412.1.5",
          "form": "아 그러시군요 ㅋ",
          "original_form": "아 그러시군요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026412.1.6",
          "form": "저도 결혼전에 일하고 집에 와서 매일 청소하고 그랫어요.",
          "original_form": "저도 결혼전에 일하고 집에 와서 매일 청소하고 그랫어요.",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026412.1.7",
          "form": "부모님이 맞벌이셨거든요",
          "original_form": "부모님이 맞벌이셨거든요",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026412.1.8",
          "form": "그렇군요~~~",
          "original_form": "그렇군요~~~",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026412.1.9",
          "form": "요새 부모님께서 코로나라 집에 계시는데",
          "original_form": "요새 부모님께서 코로나라 집에 계시는데",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026412.1.10",
          "form": "저만 일하고..",
          "original_form": "저만 일하고..",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026412.1.11",
          "form": "그래도 집에오면 편히 쉴수가 없네요~",
          "original_form": "그래도 집에오면 편히 쉴수가 없네요~",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026412.1.12",
          "form": "아 코로나때문에 일을 조금 쉬고계신가봐요",
          "original_form": "아 코로나때문에 일을 조금 쉬고계신가봐요",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026412.1.13",
          "form": "고생하시네요 ㅠ",
          "original_form": "고생하시네요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026412.1.14",
          "form": "네 코로나가 빨리 끝나야 할텐데 말이에요~~~",
          "original_form": "네 코로나가 빨리 끝나야 할텐데 말이에요~~~",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026412.1.15",
          "form": "이번엔 갑자기 밥솥이 고장나서 새로 구매하는데 고생했어요",
          "original_form": "이번엔 갑자기 밥솥이 고장나서 새로 구매하는데 고생했어요",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026412.1.16",
          "form": "아고 그러셨군요 ㅠㅠ",
          "original_form": "아고 그러셨군요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026412.1.17",
          "form": "밥솥도 비싼데 그쵸 ㅠ",
          "original_form": "밥솥도 비싼데 그쵸 ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026412.1.18",
          "form": "맞아용 가격이 천차만별이라서 고르는데 오래걸리더라구요",
          "original_form": "맞아용 가격이 천차만별이라서 고르는데 오래걸리더라구요",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026412.1.19",
          "form": "브랜드부터 시작해서 밥솥 레벨 선택하고 디자인 선택하고 등등..",
          "original_form": "브랜드부터 시작해서 밥솥 레벨 선택하고 디자인 선택하고 등등..",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026412.1.20",
          "form": "맞아요 ㅋㅋ 밥솥 종류도 너무 많죠 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ 밥솥 종류도 너무 많죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026412.1.21",
          "form": "후기도 봐야되고",
          "original_form": "후기도 봐야되고",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026412.1.22",
          "form": "네 맞아요 후기도 꼭 봐야해여",
          "original_form": "네 맞아요 후기도 꼭 봐야해여",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026412.1.23",
          "form": "그래도 잘하시네요 ㅋㅋㅋ",
          "original_form": "그래도 잘하시네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026412.1.24",
          "form": "저는 결혼전에 그런거 잘 몰랐는데 ㅋㅋ",
          "original_form": "저는 결혼전에 그런거 잘 몰랐는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026412.1.25",
          "form": "결혼은 하신지 오래됐나요?",
          "original_form": "결혼은 하신지 오래됐나요?",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026412.1.26",
          "form": "네 9년째에요 ㅋㅋ",
          "original_form": "네 9년째에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:28"
        }
      ]
    }
