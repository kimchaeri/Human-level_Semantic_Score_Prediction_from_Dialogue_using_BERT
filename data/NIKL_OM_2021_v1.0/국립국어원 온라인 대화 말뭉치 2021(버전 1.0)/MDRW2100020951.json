      "utterance": [
        {
          "id": "MDRW2100020951.1.1",
          "form": "응 해가 엄청 강렬해",
          "original_form": "응 해가 엄청 강렬해",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100020951.1.2",
          "form": "ㅇㅇ 난 집에있는데도 덥더라",
          "original_form": "ㅇㅇ 난 집에있는데도 덥더라",
          "speaker_id": "1",
          "time": "20210828 15:04"
        },
        {
          "id": "MDRW2100020951.1.3",
          "form": "아까 바에서는 안더웠는데\r\n밖이 장난 아니네",
          "original_form": "아까 바에서는 안더웠는데\r\n밖이 장난 아니네",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100020951.1.4",
          "form": "나 지금 걷고 있는데 생각보단 바람 부는거 같긴하지만 더워ㅋ",
          "original_form": "나 지금 걷고 있는데 생각보단 바람 부는거 같긴하지만 더워ㅋ",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100020951.1.5",
          "form": "어여 숙사 들어가야되겠다",
          "original_form": "어여 숙사 들어가야되겠다",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100020951.1.6",
          "form": "여름 끝난줄 알았는데 덥네",
          "original_form": "여름 끝난줄 알았는데 덥네",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100020951.1.7",
          "form": "한시간쯤 걸릴듯\r\n운동삼아 걸어 돌아가는거라",
          "original_form": "한시간쯤 걸릴듯\r\n운동삼아 걸어 돌아가는거라",
          "speaker_id": "2",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100020951.1.8",
          "form": "날씨는 한여름같아ㅋㅋ",
          "original_form": "날씨는 한여름같아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100020951.1.9",
          "form": "우와 좋다 걷기 넘 좋지",
          "original_form": "우와 좋다 걷기 넘 좋지",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100020951.1.10",
          "form": "난 여름 되고 나서는 걷기 포기함",
          "original_form": "난 여름 되고 나서는 걷기 포기함",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100020951.1.11",
          "form": "땀 나는게 너무 싫어서",
          "original_form": "땀 나는게 너무 싫어서",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100020951.1.12",
          "form": "맞어 그래도 바람이 좀 간간히 불어서 걸을만은 하네",
          "original_form": "맞어 그래도 바람이 좀 간간히 불어서 걸을만은 하네",
          "speaker_id": "2",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100020951.1.13",
          "form": "ㅇㅇ바람불어서 다행이다",
          "original_form": "ㅇㅇ바람불어서 다행이다",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100020951.1.14",
          "form": "응 일자 길이라 길 잃어버릴 일도 없어ㅎ",
          "original_form": "응 일자 길이라 길 잃어버릴 일도 없어ㅎ",
          "speaker_id": "2",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100020951.1.15",
          "form": "이럴때 건강관리도 하고 풍경도 보는거지",
          "original_form": "이럴때 건강관리도 하고 풍경도 보는거지",
          "speaker_id": "1",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100020951.1.16",
          "form": "품경ㅋㅋ공장 풍경ㅋㅋ",
          "original_form": "품경ㅋㅋ공장 풍경ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:18"
        },
        {
          "id": "MDRW2100020951.1.17",
          "form": "헐 공장 ㅋㅋㅋ",
          "original_form": "헐 공장 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100020951.1.18",
          "form": "구미가 공장단지잖아ㅎ",
          "original_form": "구미가 공장단지잖아ㅎ",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100020951.1.19",
          "form": "공장단지를 걸어가는중",
          "original_form": "공장단지를 걸어가는중",
          "speaker_id": "2",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100020951.1.20",
          "form": "ㅋㅋ 그래도 나무도 있지않나",
          "original_form": "ㅋㅋ 그래도 나무도 있지않나",
          "speaker_id": "1",
          "time": "20210828 15:21"
        }
