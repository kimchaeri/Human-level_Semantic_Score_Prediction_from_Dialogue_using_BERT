      "utterance": [
        {
          "id": "MDRW2100023591.1.1",
          "form": "애들은 수업 다 끝났어?",
          "original_form": "애들은 수업 다 끝났어?",
          "speaker_id": "1",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100023591.1.2",
          "form": "ㅇㅇ 다 끝났어 방금",
          "original_form": "ㅇㅇ 다 끝났어 방금",
          "speaker_id": "2",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100023591.1.3",
          "form": "ㅎㅎ 고생 많았네",
          "original_form": "ㅎㅎ 고생 많았네",
          "speaker_id": "1",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100023591.1.4",
          "form": "내가 고생 많았어 ㅋㅋ 숙제가 많아서",
          "original_form": "내가 고생 많았어 ㅋㅋ 숙제가 많아서",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023591.1.5",
          "form": "이런이런.. 또 엄마 숙제였어?",
          "original_form": "이런이런.. 또 엄마 숙제였어?",
          "speaker_id": "1",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023591.1.6",
          "form": "방학동안 있었던 일 15가지 그리기 ㅋㅋㅋ",
          "original_form": "방학동안 있었던 일 15가지 그리기 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023591.1.7",
          "form": "훌.. 한가지도 아니고 15가지?",
          "original_form": "훌.. 한가지도 아니고 15가지?",
          "speaker_id": "1",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023591.1.8",
          "form": "코로나 때문에 집에만 있었는데 ㅠ 진짜 15개 겨우겨우 채움",
          "original_form": "코로나 때문에 집에만 있었는데 ㅠ 진짜 15개 겨우겨우 채움",
          "speaker_id": "2",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100023591.1.9",
          "form": "하느라 시간 오래걸렸겠네",
          "original_form": "하느라 시간 오래걸렸겠네",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100023591.1.10",
          "form": "15개 방학동안 한 일만 같이 생각해주고 나머지는 스스로 하는거지 뭐 ㅎㅎ",
          "original_form": "15개 방학동안 한 일만 같이 생각해주고 나머지는 스스로 하는거지 뭐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100023591.1.11",
          "form": "15개는 너무 했다 ㅋㅋ 언제 다 그려",
          "original_form": "15개는 너무 했다 ㅋㅋ 언제 다 그려",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100023591.1.12",
          "form": "몰라 ㅎ 오늘 숙제 늦게 내는 친구들 많을 듯",
          "original_form": "몰라 ㅎ 오늘 숙제 늦게 내는 친구들 많을 듯",
          "speaker_id": "2",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100023591.1.13",
          "form": "가끔 이해 안되는 숙제들이 있구만 ㅎㅎ",
          "original_form": "가끔 이해 안되는 숙제들이 있구만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100023591.1.14",
          "form": "그래도 알아서 잘 끝냈어",
          "original_form": "그래도 알아서 잘 끝냈어",
          "speaker_id": "2",
          "time": "20210830 13:39"
        }
