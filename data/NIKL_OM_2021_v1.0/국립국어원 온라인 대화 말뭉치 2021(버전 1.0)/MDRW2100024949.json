      "utterance": [
        {
          "id": "MDRW2100024949.1.1",
          "form": "저녁 뭐뭄??",
          "original_form": "저녁 뭐뭄??",
          "speaker_id": "1",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024949.1.2",
          "form": "안뭄 ㅋㅋㅋ",
          "original_form": "안뭄 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024949.1.3",
          "form": "저녁 먹엇나",
          "original_form": "저녁 먹엇나",
          "speaker_id": "2",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024949.1.4",
          "form": "당연하지 나는 오징어 두루치기 먹음",
          "original_form": "당연하지 나는 오징어 두루치기 먹음",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024949.1.5",
          "form": "계란후라이 얹어서",
          "original_form": "계란후라이 얹어서",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024949.1.6",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "2",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024949.1.7",
          "form": "와 ㅠㅠ 미쳣다 배고파...ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 ㅠㅠ 미쳣다 배고파...ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024949.1.8",
          "form": "님 역시 맛잘알이네",
          "original_form": "님 역시 맛잘알이네",
          "speaker_id": "2",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024949.1.9",
          "form": "계란후라이는 반숙이엇니",
          "original_form": "계란후라이는 반숙이엇니",
          "speaker_id": "2",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024949.1.10",
          "form": "너무맛잇었단다 근데 좀 양조절목해서",
          "original_form": "너무맛잇었단다 근데 좀 양조절목해서",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024949.1.11",
          "form": "밥을 계속 리필햇어 ㅎ",
          "original_form": "밥을 계속 리필햇어 ㅎ",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024949.1.12",
          "form": "끝마무리쯤에 짜더라구",
          "original_form": "끝마무리쯤에 짜더라구",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024949.1.13",
          "form": "그럴땐 또 단거 넣어줘야하는데",
          "original_form": "그럴땐 또 단거 넣어줘야하는데",
          "speaker_id": "2",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100024949.1.14",
          "form": "어제 티라미수빙수 먹었는데...또 먹고싶네",
          "original_form": "어제 티라미수빙수 먹었는데...또 먹고싶네",
          "speaker_id": "2",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100024949.1.15",
          "form": "헐 티라미수빙수?",
          "original_form": "헐 티라미수빙수?",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100024949.1.16",
          "form": "어디브랜드꺼야????",
          "original_form": "어디브랜드꺼야????",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024949.1.17",
          "form": "설빙 티라미수빙수",
          "original_form": "설빙 티라미수빙수",
          "speaker_id": "2",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024949.1.18",
          "form": "위에 티라미수 한조각 올려주는데",
          "original_form": "위에 티라미수 한조각 올려주는데",
          "speaker_id": "2",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024949.1.19",
          "form": "하 설빙이라니...너무오랜만에 듣는나",
          "original_form": "하 설빙이라니...너무오랜만에 듣는나",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024949.1.20",
          "form": "맛있겟다...",
          "original_form": "맛있겟다...",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024949.1.21",
          "form": "꾸덕 + 살짝 얼음 ㅋㅋ",
          "original_form": "꾸덕 + 살짝 얼음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024949.1.22",
          "form": "김천 놀러와라...",
          "original_form": "김천 놀러와라...",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024949.1.23",
          "form": "...1인 1빙수했어?",
          "original_form": "...1인 1빙수했어?",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024949.1.24",
          "form": "또 곱전 먹고 설빙가자",
          "original_form": "또 곱전 먹고 설빙가자",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024949.1.25",
          "form": "곱창 마무리로 빙수최곤데",
          "original_form": "곱창 마무리로 빙수최곤데",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024949.1.26",
          "form": "ㅋㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024949.1.27",
          "form": "ㅇㅇ딱 마무리되는 느낌이엇음ㅋㅋㅋ",
          "original_form": "ㅇㅇ딱 마무리되는 느낌이엇음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024949.1.28",
          "form": "아 나는 아까 투게더 먹엇네",
          "original_form": "아 나는 아까 투게더 먹엇네",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024949.1.29",
          "form": "마저 난 반숙을 조아하지않아",
          "original_form": "마저 난 반숙을 조아하지않아",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024949.1.30",
          "form": "계란은 완숙이지~~~",
          "original_form": "계란은 완숙이지~~~",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024949.1.31",
          "form": "ㅎ ㅏ 완숙은 간계밥에만 먹지",
          "original_form": "ㅎ ㅏ 완숙은 간계밥에만 먹지",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024949.1.32",
          "form": "반숙 밥 비벼먹으면 최곤데..",
          "original_form": "반숙 밥 비벼먹으면 최곤데..",
          "speaker_id": "2",
          "time": "20210831 21:24"
        }
      ]
    }
