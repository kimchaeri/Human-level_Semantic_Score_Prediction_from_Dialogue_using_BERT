      "utterance": [
        {
          "id": "MDRW2100037385.1.1",
          "form": "요즘 날씨 진짜 좋다",
          "original_form": "요즘 날씨 진짜 좋다",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100037385.1.2",
          "form": "비만 안온다면.",
          "original_form": "비만 안온다면.",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100037385.1.3",
          "form": "일기예보만 좀 맞다면.",
          "original_form": "일기예보만 좀 맞다면.",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100037385.1.4",
          "form": "맞아 요즘 밤에 걷기 너무좋아",
          "original_form": "맞아 요즘 밤에 걷기 너무좋아",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100037385.1.5",
          "form": "너 우리집에 놀러오면 밤산책가자",
          "original_form": "너 우리집에 놀러오면 밤산책가자",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100037385.1.6",
          "form": "코로나만 없었어도 피크닉 가고깊었을거야",
          "original_form": "코로나만 없었어도 피크닉 가고깊었을거야",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100037385.1.7",
          "form": "우리학교 밤에 걸으면 나름 밝아",
          "original_form": "우리학교 밤에 걸으면 나름 밝아",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.8",
          "form": "벌레는 없어?",
          "original_form": "벌레는 없어?",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.9",
          "form": "가을벌레들이 기승이더라",
          "original_form": "가을벌레들이 기승이더라",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.10",
          "form": "음 흐린눈하면돼!",
          "original_form": "음 흐린눈하면돼!",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.11",
          "form": "모기약읗 들고가랴겠어",
          "original_form": "모기약읗 들고가랴겠어",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.12",
          "form": "날이 선선해지니까 아무래도 벌레들도 좋나봐",
          "original_form": "날이 선선해지니까 아무래도 벌레들도 좋나봐",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.13",
          "form": "맞아 계절따라 벌레들이 기승부리는 시기가있지",
          "original_form": "맞아 계절따라 벌레들이 기승부리는 시기가있지",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.14",
          "form": "큰일이야.",
          "original_form": "큰일이야.",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.15",
          "form": "좋은걸 함께 누리려하네",
          "original_form": "좋은걸 함께 누리려하네",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.16",
          "form": "겨울되면 벌레가 적어지겠지?",
          "original_form": "겨울되면 벌레가 적어지겠지?",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.17",
          "form": "응 근데 겨울엔 사람도 나가기가 싱ㅎ어ㅠ",
          "original_form": "응 근데 겨울엔 사람도 나가기가 싱ㅎ어ㅠ",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.18",
          "form": "정말 계절마다 장단점이 있어",
          "original_form": "정말 계절마다 장단점이 있어",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.19",
          "form": "넌 겨울을 좋아해?",
          "original_form": "넌 겨울을 좋아해?",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037385.1.20",
          "form": "나는 여름보다는 나아",
          "original_form": "나는 여름보다는 나아",
          "speaker_id": "2",
          "time": "20210831 23:09"
        }
      ]
    }
