      "utterance": [
        {
          "id": "MDRW2100044362.1.1",
          "form": "챙겨보는 드라마 잇으세요?",
          "original_form": "챙겨보는 드라마 잇으세요?",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044362.1.2",
          "form": "저는 드라마 기다리는 걸 잘 못해서 완결나면 봐요",
          "original_form": "저는 드라마 기다리는 걸 잘 못해서 완결나면 봐요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044362.1.3",
          "form": "최근에 작년 드라마 365일 봤는데 재미있더라구요",
          "original_form": "최근에 작년 드라마 365일 봤는데 재미있더라구요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044362.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044362.1.5",
          "form": "일주일 기다리기 힘들긴해요\r\n이번에 새로시작한 갯마을봐요",
          "original_form": "일주일 기다리기 힘들긴해요\r\n이번에 새로시작한 갯마을봐요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044362.1.6",
          "form": "갯마을 차차차 재밌나요?",
          "original_form": "갯마을 차차차 재밌나요?",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044362.1.7",
          "form": "웃기더라고요ㅋㅋ\r\n주인공들고 예쁘고요",
          "original_form": "웃기더라고요ㅋㅋ\r\n주인공들고 예쁘고요",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044362.1.8",
          "form": "신민아씨 나오는거죠?",
          "original_form": "신민아씨 나오는거죠?",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044362.1.9",
          "form": "배우들 좋아보이더라고요",
          "original_form": "배우들 좋아보이더라고요",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044362.1.10",
          "form": "네 김선호랑요",
          "original_form": "네 김선호랑요",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044362.1.11",
          "form": "옛날에 햇던거 리메이크잖아요 그때 재밋게봐서",
          "original_form": "옛날에 햇던거 리메이크잖아요 그때 재밋게봐서",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044362.1.12",
          "form": "홍반장인가 그거 영화 리메이크랬죠?",
          "original_form": "홍반장인가 그거 영화 리메이크랬죠?",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044362.1.13",
          "form": "네 엄정화나왓엇엇여",
          "original_form": "네 엄정화나왓엇엇여",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044362.1.14",
          "form": "영화 재밌었는데",
          "original_form": "영화 재밌었는데",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044362.1.15",
          "form": "김주혁씨 나왔던 기억나요",
          "original_form": "김주혁씨 나왔던 기억나요",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044362.1.16",
          "form": "맞아요\r\n이번거도 재밋더라고요",
          "original_form": "맞아요\r\n이번거도 재밋더라고요",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044362.1.17",
          "form": "전 슬의생도 봐요",
          "original_form": "전 슬의생도 봐요",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044362.1.18",
          "form": "저희 가족들도 슬의생 꼬박꼬박 챙겨보더라고요",
          "original_form": "저희 가족들도 슬의생 꼬박꼬박 챙겨보더라고요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044362.1.19",
          "form": "저는 노래가 좋아서 가끔 노래만 들어요",
          "original_form": "저는 노래가 좋아서 가끔 노래만 들어요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044362.1.20",
          "form": "재미있어서 챙겨봐요",
          "original_form": "재미있어서 챙겨봐요",
          "speaker_id": "1",
          "time": "20210907 22:42"
        }
      ]
