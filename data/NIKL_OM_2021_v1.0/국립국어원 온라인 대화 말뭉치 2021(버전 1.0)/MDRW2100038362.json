      "utterance": [
        {
          "id": "MDRW2100038362.1.1",
          "form": "지금 밖에 바람이 엄청불어요",
          "original_form": "지금 밖에 바람이 엄청불어요",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038362.1.2",
          "form": "벌써 겨울인가요",
          "original_form": "벌써 겨울인가요",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038362.1.3",
          "form": "네 겨울이에요",
          "original_form": "네 겨울이에요",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038362.1.4",
          "form": "가을은없나요",
          "original_form": "가을은없나요",
          "speaker_id": "2",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100038362.1.5",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100038362.1.6",
          "form": "스쳐지나가는 계절?",
          "original_form": "스쳐지나가는 계절?",
          "speaker_id": "1",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100038362.1.7",
          "form": "단타 때리기군요",
          "original_form": "단타 때리기군요",
          "speaker_id": "2",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100038362.1.8",
          "form": "단타 요새 주식으로 하고 있어여",
          "original_form": "단타 요새 주식으로 하고 있어여",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100038362.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:10"
        },
        {
          "id": "MDRW2100038362.1.10",
          "form": "ㅋㅋ하늘은 단타로 날씨도 바꾸고 비도 때리네요",
          "original_form": "ㅋㅋ하늘은 단타로 날씨도 바꾸고 비도 때리네요",
          "speaker_id": "2",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100038362.1.11",
          "form": "이제 맑아졌네요ㅋㅋ",
          "original_form": "이제 맑아졌네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100038362.1.12",
          "form": "여기두요",
          "original_form": "여기두요",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100038362.1.13",
          "form": "해가 쨍쨍?",
          "original_form": "해가 쨍쨍?",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100038362.1.14",
          "form": "네 그래도이젠 쌀쌀해요",
          "original_form": "네 그래도이젠 쌀쌀해요",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100038362.1.15",
          "form": "여기도 추워요ㅜ",
          "original_form": "여기도 추워요ㅜ",
          "speaker_id": "1",
          "time": "20210902 13:36"
        }
