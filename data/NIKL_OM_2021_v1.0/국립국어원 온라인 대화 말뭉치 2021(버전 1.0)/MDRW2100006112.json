      "utterance": [
        {
          "id": "MDRW2100006112.1.1",
          "form": "술채우는게 좀 힘들긴했는데",
          "original_form": "술채우는게 좀 힘들긴했는데",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006112.1.2",
          "form": "할만했어요",
          "original_form": "할만했어요",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006112.1.3",
          "form": "그거 많이 무겁져 ㅠ",
          "original_form": "그거 많이 무겁져 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100006112.1.4",
          "form": "네ㅠㅜ 유리병이라 더 무겁더라구요",
          "original_form": "네ㅠㅜ 유리병이라 더 무겁더라구요",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100006112.1.5",
          "form": "아이공..고생하셧네",
          "original_form": "아이공..고생하셧네",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006112.1.6",
          "form": "거기 알바는 왜 그만두신건가요? 취업준비 따로 하세요?",
          "original_form": "거기 알바는 왜 그만두신건가요? 취업준비 따로 하세요?",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006112.1.7",
          "form": "아뇨ㅠㅜ 가게가 사라져버렸어요...",
          "original_form": "아뇨ㅠㅜ 가게가 사라져버렸어요...",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006112.1.8",
          "form": "세상에...",
          "original_form": "세상에...",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006112.1.9",
          "form": "코로나로 닫았군요...",
          "original_form": "코로나로 닫았군요...",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006112.1.10",
          "form": "네ㅠㅜ 실직했습니다..",
          "original_form": "네ㅠㅜ 실직했습니다..",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006112.1.11",
          "form": "어이고 ㅠㅠㅠ요즘 알바구하기도 정말 힘들더라구요",
          "original_form": "어이고 ㅠㅠㅠ요즘 알바구하기도 정말 힘들더라구요",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006112.1.12",
          "form": "다른 알바 구하고 계시나요",
          "original_form": "다른 알바 구하고 계시나요",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006112.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006112.1.14",
          "form": "네 구하고있는데",
          "original_form": "네 구하고있는데",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006112.1.15",
          "form": "시기가 시기인지 구하기가 힘드네요",
          "original_form": "시기가 시기인지 구하기가 힘드네요",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006112.1.16",
          "form": "고생이 많으셔요",
          "original_form": "고생이 많으셔요",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006112.1.17",
          "form": "요즘 자영업자들도 너무 힘들어서 알바도 없더라구요",
          "original_form": "요즘 자영업자들도 너무 힘들어서 알바도 없더라구요",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006112.1.18",
          "form": "맞아요... 손님도 너무 없어서",
          "original_form": "맞아요... 손님도 너무 없어서",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006112.1.19",
          "form": "그만둘때도ㅜ 이해가 가더라구요ㅠㅜ",
          "original_form": "그만둘때도ㅜ 이해가 가더라구요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006112.1.20",
          "form": "손님없으면 괜히 제가 더 눈치보이고 그러더라구요",
          "original_form": "손님없으면 괜히 제가 더 눈치보이고 그러더라구요",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006112.1.21",
          "form": "맞아요...너무 없어도 좀 그래요",
          "original_form": "맞아요...너무 없어도 좀 그래요",
          "speaker_id": "2",
          "time": "20210907 20:57"
        }
      ]
