      "utterance": [
        {
          "id": "MDRW2100011944.1.1",
          "form": "나 고2때 삿던",
          "original_form": "나 고2때 삿던",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100011944.1.2",
          "form": "사피엔스도 읽어야하는데",
          "original_form": "사피엔스도 읽어야하는데",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100011944.1.3",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100011944.1.4",
          "form": "쟨 언제쯤 읽을지모르겠다",
          "original_form": "쟨 언제쯤 읽을지모르겠다",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100011944.1.5",
          "form": "펼칠 엄두가안나",
          "original_form": "펼칠 엄두가안나",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100011944.1.6",
          "form": "이름부터 어렵다",
          "original_form": "이름부터 어렵다",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100011944.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100011944.1.8",
          "form": "이책 너도 알걸?",
          "original_form": "이책 너도 알걸?",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100011944.1.9",
          "form": "아???",
          "original_form": "아???",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100011944.1.10",
          "form": "인류학자 유발 하라리 책임",
          "original_form": "인류학자 유발 하라리 책임",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100011944.1.11",
          "form": "그게 누구져",
          "original_form": "그게 누구져",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100011944.1.12",
          "form": "내말은 사피엔스를 읽으며 여행하고싶다는거였음",
          "original_form": "내말은 사피엔스를 읽으며 여행하고싶다는거였음",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100011944.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100011944.1.14",
          "form": "아 진짜",
          "original_form": "아 진짜",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100011944.1.15",
          "form": "아~ 고2때 산 책읽으면서 여행하고싶다~",
          "original_form": "아~ 고2때 산 책읽으면서 여행하고싶다~",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100011944.1.16",
          "form": "근데 책에 나오는곳",
          "original_form": "근데 책에 나오는곳",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100011944.1.17",
          "form": "여행가보고 싶기는 하다",
          "original_form": "여행가보고 싶기는 하다",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100011944.1.18",
          "form": "ㅋㅋㅋ해리포터?",
          "original_form": "ㅋㅋㅋ해리포터?",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100011944.1.19",
          "form": "영화에 나오는 곳아나 책에 나오는 곳 여행가면",
          "original_form": "영화에 나오는 곳아나 책에 나오는 곳 여행가면",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100011944.1.20",
          "form": "좋을것 같아",
          "original_form": "좋을것 같아",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100011944.1.21",
          "form": "해리포터는 약간 기본으로 깔려있고",
          "original_form": "해리포터는 약간 기본으로 깔려있고",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100011944.1.22",
          "form": "기억에 완전 오래남을듯",
          "original_form": "기억에 완전 오래남을듯",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100011944.1.23",
          "form": "다른 책이나 영화들도",
          "original_form": "다른 책이나 영화들도",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100011944.1.24",
          "form": "가서 주인공처럼 사진 찍기",
          "original_form": "가서 주인공처럼 사진 찍기",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100011944.1.25",
          "form": "그 라푼젤에 나온 프랑스도 가보고 싶어",
          "original_form": "그 라푼젤에 나온 프랑스도 가보고 싶어",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100011944.1.26",
          "form": "프랑수는 싫은데",
          "original_form": "프랑수는 싫은데",
          "speaker_id": "2",
          "time": "20210826 15:27"
        }
      ]
    }
  ]
