      "utterance": [
        {
          "id": "MDRW2100045123.1.1",
          "form": "님 굽네 조아해요?",
          "original_form": "님 굽네 조아해요?",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045123.1.2",
          "form": "네 제 최애는 볼케이노 순살인데",
          "original_form": "네 제 최애는 볼케이노 순살인데",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045123.1.3",
          "form": "요즘 갈릭마왕으로 바꼈어요",
          "original_form": "요즘 갈릭마왕으로 바꼈어요",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045123.1.4",
          "form": "헉 그게 그렇게 맛나나요",
          "original_form": "헉 그게 그렇게 맛나나요",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045123.1.5",
          "form": "제가 마늘을 원래 좀 좋아해요 ㅎㅎ",
          "original_form": "제가 마늘을 원래 좀 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045123.1.6",
          "form": "name1님은 굽네에서 뭐 좋아해요?",
          "original_form": "&name1&님은 굽네에서 뭐 좋아해요?",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045123.1.7",
          "form": "마라 ??",
          "original_form": "마라 ??",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045123.1.8",
          "form": "고 추 바사삭이요",
          "original_form": "고 추 바사삭이요",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045123.1.9",
          "form": "갈릭 안무밧어요",
          "original_form": "갈릭 안무밧어요",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045123.1.10",
          "form": "마라는 단종된지가 언젠데요;;",
          "original_form": "마라는 단종된지가 언젠데요;;",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045123.1.11",
          "form": "아예...잘아시네요",
          "original_form": "아예...잘아시네요",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045123.1.12",
          "form": "친구가 사줘서 먹은거라 ㅎㅎㅎ",
          "original_form": "친구가 사줘서 먹은거라 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045123.1.13",
          "form": "왜없어졋대요",
          "original_form": "왜없어졋대요",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045123.1.14",
          "form": "저희 예전엔 굽네에 오리지널밖에 없어서 오리지널만 먹었던거 기억나요??",
          "original_form": "저희 예전엔 굽네에 오리지널밖에 없어서 오리지널만 먹었던거 기억나요??",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045123.1.15",
          "form": "김치랑 밥이랑 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "김치랑 밥이랑 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045123.1.16",
          "form": "와 언제적 ㄷㄷ",
          "original_form": "와 언제적 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045123.1.17",
          "form": "계란도왓어요",
          "original_form": "계란도왓어요",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045123.1.18",
          "form": "아심 ?",
          "original_form": "아심 ?",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045123.1.19",
          "form": "알죠 ㅋㅋㅋㅋㅋㅋㅋ그거 계란값 한번 폭등했을때 사라졌잖아여...",
          "original_form": "알죠 ㅋㅋㅋㅋㅋㅋㅋ그거 계란값 한번 폭등했을때 사라졌잖아여...",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045123.1.20",
          "form": "그게 이유엿어요?",
          "original_form": "그게 이유엿어요?",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045123.1.21",
          "form": "네 돈이 다죠 뭐",
          "original_form": "네 돈이 다죠 뭐",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045123.1.22",
          "form": "마라 없어진거도 안팔려서죠 뭐",
          "original_form": "마라 없어진거도 안팔려서죠 뭐",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045123.1.23",
          "form": "아하... 왜안팔리지\r\n.노이해",
          "original_form": "아하... 왜안팔리지\r\n.노이해",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045123.1.24",
          "form": "혹시 굽네 사장이세여?",
          "original_form": "혹시 굽네 사장이세여?",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045123.1.25",
          "form": "잘아시네요",
          "original_form": "잘아시네요",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045123.1.26",
          "form": "그만하세요",
          "original_form": "그만하세요",
          "speaker_id": "2",
          "time": "20210908 01:31"
        }
      ]
    }
