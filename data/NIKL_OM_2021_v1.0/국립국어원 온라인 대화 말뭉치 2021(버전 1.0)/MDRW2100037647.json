      "utterance": [
        {
          "id": "MDRW2100037647.1.1",
          "form": "이제 신나는 우인단과의 싸움이 또",
          "original_form": "이제 신나는 우인단과의 싸움이 또",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037647.1.2",
          "form": "오 그럼 오늘 전투하고 끝날듯?",
          "original_form": "오 그럼 오늘 전투하고 끝날듯?",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037647.1.3",
          "form": "기다리고있겠지?",
          "original_form": "기다리고있겠지?",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037647.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037647.1.5",
          "form": "맞아 바로 그거야",
          "original_form": "맞아 바로 그거야",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037647.1.6",
          "form": "20렙 라이덴은",
          "original_form": "20렙 라이덴은",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037647.1.7",
          "form": "케이아를 뺄 수준은",
          "original_form": "케이아를 뺄 수준은",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037647.1.8",
          "form": "아닌것같아",
          "original_form": "아닌것같아",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037647.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 20렙이잖아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 20렙이잖아",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037647.1.10",
          "form": "성유물은 채워줫어?",
          "original_form": "성유물은 채워줫어?",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037647.1.11",
          "form": "원래는 내가",
          "original_form": "원래는 내가",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037647.1.12",
          "form": "아 ㅇㅇ그 뭐지",
          "original_form": "아 ㅇㅇ그 뭐지",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037647.1.13",
          "form": "빨간색",
          "original_form": "빨간색",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037647.1.14",
          "form": "전투광?",
          "original_form": "전투광?",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037647.1.15",
          "form": "4세트가 장병기 한손검 양손검 공격증가",
          "original_form": "4세트가 장병기 한손검 양손검 공격증가",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037647.1.16",
          "form": "그걸로 일단 채워줬어",
          "original_form": "그걸로 일단 채워줬어",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037647.1.17",
          "form": "원래는 몹잡는걸",
          "original_form": "원래는 몹잡는걸",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100037647.1.18",
          "form": "찍으려고 했거든",
          "original_form": "찍으려고 했거든",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100037647.1.19",
          "form": "그래서 궁채우고 딱 츄츄족한테 갔는데",
          "original_form": "그래서 궁채우고 딱 츄츄족한테 갔는데",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100037647.1.20",
          "form": "그 폭발통 한방에",
          "original_form": "그 폭발통 한방에",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100037647.1.21",
          "form": "죽어버렷어.....",
          "original_form": "죽어버렷어.....",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100037647.1.22",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100037647.1.23",
          "form": "레벨 몇이야?",
          "original_form": "레벨 몇이야?",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100037647.1.24",
          "form": "지금 20이고",
          "original_form": "지금 20이고",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037647.1.25",
          "form": "그 열매 대체",
          "original_form": "그 열매 대체",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037647.1.26",
          "form": "어딨는건지",
          "original_form": "어딨는건지",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037647.1.27",
          "form": "모르겟어........",
          "original_form": "모르겟어........",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037647.1.28",
          "form": "어지러워서 쉬고잇어",
          "original_form": "어지러워서 쉬고잇어",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037647.1.29",
          "form": "세이라이섬에 있다는데",
          "original_form": "세이라이섬에 있다는데",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037647.1.30",
          "form": "신규 섬 신상 다 열었어?",
          "original_form": "신규 섬 신상 다 열었어?",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037647.1.31",
          "form": "거기가 어디야.......",
          "original_form": "거기가 어디야.......",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037647.1.32",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037647.1.33",
          "form": "다열엇어",
          "original_form": "다열엇어",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037647.1.34",
          "form": "근데 없어??",
          "original_form": "근데 없어??",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100037647.1.35",
          "form": "생긴건 제비꽃 열매처럼 걍 나무에 매달려있을거같이 보이는데",
          "original_form": "생긴건 제비꽃 열매처럼 걍 나무에 매달려있을거같이 보이는데",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100037647.1.36",
          "form": "꼼꼼히 안뒤지긴 했는데",
          "original_form": "꼼꼼히 안뒤지긴 했는데",
          "speaker_id": "2",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100037647.1.37",
          "form": "나무에 매달린건 걍",
          "original_form": "나무에 매달린건 걍",
          "speaker_id": "2",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100037647.1.38",
          "form": "제비꽃 열매야",
          "original_form": "제비꽃 열매야",
          "speaker_id": "2",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100037647.1.39",
          "form": "ㅇㅏ",
          "original_form": "ㅇㅏ",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100037647.1.40",
          "form": "내가 뒤지던건",
          "original_form": "내가 뒤지던건",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100037647.1.41",
          "form": "와타츠미섬이래....",
          "original_form": "와타츠미섬이래....",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100037647.1.42",
          "form": "다시간다",
          "original_form": "다시간다",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100037647.1.43",
          "form": "ㅇ ㅏ....",
          "original_form": "ㅇ ㅏ....",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100037647.1.44",
          "form": "이거 은근 공간감각 필요해 ㅋㅋㅋㅋㅋ",
          "original_form": "이거 은근 공간감각 필요해 ㅋㅋㅋㅋㅋ",
