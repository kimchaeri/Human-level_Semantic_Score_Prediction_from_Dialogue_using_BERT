      "utterance": [
        {
          "id": "MDRW2100026598.1.1",
          "form": "진짜 내돈 쓰기도 힘들더라구여ㅋ",
          "original_form": "진짜 내돈 쓰기도 힘들더라구여ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026598.1.2",
          "form": "사람들이 어케들 조은건 귀신같이 알고 ㅋㅋㅋ",
          "original_form": "사람들이 어케들 조은건 귀신같이 알고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026598.1.3",
          "form": "또 사고 먹음 좋긴 좋더라구요ㅋㅋ",
          "original_form": "또 사고 먹음 좋긴 좋더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026598.1.4",
          "form": "ㅋㅋㅋㅋㅋ 글쵸 ㅠㅠ휴 마음샌드 먹어보고싶다 ㅋㅋㅋ ㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 글쵸 ㅠㅠ휴 마음샌드 먹어보고싶다 ㅋㅋㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026598.1.5",
          "form": "제주말고 강원도 이런데도 가보셧나요",
          "original_form": "제주말고 강원도 이런데도 가보셧나요",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026598.1.6",
          "form": "강원도는 뚜벅이들 다니기 너무 좋아여",
          "original_form": "강원도는 뚜벅이들 다니기 너무 좋아여",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026598.1.7",
          "form": "저는 속초 종종 갔고 고성도 너무 좋아여",
          "original_form": "저는 속초 종종 갔고 고성도 너무 좋아여",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026598.1.8",
          "form": "강원도 좋아 하시나요?",
          "original_form": "강원도 좋아 하시나요?",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026598.1.9",
          "form": "조아하는데 가기가 어렵네요 여기서 ㅠㅠㅠ",
          "original_form": "조아하는데 가기가 어렵네요 여기서 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026598.1.10",
          "form": "자주가고싶어요",
          "original_form": "자주가고싶어요",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026598.1.11",
          "form": "실례지만 어디신가요?",
          "original_form": "실례지만 어디신가요?",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026598.1.12",
          "form": "뚜벅이가 갠찬구나",
          "original_form": "뚜벅이가 갠찬구나",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026598.1.13",
          "form": "대전이용",
          "original_form": "대전이용",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026598.1.14",
          "form": "서울에서 가기조아보이던뎅",
          "original_form": "서울에서 가기조아보이던뎅",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026598.1.15",
          "form": "아하! 찐 선심당",
          "original_form": "아하! 찐 선심당",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026598.1.16",
          "form": "전 팝업스토어만 가봤어여",
          "original_form": "전 팝업스토어만 가봤어여",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026598.1.17",
          "form": "뚜벅이는 속초 좋아여 터미널에서 왠만한데 도보로 쌉가능",
          "original_form": "뚜벅이는 속초 좋아여 터미널에서 왠만한데 도보로 쌉가능",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026598.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026598.1.19",
          "form": "오.... ???",
          "original_form": "오.... ???",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026598.1.20",
          "form": "그정돈가요",
          "original_form": "그정돈가요",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026598.1.21",
          "form": "도보",
          "original_form": "도보",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026598.1.22",
          "form": "ㅋㅋㅋㅋ도보함가야겟다",
          "original_form": "ㅋㅋㅋㅋ도보함가야겟다",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026598.1.23",
          "form": "차 혼자가져가긴 기름값 넘마니들어서 ㅋㅋ",
          "original_form": "차 혼자가져가긴 기름값 넘마니들어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026598.1.24",
          "form": "시장도 전망대도 바다도 걷는거 괜찮으시면",
          "original_form": "시장도 전망대도 바다도 걷는거 괜찮으시면",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026598.1.25",
          "form": "다 거기서 거기에요ㅋ",
          "original_form": "다 거기서 거기에요ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026598.1.26",
          "form": "고성 넘어가도 택시비 만원 이하",
          "original_form": "고성 넘어가도 택시비 만원 이하",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026598.1.27",
          "form": "우와 좋다",
          "original_form": "우와 좋다",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026598.1.28",
          "form": "속초는 어디조아하세요",
          "original_form": "속초는 어디조아하세요",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026598.1.29",
          "form": "속초는 시장도 좋고 시장에서 배타고",
          "original_form": "속초는 시장도 좋고 시장에서 배타고",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026598.1.30",
          "form": "어디 들어가는데 있었는데",
          "original_form": "어디 들어가는데 있었는데",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026598.1.31",
          "form": "아바이마을!!",
          "original_form": "아바이마을!!",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026598.1.32",
          "form": "순대!?!?!",
          "original_form": "순대!?!?!",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026598.1.33",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026598.1.34",
          "form": "구거랑 씨앗오징어인가",
          "original_form": "구거랑 씨앗오징어인가",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026598.1.35",
          "form": "씨앗오징어는 머죠 ㅋㅋ",
          "original_form": "씨앗오징어는 머죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026598.1.36",
          "form": "씨앗젓갈이네여",
          "original_form": "씨앗젓갈이네여",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026598.1.37",
          "form": "밥비벼 먹는거에요",
          "original_form": "밥비벼 먹는거에요",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026598.1.38",
          "form": "씨앗이들어가요 젓갈에?",
          "original_form": "씨앗이들어가요 젓갈에?",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026598.1.39",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 23:21"
        }
      ]
    }
  ]
