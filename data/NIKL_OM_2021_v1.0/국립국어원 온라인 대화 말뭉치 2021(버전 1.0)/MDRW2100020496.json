      "utterance": [
        {
          "id": "MDRW2100020496.1.1",
          "form": "음 또 망고는 먹어봣나?",
          "original_form": "음 또 망고는 먹어봣나?",
          "speaker_id": "2",
          "time": "20210828 08:26"
        },
        {
          "id": "MDRW2100020496.1.2",
          "form": "망고설빙",
          "original_form": "망고설빙",
          "speaker_id": "2",
          "time": "20210828 08:26"
        },
        {
          "id": "MDRW2100020496.1.3",
          "form": "노노",
          "original_form": "노노",
          "speaker_id": "1",
          "time": "20210828 08:26"
        },
        {
          "id": "MDRW2100020496.1.4",
          "form": "엄마가 망고시러해서",
          "original_form": "엄마가 망고시러해서",
          "speaker_id": "2",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.5",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.6",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "1",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.7",
          "form": "친구들은?",
          "original_form": "친구들은?",
          "speaker_id": "2",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.8",
          "form": "음 잘 모르겟어",
          "original_form": "음 잘 모르겟어",
          "speaker_id": "1",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.9",
          "form": "그럴수잇지",
          "original_form": "그럴수잇지",
          "speaker_id": "2",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.10",
          "form": "마쟈 망고도 맛잇을듯",
          "original_form": "마쟈 망고도 맛잇을듯",
          "speaker_id": "1",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.11",
          "form": "나도 예~전에먹구 안먹엇어",
          "original_form": "나도 예~전에먹구 안먹엇어",
          "speaker_id": "2",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.12",
          "form": "기대돼 망고!",
          "original_form": "기대돼 망고!",
          "speaker_id": "1",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.13",
          "form": "나두!!!",
          "original_form": "나두!!!",
          "speaker_id": "2",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.14",
          "form": "치즈큐브도잇지?",
          "original_form": "치즈큐브도잇지?",
          "speaker_id": "1",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.15",
          "form": "응응 마자",
          "original_form": "응응 마자",
          "speaker_id": "2",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.16",
          "form": "그 큐브가 진짜 맛잇어",
          "original_form": "그 큐브가 진짜 맛잇어",
          "speaker_id": "1",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.17",
          "form": "아 인졍",
          "original_form": "아 인졍",
          "speaker_id": "2",
          "time": "20210828 08:27"
        },
        {
          "id": "MDRW2100020496.1.18",
          "form": "망고는 냉동인가?",
          "original_form": "망고는 냉동인가?",
          "speaker_id": "1",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020496.1.19",
          "form": "아마 그럴거야",
          "original_form": "아마 그럴거야",
          "speaker_id": "2",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020496.1.20",
          "form": "그건 아쉽다",
          "original_form": "그건 아쉽다",
          "speaker_id": "1",
          "time": "20210828 08:28"
        },
