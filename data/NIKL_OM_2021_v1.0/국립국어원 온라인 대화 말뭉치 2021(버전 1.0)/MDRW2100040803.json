      "utterance": [
        {
          "id": "MDRW2100040803.1.1",
          "form": "코로나 부작용 생겨서 죽은 사람 들요",
          "original_form": "코로나 부작용 생겨서 죽은 사람 들요",
          "speaker_id": "1",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100040803.1.2",
          "form": "그 사람들 보상금 받을 수 있을까요?",
          "original_form": "그 사람들 보상금 받을 수 있을까요?",
          "speaker_id": "1",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100040803.1.3",
          "form": "ㅠ인과성이 입증되어야 한다던데",
          "original_form": "ㅠ인과성이 입증되어야 한다던데",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040803.1.4",
          "form": "쉽진 않을 듯해요..",
          "original_form": "쉽진 않을 듯해요..",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040803.1.5",
          "form": "네 인정을 안 해 줄 거 같아요",
          "original_form": "네 인정을 안 해 줄 거 같아요",
          "speaker_id": "1",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040803.1.6",
          "form": "그거 안 맞았으면 코로나 걸려서 죽을 사람 아닌  거 같은데",
          "original_form": "그거 안 맞았으면 코로나 걸려서 죽을 사람 아닌  거 같은데",
          "speaker_id": "1",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040803.1.7",
          "form": "당사자나 가족들 입장에선",
          "original_form": "당사자나 가족들 입장에선",
          "speaker_id": "2",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040803.1.8",
          "form": "건강하던 사람이",
          "original_form": "건강하던 사람이",
          "speaker_id": "2",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040803.1.9",
          "form": "백신 맞고 아프거나 죽 으면",
          "original_form": "백신 맞고 아프거나 죽 으면",
          "speaker_id": "2",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040803.1.10",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040803.1.11",
          "form": "그게 무조건 입증 안 됐다고 말할수없는거같아요",
          "original_form": "그게 무조건 입증 안 됐다고 말할수없는거같아요",
          "speaker_id": "1",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040803.1.12",
          "form": "의료계 사람들은 무조건 우연히 일어난 일이라고 말 안 하는데",
          "original_form": "의료계 사람들은 무조건 우연히 일어난 일이라고 말 안 하는데",
          "speaker_id": "1",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040803.1.13",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040803.1.14",
          "form": "건강하려고 맞는건데",
          "original_form": "건강하려고 맞는건데",
          "speaker_id": "2",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040803.1.15",
          "form": "그 사람들이 너무 지나치게 옹호 하는 거 같더라구요",
          "original_form": "그 사람들이 너무 지나치게 옹호 하는 거 같더라구요",
          "speaker_id": "1",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040803.1.16",
          "form": "백신 접종하라고 해야하니까 그렇겠죠",
          "original_form": "백신 접종하라고 해야하니까 그렇겠죠",
          "speaker_id": "2",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100040803.1.17",
          "form": "그러니까요 자기들 권위를 계속 유지하고 싶어 하는 거 같아요",
          "original_form": "그러니까요 자기들 권위를 계속 유지하고 싶어 하는 거 같아요",
          "speaker_id": "1",
          "time": "20210905 08:52"
        },
        {
          "id": "MDRW2100040803.1.18",
          "form": "그냥 기승전 백신 이런느낌요",
          "original_form": "그냥 기승전 백신 이런느낌요",
          "speaker_id": "1",
          "time": "20210905 08:52"
        },
        {
          "id": "MDRW2100040803.1.19",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210905 08:52"
        },
        {
          "id": "MDRW2100040803.1.20",
          "form": "화이자 승인도 얼마 전에야 났다는데",
          "original_form": "화이자 승인도 얼마 전에야 났다는데",
          "speaker_id": "2",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100040803.1.21",
          "form": "요즘은 사회의 불신이 많은 거 같아요",
          "original_form": "요즘은 사회의 불신이 많은 거 같아요",
          "speaker_id": "1",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100040803.1.22",
          "form": "신뢰가 생기기 힘들어요 아무리 봐도",
          "original_form": "신뢰가 생기기 힘들어요 아무리 봐도",
          "speaker_id": "1",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100040803.1.23",
          "form": "걱정되죠 아무래도",
          "original_form": "걱정되죠 아무래도",
          "speaker_id": "2",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100040803.1.24",
          "form": "확률이 아무리 낮아도 내가 죽으면 1이잖아요",
          "original_form": "확률이 아무리 낮아도 내가 죽으면 1이잖아요",
