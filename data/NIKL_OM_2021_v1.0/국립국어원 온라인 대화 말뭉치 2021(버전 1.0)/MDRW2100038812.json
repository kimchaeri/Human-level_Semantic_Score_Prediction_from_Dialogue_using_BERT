      "utterance": [
        {
          "id": "MDRW2100038812.1.1",
          "form": "저 쿠팡이요! 근데 맛이없네요 삼립껀",
          "original_form": "저 쿠팡이요! 근데 맛이없네요 삼립껀",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100038812.1.2",
          "form": "그냥 빵집 가서 찾아봐야겠네요 ㅋㅋ",
          "original_form": "그냥 빵집 가서 찾아봐야겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100038812.1.3",
          "form": "롯데꺼는 지인이줘서 먹어봤는데 괜찮았어요",
          "original_form": "롯데꺼는 지인이줘서 먹어봤는데 괜찮았어요",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100038812.1.4",
          "form": "빵 좋아하세요?",
          "original_form": "빵 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100038812.1.5",
          "form": "네 완젼 좋아해요!",
          "original_form": "네 완젼 좋아해요!",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100038812.1.6",
          "form": "특히 피자빵 이런거요 ㅎ",
          "original_form": "특히 피자빵 이런거요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100038812.1.7",
          "form": "케이크도 좋고요",
          "original_form": "케이크도 좋고요",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100038812.1.8",
          "form": "전 그 설탕 엄청 발라져있는 토스트? 그거 좋아해요 ㅋㅋ",
          "original_form": "전 그 설탕 엄청 발라져있는 토스트? 그거 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038812.1.9",
          "form": "와 그거 최고죠 진짜",
          "original_form": "와 그거 최고죠 진짜",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038812.1.10",
          "form": "가끔 식빵사서 집에서 해먹어요",
          "original_form": "가끔 식빵사서 집에서 해먹어요",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038812.1.11",
          "form": "최근에 유튜브에서 레시피 봤는데 어렵지 않더라구요 ㅎㅎ",
          "original_form": "최근에 유튜브에서 레시피 봤는데 어렵지 않더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100038812.1.12",
          "form": "근데 파는 것만큼 맛있게 될지 모르겠어요",
          "original_form": "근데 파는 것만큼 맛있게 될지 모르겠어요",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100038812.1.13",
          "form": "맞아요 저도 유튜브보고 했는데 ㅎ",
          "original_form": "맞아요 저도 유튜브보고 했는데 ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038812.1.14",
          "form": "버터 넣고하니까 꼬숩더라고요",
          "original_form": "버터 넣고하니까 꼬숩더라고요",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100038812.1.15",
          "form": "그냥 냉동실에 있는 얇은 식빵으로라도 한 번 해봐야겠어요",
          "original_form": "그냥 냉동실에 있는 얇은 식빵으로라도 한 번 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100038812.1.16",
          "form": "식빵 냉동실에 넣어두면 맛있지않나요",
          "original_form": "식빵 냉동실에 넣어두면 맛있지않나요",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038812.1.17",
          "form": "바로 먹을 거 아니면 일찍 냉동시켜놓는게 훨씬 맛있더라구요",
          "original_form": "바로 먹을 거 아니면 일찍 냉동시켜놓는게 훨씬 맛있더라구요",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038812.1.18",
          "form": "맞아요ㅎ 빵 냉장고에 넣어먹는것도 좋아요",
          "original_form": "맞아요ㅎ 빵 냉장고에 넣어먹는것도 좋아요",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100038812.1.19",
          "form": "그 식감이 좋아요",
          "original_form": "그 식감이 좋아요",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100038812.1.20",
          "form": "조만간 빵 가득 사와야겠어요ㅎㅎ",
          "original_form": "조만간 빵 가득 사와야겠어요ㅎㅎ",
