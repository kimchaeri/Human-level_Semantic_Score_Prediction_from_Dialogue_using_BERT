      "utterance": [
        {
          "id": "MDRW2100015945.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 13:11"
        },
        {
          "id": "MDRW2100015945.1.2",
          "form": "저 뮤지컬은 웃는남자라고",
          "original_form": "저 뮤지컬은 웃는남자라고",
          "speaker_id": "1",
          "time": "20210830 13:11"
        },
        {
          "id": "MDRW2100015945.1.3",
          "form": "한번 본적있는데",
          "original_form": "한번 본적있는데",
          "speaker_id": "1",
          "time": "20210830 13:11"
        },
        {
          "id": "MDRW2100015945.1.4",
          "form": "뮤지컬이랑은 안맞더라구요ㅠㅠ",
          "original_form": "뮤지컬이랑은 안맞더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100015945.1.5",
          "form": "앗 그렇군요ㅠㅠㅠㅠ 그럼 콘서트 이런건 좋아하세요??",
          "original_form": "앗 그렇군요ㅠㅠㅠㅠ 그럼 콘서트 이런건 좋아하세요??",
          "speaker_id": "2",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100015945.1.6",
          "form": "콘서트는 가본적이 없는데 name2님은 가보셨어용??",
          "original_form": "콘서트는 가본적이 없는데 &name2&님은 가보셨어용??",
          "speaker_id": "1",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100015945.1.7",
          "form": "앗 저는 딱 한번 가봤어요!",
          "original_form": "앗 저는 딱 한번 가봤어요!",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100015945.1.8",
          "form": "예전에 10센치랑 자이언티랑",
          "original_form": "예전에 10센치랑 자이언티랑",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100015945.1.9",
          "form": "합동콘서트 한적있었거든요ㅎㅎ",
          "original_form": "합동콘서트 한적있었거든요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100015945.1.10",
          "form": "한 5년? 전인거같아요 엄청 예전이네요 ㅎㅎ",
          "original_form": "한 5년? 전인거같아요 엄청 예전이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100015945.1.11",
          "form": "우왕 어떠셨어요??",
          "original_form": "우왕 어떠셨어요??",
          "speaker_id": "1",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100015945.1.12",
          "form": "콘서트 티켓값이 아깝지 않을정도였나용?",
          "original_form": "콘서트 티켓값이 아깝지 않을정도였나용?",
          "speaker_id": "1",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100015945.1.13",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100015945.1.14",
          "form": "나름 소규모로 하다보니까",
          "original_form": "나름 소규모로 하다보니까",
          "speaker_id": "2",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100015945.1.15",
          "form": "앉아서 공연 즐길수 있었구",
          "original_form": "앉아서 공연 즐길수 있었구",
          "speaker_id": "2",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100015945.1.16",
          "form": "앵콜도 해주고 자이언티랑 십센치 둘 다 노래 넘 잘불러서 좋앗어요 ㅋㅋ",
          "original_form": "앵콜도 해주고 자이언티랑 십센치 둘 다 노래 넘 잘불러서 좋앗어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100015945.1.17",
          "form": "앗 콘서트는 맨날 올림픽홀 이런곳인줄 알았는데 소규모면 저도 가보고싶어요ㅋㅋㅋ",
          "original_form": "앗 콘서트는 맨날 올림픽홀 이런곳인줄 알았는데 소규모면 저도 가보고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100015945.1.18",
          "form": "코엑스 이런데 작은 홀에서 콘서트 한다는 느낌으로 하더라구요!!ㅋㅋㅋㅋ",
          "original_form": "코엑스 이런데 작은 홀에서 콘서트 한다는 느낌으로 하더라구요!!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100015945.1.19",
          "form": "그래서 자리에앉아서 사람들한테 안치이고 ㅎㅎㅎ 무사히 공연 볼 수 있었어요",
          "original_form": "그래서 자리에앉아서 사람들한테 안치이고 ㅎㅎㅎ 무사히 공연 볼 수 있었어요",
          "speaker_id": "2",
          "time": "20210830 15:04"
        },
        {
          "id": "MDRW2100015945.1.20",
          "form": "와아 너무 좋네요",
          "original_form": "와아 너무 좋네요",
          "speaker_id": "1",
          "time": "20210830 15:04"
        },
        {
          "id": "MDRW2100015945.1.21",
          "form": "요즘 콘서트도 소인원으로",
          "original_form": "요즘 콘서트도 소인원으로",
          "speaker_id": "1",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100015945.1.22",
          "form": "자리 넓게해서 한다던데",
          "original_form": "자리 넓게해서 한다던데",
          "speaker_id": "1",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100015945.1.23",
          "form": "가려면 요즘 가야할까봐요ㅋㅋㅋ",
          "original_form": "가려면 요즘 가야할까봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100015945.1.24",
          "form": "그런 옹기종기 모인 분위기있는 공연 좋아해서요ㅋㅋㅋ",
          "original_form": "그런 옹기종기 모인 분위기있는 공연 좋아해서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100015945.1.25",
          "form": "앜ㅋㅋㅋㅋㅋ 요즘도 오프라인 콘서트가 열리나요???",
          "original_form": "앜ㅋㅋㅋㅋㅋ 요즘도 오프라인 콘서트가 열리나요???",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100015945.1.26",
          "form": "요즘도 열리는줄은 생각도 못했어요...!!!",
          "original_form": "요즘도 열리는줄은 생각도 못했어요...!!!",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100015945.1.27",
          "form": "요즘 거리두기하고 소규모로 콘서트 가끔 열리더라구요! 4단계 시작하고나서는 아닐거에용ㅎㅎ",
          "original_form": "요즘 거리두기하고 소규모로 콘서트 가끔 열리더라구요! 4단계 시작하고나서는 아닐거에용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100015945.1.28",
          "form": "아하!! 3단계까지는",
          "original_form": "아하!! 3단계까지는",
          "speaker_id": "2",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100015945.1.29",
          "form": "콘서트도 정상적으로 하나보네요ㅎㅎㅎ",
          "original_form": "콘서트도 정상적으로 하나보네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:30"
