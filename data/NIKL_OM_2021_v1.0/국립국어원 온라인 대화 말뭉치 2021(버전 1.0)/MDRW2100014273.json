        {
          "id": "MDRW2100014273.1.1",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014273.1.2",
          "form": "먹는것도 꼭 소스를.찍어야(?(",
          "original_form": "먹는것도 꼭 소스를.찍어야(?(",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014273.1.3",
          "form": "운동 하시는거 있으신가요",
          "original_form": "운동 하시는거 있으신가요",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014273.1.4",
          "form": "자신의 주관이 있는 패션이 짱이지",
          "original_form": "자신의 주관이 있는 패션이 짱이지",
          "speaker_id": "1",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014273.1.5",
          "form": "저 숨쉬기 운동만합니다",
          "original_form": "저 숨쉬기 운동만합니다",
          "speaker_id": "1",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014273.1.6",
          "form": "ㅋㅋㅋㅋ운동 너무싫어요ㅠ",
          "original_form": "ㅋㅋㅋㅋ운동 너무싫어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014273.1.7",
          "form": "운동하시나요",
          "original_form": "운동하시나요",
          "speaker_id": "1",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014273.1.8",
          "form": "운동 오늘도 하고 왓어요",
          "original_form": "운동 오늘도 하고 왓어요",
          "speaker_id": "2",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014273.1.9",
          "form": "운동 한다니까 바로 3대 몇치냐고 물어보시대요 보통",
          "original_form": "운동 한다니까 바로 3대 몇치냐고 물어보시대요 보통",
          "speaker_id": "2",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014273.1.10",
          "form": "하지만 다행입니다 저도 운동 싫어하기때문에",
          "original_form": "하지만 다행입니다 저도 운동 싫어하기때문에",
          "speaker_id": "2",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014273.1.11",
          "form": "그런거 묻지 않으실거라 생각하니 맘이 편안해지는군여",
          "original_form": "그런거 묻지 않으실거라 생각하니 맘이 편안해지는군여",
          "speaker_id": "2",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014273.1.12",
          "form": "운동하고 오셨다면서요ㅋㅋㅋㅋㅋ 무슨운동하세요",
          "original_form": "운동하고 오셨다면서요ㅋㅋㅋㅋㅋ 무슨운동하세요",
          "speaker_id": "1",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014273.1.13",
          "form": "저 헬스요",
          "original_form": "저 헬스요",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014273.1.14",
          "form": "그거랑",
          "original_form": "그거랑",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014273.1.15",
          "form": "볼링도 좋아해요 근데 중요한건 잘 못쳐요",
          "original_form": "볼링도 좋아해요 근데 중요한건 잘 못쳐요",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014273.1.16",
          "form": "헬스가면 주로 뭐해요",
          "original_form": "헬스가면 주로 뭐해요",
          "speaker_id": "1",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014273.1.17",
          "form": "약간 재능없는 거에 혼자 짝사랑하는 타입",
          "original_form": "약간 재능없는 거에 혼자 짝사랑하는 타입",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014273.1.18",
          "form": "헬스가면 이제",
          "original_form": "헬스가면 이제",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014273.1.19",
          "form": "주4회 가거든요 그러면",
          "original_form": "주4회 가거든요 그러면",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014273.1.20",
          "form": "상체 하체 나눠서 근력하고 30분 유산소하고 와요",
          "original_form": "상체 하체 나눠서 근력하고 30분 유산소하고 와요",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014273.1.21",
          "form": "전문적이시네요",
          "original_form": "전문적이시네요",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014273.1.22",
          "form": "볼링 못치면 어때요",
          "original_form": "볼링 못치면 어때요",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014273.1.23",
          "form": "좋아하면 되는거지",
          "original_form": "좋아하면 되는거지",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014273.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014273.1.25",
          "form": "볼링이나 당구는 치시나요",
          "original_form": "볼링이나 당구는 치시나요",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014273.1.26",
          "form": "다만 우리팀한테 눈치받겠지",
          "original_form": "다만 우리팀한테 눈치받겠지",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014273.1.27",
          "form": "약간 내기할때 이기려고",
          "original_form": "약간 내기할때 이기려고",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014273.1.28",
          "form": "하기시작했는데",
          "original_form": "하기시작했는데",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014273.1.29",
          "form": "내기 하면 못끊는다 이제",
          "original_form": "내기 하면 못끊는다 이제",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014273.1.30",
          "form": "중독이 되셔버렸나요",
          "original_form": "중독이 되셔버렸나요",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014273.1.31",
          "form": "네네 근데",
          "original_form": "네네 근데",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014273.1.32",
          "form": "코로나가 그 중독을 끊어주었습니다",
          "original_form": "코로나가 그 중독을 끊어주었습니다",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014273.1.33",
          "form": "짜장면 중독도 함께요",
          "original_form": "짜장면 중독도 함께요",
          "speaker_id": "2",
          "time": "20210825 18:12"
        }
      ]
