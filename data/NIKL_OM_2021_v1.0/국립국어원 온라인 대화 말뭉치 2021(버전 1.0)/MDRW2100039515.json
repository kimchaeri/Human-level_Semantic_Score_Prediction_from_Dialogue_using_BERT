      "utterance": [
        {
          "id": "MDRW2100039515.1.1",
          "form": "하남쭈꾸미를 검색하세요ㅋ",
          "original_form": "하남쭈꾸미를 검색하세요ㅋ",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039515.1.2",
          "form": "오 인기가 많은거네요",
          "original_form": "오 인기가 많은거네요",
          "speaker_id": "2",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100039515.1.3",
          "form": "한달에 한번씩사는사람도 있네요",
          "original_form": "한달에 한번씩사는사람도 있네요",
          "speaker_id": "2",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100039515.1.4",
          "form": "쟁여놔야해요ㅋㅋ",
          "original_form": "쟁여놔야해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039515.1.5",
          "form": "없으면 불안",
          "original_form": "없으면 불안",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039515.1.6",
          "form": "오 그정도예요?",
          "original_form": "오 그정도예요?",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100039515.1.7",
          "form": "어디서사세요?",
          "original_form": "어디서사세요?",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100039515.1.8",
          "form": "고양시요",
          "original_form": "고양시요",
          "speaker_id": "1",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039515.1.9",
          "form": "풉...",
          "original_form": "풉...",
          "speaker_id": "2",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039515.1.10",
          "form": "주문을 어느사이트에서 하시는지 여쭌건데\r\n어찌보면 거주지여쭌거같기도 하네요",
          "original_form": "주문을 어느사이트에서 하시는지 여쭌건데\r\n어찌보면 거주지여쭌거같기도 하네요",
          "speaker_id": "2",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039515.1.11",
          "form": "저는 대구요",
          "original_form": "저는 대구요",
          "speaker_id": "2",
          "time": "20210903 19:00"
        },
        {
          "id": "MDRW2100039515.1.12",
          "form": "아ㅋㅋ 그러네요ㅋㅋㅋ",
          "original_form": "아ㅋㅋ 그러네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039515.1.13",
          "form": "전거의 지에스프레시서 사요",
          "original_form": "전거의 지에스프레시서 사요",
          "speaker_id": "1",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039515.1.14",
          "form": "많은분들이 지에스 이야기하시네요",
          "original_form": "많은분들이 지에스 이야기하시네요",
          "speaker_id": "2",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039515.1.15",
          "form": "하남쭈꾸미 대체로 용두동쭈꾸미도 맛있다네요",
          "original_form": "하남쭈꾸미 대체로 용두동쭈꾸미도 맛있다네요",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039515.1.16",
          "form": "하남은 넘비싸요",
          "original_form": "하남은 넘비싸요",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039515.1.17",
          "form": "얼마예요?",
          "original_form": "얼마예요?",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039515.1.18",
          "form": "세일안하면 한팩마논넘어요",
          "original_form": "세일안하면 한팩마논넘어요",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039515.1.19",
          "form": "한팩이 몇인분이예요?",
          "original_form": "한팩이 몇인분이예요?",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100039515.1.20",
          "form": "둘이먹거나 야채많이 넣으면 셋이요",
          "original_form": "둘이먹거나 야채많이 넣으면 셋이요",
          "speaker_id": "1",
          "time": "20210903 20:39"
        }
      ]
