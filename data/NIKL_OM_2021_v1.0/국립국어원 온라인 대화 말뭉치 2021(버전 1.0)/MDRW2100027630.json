          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100027630.1.30",
          "form": "응응 ㅠ",
          "original_form": "응응 ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100027630.1.31",
