        },
        {
          "id": "MDRW2100027630.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100027630.1.26",
          "form": "해서 조금씩 체념하시....",
          "original_form": "해서 조금씩 체념하시....",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100027630.1.27",
          "form": "오오 name1네 부모님도 그러셨어?!",
          "original_form": "오오 &name1&네 부모님도 그러셨어?!",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100027630.1.28",
          "form": "와근데 확실히 이런거 보면 경제력이 중요해",
          "original_form": "와근데 확실히 이런거 보면 경제력이 중요해",
