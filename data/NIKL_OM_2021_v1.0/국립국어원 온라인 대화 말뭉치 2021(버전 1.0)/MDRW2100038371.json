      "utterance": [
        {
          "id": "MDRW2100038371.1.1",
          "form": "좋아하는 음료있으세요?",
          "original_form": "좋아하는 음료있으세요?",
          "speaker_id": "1",
          "time": "20210902 11:50"
        },
        {
          "id": "MDRW2100038371.1.2",
          "form": "저는 민트 음료 좋아해요",
          "original_form": "저는 민트 음료 좋아해요",
          "speaker_id": "2",
          "time": "20210902 11:51"
        },
        {
          "id": "MDRW2100038371.1.3",
          "form": "민트초코우유맛있던데ㅋㅋ",
          "original_form": "민트초코우유맛있던데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:10"
        },
        {
          "id": "MDRW2100038371.1.4",
          "form": "이미 먹어봤죠",
          "original_form": "이미 먹어봤죠",
          "speaker_id": "2",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100038371.1.5",
          "form": "근데 딱히 좋진 않았어요",
          "original_form": "근데 딱히 좋진 않았어요",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038371.1.6",
          "form": "추천해주실 음료있으세요?",
          "original_form": "추천해주실 음료있으세요?",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100038371.1.7",
          "form": "공차에서 파는 민트 음료 맛있어요",
          "original_form": "공차에서 파는 민트 음료 맛있어요",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038371.1.8",
          "form": "아! 한번마셔봐야겠네요!",
          "original_form": "아! 한번마셔봐야겠네요!",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100038371.1.9",
          "form": "민트에 버블 들어간게 잘 어울리더라구요",
          "original_form": "민트에 버블 들어간게 잘 어울리더라구요",
          "speaker_id": "2",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100038371.1.10",
          "form": "와 혹시 어떻게 주문하면 될지 얘기해주시겠어요?",
          "original_form": "와 혹시 어떻게 주문하면 될지 얘기해주시겠어요?",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100038371.1.11",
          "form": "주말에 그대로 시켜먹어보게요ㅎㅎ",
          "original_form": "주말에 그대로 시켜먹어보게요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100038371.1.12",
          "form": "민트 초코칩 밀크티에 펄 추가하시구",
          "original_form": "민트 초코칩 밀크티에 펄 추가하시구",
          "speaker_id": "2",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100038371.1.13",
          "form": "얼음은 꼭 빼달라고 하신후에 원하는 당도 맞추세요",
          "original_form": "얼음은 꼭 빼달라고 하신후에 원하는 당도 맞추세요",
          "speaker_id": "2",
          "time": "20210902 17:01"
        },
        {
          "id": "MDRW2100038371.1.14",
          "form": "얼음 넣으면 끝에 가서 싱거워지거든요",
          "original_form": "얼음 넣으면 끝에 가서 싱거워지거든요",
          "speaker_id": "2",
          "time": "20210902 17:01"
        },
        {
          "id": "MDRW2100038371.1.15",
          "form": "기본이 시럽몇번 펌핑되나요?",
          "original_form": "기본이 시럽몇번 펌핑되나요?",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100038371.1.16",
          "form": "2번은 합니다",
          "original_form": "2번은 합니다",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100038371.1.17",
          "form": "1번은 안한거랑 비슷하더라구요",
          "original_form": "1번은 안한거랑 비슷하더라구요",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100038371.1.18",
          "form": "아 그렇군요!",
          "original_form": "아 그렇군요!",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100038371.1.19",
          "form": "팁 감사합니다!",
          "original_form": "팁 감사합니다!",
          "speaker_id": "1",
