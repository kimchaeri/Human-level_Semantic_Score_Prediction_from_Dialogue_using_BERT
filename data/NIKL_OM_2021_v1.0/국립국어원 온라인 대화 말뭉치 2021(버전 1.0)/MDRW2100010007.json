      "utterance": [
        {
          "id": "MDRW2100010007.1.1",
          "form": "요즘 노동요로 예전 케이팝 노래 듣는다",
          "original_form": "요즘 노동요로 예전 케이팝 노래 듣는다",
          "speaker_id": "1",
          "time": "20210823 13:09"
        },
        {
          "id": "MDRW2100010007.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ나도 가끔 유튜브에 모음있는거 들어ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ나도 가끔 유튜브에 모음있는거 들어ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010007.1.3",
          "form": "추억돋아",
          "original_form": "추억돋아",
          "speaker_id": "2",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010007.1.4",
          "form": "인피니트꺼 듣는데",
          "original_form": "인피니트꺼 듣는데",
          "speaker_id": "1",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010007.1.5",
          "form": "와 진짜 추억돋아버렸다ㅋ",
          "original_form": "와 진짜 추억돋아버렸다ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010007.1.6",
          "form": "그때 너랑 놀던것도 생각나",
          "original_form": "그때 너랑 놀던것도 생각나",
          "speaker_id": "1",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010007.1.7",
          "form": "중3시절 급 아련",
          "original_form": "중3시절 급 아련",
          "speaker_id": "1",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010007.1.8",
          "form": "아 인피니트",
          "original_form": "아 인피니트",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010007.1.9",
          "form": "아련하다",
          "original_form": "아련하다",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010007.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010007.1.11",
          "form": "요새 인피니트뭐하지",
          "original_form": "요새 인피니트뭐하지",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010007.1.12",
          "form": "성규 울림 퇴사하고 다른애들 군대갔을걸?",
          "original_form": "성규 울림 퇴사하고 다른애들 군대갔을걸?",
          "speaker_id": "1",
          "time": "20210823 13:58"
        },
        {
          "id": "MDRW2100010007.1.13",
          "form": "성규 동우 성열 제대, 우현 복무중? 엘은 군대간듯",
          "original_form": "성규 동우 성열 제대, 우현 복무중? 엘은 군대간듯",
          "speaker_id": "1",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010007.1.14",
          "form": "헐 엘 군대??",
          "original_form": "헐 엘 군대??",
          "speaker_id": "2",
          "time": "20210823 17:56"
        },
        {
          "id": "MDRW2100010007.1.15",
          "form": "잘생겼는데 군대갔네..",
          "original_form": "잘생겼는데 군대갔네..",
          "speaker_id": "2",
          "time": "20210823 17:56"
        },
        {
          "id": "MDRW2100010007.1.16",
          "form": "진짜 못생긴넘들이 대신가라",
          "original_form": "진짜 못생긴넘들이 대신가라",
          "speaker_id": "1",
          "time": "20210823 18:03"
        },
        {
          "id": "MDRW2100010007.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:12"
        },
        {
          "id": "MDRW2100010007.1.18",
          "form": "아 오늘 경찰수업볼지 너는나의봄볼지 고민된다",
          "original_form": "아 오늘 경찰수업볼지 너는나의봄볼지 고민된다",
          "speaker_id": "2",
          "time": "20210823 18:12"
        },
        {
          "id": "MDRW2100010007.1.19",
          "form": "두개가 시간대가 겹쳐?",
          "original_form": "두개가 시간대가 겹쳐?",
          "speaker_id": "1",
          "time": "20210823 18:14"
        },
        {
          "id": "MDRW2100010007.1.20",
          "form": "행복한 고민이다ㅋㅋㅋ",
          "original_form": "행복한 고민이다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:14"
        },
        {
          "id": "MDRW2100010007.1.21",
          "form": "경찰수업이 9시 30분이고 너는나의봄이9시ㅋㅋㅋㅋ",
          "original_form": "경찰수업이 9시 30분이고 너는나의봄이9시ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:15"
        },
        {
          "id": "MDRW2100010007.1.22",
          "form": "너는나의봄은 넷플에있어서 경찰수업볼까",
          "original_form": "너는나의봄은 넷플에있어서 경찰수업볼까",
          "speaker_id": "2",
          "time": "20210823 18:15"
        },
        {
          "id": "MDRW2100010007.1.23",
          "form": "넷플에있으면 당연 경찰수업이지",
          "original_form": "넷플에있으면 당연 경찰수업이지",
          "speaker_id": "1",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010007.1.24",
          "form": "ㅋㅋㅋㅋ그치 티빙이랑 웨이브도 한글자막있었으면 좋겠어..",
          "original_form": "ㅋㅋㅋㅋ그치 티빙이랑 웨이브도 한글자막있었으면 좋겠어..",
          "speaker_id": "2",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010007.1.25",
          "form": "요즘 신곡은 들을게 없어",
          "original_form": "요즘 신곡은 들을게 없어",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100010007.1.26",
          "form": "그래서 계속 예전노래들어",
          "original_form": "그래서 계속 예전노래들어",
          "speaker_id": "1",
          "time": "20210824 15:38"
        }
      ]
