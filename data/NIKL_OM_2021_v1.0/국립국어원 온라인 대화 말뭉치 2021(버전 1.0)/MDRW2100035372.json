      "utterance": [
        {
          "id": "MDRW2100035372.1.1",
          "form": "만두 만들려고 하시나요?",
          "original_form": "만두 만들려고 하시나요?",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100035372.1.2",
          "form": "김말이 대신에 고.추로 해보려고요 ㅎㅎ",
          "original_form": "김말이 대신에 고.추로 해보려고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100035372.1.3",
          "form": "만두는 귀찮아서 못해용",
          "original_form": "만두는 귀찮아서 못해용",
          "speaker_id": "1",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100035372.1.4",
          "form": "아하 그걸 만드신다는 얘기셨군요",
          "original_form": "아하 그걸 만드신다는 얘기셨군요",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100035372.1.5",
          "form": "그것도 손이 은근 많이 갈거 같아요",
          "original_form": "그것도 손이 은근 많이 갈거 같아요",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100035372.1.6",
          "form": "ㅋㅋㅋㅋ 집에 고추가 많아서 제가 열심히 뭘 만들어야해요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 집에 고추가 많아서 제가 열심히 뭘 만들어야해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100035372.1.7",
          "form": "많으시군요!  구매하신건가요?",
          "original_form": "많으시군요!  구매하신건가요?",
          "speaker_id": "2",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100035372.1.8",
          "form": "아빠가 얻어왔는데\n어제 어느 요리양상을보고 영감을 얻었어요",
          "original_form": "아빠가 얻어왔는데\n어제 어느 요리양상을보고 영감을 얻었어요",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100035372.1.9",
          "form": "이제 그 영감받은걸 실현시킬 일만 남았네요",
          "original_form": "이제 그 영감받은걸 실현시킬 일만 남았네요",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100035372.1.10",
          "form": "ㅋㅋㅋ 맞아요.. 그런데 지금은 별로 안하고싶네요 ㅜㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 맞아요.. 그런데 지금은 별로 안하고싶네요 ㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:01"
        },
        {
          "id": "MDRW2100035372.1.11",
          "form": "요리도 하고 싶을 때가 있고,\n정말 하기 싫은 날이 있죠",
          "original_form": "요리도 하고 싶을 때가 있고,\n정말 하기 싫은 날이 있죠",
          "speaker_id": "2",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100035372.1.12",
          "form": "ㅋㅋ어떨때가 제일 하기싫으세요?",
          "original_form": "ㅋㅋ어떨때가 제일 하기싫으세요?",
          "speaker_id": "1",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100035372.1.13",
          "form": "그냥 그런 날이 있더라구요",
          "original_form": "그냥 그런 날이 있더라구요",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100035372.1.14",
          "form": "몸도 마음도 피곤하고 만사가 귀찮은 날?ㅋㅋㅋ",
          "original_form": "몸도 마음도 피곤하고 만사가 귀찮은 날?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100035372.1.15",
          "form": "아... 전날에 별일 없어도요??",
          "original_form": "아... 전날에 별일 없어도요??",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100035372.1.16",
          "form": "네 몸이 아프거나 해서 그런 날도 있지만",
          "original_form": "네 몸이 아프거나 해서 그런 날도 있지만",
          "speaker_id": "2",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100035372.1.17",
          "form": "일어났을때 딱 몸이 무겁고 피곤하고 그런 날이 있더라구요",
          "original_form": "일어났을때 딱 몸이 무겁고 피곤하고 그런 날이 있더라구요",
          "speaker_id": "2",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100035372.1.18",
          "form": "아.. 그러시군요.. 그런 날엔 어쩔수없죠",
          "original_form": "아.. 그러시군요.. 그런 날엔 어쩔수없죠",
          "speaker_id": "1",
          "time": "20210829 16:56"
        }
      ]
