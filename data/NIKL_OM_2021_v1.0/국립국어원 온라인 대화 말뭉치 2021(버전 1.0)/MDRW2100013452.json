      "utterance": [
        {
          "id": "MDRW2100013452.1.1",
          "form": "모기는 근데 또",
          "original_form": "모기는 근데 또",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013452.1.2",
          "form": "엄청많지않나요",
          "original_form": "엄청많지않나요",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013452.1.3",
          "form": "맞아요..모기는 이제 그만..",
          "original_form": "맞아요..모기는 이제 그만..",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013452.1.4",
          "form": "헌혈했어요 거의 ㅋㅋㅋ",
          "original_form": "헌혈했어요 거의 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013452.1.5",
          "form": "저는 피부 예민한편이라",
          "original_form": "저는 피부 예민한편이라",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013452.1.6",
          "form": "모기 물리면 2주가량 부어있어요",
          "original_form": "모기 물리면 2주가량 부어있어요",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013452.1.7",
          "form": "헌혈정도면 많이 가려우셨겠다..",
          "original_form": "헌혈정도면 많이 가려우셨겠다..",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013452.1.8",
          "form": "2주나요???",
          "original_form": "2주나요???",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013452.1.9",
          "form": "헐 온몸이 붓겠는데요",
          "original_form": "헐 온몸이 붓겠는데요",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013452.1.10",
          "form": "아주 어릴때 모기 처음에 물렸을때",
          "original_form": "아주 어릴때 모기 처음에 물렸을때",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013452.1.11",
          "form": "엄마가 놀라서 병원 데려갔다는 말을 들었어요",
          "original_form": "엄마가 놀라서 병원 데려갔다는 말을 들었어요",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013452.1.12",
          "form": "진짜 놀라실만 했을거같아요",
          "original_form": "진짜 놀라실만 했을거같아요",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013452.1.13",
          "form": "그렇게 부으면..",
          "original_form": "그렇게 부으면..",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013452.1.14",
          "form": "그래서 여름철마다 꼼꼼하게 씻고 집안 청소 더 열심히해요",
          "original_form": "그래서 여름철마다 꼼꼼하게 씻고 집안 청소 더 열심히해요",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013452.1.15",
          "form": "모기 같은거 발견하면 바로바로 잡고",
          "original_form": "모기 같은거 발견하면 바로바로 잡고",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013452.1.16",
          "form": "여름이 올때마다 창틀 부위 점검하고",
          "original_form": "여름이 올때마다 창틀 부위 점검하고",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013452.1.17",
          "form": "여름은 청량해서 좋긴한데 여러모로 힘든 계절인거같아요",
          "original_form": "여름은 청량해서 좋긴한데 여러모로 힘든 계절인거같아요",
          "speaker_id": "2",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100013452.1.18",
          "form": "그 전기모기채 저도 엄청 휘둘러요",
          "original_form": "그 전기모기채 저도 엄청 휘둘러요",
          "speaker_id": "1",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100013452.1.19",
          "form": "창틀 점검까지 ㅋㅋ ㅜㅜ",
          "original_form": "창틀 점검까지 ㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100013452.1.20",
          "form": "모기채 사용하는거 보면",
          "original_form": "모기채 사용하는거 보면",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100013452.1.21",
          "form": "뭔가 쉐도우복싱처럼 쉐도우테니스닝하는거 같아요",
          "original_form": "뭔가 쉐도우복싱처럼 쉐도우테니스닝하는거 같아요",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100013452.1.22",
          "form": "저는 정신과 근육이 따로 놀아서",
          "original_form": "저는 정신과 근육이 따로 놀아서",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100013452.1.23",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋ혼자만의싸움",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋ혼자만의싸움",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100013452.1.24",
          "form": "전기 모기채 쓰면 문어처럼 흐느적 흐느적거려요",
          "original_form": "전기 모기채 쓰면 문어처럼 흐느적 흐느적거려요",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100013452.1.25",
          "form": "맞아요 그리고 좀 무서워요 ㅋㅋㅋㅋㅋ 감전될까봐",
          "original_form": "맞아요 그리고 좀 무서워요 ㅋㅋㅋㅋㅋ 감전될까봐",
          "speaker_id": "1",
          "time": "20210825 14:45"
        }
      ]
