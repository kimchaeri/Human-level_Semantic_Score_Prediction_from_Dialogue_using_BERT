      "utterance": [
        {
          "id": "MDRW2100005521.1.1",
          "form": "정말 음식이 맛없으며",
          "original_form": "정말 음식이 맛없으며",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005521.1.2",
          "form": "삶이 퍽퍽해지더하고요 ㅠㅠ",
          "original_form": "삶이 퍽퍽해지더하고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005521.1.3",
          "form": "맞아요..ㅠㅠ 먹는게 조금이라도 맛있어야 운동할맛이 나죠",
          "original_form": "맞아요..ㅠㅠ 먹는게 조금이라도 맛있어야 운동할맛이 나죠",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005521.1.4",
          "form": "치팅데이는 안가지세요?",
          "original_form": "치팅데이는 안가지세요?",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005521.1.5",
          "form": "얼마전애 두달만에 한번 가져봤어요",
          "original_form": "얼마전애 두달만에 한번 가져봤어요",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005521.1.6",
          "form": "앞으로는 한 이주에 한번 가져보려고요",
          "original_form": "앞으로는 한 이주에 한번 가져보려고요",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005521.1.7",
          "form": "항상 닭슴살만 먹고살수는 없으니까",
          "original_form": "항상 닭슴살만 먹고살수는 없으니까",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005521.1.8",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005521.1.9",
          "form": "특별히 좋어하는 음식 있으세요?",
          "original_form": "특별히 좋어하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005521.1.10",
          "form": "저는 떡볶이 엄청 좋아해요 .. 배달음식 시키면 떡볶이 거의 먹어요",
          "original_form": "저는 떡볶이 엄청 좋아해요 .. 배달음식 시키면 떡볶이 거의 먹어요",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005521.1.11",
          "form": "두달만에 뭐 드셨어요? 엄청 맛있게 드셨을 것 같아요",
          "original_form": "두달만에 뭐 드셨어요? 엄청 맛있게 드셨을 것 같아요",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005521.1.12",
          "form": "아 떡볶이 너무 맛잇죠ㅠㅠ 어디 떡볶이 제일 좋아하시나요",
          "original_form": "아 떡볶이 너무 맛잇죠ㅠㅠ 어디 떡볶이 제일 좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005521.1.13",
          "form": "저는 타코 먹고왔어요! 비싼 만큼 엄청 맛있더라고요 ㅠㅠ",
          "original_form": "저는 타코 먹고왔어요! 비싼 만큼 엄청 맛있더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005521.1.14",
          "form": "행복한 하루..",
          "original_form": "행복한 하루..",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005521.1.15",
          "form": "타코 저도 진짜 좋아해요.. 왕창 싸먹으면 너무 맛있죠",
          "original_form": "타코 저도 진짜 좋아해요.. 왕창 싸먹으면 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005521.1.16",
          "form": "저는 엽떡이나 신전 좋아해요!!",
          "original_form": "저는 엽떡이나 신전 좋아해요!!",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005521.1.17",
          "form": "name1님은 어떤 떡볶이 배달시켜드세요??",
          "original_form": "&name1&님은 어떤 떡볶이 배달시켜드세요??",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005521.1.18",
          "form": "저는 엽떡파예요!!",
          "original_form": "저는 엽떡파예요!!",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005521.1.19",
          "form": "식단조절전에는 맨날 시켜먹었어요 ㅋㅋ",
          "original_form": "식단조절전에는 맨날 시켜먹었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005521.1.20",
          "form": "가끔 특별하게 중당 추가해먹고",
          "original_form": "가끔 특별하게 중당 추가해먹고",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005521.1.21",
          "form": "로제맛도 너무 맛있어요 ㅠㅠ",
          "original_form": "로제맛도 너무 맛있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005521.1.22",
          "form": "엽떡 로제맛 맛있더라구요 ㅠㅠ",
          "original_form": "엽떡 로제맛 맛있더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005521.1.23",
          "form": "다른 곳 시켜봐도 맛있긴하지만 결국에는 엽떡으로 돌아오게 되더라구요",
          "original_form": "다른 곳 시켜봐도 맛있긴하지만 결국에는 엽떡으로 돌아오게 되더라구요",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005521.1.24",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100005521.1.25",
          "form": "비싸다 생각해도 그 맛을 잊지를 못해요",
          "original_form": "비싸다 생각해도 그 맛을 잊지를 못해요",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005521.1.26",
          "form": "토실토실한 떡 ㅠㅠ",
          "original_form": "토실토실한 떡 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005521.1.27",
          "form": "사실 사이드는 진짜 부실한데",
          "original_form": "사실 사이드는 진짜 부실한데",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005521.1.28",
          "form": "저는 튀김은 신전이 좋더라고요 어묵튀김 최고",
          "original_form": "저는 튀김은 신전이 좋더라고요 어묵튀김 최고",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005521.1.29",
          "form": "맞아요 신전은 튀김이랑 김밥이 맛집이에요..",
          "original_form": "맞아요 신전은 튀김이랑 김밥이 맛집이에요..",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100005521.1.30",
          "form": "신전 맛이 가끔 땡길때가 있더라구요",
          "original_form": "신전 맛이 가끔 땡길때가 있더라구요",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005521.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100005521.1.32",
          "form": "참 로제 먹어봤는데",
          "original_form": "참 로제 먹어봤는데",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100005521.1.33",
          "form": "소스기 기가막히던데요?",
          "original_form": "소스기 기가막히던데요?",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100005521.1.34",
          "form": "저 남은 소스로 파스타도 해먹었어요",
          "original_form": "저 남은 소스로 파스타도 해먹었어요",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100005521.1.35",
          "form": "신전 로제랑 중국당면 나온다는 얘기들었을때 진짜 너무 좋더라구요..",
          "original_form": "신전 로제랑 중국당면 나온다는 얘기들었을때 진짜 너무 좋더라구요..",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100005521.1.36",
          "form": "신전 기본 맛자체가 맛있어서 로제도 맛있었어요..",
          "original_form": "신전 기본 맛자체가 맛있어서 로제도 맛있었어요..",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100005521.1.37",
          "form": "중국당면 나왔어요?!",
          "original_form": "중국당면 나왔어요?!",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100005521.1.38",
          "form": "와 맛없을 수가 없죠",
          "original_form": "와 맛없을 수가 없죠",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100005521.1.39",
          "form": "네네 토핑추가 절대없던 신전이 중국당면 사리 생겼더라구요",
          "original_form": "네네 토핑추가 절대없던 신전이 중국당면 사리 생겼더라구요",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100005521.1.40",
          "form": "꼭 드셔보세요 맛있어요ㅠㅠ",
          "original_form": "꼭 드셔보세요 맛있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:23"
        }
      ]
