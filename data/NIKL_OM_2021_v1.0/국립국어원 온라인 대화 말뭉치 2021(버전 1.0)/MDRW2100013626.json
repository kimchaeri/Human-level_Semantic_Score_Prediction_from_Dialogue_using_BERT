          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013626.1.15",
          "form": "전시회 같은건",
          "original_form": "전시회 같은건",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013626.1.16",
          "form": "보러 안가세요?",
          "original_form": "보러 안가세요?",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013626.1.17",
          "form": "저는 전시회 좋아해요",
          "original_form": "저는 전시회 좋아해요",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013626.1.18",
          "form": "얼마전에 피아노연주회갔어요",
          "original_form": "얼마전에 피아노연주회갔어요",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013626.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013626.1.20",
          "form": "다 좋아하는데",
          "original_form": "다 좋아하는데",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013626.1.21",
          "form": "저도 피아노 연주 좋아해요",
          "original_form": "저도 피아노 연주 좋아해요",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013626.1.22",
          "form": "시간이 없어요",
          "original_form": "시간이 없어요",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013626.1.23",
          "form": "저는.나중에",
