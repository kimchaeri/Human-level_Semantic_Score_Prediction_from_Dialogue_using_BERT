      "utterance": [
        {
          "id": "MDRW2100038102.1.1",
          "form": "비가 멈춤 좋겟어요",
          "original_form": "비가 멈춤 좋겟어요",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038102.1.2",
          "form": "다음주까지 온다는것",
          "original_form": "다음주까지 온다는것",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038102.1.3",
          "form": "같더라고요ㅠ",
          "original_form": "같더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038102.1.4",
          "form": "계절 머조아하세요",
          "original_form": "계절 머조아하세요",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038102.1.5",
          "form": "초겨울이요!",
          "original_form": "초겨울이요!",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038102.1.6",
          "form": "봄가을 좋아했었는데",
          "original_form": "봄가을 좋아했었는데",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038102.1.7",
          "form": "환절기에 비염이 너무심해져서",
          "original_form": "환절기에 비염이 너무심해져서",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038102.1.8",
          "form": "차라리 겨울이 낫더라고요,,",
          "original_form": "차라리 겨울이 낫더라고요,,",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038102.1.9",
          "form": "아아 ㅂㅣ염 ㅠㅠ",
          "original_form": "아아 ㅂㅣ염 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038102.1.10",
          "form": "옷은 머입는거조아하세요",
          "original_form": "옷은 머입는거조아하세요",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
