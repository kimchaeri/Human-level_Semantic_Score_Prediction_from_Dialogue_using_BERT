        {
          "id": "MDRW2100040837.1.1",
          "form": "ㅎㅎ쇼핑 좋아하시나요",
          "original_form": "ㅎㅎ쇼핑 좋아하시나요",
          "speaker_id": "2",
          "time": "20210905 09:32"
        },
        {
          "id": "MDRW2100040837.1.2",
          "form": "좋아하긴 하지만 돈이 부족해서요",
          "original_form": "좋아하긴 하지만 돈이 부족해서요",
          "speaker_id": "1",
          "time": "20210905 09:33"
        },
        {
          "id": "MDRW2100040837.1.3",
          "form": "네이버 페이 포인트 추천합니다",
          "original_form": "네이버 페이 포인트 추천합니다",
          "speaker_id": "1",
          "time": "20210905 09:33"
        },
        {
          "id": "MDRW2100040837.1.4",
          "form": "오 네이버페이 쓰는 분 많던데",
          "original_form": "오 네이버페이 쓰는 분 많던데",
          "speaker_id": "2",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100040837.1.5",
          "form": "name1님도 추천하시는군요!",
          "original_form": "&name1&님도 추천하시는군요!",
          "speaker_id": "2",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100040837.1.6",
          "form": "포인트 빨리 쌓여요",
          "original_form": "포인트 빨리 쌓여요",
          "speaker_id": "1",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100040837.1.7",
          "form": "쇼핑은 주로 인터넷으로 하세요?",
          "original_form": "쇼핑은 주로 인터넷으로 하세요?",
          "speaker_id": "2",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100040837.1.8",
          "form": "넹넹 주로 인터넷으로 해요",
          "original_form": "넹넹 주로 인터넷으로 해요",
          "speaker_id": "1",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100040837.1.9",
          "form": "포인트 중요하죠!!",
          "original_form": "포인트 중요하죠!!",
          "speaker_id": "2",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100040837.1.10",
          "form": "인터넷 쇼핑 저도 좋아해요",
          "original_form": "인터넷 쇼핑 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100040837.1.11",
          "form": "근데 하다보면 장바구니에 가득 차서",
          "original_form": "근데 하다보면 장바구니에 가득 차서",
          "speaker_id": "2",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100040837.1.12",
          "form": "그죠 많이 하면 사고 싶은게 많아서요",
          "original_form": "그죠 많이 하면 사고 싶은게 많아서요",
          "speaker_id": "1",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100040837.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100040837.1.14",
          "form": "보다보면 필요한 것 같고",
          "original_form": "보다보면 필요한 것 같고",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100040837.1.15",
          "form": "안사가 보면까 먹더라고요",
          "original_form": "안사가 보면까 먹더라고요",
          "speaker_id": "1",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100040837.1.16",
          "form": "네 저는 그래서 급한 것 아님",
          "original_form": "네 저는 그래서 급한 것 아님",
          "speaker_id": "2",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100040837.1.17",
          "form": "장바구니 담아놓고 며칠 두고봐요",
          "original_form": "장바구니 담아놓고 며칠 두고봐요",
          "speaker_id": "2",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100040837.1.18",
          "form": "저도 그래요",
