      "utterance": [
        {
          "id": "MDRW2100039177.1.1",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "1",
          "time": "20210903 12:46"
        },
        {
          "id": "MDRW2100039177.1.2",
          "form": "홈트 꾸준히 하고 있어요",
          "original_form": "홈트 꾸준히 하고 있어요",
          "speaker_id": "2",
          "time": "20210903 12:48"
        },
        {
          "id": "MDRW2100039177.1.3",
          "form": "오~~~ 유튜브 보고 하세요??",
          "original_form": "오~~~ 유튜브 보고 하세요??",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100039177.1.4",
          "form": "네ㅎㅎ 빅씨쓰 유튜브 보고 해요",
          "original_form": "네ㅎㅎ 빅씨쓰 유튜브 보고 해요",
          "speaker_id": "2",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100039177.1.5",
          "form": "앗 저도 빅씨스 봐요!",
          "original_form": "앗 저도 빅씨스 봐요!",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039177.1.6",
          "form": "배경이 파랑색이라 보기 좋아요",
          "original_form": "배경이 파랑색이라 보기 좋아요",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039177.1.7",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039177.1.8",
          "form": "노래 선정도 너무 잘하시구요",
          "original_form": "노래 선정도 너무 잘하시구요",
          "speaker_id": "2",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039177.1.9",
          "form": "네네! 전 그 40분짜리 유산소 봤어요!",
          "original_form": "네네! 전 그 40분짜리 유산소 봤어요!",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039177.1.10",
          "form": "와 40분짜리 힘들던데",
          "original_form": "와 40분짜리 힘들던데",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039177.1.11",
          "form": "맞아요 끝나면 땀이 우수수 쏟아져요ㅎㅎ",
          "original_form": "맞아요 끝나면 땀이 우수수 쏟아져요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039177.1.12",
          "form": "그럴 때 샤워 한번 하고 나면 개운하죠",
          "original_form": "그럴 때 샤워 한번 하고 나면 개운하죠",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039177.1.13",
          "form": "맞아요 맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요 맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039177.1.14",
          "form": "방금도 빅씨스 운동하고 샤워했어요ㅌㅋ",
          "original_form": "방금도 빅씨스 운동하고 샤워했어요ㅌㅋ",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039177.1.15",
          "form": "ㅎㅎ 좋아요",
          "original_form": "ㅎㅎ 좋아요",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039177.1.16",
          "form": "다이어트는 하시나요??",
          "original_form": "다이어트는 하시나요??",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039177.1.17",
          "form": "홈트로 다이어트 대신하고 있어요",
          "original_form": "홈트로 다이어트 대신하고 있어요",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039177.1.18",
          "form": "저도 더 찌지 않도록 열심히 홈트중이에요",
          "original_form": "저도 더 찌지 않도록 열심히 홈트중이에요",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039177.1.19",
          "form": "코로나 이후 살이 꽤 쪄서요",
          "original_form": "코로나 이후 살이 꽤 쪄서요",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039177.1.20",
          "form": "확찐자가 되어버렸죠",
          "original_form": "확찐자가 되어버렸죠",
