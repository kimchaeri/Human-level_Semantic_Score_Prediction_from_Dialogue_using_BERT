      "utterance": [
        {
          "id": "MDRW2100004496.1.1",
          "form": "나 아점 감자치즈전 먹었다",
          "original_form": "나 아점 감자치즈전 먹었다",
          "speaker_id": "1",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004496.1.2",
          "form": "요새 우리집 주메뉴인데 맛있다",
          "original_form": "요새 우리집 주메뉴인데 맛있다",
          "speaker_id": "1",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004496.1.3",
          "form": "나는 피자 먹었어",
          "original_form": "나는 피자 먹었어",
          "speaker_id": "2",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004496.1.4",
          "form": "배달음식이 있어서 다행인 요즘이야",
          "original_form": "배달음식이 있어서 다행인 요즘이야",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004496.1.5",
          "form": "아점을 먹었더니 지금 애매한시간인데 슬슬 배고파짐",
          "original_form": "아점을 먹었더니 지금 애매한시간인데 슬슬 배고파짐",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004496.1.6",
          "form": "나가서 간단하게 간식거리 사먹고올까",
          "original_form": "나가서 간단하게 간식거리 사먹고올까",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004496.1.7",
          "form": "(몰래",
          "original_form": "(몰래",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004496.1.8",
          "form": "크로플 어때?",
          "original_form": "크로플 어때?",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004496.1.9",
          "form": "달달하니... 아주 좋지",
          "original_form": "달달하니... 아주 좋지",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004496.1.10",
          "form": "요근처에 크로플파는데가...",
          "original_form": "요근처에 크로플파는데가...",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004496.1.11",
          "form": "컴포즈커피있으면 딱인데",
          "original_form": "컴포즈커피있으면 딱인데",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004496.1.12",
          "form": "그게 없네",
          "original_form": "그게 없네",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004496.1.13",
          "form": "그냥 소금빵하나 사먹을까",
          "original_form": "그냥 소금빵하나 사먹을까",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004496.1.14",
          "form": "얘기하니 나도 먹고 싶네",
          "original_form": "얘기하니 나도 먹고 싶네",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004496.1.15",
          "form": "소금빵?",
          "original_form": "소금빵?",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004496.1.16",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004496.1.17",
          "form": "맛있음",
          "original_form": "맛있음",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004496.1.18",
          "form": "가염버터를 가운데에두고 빵반죽을 말아서 우에 소금을 뿌려굽는빵인데",
          "original_form": "가염버터를 가운데에두고 빵반죽을 말아서 우에 소금을 뿌려굽는빵인데",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004496.1.19",
          "form": "짭잘하고 고소하니 진짜맛있음",
          "original_form": "짭잘하고 고소하니 진짜맛있음",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004496.1.20",
          "form": "으아악",
          "original_form": "으아악",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004496.1.21",
          "form": "지금 먹고 싶다 장난 아닌 맛이겠다",
          "original_form": "지금 먹고 싶다 장난 아닌 맛이겠다",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004496.1.22",
          "form": "아 먹는얘기하니깐 급격하게 배고파지기 시작함",
          "original_form": "아 먹는얘기하니깐 급격하게 배고파지기 시작함",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004496.1.23",
          "form": "얼른 옷갈아입고 산책갔다온다고하면서 빵을 사먹어야만....!",
          "original_form": "얼른 옷갈아입고 산책갔다온다고하면서 빵을 사먹어야만....!",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004496.1.24",
          "form": "우리집앞에 베이커리가 꽤 유명한 베이커리더라구",
          "original_form": "우리집앞에 베이커리가 꽤 유명한 베이커리더라구",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004496.1.25",
          "form": "그래서 그건 좋더라",
          "original_form": "그래서 그건 좋더라",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004496.1.26",
          "form": "지금 날 데리러 와... 와서 베이커리에 데리고 가..",
          "original_form": "지금 날 데리러 와... 와서 베이커리에 데리고 가..",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004496.1.27",
          "form": "상현동쪽에 본누벨이라고 아니",
          "original_form": "상현동쪽에 본누벨이라고 아니",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004496.1.28",
          "form": "거기도 유명한 베이커리더라",
          "original_form": "거기도 유명한 베이커리더라",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004496.1.29",
          "form": "담에 함같이가자",
          "original_form": "담에 함같이가자",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004496.1.30",
          "form": "지금... 차를 몰고 오는거야",
          "original_form": "지금... 차를 몰고 오는거야",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004496.1.31",
          "form": "가자! 탄수화물의 세계로!",
          "original_form": "가자! 탄수화물의 세계로!",
          "speaker_id": "2",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004496.1.32",
          "form": "아 지금은 좀 그렇지 않냐",
          "original_form": "아 지금은 좀 그렇지 않냐",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004496.1.33",
          "form": "차타고다면 저녁먹을시간임",
          "original_form": "차타고다면 저녁먹을시간임",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004496.1.34",
          "form": "저녁은 뭐 먹을건데..?",
          "original_form": "저녁은 뭐 먹을건데..?",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004496.1.35",
          "form": "저녁은ㅁ....ㅁ몰라?",
          "original_form": "저녁은ㅁ....ㅁ몰라?",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004496.1.36",
          "form": "저녁 차리는거 귀찮은데",
          "original_form": "저녁 차리는거 귀찮은데",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004496.1.37",
          "form": "요즘은 밥 먹기가 왜이리 싫은지",
          "original_form": "요즘은 밥 먹기가 왜이리 싫은지",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004496.1.38",
          "form": "그래도 사람은 먹어야한다....",
          "original_form": "그래도 사람은 먹어야한다....",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004496.1.39",
          "form": "돈까쓰먹고싶다",
          "original_form": "돈까쓰먹고싶다",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004496.1.40",
          "form": "나베돈까쓰가 먹고싶어짐",
          "original_form": "나베돈까쓰가 먹고싶어짐",
          "speaker_id": "1",
          "time": "20210821 17:22"
        }
      ]
    }
  ]
