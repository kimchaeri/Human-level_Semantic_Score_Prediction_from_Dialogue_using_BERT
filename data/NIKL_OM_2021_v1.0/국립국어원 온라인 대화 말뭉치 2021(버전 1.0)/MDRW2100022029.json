      "utterance": [
        {
          "id": "MDRW2100022029.1.1",
          "form": "저는 군대 가는 대신에 항해사 였어요ㅎㅎ",
          "original_form": "저는 군대 가는 대신에 항해사 였어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100022029.1.2",
          "form": "3년 근무해서 면제ㅎㅎ",
          "original_form": "3년 근무해서 면제ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100022029.1.3",
          "form": "군대 대신 3년간 항해사로 일했어요ㅎ 그래서 4주만 군대서 훈련받으면됬어요ㅎㅎ",
          "original_form": "군대 대신 3년간 항해사로 일했어요ㅎ 그래서 4주만 군대서 훈련받으면됬어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100022029.1.4",
          "form": "헐 우와",
          "original_form": "헐 우와",
          "speaker_id": "2",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022029.1.5",
          "form": "생각보다 멋있는 이유였네여",
          "original_form": "생각보다 멋있는 이유였네여",
          "speaker_id": "2",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022029.1.6",
          "form": "이게 멋있는이유인가요?ㄹ",
          "original_form": "이게 멋있는이유인가요?ㄹ",
          "speaker_id": "1",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022029.1.7",
          "form": "노예를 3년한건데요?ㅎㅎㅎㅎㅎ",
          "original_form": "노예를 3년한건데요?ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022029.1.8",
          "form": "막 배 몰고 하시는 거 아니에요??",
          "original_form": "막 배 몰고 하시는 거 아니에요??",
          "speaker_id": "2",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022029.1.9",
          "form": "아 그런거에요??ㅋㅋㅋㅋㅋㅋ 저 막 해경처럼 배 몰고 하신 줄",
          "original_form": "아 그런거에요??ㅋㅋㅋㅋㅋㅋ 저 막 해경처럼 배 몰고 하신 줄",
          "speaker_id": "2",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022029.1.10",
          "form": "배를 몰고 그랬죠ㅎ 물지는 않았어요ㅎ",
          "original_form": "배를 몰고 그랬죠ㅎ 물지는 않았어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022029.1.11",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022029.1.12",
          "form": "배운전 했어요ㅎ",
          "original_form": "배운전 했어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022029.1.13",
          "form": "재밌게ㅎㅎ 360도 돌려가면서ㅎㅎ",
          "original_form": "재밌게ㅎㅎ 360도 돌려가면서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022029.1.14",
          "form": "ㅎㅎㅎ 다른데서는 그런 농담 하지마세요 ㅎㅎ",
          "original_form": "ㅎㅎㅎ 다른데서는 그런 농담 하지마세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022029.1.15",
          "form": "ㅎㅎㅎㅎㄹㄹㄹㄹㄹ",
          "original_form": "ㅎㅎㅎㅎㄹㄹㄹㄹㄹ",
          "speaker_id": "1",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100022029.1.16",
          "form": "엄청... 썰렁하네여",
          "original_form": "엄청... 썰렁하네여",
          "speaker_id": "2",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100022029.1.17",
          "form": "재밌진않지만",
          "original_form": "재밌진않지만",
          "speaker_id": "1",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100022029.1.18",
          "form": "새벽이라 그러신거죠?? ㅋㅋ",
          "original_form": "새벽이라 그러신거죠?? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100022029.1.19",
          "form": "그래도 name2님 오늘하루 더웠으니깐",
          "original_form": "그래도 &name2&님 오늘하루 더웠으니깐",
          "speaker_id": "1",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100022029.1.20",
          "form": "시원해지시라고ㅎㅎㅎ",
          "original_form": "시원해지시라고ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100022029.1.21",
          "form": "지금.직업은 그럼 뭐세요?",
          "original_form": "지금.직업은 그럼 뭐세요?",
          "speaker_id": "2",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100022029.1.22",
          "form": "새벽에는 원래 판단이 흐려져요ㅎ",
          "original_form": "새벽에는 원래 판단이 흐려져요ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100022029.1.23",
          "form": "지금은 선박 안전관리자에요ㅎㅎ",
          "original_form": "지금은 선박 안전관리자에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100022029.1.24",
