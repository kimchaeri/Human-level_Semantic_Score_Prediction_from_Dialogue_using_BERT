      "utterance": [
        {
          "id": "MDRW2100041279.1.1",
          "form": "네넹 삼굡 먹엇어여 ㅎ",
          "original_form": "네넹 삼굡 먹엇어여 ㅎ",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100041279.1.2",
          "form": "삼겹살 곱배기 인건가요?",
          "original_form": "삼겹살 곱배기 인건가요?",
          "speaker_id": "1",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100041279.1.3",
          "form": "아뇨 ㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100041279.1.4",
          "form": "그럼 삼굡이 뭐예요?",
          "original_form": "그럼 삼굡이 뭐예요?",
          "speaker_id": "1",
          "time": "20210906 06:52"
        },
        {
          "id": "MDRW2100041279.1.5",
          "form": "그냥 삽겹살 귀엽게 ㅋㅋㅋ표현을",
          "original_form": "그냥 삽겹살 귀엽게 ㅋㅋㅋ표현을",
          "speaker_id": "2",
          "time": "20210906 07:48"
        },
        {
          "id": "MDRW2100041279.1.6",
          "form": "아~ 제가 이런거에 약해서..ㅎㅎ",
          "original_form": "아~ 제가 이런거에 약해서..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 08:31"
        },
        {
          "id": "MDRW2100041279.1.7",
          "form": "줄임말인줄 알았네요",
          "original_form": "줄임말인줄 알았네요",
          "speaker_id": "1",
          "time": "20210906 08:31"
        },
        {
          "id": "MDRW2100041279.1.8",
          "form": "저는 어제 저녁 갈치구이 해서 먹었어요",
          "original_form": "저는 어제 저녁 갈치구이 해서 먹었어요",
          "speaker_id": "1",
          "time": "20210906 08:31"
        },
        {
          "id": "MDRW2100041279.1.9",
          "form": "아침은 드셨나요?",
          "original_form": "아침은 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 08:32"
        },
        {
          "id": "MDRW2100041279.1.10",
          "form": "네네 김치찌개에 돌김 구어서 든든히 먹엇어요 ㅎㅎ",
          "original_form": "네네 김치찌개에 돌김 구어서 든든히 먹엇어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 09:36"
        },
        {
          "id": "MDRW2100041279.1.11",
          "form": "아침 잘 챙겨먹는군요!",
          "original_form": "아침 잘 챙겨먹는군요!",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041279.1.12",
          "form": "대체로 아침을 잘 안드시더라구요 물어보면",
          "original_form": "대체로 아침을 잘 안드시더라구요 물어보면",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041279.1.13",
          "form": "저희집도 아침은 꼭 먹는 편이라서 반갑네요",
          "original_form": "저희집도 아침은 꼭 먹는 편이라서 반갑네요",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041279.1.14",
          "form": "조금 성가시긴 하지만 아침밥 든든히 먹어야죠 ㅎ",
          "original_form": "조금 성가시긴 하지만 아침밥 든든히 먹어야죠 ㅎ",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041279.1.15",
          "form": "네 우리집 식구들은 다들 아침형 인간들이라서 아침 안먹으면 점심까지 못버텨요",
          "original_form": "네 우리집 식구들은 다들 아침형 인간들이라서 아침 안먹으면 점심까지 못버텨요",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041279.1.16",
          "form": "6시에 일어나니까 7시면 먹어야해요",
          "original_form": "6시에 일어나니까 7시면 먹어야해요",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041279.1.17",
          "form": "엇 저희집이라 비슷",
          "original_form": "엇 저희집이라 비슷",
          "speaker_id": "2",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100041279.1.18",
          "form": "오 생활패턴이 비슷한가보군요",
          "original_form": "오 생활패턴이 비슷한가보군요",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100041279.1.19",
          "form": "반갑네요",
          "original_form": "반갑네요",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100041279.1.20",
          "form": "네 즈집은 식사 시간 2번요",
          "original_form": "네 즈집은 식사 시간 2번요",
          "speaker_id": "2",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100041279.1.21",
          "form": "6시 남편  7시반 저랑 아이",
          "original_form": "6시 남편  7시반 저랑 아이",
          "speaker_id": "2",
          "time": "20210906 20:34"
        }
      ]
