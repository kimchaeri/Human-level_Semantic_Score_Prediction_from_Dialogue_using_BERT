      "utterance": [
        {
          "id": "MDRW2100023516.1.1",
          "form": "계신 곳 날씨는 어떤가요",
          "original_form": "계신 곳 날씨는 어떤가요",
          "speaker_id": "1",
          "time": "20210830 10:46"
        },
        {
          "id": "MDRW2100023516.1.2",
          "form": "아침에 비가 잠깐 왔다가 지금 해가 찡찡 하네요",
          "original_form": "아침에 비가 잠깐 왔다가 지금 해가 찡찡 하네요",
          "speaker_id": "1",
          "time": "20210830 10:46"
        },
        {
          "id": "MDRW2100023516.1.3",
          "form": "여기는 아직 우중충해요",
          "original_form": "여기는 아직 우중충해요",
          "speaker_id": "2",
          "time": "20210830 10:50"
        },
        {
          "id": "MDRW2100023516.1.4",
          "form": "비 오고 흐릴 때는 해 점 뜨 길 바랬는데",
          "original_form": "비 오고 흐릴 때는 해 점 뜨 길 바랬는데",
          "speaker_id": "1",
          "time": "20210830 10:51"
        },
        {
          "id": "MDRW2100023516.1.5",
          "form": "해가 너무 쨍쨍하니 뜨겁네요ㅋㅋㅋ",
          "original_form": "해가 너무 쨍쨍하니 뜨겁네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:51"
        },
        {
          "id": "MDRW2100023516.1.6",
          "form": "여긴 해를 볼 수 없는데 좋은데 사시네요",
          "original_form": "여긴 해를 볼 수 없는데 좋은데 사시네요",
          "speaker_id": "2",
          "time": "20210830 10:54"
        },
        {
          "id": "MDRW2100023516.1.7",
          "form": "사람 욕심은 끝이 없죠;;ㅋㅋㅋ",
          "original_form": "사람 욕심은 끝이 없죠;;ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:55"
        },
        {
          "id": "MDRW2100023516.1.8",
          "form": "전 또 흐린 날씨 좋아합니다",
          "original_form": "전 또 흐린 날씨 좋아합니다",
          "speaker_id": "1",
          "time": "20210830 10:55"
        },
        {
          "id": "MDRW2100023516.1.9",
          "form": "그쵸ㅋㅋ 쨍쨍하면 흐린게 부럽죠ㅋㅋ",
          "original_form": "그쵸ㅋㅋ 쨍쨍하면 흐린게 부럽죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:56"
        },
        {
          "id": "MDRW2100023516.1.10",
          "form": "전 요즘 계속 비가와서 해가 그리워요",
          "original_form": "전 요즘 계속 비가와서 해가 그리워요",
          "speaker_id": "2",
          "time": "20210830 10:56"
        },
        {
          "id": "MDRW2100023516.1.11",
          "form": "태풍은지나갔고..가을 장마는 아직 끝이 안 났나요??",
          "original_form": "태풍은지나갔고..가을 장마는 아직 끝이 안 났나요??",
          "speaker_id": "1",
          "time": "20210830 10:57"
        },
        {
          "id": "MDRW2100023516.1.12",
          "form": "근데 지금 계절이 가을인가요??;;",
          "original_form": "근데 지금 계절이 가을인가요??;;",
          "speaker_id": "1",
          "time": "20210830 10:57"
        },
        {
          "id": "MDRW2100023516.1.13",
          "form": "네 내일도 비온데요",
          "original_form": "네 내일도 비온데요",
          "speaker_id": "2",
          "time": "20210830 10:58"
        },
        {
          "id": "MDRW2100023516.1.14",
          "form": "가을인가봐요ㅋㅋ 뉴스에서 가을비라고 하더라고요",
          "original_form": "가을인가봐요ㅋㅋ 뉴스에서 가을비라고 하더라고요",
          "speaker_id": "2",
          "time": "20210830 10:58"
        },
        {
          "id": "MDRW2100023516.1.15",
          "form": "전 아직 여름 같아요",
          "original_form": "전 아직 여름 같아요",
          "speaker_id": "2",
          "time": "20210830 10:58"
        },
        {
          "id": "MDRW2100023516.1.16",
          "form": "저도 아직 여름 같이 느껴지는데",
          "original_form": "저도 아직 여름 같이 느껴지는데",
          "speaker_id": "1",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100023516.1.17",
          "form": "벌써가을이군요~",
          "original_form": "벌써가을이군요~",
          "speaker_id": "1",
          "time": "20210830 11:00"
        },
        {
          "id": "MDRW2100023516.1.18",
          "form": "올해는 코로나 밖에 기억나는 게 없네요ㅠ",
          "original_form": "올해는 코로나 밖에 기억나는 게 없네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:00"
        },
        {
          "id": "MDRW2100023516.1.19",
          "form": "그니까요 가을은 순식간에 지나갈 것같아요",
          "original_form": "그니까요 가을은 순식간에 지나갈 것같아요",
          "speaker_id": "2",
          "time": "20210830 11:00"
        },
        {
          "id": "MDRW2100023516.1.20",
          "form": "나이가 들수록 시간이 더 빨리 지나가는 거 같아요",
          "original_form": "나이가 들수록 시간이 더 빨리 지나가는 거 같아요",
          "speaker_id": "1",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100023516.1.21",
          "form": "그쵸 요즘 나이드는게 확 느껴져요\n계절 날씨가 변하는것에 대한 와닿음이 다르더라고요",
          "original_form": "그쵸 요즘 나이드는게 확 느껴져요\n계절 날씨가 변하는것에 대한 와닿음이 다르더라고요",
          "speaker_id": "2",
          "time": "20210830 11:05"
        },
        {
          "id": "MDRW2100023516.1.22",
          "form": "맞아요 어릴 땐 어른들 얘기하는 거 이해할 수 없었는데",
          "original_form": "맞아요 어릴 땐 어른들 얘기하는 거 이해할 수 없었는데",
          "speaker_id": "1",
          "time": "20210830 11:07"
        }
