      "utterance": [
        {
          "id": "MDRW2100002009.1.1",
          "form": "딸래미 재택근무 하니까 좋으냐",
          "original_form": "딸래미 재택근무 하니까 좋으냐",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002009.1.2",
          "form": "웅 ㅋㅋ 출퇴근 시간 아끼니까 좋다",
          "original_form": "웅 ㅋㅋ 출퇴근 시간 아끼니까 좋다",
          "speaker_id": "1",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002009.1.3",
          "form": "근데 일에 집중이 잘 안되서 그건 단점이다..",
          "original_form": "근데 일에 집중이 잘 안되서 그건 단점이다..",
          "speaker_id": "1",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002009.1.4",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002009.1.5",
          "form": "집중하려고 노력해야지",
          "original_form": "집중하려고 노력해야지",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002009.1.6",
          "form": "웅 ㅋㅋ근데 바로 다른 사람들 눈에 보이는게 아니니",
          "original_form": "웅 ㅋㅋ근데 바로 다른 사람들 눈에 보이는게 아니니",
          "speaker_id": "1",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002009.1.7",
          "form": "어쩔수가 없네 ㅋㅋ",
          "original_form": "어쩔수가 없네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002009.1.8",
          "form": "ㅋㅋ그렇지",
          "original_form": "ㅋㅋ그렇지",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002009.1.9",
          "form": "할일은 다 하고",
          "original_form": "할일은 다 하고",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002009.1.10",
          "form": "쉬엄쉬엄 해라",
          "original_form": "쉬엄쉬엄 해라",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002009.1.11",
          "form": "그렇게 하고있어용 ㅎㅎ",
          "original_form": "그렇게 하고있어용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002009.1.12",
          "form": "집중은 안되는데 그래도 마음이 편하니까 일은 잘 된다~",
          "original_form": "집중은 안되는데 그래도 마음이 편하니까 일은 잘 된다~",
          "speaker_id": "1",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002009.1.13",
          "form": "좋네",
          "original_form": "좋네",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002009.1.14",
          "form": "웅 ㅋ 재택근무 계속 하면 좋겠다",
          "original_form": "웅 ㅋ 재택근무 계속 하면 좋겠다",
          "speaker_id": "1",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002009.1.15",
          "form": "계속 하게되겠지",
          "original_form": "계속 하게되겠지",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002009.1.16",
          "form": "그렇기를~",
          "original_form": "그렇기를~",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002009.1.17",
          "form": "그럴것이다",
          "original_form": "그럴것이다",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002009.1.18",
          "form": "옹 ㅋㅋ재택근무 끝날때까지 열심히 해봐야지",
