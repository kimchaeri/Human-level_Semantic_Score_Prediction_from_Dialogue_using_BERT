      "utterance": [
        {
          "id": "MDRW2100013184.1.1",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013184.1.2",
          "form": "그래서 오늘 공연 보러가기 편하시겠다 생각했어요",
          "original_form": "그래서 오늘 공연 보러가기 편하시겠다 생각했어요",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013184.1.3",
          "form": "네ㅜㅜ 갔으면 좋았을텐데",
          "original_form": "네ㅜㅜ 갔으면 좋았을텐데",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013184.1.4",
          "form": "집에 있으면 몸은 편하니까 .. ㅋㅋ",
          "original_form": "집에 있으면 몸은 편하니까 .. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013184.1.5",
          "form": "네 어제는 엄청 습하던데요",
          "original_form": "네 어제는 엄청 습하던데요",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013184.1.6",
          "form": "네 정말 제습기 필수",
          "original_form": "네 정말 제습기 필수",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013184.1.7",
          "form": "바닥이 거의 물에 젖어 있는 느낌이었어요",
          "original_form": "바닥이 거의 물에 젖어 있는 느낌이었어요",
          "speaker_id": "2",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013184.1.8",
          "form": "저희도 보일러 틀어야되나 생각했어요",
          "original_form": "저희도 보일러 틀어야되나 생각했어요",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013184.1.9",
          "form": "비그치면 완연한 가을이겠어요",
          "original_form": "비그치면 완연한 가을이겠어요",
          "speaker_id": "2",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013184.1.10",
          "form": "네 정말요",
          "original_form": "네 정말요",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013184.1.11",
          "form": "이제 곧 단풍지겠죠",
          "original_form": "이제 곧 단풍지겠죠",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013184.1.12",
          "form": "네 ㅎㅎ 단풍  좋아하세요?",
          "original_form": "네 ㅎㅎ 단풍  좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013184.1.13",
          "form": "엄청은 아니지만",
          "original_form": "엄청은 아니지만",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013184.1.14",
          "form": "그래도 단풍놀이 챙겨가긴 해요 ㅋㅋㅋ",
          "original_form": "그래도 단풍놀이 챙겨가긴 해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013184.1.15",
          "form": "부지런하시네요 ㅎㅎ",
          "original_form": "부지런하시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013184.1.16",
          "form": "계절마다 할 수 있는건 챙기는 편이라 ㅋㅋ",
          "original_form": "계절마다 할 수 있는건 챙기는 편이라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013184.1.17",
          "form": "저는 꽃놀이만 챙기네요 ㅎ",
          "original_form": "저는 꽃놀이만 챙기네요 ㅎ",
          "speaker_id": "2",
