      "utterance": [
        {
          "id": "MDRW2100028165.1.1",
          "form": "조금 있다가 원어스 음원나와",
          "original_form": "조금 있다가 원어스 음원나와",
          "speaker_id": "2",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028165.1.2",
          "form": "원어스 너가 최근에 잡은 아이들이니?",
          "original_form": "원어스 너가 최근에 잡은 아이들이니?",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028165.1.3",
          "form": "이번에 원어스씨어터라고 영화컨셉 뮤비 프로젝트하거든",
          "original_form": "이번에 원어스씨어터라고 영화컨셉 뮤비 프로젝트하거든",
          "speaker_id": "2",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028165.1.4",
          "form": "그 마트에서 음악나와서 헐 했다던",
          "original_form": "그 마트에서 음악나와서 헐 했다던",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028165.1.5",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028165.1.6",
          "form": "오늘 공개곡은 life is beautiful 이야",
          "original_form": "오늘 공개곡은 life is beautiful 이야",
          "speaker_id": "2",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028165.1.7",
          "form": "와 영어 잘치네",
          "original_form": "와 영어 잘치네",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028165.1.8",
          "form": "기회가 되면 들어볼게",
          "original_form": "기회가 되면 들어볼게",
          "speaker_id": "1",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028165.1.9",
          "form": "얘네 오타쿠의 심장 뛰게 만드는 곡이 많아서",
          "original_form": "얘네 오타쿠의 심장 뛰게 만드는 곡이 많아서",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028165.1.10",
          "form": "오 약간 애니 오프닝 곡 느낌인가",
          "original_form": "오 약간 애니 오프닝 곡 느낌인가",
          "speaker_id": "1",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028165.1.11",
          "form": "to be or not to be 진짜 오타쿠픽",
          "original_form": "to be or not to be 진짜 오타쿠픽",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028165.1.12",
          "form": "거기 무대에 핑크착장에반짝이 핀하고 나와서",
          "original_form": "거기 무대에 핑크착장에반짝이 핀하고 나와서",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028165.1.13",
          "form": "까마귀공주라고 부르는 전설무대가 있지",
          "original_form": "까마귀공주라고 부르는 전설무대가 있지",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028165.1.14",
          "form": "다급하게 영업하는 사람",
          "original_form": "다급하게 영업하는 사람",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028165.1.15",
          "form": "여자 아이돌임?",
          "original_form": "여자 아이돌임?",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028165.1.16",
