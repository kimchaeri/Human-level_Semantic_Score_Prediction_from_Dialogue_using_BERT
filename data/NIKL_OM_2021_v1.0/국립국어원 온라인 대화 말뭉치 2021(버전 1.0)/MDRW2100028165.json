          "original_form": "남자아이돌",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028165.1.17",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028165.1.18",
