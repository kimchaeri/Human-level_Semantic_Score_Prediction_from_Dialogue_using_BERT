      "utterance": [
        {
          "id": "MDRW2100024036.1.1",
          "form": "최근에 간 맛집있으신가요?",
          "original_form": "최근에 간 맛집있으신가요?",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100024036.1.2",
          "form": "여름휴가로 제주도 우도에 하하호호 수제버거집 맛있었어요ㅎㅎ",
          "original_form": "여름휴가로 제주도 우도에 하하호호 수제버거집 맛있었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024036.1.3",
          "form": "name2님은 어디 가셨나요?",
          "original_form": "&name2&님은 어디 가셨나요?",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024036.1.4",
          "form": "제주도 좋다 부럽네요",
          "original_form": "제주도 좋다 부럽네요",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024036.1.5",
          "form": "전 지난주 이쁜 카페갔어요",
          "original_form": "전 지난주 이쁜 카페갔어요",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024036.1.6",
          "form": "수풀레케잌먹으러요",
          "original_form": "수풀레케잌먹으러요",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024036.1.7",
          "form": "수플레케잌 맛있으셨겠네요~",
          "original_form": "수플레케잌 맛있으셨겠네요~",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024036.1.8",
          "form": "요즘 디지트카페들이 참 다양하고 많지요",
          "original_form": "요즘 디지트카페들이 참 다양하고 많지요",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024036.1.9",
          "form": "넹 카페가 하나 건너 하나",
          "original_form": "넹 카페가 하나 건너 하나",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024036.1.10",
          "form": "시그니쳐 음료들도 다 개성있고요",
          "original_form": "시그니쳐 음료들도 다 개성있고요",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024036.1.11",
          "form": "네 가게들 인테리어도 너무 멋지더라구요",
          "original_form": "네 가게들 인테리어도 너무 멋지더라구요",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024036.1.12",
          "form": "근데 오래있기에는 좀 불편",
          "original_form": "근데 오래있기에는 좀 불편",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024036.1.13",
          "form": "테이블도 작고 낮고",
          "original_form": "테이블도 작고 낮고",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024036.1.14",
          "form": "맞아요ㅎㅎ 손님들 텀을 빨리 돌릴려고 꼼수를 쓰는것같아서 좀 얄미워요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ 손님들 텀을 빨리 돌릴려고 꼼수를 쓰는것같아서 좀 얄미워요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024036.1.15",
          "form": "그쵸? 솔찍히 비싼데 오래 못있고",
          "original_form": "그쵸? 솔찍히 비싼데 오래 못있고",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024036.1.16",
          "form": "네 그러다보니깐 자주가기에는 좀 아깝게 그래요ㅎㅎ",
          "original_form": "네 그러다보니깐 자주가기에는 좀 아깝게 그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024036.1.17",
          "form": "그래도 어차피 가는 거 유명한데 가보자 하는 맘도 있구요",
          "original_form": "그래도 어차피 가는 거 유명한데 가보자 하는 맘도 있구요",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024036.1.18",
          "form": "카페도 좋지만 기회가 된다면 고호재나 경복궁 생과방에 한번 가보고싶어요",
          "original_form": "카페도 좋지만 기회가 된다면 고호재나 경복궁 생과방에 한번 가보고싶어요",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024036.1.19",
          "form": "처음 들어봐요",
          "original_form": "처음 들어봐요",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024036.1.20",
          "form": "알아봐야겠네요 왠지 고즈넉할것 같은 어감",
          "original_form": "알아봐야겠네요 왠지 고즈넉할것 같은 어감",
          "speaker_id": "1",
          "time": "20210830 22:46"
        }
      ]
