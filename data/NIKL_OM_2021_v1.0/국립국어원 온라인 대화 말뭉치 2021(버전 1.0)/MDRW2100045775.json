        {
          "id": "MDRW2100045775.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ먹다가 배 터지겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ먹다가 배 터지겠어요",
          "speaker_id": "2",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100045775.1.2",
          "form": "인간사료 너무 좋아요ㅋㅋㅋㅋ",
          "original_form": "인간사료 너무 좋아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045775.1.3",
          "form": "벌키하게 파는 것들이 많아서 어른이는 행복해요",
          "original_form": "벌키하게 파는 것들이 많아서 어른이는 행복해요",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045775.1.4",
          "form": "맞아요ㅠㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045775.1.5",
          "form": "칼로리를 걱정해야하는 어른이로 자라났지만",
          "original_form": "칼로리를 걱정해야하는 어른이로 자라났지만",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045775.1.6",
          "form": "주변에 같이 먹을 사람을 섭외해서",
          "original_form": "주변에 같이 먹을 사람을 섭외해서",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045775.1.7",
          "form": "열심히 먹고 말거에요",
          "original_form": "열심히 먹고 말거에요",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045775.1.8",
          "form": "섭외해서ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "섭외해서ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100045775.1.9",
          "form": "와 저도 섭외해주세요 광란의 돼지파티를 해요",
          "original_form": "와 저도 섭외해주세요 광란의 돼지파티를 해요",
          "speaker_id": "2",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100045775.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100045775.1.11",
          "form": "돼지파티 최고져",
          "original_form": "돼지파티 최고져",
          "speaker_id": "1",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100045775.1.12",
          "form": "혼자먹어도 맛있지만",
          "original_form": "혼자먹어도 맛있지만",
          "speaker_id": "1",
          "time": "20210908 07:00"
        },
        {
          "id": "MDRW2100045775.1.13",
          "form": "여럿이 돼지가 되면 더욱 기분최고",
          "original_form": "여럿이 돼지가 되면 더욱 기분최고",
          "speaker_id": "1",
          "time": "20210908 07:00"
        },
        {
          "id": "MDRW2100045775.1.14",
          "form": "ㅌㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:00"
        },
        {
          "id": "MDRW2100045775.1.15",
          "form": "name1님은 폐지줍고 나서",
          "original_form": "&name1&님은 폐지줍고 나서",
          "speaker_id": "2",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045775.1.16",
          "form": "먹을 거에 쓰실건가요",
          "original_form": "먹을 거에 쓰실건가요",
          "speaker_id": "2",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045775.1.17",
          "form": "다른 곳에 쓰려고 했는데",
          "original_form": "다른 곳에 쓰려고 했는데",
          "speaker_id": "1",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045775.1.18",
          "form": "결국 먹을것으로 갈듯한 느낌이 드네욬ㅋㅋㅋㅋ",
          "original_form": "결국 먹을것으로 갈듯한 느낌이 드네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045775.1.19",
          "form": "먹을거 얘기를 너무 많이했더니 구매욕이 샘솟아요",
          "original_form": "먹을거 얘기를 너무 많이했더니 구매욕이 샘솟아요",
          "speaker_id": "1",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045775.1.20",
          "form": "ㅋㅋㅌㅋㅋㅋ저도요....제말이요..",
          "original_form": "ㅋㅋㅌㅋㅋㅋ저도요....제말이요..",
