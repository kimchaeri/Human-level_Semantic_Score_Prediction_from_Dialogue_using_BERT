      "utterance": [
        {
          "id": "MDRW2100019898.1.1",
          "form": "ㅎㅎ 좋죠ㅎㅎ",
          "original_form": "ㅎㅎ 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019898.1.2",
          "form": "볶음밥ㅎ 새우랑넣어도되고ㅎㅎ 소세지도 좋아하잖아요ㅎ",
          "original_form": "볶음밥ㅎ 새우랑넣어도되고ㅎㅎ 소세지도 좋아하잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019898.1.3",
          "form": "네 다른 반찬 하려면 날리치면 힘들죠",
          "original_form": "네 다른 반찬 하려면 날리치면 힘들죠",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019898.1.4",
          "form": "한방에 먹이는게 최고",
          "original_form": "한방에 먹이는게 최고",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019898.1.5",
          "form": "ㅎㅎ 그래도 준비하는것도 만만치않아요ㅎ",
          "original_form": "ㅎㅎ 그래도 준비하는것도 만만치않아요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019898.1.6",
          "form": "오히려 메인메뉴 하나가 더 낫을수있어요ㅎㅎ 볶음밥 재료 많이만들어봐서알아요ㅎ",
          "original_form": "오히려 메인메뉴 하나가 더 낫을수있어요ㅎㅎ 볶음밥 재료 많이만들어봐서알아요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019898.1.7",
          "form": "아 당근 양파 썰고 햄이나 기타등등ㅋ",
          "original_form": "아 당근 양파 썰고 햄이나 기타등등ㅋ",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019898.1.8",
          "form": "애들 당근하고 양파 좋아해요?ㅎㅎ",
          "original_form": "애들 당근하고 양파 좋아해요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019898.1.9",
          "form": "우리애는 말만 당근 양파 좋아한다고 해놓곤.. 막상주면 안먹어요ㅍ",
          "original_form": "우리애는 말만 당근 양파 좋아한다고 해놓곤.. 막상주면 안먹어요ㅍ",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019898.1.10",
          "form": "아 잘먹어요 당근생을 좋아해서",
          "original_form": "아 잘먹어요 당근생을 좋아해서",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019898.1.11",
          "form": "우와ㅎㅎ 애들이 편식을 안하나보네요ㅎ",
          "original_form": "우와ㅎㅎ 애들이 편식을 안하나보네요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019898.1.12",
          "form": "부럽네요ㅎㅎ",
          "original_form": "부럽네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019898.1.13",
          "form": "제가 먹으니 같이먹는듯",
          "original_form": "제가 먹으니 같이먹는듯",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019898.1.14",
          "form": "엄마아빠의 식습관이 중요하긴한것같애요ㅎ",
          "original_form": "엄마아빠의 식습관이 중요하긴한것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019898.1.15",
          "form": "저희애도 저닮아서 고기만 좋아해요ㅠ",
          "original_form": "저희애도 저닮아서 고기만 좋아해요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019898.1.16",
          "form": "그래도 저희는 고기잘안먹어서\r\n저는 고기좋아하는데",
          "original_form": "그래도 저희는 고기잘안먹어서\r\n저는 고기좋아하는데",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019898.1.17",
          "form": "일단은 잘먹는 모습 보여주세요ㅋ",
          "original_form": "일단은 잘먹는 모습 보여주세요ㅋ",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019898.1.18",
          "form": "저도 많이 먹으려고하는데ㅎㅎ",
          "original_form": "저도 많이 먹으려고하는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019898.1.19",
          "form": "아이가 안먹네요ㅎ 그래서 맨날 달래고 화내고 해야해요ㅎㅎ",
          "original_form": "아이가 안먹네요ㅎ 그래서 맨날 달래고 화내고 해야해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019898.1.20",
          "form": "맛있게 먹는게 중요하죠",
          "original_form": "맛있게 먹는게 중요하죠",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019898.1.21",
          "form": "ㅎㅎ 또 뭐자주먹이세요?",
          "original_form": "ㅎㅎ 또 뭐자주먹이세요?",
