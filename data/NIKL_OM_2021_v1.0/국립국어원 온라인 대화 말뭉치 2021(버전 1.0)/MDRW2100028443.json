          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028443.1.4",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028443.1.5",
          "form": "넹 name1님은 군필이신가여?",
          "original_form": "넹 &name1&님은 군필이신가여?",
          "speaker_id": "2",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028443.1.6",
          "form": "아니여 군대 갈 일이 없어여",
          "original_form": "아니여 군대 갈 일이 없어여",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028443.1.7",
          "form": "근데 뭐 군대는 피하면 좋은거니까요",
          "original_form": "근데 뭐 군대는 피하면 좋은거니까요",
          "speaker_id": "2",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028443.1.8",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028443.1.9",
          "form": "그게 장병들 마스크 벗긴다는 얘긴가요?",
          "original_form": "그게 장병들 마스크 벗긴다는 얘긴가요?",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028443.1.10",
          "form": "장병들 접종률 높으니까 마스크 벗고 생활하는게",
          "original_form": "장병들 접종률 높으니까 마스크 벗고 생활하는게",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028443.1.11",
          "form": "그게 국방부에서 질본으로 건의한거같더라고요",
          "original_form": "그게 국방부에서 질본으로 건의한거같더라고요",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028443.1.12",
          "form": "아하 그렇게 된거군요",
          "original_form": "아하 그렇게 된거군요",
          "speaker_id": "1",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028443.1.13",
          "form": "어찌보면 답답할테니 벗겨야할거 같기도 하고",
          "original_form": "어찌보면 답답할테니 벗겨야할거 같기도 하고",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028443.1.14",
          "form": "또 위험한거 같기도 하고",
          "original_form": "또 위험한거 같기도 하고",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028443.1.15",
          "form": "떼껄룩님은 어떻게 생각하세요",
          "original_form": "떼껄룩님은 어떻게 생각하세요",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028443.1.16",
          "form": "군대에서 살다시피 하니까 마스크 안쓰는게 편하긴하겠죠",
          "original_form": "군대에서 살다시피 하니까 마스크 안쓰는게 편하긴하겠죠",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028443.1.17",
          "form": "근데 막 휴가같은거 나왔는데 운나쁘게 돌파감염되면 큰일날거같긴해요",
          "original_form": "근데 막 휴가같은거 나왔는데 운나쁘게 돌파감염되면 큰일날거같긴해요",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028443.1.18",
          "form": "마스크 벗기 해도 올해 12월 되어야 하지않으려나요...",
          "original_form": "마스크 벗기 해도 올해 12월 되어야 하지않으려나요...",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028443.1.19",
          "form": "휴가가 정말 큰일인거 같긴 해요",
          "original_form": "휴가가 정말 큰일인거 같긴 해요",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028443.1.20",
          "form": "한명 걸려오면 싹 걸리겠죠..",
          "original_form": "한명 걸려오면 싹 걸리겠죠..",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028443.1.21",
          "form": "12월 되면 밖의 사람들도 백신 많이 맞아서 좀 괜찮겠죠",
