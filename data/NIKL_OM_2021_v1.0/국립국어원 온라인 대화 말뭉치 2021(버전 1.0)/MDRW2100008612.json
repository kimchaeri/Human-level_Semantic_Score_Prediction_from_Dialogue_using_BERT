      "utterance": [
        {
          "id": "MDRW2100008612.1.1",
          "form": "SNS 많이 하세요?",
          "original_form": "SNS 많이 하세요?",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008612.1.2",
          "form": "아뇨 잘하지 않는 편이에요",
          "original_form": "아뇨 잘하지 않는 편이에요",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008612.1.3",
          "form": "계정은 만들었는데 잘 사요하지 않아요",
          "original_form": "계정은 만들었는데 잘 사요하지 않아요",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008612.1.4",
          "form": "SNS 많이 하시나봐요",
          "original_form": "SNS 많이 하시나봐요",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008612.1.5",
          "form": "저는 웬만한건 다 해요",
          "original_form": "저는 웬만한건 다 해요",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008612.1.6",
          "form": "주로 쓰는건 아이돌 덕질용 트위터요",
          "original_form": "주로 쓰는건 아이돌 덕질용 트위터요",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008612.1.7",
          "form": "트위터하시는구나",
          "original_form": "트위터하시는구나",
          "speaker_id": "2",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008612.1.8",
          "form": "글도 자주 포스팅하세요?",
          "original_form": "글도 자주 포스팅하세요?",
          "speaker_id": "2",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008612.1.9",
          "form": "포스팅은 잘 안해요",
          "original_form": "포스팅은 잘 안해요",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008612.1.10",
          "form": "올라오는 거 보는 것도 벅차서",
          "original_form": "올라오는 거 보는 것도 벅차서",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008612.1.11",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008612.1.12",
          "form": "SNS에 시간을 많이 할애하시나요",
          "original_form": "SNS에 시간을 많이 할애하시나요",
          "speaker_id": "2",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008612.1.13",
          "form": "방학이라서 많이 할애하고 있어요",
          "original_form": "방학이라서 많이 할애하고 있어요",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008612.1.14",
          "form": "개강하면 다 끊을듯요",
          "original_form": "개강하면 다 끊을듯요",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008612.1.15",
          "form": "학생이시구나",
          "original_form": "학생이시구나",
          "speaker_id": "2",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008612.1.16",
          "form": "저는 대학생때",
          "original_form": "저는 대학생때",
          "speaker_id": "2",
          "time": "20210822 17:33"
        },
        {
          "id": "MDRW2100008612.1.17",
          "form": "페이스북에 학부 페이지 같은거 볼때 sns 한 기억이 있네요",
          "original_form": "페이스북에 학부 페이지 같은거 볼때 sns 한 기억이 있네요",
          "speaker_id": "2",
          "time": "20210822 17:33"
        },
        {
          "id": "MDRW2100008612.1.18",
          "form": "인스타도 하세요?",
          "original_form": "인스타도 하세요?",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008612.1.19",
          "form": "네 인스타도 하고 있어요",
          "original_form": "네 인스타도 하고 있어요",
          "speaker_id": "1",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008612.1.20",
          "form": "인스타도 포스팅은 안하세요?",
          "original_form": "인스타도 포스팅은 안하세요?",
          "speaker_id": "2",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008612.1.21",
          "form": "인스타는 친구들이랑 근황 전하는 용도라",
          "original_form": "인스타는 친구들이랑 근황 전하는 용도라",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008612.1.22",
          "form": "포스팅 자주 해요",
          "original_form": "포스팅 자주 해요",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008612.1.23",
          "form": "인스타 하신지는 얼마나 됐어요?",
          "original_form": "인스타 하신지는 얼마나 됐어요?",
          "speaker_id": "2",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008612.1.24",
          "form": "중학생 때 시작했으니 4년 됐네요",
          "original_form": "중학생 때 시작했으니 4년 됐네요",
