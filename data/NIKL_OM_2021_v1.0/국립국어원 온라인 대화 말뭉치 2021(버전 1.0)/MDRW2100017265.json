      "utterance": [
        {
          "id": "MDRW2100017265.1.1",
          "form": "학생이신가유",
          "original_form": "학생이신가유",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017265.1.2",
          "form": "아ㅜ결혼하셨다하셨죠 죄송해요",
          "original_form": "아ㅜ결혼하셨다하셨죠 죄송해요",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017265.1.3",
          "form": "네 연애얼마나 하시고 유학가신거에요?",
          "original_form": "네 연애얼마나 하시고 유학가신거에요?",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017265.1.4",
          "form": "연애 6년정도요?",
          "original_form": "연애 6년정도요?",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017265.1.5",
          "form": "안지는 10년넘었어요 ㅋㅋ",
          "original_form": "안지는 10년넘었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017265.1.6",
          "form": "아 오래된 사이네요^^학교서 만나셨나봐요?",
          "original_form": "아 오래된 사이네요^^학교서 만나셨나봐요?",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017265.1.7",
          "form": "name2님은 학생이시죠??",
          "original_form": "&name2&님은 학생이시죠??",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017265.1.8",
          "form": "아 저는 직장인인데 대학원 같이 다녀요",
          "original_form": "아 저는 직장인인데 대학원 같이 다녀요",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017265.1.9",
          "form": "죽겠어요 ㅋㅋㅋㅋ",
          "original_form": "죽겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017265.1.10",
          "form": "네네 고3때 학원에서 알게됐어요",
          "original_form": "네네 고3때 학원에서 알게됐어요",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017265.1.11",
          "form": "아 대학원생이시군요",
          "original_form": "아 대학원생이시군요",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017265.1.12",
          "form": "일하시면서 학교생활은 어때요?",
          "original_form": "일하시면서 학교생활은 어때요?",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017265.1.13",
          "form": "일이 싫을땐 공부로 도피하고",
          "original_form": "일이 싫을땐 공부로 도피하고",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017265.1.14",
          "form": "공부가 싫을땐 회사로 도피하고 ㅋㅋ그렇게지내요",
          "original_form": "공부가 싫을땐 회사로 도피하고 ㅋㅋ그렇게지내요",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017265.1.15",
          "form": "곧 개강인데 너무 힘들어요ㅠ",
          "original_form": "곧 개강인데 너무 힘들어요ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017265.1.16",
          "form": "네 힘드시겠어요 시험기간땐 더더더요",
          "original_form": "네 힘드시겠어요 시험기간땐 더더더요",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017265.1.17",
          "form": "맞습니다ㅠ 이제 곧 논문 써야하는데 큰일이에요",
          "original_form": "맞습니다ㅠ 이제 곧 논문 써야하는데 큰일이에요",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017265.1.18",
          "form": "일도너무 힘들구ㅠㅠ",
          "original_form": "일도너무 힘들구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017265.1.19",
          "form": "논문 쓰실려면 많이 힘드시겠어요..\r\n화이팅하세요",
          "original_form": "논문 쓰실려면 많이 힘드시겠어요..\r\n화이팅하세요",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017265.1.20",
          "form": "학교다닐때가 젤 좋아요^^ 저도 돌아가고 싶어오",
          "original_form": "학교다닐때가 젤 좋아요^^ 저도 돌아가고 싶어오",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017265.1.21",
          "form": "그쵸 학교가 최고입니다",
          "original_form": "그쵸 학교가 최고입니다",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017265.1.22",
          "form": "빨리 졸업하고 싶으면서도 회사 다니기시러요 ㅋㅋㅋ",
          "original_form": "빨리 졸업하고 싶으면서도 회사 다니기시러요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017265.1.23",
          "form": "회사다니는거 힘들어요\r\n학생이좋죠^^",
          "original_form": "회사다니는거 힘들어요\r\n학생이좋죠^^",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017265.1.24",
          "form": "물론 학업스트레스는 있지만요 ㅎ",
          "original_form": "물론 학업스트레스는 있지만요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017265.1.25",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017265.1.26",
          "form": "돈주는것과 돈받는것은 천지차이",
