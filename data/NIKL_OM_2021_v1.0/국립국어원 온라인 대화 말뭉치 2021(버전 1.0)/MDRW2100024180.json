      "utterance": [
        {
          "id": "MDRW2100024180.1.1",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024180.1.2",
          "form": "아침마다 악뮤노래러 출근준비해요",
          "original_form": "아침마다 악뮤노래러 출근준비해요",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024180.1.3",
          "form": "다다 다리꼬지마~ 넘 좋았는데",
          "original_form": "다다 다리꼬지마~ 넘 좋았는데",
          "speaker_id": "1",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024180.1.4",
          "form": "진짜 재치있었엉요",
          "original_form": "진짜 재치있었엉요",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024180.1.5",
          "form": "오랜날 오랜밤도 노래 좋지 않아요?",
          "original_form": "오랜날 오랜밤도 노래 좋지 않아요?",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024180.1.6",
          "form": "또 무슨 노래 좋아하세용",
          "original_form": "또 무슨 노래 좋아하세용",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024180.1.7",
          "form": "좋아하는 곡중에 하나죠 너무 잘 썼어요",
          "original_form": "좋아하는 곡중에 하나죠 너무 잘 썼어요",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024180.1.8",
          "form": "전 가을되면 시간과 낙엽",
          "original_form": "전 가을되면 시간과 낙엽",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024180.1.9",
          "form": "이거 듣는데",
          "original_form": "이거 듣는데",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024180.1.10",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024180.1.11",
          "form": "시간과 낙엽!! 되게 제목이 독특하고 좋다",
          "original_form": "시간과 낙엽!! 되게 제목이 독특하고 좋다",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024180.1.12",
          "form": "되게 가을 온 기분 들어요",
          "original_form": "되게 가을 온 기분 들어요",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024180.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024180.1.14",
          "form": "그거 들으면 진짜 가을이 벌써 찾아온 느낌 들겠어요",
          "original_form": "그거 들으면 진짜 가을이 벌써 찾아온 느낌 들겠어요",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024180.1.15",
          "form": "수현이 목소리 진짜",
          "original_form": "수현이 목소리 진짜",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024180.1.16",
          "form": "저도 가을 되면 항상 아침마다 듣는 노래 있는데!!",
          "original_form": "저도 가을 되면 항상 아침마다 듣는 노래 있는데!!",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024180.1.17",
          "form": "뭐에오",
          "original_form": "뭐에오",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024180.1.18",
          "form": "아이유의 가을아침이요 ㅜㅜ",
          "original_form": "아이유의 가을아침이요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024180.1.19",
          "form": "이걸로 조례해요",
          "original_form": "이걸로 조례해요",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024180.1.20",
          "form": "아아 마자요 그것도 진짜 가을아침 느낌",
          "original_form": "아아 마자요 그것도 진짜 가을아침 느낌",
