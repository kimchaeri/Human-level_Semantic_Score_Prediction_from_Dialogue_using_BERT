      "utterance": [
        {
          "id": "MDRW2100036387.1.1",
          "form": "맞다 상속자들에서 봤던 배우!",
          "original_form": "맞다 상속자들에서 봤던 배우!",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100036387.1.2",
          "form": "엇 누구죠?!",
          "original_form": "엇 누구죠?!",
          "speaker_id": "2",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100036387.1.3",
          "form": "상속자는 박신혜만 기억나네요ㅋㅋ",
          "original_form": "상속자는 박신혜만 기억나네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100036387.1.4",
          "form": "그 그",
          "original_form": "그 그",
          "speaker_id": "1",
          "time": "20210830 22:25"
        },
        {
          "id": "MDRW2100036387.1.5",
          "form": "탄 형네 엄마?",
          "original_form": "탄 형네 엄마?",
          "speaker_id": "1",
          "time": "20210830 22:25"
        },
        {
          "id": "MDRW2100036387.1.6",
          "form": "라고 해여하나 탄 아빠 현부인이던!!",
          "original_form": "라고 해여하나 탄 아빠 현부인이던!!",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100036387.1.7",
          "form": "사람이요!!",
          "original_form": "사람이요!!",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100036387.1.8",
          "form": "학교 이사장!",
          "original_form": "학교 이사장!",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100036387.1.9",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100036387.1.10",
          "form": "기억나는거 같아요!!!",
          "original_form": "기억나는거 같아요!!!",
          "speaker_id": "2",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100036387.1.11",
          "form": "엄청 고왔었죠~",
          "original_form": "엄청 고왔었죠~",
          "speaker_id": "2",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100036387.1.12",
          "form": "맞아요!! 포스 짱이던 ㅎ.ㅎ !!!",
          "original_form": "맞아요!! 포스 짱이던 ㅎ.ㅎ !!!",
          "speaker_id": "1",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100036387.1.13",
          "form": "name2님 싫어하는 드라마 장르도 있으신가요?",
          "original_form": "&name2&님 싫어하는 드라마 장르도 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100036387.1.14",
          "form": "무서운거요?ㅠ",
          "original_form": "무서운거요?ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100036387.1.15",
          "form": "드라마가 무서운건 별로 없지만 그건 싫더라구요",
          "original_form": "드라마가 무서운건 별로 없지만 그건 싫더라구요",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100036387.1.16",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100036387.1.17",
          "form": "저는 좀 잔잔한 거? 안 좋아하는 거 같아요 막",
          "original_form": "저는 좀 잔잔한 거? 안 좋아하는 거 같아요 막",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036387.1.18",
          "form": "음 이번생은 처음이라?",
          "original_form": "음 이번생은 처음이라?",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036387.1.19",
          "form": "같은",
          "original_form": "같은",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036387.1.20",
          "form": "아 그건 진짜 취향 타긴할거같아요",
          "original_form": "아 그건 진짜 취향 타긴할거같아요",
          "speaker_id": "2",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100036387.1.21",
          "form": "아빠보실때 살짝 봤는데 음? 했던 기억이 있네요",
          "original_form": "아빠보실때 살짝 봤는데 음? 했던 기억이 있네요",
          "speaker_id": "2",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100036387.1.22",
          "form": "네네 저 이준기 보려고 보다가 실패한..",
          "original_form": "네네 저 이준기 보려고 보다가 실패한..",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036387.1.23",
          "form": "그래도 이준기 나온 뷰티인사이드",
          "original_form": "그래도 이준기 나온 뷰티인사이드",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036387.1.24",
          "form": "완전 재밌게 봣어여",
          "original_form": "완전 재밌게 봣어여",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036387.1.25",
          "form": "대박 이준기 거기도 나왔군요",
          "original_form": "대박 이준기 거기도 나왔군요",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100036387.1.26",
          "form": "몰랐어요",
          "original_form": "몰랐어요",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100036387.1.27",
          "form": "드라마요??",
          "original_form": "드라마요??",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100036387.1.28",
          "form": "아 이준기인가 이민기인가 이름이.. 헷갈리네요.. ^,^",
          "original_form": "아 이준기인가 이민기인가 이름이.. 헷갈리네요.. ^,^",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100036387.1.29",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100036387.1.30",
          "form": "드라마 뷰티인사이드요!!",
          "original_form": "드라마 뷰티인사이드요!!",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100036387.1.31",
          "form": "오앙",
          "original_form": "오앙",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036387.1.32",
          "form": "저 그거 보고싶다고 킵했다가 아직 못봤었거든요",
          "original_form": "저 그거 보고싶다고 킵했다가 아직 못봤었거든요",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036387.1.33",
          "form": "궁금하네용ㅋㅋ",
          "original_form": "궁금하네용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036387.1.34",
          "form": "진짜 재밌어요!",
          "original_form": "진짜 재밌어요!",
          "speaker_id": "1",
          "time": "20210830 23:23"
        }
      ]
