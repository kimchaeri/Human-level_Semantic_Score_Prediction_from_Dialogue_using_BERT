      "utterance": [
        {
          "id": "MDRW2100036940.1.1",
          "form": "응!",
          "original_form": "응!",
          "speaker_id": "2",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100036940.1.2",
          "form": "날파리? 엄청 작은거 날벌레인데",
          "original_form": "날파리? 엄청 작은거 날벌레인데",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100036940.1.3",
          "form": "진짜작은거랑 막 초록색벌레랑 전쟁함",
          "original_form": "진짜작은거랑 막 초록색벌레랑 전쟁함",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100036940.1.4",
          "form": "요새 걔네 완전 날뜀",
          "original_form": "요새 걔네 완전 날뜀",
          "speaker_id": "2",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100036940.1.5",
          "form": "막 습한곳에잇는벌레알지?",
          "original_form": "막 습한곳에잇는벌레알지?",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100036940.1.6",
          "form": "초록색이라고?",
          "original_form": "초록색이라고?",
          "speaker_id": "2",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100036940.1.7",
          "form": "내가 어제 12시쯤에 공부방에들어가서",
          "original_form": "내가 어제 12시쯤에 공부방에들어가서",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100036940.1.8",
          "form": "오떤벌레지!",
          "original_form": "오떤벌레지!",
          "speaker_id": "2",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100036940.1.9",
          "form": "심슨책주문하고 문동균틀어놓고 졸앗단말이야",
          "original_form": "심슨책주문하고 문동균틀어놓고 졸앗단말이야",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100036940.1.10",
          "form": "근데 벌레가 막보이는거여 모니터앞이강 스텐드랑",
          "original_form": "근데 벌레가 막보이는거여 모니터앞이강 스텐드랑",
          "speaker_id": "1",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100036940.1.11",
          "form": "응응!!그래서 잡았어?!",
          "original_form": "응응!!그래서 잡았어?!",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100036940.1.12",
          "form": "미세방충망도",
          "original_form": "미세방충망도",
          "speaker_id": "1",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100036940.1.13",
          "form": "뚫고 들어오네하면서",
          "original_form": "뚫고 들어오네하면서",
          "speaker_id": "1",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100036940.1.14",
          "form": "한 3마리잡앗는ㄷ0",
          "original_form": "한 3마리잡앗는ㄷ0",
          "speaker_id": "1",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100036940.1.15",
          "form": "또있고 또있고",
          "original_form": "또있고 또있고",
          "speaker_id": "1",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100036940.1.16",
          "form": "밤에 불빛보고 달려드는구만!!",
          "original_form": "밤에 불빛보고 달려드는구만!!",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100036940.1.17",
          "form": "미치겠는거여",
          "original_form": "미치겠는거여",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.18",
          "form": "내가 창문을 한뼘정도 열고있엇는대..",
          "original_form": "내가 창문을 한뼘정도 열고있엇는대..",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.19",
          "form": "잠 쫓아낼라고 분신술 썼나 ㅋㅋㅋ",
          "original_form": "잠 쫓아낼라고 분신술 썼나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.20",
          "form": "나중에 2시에 알았어...",
          "original_form": "나중에 2시에 알았어...",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.21",
          "form": "ㅋㅋㅋ 그 틈으로 들어왔나봐...",
          "original_form": "ㅋㅋㅋ 그 틈으로 들어왔나봐...",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.22",
          "form": "방충망 없이 열고있었다는걸.",
          "original_form": "방충망 없이 열고있었다는걸.",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.23",
          "form": "ㅋㅋㅋㅋㅋ맙소사",
          "original_form": "ㅋㅋㅋㅋㅋ맙소사",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.24",
          "form": "구냥",
          "original_form": "구냥",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.25",
          "form": "쌩 야생이였던거지..",
          "original_form": "쌩 야생이였던거지..",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.26",
          "form": "자유출입로",
          "original_form": "자유출입로",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.27",
          "form": "ㅋㅋㅋㅋ걍 들어오라고 문열어놨네",
          "original_form": "ㅋㅋㅋㅋ걍 들어오라고 문열어놨네",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.28",
          "form": "ㅋㅋㅋ긍까유ㅠ",
          "original_form": "ㅋㅋㅋ긍까유ㅠ",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.29",
          "form": "어서옵쇼",
          "original_form": "어서옵쇼",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036940.1.30",
          "form": "방충망이 한쪽만잇으니깐",
          "original_form": "방충망이 한쪽만잇으니깐",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036940.1.31",
          "form": "저쪽문연다고 제끼고",
          "original_form": "저쪽문연다고 제끼고",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036940.1.32",
          "form": "어제열때 옮겨야햇는ㄷㅔ..폭망",
          "original_form": "어제열때 옮겨야햇는ㄷㅔ..폭망",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036940.1.33",
          "form": "ㅋㅋㅋ깜빡..",
          "original_form": "ㅋㅋㅋ깜빡..",
          "speaker_id": "2",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036940.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ내가또 방문열고잇어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ내가또 방문열고잇어서",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036940.1.35",
          "form": "집오는길에 우의 입고 산책하는 강아지 봄ㅋㅋㅋㅋ",
          "original_form": "집오는길에 우의 입고 산책하는 강아지 봄ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100036940.1.36",
          "form": "완전 귀여워 ㅋㅋㅋㅋ",
          "original_form": "완전 귀여워 ㅋㅋㅋㅋ",
