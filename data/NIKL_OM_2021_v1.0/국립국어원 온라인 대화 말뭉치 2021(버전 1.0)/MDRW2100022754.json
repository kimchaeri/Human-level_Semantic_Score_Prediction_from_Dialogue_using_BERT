      "utterance": [
        {
          "id": "MDRW2100022754.1.1",
          "form": "근데 진짜 펫샵 문제가",
          "original_form": "근데 진짜 펫샵 문제가",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.2",
          "form": "우리이모도 제작년에 펫샵에서 아기고양이샀다가",
          "original_form": "우리이모도 제작년에 펫샵에서 아기고양이샀다가",
          "speaker_id": "1",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.3",
          "form": "한달도안돼서 다시 펫샵에 돌려주고왔대",
          "original_form": "한달도안돼서 다시 펫샵에 돌려주고왔대",
          "speaker_id": "1",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.4",
          "form": "강아지 농장때문이잖아",
          "original_form": "강아지 농장때문이잖아",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.5",
          "form": "너무 치대서 피곤하다고 ㅋㅋ",
          "original_form": "너무 치대서 피곤하다고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.6",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.7",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.8",
          "form": "나 뒤늦게듣고 ㄹㅇ화냈잔ㄹ음",
          "original_form": "나 뒤늦게듣고 ㄹㅇ화냈잔ㄹ음",
          "speaker_id": "1",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.9",
          "form": "근데 펫샵에서 데려가놓고",
          "original_form": "근데 펫샵에서 데려가놓고",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.10",
          "form": "약한야면",
          "original_form": "약한야면",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.11",
          "form": "약한 동물이면",
          "original_form": "약한 동물이면",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.12",
          "form": "막 아프고 이런애들 있잖아",
          "original_form": "막 아프고 이런애들 있잖아",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022754.1.13",
          "form": "선천적으로 좀 약하게 태어난 애들",
          "original_form": "선천적으로 좀 약하게 태어난 애들",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022754.1.14",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022754.1.15",
          "form": "있지잇지",
          "original_form": "있지잇지",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022754.1.16",
          "form": "그런 강아지 다시 펫샵 데려와서",
          "original_form": "그런 강아지 다시 펫샵 데려와서",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022754.1.17",
          "form": "화냐면서 던진 사람 있다는 이야기 듣고",
          "original_form": "화냐면서 던진 사람 있다는 이야기 듣고",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022754.1.18",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022754.1.19",
          "form": "진짜 경악했음",
          "original_form": "진짜 경악했음",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022754.1.20",
          "form": "와ㅋㅋㅋㅋㅋㅋ",
          "original_form": "와ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022754.1.21",
          "form": "동물을 뭐라고생각하는거임 진짜",
          "original_form": "동물을 뭐라고생각하는거임 진짜",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022754.1.22",
          "form": "요즘은",
          "original_form": "요즘은",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022754.1.23",
          "form": "동물병원이랑 협약같은거 해서",
          "original_form": "동물병원이랑 협약같은거 해서",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022754.1.24",
          "form": "2층 병원 1층 펫샵 이렇게 한다음에",
          "original_form": "2층 병원 1층 펫샵 이렇게 한다음에",
          "speaker_id": "2",
          "time": "20210829 17:43"
