        {
          "id": "MDRW2100006263.1.1",
          "form": "오늘은 이마트 주문을 안했어요",
          "original_form": "오늘은 이마트 주문을 안했어요",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006263.1.2",
          "form": "이마트에서 시킬게 없어?",
          "original_form": "이마트에서 시킬게 없어?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006263.1.3",
          "form": "구경 맨날 하다가보니 다 시킨느낌이에요",
          "original_form": "구경 맨날 하다가보니 다 시킨느낌이에요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006263.1.4",
          "form": "이제 적당히 사",
          "original_form": "이제 적당히 사",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006263.1.5",
          "form": "그러려고요",
          "original_form": "그러려고요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006263.1.6",
          "form": "모기약 사라구",
          "original_form": "모기약 사라구",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006263.1.7",
          "form": "네 살게요",
          "original_form": "네 살게요",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006263.1.8",
          "form": "요즘 모기가 많아 빨리 사",
          "original_form": "요즘 모기가 많아 빨리 사",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006263.1.9",
          "form": "맞아요 엄청 많아요",
          "original_form": "맞아요 엄청 많아요",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006263.1.10",
          "form": "그리고 반찬도 좀 사",
          "original_form": "그리고 반찬도 좀 사",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006263.1.11",
          "form": "싫은데 그건",
          "original_form": "싫은데 그건",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006263.1.12",
          "form": "주말에 먹을 게 없어",
          "original_form": "주말에 먹을 게 없어",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006263.1.13",
          "form": "사야겠네요",
