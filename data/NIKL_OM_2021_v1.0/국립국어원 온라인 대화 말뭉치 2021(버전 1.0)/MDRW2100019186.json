      "utterance": [
        {
          "id": "MDRW2100019186.1.1",
          "form": "최근 여행 다녀오셨나요?",
          "original_form": "최근 여행 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100019186.1.2",
          "form": "몇달 된 듯요 ㅜ",
          "original_form": "몇달 된 듯요 ㅜ",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019186.1.3",
          "form": "저도 6월에 바다 갔다오고 추석 연휴에 바다 한 번 더 다녀오려구요",
          "original_form": "저도 6월에 바다 갔다오고 추석 연휴에 바다 한 번 더 다녀오려구요",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019186.1.4",
          "form": "바다 좋아하시나봐요",
          "original_form": "바다 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019186.1.5",
          "form": "네! 산보단 바다를 조아해요",
          "original_form": "네! 산보단 바다를 조아해요",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019186.1.6",
          "form": "바다에서 뭐라세요?",
          "original_form": "바다에서 뭐라세요?",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019186.1.7",
          "form": "6월엔 바다 물에서 놀았는데\r\n추석때는 추워져서 발만 담글거 같아요",
          "original_form": "6월엔 바다 물에서 놀았는데\r\n추석때는 추워져서 발만 담글거 같아요",
          "speaker_id": "2",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019186.1.8",
          "form": "아 저흰 바다 별로 안가요 ㅋㅋ",
          "original_form": "아 저흰 바다 별로 안가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019186.1.9",
          "form": "그럼 어디가세요?",
          "original_form": "그럼 어디가세요?",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019186.1.10",
          "form": "여기저기 관광지 공원 카페 드",
          "original_form": "여기저기 관광지 공원 카페 드",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019186.1.11",
          "form": "산은 안가세요?",
          "original_form": "산은 안가세요?",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019186.1.12",
          "form": "별로요 ㅋㅋ",
          "original_form": "별로요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019186.1.13",
          "form": "그쵸 저도 산은 별로에요.",
          "original_form": "그쵸 저도 산은 별로에요.",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019186.1.14",
          "form": "ㅎㅎㅎ식물원이나 공원  아이 위한 동물원이 좋아요 ㅎ",
          "original_form": "ㅎㅎㅎ식물원이나 공원  아이 위한 동물원이 좋아요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019186.1.15",
          "form": "아이 있음 그런 곳이 여행하기 좋죠",
          "original_form": "아이 있음 그런 곳이 여행하기 좋죠",
