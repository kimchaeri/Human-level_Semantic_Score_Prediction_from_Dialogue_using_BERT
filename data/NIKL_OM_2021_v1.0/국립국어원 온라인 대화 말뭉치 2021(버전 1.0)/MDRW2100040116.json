      "utterance": [
        {
          "id": "MDRW2100040116.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 10:41"
        },
        {
          "id": "MDRW2100040116.1.2",
          "form": "운동하는거있으신가용",
          "original_form": "운동하는거있으신가용",
          "speaker_id": "1",
          "time": "20210904 10:41"
        },
        {
          "id": "MDRW2100040116.1.3",
          "form": "아니요 홈트 간신히 해요",
          "original_form": "아니요 홈트 간신히 해요",
          "speaker_id": "2",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100040116.1.4",
          "form": "오 홈트 하시는구나",
          "original_form": "오 홈트 하시는구나",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100040116.1.5",
          "form": "저는 산책과 댄스합니더",
          "original_form": "저는 산책과 댄스합니더",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100040116.1.6",
          "form": "댄스요?",
          "original_form": "댄스요?",
          "speaker_id": "2",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100040116.1.7",
          "form": "네네아이돌 댄스배워요",
          "original_form": "네네아이돌 댄스배워요",
          "speaker_id": "1",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100040116.1.8",
          "form": "우왕!! 저도 배워봤는데 전 못하겠더라고요 어떠신가요? 잘 따라하세요?",
          "original_form": "우왕!! 저도 배워봤는데 전 못하겠더라고요 어떠신가요? 잘 따라하세요?",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100040116.1.9",
          "form": "전 너뮤 재밌어요",
          "original_form": "전 너뮤 재밌어요",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100040116.1.10",
          "form": "요즘 뭐 배우세요?",
          "original_form": "요즘 뭐 배우세요?",
          "speaker_id": "2",
          "time": "20210904 11:51"
        },
        {
          "id": "MDRW2100040116.1.11",
          "form": "아스트로 애푸터 미드나잇 배웁니다",
          "original_form": "아스트로 애푸터 미드나잇 배웁니다",
          "speaker_id": "1",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100040116.1.12",
          "form": "앗!! 어렵지 않나요????",
          "original_form": "앗!! 어렵지 않나요????",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040116.1.13",
          "form": "저 아주 옛날에 gee배우다 중도 하차.. ㅋㅋㅋㅋ",
          "original_form": "저 아주 옛날에 gee배우다 중도 하차.. ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040116.1.14",
          "form": "우와 쥐 재밌었겠어요",
          "original_form": "우와 쥐 재밌었겠어요",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040116.1.15",
          "form": "이번거 빠르긴한데재밌어요",
          "original_form": "이번거 빠르긴한데재밌어요",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040116.1.16",
          "form": "엄정화에 디스코도 배웠었어요..",
          "original_form": "엄정화에 디스코도 배웠었어요..",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040116.1.17",
          "form": "하지만 제가 추면 춤이 아닌 느낌 ㅠ",
          "original_form": "하지만 제가 추면 춤이 아닌 느낌 ㅠ",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040116.1.18",
          "form": "잘 추시나봐요",
          "original_form": "잘 추시나봐요",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040116.1.19",
          "form": "우와 대단하시다",
          "original_form": "우와 대단하시다",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040116.1.20",
          "form": "그냥 제흥에 추는거죠 ㅋㅋ",
          "original_form": "그냥 제흥에 추는거죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040116.1.21",
          "form": "전 추면서 흥이 안나요 ㅋㅋㅋ",
          "original_form": "전 추면서 흥이 안나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100040116.1.22",
          "form": "흥도 잘 춰야 나는 것이라고 봅니다 ㅋ",
          "original_form": "흥도 잘 춰야 나는 것이라고 봅니다 ㅋ",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100040116.1.23",
          "form": "아구런가요",
          "original_form": "아구런가요",
          "speaker_id": "1",
          "time": "20210904 12:01"
        },
        {
          "id": "MDRW2100040116.1.24",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:01"
        },
        {
          "id": "MDRW2100040116.1.25",
          "form": "친구랑 겉이해서 재미나요",
          "original_form": "친구랑 겉이해서 재미나요",
          "speaker_id": "1",
          "time": "20210904 12:01"
        },
        {
          "id": "MDRW2100040116.1.26",
          "form": "저도 동생이랑 같이 했는데 둘다 흥미를 잃었어요",
          "original_form": "저도 동생이랑 같이 했는데 둘다 흥미를 잃었어요",
          "speaker_id": "2",
          "time": "20210904 12:27"
        }
      ]
