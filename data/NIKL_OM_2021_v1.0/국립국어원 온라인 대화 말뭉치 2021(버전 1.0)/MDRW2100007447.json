        },
        {
          "id": "MDRW2100007447.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007447.1.6",
          "form": "오늘도 새로 공개된 거 봤어요",
          "original_form": "오늘도 새로 공개된 거 봤어요",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007447.1.7",
          "form": "더 체어",
          "original_form": "더 체어",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007447.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007447.1.9",
          "form": "전 영화 가끔 보고",
          "original_form": "전 영화 가끔 보고",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007447.1.10",
          "form": "애니 많이 봐요",
          "original_form": "애니 많이 봐요",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007447.1.11",
          "form": "지브리나 보스베이비 이누야샤 이런거",
          "original_form": "지브리나 보스베이비 이누야샤 이런거",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007447.1.12",
          "form": "저도 애니메이션도 좋아해요",
          "original_form": "저도 애니메이션도 좋아해요",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007447.1.13",
          "form": "지브리 최애작 물어봐도 될까요",
          "original_form": "지브리 최애작 물어봐도 될까요",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007447.1.14",
          "form": "미야자키 하야오 작품 거의 봤는데",
          "original_form": "미야자키 하야오 작품 거의 봤는데",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007447.1.15",
          "form": "그래도 센과 치히로의 행방불명이 젤 재미있는 거 같고",
          "original_form": "그래도 센과 치히로의 행방불명이 젤 재미있는 거 같고",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007447.1.16",
          "form": "마루밑아리에티랑 모노노케 히메도 재밌었죠",
          "original_form": "마루밑아리에티랑 모노노케 히메도 재밌었죠",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007447.1.17",
          "form": "저는 모노노케히메 제일 좋아해요",
          "original_form": "저는 모노노케히메 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007447.1.18",
          "form": "물론 다른 것들도 좋아하구요ㅎㅎ",
          "original_form": "물론 다른 것들도 좋아하구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007447.1.19",
          "form": "기분에 따라 생각나는 작품이 다르더라구요",
          "original_form": "기분에 따라 생각나는 작품이 다르더라구요",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007447.1.20",
          "form": "오 근데 지브리는 대체로 내용이 좀 심오하지않나요?",
          "original_form": "오 근데 지브리는 대체로 내용이 좀 심오하지않나요?",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007447.1.21",
          "form": "메시지를 다 담고 있는데 무겁게 담고 있는 느낌",
          "original_form": "메시지를 다 담고 있는데 무겁게 담고 있는 느낌",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007447.1.22",
          "form": "맞아요 밝은 분위기인 작품들도 메시지는 뚜렷하죠",
          "original_form": "맞아요 밝은 분위기인 작품들도 메시지는 뚜렷하죠",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007447.1.23",
          "form": "네 유튜브도 좋아하세요?",
          "original_form": "네 유튜브도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007447.1.24",
          "form": "전 asmr 잘보는데",
          "original_form": "전 asmr 잘보는데",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007447.1.25",
          "form": "저는 챙겨보는 채널은 없고 가끔 보고 싶은 게 있을 때만 검색해서 봐요",
          "original_form": "저는 챙겨보는 채널은 없고 가끔 보고 싶은 게 있을 때만 검색해서 봐요",
          "speaker_id": "1",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007447.1.26",
          "form": "전 유튜브를 더 많이 보는 거 같아요",
          "original_form": "전 유튜브를 더 많이 보는 거 같아요",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007447.1.27",
          "form": "짧게 잠깐 볼 수 있어서",
          "original_form": "짧게 잠깐 볼 수 있어서",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007447.1.28",
          "form": "넷플같은 건 보는 것도 각잡고 봐야돼서 잘 안봐지더라구요",
          "original_form": "넷플같은 건 보는 것도 각잡고 봐야돼서 잘 안봐지더라구요",
          "speaker_id": "2",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007447.1.29",
          "form": "맞아요 넷플은 그래서 찜목록에만 넣어놓구 안 보는 것들도 많잖아요",
          "original_form": "맞아요 넷플은 그래서 찜목록에만 넣어놓구 안 보는 것들도 많잖아요",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007447.1.30",
          "form": "맞아요 다큐도 맨날 본다고 목록 저장만 해놔요",
