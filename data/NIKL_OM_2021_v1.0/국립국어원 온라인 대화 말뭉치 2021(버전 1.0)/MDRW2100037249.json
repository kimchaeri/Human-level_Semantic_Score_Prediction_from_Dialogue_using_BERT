      "utterance": [
        {
          "id": "MDRW2100037249.1.1",
          "form": "저녁이나 먹지 왜 사먹었어",
          "original_form": "저녁이나 먹지 왜 사먹었어",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037249.1.2",
          "form": "그냥 ㅋㅋ 카페가 땡겨서~",
          "original_form": "그냥 ㅋㅋ 카페가 땡겨서~",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037249.1.3",
          "form": "무슨 빵 먹었는대",
          "original_form": "무슨 빵 먹었는대",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037249.1.4",
          "form": "스콘 ㅎㅎ",
          "original_form": "스콘 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037249.1.5",
          "form": "스콘 퍽퍽한 빵 ?",
          "original_form": "스콘 퍽퍽한 빵 ?",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037249.1.6",
          "form": "웅 밀가루맛나는거 ㅋㅋ",
          "original_form": "웅 밀가루맛나는거 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037249.1.7",
          "form": "맛없는거 왜 먹었어",
          "original_form": "맛없는거 왜 먹었어",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037249.1.8",
          "form": "맛있당~ 쨈이랑 같이 먹으면 ㅎㅎ",
          "original_form": "맛있당~ 쨈이랑 같이 먹으면 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037249.1.9",
          "form": "그렇나",
          "original_form": "그렇나",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037249.1.10",
          "form": "웅웅 커피랑도 어울리고",
          "original_form": "웅웅 커피랑도 어울리고",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037249.1.11",
          "form": "맛났겠네",
          "original_form": "맛났겠네",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037249.1.12",
          "form": "옹ㅋㅋ 그래서 배가 찼다",
          "original_form": "옹ㅋㅋ 그래서 배가 찼다",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037249.1.13",
          "form": "그래도 밥도 먹어라",
          "original_form": "그래도 밥도 먹어라",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037249.1.14",
          "form": "알았당 ㅎ",
          "original_form": "알았당 ㅎ",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037249.1.15",
          "form": "미역국 맛이 좋넴~",
