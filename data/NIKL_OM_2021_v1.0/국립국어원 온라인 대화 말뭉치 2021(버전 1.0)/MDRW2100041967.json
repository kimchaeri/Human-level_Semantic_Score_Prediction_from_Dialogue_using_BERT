      "utterance": [
        {
          "id": "MDRW2100041967.1.1",
          "form": "떡 완전 좋아해요 ㅎㅎㅎ",
          "original_form": "떡 완전 좋아해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041967.1.2",
          "form": "떡 전반적으로 좋아하는데",
          "original_form": "떡 전반적으로 좋아하는데",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041967.1.3",
          "form": "제가 초딩입맛이라,,,",
          "original_form": "제가 초딩입맛이라,,,",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041967.1.4",
          "form": "꿀떡 좋아요 달콤달콤",
          "original_form": "꿀떡 좋아요 달콤달콤",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041967.1.5",
          "form": "그럼 안에 뭐 들은게 좋으세요?",
          "original_form": "그럼 안에 뭐 들은게 좋으세요?",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041967.1.6",
          "form": "꿀이랑 깨요!!!",
          "original_form": "꿀이랑 깨요!!!",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041967.1.7",
          "form": "앙금도 좋아해요 ㅎㅎ",
          "original_form": "앙금도 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041967.1.8",
          "form": "그러고보니 곧 추석이네요?!",
          "original_form": "그러고보니 곧 추석이네요?!",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041967.1.9",
          "form": "오! 그럼 다른 떡에 들어있는 앙금도 좋아하세요?",
          "original_form": "오! 그럼 다른 떡에 들어있는 앙금도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100041967.1.10",
          "form": "네네네!!!!",
          "original_form": "네네네!!!!",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100041967.1.11",
          "form": "빵 속에 있는 앙금도 좋아하구요,,,,,",
          "original_form": "빵 속에 있는 앙금도 좋아하구요,,,,,",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100041967.1.12",
          "form": "흑 오늘 밥을 너무 심심하게 먹어서",
          "original_form": "흑 오늘 밥을 너무 심심하게 먹어서",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100041967.1.13",
          "form": "맛있는게 먹고싶네요!!!",
          "original_form": "맛있는게 먹고싶네요!!!",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100041967.1.14",
          "form": "뭐 드셨길래 심심했어용?",
          "original_form": "뭐 드셨길래 심심했어용?",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100041967.1.15",
          "form": "그냥 비비고 김이랑",
          "original_form": "그냥 비비고 김이랑",
          "speaker_id": "2",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100041967.1.16",
          "form": "콩나물 무침을 먹었답니다...",
          "original_form": "콩나물 무침을 먹었답니다...",
          "speaker_id": "2",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100041967.1.17",
          "form": "name1님은요?!",
          "original_form": "&name1&님은요?!",
          "speaker_id": "2",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100041967.1.18",
          "form": "아 저 스파게티 먹었어요 갑자기 확 땡기더라구요!!",
          "original_form": "아 저 스파게티 먹었어요 갑자기 확 땡기더라구요!!",
          "speaker_id": "1",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100041967.1.19",
          "form": "그래서 후딱 만들어서 먹었답니다 ㅋㅋㅋㅋ",
          "original_form": "그래서 후딱 만들어서 먹었답니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100041967.1.20",
          "form": "우와앗!!!!",
          "original_form": "우와앗!!!!",
          "speaker_id": "2",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100041967.1.21",
          "form": "스파게티를 만드셨다니!",
          "original_form": "스파게티를 만드셨다니!",
          "speaker_id": "2",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100041967.1.22",
          "form": "어떤 스파게티 좋아하시나요?",
          "original_form": "어떤 스파게티 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100041967.1.23",
          "form": "시판 토마토소스에 면만 넣어서 비벼 먹었어요 ㅋㅋㅋㅋ",
          "original_form": "시판 토마토소스에 면만 넣어서 비벼 먹었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:23"
        },
        {
          "id": "MDRW2100041967.1.24",
          "form": "시판 소스 최고!",
          "original_form": "시판 소스 최고!",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100041967.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100041967.1.26",
          "form": "토마토베이스를 제일 좋아하는데 오일이나 크림도 좋아해요",
          "original_form": "토마토베이스를 제일 좋아하는데 오일이나 크림도 좋아해요",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100041967.1.27",
          "form": "들디님 스파게티 좋아하세요?",
          "original_form": "들디님 스파게티 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100041967.1.28",
          "form": "네네!! 저 완전 좋아해요",
          "original_form": "네네!! 저 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210906 19:28"
        },
        {
          "id": "MDRW2100041967.1.29",
          "form": "저랑 비슷하시네요 ㅎㅎㅎ",
          "original_form": "저랑 비슷하시네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 19:28"
        },
        {
          "id": "MDRW2100041967.1.30",
          "form": "최애는 로제예요!!!",
          "original_form": "최애는 로제예요!!!",
          "speaker_id": "2",
          "time": "20210906 19:28"
        },
        {
          "id": "MDRW2100041967.1.31",
          "form": "햐 로제 맛있죠 저도 로제처럼 만들어서 먹었어요! 토마토소스에 우유랑 치즈 넣으면 로제맛 나요",
          "original_form": "햐 로제 맛있죠 저도 로제처럼 만들어서 먹었어요! 토마토소스에 우유랑 치즈 넣으면 로제맛 나요",
          "speaker_id": "1",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100041967.1.32",
          "form": "와 정말 맛잘알이시네요!!!",
          "original_form": "와 정말 맛잘알이시네요!!!",
          "speaker_id": "2",
