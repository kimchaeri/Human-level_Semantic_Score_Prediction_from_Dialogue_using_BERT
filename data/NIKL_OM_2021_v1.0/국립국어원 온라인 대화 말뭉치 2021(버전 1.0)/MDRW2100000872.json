      "utterance": [
        {
          "id": "MDRW2100000872.1.1",
          "form": "강아지들 델꼬 산책갈까",
          "original_form": "강아지들 델꼬 산책갈까",
          "speaker_id": "1",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000872.1.2",
          "form": "좀 이따가 가자",
          "original_form": "좀 이따가 가자",
          "speaker_id": "2",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000872.1.3",
          "form": "구랭 지금은 넘 배불",
          "original_form": "구랭 지금은 넘 배불",
          "speaker_id": "1",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000872.1.4",
          "form": "그래 배부른데 바로 운동하면 안 좋다",
          "original_form": "그래 배부른데 바로 운동하면 안 좋다",
          "speaker_id": "2",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000872.1.5",
          "form": "글쿤 ㅋㅋ 강아지산책 정도는 괜찮지 않나",
          "original_form": "글쿤 ㅋㅋ 강아지산책 정도는 괜찮지 않나",
          "speaker_id": "1",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000872.1.6",
          "form": "10분 뒤에 가자",
          "original_form": "10분 뒤에 가자",
          "speaker_id": "2",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000872.1.7",
          "form": "오키 ㅎㅎ 울 강쥐들 낮에 못가서 가고싶어하겠따",
          "original_form": "오키 ㅎㅎ 울 강쥐들 낮에 못가서 가고싶어하겠따",
          "speaker_id": "1",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000872.1.8",
          "form": "그래 가고 싶어서 눈이 초롱초롱하다",
          "original_form": "그래 가고 싶어서 눈이 초롱초롱하다",
          "speaker_id": "2",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000872.1.9",
          "form": "ㅋㅋ귀여운 것들",
          "original_form": "ㅋㅋ귀여운 것들",
          "speaker_id": "1",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000872.1.10",
          "form": "너무 사랑스럽다",
          "original_form": "너무 사랑스럽다",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000872.1.11",
          "form": "마장 ㅠㅠ 러블리한 울 멍멍이들",
          "original_form": "마장 ㅠㅠ 러블리한 울 멍멍이들",
          "speaker_id": "1",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000872.1.12",
          "form": "자기 얘기한다고 쳐다본다",
          "original_form": "자기 얘기한다고 쳐다본다",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000872.1.13",
          "form": "ㅋㅋㅋ똑똑하당",
          "original_form": "ㅋㅋㅋ똑똑하당",
          "speaker_id": "1",
          "time": "20210819 20:18"
        },
        {
          "id": "MDRW2100000872.1.14",
          "form": "우리 강아지들 지금 뭐해",
