      "utterance": [
        {
          "id": "MDRW2100039551.1.1",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039551.1.2",
          "form": "저는 아직도 너뮤 더뷰륙해서 아메리카노 사러가는중임다",
          "original_form": "저는 아직도 너뮤 더뷰륙해서 아메리카노 사러가는중임다",
          "speaker_id": "2",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039551.1.3",
          "form": "한샷만 넣어달라고 햐ㅛㅓ 마셔야겠어요",
          "original_form": "한샷만 넣어달라고 햐ㅛㅓ 마셔야겠어요",
          "speaker_id": "2",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039551.1.4",
          "form": "우악 아메리카노 드시는군요!!!",
          "original_form": "우악 아메리카노 드시는군요!!!",
          "speaker_id": "1",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039551.1.5",
          "form": "전 아직은 좀 써서 라떼 공략중이에요!",
          "original_form": "전 아직은 좀 써서 라떼 공략중이에요!",
          "speaker_id": "1",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039551.1.6",
          "form": "어디 커피 재일 좋아하셔요?",
          "original_form": "어디 커피 재일 좋아하셔요?",
          "speaker_id": "1",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039551.1.7",
          "form": "저도 못마셨었는데 한번 맛들리면? 계속 마시게되는거 같아요",
          "original_form": "저도 못마셨었는데 한번 맛들리면? 계속 마시게되는거 같아요",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039551.1.8",
          "form": "요줌 저가 커피가 워낙 잘나와서",
          "original_form": "요줌 저가 커피가 워낙 잘나와서",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039551.1.9",
          "form": "저는 메가커피 제일 자주 가는거 같아요!",
          "original_form": "저는 메가커피 제일 자주 가는거 같아요!",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039551.1.10",
          "form": "제일 좋아하는 카페 있으세요?",
          "original_form": "제일 좋아하는 카페 있으세요?",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039551.1.11",
          "form": "저는 스타벅스 무난하게 먹고요!! 이디야 짱 좋어해요",
          "original_form": "저는 스타벅스 무난하게 먹고요!! 이디야 짱 좋어해요",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039551.1.12",
          "form": "라떼 맛집이거든요!",
          "original_form": "라떼 맛집이거든요!",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039551.1.13",
          "form": "이디야가 라떼 맛집이군요!!",
          "original_form": "이디야가 라떼 맛집이군요!!",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039551.1.14",
          "form": "다음에 이디야를 마주치면 한번 가볼게요",
          "original_form": "다음에 이디야를 마주치면 한번 가볼게요",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039551.1.15",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039551.1.16",
          "form": "네! 거기서는 ㄴ꼭 바닐라 라데를 드셔야해요!!!!!!!!",
          "original_form": "네! 거기서는 ㄴ꼭 바닐라 라데를 드셔야해요!!!!!!!!",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039551.1.17",
          "form": "진짜 맛나요",
          "original_form": "진짜 맛나요",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039551.1.18",
          "form": "바닐라라떼 기억하겠금다",
          "original_form": "바닐라라떼 기억하겠금다",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039551.1.19",
          "form": "친구들 보니가",
          "original_form": "친구들 보니가",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039551.1.20",
          "form": "파우더로 만든 바닐라라떼랑",
          "original_form": "파우더로 만든 바닐라라떼랑",
          "speaker_id": "2",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100039551.1.21",
          "form": "시럽으로 만든 바닐라라떼파가",
          "original_form": "시럽으로 만든 바닐라라떼파가",
          "speaker_id": "2",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100039551.1.22",
          "form": "갈리더라고요",
          "original_form": "갈리더라고요",
          "speaker_id": "2",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100039551.1.23",
          "form": "허얼.. 전 막입이라..",
          "original_form": "허얼.. 전 막입이라..",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039551.1.24",
          "form": "두 개가.. 다른지도..",
          "original_form": "두 개가.. 다른지도..",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039551.1.25",
          "form": "방금 알앗네요...?",
          "original_form": "방금 알앗네요...?",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039551.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ놈 웃겨여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ놈 웃겨여",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039551.1.27",
          "form": "어떤거 더 조아하시는지 물어보려고 해ㅛ는데...",
          "original_form": "어떤거 더 조아하시는지 물어보려고 해ㅛ는데...",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039551.1.28",
          "form": "ㅎ.ㅎ .. 제가 증말 막입이거덩요...",
          "original_form": "ㅎ.ㅎ .. 제가 증말 막입이거덩요...",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039551.1.29",
          "form": "name2님은 입맛이 좀 민감하신 편이신가요?!",
          "original_form": "&name2&님은 입맛이 좀 민감하신 편이신가요?!",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039551.1.30",
          "form": "전 비린내 빼고는 다 잘 몰라요",
          "original_form": "전 비린내 빼고는 다 잘 몰라요",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039551.1.31",
          "form": "ㅋㅋㅋㅋ저도 전혀 민감하지 않은 편입니다",
          "original_form": "ㅋㅋㅋㅋ저도 전혀 민감하지 않은 편입니다",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039551.1.32",
          "form": "가리는 음식도 별로 없어요!",
          "original_form": "가리는 음식도 별로 없어요!",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039551.1.33",
          "form": "우왁!!!! 편식 안 하시는구나",
          "original_form": "우왁!!!! 편식 안 하시는구나",
          "speaker_id": "1",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039551.1.34",
          "form": "전 완전 편식쟁이거든요..",
          "original_form": "전 완전 편식쟁이거든요..",
          "speaker_id": "1",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039551.1.35",
          "form": "편식 좀 할수도 있죠!!",
          "original_form": "편식 좀 할수도 있죠!!",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039551.1.36",
          "form": "저도 아예 안하는건 아니구 홍어? 이런군 잘 못먹어요",
          "original_form": "저도 아예 안하는건 아니구 홍어? 이런군 잘 못먹어요",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039551.1.37",
          "form": "헛 홍어는 정말 먹기 힘든 음식이죠....",
          "original_form": "헛 홍어는 정말 먹기 힘든 음식이죠....",
