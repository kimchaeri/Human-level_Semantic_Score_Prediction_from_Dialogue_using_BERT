      "utterance": [
        {
          "id": "MDRW2100035980.1.1",
          "form": "오! 딸기도 좋죠!",
          "original_form": "오! 딸기도 좋죠!",
          "speaker_id": "2",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100035980.1.2",
          "form": "생크림 좋아하세요?",
          "original_form": "생크림 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100035980.1.3",
          "form": "네!!!!!!! 저 진짜 입에 바로 짜먹을 수 있을 정도에요...",
          "original_form": "네!!!!!!! 저 진짜 입에 바로 짜먹을 수 있을 정도에요...",
          "speaker_id": "1",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100035980.1.4",
          "form": "대박대박 어쩜 이리 통할수가 있나요",
          "original_form": "대박대박 어쩜 이리 통할수가 있나요",
          "speaker_id": "2",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100035980.1.5",
          "form": "누가 주기적으로 후식으로 갖다주면 좋겠어요",
          "original_form": "누가 주기적으로 후식으로 갖다주면 좋겠어요",
          "speaker_id": "2",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100035980.1.6",
          "form": "그니까요... 전 요새 크루아상에 빠져서",
          "original_form": "그니까요... 전 요새 크루아상에 빠져서",
          "speaker_id": "1",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100035980.1.7",
          "form": "하루에 5개는 먹고 있어요.. ㅎ.ㅎ",
          "original_form": "하루에 5개는 먹고 있어요.. ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100035980.1.8",
          "form": "저는 베이글!",
          "original_form": "저는 베이글!",
          "speaker_id": "2",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100035980.1.9",
          "form": "뒤돌면 먹어요ㅋㅋㅋ",
          "original_form": "뒤돌면 먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100035980.1.10",
          "form": "아잇 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그럴만하죠 베이글 맛있잖아요",
          "original_form": "아잇 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그럴만하죠 베이글 맛있잖아요",
          "speaker_id": "1",
          "time": "20210830 18:13"
        },
        {
          "id": "MDRW2100035980.1.11",
          "form": "전 거기에 땅콩버터 올려먹는 거 좋아해요",
          "original_form": "전 거기에 땅콩버터 올려먹는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210830 18:14"
        },
        {
          "id": "MDRW2100035980.1.12",
          "form": "크으ㅋㅋㅋㅋ 단어만으로도 최고에요 진짜아",
          "original_form": "크으ㅋㅋㅋㅋ 단어만으로도 최고에요 진짜아",
          "speaker_id": "2",
          "time": "20210830 18:18"
        },
        {
          "id": "MDRW2100035980.1.13",
          "form": "저녁은 뭐드세욥?!",
          "original_form": "저녁은 뭐드세욥?!",
          "speaker_id": "2",
          "time": "20210830 18:18"
        },
        {
          "id": "MDRW2100035980.1.14",
          "form": "저 방금 햄버거 먹었어요!!!!!!! 버거킹이요",
          "original_form": "저 방금 햄버거 먹었어요!!!!!!! 버거킹이요",
          "speaker_id": "1",
          "time": "20210830 18:18"
        },
        {
          "id": "MDRW2100035980.1.15",
          "form": "햄버거 좋아하시나요?!",
          "original_form": "햄버거 좋아하시나요?!",
          "speaker_id": "1",
          "time": "20210830 18:18"
        },
        {
          "id": "MDRW2100035980.1.16",
          "form": "넹!! 버거킹 진짜 좋아해요",
          "original_form": "넹!! 버거킹 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100035980.1.17",
          "form": "근데 5분거리앞까지는 배달되는데 저희집까지는 배달 안해줘서 자주 못먹어요ㅜㅠ",
          "original_form": "근데 5분거리앞까지는 배달되는데 저희집까지는 배달 안해줘서 자주 못먹어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 18:28"
        },
        {
          "id": "MDRW2100035980.1.18",
          "form": "헐... 말도 안 돼",
          "original_form": "헐... 말도 안 돼",
          "speaker_id": "1",
          "time": "20210830 18:29"
        },
        {
          "id": "MDRW2100035980.1.19",
          "form": "5분만 더 와주지.......",
          "original_form": "5분만 더 와주지.......",
          "speaker_id": "1",
          "time": "20210830 18:29"
        },
        {
          "id": "MDRW2100035980.1.20",
          "form": "그럼 다른 브랜드로 드시는 편이세요?",
          "original_form": "그럼 다른 브랜드로 드시는 편이세요?",
          "speaker_id": "1",
          "time": "20210830 18:29"
        },
        {
          "id": "MDRW2100035980.1.21",
          "form": "그니까요..  맴스터치나 롯데리아요!",
          "original_form": "그니까요..  맴스터치나 롯데리아요!",
          "speaker_id": "2",
          "time": "20210830 18:36"
        },
        {
          "id": "MDRW2100035980.1.22",
          "form": "오는게 그 둘 뿐이에요ㅜㅜㅜㅠ",
          "original_form": "오는게 그 둘 뿐이에요ㅜㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 18:36"
        },
        {
          "id": "MDRW2100035980.1.23",
          "form": "그래도 ㅠ 맘스터치 맛있어서 다행이에요...!!!!",
          "original_form": "그래도 ㅠ 맘스터치 맛있어서 다행이에요...!!!!",
          "speaker_id": "1",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100035980.1.24",
          "form": "저 롯대리아는 사알짝 불호라 ㅎ.ㅎ",
          "original_form": "저 롯대리아는 사알짝 불호라 ㅎ.ㅎ",
