      "utterance": [
        {
          "id": "MDRW2100005939.1.1",
          "form": "노래 뭐 들어?",
          "original_form": "노래 뭐 들어?",
          "speaker_id": "1",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100005939.1.2",
          "form": "나는 너 좋다 들어",
          "original_form": "나는 너 좋다 들어",
          "speaker_id": "2",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100005939.1.3",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100005939.1.4",
          "form": "나도 너 좋아해",
          "original_form": "나도 너 좋아해",
          "speaker_id": "1",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100005939.1.5",
          "form": "유튜브 뭐봐",
          "original_form": "유튜브 뭐봐",
          "speaker_id": "2",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100005939.1.6",
          "form": "요즘 잘 안봐",
          "original_form": "요즘 잘 안봐",
          "speaker_id": "1",
          "time": "20210907 16:59"
        },
        {
          "id": "MDRW2100005939.1.7",
          "form": "영화는 무슨 장르 좋아해",
          "original_form": "영화는 무슨 장르 좋아해",
          "speaker_id": "2",
          "time": "20210907 16:59"
        },
        {
          "id": "MDRW2100005939.1.8",
          "form": "영화는 안좋아하고 티비봐",
          "original_form": "영화는 안좋아하고 티비봐",
          "speaker_id": "1",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100005939.1.9",
          "form": "티비뭐보는데",
          "original_form": "티비뭐보는데",
          "speaker_id": "2",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100005939.1.10",
          "form": "대탈출알아?",
          "original_form": "대탈출알아?",
          "speaker_id": "1",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100005939.1.11",
          "form": "대탈출 왜모르겠어 나 악마편 봐야하는데 학교로 끌려와서 못봐",
          "original_form": "대탈출 왜모르겠어 나 악마편 봐야하는데 학교로 끌려와서 못봐",
          "speaker_id": "2",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100005939.1.12",
          "form": "나도! 수능만 끝나면 다 볼거야",
          "original_form": "나도! 수능만 끝나면 다 볼거야",
          "speaker_id": "1",
          "time": "20210907 17:09"
        },
        {
          "id": "MDRW2100005939.1.13",
          "form": "그래 나랑 넷플릭스도봐",
          "original_form": "그래 나랑 넷플릭스도봐",
          "speaker_id": "2",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100005939.1.14",
          "form": "넷플릭스 재밌겠다!",
          "original_form": "넷플릭스 재밌겠다!",
          "speaker_id": "1",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100005939.1.15",
          "form": "넷플릭스 한달무료니까 빨리 봐야해",
          "original_form": "넷플릭스 한달무료니까 빨리 봐야해",
