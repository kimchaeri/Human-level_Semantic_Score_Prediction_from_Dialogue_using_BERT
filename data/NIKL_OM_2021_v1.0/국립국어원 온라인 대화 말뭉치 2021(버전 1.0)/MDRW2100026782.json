      "utterance": [
        {
          "id": "MDRW2100026782.1.1",
          "form": "근데 하나도 안빠진다고 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 하나도 안빠진다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026782.1.2",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026782.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026782.1.4",
          "form": "그래서 뒷광고 제안도 엄청 많이 들어왔다더라",
          "original_form": "그래서 뒷광고 제안도 엄청 많이 들어왔다더라",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026782.1.5",
          "form": "근데진짜",
          "original_form": "근데진짜",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026782.1.6",
          "form": "그거 대체식 먹는사람들보면",
          "original_form": "그거 대체식 먹는사람들보면",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026782.1.7",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026782.1.8",
          "form": "한.끼에..물이나 우유에 가루..탄거",
          "original_form": "한.끼에..물이나 우유에 가루..탄거",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026782.1.9",
          "form": "딱 그거만 먹고 다른거 아무것도안먹잖아",
          "original_form": "딱 그거만 먹고 다른거 아무것도안먹잖아",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026782.1.10",
          "form": "그거보면...걍 뭘먹어도",
          "original_form": "그거보면...걍 뭘먹어도",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026782.1.11",
          "form": "그정도만 먹으면",
          "original_form": "그정도만 먹으면",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026782.1.12",
          "form": "살빠질거같음ㅋㅋㅋㅋㅋ",
          "original_form": "살빠질거같음ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026782.1.13",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026782.1.14",
          "form": "어우 또 늦게 오네",
          "original_form": "어우 또 늦게 오네",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026782.1.15",
          "form": "암튼 근데 그사람이",
          "original_form": "암튼 근데 그사람이",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026782.1.16",
          "form": "초고도 비만이었어서",
          "original_form": "초고도 비만이었어서",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026782.1.17",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026782.1.18",
          "form": "그 다이어트 관련 동영상 누를때 어떤 마음으로 누르는지 안다고",
          "original_form": "그 다이어트 관련 동영상 누를때 어떤 마음으로 누르는지 안다고",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026782.1.19",
          "form": "다 거절했대",
          "original_form": "다 거절했대",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026782.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026782.1.21",
          "form": "그 사망여우에서 다이어트 보조제 광고 분석(?)한거 보면",
          "original_form": "그 사망여우에서 다이어트 보조제 광고 분석(?)한거 보면",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026782.1.22",
          "form": "다 효과 없는것 같더라",
          "original_form": "다 효과 없는것 같더라",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026782.1.23",
          "form": "엥 이게왜 ㅏ쁜말",
          "original_form": "엥 이게왜 ㅏ쁜말",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026782.1.24",
          "form": "신.념.있네",
          "original_form": "신.념.있네",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026782.1.25",
          "form": "맞아맞아 그",
          "original_form": "맞아맞아 그",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026782.1.26",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026782.1.27",
          "form": "이름뭐였더라",
          "original_form": "이름뭐였더라",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026782.1.28",
          "form": "유튭에서 많이봤는데",
          "original_form": "유튭에서 많이봤는데",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026782.1.29",
          "form": "무슨 탕국?",
          "original_form": "무슨 탕국?",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026782.1.30",
          "form": "아 맞아맞아",
          "original_form": "아 맞아맞아",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026782.1.31",
          "form": "서양탕국이었나",
          "original_form": "서양탕국이었나",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026782.1.32",
          "form": "맞음ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞음ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026782.1.33",
          "form": "뭐 그런 이름이었던것 같은데",
          "original_form": "뭐 그런 이름이었던것 같은데",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026782.1.34",
          "form": "그거만든 사장이",
          "original_form": "그거만든 사장이",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026782.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026782.1.36",
          "form": "자기랑 친한 예쁜 여자지인들한테",
          "original_form": "자기랑 친한 예쁜 여자지인들한테",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026782.1.37",
          "form": "광고 싹돌려서",
          "original_form": "광고 싹돌려서",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026782.1.38",
          "form": "아 맞음맞음...",
          "original_form": "아 맞음맞음...",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026782.1.39",
          "form": "유명해졌잖아..",
          "original_form": "유명해졌잖아..",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026782.1.40",
          "form": "사실 효과 1도없는데ㅋㅋㅋ",
          "original_form": "사실 효과 1도없는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026782.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋ....ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ....ㅋㅋㅋㅋ",
