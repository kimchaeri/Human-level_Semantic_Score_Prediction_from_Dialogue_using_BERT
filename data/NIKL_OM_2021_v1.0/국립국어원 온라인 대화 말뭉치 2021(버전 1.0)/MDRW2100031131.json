      "utterance": [
        {
          "id": "MDRW2100031131.1.1",
          "form": "밀크티!",
          "original_form": "밀크티!",
          "speaker_id": "1",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100031131.1.2",
          "form": "얼그레이도 좋아하고",
          "original_form": "얼그레이도 좋아하고",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031131.1.3",
          "form": "아쌈도 좋아요",
          "original_form": "아쌈도 좋아요",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031131.1.4",
          "form": "가향차 중에서도 좋아하는 차가 있었는데",
          "original_form": "가향차 중에서도 좋아하는 차가 있었는데",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031131.1.5",
          "form": "이름이 생각이 안나네요",
          "original_form": "이름이 생각이 안나네요",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031131.1.6",
          "form": "아앗 차 토크 너무좋아요 후후후",
          "original_form": "아앗 차 토크 너무좋아요 후후후",
          "speaker_id": "1",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031131.1.7",
          "form": "차 좋아하시면 밀크티는 만들어서 드세요!",
          "original_form": "차 좋아하시면 밀크티는 만들어서 드세요!",
          "speaker_id": "1",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031131.1.8",
          "form": "찻잎 같은걸 어떤걸 사면 좋을지 모르겠어용 ㅠ",
          "original_form": "찻잎 같은걸 어떤걸 사면 좋을지 모르겠어용 ㅠ",
          "speaker_id": "2",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031131.1.9",
          "form": "만드는 방법이 어렵지 않나요?",
          "original_form": "만드는 방법이 어렵지 않나요?",
          "speaker_id": "2",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031131.1.10",
          "form": "전혀요!!! 제가 알려드릴게요",
          "original_form": "전혀요!!! 제가 알려드릴게요",
          "speaker_id": "1",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100031131.1.11",
          "form": "일단 쿠팡에서 요크셔 골드 티를 사세요ㅋㅋㅋㅋ",
          "original_form": "일단 쿠팡에서 요크셔 골드 티를 사세요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100031131.1.12",
          "form": "와! 저 지금부터 메모 시작합니다!!",
          "original_form": "와! 저 지금부터 메모 시작합니다!!",
          "speaker_id": "2",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100031131.1.13",
          "form": "전자레인지로 만드는 방법이 하나, 거름망 사용해서 만드는 방법이 두개 있어요",
          "original_form": "전자레인지로 만드는 방법이 하나, 거름망 사용해서 만드는 방법이 두개 있어요",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100031131.1.14",
          "form": "고르세요!",
          "original_form": "고르세요!",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100031131.1.15",
          "form": "음... 거름망은 얼마나 되는 크기 사용해야하나요??",
          "original_form": "음... 거름망은 얼마나 되는 크기 사용해야하나요??",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100031131.1.16",
          "form": "있다면 거름망 방법도 괜찮을 것 같아요!",
          "original_form": "있다면 거름망 방법도 괜찮을 것 같아요!",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100031131.1.17",
          "form": "일회용 차우림망 50개입 마트에서 파는걸 사셔도 되고",
          "original_form": "일회용 차우림망 50개입 마트에서 파는걸 사셔도 되고",
          "speaker_id": "1",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100031131.1.18",
          "form": "다회용 스트레이너도 좋아요!",
          "original_form": "다회용 스트레이너도 좋아요!",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100031131.1.19",
          "form": "마트에서 차우림망을 따로 파는군요!",
          "original_form": "마트에서 차우림망을 따로 파는군요!",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100031131.1.20",
          "form": "주의 깊게 살펴보지 않아서 몰랐어요",
          "original_form": "주의 깊게 살펴보지 않아서 몰랐어요",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100031131.1.21",
          "form": "네네! 그거랑 전자레인지만 있으면",
          "original_form": "네네! 그거랑 전자레인지만 있으면",
          "speaker_id": "1",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100031131.1.22",
          "form": "완전 편하게 만들 수 있어요",
          "original_form": "완전 편하게 만들 수 있어요",
          "speaker_id": "1",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100031131.1.23",
          "form": "오오!! 그리고 어떻게 하면 되나요!!",
          "original_form": "오오!! 그리고 어떻게 하면 되나요!!",
          "speaker_id": "2",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100031131.1.24",
          "form": "메모할 준비가 되셨나요!",
          "original_form": "메모할 준비가 되셨나요!",
          "speaker_id": "1",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100031131.1.25",
          "form": "머그컵 한 컵 (300ml) 기준으로 알려드릴게요",
          "original_form": "머그컵 한 컵 (300ml) 기준으로 알려드릴게요",
          "speaker_id": "1",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100031131.1.26",
          "form": "네 선생님!!",
          "original_form": "네 선생님!!",
          "speaker_id": "2",
          "time": "20210906 15:35"
        },
        {
          "id": "MDRW2100031131.1.27",
          "form": "말씀해주세요!!!",
          "original_form": "말씀해주세요!!!",
          "speaker_id": "2",
          "time": "20210906 15:35"
        }
      ]
    }
  ]
