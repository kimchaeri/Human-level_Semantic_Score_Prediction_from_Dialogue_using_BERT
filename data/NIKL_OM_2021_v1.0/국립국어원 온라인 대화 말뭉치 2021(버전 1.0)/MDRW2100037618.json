      "utterance": [
        {
          "id": "MDRW2100037618.1.1",
          "form": "라면 좋아하세요?",
          "original_form": "라면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037618.1.2",
          "form": "막 좋아하진 않는데 별식 느낌으로 먹어요",
          "original_form": "막 좋아하진 않는데 별식 느낌으로 먹어요",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037618.1.3",
          "form": "드시면 어떤 라면 드세요?",
          "original_form": "드시면 어떤 라면 드세요?",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037618.1.4",
          "form": "국물있는거 드시나용",
          "original_form": "국물있는거 드시나용",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037618.1.5",
          "form": "국물 볶음ㅁ면 다 먹어요ㅛㅛ",
          "original_form": "국물 볶음ㅁ면 다 먹어요ㅛㅛ",
          "speaker_id": "1",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100037618.1.6",
          "form": "아니오타가",
          "original_form": "아니오타가",
          "speaker_id": "1",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100037618.1.7",
          "form": "아ㅋㅋ 그러시구나!",
          "original_form": "아ㅋㅋ 그러시구나!",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037618.1.8",
          "form": "어떤라면 최애세요?",
          "original_form": "어떤라면 최애세요?",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037618.1.9",
          "form": "name2님은 최애라면이 있나요?",
          "original_form": "&name2&님은 최애라면이 있나요?",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037618.1.10",
          "form": "전 말씀드렸다시피 아주 막 좋아하고 이런게 아니라 그닥,,",
          "original_form": "전 말씀드렸다시피 아주 막 좋아하고 이런게 아니라 그닥,,",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100037618.1.11",
          "form": "저는 열라면 제일 좋아해요 ..! 라면 좋아하는데 몸에 안좋다해서 .. 자주는 못먹지만요",
          "original_form": "저는 열라면 제일 좋아해요 ..! 라면 좋아하는데 몸에 안좋다해서 .. 자주는 못먹지만요",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037618.1.12",
          "form": "야채나 단백질만 충분히 같이 섭취해주고",
          "original_form": "야채나 단백질만 충분히 같이 섭취해주고",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100037618.1.13",
          "form": "국물을 다 마시지 않으면 건강에 그렇게 나쁘진 않아요",
          "original_form": "국물을 다 마시지 않으면 건강에 그렇게 나쁘진 않아요",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100037618.1.14",
          "form": "그건 맞죠!!",
          "original_form": "그건 맞죠!!",
          "speaker_id": "2",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100037618.1.15",
          "form": "집에서 요리 자주 해드세요?",
          "original_form": "집에서 요리 자주 해드세요?",
          "speaker_id": "2",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100037618.1.16",
          "form": "음..되도록 집에서 먹으려해요",
          "original_form": "음..되도록 집에서 먹으려해요",
          "speaker_id": "1",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100037618.1.17",
          "form": "요리라고 해야할지..암튼 네.",
          "original_form": "요리라고 해야할지..암튼 네.",
          "speaker_id": "1",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100037618.1.18",
          "form": "그쵸 집에서 간단하게라도 해먹죠  ..!",
          "original_form": "그쵸 집에서 간단하게라도 해먹죠  ..!",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037618.1.19",
          "form": "어떤걸 제일 많이해드세요? 저는 파스타 간단해서 많이 해먹는 것 같아요",
          "original_form": "어떤걸 제일 많이해드세요? 저는 파스타 간단해서 많이 해먹는 것 같아요",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037618.1.20",
          "form": "파스타하면 단백질 먹기가 어려워서",
          "original_form": "파스타하면 단백질 먹기가 어려워서",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100037618.1.21",
          "form": "냉동닭  부위별로 번갈아가며 굽고",
          "original_form": "냉동닭  부위별로 번갈아가며 굽고",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100037618.1.22",
          "form": "대충 야채랑 밥이요",
          "original_form": "대충 야채랑 밥이요",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100037618.1.23",
          "form": "오 단백질 잘 챙겨드시나봐요 식단관리하시나요?",
          "original_form": "오 단백질 잘 챙겨드시나봐요 식단관리하시나요?",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037618.1.24",
          "form": "잘챙겨먹으려고 하는거지 관리까진 아니에요 ㅋㅋ",
          "original_form": "잘챙겨먹으려고 하는거지 관리까진 아니에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037618.1.25",
          "form": "오.. 그래도 영양소 챙기시는거 보면 대단하셔요!",
          "original_form": "오.. 그래도 영양소 챙기시는거 보면 대단하셔요!",
          "speaker_id": "2",
