      "utterance": [
        {
          "id": "MDRW2100018897.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018897.1.2",
          "form": "여행계획있으신가요??",
          "original_form": "여행계획있으신가요??",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018897.1.3",
          "form": "코로나 좀 잠잠해지면 경주가보려구요",
          "original_form": "코로나 좀 잠잠해지면 경주가보려구요",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018897.1.4",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018897.1.5",
          "form": "넵 저도 코로나 잠잠해지면 강릉 가려구요 ㅎㅎ",
          "original_form": "넵 저도 코로나 잠잠해지면 강릉 가려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:13"
        },
        {
          "id": "MDRW2100018897.1.6",
          "form": "오호! 강릉 좋죠!\r\n동해쪽은 다 좋은거같아요",
          "original_form": "오호! 강릉 좋죠!\r\n동해쪽은 다 좋은거같아요",
          "speaker_id": "2",
          "time": "20210827 14:13"
        },
        {
          "id": "MDRW2100018897.1.7",
          "form": "강릉 커피거리도 유명 하더라구요",
          "original_form": "강릉 커피거리도 유명 하더라구요",
          "speaker_id": "2",
          "time": "20210827 14:13"
        },
        {
          "id": "MDRW2100018897.1.8",
          "form": "저도 경주 갔을때 너무 좋았어요!!",
          "original_form": "저도 경주 갔을때 너무 좋았어요!!",
          "speaker_id": "1",
          "time": "20210827 14:13"
        },
        {
          "id": "MDRW2100018897.1.9",
          "form": "아 경주 가보셨구나",
          "original_form": "아 경주 가보셨구나",
          "speaker_id": "2",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018897.1.10",
          "form": "경주가시면 유적지 중심으로 여행가실건강ㅎ",
          "original_form": "경주가시면 유적지 중심으로 여행가실건강ㅎ",
          "speaker_id": "1",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018897.1.11",
          "form": "건가요??",
          "original_form": "건가요??",
          "speaker_id": "1",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018897.1.12",
          "form": "야경이 진짜 이쁜거같더라구요",
          "original_form": "야경이 진짜 이쁜거같더라구요",
          "speaker_id": "2",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018897.1.13",
          "form": "네 유적지도 가보고",
          "original_form": "네 유적지도 가보고",
          "speaker_id": "2",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018897.1.14",
          "form": "친구들은 놀이공원도 가자고 하더라구요ㅠ",
          "original_form": "친구들은 놀이공원도 가자고 하더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018897.1.15",
          "form": "아 이월드? 맞죠",
          "original_form": "아 이월드? 맞죠",
          "speaker_id": "1",
          "time": "20210827 14:15"
        },
        {
          "id": "MDRW2100018897.1.16",
          "form": "이름은 잘 모르겠어요ㅋㅋ\r\n드라켄? 그거 있는곳이요",
          "original_form": "이름은 잘 모르겠어요ㅋㅋ\r\n드라켄? 그거 있는곳이요",
          "speaker_id": "2",
          "time": "20210827 14:18"
        },
        {
          "id": "MDRW2100018897.1.17",
          "form": "전 놀이기구 잘 못타가지구 안가고싶은데ㅠ",
          "original_form": "전 놀이기구 잘 못타가지구 안가고싶은데ㅠ",
          "speaker_id": "2",
          "time": "20210827 14:18"
        },
        {
          "id": "MDRW2100018897.1.18",
          "form": "강릉은 뭐 때문에 가고싶으세요?",
          "original_form": "강릉은 뭐 때문에 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210827 14:18"
        },
        {
          "id": "MDRW2100018897.1.19",
          "form": "ㅠㅠ전 놀이기구타는거 좋아해서 놀이공원가고싶네용",
          "original_form": "ㅠㅠ전 놀이기구타는거 좋아해서 놀이공원가고싶네용",
          "speaker_id": "1",
          "time": "20210827 17:58"
        },
        {
          "id": "MDRW2100018897.1.20",
          "form": "강릉은 친구가 같이가자고 여러번 졸라서",
          "original_form": "강릉은 친구가 같이가자고 여러번 졸라서",
          "speaker_id": "1",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100018897.1.21",
          "form": "가려구요!!!",
          "original_form": "가려구요!!!",
          "speaker_id": "1",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100018897.1.22",
          "form": "놀이기구 좋아하면 가볼만 하긴 할거같아요",
          "original_form": "놀이기구 좋아하면 가볼만 하긴 할거같아요",
          "speaker_id": "2",
          "time": "20210827 18:02"
        },
        {
          "id": "MDRW2100018897.1.23",
          "form": "거긴 퍼레이드 이런거없나요??",
          "original_form": "거긴 퍼레이드 이런거없나요??",
          "speaker_id": "1",
          "time": "20210827 18:08"
        },
        {
          "id": "MDRW2100018897.1.24",
          "form": "놀이공원싫어해도 퍼레이드 보면 너무 좋던데..",
          "original_form": "놀이공원싫어해도 퍼레이드 보면 너무 좋던데..",
          "speaker_id": "1",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100018897.1.25",
          "form": "경주는 잘 모르겠어요ㅠ\r\n아예 찾아보지도 않았어서ㅋㅋㅋ\r\n퍼레이드는 좋죠ㅋㅋㅋ",
          "original_form": "경주는 잘 모르겠어요ㅠ\r\n아예 찾아보지도 않았어서ㅋㅋㅋ\r\n퍼레이드는 좋죠ㅋㅋㅋ",
          "speaker_id": "2",
