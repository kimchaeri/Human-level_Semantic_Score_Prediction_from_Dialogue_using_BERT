      "utterance": [
        {
          "id": "MDRW2100028483.1.1",
          "form": "룸메이트 있으세요",
          "original_form": "룸메이트 있으세요",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028483.1.2",
          "form": "음 와이프가 룸메이트기도 하죠 ㅋㅋ",
          "original_form": "음 와이프가 룸메이트기도 하죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028483.1.3",
          "form": "결혼하셨군요 저는 아직 미혼인데",
          "original_form": "결혼하셨군요 저는 아직 미혼인데",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028483.1.4",
          "form": "남친이 해외유학 중이에요ㅠㅠ",
          "original_form": "남친이 해외유학 중이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028483.1.5",
          "form": "아 이시기에. . .",
          "original_form": "아 이시기에. . .",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028483.1.6",
          "form": "보기 힘드시겠어요 더욱ㅠ",
          "original_form": "보기 힘드시겠어요 더욱ㅠ",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028483.1.7",
          "form": "네 가자마자 코로나...",
          "original_form": "네 가자마자 코로나...",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028483.1.8",
          "form": "결혼생활 궁금해요",
          "original_form": "결혼생활 궁금해요",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028483.1.9",
          "form": "뭐 사람 사는게",
          "original_form": "뭐 사람 사는게",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028483.1.10",
          "form": "똑같죠 ㅎㅎ",
          "original_form": "똑같죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028483.1.11",
          "form": "그런가요 ㅋㅋ",
          "original_form": "그런가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028483.1.12",
          "form": "전 너무 기대중인데",
          "original_form": "전 너무 기대중인데",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028483.1.13",
          "form": "ㅋㅋ음 어떤점이요?",
          "original_form": "ㅋㅋ음 어떤점이요?",
          "speaker_id": "2",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028483.1.14",
          "form": "뭐 행복하긴 합니다 ㅋㅋ",
          "original_form": "뭐 행복하긴 합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028483.1.15",
          "form": "뭔가 안정감과 행복감이쿨고같아서요",
          "original_form": "뭔가 안정감과 행복감이쿨고같아서요",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028483.1.16",
          "form": "아 그건 좋죠",
          "original_form": "아 그건 좋죠",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028483.1.17",
          "form": "근데 이제 알아서 해야하는것이 ㅎㅎ",
          "original_form": "근데 이제 알아서 해야하는것이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028483.1.18",
          "form": "그렇군요 ! ㅋㅋㅋ",
          "original_form": "그렇군요 ! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:44"
        },
        {
          "id": "MDRW2100028483.1.19",
          "form": "집안일 어떤게 힘드세용?",
          "original_form": "집안일 어떤게 힘드세용?",
          "speaker_id": "1",
          "time": "20210903 08:44"
        },
        {
          "id": "MDRW2100028483.1.20",
          "form": "힘들다기보다는 음",
          "original_form": "힘들다기보다는 음",
          "speaker_id": "2",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100028483.1.21",
          "form": "원래는 엄마 이거 안되는데",
          "original_form": "원래는 엄마 이거 안되는데",
          "speaker_id": "2",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100028483.1.22",
          "form": "에서",
          "original_form": "에서",
          "speaker_id": "2",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100028483.1.23",
          "form": "그냥 제가 해야하는거죠 ㅋㅋ",
          "original_form": "그냥 제가 해야하는거죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100028483.1.24",
          "form": "앜ㅋㅋㅋㅋㅋ다 하시는균요",
          "original_form": "앜ㅋㅋㅋㅋㅋ다 하시는균요",
          "speaker_id": "1",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100028483.1.25",
          "form": "네 다 알아서 ㅎㅎ 분리수거부터해서",
          "original_form": "네 다 알아서 ㅎㅎ 분리수거부터해서",
          "speaker_id": "2",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100028483.1.26",
          "form": "맞아요 분리수거 설거지 빨래 청소기 너무많죠",
          "original_form": "맞아요 분리수거 설거지 빨래 청소기 너무많죠",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100028483.1.27",
          "form": "설거지는 식기세척기가!!",
          "original_form": "설거지는 식기세척기가!!",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100028483.1.28",
          "form": "와 식기세척기 어디꺼 쓰세요 사고파요",
          "original_form": "와 식기세척기 어디꺼 쓰세요 사고파요",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100028483.1.29",
          "form": "쿠쿠꺼 추천해요",
          "original_form": "쿠쿠꺼 추천해요",
          "speaker_id": "2",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100028483.1.30",
          "form": "가성비 좋습니다",
          "original_form": "가성비 좋습니다",
          "speaker_id": "2",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100028483.1.31",
          "form": "쿠쿠 한번 알아봐야겠네요",
          "original_form": "쿠쿠 한번 알아봐야겠네요",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100028483.1.32",
          "form": "비스포크 보고있었는데 너무 비싸서ㅠㅠ",
          "original_form": "비스포크 보고있었는데 너무 비싸서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100028483.1.33",
          "form": "비스포크 이쁘긴한데 가격이 깡패라 ㅠㅠ",
          "original_form": "비스포크 이쁘긴한데 가격이 깡패라 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100028483.1.34",
          "form": "저 냉장고는 비스포크입니다 ㅎㅎ",
          "original_form": "저 냉장고는 비스포크입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100028483.1.35",
          "form": "와 냉장고 이쁘시겠다 무슨 색이에요",
          "original_form": "와 냉장고 이쁘시겠다 무슨 색이에요",
          "speaker_id": "1",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100028483.1.36",
          "form": "하양분홍\r\n분홍하양",
          "original_form": "하양분홍\r\n분홍하양",
          "speaker_id": "2",
          "time": "20210903 15:16"
        },
        {
          "id": "MDRW2100028483.1.37",
          "form": "색깔 더 넣을랬는데",
          "original_form": "색깔 더 넣을랬는데",
          "speaker_id": "2",
          "time": "20210903 15:16"
        },
        {
          "id": "MDRW2100028483.1.38",
          "form": "반대당했어요",
          "original_form": "반대당했어요",
          "speaker_id": "2",
          "time": "20210903 15:16"
        },
        {
          "id": "MDRW2100028483.1.39",
          "form": "앜ㅋㅋ딱 이쁘겠어요",
          "original_form": "앜ㅋㅋ딱 이쁘겠어요",
          "speaker_id": "1",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100028483.1.40",
          "form": "아 노랑색",
          "original_form": "아 노랑색",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100028483.1.41",
          "form": "하나 넣었어야 하는데",
          "original_form": "하나 넣었어야 하는데",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100028483.1.42",
          "form": "아.. 지금이 더 이쁘실거에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아.. 지금이 더 이쁘실거에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100028483.1.43",
          "form": "근데 지금 집이 좁아서",
          "original_form": "근데 지금 집이 좁아서",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100028483.1.44",
          "form": "정면이 잘 안보입니다ㅋㅋㅋ",
          "original_form": "정면이 잘 안보입니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100028483.1.45",
          "form": "그럴군요 ㅋㅋㅋㅋ",
          "original_form": "그럴군요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100028483.1.46",
          "form": "비스포크 이쁘겠다",
          "original_form": "비스포크 이쁘겠다",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100028483.1.47",
          "form": "ㅋㅋ 추천해요",
          "original_form": "ㅋㅋ 추천해요",
          "speaker_id": "2",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100028483.1.48",
          "form": "결혼하면 사고파요 비스포크",
          "original_form": "결혼하면 사고파요 비스포크",
