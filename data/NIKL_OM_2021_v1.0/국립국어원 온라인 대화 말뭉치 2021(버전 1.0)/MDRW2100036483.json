      "utterance": [
        {
          "id": "MDRW2100036483.1.1",
          "form": "엉 뉴욕 메트로폴리탄",
          "original_form": "엉 뉴욕 메트로폴리탄",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036483.1.2",
          "form": "러시아 에르미타주 미술관도 잇고",
          "original_form": "러시아 에르미타주 미술관도 잇고",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036483.1.3",
          "form": "그래 맞다",
          "original_form": "그래 맞다",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036483.1.4",
          "form": "마드리드의 프라도 미술관도 3대 미술관에 속함",
          "original_form": "마드리드의 프라도 미술관도 3대 미술관에 속함",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036483.1.5",
          "form": "프랑스는 아니고",
          "original_form": "프랑스는 아니고",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036483.1.6",
          "form": "ㅋㅋ주관적인거니까",
          "original_form": "ㅋㅋ주관적인거니까",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036483.1.7",
          "form": "ㅋㅋ프라도 미술관은 어쩌는데",
          "original_form": "ㅋㅋ프라도 미술관은 어쩌는데",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036483.1.8",
          "form": "스페인 회화의 보고라고 불린데",
          "original_form": "스페인 회화의 보고라고 불린데",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036483.1.9",
          "form": "회화가 많겠다 그럼",
          "original_form": "회화가 많겠다 그럼",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036483.1.10",
          "form": "엉 유럽 미술사의 보고라고도 불리니 엄청 중요하면서 많을듯",
          "original_form": "엉 유럽 미술사의 보고라고도 불리니 엄청 중요하면서 많을듯",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036483.1.11",
          "form": "유명화가는",
          "original_form": "유명화가는",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036483.1.12",
          "form": "엘그레코, 벨라스케스, 그리고 고야",
          "original_form": "엘그레코, 벨라스케스, 그리고 고야",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036483.1.13",
          "form": "고야그림은 나도 보고 싶다",
          "original_form": "고야그림은 나도 보고 싶다",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036483.1.14",
          "form": "엉 그밖에도 여러 거장  화가가 있어서 볼게 많을듯하다",
          "original_form": "엉 그밖에도 여러 거장  화가가 있어서 볼게 많을듯하다",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036483.1.15",
          "form": "세비야 대성당 알제",
          "original_form": "세비야 대성당 알제",
