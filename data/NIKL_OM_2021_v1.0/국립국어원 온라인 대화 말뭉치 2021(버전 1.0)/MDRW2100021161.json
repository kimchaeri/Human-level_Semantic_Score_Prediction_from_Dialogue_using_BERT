      "utterance": [
        {
          "id": "MDRW2100021161.1.1",
          "form": "소고기 미역국에 밥 말아 먹었어요",
          "original_form": "소고기 미역국에 밥 말아 먹었어요",
          "speaker_id": "2",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100021161.1.2",
          "form": "이것도 국이군요! ",
          "original_form": "이것도 국이군요! ",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021161.1.3",
          "form": "저도 소고기 미역국을 ㅡ좋아해요 조개보단",
          "original_form": "저도 소고기 미역국을 ㅡ좋아해요 조개보단",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021161.1.4",
          "form": "조개 넣어도 맛있겠네요. 국물이 더 시원할것 같아요",
          "original_form": "조개 넣어도 맛있겠네요. 국물이 더 시원할것 같아요",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021161.1.5",
          "form": "네 시원하고 조개류의 씹히는 맛이 있어요",
          "original_form": "네 시원하고 조개류의 씹히는 맛이 있어요",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021161.1.6",
          "form": "많이 넣어도 맛이 별로 안 진하구요",
          "original_form": "많이 넣어도 맛이 별로 안 진하구요",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021161.1.7",
          "form": "그럼 조미료를 살짝넣으세요",
          "original_form": "그럼 조미료를 살짝넣으세요",
          "speaker_id": "2",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021161.1.8",
          "form": "사실 좀 넣어요 ㅋㅋㅋㅋㅋㅋㅋ조개맛다시다요",
          "original_form": "사실 좀 넣어요 ㅋㅋㅋㅋㅋㅋㅋ조개맛다시다요",
          "speaker_id": "1",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021161.1.9",
          "form": "그쵸.넣어야 해요",
          "original_form": "그쵸.넣어야 해요",
          "speaker_id": "2",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021161.1.10",
          "form": "안넣으면 맛이 안나요",
          "original_form": "안넣으면 맛이 안나요",
          "speaker_id": "2",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021161.1.11",
          "form": "맞아요 ㅋㅋㅋ 고향의 맛 다시다",
          "original_form": "맞아요 ㅋㅋㅋ 고향의 맛 다시다",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100021161.1.12",
          "form": "많이 넣으면. 느끼한데 적당히 넣으면 맛이 배가 되요",
          "original_form": "많이 넣으면. 느끼한데 적당히 넣으면 맛이 배가 되요",
          "speaker_id": "2",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100021161.1.13",
          "form": "적당한 우마미가 역시 밥도둑",
          "original_form": "적당한 우마미가 역시 밥도둑",
          "speaker_id": "1",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100021161.1.14",
          "form": "우마미가 뭔가요?",
          "original_form": "우마미가 뭔가요?",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021161.1.15",
          "form": "글루탐산 나트륨 맛을 우마미라고 해요 일본인이 첨 만들어서요",
          "original_form": "글루탐산 나트륨 맛을 우마미라고 해요 일본인이 첨 만들어서요",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021161.1.16",
          "form": "아항~ 우마미",
          "original_form": "아항~ 우마미",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021161.1.17",
          "form": "그래서 영어로umami= 미원류 감칠맛",
          "original_form": "그래서 영어로umami= 미원류 감칠맛",
          "speaker_id": "1",
