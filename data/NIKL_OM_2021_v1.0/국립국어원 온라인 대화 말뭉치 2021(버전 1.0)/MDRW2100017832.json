      "utterance": [
        {
          "id": "MDRW2100017832.1.1",
          "form": "읽을만한 책 있으면 추천해줘",
          "original_form": "읽을만한 책 있으면 추천해줘",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017832.1.2",
          "form": "나는 좁고 얕은 책만 읽는걸...",
          "original_form": "나는 좁고 얕은 책만 읽는걸...",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017832.1.3",
          "form": "내 취향이 언니 취향에 맞을까..?",
          "original_form": "내 취향이 언니 취향에 맞을까..?",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017832.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ 나 장르소설은 생각보다 취향이 포괄적이야",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 나 장르소설은 생각보다 취향이 포괄적이야",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017832.1.5",
          "form": "그냥 다 찍먹하는 중이긴 한데\n\n장르를 바꿔보겠습니다 였나 그거 웹툰 보다가 소설 넘어갔는데 나쁘진 않았엌",
          "original_form": "그냥 다 찍먹하는 중이긴 한데\n\n장르를 바꿔보겠습니다 였나 그거 웹툰 보다가 소설 넘어갔는데 나쁘진 않았엌",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017832.1.6",
          "form": "그거 무슨 내용이엇지...?",
          "original_form": "그거 무슨 내용이엇지...?",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017832.1.7",
          "form": "소설 주인공 이모? 로 빙의했는데 삼촌이 찾아오니까",
          "original_form": "소설 주인공 이모? 로 빙의했는데 삼촌이 찾아오니까",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017832.1.8",
          "form": "엄마! 하면서 같이 데려가서? 지내는 내용?",
          "original_form": "엄마! 하면서 같이 데려가서? 지내는 내용?",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017832.1.9",
          "form": "조아라에 연재됐던거지?",
          "original_form": "조아라에 연재됐던거지?",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017832.1.10",
          "form": "왜 기억이 안나지...",
          "original_form": "왜 기억이 안나지...",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017832.1.11",
          "form": "아마..? 난 카카페 런칭하고 나서야 알았어",
          "original_form": "아마..? 난 카카페 런칭하고 나서야 알았어",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017832.1.12",
          "form": "조아라에 관심이 죽어가는 중이라..",
          "original_form": "조아라에 관심이 죽어가는 중이라..",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017832.1.13",
          "form": "조아라 요즘 너무 볼 게 없긴 해...",
          "original_form": "조아라 요즘 너무 볼 게 없긴 해...",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017832.1.14",
          "form": "소설 연재하던 것도 일하면서 못하고 잇고",
          "original_form": "소설 연재하던 것도 일하면서 못하고 잇고",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017832.1.15",
          "form": "ㅋ큐ㅠ",
          "original_form": "ㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017832.1.16",
          "form": "처음엔 카카페 수준으로 하향평준화 된다고 아쉬워했는데",
          "original_form": "처음엔 카카페 수준으로 하향평준화 된다고 아쉬워했는데",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017832.1.17",
          "form": "이제는 카카페도 못갈 수준임...",
          "original_form": "이제는 카카페도 못갈 수준임...",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017832.1.18",
          "form": "문장구사력 무슨일....",
          "original_form": "문장구사력 무슨일....",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017832.1.19",
          "form": "하향평준화도.. 감지덕지였음..",
          "original_form": "하향평준화도.. 감지덕지였음..",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017832.1.20",
          "form": "그니까...",
          "original_form": "그니까...",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017832.1.21",
          "form": "아니 나쁜말이라 다 안된대,,",
          "original_form": "아니 나쁜말이라 다 안된대,,",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017832.1.22",
          "form": "ㅋ큐ㅠㅠ",
          "original_form": "ㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017832.1.23",
          "form": "이럴때 치고 나가야 하는데(이거 아님",
          "original_form": "이럴때 치고 나가야 하는데(이거 아님",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017832.1.24",
          "form": "아이고 저런...",
          "original_form": "아이고 저런...",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017832.1.25",
          "form": "난 맞춤법 틀렸다는 소리밖에 안했는데...",
          "original_form": "난 맞춤법 틀렸다는 소리밖에 안했는데...",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017832.1.26",
          "form": "멍멍이처럼",
          "original_form": "멍멍이처럼",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017832.1.27",
          "form": "그렇다는 말이구나",
          "original_form": "그렇다는 말이구나",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017832.1.28",
          "form": "나도 나쁜말이라 안된대 멍멍이 그런말이",
          "original_form": "나도 나쁜말이라 안된대 멍멍이 그런말이",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017832.1.29",
          "form": "개연성도 너무 없어서",
          "original_form": "개연성도 너무 없어서",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017832.1.30",
          "form": "장르소설인거 알고 읽으면서도 어이가 없어",
          "original_form": "장르소설인거 알고 읽으면서도 어이가 없어",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017832.1.31",
          "form": "베스트 셀러가 죄다 육아물일때부터 알아보긴 했지만...",
          "original_form": "베스트 셀러가 죄다 육아물일때부터 알아보긴 했지만...",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017832.1.32",
          "form": "지금 어떤거 쓰고있는데?",
          "original_form": "지금 어떤거 쓰고있는데?",
          "speaker_id": "1",
          "time": "20210826 21:32"
        }
      ]
