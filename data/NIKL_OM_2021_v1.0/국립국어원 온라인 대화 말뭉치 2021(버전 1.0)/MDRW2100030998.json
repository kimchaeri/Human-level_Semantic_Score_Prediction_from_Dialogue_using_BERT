      "utterance": [
        {
          "id": "MDRW2100030998.1.1",
          "form": "친구야 백신예약 했니",
          "original_form": "친구야 백신예약 했니",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100030998.1.2",
          "form": "나 9월 중순에 예약했어",
          "original_form": "나 9월 중순에 예약했어",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100030998.1.3",
          "form": "나도 다음주에 가기로 했어.",
          "original_form": "나도 다음주에 가기로 했어.",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030998.1.4",
          "form": "근데 진짜 저 10부제해서 사람들이 확 몰려서 예약창 터지고 그러지 않는건 좋은 일인데",
          "original_form": "근데 진짜 저 10부제해서 사람들이 확 몰려서 예약창 터지고 그러지 않는건 좋은 일인데",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030998.1.5",
          "form": "시간 선택이 오전 한자리 오후 한자리 밖에 없더라",
          "original_form": "시간 선택이 오전 한자리 오후 한자리 밖에 없더라",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030998.1.6",
          "form": "문제는 내 생일때매 내가 꼴지예약이라고",
          "original_form": "문제는 내 생일때매 내가 꼴지예약이라고",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030998.1.7",
          "form": "그거 당일에 예약 꽉 차서 그래",
          "original_form": "그거 당일에 예약 꽉 차서 그래",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030998.1.8",
          "form": "아 맞아... 너무 하다구 흑흑",
          "original_form": "아 맞아... 너무 하다구 흑흑",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030998.1.9",
          "form": "그 시간밖에 자리가 안 비어서",
          "original_form": "그 시간밖에 자리가 안 비어서",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030998.1.10",
          "form": "내 주변에는 다들 일찍 예약이 됐는데 나는 생일때문에 9월 중순되서 좀 그랴",
          "original_form": "내 주변에는 다들 일찍 예약이 됐는데 나는 생일때문에 9월 중순되서 좀 그랴",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030998.1.11",
          "form": "그러니께",
          "original_form": "그러니께",
          "speaker_id": "2",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030998.1.12",
          "form": "10부제하면서",
          "original_form": "10부제하면서",
          "speaker_id": "2",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030998.1.13",
          "form": "자리도 쪼개서",
          "original_form": "자리도 쪼개서",
          "speaker_id": "2",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030998.1.14",
          "form": "10부제 안 했으면 예약이 엄청 박터지게 어려웠을거 같긴 하지만 그래두 맨날 나만 생일때문에 꼴찌야",
          "original_form": "10부제 안 했으면 예약이 엄청 박터지게 어려웠을거 같긴 하지만 그래두 맨날 나만 생일때문에 꼴찌야",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030998.1.15",
          "form": "내놨으면 하는것도 있었다",
          "original_form": "내놨으면 하는것도 있었다",
          "speaker_id": "2",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030998.1.16",
          "form": "거기까지하기엔 어려웠겠지",
          "original_form": "거기까지하기엔 어려웠겠지",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030998.1.17",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030998.1.18",
          "form": "집근처 어디서 해 소아과?",
          "original_form": "집근처 어디서 해 소아과?",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030998.1.19",
          "form": "일처리가 다른 나라에 비해서 좋다는거 알지만 그래두 불편함",
          "original_form": "일처리가 다른 나라에 비해서 좋다는거 알지만 그래두 불편함",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030998.1.20",
          "form": "넵 소아과에서 할거구 뽀로로 스티커 노리고 있다",
          "original_form": "넵 소아과에서 할거구 뽀로로 스티커 노리고 있다",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030998.1.21",
          "form": "응...나두... 빨리 맞고 2차하고싶었는데",
          "original_form": "응...나두... 빨리 맞고 2차하고싶었는데",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030998.1.22",
          "form": "부럽다 진짜",
          "original_form": "부럽다 진짜",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030998.1.23",
          "form": "나도 뽀로로 스티커...",
          "original_form": "나도 뽀로로 스티커...",
          "speaker_id": "2",
          "time": "20210904 15:19"
        }
      ]
    }
