        {
          "id": "MDRW2100019798.1.1",
          "form": "최근에 가본 여행지는?",
          "original_form": "최근에 가본 여행지는?",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019798.1.2",
          "form": "전주!",
          "original_form": "전주!",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019798.1.3",
          "form": "저는 부산이요",
          "original_form": "저는 부산이요",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019798.1.4",
          "form": "좋으셨겠어요",
          "original_form": "좋으셨겠어요",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100019798.1.5",
          "form": "날도 선선하니 너무좋더라구요",
          "original_form": "날도 선선하니 너무좋더라구요",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019798.1.6",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019798.1.7",
          "form": "치즈쭈꾸미먹었어요",
          "original_form": "치즈쭈꾸미먹었어요",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019798.1.8",
          "form": "맛있었겠어요",
          "original_form": "맛있었겠어요",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019798.1.9",
          "form": "넹 바다보면서 먹으니 맛잏더라구요",
          "original_form": "넹 바다보면서 먹으니 맛잏더라구요",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019798.1.10",
          "form": "제주도가고싶네요",
          "original_form": "제주도가고싶네요",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019798.1.11",
          "form": "어디든 가고싶은요즘이죠",
          "original_form": "어디든 가고싶은요즘이죠",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019798.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019798.1.13",
          "form": "집에서 여행브이로그 보는 게 다지만요",
          "original_form": "집에서 여행브이로그 보는 게 다지만요",
