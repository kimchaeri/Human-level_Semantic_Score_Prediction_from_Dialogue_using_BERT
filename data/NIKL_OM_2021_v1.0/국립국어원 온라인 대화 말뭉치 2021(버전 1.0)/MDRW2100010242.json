      "utterance": [
        {
          "id": "MDRW2100010242.1.1",
          "form": "저녁 뭐먹을거야",
          "original_form": "저녁 뭐먹을거야",
          "speaker_id": "1",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100010242.1.2",
          "form": "야채 많이넣고 우동을 해먹을지 다른걸 먹을지 고민중",
          "original_form": "야채 많이넣고 우동을 해먹을지 다른걸 먹을지 고민중",
          "speaker_id": "2",
          "time": "20210823 16:00"
        },
        {
          "id": "MDRW2100010242.1.3",
          "form": "야근안하니?",
          "original_form": "야근안하니?",
          "speaker_id": "1",
          "time": "20210823 16:00"
        },
        {
          "id": "MDRW2100010242.1.4",
          "form": "배고파...",
          "original_form": "배고파...",
          "speaker_id": "1",
          "time": "20210823 16:00"
        },
        {
          "id": "MDRW2100010242.1.5",
          "form": "사세 닭봉을 먹어봤는데 좋지 않은 향이 났다",
          "original_form": "사세 닭봉을 먹어봤는데 좋지 않은 향이 났다",
          "speaker_id": "2",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010242.1.6",
          "form": "야근은 하지 않아서 집에서 밥을 먹을 수 있다",
          "original_form": "야근은 하지 않아서 집에서 밥을 먹을 수 있다",
          "speaker_id": "2",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010242.1.7",
          "form": "무슨 향이지",
          "original_form": "무슨 향이지",
          "speaker_id": "1",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010242.1.8",
          "form": "윙은 맛있었는데",
          "original_form": "윙은 맛있었는데",
          "speaker_id": "1",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010242.1.9",
          "form": "응 윙은 맛있었는데 스틱이라고 하는 닭봉은 닭의 향기가 매우 났어",
          "original_form": "응 윙은 맛있었는데 스틱이라고 하는 닭봉은 닭의 향기가 매우 났어",
          "speaker_id": "2",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010242.1.10",
          "form": "아니면 신전로제를 사가서 냉동 오징어튀김을 돌려서 같이 먹을까도 생각했지만",
          "original_form": "아니면 신전로제를 사가서 냉동 오징어튀김을 돌려서 같이 먹을까도 생각했지만",
          "speaker_id": "2",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010242.1.11",
          "form": "건강에 좋지 않을 것 같아",
          "original_form": "건강에 좋지 않을 것 같아",
          "speaker_id": "2",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010242.1.12",
          "form": "대박이군",
          "original_form": "대박이군",
          "speaker_id": "1",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010242.1.13",
          "form": "난 서브웨이나 사먹을까 고민중이야",
          "original_form": "난 서브웨이나 사먹을까 고민중이야",
          "speaker_id": "1",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010242.1.14",
          "form": "학교에서 먹니?",
          "original_form": "학교에서 먹니?",
          "speaker_id": "2",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010242.1.15",
          "form": "우리 동북미식언제가",
          "original_form": "우리 동북미식언제가",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010242.1.16",
          "form": "오늘?",
          "original_form": "오늘?",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010242.1.17",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010242.1.18",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010242.1.19",
          "form": "야 나 깜짝놀랐는데",
          "original_form": "야 나 깜짝놀랐는데",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010242.1.20",
          "form": "아마 학교에서 먹을듯",
          "original_form": "아마 학교에서 먹을듯",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010242.1.21",
          "form": "팔뚝김밥아니",
          "original_form": "팔뚝김밥아니",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010242.1.22",
          "form": "알지",
          "original_form": "알지",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010242.1.23",
          "form": "거기폐업했더라",
          "original_form": "거기폐업했더라",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010242.1.24",
          "form": "너무슬펐어 추억의 맛집이 또 사라짐",
          "original_form": "너무슬펐어 추억의 맛집이 또 사라짐",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010242.1.25",
          "form": "띠용 요새 많이 사라지네",
          "original_form": "띠용 요새 많이 사라지네",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010242.1.26",
          "form": "오래누드김밥이라는 곳에",
          "original_form": "오래누드김밥이라는 곳에",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010242.1.27",
          "form": "계란말이김밥맛있음",
          "original_form": "계란말이김밥맛있음",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010242.1.28",
          "form": "성신여고올라가는그쪽맞지",
          "original_form": "성신여고올라가는그쪽맞지",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010242.1.29",
          "form": "음 아마도?",
          "original_form": "음 아마도?",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010242.1.30",
          "form": "가보려고했는데 좀처럼 가지질않아",
          "original_form": "가보려고했는데 좀처럼 가지질않아",
          "speaker_id": "2",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010242.1.31",
          "form": "애매한 위치지",
          "original_form": "애매한 위치지",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010242.1.32",
          "form": "요새 초밥이 땡긴다",
          "original_form": "요새 초밥이 땡긴다",
