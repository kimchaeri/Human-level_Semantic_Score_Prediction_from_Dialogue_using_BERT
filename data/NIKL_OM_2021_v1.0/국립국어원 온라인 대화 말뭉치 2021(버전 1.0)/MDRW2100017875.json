      "utterance": [
        {
          "id": "MDRW2100017875.1.1",
          "form": "미용에 관심 많아요?",
          "original_form": "미용에 관심 많아요?",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017875.1.2",
          "form": "미용 큰 관심은 없긴하지만",
          "original_form": "미용 큰 관심은 없긴하지만",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017875.1.3",
          "form": "기본적인 미용은 그래도",
          "original_form": "기본적인 미용은 그래도",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017875.1.4",
          "form": "하는편이긴하죵",
          "original_form": "하는편이긴하죵",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017875.1.5",
          "form": "미용을 위해 노력하는 것 있어요?",
          "original_form": "미용을 위해 노력하는 것 있어요?",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017875.1.6",
          "form": "저는 세안하고 스킨 로션 바르는 게 다에요 ㅋㅋ",
          "original_form": "저는 세안하고 스킨 로션 바르는 게 다에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017875.1.7",
          "form": "저도 그정도 ㅋㅋㅋㅋ",
          "original_form": "저도 그정도 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017875.1.8",
          "form": "이제 염색은 고민은 하고있는데 아직까지 안하고있네요",
          "original_form": "이제 염색은 고민은 하고있는데 아직까지 안하고있네요",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017875.1.9",
          "form": "염색 해본 경험 있어요?",
          "original_form": "염색 해본 경험 있어요?",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017875.1.10",
          "form": "아뇨! 해본적은 없어용",
          "original_form": "아뇨! 해본적은 없어용",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017875.1.11",
          "form": "그래서 해보고싶긴한데 아직까지 안하구있네영",
          "original_form": "그래서 해보고싶긴한데 아직까지 안하구있네영",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017875.1.12",
          "form": "무슨 색깔 도전해보고 싶어요?",
          "original_form": "무슨 색깔 도전해보고 싶어요?",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017875.1.13",
          "form": "탈색안하는 색깔로 해보고싶어요!",
          "original_form": "탈색안하는 색깔로 해보고싶어요!",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017875.1.14",
          "form": "옅은 갈색? 계열이용",
          "original_form": "옅은 갈색? 계열이용",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017875.1.15",
          "form": "자연갈색 같은거 말씀이시죠?ㅎㅎㅎ",
          "original_form": "자연갈색 같은거 말씀이시죠?ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017875.1.16",
          "form": "아하 넹넹",
          "original_form": "아하 넹넹",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017875.1.17",
          "form": "실패 없는 색깔이라 도전해봐도 될 것 같은데",
          "original_form": "실패 없는 색깔이라 도전해봐도 될 것 같은데",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017875.1.18",
          "form": "왜 고민이에요?",
          "original_form": "왜 고민이에요?",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017875.1.19",
          "form": "미용실을 갈까 하는데",
          "original_form": "미용실을 갈까 하는데",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017875.1.20",
          "form": "요즘 주변에 미용실 코로나 확진자뜨고 막 난리도 아니라서 ㄷㄷ..,;",
          "original_form": "요즘 주변에 미용실 코로나 확진자뜨고 막 난리도 아니라서 ㄷㄷ..,;",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017875.1.21",
          "form": "그래서 고민중이에요",
          "original_form": "그래서 고민중이에요",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017875.1.22",
          "form": "좀 조용한 곳 가서 할까싶어요",
          "original_form": "좀 조용한 곳 가서 할까싶어요",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017875.1.23",
          "form": "헐 대박 ㅜㅜ",
          "original_form": "헐 대박 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017875.1.24",
          "form": "염색은 진짜 좋은 약 쓰는데로 가셔야해요",
          "original_form": "염색은 진짜 좋은 약 쓰는데로 가셔야해요",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017875.1.25",
          "form": "안그러면 머리 폭탄 돼요~!",
          "original_form": "안그러면 머리 폭탄 돼요~!",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017875.1.26",
          "form": "맞아요 그것도 고민이 좀 있긴해서 ㅠㅠ",
          "original_form": "맞아요 그것도 고민이 좀 있긴해서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017875.1.27",
          "form": "조용한곳에서 잘못하다가 머리 이상해져버리면 으짜나..이생각하고",
          "original_form": "조용한곳에서 잘못하다가 머리 이상해져버리면 으짜나..이생각하고",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017875.1.28",
          "form": "그러다보니 지금까지왔네요 ㅎㅎ ㅠㅠ",
          "original_form": "그러다보니 지금까지왔네요 ㅎㅎ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017875.1.29",
          "form": "로레알 염색약 쓰시는데로 가면 실패는 안하실 거에요~",
          "original_form": "로레알 염색약 쓰시는데로 가면 실패는 안하실 거에요~",
          "speaker_id": "1",
          "time": "20210826 21:43"
        }
      ]
