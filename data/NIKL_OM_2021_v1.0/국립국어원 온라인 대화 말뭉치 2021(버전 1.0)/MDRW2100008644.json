      "utterance": [
        {
          "id": "MDRW2100008644.1.1",
          "form": "아 ㅋㅋㅋㅋㅋ 부러워요",
          "original_form": "아 ㅋㅋㅋㅋㅋ 부러워요",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008644.1.2",
          "form": "저도 가족들이 같이하면 참 좋을탠데",
          "original_form": "저도 가족들이 같이하면 참 좋을탠데",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008644.1.3",
          "form": "일은 무슨일하세요?",
          "original_form": "일은 무슨일하세요?",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008644.1.4",
          "form": "저는 아직 공부중이에요 ㅋㅋㅋㅋ 세무사 공부중+",
          "original_form": "저는 아직 공부중이에요 ㅋㅋㅋㅋ 세무사 공부중+",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008644.1.5",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008644.1.6",
          "form": "저도 공부중이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 공부중이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008644.1.7",
          "form": "얼마전에 실습나갔다가 와.. 나 진짜 허접하구나 하고 돌아왔어요 ㅠㅠ",
          "original_form": "얼마전에 실습나갔다가 와.. 나 진짜 허접하구나 하고 돌아왔어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008644.1.8",
          "form": "앗 어떤 실습인지 여쭤봐도 되나요?",
          "original_form": "앗 어떤 실습인지 여쭤봐도 되나요?",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008644.1.9",
          "form": "그냥 회사 현장 나가서 체험하는 거였어요",
          "original_form": "그냥 회사 현장 나가서 체험하는 거였어요",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008644.1.10",
          "form": "어우 ㅠㅠ 사회생활 엄청 눈치보이더라고요",
          "original_form": "어우 ㅠㅠ 사회생활 엄청 눈치보이더라고요",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008644.1.11",
          "form": "아하아하 인턴같은 거였나봐요!",
          "original_form": "아하아하 인턴같은 거였나봐요!",
          "speaker_id": "2",
          "time": "20210822 17:33"
        },
        {
          "id": "MDRW2100008644.1.12",
          "form": "저도 사회생활 정말 힘들어요ㅜㅜ",
          "original_form": "저도 사회생활 정말 힘들어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 17:33"
        },
        {
          "id": "MDRW2100008644.1.13",
          "form": "맞아요! 기간 짧은 인턴 ㅠㅠ",
          "original_form": "맞아요! 기간 짧은 인턴 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008644.1.14",
          "form": "사회생활 시작하고..",
          "original_form": "사회생활 시작하고..",
          "speaker_id": "1",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008644.1.15",
          "form": "커피가 늘얶어요",
          "original_form": "커피가 늘얶어요",
          "speaker_id": "1",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008644.1.16",
          "form": "커피.. 힐링 포션..",
          "original_form": "커피.. 힐링 포션..",
          "speaker_id": "1",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008644.1.17",
          "form": "아아...인정... 아침커피가 없다면 전 죽은목숨....",
          "original_form": "아아...인정... 아침커피가 없다면 전 죽은목숨....",
          "speaker_id": "2",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008644.1.18",
          "form": "솔직히 지하철이 젤 힘듭니다 전 ㅋㅋㅋㅋㅋ",
          "original_form": "솔직히 지하철이 젤 힘듭니다 전 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008644.1.19",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100008644.1.20",
          "form": "아침 지하철 진심 힘들어요 ㅠㅠ",
          "original_form": "아침 지하철 진심 힘들어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100008644.1.21",
          "form": "진짜 앉아본 날이 손에 ㅁ꼽아요",
          "original_form": "진짜 앉아본 날이 손에 ㅁ꼽아요",
          "speaker_id": "1",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100008644.1.22",
          "form": "전... 하루도없었어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전... 하루도없었어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008644.1.23",
          "form": "망할 회사!",
          "original_form": "망할 회사!",
          "speaker_id": "2",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008644.1.24",
          "form": "와... 진짜 사람 많은 쪽에서 타시나봐요...",
          "original_form": "와... 진짜 사람 많은 쪽에서 타시나봐요...",
          "speaker_id": "1",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100008644.1.25",
          "form": "망할 회사...",
          "original_form": "망할 회사...",
          "speaker_id": "1",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100008644.1.26",
          "form": "또 점심먹고나면 졸음을 못참겠더라고요",
          "original_form": "또 점심먹고나면 졸음을 못참겠더라고요",
          "speaker_id": "1",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100008644.1.27",
          "form": "맞아요 흑흑... 콩나물국같아요...",
          "original_form": "맞아요 흑흑... 콩나물국같아요...",
          "speaker_id": "2",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008644.1.28",
          "form": "진짜 요즘 밥먹고 너무 졸려서",
          "original_form": "진짜 요즘 밥먹고 너무 졸려서",
          "speaker_id": "2",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008644.1.29",
          "form": "커피도 소용없는거있죠",
          "original_form": "커피도 소용없는거있죠",
          "speaker_id": "2",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008644.1.30",
          "form": "너무 충격",
          "original_form": "너무 충격",
          "speaker_id": "2",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008644.1.31",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008644.1.32",
          "form": "사실 저는 커피.. 마시면 더졸려서",
          "original_form": "사실 저는 커피.. 마시면 더졸려서",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008644.1.33",
          "form": "거의 의미가 없더라고요",
          "original_form": "거의 의미가 없더라고요",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008644.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 전 원래 커피 진짜 잘들었는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 전 원래 커피 진짜 잘들었는데",
          "speaker_id": "2",
