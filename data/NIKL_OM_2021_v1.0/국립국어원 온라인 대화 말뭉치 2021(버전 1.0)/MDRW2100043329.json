      "utterance": [
        {
          "id": "MDRW2100043329.1.1",
          "form": "밥을 못 먹으면 되나",
          "original_form": "밥을 못 먹으면 되나",
          "speaker_id": "1",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100043329.1.2",
          "form": "다 먹고 살자고 하는 건데",
          "original_form": "다 먹고 살자고 하는 건데",
          "speaker_id": "1",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100043329.1.3",
          "form": "배고픔에 짜증나잖아",
          "original_form": "배고픔에 짜증나잖아",
          "speaker_id": "1",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100043329.1.4",
          "form": "배고픈 거 제일 싫다",
          "original_form": "배고픈 거 제일 싫다",
          "speaker_id": "1",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100043329.1.5",
          "form": "배고픈 지도 몰랐어요 언니ㅋㅋㅋ",
          "original_form": "배고픈 지도 몰랐어요 언니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:46"
        },
        {
          "id": "MDRW2100043329.1.6",
          "form": "이기세를 몰아서 안 먹어야 되는데",
          "original_form": "이기세를 몰아서 안 먹어야 되는데",
          "speaker_id": "2",
          "time": "20210907 17:46"
        },
        {
          "id": "MDRW2100043329.1.7",
          "form": "어 대단한데",
          "original_form": "어 대단한데",
          "speaker_id": "1",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.8",
          "form": "얼마나 바빴으면",
          "original_form": "얼마나 바빴으면",
          "speaker_id": "1",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.9",
          "form": "지금 식욕 폭발",
          "original_form": "지금 식욕 폭발",
          "speaker_id": "2",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.10",
          "form": "지금 안 먹으면",
          "original_form": "지금 안 먹으면",
          "speaker_id": "1",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.11",
          "form": "더 폭식할 수 있다",
          "original_form": "더 폭식할 수 있다",
          "speaker_id": "1",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.12",
          "form": "바빠도 안 바빠도 돈은 똑같은데",
          "original_form": "바빠도 안 바빠도 돈은 똑같은데",
          "speaker_id": "2",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.13",
          "form": "빨리 먹어라",
          "original_form": "빨리 먹어라",
          "speaker_id": "1",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.14",
          "form": "안  바빴음 좋겠네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "안  바빴음 좋겠네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.15",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.16",
          "form": "커피 마시고 수다 떨 때가 좋았는데",
          "original_form": "커피 마시고 수다 떨 때가 좋았는데",
          "speaker_id": "1",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.17",
          "form": "요새 너무 바쁘네",
          "original_form": "요새 너무 바쁘네",
          "speaker_id": "1",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.18",
          "form": "계절 바뀔 때마다",
          "original_form": "계절 바뀔 때마다",
          "speaker_id": "1",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.19",
          "form": "그러니까요 언니ㅠ",
          "original_form": "그러니까요 언니ㅠ",
          "speaker_id": "2",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043329.1.20",
          "form": "예전으로 돌려줘요ㅋㅋㅋㅋ",
          "original_form": "예전으로 돌려줘요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100043329.1.21",
          "form": "지금 재난 지원금 때문에",
          "original_form": "지금 재난 지원금 때문에",
          "speaker_id": "2",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100043329.1.22",
          "form": "힘드네",
          "original_form": "힘드네",
          "speaker_id": "1",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100043329.1.23",
          "form": "더 바빠요",
          "original_form": "더 바빠요",
          "speaker_id": "2",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100043329.1.24",
          "form": "아 재난 지원금",
          "original_form": "아 재난 지원금",
          "speaker_id": "1",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100043329.1.25",
          "form": "이거 동네에서만 쓸 수 있잖아요",
          "original_form": "이거 동네에서만 쓸 수 있잖아요",
          "speaker_id": "2",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100043329.1.26",
          "form": "안 그래도 사람들",
          "original_form": "안 그래도 사람들",
          "speaker_id": "1",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100043329.1.27",
          "form": "뭐 사냐고 막 묻대",
          "original_form": "뭐 사냐고 막 묻대",
          "speaker_id": "1",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100043329.1.28",
          "form": "아 그거 동네에서만 쓸 수 있니??",
          "original_form": "아 그거 동네에서만 쓸 수 있니??",
          "speaker_id": "1",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100043329.1.29",
          "form": "달성에서만??",
          "original_form": "달성에서만??",
          "speaker_id": "1",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100043329.1.30",
          "form": "대형 마트는 안 되니까",
          "original_form": "대형 마트는 안 되니까",
          "speaker_id": "2",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100043329.1.31",
          "form": "아니요 대구 대구",
          "original_form": "아니요 대구 대구",
          "speaker_id": "2",
          "time": "20210907 17:48"
        }
      ]
