        {
          "id": "MDRW2100022709.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022709.1.2",
          "form": "아직이요!",
          "original_form": "아직이요!",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022709.1.3",
          "form": "메뉴는요?",
          "original_form": "메뉴는요?",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022709.1.4",
          "form": "다이어트도시락이요ㅎ",
          "original_form": "다이어트도시락이요ㅎ",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022709.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022709.1.6",
          "form": "만두얘정이ㅛ요",
          "original_form": "만두얘정이ㅛ요",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022709.1.7",
          "form": "맛있겠어요 만두만 드시나요",
          "original_form": "맛있겠어요 만두만 드시나요",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022709.1.8",
          "form": "일단은요",
          "original_form": "일단은요",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022709.1.9",
          "form": "저도 만두 땡기지만 참겠습니다...",
          "original_form": "저도 만두 땡기지만 참겠습니다...",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022709.1.10",
          "form": "군만두요",
          "original_form": "군만두요",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022709.1.11",
          "form": "ㅋㅋㅋ네에 군만두 맛있죠 짜장면이랑 먹어줘야하는데",
          "original_form": "ㅋㅋㅋ네에 군만두 맛있죠 짜장면이랑 먹어줘야하는데",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022709.1.12",
          "form": "아 굿이조",
          "original_form": "아 굿이조",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022709.1.13",
          "form": "대화하던 와중에 도시락 순삭했네요",
          "original_form": "대화하던 와중에 도시락 순삭했네요",
