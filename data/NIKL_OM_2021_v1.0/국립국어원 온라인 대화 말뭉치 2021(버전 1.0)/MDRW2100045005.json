      "utterance": [
        {
          "id": "MDRW2100045005.1.1",
          "form": "어제구매하신거잇나요?ㅋ",
          "original_form": "어제구매하신거잇나요?ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100045005.1.2",
          "form": "단백질 파우더요.",
          "original_form": "단백질 파우더요.",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045005.1.3",
          "form": "오 운동하시려구용?",
          "original_form": "오 운동하시려구용?",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045005.1.4",
          "form": "그건 아니고 엄마 드릴려구요.ㅋㅋㅋ",
          "original_form": "그건 아니고 엄마 드릴려구요.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045005.1.5",
          "form": "단백질 드신다 하셔서.^^",
          "original_form": "단백질 드신다 하셔서.^^",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045005.1.6",
          "form": "와 효녀효자싣ㄱᆞ",
          "original_form": "와 효녀효자싣ㄱᆞ",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045005.1.7",
          "form": "아니에요.ㅋㅋㅋ",
          "original_form": "아니에요.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045005.1.8",
          "form": "쿠폰같은거 마니알아보세요?",
          "original_form": "쿠폰같은거 마니알아보세요?",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045005.1.9",
          "form": "살 때 쿠폰 항상 적용하죠.ㅋㅋㅋ",
          "original_form": "살 때 쿠폰 항상 적용하죠.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045005.1.10",
          "form": "최저가보다 가끔 쿠폰적용이 싸더라구요ㅋ",
          "original_form": "최저가보다 가끔 쿠폰적용이 싸더라구요ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045005.1.11",
          "form": "십일절에 뭐살지 벌써 고민중입니당ㅋ",
          "original_form": "십일절에 뭐살지 벌써 고민중입니당ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045005.1.12",
          "form": "저는 라면 사야하는데",
          "original_form": "저는 라면 사야하는데",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045005.1.13",
          "form": "11번가에서 구매 많이 하시나요?",
          "original_form": "11번가에서 구매 많이 하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045005.1.14",
          "form": "매달행사하는 쇼핑몰에 맞춰서",
          "original_form": "매달행사하는 쇼핑몰에 맞춰서",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045005.1.15",
          "form": "기다렸다가 사는편이에요ㅋ",
          "original_form": "기다렸다가 사는편이에요ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045005.1.16",
          "form": "진라면 매운맛 vs 순한맛 선호하시는건요?",
          "original_form": "진라면 매운맛 vs 순한맛 선호하시는건요?",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045005.1.17",
          "form": "매운맛이요.",
          "original_form": "매운맛이요.",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045005.1.18",
          "form": "진라면 드시나요?",
          "original_form": "진라면 드시나요?",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045005.1.19",
          "form": "갑자기먹고싶어서요ㅋㅋㅋ",
          "original_form": "갑자기먹고싶어서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045005.1.20",
          "form": "근데 라면은 많이사두면 안된데요!",
          "original_form": "근데 라면은 많이사두면 안된데요!",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045005.1.21",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045005.1.22",
          "form": "은근 유통기한짧다던데요ㅋ",
          "original_form": "은근 유통기한짧다던데요ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045005.1.23",
          "form": "울 집엔 라면 킬러가 있어요",
          "original_form": "울 집엔 라면 킬러가 있어요",
