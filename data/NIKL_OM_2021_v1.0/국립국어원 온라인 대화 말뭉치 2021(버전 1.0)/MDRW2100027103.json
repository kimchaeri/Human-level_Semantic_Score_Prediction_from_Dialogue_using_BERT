      "utterance": [
        {
          "id": "MDRW2100027103.1.1",
          "form": "음식은 해서 먹는것보다",
          "original_form": "음식은 해서 먹는것보다",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100027103.1.2",
          "form": "사먹는편입니다",
          "original_form": "사먹는편입니다",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100027103.1.3",
          "form": "저도 좋아합니다",
          "original_form": "저도 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027103.1.4",
          "form": "치즈케익이 맛있어요",
          "original_form": "치즈케익이 맛있어요",
          "speaker_id": "1",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027103.1.5",
          "form": "요즘 커피 조각케익 세트 좋더라구요",
          "original_form": "요즘 커피 조각케익 세트 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027103.1.6",
          "form": "생크림케익도 맛있죠",
          "original_form": "생크림케익도 맛있죠",
          "speaker_id": "1",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027103.1.7",
          "form": "베이글이나 번도 팔고",
          "original_form": "베이글이나 번도 팔고",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027103.1.8",
          "form": "샌드위치도 맛있고",
          "original_form": "샌드위치도 맛있고",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027103.1.9",
          "form": "베이글에는 크림치즈죠",
          "original_form": "베이글에는 크림치즈죠",
          "speaker_id": "1",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027103.1.10",
          "form": "번은 어디서 사나요",
          "original_form": "번은 어디서 사나요",
          "speaker_id": "1",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027103.1.11",
          "form": "이삭 드셔보셨나요",
          "original_form": "이삭 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027103.1.12",
          "form": "소스가 일품",
          "original_form": "소스가 일품",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027103.1.13",
          "form": "이삭 좋아해요",
          "original_form": "이삭 좋아해요",
          "speaker_id": "1",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027103.1.14",
          "form": "서브웨이도",
          "original_form": "서브웨이도",
          "speaker_id": "1",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027103.1.15",
          "form": "이디야에서 커피먹을때",
          "original_form": "이디야에서 커피먹을때",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100027103.1.16",
          "form": "핫도그도 생각나네요",
          "original_form": "핫도그도 생각나네요",
          "speaker_id": "1",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100027103.1.17",
          "form": "번 같이사서 먹어이",
          "original_form": "번 같이사서 먹어이",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100027103.1.18",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100027103.1.19",
          "form": "서브웨이 존맛이죠",
          "original_form": "서브웨이 존맛이죠",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100027103.1.20",
          "form": "괴물이 명랑핫도그 사주었지요",
          "original_form": "괴물이 명랑핫도그 사주었지요",
          "speaker_id": "1",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100027103.1.21",
          "form": "치즈핫도그 좋아해요",
          "original_form": "치즈핫도그 좋아해요",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100027103.1.22",
          "form": "서브웨이 매장이 없어요",
          "original_form": "서브웨이 매장이 없어요",
          "speaker_id": "1",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100027103.1.23",
          "form": "여긴 있어요",
          "original_form": "여긴 있어요",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100027103.1.24",
          "form": "부산오시면 사드릴게요",
          "original_form": "부산오시면 사드릴게요",
          "speaker_id": "2",
          "time": "20210902 09:36"
        }
      ]
