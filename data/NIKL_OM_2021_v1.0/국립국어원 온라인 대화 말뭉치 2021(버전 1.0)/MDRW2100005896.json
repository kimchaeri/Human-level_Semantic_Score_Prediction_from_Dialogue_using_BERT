      "utterance": [
        {
          "id": "MDRW2100005896.1.1",
          "form": "이제 앞으로 위드코로나가 되면 무엇을 가장 하고 싶나요?",
          "original_form": "이제 앞으로 위드코로나가 되면 무엇을 가장 하고 싶나요?",
          "speaker_id": "1",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100005896.1.2",
          "form": "해외여행이요^^",
          "original_form": "해외여행이요^^",
          "speaker_id": "2",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100005896.1.3",
          "form": "그렇네요",
          "original_form": "그렇네요",
          "speaker_id": "1",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100005896.1.4",
          "form": "지금도 갈수는 있지만 제약이 많지요",
          "original_form": "지금도 갈수는 있지만 제약이 많지요",
          "speaker_id": "1",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100005896.1.5",
          "form": "간다면 어디 가고 싶나요?",
          "original_form": "간다면 어디 가고 싶나요?",
          "speaker_id": "1",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100005896.1.6",
          "form": "독일이나 이탈리아요",
          "original_form": "독일이나 이탈리아요",
          "speaker_id": "2",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100005896.1.7",
          "form": "친구들이 있어서 보고싶네요",
          "original_form": "친구들이 있어서 보고싶네요",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100005896.1.8",
          "form": "못만난지 오래됐겠군요",
          "original_form": "못만난지 오래됐겠군요",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005896.1.9",
          "form": "그립겠어요",
          "original_form": "그립겠어요",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005896.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005896.1.11",
          "form": "접종은 하셨죠?",
          "original_form": "접종은 하셨죠?",
          "speaker_id": "2",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005896.1.12",
          "form": "완료자 됐습니다",
          "original_form": "완료자 됐습니다",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100005896.1.13",
          "form": "저는 내일 맞아요",
          "original_form": "저는 내일 맞아요",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100005896.1.14",
          "form": "접종끝나면 좀더 마음편해서 위드코로나 할수있을듯요",
          "original_form": "접종끝나면 좀더 마음편해서 위드코로나 할수있을듯요",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100005896.1.15",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100005896.1.16",
          "form": "부부가 다 맞고나니 마음은 편하네요",
          "original_form": "부부가 다 맞고나니 마음은 편하네요",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100005896.1.17",
          "form": "그래도 마스크는 계속 써야할 것 같아요",
          "original_form": "그래도 마스크는 계속 써야할 것 같아요",
          "speaker_id": "2",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100005896.1.18",
          "form": "그래야지요",
          "original_form": "그래야지요",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100005896.1.19",
          "form": "돌파감염도 있고 변이도 있고",
          "original_form": "돌파감염도 있고 변이도 있고",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100005896.1.20",
          "form": "그래도 일단 백신 맞았다는 생각에 좀 안심이 되네요",
          "original_form": "그래도 일단 백신 맞았다는 생각에 좀 안심이 되네요",
          "speaker_id": "1",
          "time": "20210907 16:08"
        },
        {
          "id": "MDRW2100005896.1.21",
          "form": "애들이 고생이예요",
          "original_form": "애들이 고생이예요",
          "speaker_id": "2",
          "time": "20210907 16:08"
        },
        {
          "id": "MDRW2100005896.1.22",
          "form": "부작용은 없으셨나요",
          "original_form": "부작용은 없으셨나요",
          "speaker_id": "2",
          "time": "20210907 16:08"
        },
        {
          "id": "MDRW2100005896.1.23",
          "form": "다행히도 우리부부는 둘다 괜찮았습니다",
          "original_form": "다행히도 우리부부는 둘다 괜찮았습니다",
          "speaker_id": "1",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100005896.1.24",
          "form": "맞기 전엔 좀 걱정됐었지요",
          "original_form": "맞기 전엔 좀 걱정됐었지요",
          "speaker_id": "1",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100005896.1.25",
          "form": "둘다 기저기환자라서",
          "original_form": "둘다 기저기환자라서",
          "speaker_id": "1",
          "time": "20210907 16:10"
        },
        {
          "id": "MDRW2100005896.1.26",
          "form": "그런데 무사히 넘어가서 다행입니다",
          "original_form": "그런데 무사히 넘어가서 다행입니다",
          "speaker_id": "1",
          "time": "20210907 16:10"
        }
      ]
    }
