      "utterance": [
        {
          "id": "MDRW2100013999.1.1",
          "form": "너는 캔디바 머가 좋아??",
          "original_form": "너는 캔디바 머가 좋아??",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013999.1.2",
          "form": "안에 바닐라아이스크림",
          "original_form": "안에 바닐라아이스크림",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013999.1.3",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013999.1.4",
          "form": "그거만??",
          "original_form": "그거만??",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013999.1.5",
          "form": "어 난 그거만",
          "original_form": "어 난 그거만",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013999.1.6",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013999.1.7",
          "form": "겉에거는 너무 딱딱한",
          "original_form": "겉에거는 너무 딱딱한",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013999.1.8",
          "form": "응으",
          "original_form": "응으",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013999.1.9",
          "form": "아이스크림이라서",
          "original_form": "아이스크림이라서",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013999.1.10",
          "form": "싫어",
          "original_form": "싫어",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013999.1.11",
          "form": "아그래??",
          "original_form": "아그래??",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013999.1.12",
          "form": "그거 소다맛이잔아",
          "original_form": "그거 소다맛이잔아",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013999.1.13",
          "form": "소다맛 얼음붙어있는거같잖아",
          "original_form": "소다맛 얼음붙어있는거같잖아",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013999.1.14",
          "form": "뽕따맛인데",
          "original_form": "뽕따맛인데",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013999.1.15",
          "form": "그래서 맛잇지않아??",
          "original_form": "그래서 맛잇지않아??",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013999.1.16",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013999.1.17",
          "form": "칫..글쿤",
          "original_form": "칫..글쿤",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013999.1.18",
          "form": "나는 바닐라아이스크림",
          "original_form": "나는 바닐라아이스크림",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013999.1.19",
          "form": "그 캔디바만의 바닐라아이스크림",
          "original_form": "그 캔디바만의 바닐라아이스크림",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013999.1.20",
          "form": "그래 그럼 나중에 내가 겉에거만 다 먹고 줄게",
          "original_form": "그래 그럼 나중에 내가 겉에거만 다 먹고 줄게",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013999.1.21",
          "form": "그게 맛있어",
          "original_form": "그게 맛있어",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013999.1.22",
