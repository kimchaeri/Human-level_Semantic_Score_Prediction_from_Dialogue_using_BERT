        },
        {
          "id": "MDRW2100004947.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 19:30"
        },
        {
          "id": "MDRW2100004947.1.3",
          "form": "최근에 뭐 구매하신 적 있으신가요?",
          "original_form": "최근에 뭐 구매하신 적 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004947.1.4",
          "form": "최근에 물욕이 떨어져서 뭘 사진 않은거같아요..",
          "original_form": "최근에 물욕이 떨어져서 뭘 사진 않은거같아요..",
          "speaker_id": "2",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004947.1.5",
          "form": "뭐 사신거 있으세요?",
          "original_form": "뭐 사신거 있으세요?",
          "speaker_id": "2",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004947.1.6",
          "form": "아 전 가을 옷 샀어요!",
          "original_form": "아 전 가을 옷 샀어요!",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004947.1.7",
          "form": "근데 싼 거 여러벌 사고 그랬었는데",
          "original_form": "근데 싼 거 여러벌 사고 그랬었는데",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004947.1.8",
          "form": "이젠 좋은 옷으로 조금씩 모으려구요",
          "original_form": "이젠 좋은 옷으로 조금씩 모으려구요",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004947.1.9",
          "form": "앗 그러고보니 벌써 가을옷 살때가 됐네요",
          "original_form": "앗 그러고보니 벌써 가을옷 살때가 됐네요",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004947.1.10",
          "form": "좋은옷 사는거 공감해요 싼옷은 싼만큼 금방 버리게되서..",
          "original_form": "좋은옷 사는거 공감해요 싼옷은 싼만큼 금방 버리게되서..",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004947.1.11",
          "form": "상의사셨나요??",
          "original_form": "상의사셨나요??",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004947.1.12",
          "form": "네네! 저는 셔츠를 좋아해서 셔츠만 두벌 샀어요",
          "original_form": "네네! 저는 셔츠를 좋아해서 셔츠만 두벌 샀어요",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004947.1.13",
          "form": "저두 셔츠 좋아해요! 무채색으로만 사는편인데",
          "original_form": "저두 셔츠 좋아해요! 무채색으로만 사는편인데",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004947.1.14",
          "form": "올해는 다른 색도 좀 사볼까봐요",
          "original_form": "올해는 다른 색도 좀 사볼까봐요",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004947.1.15",
          "form": "저도 검정 하양만 입어요!!",
          "original_form": "저도 검정 하양만 입어요!!",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004947.1.16",
          "form": "옷장 정리를 해보니까 검은티가 정말 많더라고요ㅋㅋㅋ",
          "original_form": "옷장 정리를 해보니까 검은티가 정말 많더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004947.1.17",
          "form": "다른 색도 도전을 해봅시다!!",
          "original_form": "다른 색도 도전을 해봅시다!!",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004947.1.18",
          "form": "올해는 채도낮은 보라색 셔츠 도전 가보겠습니다!",
          "original_form": "올해는 채도낮은 보라색 셔츠 도전 가보겠습니다!",
          "speaker_id": "2",
          "time": "20210821 19:52"
        },
        {
          "id": "MDRW2100004947.1.19",
          "form": "항상 고민만 하다가 안샀어요",
          "original_form": "항상 고민만 하다가 안샀어요",
          "speaker_id": "2",
          "time": "20210821 19:52"
        },
        {
          "id": "MDRW2100004947.1.20",
          "form": "고민만 하는 쇼핑템 있으신가요",
          "original_form": "고민만 하는 쇼핑템 있으신가요",
          "speaker_id": "2",
          "time": "20210821 19:52"
        },
        {
          "id": "MDRW2100004947.1.21",
          "form": "보라색!!!!!!! 너무 좋은 것 같아요",
          "original_form": "보라색!!!!!!! 너무 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100004947.1.22",
          "form": "저는 구두랑 셋업이요",
          "original_form": "저는 구두랑 셋업이요",
          "speaker_id": "1",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100004947.1.23",
          "form": "뭔가 갖춰 입어야할 때 필요할 것 같은데 학생 신분이라 어느정도 가격대에 어느 정도가 좋을지 항상 고민만 해요",
          "original_form": "뭔가 갖춰 입어야할 때 필요할 것 같은데 학생 신분이라 어느정도 가격대에 어느 정도가 좋을지 항상 고민만 해요",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004947.1.24",
          "form": "근데 그건 사회인되어서도 다들 고민하는거같아요 ㅋㅋㅋㅋ",
          "original_form": "근데 그건 사회인되어서도 다들 고민하는거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100004947.1.25",
          "form": "봐둔게 있으신가요??",
          "original_form": "봐둔게 있으신가요??",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100004947.1.26",
          "form": "아뇨 딱히 봐둔 건 없는데 볼 때마다 살까 말까하다가 돈 더 모아서 더 좋은 거 사자고 하면서 미루는 것 같아요ㅋㅋㅋ",
          "original_form": "아뇨 딱히 봐둔 건 없는데 볼 때마다 살까 말까하다가 돈 더 모아서 더 좋은 거 사자고 하면서 미루는 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004947.1.27",
          "form": "그럼 그냥 필요할 때 사면 좋을거같아요! ㅋㅋㅋ",
          "original_form": "그럼 그냥 필요할 때 사면 좋을거같아요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:07"
        },
        {
          "id": "MDRW2100004947.1.28",
          "form": "막상 사둬두 별일없으면 안 입게되더라구요!",
          "original_form": "막상 사둬두 별일없으면 안 입게되더라구요!",
          "speaker_id": "2",
          "time": "20210821 20:07"
        }
