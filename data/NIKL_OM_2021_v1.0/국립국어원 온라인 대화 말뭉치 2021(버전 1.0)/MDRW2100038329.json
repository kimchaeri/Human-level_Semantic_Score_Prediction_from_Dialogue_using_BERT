      "utterance": [
        {
          "id": "MDRW2100038329.1.1",
          "form": "개강해버렸습니다...",
          "original_form": "개강해버렸습니다...",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038329.1.2",
          "form": "와우 학생이시군요",
          "original_form": "와우 학생이시군요",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038329.1.3",
          "form": "네ㅠ 어제 개강하서 정신이 없어요",
          "original_form": "네ㅠ 어제 개강하서 정신이 없어요",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038329.1.4",
          "form": "비대면이 아닌가봐요?",
          "original_form": "비대면이 아닌가봐요?",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038329.1.5",
          "form": "아직은 비대면이지만",
          "original_form": "아직은 비대면이지만",
          "speaker_id": "2",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100038329.1.6",
          "form": "줌으로 전부 하다보니ㅠ",
          "original_form": "줌으로 전부 하다보니ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100038329.1.7",
          "form": "아 완전 학비아까울듯",
          "original_form": "아 완전 학비아까울듯",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100038329.1.8",
          "form": "맞아요... 등록금은 비싼데 하는게 없어요",
          "original_form": "맞아요... 등록금은 비싼데 하는게 없어요",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100038329.1.9",
          "form": "완전 추억도 없고 ㅠ",
          "original_form": "완전 추억도 없고 ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100038329.1.10",
          "form": "공부도 안되고",
          "original_form": "공부도 안되고",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100038329.1.11",
          "form": "어쩔 수 없는거 같아요",
          "original_form": "어쩔 수 없는거 같아요",
          "speaker_id": "2",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100038329.1.12",
          "form": "코로나가 빨리 끝나기를 바라는수밖에...",
          "original_form": "코로나가 빨리 끝나기를 바라는수밖에...",
          "speaker_id": "2",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100038329.1.13",
          "form": "몇년은 가니깐 ㅠ",
          "original_form": "몇년은 가니깐 ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100038329.1.14",
          "form": "내년엔 끝나겠죠?",
          "original_form": "내년엔 끝나겠죠?",
          "speaker_id": "2",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100038329.1.15",
          "form": "아닐거같아요 계속 나오니깐 ㅜ",
          "original_form": "아닐거같아요 계속 나오니깐 ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100038329.1.16",
          "form": "백신 접종은 언제 다 이루어질런지",
          "original_form": "백신 접종은 언제 다 이루어질런지",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100038329.1.17",
          "form": "학교 생활이 그립겠어요",
          "original_form": "학교 생활이 그립겠어요",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100038329.1.18",
          "form": "졸업한 지 얼마 안됐지만",
          "original_form": "졸업한 지 얼마 안됐지만",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100038329.1.19",
          "form": "그립긴해요",
          "original_form": "그립긴해요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        }
      ]
