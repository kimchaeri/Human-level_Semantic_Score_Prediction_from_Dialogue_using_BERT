          "time": "20210819 15:29"
        },
        {
          "id": "MDRW2100000353.1.2",
          "form": "그 맘 잘 알지",
          "original_form": "그 맘 잘 알지",
          "speaker_id": "2",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000353.1.3",
          "form": "그래도 이틀만 참으면 주말이잖아",
