      "utterance": [
        {
          "id": "MDRW2100047360.1.1",
          "form": "하이볼  마실 때 주로 뭘 곁들이세요?",
          "original_form": "하이볼  마실 때 주로 뭘 곁들이세요?",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100047360.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047360.1.3",
          "form": "저는 주로",
          "original_form": "저는 주로",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047360.1.4",
          "form": "탕류 좋아해요ㅋㅋㅋ안주로",
          "original_form": "탕류 좋아해요ㅋㅋㅋ안주로",
          "speaker_id": "2",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100047360.1.5",
          "form": "국물요리요!",
          "original_form": "국물요리요!",
          "speaker_id": "2",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100047360.1.6",
          "form": "오오 예상 외네요",
          "original_form": "오오 예상 외네요",
          "speaker_id": "1",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047360.1.7",
          "form": "국물과 하이볼도 잘 어울리나봐요",
          "original_form": "국물과 하이볼도 잘 어울리나봐요",
          "speaker_id": "1",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047360.1.8",
          "form": "ㅋㅋㅋ네네 하이볼이",
          "original_form": "ㅋㅋㅋ네네 하이볼이",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100047360.1.9",
          "form": "달달하니 저같이 소주의 쓴맛을싫어하는사람에겐 최고죠",
          "original_form": "달달하니 저같이 소주의 쓴맛을싫어하는사람에겐 최고죠",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100047360.1.10",
          "form": "진짜 달고 맛있어요",
          "original_form": "진짜 달고 맛있어요",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100047360.1.11",
          "form": "ㅎㅎ 저 종종 하이볼가든 가서 낮술했는데",
          "original_form": "ㅎㅎ 저 종종 하이볼가든 가서 낮술했는데",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100047360.1.12",
          "form": "딱 하나 안주가 캐쥬얼한 것밖에 없어 아쉬웠어요",
          "original_form": "딱 하나 안주가 캐쥬얼한 것밖에 없어 아쉬웠어요",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100047360.1.13",
          "form": "거기는 왜 국물요리가 없었지ㅜ",
          "original_form": "거기는 왜 국물요리가 없었지ㅜ",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100047360.1.14",
          "form": "이런 아쉬운가게네요ㅋㅋㅋㅋ",
          "original_form": "이런 아쉬운가게네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100047360.1.15",
          "form": "저는 진짜 어디가서뭐안주시키려면",
          "original_form": "저는 진짜 어디가서뭐안주시키려면",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100047360.1.16",
          "form": "탕 부터들어오더라구요",
          "original_form": "탕 부터들어오더라구요",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100047360.1.17",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100047360.1.18",
          "form": "저도 국물파예요!",
          "original_form": "저도 국물파예요!",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100047360.1.19",
          "form": "회 암만 배불리 먹어도 매운탕 필수고",
          "original_form": "회 암만 배불리 먹어도 매운탕 필수고",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100047360.1.20",
          "form": "하이볼 파는 곳 잘 아는 곳 있으신가요?",
          "original_form": "하이볼 파는 곳 잘 아는 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100047360.1.21",
          "form": "탕요리 좋은 곳이요ㅎ",
          "original_form": "탕요리 좋은 곳이요ㅎ",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100047360.1.22",
          "form": "ㅎㅎㅎ 사실 잘아는곳은없어요 단골은없고\r\n그냥 가게가서 하이볼팔면",
          "original_form": "ㅎㅎㅎ 사실 잘아는곳은없어요 단골은없고\r\n그냥 가게가서 하이볼팔면",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100047360.1.23",
          "form": "무조건시키는편?! 소주맥주대신에ㅋㅋㅋ",
          "original_form": "무조건시키는편?! 소주맥주대신에ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100047360.1.24",
          "form": "집에서도 가끔 말아먹고요",
          "original_form": "집에서도 가끔 말아먹고요",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100047360.1.25",
          "form": "오오 직접 해드시는구나",
          "original_form": "오오 직접 해드시는구나",
          "speaker_id": "1",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100047360.1.26",
          "form": "집에 재료가 있나봐요",
          "original_form": "집에 재료가 있나봐요",
          "speaker_id": "1",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100047360.1.27",
          "form": "ㅋㅋㅋ 그건아니고 편의점에도 가벼운위스키들팔잖아요?",
          "original_form": "ㅋㅋㅋ 그건아니고 편의점에도 가벼운위스키들팔잖아요?",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100047360.1.28",
          "form": "그거랑 토닉워터나 탄산수 레몬등등 넣으면",
          "original_form": "그거랑 토닉워터나 탄산수 레몬등등 넣으면",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100047360.1.29",
          "form": "뚝딱이랍니다ㅋㅋㅋ",
          "original_form": "뚝딱이랍니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100047360.1.30",
          "form": "ㅎㅎ 생각보다 간단하네요",
          "original_form": "ㅎㅎ 생각보다 간단하네요",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100047360.1.31",
          "form": "마실 생각만 했지 만들 생각은 못해봤어요",
          "original_form": "마실 생각만 했지 만들 생각은 못해봤어요",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100047360.1.32",
          "form": "위스키와 탄산수는 있으니 토닉워터만 구비하면 되겠네요",
          "original_form": "위스키와 탄산수는 있으니 토닉워터만 구비하면 되겠네요",
          "speaker_id": "1",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100047360.1.33",
          "form": "혼술도 즐기시나요?",
          "original_form": "혼술도 즐기시나요?",
          "speaker_id": "1",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100047360.1.34",
          "form": "레몬즙 필수랍니다ㅋㅋㅋ 진저에일\r\n+얼음컵도요ㅋㅋㅋ",
          "original_form": "레몬즙 필수랍니다ㅋㅋㅋ 진저에일\r\n+얼음컵도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100047360.1.35",
          "form": "술은 혼술이죠",
          "original_form": "술은 혼술이죠",
          "speaker_id": "2",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100047360.1.36",
          "form": "ㅎㅎ저도 혼술 좋아해요",
          "original_form": "ㅎㅎ저도 혼술 좋아해요",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100047360.1.37",
          "form": "다음날 출근이 부담스러우면 탄산수 섞어 마셔요",
          "original_form": "다음날 출근이 부담스러우면 탄산수 섞어 마셔요",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100047360.1.38",
          "form": "어차피 술맛알못이라ㅎ",
          "original_form": "어차피 술맛알못이라ㅎ",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100047360.1.39",
          "form": "탄산수가 참 만능이네요ㅋㅋㅋ",
          "original_form": "탄산수가 참 만능이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:21"
        }
      ]
