        {
          "id": "MDRW2100040810.1.1",
          "form": "나 너계정으로 박준철 행정법총론받아버림",
          "original_form": "나 너계정으로 박준철 행정법총론받아버림",
          "speaker_id": "1",
          "time": "20210905 08:05"
        },
        {
          "id": "MDRW2100040810.1.2",
          "form": "ㅋㅋㅋㅋㅋ공단기 틀고 그래 구문해보자하눈데",
          "original_form": "ㅋㅋㅋㅋㅋ공단기 틀고 그래 구문해보자하눈데",
          "speaker_id": "1",
          "time": "20210905 08:05"
        },
        {
          "id": "MDRW2100040810.1.3",
          "form": "옆에 무료배포어쩌고잇길래 눌럿다가 아직 수량이있길래 눌럿더니 됏데 ㅋㅋㅋ근데 아 ㅋㅋㅋㅋㅋ",
          "original_form": "옆에 무료배포어쩌고잇길래 눌럿다가 아직 수량이있길래 눌럿더니 됏데 ㅋㅋㅋ근데 아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100040810.1.4",
          "form": "로그인은 너껄로하고 ㅋㅋㅋ",
          "original_form": "로그인은 너껄로하고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100040810.1.5",
          "form": "ㅋㅋ기술직인데 내껄로 받아져?",
          "original_form": "ㅋㅋ기술직인데 내껄로 받아져?",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100040810.1.6",
          "form": "그냥무려배포라그런가 ㅋㅋㅋ",
          "original_form": "그냥무려배포라그런가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:11"
        },
        {
          "id": "MDRW2100040810.1.7",
          "form": "무튼 5만보에서 아직 9쳔몇보남앗더라궁 ㅋㅋ",
          "original_form": "무튼 5만보에서 아직 9쳔몇보남앗더라궁 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:11"
        },
        {
          "id": "MDRW2100040810.1.8",
          "form": "해피 쿨라타나왓다 ㅋㅋ주사위찍는거 1번",
          "original_form": "해피 쿨라타나왓다 ㅋㅋ주사위찍는거 1번",
          "speaker_id": "1",
          "time": "20210905 08:16"
        },
        {
          "id": "MDRW2100040810.1.9",
          "form": "ㅋㅋㅋ난 쓸데없는 겜 아이템",
          "original_form": "ㅋㅋㅋ난 쓸데없는 겜 아이템",
          "speaker_id": "2",
          "time": "20210905 08:27"
        },
        {
          "id": "MDRW2100040810.1.10",
          "form": "에라잇 모찌나와라 !!",
          "original_form": "에라잇 모찌나와라 !!",
          "speaker_id": "1",
          "time": "20210905 08:27"
        },
        {
          "id": "MDRW2100040810.1.11",
          "form": "내일은 !!",
          "original_form": "내일은 !!",
          "speaker_id": "1",
          "time": "20210905 08:27"
        },
        {
          "id": "MDRW2100040810.1.12",
          "form": "확줄였는지",
          "original_form": "확줄였는지",
          "speaker_id": "1",
          "time": "20210905 08:28"
        },
        {
          "id": "MDRW2100040810.1.13",
          "form": "12시넘어서 당타타임 안터지더라",
          "original_form": "12시넘어서 당타타임 안터지더라",
          "speaker_id": "1",
          "time": "20210905 08:28"
        },
        {
          "id": "MDRW2100040810.1.14",
          "form": "나 선풍기틈 ㅋㅋㅋㅋ",
          "original_form": "나 선풍기틈 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:28"
        },
        {
          "id": "MDRW2100040810.1.15",
          "form": "헐ㅋㅋㅋ난 아직 전기장판밑에 굽는 중",
          "original_form": "헐ㅋㅋㅋ난 아직 전기장판밑에 굽는 중",
          "speaker_id": "2",
          "time": "20210905 08:28"
        },
        {
          "id": "MDRW2100040810.1.16",
          "form": "나 가슴팍에 땀나 ㅡㅡ",
          "original_form": "나 가슴팍에 땀나 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210905 08:29"
        },
        {
          "id": "MDRW2100040810.1.17",
          "form": "진짜 따아때문인가 ㅋㅋ",
          "original_form": "진짜 따아때문인가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:29"
        },
        {
          "id": "MDRW2100040810.1.18",
          "form": "비염약두 먹고싶어서 요거트도 머금 빈속은 좀 그렇겟지?",
          "original_form": "비염약두 먹고싶어서 요거트도 머금 빈속은 좀 그렇겟지?",
          "speaker_id": "1",
          "time": "20210905 08:30"
        },
        {
          "id": "MDRW2100040810.1.19",
          "form": "아침드시고 공부하세요!",
          "original_form": "아침드시고 공부하세요!",
          "speaker_id": "1",
          "time": "20210905 08:30"
        },
        {
          "id": "MDRW2100040810.1.20",
          "form": "그런가 ㅋㅋㅋ",
          "original_form": "그런가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:30"
        },
        {
          "id": "MDRW2100040810.1.21",
          "form": "전기장판은 방에잇나?",
          "original_form": "전기장판은 방에잇나?",
