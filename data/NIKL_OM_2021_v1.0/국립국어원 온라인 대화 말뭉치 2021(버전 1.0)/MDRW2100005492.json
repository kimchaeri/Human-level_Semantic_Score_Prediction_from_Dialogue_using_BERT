      "utterance": [
        {
          "id": "MDRW2100005492.1.1",
          "form": "말하려고 했는데!!! 까먹었어요ㅠ",
          "original_form": "말하려고 했는데!!! 까먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005492.1.2",
          "form": "치킨킬러...였었나",
          "original_form": "치킨킬러...였었나",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005492.1.3",
          "form": "넵.ㅋㅋㅋ",
          "original_form": "넵.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005492.1.4",
          "form": "주 2회는 치킨 먹어요.ㅋㅋㅋㅋ",
          "original_form": "주 2회는 치킨 먹어요.ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005492.1.5",
          "form": "와 많이 드시네요ㅋㅋㅋㅈ",
          "original_form": "와 많이 드시네요ㅋㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005492.1.6",
          "form": "저도 적어도 주1회는 먹는 것 같아요",
          "original_form": "저도 적어도 주1회는 먹는 것 같아요",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005492.1.7",
          "form": "ㅋㅋㅋ 어디까 주로 시켜드세요??",
          "original_form": "ㅋㅋㅋ 어디까 주로 시켜드세요??",
          "speaker_id": "2",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005492.1.8",
          "form": "매번 주문할 때마다 다른 것 같아요",
          "original_form": "매번 주문할 때마다 다른 것 같아요",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005492.1.9",
          "form": "다 괜찮아서...ㅎㅎ",
          "original_form": "다 괜찮아서...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005492.1.10",
          "form": "저는 bbq 황금 올리브요",
          "original_form": "저는 bbq 황금 올리브요",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005492.1.11",
          "form": "이거 드셔보셨어요?",
          "original_form": "이거 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005492.1.12",
          "form": "네네! 황올 맛있죠ㅠㅠ",
          "original_form": "네네! 황올 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005492.1.13",
          "form": "후라이드는 진짜 이게 원탑인거 같아요",
          "original_form": "후라이드는 진짜 이게 원탑인거 같아요",
          "speaker_id": "2",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005492.1.14",
          "form": "맞아요 황올만한 후라이드는 없죠",
          "original_form": "맞아요 황올만한 후라이드는 없죠",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005492.1.15",
          "form": "가격이 비싸서ㅜㅜㅜㅜ 그렇지...",
          "original_form": "가격이 비싸서ㅜㅜㅜㅜ 그렇지...",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005492.1.16",
          "form": "하.. 그죠...",
          "original_form": "하.. 그죠...",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005492.1.17",
          "form": "닭 한마리가 이렇게 비쌀 이유가 있냐고요.ㅠㅠ",
          "original_form": "닭 한마리가 이렇게 비쌀 이유가 있냐고요.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005492.1.18",
          "form": "그러니까요ㅠ 치킨값이 전체적으로 너무 올랐어요",
          "original_form": "그러니까요ㅠ 치킨값이 전체적으로 너무 올랐어요",
          "speaker_id": "1",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005492.1.19",
          "form": "후.. 한마리에 만원이면 진짜 반찬 하나로 매일 먹을텐데..ㅠㅠ",
          "original_form": "후.. 한마리에 만원이면 진짜 반찬 하나로 매일 먹을텐데..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005492.1.20",
          "form": "그러니까요ㅠ",
