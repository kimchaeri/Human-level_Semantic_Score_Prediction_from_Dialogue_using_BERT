      "utterance": [
        {
          "id": "MDRW2100035175.1.1",
          "form": "오늘 뭐 드신거 있나요?",
          "original_form": "오늘 뭐 드신거 있나요?",
          "speaker_id": "1",
          "time": "20210829 10:22"
        },
        {
          "id": "MDRW2100035175.1.2",
          "form": "밥 먹고 복숭아 먹고 있어요",
          "original_form": "밥 먹고 복숭아 먹고 있어요",
          "speaker_id": "2",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100035175.1.3",
          "form": "아침 챙겨 드셨나요?",
          "original_form": "아침 챙겨 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100035175.1.4",
          "form": "아침은 안먹고요 아점먹어요",
          "original_form": "아침은 안먹고요 아점먹어요",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100035175.1.5",
          "form": "아점 뭐 드세요?",
          "original_form": "아점 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100035175.1.6",
          "form": "중국집 볶음밥 먹을 예정이에요",
          "original_form": "중국집 볶음밥 먹을 예정이에요",
          "speaker_id": "1",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100035175.1.7",
          "form": "맛있겠네요!",
          "original_form": "맛있겠네요!",
          "speaker_id": "2",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100035175.1.8",
          "form": "사이드 메뉴도 있나요?",
          "original_form": "사이드 메뉴도 있나요?",
          "speaker_id": "2",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100035175.1.9",
          "form": "양이 많아서 사이드는 안시켰어요ㅎㅎ",
          "original_form": "양이 많아서 사이드는 안시켰어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035175.1.10",
          "form": "name2님은 점심 드셧나요?",
          "original_form": "&name2&님은 점심 드셧나요?",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035175.1.11",
          "form": "저는 국밥 먹고싶어서 포장하러 가려구요 ㅎㅎ",
          "original_form": "저는 국밥 먹고싶어서 포장하러 가려구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100035175.1.12",
          "form": "와 무슨국밥이요?",
          "original_form": "와 무슨국밥이요?",
          "speaker_id": "1",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100035175.1.13",
          "form": "돼지국밥이요!",
          "original_form": "돼지국밥이요!",
          "speaker_id": "2",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100035175.1.14",
          "form": "날이 흐릿흐릿해서 돼지국밥이 먹고싶네요 ㅎㅎ",
          "original_form": "날이 흐릿흐릿해서 돼지국밥이 먹고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100035175.1.15",
          "form": "돼지국밥 좋죠ㅎ name2님은 외식 자주 하시나요?",
          "original_form": "돼지국밥 좋죠ㅎ &name2&님은 외식 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100035175.1.16",
          "form": "코로나 전에는 자주했는데, 요즘은 마스크 벗고 있는게 불안해서 주로 포장해 와요ㅠㅠ",
          "original_form": "코로나 전에는 자주했는데, 요즘은 마스크 벗고 있는게 불안해서 주로 포장해 와요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035175.1.17",
          "form": "맛집 자주 다니세요?",
          "original_form": "맛집 자주 다니세요?",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035175.1.18",
          "form": "저도 포장이나 배달 자주 해먹어요ㅋㅋ 요즘은 마켓컬리도 많이 쓰고요",
          "original_form": "저도 포장이나 배달 자주 해먹어요ㅋㅋ 요즘은 마켓컬리도 많이 쓰고요",
          "speaker_id": "1",
          "time": "20210829 14:23"
        }
      ]
