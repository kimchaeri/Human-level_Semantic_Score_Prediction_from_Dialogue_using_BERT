        },
        {
          "id": "MDRW2100039546.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100039546.1.3",
          "form": "건강이 최고죠!! 운동 하시는 거 있으신가요??",
          "original_form": "건강이 최고죠!! 운동 하시는 거 있으신가요??",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100039546.1.4",
          "form": "저 유튜브로 홈트해요!!",
          "original_form": "저 유튜브로 홈트해요!!",
          "speaker_id": "1",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039546.1.5",
          "form": "어느 채널 보시나요!!?",
          "original_form": "어느 채널 보시나요!!?",
          "speaker_id": "2",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039546.1.6",
          "form": "땅끄부부 보면서요!ㅋㅋㅋ",
          "original_form": "땅끄부부 보면서요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039546.1.7",
          "form": "저도 땅끄 알아요!",
          "original_form": "저도 땅끄 알아요!",
          "speaker_id": "2",
          "time": "20210903 19:01"
        },
        {
          "id": "MDRW2100039546.1.8",
          "form": "예전에 열심히 했었어요 ㅋㅋㅋㅋ",
          "original_form": "예전에 열심히 했었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:01"
        },
        {
          "id": "MDRW2100039546.1.9",
          "form": "저는 이제 저스트댄스가 하고싶어요!",
          "original_form": "저는 이제 저스트댄스가 하고싶어요!",
          "speaker_id": "2",
          "time": "20210903 19:01"
        },
        {
          "id": "MDRW2100039546.1.10",
          "form": "헉 저스트댄스!!",
          "original_form": "헉 저스트댄스!!",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100039546.1.11",
          "form": "예랑가랑 보면서 제일 하고싶었던 게임이에요ㅋㅋㅋ",
          "original_form": "예랑가랑 보면서 제일 하고싶었던 게임이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100039546.1.12",
          "form": "완전 재밌어요!!",
          "original_form": "완전 재밌어요!!",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100039546.1.13",
          "form": "핸드폰으로는 언제든 할 수 있으니까 깔아서 하게요! ㅋㅋㅋㅋ",
          "original_form": "핸드폰으로는 언제든 할 수 있으니까 깔아서 하게요! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039546.1.14",
          "form": "스위치는 못사도... 어플은 구독 할 수 있다...",
          "original_form": "스위치는 못사도... 어플은 구독 할 수 있다...",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039546.1.15",
          "form": "그쵸그쵸!!ㅋㅋㅋㅋ",
          "original_form": "그쵸그쵸!!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039546.1.16",
          "form": "춤은 못추지만 재밌어요ㅋㅋㅋㅋ",
          "original_form": "춤은 못추지만 재밌어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039546.1.17",
          "form": "스트릿 우먼 파이터 아세요??",
          "original_form": "스트릿 우먼 파이터 아세요??",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039546.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ 그렇게 추고싶은데 현실은 그냥 파닥파닥 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그렇게 추고싶은데 현실은 그냥 파닥파닥 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039546.1.19",
          "form": "ㅋㅋㅋㅋㅋ맞아요 걍 가게풍선 같아요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 걍 가게풍선 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039546.1.20",
          "form": "그래도 운동하는거니까 운동만 되면 되죠!! ㅋㅋㅋㅋ",
          "original_form": "그래도 운동하는거니까 운동만 되면 되죠!! ㅋㅋㅋㅋ",
