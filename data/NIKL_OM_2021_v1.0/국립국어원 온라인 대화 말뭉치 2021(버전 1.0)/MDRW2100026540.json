      "utterance": [
        {
          "id": "MDRW2100026540.1.1",
          "form": "취준중이세요???",
          "original_form": "취준중이세요???",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026540.1.2",
          "form": "네네 지금현재 취준중입니다.",
          "original_form": "네네 지금현재 취준중입니다.",
          "speaker_id": "2",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100026540.1.3",
          "form": "바로 딱 맞추셨네요",
          "original_form": "바로 딱 맞추셨네요",
          "speaker_id": "2",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100026540.1.4",
          "form": "저는 이번학기 막학기거든요...ㅜㅠ",
          "original_form": "저는 이번학기 막학기거든요...ㅜㅠ",
          "speaker_id": "1",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100026540.1.5",
          "form": "취준시장은 어떻나요...?",
          "original_form": "취준시장은 어떻나요...?",
          "speaker_id": "1",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100026540.1.6",
          "form": "상상 이상인가요...?",
          "original_form": "상상 이상인가요...?",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100026540.1.7",
          "form": "장난아니네요..",
          "original_form": "장난아니네요..",
          "speaker_id": "2",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100026540.1.8",
          "form": "눈을 낮추면 확실히 갈수있긴하지만, 사람이 안구해지는데 이유가 있더라구요",
          "original_form": "눈을 낮추면 확실히 갈수있긴하지만, 사람이 안구해지는데 이유가 있더라구요",
          "speaker_id": "2",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100026540.1.9",
          "form": "그 이유들에는 어떤게있나요...?",
          "original_form": "그 이유들에는 어떤게있나요...?",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100026540.1.10",
          "form": "페이, 인간관계...?",
          "original_form": "페이, 인간관계...?",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100026540.1.11",
          "form": "페이도 그렇고 인간관계도 그렇고 가장 그런건 일이...",
          "original_form": "페이도 그렇고 인간관계도 그렇고 가장 그런건 일이...",
          "speaker_id": "2",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100026540.1.12",
          "form": "일이 힘들고 어려운데 페이도 적고 그렇다고 알아주지고 않고",
          "original_form": "일이 힘들고 어려운데 페이도 적고 그렇다고 알아주지고 않고",
          "speaker_id": "2",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100026540.1.13",
          "form": "일이 힘들고 어려운데",
          "original_form": "일이 힘들고 어려운데",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100026540.1.14",
          "form": "나중에 경력이나 능력치로 들어가지 않는다는거죠?",
          "original_form": "나중에 경력이나 능력치로 들어가지 않는다는거죠?",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100026540.1.15",
          "form": "경력으로 들어갈수는 있는데 골병도 같이 얻어 가요",
          "original_form": "경력으로 들어갈수는 있는데 골병도 같이 얻어 가요",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100026540.1.16",
          "form": "아ㅠㅠ",
          "original_form": "아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100026540.1.17",
          "form": "정신도 몸도 아픈가요",
          "original_form": "정신도 몸도 아픈가요",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100026540.1.18",
          "form": "전 입사하면 멘탈관리가 중요할것같아요",
          "original_form": "전 입사하면 멘탈관리가 중요할것같아요",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100026540.1.19",
          "form": "보통 일이 힘들면 다른 분들도 같이 힘드니 자그만 실수로도 멘탈을 탈탈 털더라구요",
          "original_form": "보통 일이 힘들면 다른 분들도 같이 힘드니 자그만 실수로도 멘탈을 탈탈 털더라구요",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100026540.1.20",
          "form": "그래서 멘탈관리가 힘든거 같아요 ㅠ",
          "original_form": "그래서 멘탈관리가 힘든거 같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100026540.1.21",
          "form": "그리고 어디에나 있는 이상한 사람.....",
          "original_form": "그리고 어디에나 있는 이상한 사람.....",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100026540.1.22",
          "form": "얼마전에 절 괴롭히던 사람이 있었어서",
          "original_form": "얼마전에 절 괴롭히던 사람이 있었어서",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100026540.1.23",
          "form": "사회생활이 더 두려워요",
          "original_form": "사회생활이 더 두려워요",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100026540.1.24",
          "form": "괴롭히던 사람이요?",
          "original_form": "괴롭히던 사람이요?",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100026540.1.25",
          "form": "회사에서 그랬어요?",
          "original_form": "회사에서 그랬어요?",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100026540.1.26",
          "form": "아니요 학교 실험실이요ㅋㅋ",
          "original_form": "아니요 학교 실험실이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100026540.1.27",
          "form": "대학원...?",
          "original_form": "대학원...?",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100026540.1.28",
          "form": "대학원이나 학교 실험실이라고 생각하시면 될것같아요 ㅋㅋ",
          "original_form": "대학원이나 학교 실험실이라고 생각하시면 될것같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:01"
        }
      ]
    }
