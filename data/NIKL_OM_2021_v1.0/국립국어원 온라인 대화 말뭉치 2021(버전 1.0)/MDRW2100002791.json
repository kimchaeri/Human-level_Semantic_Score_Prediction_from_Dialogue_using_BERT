      "utterance": [
        {
          "id": "MDRW2100002791.1.1",
          "form": "너의 이상형이 선호였나?",
          "original_form": "너의 이상형이 선호였나?",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002791.1.2",
          "form": "이상형은 아무래도 최우식이 가까운 것 같아",
          "original_form": "이상형은 아무래도 최우식이 가까운 것 같아",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002791.1.3",
          "form": "웃음이 예쁜 사람을 좋아하는구나.",
          "original_form": "웃음이 예쁜 사람을 좋아하는구나.",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002791.1.4",
          "form": "김선호는 눈이 너무 동그랗거든",
          "original_form": "김선호는 눈이 너무 동그랗거든",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002791.1.5",
          "form": "키도 크고 잘생긴 게 좋겠지?",
          "original_form": "키도 크고 잘생긴 게 좋겠지?",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002791.1.6",
          "form": "눈이 동그랗긴 하더라.",
          "original_form": "눈이 동그랗긴 하더라.",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002791.1.7",
          "form": "당연하지 키는 필수야",
          "original_form": "당연하지 키는 필수야",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.8",
          "form": "너두 그렇지않니?",
          "original_form": "너두 그렇지않니?",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.9",
          "form": "최우식은 성격이 좋아보여",
          "original_form": "최우식은 성격이 좋아보여",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.10",
          "form": "키크고 괜찮고 성격좋은 사람이 좋구나",
          "original_form": "키크고 괜찮고 성격좋은 사람이 좋구나",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.11",
          "form": "아무래도 캐나다 남자라서 그런가봐",
          "original_form": "아무래도 캐나다 남자라서 그런가봐",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.12",
          "form": "나도 그래",
          "original_form": "나도 그래",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.13",
          "form": "능력까지 좋으면 금상첨화겠구나",
          "original_form": "능력까지 좋으면 금상첨화겠구나",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.14",
          "form": "너는 키가 최소 몇이었으면 좋겠어?",
          "original_form": "너는 키가 최소 몇이었으면 좋겠어?",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.15",
          "form": "최소 177정도는 되어야.",
          "original_form": "최소 177정도는 되어야.",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.16",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.17",
          "form": "나는 키 작으면 싫어.",
          "original_form": "나는 키 작으면 싫어.",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.18",
          "form": "나도 그정도",
          "original_form": "나도 그정도",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002791.1.19",
          "form": "근데 요즘 길에 키가 비슷한 커플이 많아",
          "original_form": "근데 요즘 길에 키가 비슷한 커플이 많아",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002791.1.20",
          "form": "안타까워라",
          "original_form": "안타까워라",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002791.1.21",
          "form": "둘다 작고 아담해보이더라",
          "original_form": "둘다 작고 아담해보이더라",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002791.1.22",
          "form": "나도 종종 보곤 해.",
          "original_form": "나도 종종 보곤 해.",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002791.1.23",
          "form": "정말 귀여운 커플이야",
          "original_form": "정말 귀여운 커플이야",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002791.1.24",
          "form": "그렇지않니?",
          "original_form": "그렇지않니?",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002791.1.25",
          "form": "맞아 안타깝고 귀여워",
          "original_form": "맞아 안타깝고 귀여워",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002791.1.26",
          "form": "그럼 너는 학벌도 보니??",
          "original_form": "그럼 너는 학벌도 보니??",
          "speaker_id": "2",
          "time": "20210820 15:37"
        }
      ]
    }
