      "utterance": [
        {
          "id": "MDRW2100041757.1.1",
          "form": "봄가을이 선선하니 좋지요ㅎㅎ",
          "original_form": "봄가을이 선선하니 좋지요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041757.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100041757.1.3",
          "form": "전 특히 가을을 좋아해요",
          "original_form": "전 특히 가을을 좋아해요",
          "speaker_id": "1",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100041757.1.4",
          "form": "가을 단풍을 좋아하거든요",
          "original_form": "가을 단풍을 좋아하거든요",
          "speaker_id": "1",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100041757.1.5",
          "form": "단풍구경 가고싶네요ㅎㅎ",
          "original_form": "단풍구경 가고싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100041757.1.6",
          "form": "해마다 다녔었는데 아쉽게도 작년부터 못가고 있어요",
          "original_form": "해마다 다녔었는데 아쉽게도 작년부터 못가고 있어요",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041757.1.7",
          "form": "비가 다시 오려는지 흐리네요",
          "original_form": "비가 다시 오려는지 흐리네요",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041757.1.8",
          "form": "여긴 이미 비가 시작됐어요",
          "original_form": "여긴 이미 비가 시작됐어요",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041757.1.9",
          "form": "가을장마는 아니고 태풍이 지나가는걸까요",
          "original_form": "가을장마는 아니고 태풍이 지나가는걸까요",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041757.1.10",
          "form": "바람도 꽤 불어요",
          "original_form": "바람도 꽤 불어요",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041757.1.11",
          "form": "이번 비는 오래는 안 내릴 것 같아요",
          "original_form": "이번 비는 오래는 안 내릴 것 같아요",
          "speaker_id": "2",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100041757.1.12",
          "form": "내일까지 온다는 거보니까 내일 오다가 멈출건가봐요",
          "original_form": "내일까지 온다는 거보니까 내일 오다가 멈출건가봐요",
          "speaker_id": "1",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100041757.1.13",
          "form": "그럴 것 같아요 ㅎㅎ",
          "original_form": "그럴 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100041757.1.14",
          "form": "이번 비는 장맛비가 아니라 지나가는 비였으면 좋겠어요",
          "original_form": "이번 비는 장맛비가 아니라 지나가는 비였으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100041757.1.15",
          "form": "이제 비는 그만오고 맑은 가을 하늘을 오래오래 느끼고  싶네요",
          "original_form": "이제 비는 그만오고 맑은 가을 하늘을 오래오래 느끼고  싶네요",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100041757.1.16",
          "form": "저두요ㅎㅎ 가을옷 좀 입고 다닐 수 있었으면하네요",
          "original_form": "저두요ㅎㅎ 가을옷 좀 입고 다닐 수 있었으면하네요",
          "speaker_id": "2",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100041757.1.17",
          "form": "정말 가을옷 몇번이나 입을 수 있을까요",
          "original_form": "정말 가을옷 몇번이나 입을 수 있을까요",
          "speaker_id": "1",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100041757.1.18",
          "form": "매번 금방 지나가버리니..",
          "original_form": "매번 금방 지나가버리니..",
          "speaker_id": "2",
          "time": "20210906 20:50"
        }
      ]
