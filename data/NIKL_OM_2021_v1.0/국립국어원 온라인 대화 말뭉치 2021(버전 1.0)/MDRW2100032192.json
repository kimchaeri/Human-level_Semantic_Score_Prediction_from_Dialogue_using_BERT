      "utterance": [
        {
          "id": "MDRW2100032192.1.1",
          "form": "혹시 와이프분 임신중이신가요?!",
          "original_form": "혹시 와이프분 임신중이신가요?!",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032192.1.2",
          "form": "아니요 아직이요",
          "original_form": "아니요 아직이요",
          "speaker_id": "1",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100032192.1.3",
          "form": "이번달에 시도할거라서 ㅎㅎ",
          "original_form": "이번달에 시도할거라서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032192.1.4",
          "form": "전 제동생이 임신중인데 먹을거진짜 잘먹어야하는거같아요",
          "original_form": "전 제동생이 임신중인데 먹을거진짜 잘먹어야하는거같아요",
          "speaker_id": "2",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032192.1.5",
          "form": "그쵸 임신중에는 ㅎㅎ",
          "original_form": "그쵸 임신중에는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032192.1.6",
          "form": "보양식 위주로",
          "original_form": "보양식 위주로",
          "speaker_id": "1",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032192.1.7",
          "form": "다행히 입덧은 없더라구요 엄마가 별로 안심하셨어서 그런가봐요 ㅎㅎ",
          "original_form": "다행히 입덧은 없더라구요 엄마가 별로 안심하셨어서 그런가봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032192.1.8",
          "form": "옆에서도 고생일텐데",
          "original_form": "옆에서도 고생일텐데",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032192.1.9",
          "form": "본인에 비할바는 ㅋㅋ",
          "original_form": "본인에 비할바는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032192.1.10",
          "form": "당연하죠 ㅜ본인이 가장 힘들죠 ㅜㅜ 친구들 고생하는애들은 장난아니던걸요.. 아예 뭘 못먹는 분들도 있잖아예",
          "original_form": "당연하죠 ㅜ본인이 가장 힘들죠 ㅜㅜ 친구들 고생하는애들은 장난아니던걸요.. 아예 뭘 못먹는 분들도 있잖아예",
          "speaker_id": "2",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032192.1.11",
          "form": "아요",
          "original_form": "아요",
          "speaker_id": "2",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032192.1.12",
          "form": "아빠 닮아라 아가야",
          "original_form": "아빠 닮아라 아가야",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100032192.1.13",
          "form": "저녁뭐드시나요 ㅎㅎ",
          "original_form": "저녁뭐드시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 17:40"
        },
        {
          "id": "MDRW2100032192.1.14",
          "form": "고기 먹고 술이요ㅎㅎ",
          "original_form": "고기 먹고 술이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:08"
        },
        {
          "id": "MDRW2100032192.1.15",
          "form": "전 컵라면 ㅋㅋㅋㅋㅋㅋ 그냥 잘랬는데 코로나 지원금 탈락해서 하나 물부었어요... ㅎㅎ 내25만원...",
          "original_form": "전 컵라면 ㅋㅋㅋㅋㅋㅋ 그냥 잘랬는데 코로나 지원금 탈락해서 하나 물부었어요... ㅎㅎ 내25만원...",
          "speaker_id": "2",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100032192.1.16",
          "form": "저도 탈락입니다 ㅋㅋ",
          "original_form": "저도 탈락입니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 08:25"
        },
        {
          "id": "MDRW2100032192.1.17",
          "form": "애없는 맞벌이는 도저히 받을수가...",
          "original_form": "애없는 맞벌이는 도저히 받을수가...",
          "speaker_id": "1",
          "time": "20210906 08:25"
        },
        {
          "id": "MDRW2100032192.1.18",
          "form": "부양식구가 있으면 아무래도 받겠군요! 은근 많이들 떨어진거같아요 ㅋㅋㅋ 전애도없고 결혼도없고 지원금도없네요 재산도없는데무슨일이야 ㅋㅋㅋㅋ 점심 드셨나요?",
          "original_form": "부양식구가 있으면 아무래도 받겠군요! 은근 많이들 떨어진거같아요 ㅋㅋㅋ 전애도없고 결혼도없고 지원금도없네요 재산도없는데무슨일이야 ㅋㅋㅋㅋ 점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210906 11:50"
        },
        {
          "id": "MDRW2100032192.1.19",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100032192.1.20",
          "form": "네 순대국 먹었습니다",
          "original_form": "네 순대국 먹었습니다",
          "speaker_id": "1",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100032192.1.21",
          "form": "뭐 드세요 ㅋㅋ",
          "original_form": "뭐 드세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100032192.1.22",
          "form": "전 돈까스요 ㅋㅋㅋㅋ 맛이없네요",
          "original_form": "전 돈까스요 ㅋㅋㅋㅋ 맛이없네요",
          "speaker_id": "2",
          "time": "20210906 12:46"
        }
      ]
