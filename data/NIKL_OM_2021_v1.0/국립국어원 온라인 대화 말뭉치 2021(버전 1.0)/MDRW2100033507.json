      "utterance": [
        {
          "id": "MDRW2100033507.1.1",
          "form": "아 아 그거요그거! ㅋㅋ",
          "original_form": "아 아 그거요그거! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:00"
        },
        {
          "id": "MDRW2100033507.1.2",
          "form": "혹시 보신적 있으신가요",
          "original_form": "혹시 보신적 있으신가요",
          "speaker_id": "1",
          "time": "20210824 09:01"
        },
        {
          "id": "MDRW2100033507.1.3",
          "form": "진짜 와우 막장은 이렇게 쓰는거구나",
          "original_form": "진짜 와우 막장은 이렇게 쓰는거구나",
          "speaker_id": "1",
          "time": "20210824 09:01"
        },
        {
          "id": "MDRW2100033507.1.4",
          "form": "아뇨아뇨",
          "original_form": "아뇨아뇨",
          "speaker_id": "2",
          "time": "20210824 09:01"
        },
        {
          "id": "MDRW2100033507.1.5",
          "form": "연락두절된거에요",
          "original_form": "연락두절된거에요",
          "speaker_id": "2",
          "time": "20210824 09:02"
        },
        {
          "id": "MDRW2100033507.1.6",
          "form": "그거본다고 ㅋㅋㅋ",
          "original_form": "그거본다고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:02"
        },
        {
          "id": "MDRW2100033507.1.7",
          "form": "ㅋㅋㅋ 그거 보면 헤어나올 수 가 없어요",
          "original_form": "ㅋㅋㅋ 그거 보면 헤어나올 수 가 없어요",
          "speaker_id": "1",
          "time": "20210824 09:02"
        },
        {
          "id": "MDRW2100033507.1.8",
          "form": "그전까지는 심심해서 죽을라했거든요 ㅋㅋ",
          "original_form": "그전까지는 심심해서 죽을라했거든요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:02"
        },
        {
          "id": "MDRW2100033507.1.9",
          "form": "약간 짜증나는데 끓을 수 없는 전개",
          "original_form": "약간 짜증나는데 끓을 수 없는 전개",
          "speaker_id": "1",
          "time": "20210824 09:02"
        },
        {
          "id": "MDRW2100033507.1.10",
          "form": "그게 너무 어이없었 ㅋㅋ",
          "original_form": "그게 너무 어이없었 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:02"
        },
        {
          "id": "MDRW2100033507.1.11",
          "form": "ㅋㅋㅋ 동생분 심정을 이해합니다",
          "original_form": "ㅋㅋㅋ 동생분 심정을 이해합니다",
          "speaker_id": "1",
          "time": "20210824 09:02"
        },
        {
          "id": "MDRW2100033507.1.12",
          "form": "저한테 보지말라고",
          "original_form": "저한테 보지말라고",
          "speaker_id": "2",
          "time": "20210824 09:03"
        },
        {
          "id": "MDRW2100033507.1.13",
          "form": "자기가 이야기해주겠대요 ㅋㅋ",
          "original_form": "자기가 이야기해주겠대요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:03"
        },
        {
          "id": "MDRW2100033507.1.14",
          "form": "진짜 불륜드라마의 완전체같은 느낌",
          "original_form": "진짜 불륜드라마의 완전체같은 느낌",
          "speaker_id": "1",
          "time": "20210824 09:03"
        },
        {
          "id": "MDRW2100033507.1.15",
          "form": "부부의 세계와는 결이 다른",
          "original_form": "부부의 세계와는 결이 다른",
          "speaker_id": "1",
          "time": "20210824 09:03"
        },
        {
          "id": "MDRW2100033507.1.16",
          "form": "진짜 뭐 이런 전개가 다있나 싶은 미쳐버린드라마같아요",
          "original_form": "진짜 뭐 이런 전개가 다있나 싶은 미쳐버린드라마같아요",
          "speaker_id": "1",
          "time": "20210824 09:06"
        },
        {
          "id": "MDRW2100033507.1.17",
          "form": "자꾸 동생이 엄마한테는 영업해요",
          "original_form": "자꾸 동생이 엄마한테는 영업해요",
          "speaker_id": "2",
          "time": "20210824 09:09"
        },
        {
          "id": "MDRW2100033507.1.18",
          "form": "보라고 얼른",
          "original_form": "보라고 얼른",
          "speaker_id": "2",
          "time": "20210824 09:09"
        },
        {
          "id": "MDRW2100033507.1.19",
          "form": "아 그거 방송사가 어디에요",
          "original_form": "아 그거 방송사가 어디에요",
          "speaker_id": "2",
          "time": "20210824 09:09"
        },
        {
          "id": "MDRW2100033507.1.20",
          "form": "티비조선인가 그랬던거같아요",
          "original_form": "티비조선인가 그랬던거같아요",
          "speaker_id": "1",
          "time": "20210824 09:10"
        },
        {
          "id": "MDRW2100033507.1.21",
          "form": "저도 안보려다 하도 보라그래서",
          "original_form": "저도 안보려다 하도 보라그래서",
          "speaker_id": "1",
          "time": "20210824 09:10"
        },
        {
          "id": "MDRW2100033507.1.22",
          "form": "봤는데 욕하면서 보게되더라구요",
          "original_form": "봤는데 욕하면서 보게되더라구요",
          "speaker_id": "1",
          "time": "20210824 09:10"
        },
        {
          "id": "MDRW2100033507.1.23",
          "form": "드라마가 부작용이 있어서 저는 보지말라는데 ㅋㅋ",
          "original_form": "드라마가 부작용이 있어서 저는 보지말라는데 ㅋㅋ",
