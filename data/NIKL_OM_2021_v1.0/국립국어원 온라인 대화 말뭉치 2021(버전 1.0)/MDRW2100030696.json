        },
        {
          "id": "MDRW2100030696.1.3",
          "form": "돌리는 게임 아닌가영ㅎ",
          "original_form": "돌리는 게임 아닌가영ㅎ",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030696.1.4",
          "form": "네 밤치형 게임이에요",
          "original_form": "네 밤치형 게임이에요",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030696.1.5",
          "form": "넹 전 그런 겜은 안해봤어요",
          "original_form": "넹 전 그런 겜은 안해봤어요",
          "speaker_id": "2",
