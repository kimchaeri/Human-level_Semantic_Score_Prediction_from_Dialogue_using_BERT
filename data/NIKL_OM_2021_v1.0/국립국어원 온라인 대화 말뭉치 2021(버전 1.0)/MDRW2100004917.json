      "utterance": [
        {
          "id": "MDRW2100004917.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004917.1.2",
          "form": "혹시 왓챠보시나요?",
          "original_form": "혹시 왓챠보시나요?",
          "speaker_id": "1",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004917.1.3",
          "form": "저번 달까지 구독하고 있었어요",
          "original_form": "저번 달까지 구독하고 있었어요",
          "speaker_id": "2",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004917.1.4",
          "form": "name1 님도 왓챠 보시나요?",
          "original_form": "&name1& 님도 왓챠 보시나요?",
          "speaker_id": "2",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004917.1.5",
          "form": "아 저는 그저께까지 구독했었어요!",
          "original_form": "아 저는 그저께까지 구독했었어요!",
          "speaker_id": "1",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004917.1.6",
          "form": "지그재그 앱 이벤트로 한달만 구독했거든요",
          "original_form": "지그재그 앱 이벤트로 한달만 구독했거든요",
          "speaker_id": "1",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004917.1.7",
          "form": "그런 이벤트도 했었군요",
          "original_form": "그런 이벤트도 했었군요",
          "speaker_id": "2",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004917.1.8",
          "form": "재밌게 본 작품 있으세요?",
          "original_form": "재밌게 본 작품 있으세요?",
          "speaker_id": "2",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004917.1.9",
          "form": "제일 재밌게 본 건 와이우먼킬!",
          "original_form": "제일 재밌게 본 건 와이우먼킬!",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004917.1.10",
          "form": "아세요? 아마 미드일거예요",
          "original_form": "아세요? 아마 미드일거예요",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004917.1.11",
          "form": "추천 받은 적 있는데 아직 못 봤어요",
          "original_form": "추천 받은 적 있는데 아직 못 봤어요",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004917.1.12",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004917.1.13",
          "form": "제가 왁챠로 본 것중에 제일 몰입하면서 봤던거예요",
          "original_form": "제가 왁챠로 본 것중에 제일 몰입하면서 봤던거예요",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004917.1.14",
          "form": "정말 추천....!!!",
          "original_form": "정말 추천....!!!",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004917.1.15",
          "form": "같은 집에서 살았던, 시대가 다른 여자 3명의 스토리가 병렬적으로? 진행되는데",
          "original_form": "같은 집에서 살았던, 시대가 다른 여자 3명의 스토리가 병렬적으로? 진행되는데",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004917.1.16",
          "form": "진짜 계속 다음화가 보고싶게 만드는 스토리예요",
          "original_form": "진짜 계속 다음화가 보고싶게 만드는 스토리예요",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004917.1.17",
          "form": "오 너무 재밌을 거 같아요",
          "original_form": "오 너무 재밌을 거 같아요",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004917.1.18",
          "form": "정말 재밌어요 이번에 시즌 2도 하고 있어요",
          "original_form": "정말 재밌어요 이번에 시즌 2도 하고 있어요",
          "speaker_id": "1",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004917.1.19",
          "form": "진짜 재밌어서 추천드리고싶네요",
          "original_form": "진짜 재밌어서 추천드리고싶네요",
          "speaker_id": "1",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004917.1.20",
          "form": "왓챠 구독이 부담스러우시면 왓챠팝콘 구매 추천드려요! ㅋㅋ",
          "original_form": "왓챠 구독이 부담스러우시면 왓챠팝콘 구매 추천드려요! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004917.1.21",
          "form": "백프로 이용권 나오는건데 저는 어제 샀어요 ㅎㅎ",
          "original_form": "백프로 이용권 나오는건데 저는 어제 샀어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004917.1.22",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004917.1.23",
          "form": "꿀팁 감사드려요",
          "original_form": "꿀팁 감사드려요",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004917.1.24",
          "form": "진짜 굿이죠...!!!!",
          "original_form": "진짜 굿이죠...!!!!",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004917.1.25",
          "form": "이마트편의점에서 팔아요",
          "original_form": "이마트편의점에서 팔아요",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004917.1.26",
          "form": "단돈 이처넌 .!!!!",
          "original_form": "단돈 이처넌 .!!!!",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004917.1.27",
          "form": "집 앞에 있는데 다음에 다녀와야 겠어요ㅎㅎ",
          "original_form": "집 앞에 있는데 다음에 다녀와야 겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004917.1.28",
          "form": "빵과 스프, 고양이랑 함께하기 좋은 날? 이거 되게 재밌게 봤어요",
          "original_form": "빵과 스프, 고양이랑 함께하기 좋은 날? 이거 되게 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004917.1.29",
          "form": "약간 리틀포레스트 느낌",
          "original_form": "약간 리틀포레스트 느낌",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004917.1.30",
          "form": "밥 먹으면서 보기 좋더라고요",
          "original_form": "밥 먹으면서 보기 좋더라고요",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004917.1.31",
          "form": "오 제목이 귀여워요",
          "original_form": "오 제목이 귀여워요",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004917.1.32",
          "form": "외국영화인가요?",
          "original_form": "외국영화인가요?",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004917.1.33",
          "form": "일본이요",
          "original_form": "일본이요",
          "speaker_id": "2",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004917.1.34",
          "form": "여유롭고 잔잔해요",
          "original_form": "여유롭고 잔잔해요",
          "speaker_id": "2",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004917.1.35",
          "form": "역시~ 뭔가 그럴 것 같았어요",
          "original_form": "역시~ 뭔가 그럴 것 같았어요",
          "speaker_id": "1",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004917.1.36",
          "form": "한번 봐봐야겠어요",
          "original_form": "한번 봐봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004917.1.37",
          "form": "저 일본 드라마나 영화 애니 등.. 관심많거든요",
          "original_form": "저 일본 드라마나 영화 애니 등.. 관심많거든요",
