      "utterance": [
        {
          "id": "MDRW2100039853.1.1",
          "form": "김희재도 드라마데뷔",
          "original_form": "김희재도 드라마데뷔",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039853.1.2",
          "form": "김희재가 누구예요?, 처음 들어봐요",
          "original_form": "김희재가 누구예요?, 처음 들어봐요",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039853.1.3",
          "form": "그러게 봉숭아학당 얘기도 나오고 가수라는데",
          "original_form": "그러게 봉숭아학당 얘기도 나오고 가수라는데",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039853.1.4",
          "form": "봉숭아학당요?  개그맨인가요?",
          "original_form": "봉숭아학당요?  개그맨인가요?",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039853.1.5",
          "form": "미스터트롯이다 검색해보니",
          "original_form": "미스터트롯이다 검색해보니",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039853.1.6",
          "form": "그럼 뽕숭아학당이네요ㅋ",
          "original_form": "그럼 뽕숭아학당이네요ㅋ",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039853.1.7",
          "form": "아~그런가보네",
          "original_form": "아~그런가보네",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039853.1.8",
          "form": "아 누군지 알거  같네요ㅋ",
          "original_form": "아 누군지 알거  같네요ㅋ",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039853.1.9",
          "form": "트롯트 가수",
          "original_form": "트롯트 가수",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039853.1.10",
          "form": "고규필도 조연으로 나오네",
          "original_form": "고규필도 조연으로 나오네",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039853.1.11",
          "form": "고규필은 진짜 모르겠네요",
          "original_form": "고규필은 진짜 모르겠네요",
          "speaker_id": "1",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039853.1.12",
          "form": "조금 살찐분인데",
          "original_form": "조금 살찐분인데",
          "speaker_id": "2",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039853.1.13",
          "form": "사랑의 불시착 홍창식",
          "original_form": "사랑의 불시착 홍창식",
          "speaker_id": "2",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039853.1.14",
          "form": "검색해 보께요. 이분도 가수예요?",
          "original_form": "검색해 보께요. 이분도 가수예요?",
          "speaker_id": "1",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039853.1.15",
          "form": "얼굴보니 알겠네요",
          "original_form": "얼굴보니 알겠네요",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039853.1.16",
          "form": "가수는 아니고 배우 얼굴보면 알껄",
          "original_form": "가수는 아니고 배우 얼굴보면 알껄",
