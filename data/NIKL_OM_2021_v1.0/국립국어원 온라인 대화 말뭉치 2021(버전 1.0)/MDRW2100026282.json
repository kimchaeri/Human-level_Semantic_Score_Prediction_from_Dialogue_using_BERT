      "utterance": [
        {
          "id": "MDRW2100026282.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026282.1.2",
          "form": "좋아하는 음식이 뭐야?",
          "original_form": "좋아하는 음식이 뭐야?",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026282.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026282.1.4",
          "form": "저는 대부분 다 잘 먹어요",
          "original_form": "저는 대부분 다 잘 먹어요",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026282.1.5",
          "form": "고수, 닭발, 선지 이런 거 말고는...!",
          "original_form": "고수, 닭발, 선지 이런 거 말고는...!",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026282.1.6",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026282.1.7",
          "form": "고수는 정말 안맞더라고요",
          "original_form": "고수는 정말 안맞더라고요",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026282.1.8",
          "form": "저도요...",
          "original_form": "저도요...",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026282.1.9",
          "form": "원래 편식 잘 안하는데",
          "original_form": "원래 편식 잘 안하는데",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026282.1.10",
          "form": "고수는 좀 어려운 친구예요...",
          "original_form": "고수는 좀 어려운 친구예요...",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026282.1.11",
          "form": "고수 들어가면 음식이 냄새가 달라지더라고요",
          "original_form": "고수 들어가면 음식이 냄새가 달라지더라고요",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026282.1.12",
          "form": "오 그럼 쌀국수는 드세요?",
          "original_form": "오 그럼 쌀국수는 드세요?",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026282.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026282.1.14",
          "form": "대신 고수없는 걸 먹어요!",
          "original_form": "대신 고수없는 걸 먹어요!",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026282.1.15",
          "form": "쌀국수 진짜 좋아합니다 ㅎㅎ",
          "original_form": "쌀국수 진짜 좋아합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026282.1.16",
          "form": "오 저는 쌀국수 안먹어 봤는데",
          "original_form": "오 저는 쌀국수 안먹어 봤는데",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026282.1.17",
          "form": "맛있어요??",
          "original_form": "맛있어요??",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026282.1.18",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026282.1.19",
          "form": "아 name1 10대시군요!!!",
          "original_form": "아 &name1& 10대시군요!!!",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026282.1.20",
          "form": "그러면 안드셔보셨을 수도 있겠네요 ㅎㅎ",
          "original_form": "그러면 안드셔보셨을 수도 있겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026282.1.21",
          "form": "쌀국수 되게 소화 잘되고 좋아요~",
          "original_form": "쌀국수 되게 소화 잘되고 좋아요~",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026282.1.22",
          "form": "약간 고수에 거부감이 있으니까",
          "original_form": "약간 고수에 거부감이 있으니까",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026282.1.23",
          "form": "쌀국수도 조심하게 되더라고요",
          "original_form": "쌀국수도 조심하게 되더라고요",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026282.1.24",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026282.1.25",
          "form": "식당에 따라서는",
          "original_form": "식당에 따라서는",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026282.1.26",
          "form": "고수를 기본적으로 주는 데도 있으니...!",
          "original_form": "고수를 기본적으로 주는 데도 있으니...!",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026282.1.27",
          "form": "완전 정식대로 하는 전문점에서는 고수를 거의 넣는거 같더라고요",
          "original_form": "완전 정식대로 하는 전문점에서는 고수를 거의 넣는거 같더라고요",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026282.1.28",
          "form": "맞아요맞아요!",
          "original_form": "맞아요맞아요!",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026282.1.29",
          "form": "저는 예전에는 음식 가리는게 없다고 생각했는데",
          "original_form": "저는 예전에는 음식 가리는게 없다고 생각했는데",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026282.1.30",
          "form": "고수먹고...",
          "original_form": "고수먹고...",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026282.1.31",
          "form": "아 닭발도 못드신다고 하셨죠? 어떤 점때문에 못 드세요?",
          "original_form": "아 닭발도 못드신다고 하셨죠? 어떤 점때문에 못 드세요?",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026282.1.32",
          "form": "맞아요 고수는 정말 난해해요...",
          "original_form": "맞아요 고수는 정말 난해해요...",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026282.1.33",
          "form": "좋아하는 사람들은 진짜 좋아하더라구요",
          "original_form": "좋아하는 사람들은 진짜 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026282.1.34",
          "form": "닭발은,,, 뭐랄까요,,, 생김새 때문인 것 같아요!",
          "original_form": "닭발은,,, 뭐랄까요,,, 생김새 때문인 것 같아요!",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026282.1.35",
          "form": "name1은 잘 드시나요?",
          "original_form": "&name1&은 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026282.1.36",
          "form": "저는 닭발 잘먹어요!!",
          "original_form": "저는 닭발 잘먹어요!!",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026282.1.37",
          "form": "혹시 치킨에 오돌뼈 좋아하세요?",
          "original_form": "혹시 치킨에 오돌뼈 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026282.1.38",
          "form": "오오 잘 드시는구나 ><",
          "original_form": "오오 잘 드시는구나 ><",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026282.1.39",
          "form": "누군가 잘 먹으면",
          "original_form": "누군가 잘 먹으면",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026282.1.40",
          "form": "정말 좋죠 안남길 수 있구~!",
          "original_form": "정말 좋죠 안남길 수 있구~!",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026282.1.41",
          "form": "사실 오돌뼈도 안먹어요,,,, 저 안먹는거 꽤 많네요?",
          "original_form": "사실 오돌뼈도 안먹어요,,,, 저 안먹는거 꽤 많네요?",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026282.1.42",
          "form": "아 그래서 닭발도 안드시나봐요!",
          "original_form": "아 그래서 닭발도 안드시나봐요!",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026282.1.43",
          "form": "오돌뼈 먹는 사람이 닭발도 먹더라고요",
          "original_form": "오돌뼈 먹는 사람이 닭발도 먹더라고요",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026282.1.44",
          "form": "오오 그런 경향성이 있나보군요!",
          "original_form": "오오 그런 경향성이 있나보군요!",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026282.1.45",
          "form": "일리 있는 말이에요 ",
          "original_form": "일리 있는 말이에요 ",
