      "utterance": [
        {
          "id": "MDRW2100024092.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100024092.1.2",
          "form": "안녕하세요~~^^",
          "original_form": "안녕하세요~~^^",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100024092.1.3",
          "form": "배달음식 많이 드시나요?",
          "original_form": "배달음식 많이 드시나요?",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100024092.1.4",
          "form": "이틀에 한번꼴로요",
          "original_form": "이틀에 한번꼴로요",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100024092.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100024092.1.6",
          "form": "전 이주에 한번요?ㅎㅎ",
          "original_form": "전 이주에 한번요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100024092.1.7",
          "form": "맛집을 못찾아서ㅜㅜ",
          "original_form": "맛집을 못찾아서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100024092.1.8",
          "form": "ㅎㅎ 주로 뭐 시켜드세요?",
          "original_form": "ㅎㅎ 주로 뭐 시켜드세요?",
          "speaker_id": "1",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100024092.1.9",
          "form": "곱창이요!ㅋㅋㅋ",
          "original_form": "곱창이요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100024092.1.10",
          "form": "곱창전골너무 맛있어요ㅜㅜ",
          "original_form": "곱창전골너무 맛있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100024092.1.11",
          "form": "이틀마다 시키시면 종목고르는게 힘들진 않으세요?",
          "original_form": "이틀마다 시키시면 종목고르는게 힘들진 않으세요?",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100024092.1.12",
          "form": "헉 ㅜㅜ 근처에 곱창전골 맛집이 있나요",
          "original_form": "헉 ㅜㅜ 근처에 곱창전골 맛집이 있나요",
          "speaker_id": "1",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100024092.1.13",
          "form": "전 배달음식 난민이에요 아직도ㅜ",
          "original_form": "전 배달음식 난민이에요 아직도ㅜ",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024092.1.14",
          "form": "네! ㅠㅠ\r\n한군데 찾아서 자주 애용해요ㅜㅜ",
          "original_form": "네! ㅠㅠ\r\n한군데 찾아서 자주 애용해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024092.1.15",
          "form": "아...ㅜㅜ\r\n슬프시겠다..중요한건데..",
          "original_form": "아...ㅜㅜ\r\n슬프시겠다..중요한건데..",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024092.1.16",
          "form": "ㅜㅇㅜ 회 맛집은 찾았는데 곱창 맛집이 없어요ㅜ",
          "original_form": "ㅜㅇㅜ 회 맛집은 찾았는데 곱창 맛집이 없어요ㅜ",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024092.1.17",
          "form": "죄다 돼지곱창ㅜ",
          "original_form": "죄다 돼지곱창ㅜ",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024092.1.18",
          "form": "헉!\r\n회맛집!\r\n그게 더 찾기 힘들지 않나요?",
          "original_form": "헉!\r\n회맛집!\r\n그게 더 찾기 힘들지 않나요?",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024092.1.19",
          "form": "ㅎㅎ제가 회 킬러라",
          "original_form": "ㅎㅎ제가 회 킬러라",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024092.1.20",
          "form": "이것만 중점 팠지요",
          "original_form": "이것만 중점 팠지요",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024092.1.21",
          "form": "회 좋아하세요?",
          "original_form": "회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024092.1.22",
          "form": "네! 회도 좋고 해산물은 다 좋아요ㅎㅎ",
          "original_form": "네! 회도 좋고 해산물은 다 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024092.1.23",
          "form": "와 가을전어 드셨나요!!",
          "original_form": "와 가을전어 드셨나요!!",
          "speaker_id": "1",
          "time": "20210830 22:17"
        }
      ]
