      "utterance": [
        {
          "id": "MDRW2100033835.1.1",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100033835.1.2",
          "form": "아니요ㅎㅎ",
          "original_form": "아니요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100033835.1.3",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "2",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100033835.1.4",
          "form": "홈트로 하루한번은 하려고 노력중이에용",
          "original_form": "홈트로 하루한번은 하려고 노력중이에용",
          "speaker_id": "1",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100033835.1.5",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100033835.1.6",
          "form": "하루한번이면",
          "original_form": "하루한번이면",
          "speaker_id": "2",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100033835.1.7",
          "form": "엄청난대요",
          "original_form": "엄청난대요",
          "speaker_id": "2",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100033835.1.8",
          "form": "그런가요 ㅋㅋㅋ",
          "original_form": "그런가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100033835.1.9",
          "form": "확실히 나이들수록 운동이 필요하더라구요 ㅠ",
          "original_form": "확실히 나이들수록 운동이 필요하더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100033835.1.10",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100033835.1.11",
          "form": "전 너무안해서큰일이에요",
          "original_form": "전 너무안해서큰일이에요",
          "speaker_id": "2",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100033835.1.12",
          "form": "산책 좋아하세요?",
          "original_form": "산책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100033835.1.13",
          "form": "네네 산책좋죠",
          "original_form": "네네 산책좋죠",
          "speaker_id": "2",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100033835.1.14",
          "form": "근데 귀찮긴해요ㅠ",
          "original_form": "근데 귀찮긴해요ㅠ",
          "speaker_id": "2",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100033835.1.15",
          "form": "생각난김에 걸으러나갈까 고민중이에여 ㅋㅋ",
          "original_form": "생각난김에 걸으러나갈까 고민중이에여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100033835.1.16",
          "form": "지금",
          "original_form": "지금",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100033835.1.17",
          "form": "비오지않나요? ㄷㄷ",
          "original_form": "비오지않나요? ㄷㄷ",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100033835.1.18",
          "form": "ㅋㅋㅋㅋ아직은 안오는데 또나가면 오려나요 ㅠ",
          "original_form": "ㅋㅋㅋㅋ아직은 안오는데 또나가면 오려나요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100033835.1.19",
          "form": "오늘도 홈트해야겟네용",
          "original_form": "오늘도 홈트해야겟네용",
          "speaker_id": "1",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100033835.1.20",
          "form": "장마라더라고요ㅠ",
          "original_form": "장마라더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100033835.1.21",
          "form": "전 피티하고싶어요",
          "original_form": "전 피티하고싶어요",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100033835.1.22",
          "form": "피티 힘들거같야용 ㅠ",
          "original_form": "피티 힘들거같야용 ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100033835.1.23",
          "form": "글고",
          "original_form": "글고",
          "speaker_id": "2",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100033835.1.24",
          "form": "너무비싸요",
          "original_form": "너무비싸요",
