      "utterance": [
        {
          "id": "MDRW2100028526.1.1",
          "form": "배구는 관람료가 저렴해요??",
          "original_form": "배구는 관람료가 저렴해요??",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028526.1.2",
          "form": "한번도 가본적이없어성 ㅎㅎ",
          "original_form": "한번도 가본적이없어성 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028526.1.3",
          "form": "네 야구가 비싸고 배구는 저렴ㅎㅎ",
          "original_form": "네 야구가 비싸고 배구는 저렴ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028526.1.4",
          "form": "아무래더 야구가 유명하니 그란가바여",
          "original_form": "아무래더 야구가 유명하니 그란가바여",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028526.1.5",
          "form": "네 유료관객수가 다르다보니 ㅠ",
          "original_form": "네 유료관객수가 다르다보니 ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028526.1.6",
          "form": "그래더 이번 올림픽 덕에",
          "original_form": "그래더 이번 올림픽 덕에",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028526.1.7",
          "form": "사람돔 많아지지않았을까요??ㅎㅎ",
          "original_form": "사람돔 많아지지않았을까요??ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028526.1.8",
          "form": "무관중 경기지 않나요?",
          "original_form": "무관중 경기지 않나요?",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028526.1.9",
          "form": "코로나라 무관중일수있어요",
          "original_form": "코로나라 무관중일수있어요",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028526.1.10",
          "form": "아..그러겧네여 ㅠㅠ",
          "original_form": "아..그러겧네여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028526.1.11",
          "form": "좀 띄엄띄엄앉아서 하는줄 알았어요",
          "original_form": "좀 띄엄띄엄앉아서 하는줄 알았어요",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028526.1.12",
          "form": "그것두 시기랑 협회마다 달라요",
          "original_form": "그것두 시기랑 협회마다 달라요",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028526.1.13",
          "form": "호 엄청잘아시네요",
          "original_form": "호 엄청잘아시네요",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028526.1.14",
          "form": "경기에 관심 많으신가바여",
          "original_form": "경기에 관심 많으신가바여",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028526.1.15",
          "form": "보러갈까 고민했었거든요 ㅋㅋ",
          "original_form": "보러갈까 고민했었거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028526.1.16",
          "form": "어차피 못가는 것이었음을,,",
          "original_form": "어차피 못가는 것이었음을,,",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028526.1.17",
          "form": "하...정말 코로나 ㅠㅠㅠ",
          "original_form": "하...정말 코로나 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028526.1.18",
          "form": "답답해여",
          "original_form": "답답해여",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028526.1.19",
          "form": "그러게여 스포츠도 못볼줄은",
          "original_form": "그러게여 스포츠도 못볼줄은",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028526.1.20",
          "form": "정말 아쉽네여..",
          "original_form": "정말 아쉽네여..",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028526.1.21",
          "form": "저누어렸을때 야구한번 본거이후로",
          "original_form": "저누어렸을때 야구한번 본거이후로",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028526.1.22",
          "form": "본적이없는데",
          "original_form": "본적이없는데",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028526.1.23",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028526.1.24",
          "form": "기회되면 배구 직과 ㄴ해보세요",
          "original_form": "기회되면 배구 직과 ㄴ해보세요",
