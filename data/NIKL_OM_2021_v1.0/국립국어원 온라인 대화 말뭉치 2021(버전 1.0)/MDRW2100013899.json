      "utterance": [
        {
          "id": "MDRW2100013899.1.1",
          "form": "공연이나 전시회 보시는 거 좋아하시나요??",
          "original_form": "공연이나 전시회 보시는 거 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013899.1.2",
          "form": "네ㅋㅋ근데 요즘에는 못가보고있어요ㅜ",
          "original_form": "네ㅋㅋ근데 요즘에는 못가보고있어요ㅜ",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013899.1.3",
          "form": "오 어떤 장르 좋아하세요??",
          "original_form": "오 어떤 장르 좋아하세요??",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013899.1.4",
          "form": "뮤지컬이랑 연극이요ㅋㅋ",
          "original_form": "뮤지컬이랑 연극이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013899.1.5",
          "form": "오 제가 연극은 본 적이 얼마 없는데",
          "original_form": "오 제가 연극은 본 적이 얼마 없는데",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013899.1.6",
          "form": "주로 어디 가서 보시나요??",
          "original_form": "주로 어디 가서 보시나요??",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013899.1.7",
          "form": "대학로요ㅋㅋ",
          "original_form": "대학로요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013899.1.8",
          "form": "홍대에서도 본 적 있구요",
          "original_form": "홍대에서도 본 적 있구요",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013899.1.9",
          "form": "역시 대학로가 연극 많이 하나보네요",
          "original_form": "역시 대학로가 연극 많이 하나보네요",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013899.1.10",
          "form": "그쵸ㅋㅋ대학로는 진짜 연극 많이해요",
          "original_form": "그쵸ㅋㅋ대학로는 진짜 연극 많이해요",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013899.1.11",
          "form": "저도 대학로에서 연극 한 번 본 적 있어요",
          "original_form": "저도 대학로에서 연극 한 번 본 적 있어요",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013899.1.12",
          "form": "제가 고른 건 아니고.. 학교에서 단체로 가서..ㅎㅎ",
          "original_form": "제가 고른 건 아니고.. 학교에서 단체로 가서..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013899.1.13",
          "form": "저도 학과에서 단체로 홍대 가서 연극 봤었어요ㅋㅋ",
          "original_form": "저도 학과에서 단체로 홍대 가서 연극 봤었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013899.1.14",
          "form": "그 때 홍대에서 당신은 주인공인가",
          "original_form": "그 때 홍대에서 당신은 주인공인가",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013899.1.15",
          "form": "그거 했는데 관객참여 100퍼더라구요ㅋㅋ",
          "original_form": "그거 했는데 관객참여 100퍼더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013899.1.16",
          "form": "옼ㅋㅋㅋㅋㅋㅋ 제가 갔던 공연은 그런 건 없었어요ㅋㅋ",
          "original_form": "옼ㅋㅋㅋㅋㅋㅋ 제가 갔던 공연은 그런 건 없었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013899.1.17",
          "form": "사실 되게 오래돼서 잘 기억도 안 나요ㅋㅋ",
          "original_form": "사실 되게 오래돼서 잘 기억도 안 나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013899.1.18",
          "form": "ㅋㅋㅋㅋ뮤지컬은 보신 적 있나요?",
          "original_form": "ㅋㅋㅋㅋ뮤지컬은 보신 적 있나요?",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013899.1.19",
          "form": "네네 주로 유명한 거 봤었어요ㅋㅋ",
          "original_form": "네네 주로 유명한 거 봤었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
