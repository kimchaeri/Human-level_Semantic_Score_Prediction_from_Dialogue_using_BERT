      "utterance": [
        {
          "id": "MDRW2100002708.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002708.1.2",
          "form": "네! 운동하죠!",
          "original_form": "네! 운동하죠!",
          "speaker_id": "2",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002708.1.3",
          "form": "어떤?",
          "original_form": "어떤?",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002708.1.4",
          "form": "축구도 하고 홈트도 하고 다양하게 해요!",
          "original_form": "축구도 하고 홈트도 하고 다양하게 해요!",
          "speaker_id": "2",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002708.1.5",
          "form": "코로나때문에 집에만 있어요",
          "original_form": "코로나때문에 집에만 있어요",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002708.1.6",
          "form": "아하 ㅠㅠ",
          "original_form": "아하 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002708.1.7",
          "form": "저도 요근래에눈",
          "original_form": "저도 요근래에눈",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002708.1.8",
          "form": "거의 집에서 홈트로 운동해요!",
          "original_form": "거의 집에서 홈트로 운동해요!",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002708.1.9",
          "form": "플랭크해요?",
          "original_form": "플랭크해요?",
          "speaker_id": "1",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002708.1.10",
          "form": "네 플랭크도 하고",
          "original_form": "네 플랭크도 하고",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002708.1.11",
          "form": "요가랑",
          "original_form": "요가랑",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002708.1.12",
          "form": "등등 다 하는거같아요!",
          "original_form": "등등 다 하는거같아요!",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002708.1.13",
          "form": "부지런하네요",
          "original_form": "부지런하네요",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002708.1.14",
          "form": "뱃살은 절대로 빼고싶어서",
          "original_form": "뱃살은 절대로 빼고싶어서",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002708.1.15",
          "form": "열심히 하고있습니다 ㅎㅎ",
          "original_form": "열심히 하고있습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002708.1.16",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210820 15:23"
        },
        {
          "id": "MDRW2100002708.1.17",
          "form": "홈트 열심히 해서 같이 빼봐요!",
          "original_form": "홈트 열심히 해서 같이 빼봐요!",
          "speaker_id": "2",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002708.1.18",
          "form": "저도 2달넘게했는데 조금씩 빠지는게 보여서 포기안하고 쭉 가봐요~",
          "original_form": "저도 2달넘게했는데 조금씩 빠지는게 보여서 포기안하고 쭉 가봐요~",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002708.1.19",
