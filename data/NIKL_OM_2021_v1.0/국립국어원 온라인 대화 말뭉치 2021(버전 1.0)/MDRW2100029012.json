      "utterance": [
        {
          "id": "MDRW2100029012.1.1",
          "form": "저녁 메뉴 뭐였어요?",
          "original_form": "저녁 메뉴 뭐였어요?",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029012.1.2",
          "form": "저는 잔치국수와",
          "original_form": "저는 잔치국수와",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029012.1.3",
          "form": "부추전을 먹었슴니댜",
          "original_form": "부추전을 먹었슴니댜",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029012.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029012.1.5",
          "form": "저는 샤브샤브입니다",
          "original_form": "저는 샤브샤브입니다",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029012.1.6",
          "form": "잔치국수랑 부추전은 직접 만드셨나요?",
          "original_form": "잔치국수랑 부추전은 직접 만드셨나요?",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029012.1.7",
          "form": "오 샤브샤브",
          "original_form": "오 샤브샤브",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029012.1.8",
          "form": "네넹 만들어 묵었슴니다",
          "original_form": "네넹 만들어 묵었슴니다",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029012.1.9",
          "form": "맛나더군요",
          "original_form": "맛나더군요",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029012.1.10",
          "form": "샤브샤브 칼국수랑 죽 먹었나요?",
          "original_form": "샤브샤브 칼국수랑 죽 먹었나요?",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029012.1.11",
          "form": "칼국수만 먹었어요",
          "original_form": "칼국수만 먹었어요",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029012.1.12",
          "form": "왜...",
          "original_form": "왜...",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029012.1.13",
          "form": "죽은 계란이 없어서 포기",
          "original_form": "죽은 계란이 없어서 포기",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029012.1.14",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029012.1.15",
          "form": "죽 계란 읍어도 되는데",
          "original_form": "죽 계란 읍어도 되는데",
          "speaker_id": "1",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029012.1.16",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029012.1.17",
          "form": "죽을 배신 하셨어요",
          "original_form": "죽을 배신 하셨어요",
          "speaker_id": "1",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029012.1.18",
          "form": "아깝네요",
          "original_form": "아깝네요",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029012.1.19",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029012.1.20",
          "form": "햇반도 많은데",
          "original_form": "햇반도 많은데",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029012.1.21",
          "form": "슬퍼오",
          "original_form": "슬퍼오",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029012.1.22",
          "form": "내일 아침메뉴는 뭐에오???",
          "original_form": "내일 아침메뉴는 뭐에오???",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029012.1.23",
          "form": "내일은",
          "original_form": "내일은",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029012.1.24",
          "form": "아침은 공복임니댜",
          "original_form": "아침은 공복임니댜",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029012.1.25",
          "form": "아침은 안먹...",
          "original_form": "아침은 안먹...",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029012.1.26",
          "form": "님은 뭐 뭐묵나오",
          "original_form": "님은 뭐 뭐묵나오",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029012.1.27",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029012.1.28",
          "form": "공기와 물을 먹고 삽니다",
          "original_form": "공기와 물을 먹고 삽니다",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029012.1.29",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029012.1.30",
          "form": "왜 안묵어요",
          "original_form": "왜 안묵어요",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029012.1.31",
          "form": "샤인머스캣맛 마셔봄??",
          "original_form": "샤인머스캣맛 마셔봄??",
          "speaker_id": "2",
