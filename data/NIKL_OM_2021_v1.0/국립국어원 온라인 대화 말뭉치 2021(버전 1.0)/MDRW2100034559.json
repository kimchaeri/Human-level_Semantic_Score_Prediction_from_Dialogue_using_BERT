      "utterance": [
        {
          "id": "MDRW2100034559.1.1",
          "form": "하시는 운동있으세요?",
          "original_form": "하시는 운동있으세요?",
          "speaker_id": "1",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100034559.1.2",
          "form": "아니오 꾸준히 해야하는데 이게 힘드네요 ㅠ",
          "original_form": "아니오 꾸준히 해야하는데 이게 힘드네요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100034559.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100034559.1.4",
          "form": "저는 실내자전거만요 ㅋㅋ",
          "original_form": "저는 실내자전거만요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100034559.1.5",
          "form": "우와 실내자전걸 어떤걸 쓰세요? 전 너무 싼걸 샀더니 ㅋㅋ 별로 안좋아요 제건 브랜드도 없어요",
          "original_form": "우와 실내자전걸 어떤걸 쓰세요? 전 너무 싼걸 샀더니 ㅋㅋ 별로 안좋아요 제건 브랜드도 없어요",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100034559.1.6",
          "form": "저 젤싼걸걸요? ㅋㅋㅋㅋ 십만원초반대에요",
          "original_form": "저 젤싼걸걸요? ㅋㅋㅋㅋ 십만원초반대에요",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100034559.1.7",
          "form": "네이버랭킹순으로 샀어요",
          "original_form": "네이버랭킹순으로 샀어요",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100034559.1.8",
          "form": "그럼 제거랑 비슷한 가격댄데 그럼 제가 게을러서 운동을 잘 안하는걸루,,, ㅠㅋㅋ",
          "original_form": "그럼 제거랑 비슷한 가격댄데 그럼 제가 게을러서 운동을 잘 안하는걸루,,, ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100034559.1.9",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100034559.1.10",
          "form": "보통 옷걸이가 되죠 고급옷걸이요 ㅋㅋㅋㅋ",
          "original_form": "보통 옷걸이가 되죠 고급옷걸이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100034559.1.11",
          "form": "ㅠ 저 거기에 전기피라채도 올려놓고 티비보다가 간식도 올려놔요",
          "original_form": "ㅠ 저 거기에 전기피라채도 올려놓고 티비보다가 간식도 올려놔요",
          "speaker_id": "2",
          "time": "20210828 15:34"
        },
        {
          "id": "MDRW2100034559.1.12",
          "form": "전기파리채 저도 써요 ㅋㅋㅋㅋㅋ",
          "original_form": "전기파리채 저도 써요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100034559.1.13",
          "form": "간식은 새로운 쓸모네요 ㅋㅋㅋ",
          "original_form": "간식은 새로운 쓸모네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100034559.1.14",
          "form": "안장이 생각보다 넓더라고요 ㅋㅋㅋ",
          "original_form": "안장이 생각보다 넓더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100034559.1.15",
          "form": "아 저희건 안장이 콩알만해서",
          "original_form": "아 저희건 안장이 콩알만해서",
          "speaker_id": "1",
          "time": "20210828 15:37"
        },
        {
          "id": "MDRW2100034559.1.16",
          "form": "맨날 엉덩이 아파요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맨날 엉덩이 아파요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:37"
        },
        {
          "id": "MDRW2100034559.1.17",
          "form": "제건 엉덩이는 넓은데 숙이는 디자인이라서 허리가 아파요 ㅋㅋ",
          "original_form": "제건 엉덩이는 넓은데 숙이는 디자인이라서 허리가 아파요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:38"
        },
        {
          "id": "MDRW2100034559.1.18",
          "form": "아 저희건 그냥 꼿꼿한거에요 ㅋㅋㅋ",
          "original_form": "아 저희건 그냥 꼿꼿한거에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:38"
        }
      ]
