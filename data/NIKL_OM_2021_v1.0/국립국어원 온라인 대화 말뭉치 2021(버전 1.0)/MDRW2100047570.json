      "utterance": [
        {
          "id": "MDRW2100047570.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저녁은 뭘드셧어요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저녁은 뭘드셧어요?",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100047570.1.2",
          "form": "치킨요 ㅋㅋ",
          "original_form": "치킨요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100047570.1.3",
          "form": "아파트 장이서서",
          "original_form": "아파트 장이서서",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100047570.1.4",
          "form": "아들이 사왔어요",
          "original_form": "아들이 사왔어요",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100047570.1.5",
          "form": "아 밥먹으려고 밥 다하고",
          "original_form": "아 밥먹으려고 밥 다하고",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047570.1.6",
          "form": "치킨!! 훌륭한 메뉴선택이네요 ㅋㅋㅋ",
          "original_form": "치킨!! 훌륭한 메뉴선택이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047570.1.7",
          "form": "치킨 먹었네요",
          "original_form": "치킨 먹었네요",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047570.1.8",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047570.1.9",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047570.1.10",
          "form": "전 닭볶음탕 ㅋㅋㅋㅋ",
          "original_form": "전 닭볶음탕 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047570.1.11",
          "form": "어머 진짜 저녁메뉴로 젤좋ㄴ데요 ㅎㅎ",
          "original_form": "어머 진짜 저녁메뉴로 젤좋ㄴ데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047570.1.12",
          "form": "좋은데요",
          "original_form": "좋은데요",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047570.1.13",
          "form": "소주도?",
          "original_form": "소주도?",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047570.1.14",
          "form": "하셨어요?",
          "original_form": "하셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047570.1.15",
          "form": "술은 자제중이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "술은 자제중이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100047570.1.16",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100047570.1.17",
          "form": "저도 술은 한잔 진짜 딱한잔만해요",
          "original_form": "저도 술은 한잔 진짜 딱한잔만해요",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100047570.1.18",
          "form": "가끔",
          "original_form": "가끔",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100047570.1.19",
          "form": "저희 남편은 애주가고요",
          "original_form": "저희 남편은 애주가고요",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100047570.1.20",
          "form": "주량이 한잔이신거예요?",
          "original_form": "주량이 한잔이신거예요?",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100047570.1.21",
          "form": "닭볶음탕은 밥 한그릇 뚝딱이죠",
          "original_form": "닭볶음탕은 밥 한그릇 뚝딱이죠",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100047570.1.22",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100047570.1.23",
          "form": "소주한잔",
          "original_form": "소주한잔",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100047570.1.24",
          "form": "맥주 두잔",
          "original_form": "맥주 두잔",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100047570.1.25",
          "form": "예전엔 잘마셨는데 요샌 못마셔요",
          "original_form": "예전엔 잘마셨는데 요샌 못마셔요",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100047570.1.26",
          "form": "어떻게 되세요?주량이?",
          "original_form": "어떻게 되세요?주량이?",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100047570.1.27",
          "form": "저도 비슷해요",
          "original_form": "저도 비슷해요",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100047570.1.28",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100047570.1.29",
          "form": "맥주 두잔정도?",
          "original_form": "맥주 두잔정도?",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100047570.1.30",
          "form": "괜히 먹었다 머리 아플까 피해요 ㅎㅎ",
          "original_form": "괜히 먹었다 머리 아플까 피해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100047570.1.31",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100047570.1.32",
          "form": "딱좋아요 아딸딸하게",
          "original_form": "딱좋아요 아딸딸하게",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100047570.1.33",
          "form": "그래도 맥주랑 소주는 좀 마시는데",
          "original_form": "그래도 맥주랑 소주는 좀 마시는데",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100047570.1.34",
          "form": "막걸리는 ㄴㄴ...",
          "original_form": "막걸리는 ㄴㄴ...",
          "speaker_id": "2",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047570.1.35",
          "form": "뭐든 맛날때 까지만 묵어야죠",
          "original_form": "뭐든 맛날때 까지만 묵어야죠",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047570.1.36",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047570.1.37",
          "form": "막걸리마시면 앓아누워요",
          "original_form": "막걸리마시면 앓아누워요",
          "speaker_id": "2",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047570.1.38",
          "form": "저 입만되도",
          "original_form": "저 입만되도",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047570.1.39",
          "form": "두통으로 죽어요",
          "original_form": "두통으로 죽어요",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047570.1.40",
          "form": "똑같아요",
          "original_form": "똑같아요",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047570.1.41",
          "form": "막걸리는 진짜 쥐약이예요",
          "original_form": "막걸리는 진짜 쥐약이예요",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047570.1.42",
          "form": "맥주가 딱 맛나요",
          "original_form": "맥주가 딱 맛나요",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047570.1.43",
          "form": "저도 그래요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 그래요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100047570.1.44",
          "form": "와인도 못마셔요",
          "original_form": "와인도 못마셔요",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100047570.1.45",
          "form": "와인고 엄청 금방 훅가요",
          "original_form": "와인고 엄청 금방 훅가요",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100047570.1.46",
          "form": "와인도 우리나라꺼 반잔 마시고 땡이요 ㅎㅎ",
          "original_form": "와인도 우리나라꺼 반잔 마시고 땡이요 ㅎㅎ",
          "speaker_id": "1",
