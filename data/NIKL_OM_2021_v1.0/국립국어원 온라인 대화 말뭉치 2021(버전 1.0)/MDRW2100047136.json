      "utterance": [
        {
          "id": "MDRW2100047136.1.1",
          "form": "ㅎㅎ 웹툰 웹소설 뭐봐요!!",
          "original_form": "ㅎㅎ 웹툰 웹소설 뭐봐요!!",
          "speaker_id": "1",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100047136.1.2",
          "form": "아 증망 많이봅니다",
          "original_form": "아 증망 많이봅니다",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047136.1.3",
          "form": "장르별로 있는데 최근 좋아하는건 화산귀환이라는 작품이고",
          "original_form": "장르별로 있는데 최근 좋아하는건 화산귀환이라는 작품이고",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047136.1.4",
          "form": "로판도 많이 봐요",
          "original_form": "로판도 많이 봐요",
          "speaker_id": "2",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100047136.1.5",
          "form": "어떤거 주로 보세요?",
          "original_form": "어떤거 주로 보세요?",
          "speaker_id": "2",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100047136.1.6",
          "form": "오 처음보는 작품이네요",
          "original_form": "오 처음보는 작품이네요",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100047136.1.7",
          "form": "저는 여러가지 다양하게 보는것같아요",
          "original_form": "저는 여러가지 다양하게 보는것같아요",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100047136.1.8",
          "form": "잼나답니다",
          "original_form": "잼나답니다",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100047136.1.9",
          "form": "한번 볼개요 ㅎㅎ",
          "original_form": "한번 볼개요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100047136.1.10",
          "form": "원래 웹소설로 1억뷰 찍었는데",
          "original_form": "원래 웹소설로 1억뷰 찍었는데",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100047136.1.11",
          "form": "요즘 볼개 떨어져서",
          "original_form": "요즘 볼개 떨어져서",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100047136.1.12",
          "form": "최근에 웹툰화 됐어요",
          "original_form": "최근에 웹툰화 됐어요",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100047136.1.13",
          "form": "어느 장르 좋아하세요?!",
          "original_form": "어느 장르 좋아하세요?!",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100047136.1.14",
          "form": "와 인기 많나봐요",
          "original_form": "와 인기 많나봐요",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100047136.1.15",
          "form": "저 일상.학원 많이보고",
          "original_form": "저 일상.학원 많이보고",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100047136.1.16",
          "form": "로맨스?",
          "original_form": "로맨스?",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100047136.1.17",
          "form": "판타지?",
          "original_form": "판타지?",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100047136.1.18",
          "form": "스토리만 쟈밌으면 장르안가리고 잘봐요",
          "original_form": "스토리만 쟈밌으면 장르안가리고 잘봐요",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100047136.1.19",
          "form": "일상이면 모죠의 일기도 보시나요",
          "original_form": "일상이면 모죠의 일기도 보시나요",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100047136.1.20",
          "form": "대학일기 먼가 보실것 같고",
          "original_form": "대학일기 먼가 보실것 같고",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100047136.1.21",
          "form": "전독시가 요즘 인기많고",
          "original_form": "전독시가 요즘 인기많고",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100047136.1.22",
          "form": "판타지중에 제일 유명한게..그 나혼자만 레벨업이 유명하더라고요",
          "original_form": "판타지중에 제일 유명한게..그 나혼자만 레벨업이 유명하더라고요",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100047136.1.23",
          "form": "아 모죠일기 너무 쟈밌죠",
          "original_form": "아 모죠일기 너무 쟈밌죠",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100047136.1.24",
          "form": "나혼자만 레벨업? 쟈밌나요?",
          "original_form": "나혼자만 레벨업? 쟈밌나요?",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100047136.1.25",
          "form": "전독시는 보다가 멈췄는데..",
          "original_form": "전독시는 보다가 멈췄는데..",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100047136.1.26",
          "form": "내용을 까먹어서 큰일났어요",
          "original_form": "내용을 까먹어서 큰일났어요",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100047136.1.27",
          "form": "한 100화만 더보먄 됐는데..",
          "original_form": "한 100화만 더보먄 됐는데..",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100047136.1.28",
          "form": "헙헙",
          "original_form": "헙헙",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100047136.1.29",
          "form": "전독시 재밋고",
          "original_form": "전독시 재밋고",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100047136.1.30",
          "form": "근데 전독시보다 나혼렙이 진짜 잼나요",
          "original_form": "근데 전독시보다 나혼렙이 진짜 잼나요",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100047136.1.31",
          "form": "진짜 추천합니다",
          "original_form": "진짜 추천합니다",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100047136.1.32",
          "form": "나혼렙이 3억뷰인가 그러더라고요",
          "original_form": "나혼렙이 3억뷰인가 그러더라고요",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100047136.1.33",
          "form": "와 진짜 재밌나봐요",
          "original_form": "와 진짜 재밌나봐요",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100047136.1.34",
          "form": "저 볼게 잔뜩 늘어났어요",
          "original_form": "저 볼게 잔뜩 늘어났어요",
          "speaker_id": "1",
          "time": "20210822 17:37"
        }
      ]
    }
