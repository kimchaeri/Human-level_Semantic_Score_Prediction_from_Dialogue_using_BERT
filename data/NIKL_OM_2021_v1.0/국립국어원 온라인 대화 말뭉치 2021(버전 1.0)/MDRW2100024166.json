      "utterance": [
        {
          "id": "MDRW2100024166.1.1",
          "form": "학생 때 했었어요",
          "original_form": "학생 때 했었어요",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024166.1.2",
          "form": "저도 그랬는데 얼마전에 부활해서 계정찾으려고요",
          "original_form": "저도 그랬는데 얼마전에 부활해서 계정찾으려고요",
          "speaker_id": "1",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024166.1.3",
          "form": "저도 찾으려고요",
          "original_form": "저도 찾으려고요",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024166.1.4",
          "form": "흑역사 지워야죠",
          "original_form": "흑역사 지워야죠",
          "speaker_id": "1",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100024166.1.5",
          "form": "저도 그러려공ᆢㄷ",
          "original_form": "저도 그러려공ᆢㄷ",
          "speaker_id": "2",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100024166.1.6",
          "form": "하지만 추억도 있으니까 개인소장하려고요",
          "original_form": "하지만 추억도 있으니까 개인소장하려고요",
          "speaker_id": "1",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100024166.1.7",
          "form": "네 저도요",
          "original_form": "네 저도요",
          "speaker_id": "2",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100024166.1.8",
          "form": "싸이월드 일촌평도 남기고그랬나요",
          "original_form": "싸이월드 일촌평도 남기고그랬나요",
          "speaker_id": "1",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024166.1.9",
          "form": "남겼죠 배경음악도 듣고요",
          "original_form": "남겼죠 배경음악도 듣고요",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024166.1.10",
          "form": "맞아요 요즘 싸이월드 추억의 음악 듣기도 있더라고요",
          "original_form": "맞아요 요즘 싸이월드 추억의 음악 듣기도 있더라고요",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024166.1.11",
          "form": "저도 종종 들어요",
          "original_form": "저도 종종 들어요",
          "speaker_id": "2",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024166.1.12",
          "form": "도토리 선물도 생각나고하네요",
          "original_form": "도토리 선물도 생각나고하네요",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024166.1.13",
          "form": "많이 받아봤나요",
          "original_form": "많이 받아봤나요",
          "speaker_id": "2",
          "time": "20210830 22:55"
        },
