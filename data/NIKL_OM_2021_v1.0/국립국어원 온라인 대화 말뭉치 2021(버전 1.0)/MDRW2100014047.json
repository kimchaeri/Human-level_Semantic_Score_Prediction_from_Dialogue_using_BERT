      "utterance": [
        {
          "id": "MDRW2100014047.1.1",
          "form": "최근에 공연본거있어?",
          "original_form": "최근에 공연본거있어?",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014047.1.2",
          "form": "아니 본적 없어",
          "original_form": "아니 본적 없어",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014047.1.3",
          "form": "영화고 없어?",
          "original_form": "영화고 없어?",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014047.1.4",
          "form": "응 넌 본거있어?",
          "original_form": "응 넌 본거있어?",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014047.1.5",
          "form": "나도 없어 요즘 모가디슈가 재미있다던데",
          "original_form": "나도 없어 요즘 모가디슈가 재미있다던데",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014047.1.6",
          "form": "나도 재미있다는 애기는 들어봤어",
          "original_form": "나도 재미있다는 애기는 들어봤어",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014047.1.7",
          "form": "옹 시간되면 보자 ㅋㅋ",
          "original_form": "옹 시간되면 보자 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014047.1.8",
          "form": "보고 감상 말해주기",
          "original_form": "보고 감상 말해주기",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014047.1.9",
          "form": "감상 ㅇㅋ",
          "original_form": "감상 ㅇㅋ",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014047.1.10",
          "form": "다른 재미있는건 없어?",
          "original_form": "다른 재미있는건 없어?",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014047.1.11",
          "form": "글쎄 나도 요즘은 관심을 못가져서",
          "original_form": "글쎄 나도 요즘은 관심을 못가져서",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014047.1.12",
          "form": "이광수 나오는 영화도 있더라",
          "original_form": "이광수 나오는 영화도 있더라",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014047.1.13",
          "form": "이광수라 글쎄네 ㅋㅋㅋ",
          "original_form": "이광수라 글쎄네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014047.1.14",
          "form": "난 좋은데 차승원도 나온다하고 한 번 킬링타임용으로 볼만할거같아",
          "original_form": "난 좋은데 차승원도 나온다하고 한 번 킬링타임용으로 볼만할거같아",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014047.1.15",
          "form": "그거 감독 7광구 감독이다 ㅋㅋㅋㅋㅋ",
          "original_form": "그거 감독 7광구 감독이다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
