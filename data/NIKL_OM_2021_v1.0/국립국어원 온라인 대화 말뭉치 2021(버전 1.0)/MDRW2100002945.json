      "utterance": [
        {
          "id": "MDRW2100002945.1.1",
          "form": "유럽 가본적 있어?",
          "original_form": "유럽 가본적 있어?",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002945.1.2",
          "form": "아니 없어",
          "original_form": "아니 없어",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002945.1.3",
          "form": "너는 있어?",
          "original_form": "너는 있어?",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002945.1.4",
          "form": "나도 없어",
          "original_form": "나도 없어",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002945.1.5",
          "form": "한번은 가보고 싶은데",
          "original_form": "한번은 가보고 싶은데",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002945.1.6",
          "form": "너는 가보고싶어?",
          "original_form": "너는 가보고싶어?",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002945.1.7",
          "form": "나는 해외여행 가본적 없어서",
          "original_form": "나는 해외여행 가본적 없어서",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002945.1.8",
          "form": "어디든 가보고 싶어",
          "original_form": "어디든 가보고 싶어",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002945.1.9",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002945.1.10",
          "form": "그럼 가까운 일본부터 다음에 같이 가보자",
          "original_form": "그럼 가까운 일본부터 다음에 같이 가보자",
          "speaker_id": "1",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002945.1.11",
          "form": "30만원+- 10만원만 있으면 돼",
          "original_form": "30만원+- 10만원만 있으면 돼",
          "speaker_id": "1",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002945.1.12",
          "form": "돈 잘 모아두자",
          "original_form": "돈 잘 모아두자",
          "speaker_id": "1",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002945.1.13",
          "form": "가까워서 그런가 생각보다 저렴하네",
          "original_form": "가까워서 그런가 생각보다 저렴하네",
          "speaker_id": "2",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002945.1.14",
          "form": "응 가이드 없이 가면 더 싸게 갈 수도 있어",
          "original_form": "응 가이드 없이 가면 더 싸게 갈 수도 있어",
          "speaker_id": "1",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002945.1.15",
          "form": "근데 잘 모르니까 가이드 있는게 편하지",
          "original_form": "근데 잘 모르니까 가이드 있는게 편하지",
          "speaker_id": "1",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002945.1.16",
          "form": "아하 가이드 포함이구나",
          "original_form": "아하 가이드 포함이구나",
          "speaker_id": "2",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002945.1.17",
          "form": "응 포함이야",
          "original_form": "응 포함이야",
          "speaker_id": "1",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002945.1.18",
          "form": "가이드 없이 가봤는데 정말...힘들더라",
          "original_form": "가이드 없이 가봤는데 정말...힘들더라",
          "speaker_id": "1",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002945.1.19",
          "form": "아.. 그렇겠다",
          "original_form": "아.. 그렇겠다",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002945.1.20",
          "form": "소통도 힘들고",
          "original_form": "소통도 힘들고",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002945.1.21",
          "form": "길찾기도 힘들테고",
          "original_form": "길찾기도 힘들테고",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002945.1.22",
          "form": "식당을 못찾아서 몇시간을 쫄쫄 굶었어",
          "original_form": "식당을 못찾아서 몇시간을 쫄쫄 굶었어",
          "speaker_id": "1",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002945.1.23",
          "form": "아..힘들었겠다",
          "original_form": "아..힘들었겠다",
          "speaker_id": "2",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002945.1.24",
          "form": "길바닥에서 울었어",
          "original_form": "길바닥에서 울었어",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002945.1.25",
          "form": "배고파서ㅋㅋㅋ",
          "original_form": "배고파서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:03"
        }
      ]
