      "utterance": [
        {
          "id": "MDRW2100043165.1.1",
          "form": "안녕안녕",
          "original_form": "안녕안녕",
          "speaker_id": "1",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043165.1.2",
          "form": "혹시 누누가 차은우니?",
          "original_form": "혹시 누누가 차은우니?",
          "speaker_id": "1",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043165.1.3",
          "form": "아니!!",
          "original_form": "아니!!",
          "speaker_id": "2",
          "time": "20210907 18:00"
        },
        {
          "id": "MDRW2100043165.1.4",
          "form": "아 아니구나 무슨 뜻이야?",
          "original_form": "아 아니구나 무슨 뜻이야?",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043165.1.5",
          "form": "누누씨라는 인스타에 귀여운그림올리는",
          "original_form": "누누씨라는 인스타에 귀여운그림올리는",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043165.1.6",
          "form": "계정!!",
          "original_form": "계정!!",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043165.1.7",
          "form": "우와 정말",
          "original_form": "우와 정말",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043165.1.8",
          "form": "누누씨 찾아봐야겠다",
          "original_form": "누누씨 찾아봐야겠다",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043165.1.9",
          "form": "나는 차은우 팬이라 누누가 차은우 인줄 알았어",
          "original_form": "나는 차은우 팬이라 누누가 차은우 인줄 알았어",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043165.1.10",
          "form": "넘  덕후 같았지 미안 ㅋㅋㅋㅋ",
          "original_form": "넘  덕후 같았지 미안 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043165.1.11",
          "form": "아니야ㅋㅋㅋ",
          "original_form": "아니야ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043165.1.12",
          "form": "난 연예인에 큰 관심이 없긴한데",
          "original_form": "난 연예인에 큰 관심이 없긴한데",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043165.1.13",
          "form": "차은우 잘생겼어",
          "original_form": "차은우 잘생겼어",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043165.1.14",
          "form": "와 그렇구나",
          "original_form": "와 그렇구나",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043165.1.15",
          "form": "최근 본 영화 잇어?",
          "original_form": "최근 본 영화 잇어?",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043165.1.16",
          "form": "아니아니ㅠ",
          "original_form": "아니아니ㅠ",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043165.1.17",
          "form": "영화 보고싶다...!",
          "original_form": "영화 보고싶다...!",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043165.1.18",
          "form": "나는 에밀리인파리 봤어",
          "original_form": "나는 에밀리인파리 봤어",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043165.1.19",
          "form": "극장은 노노",
          "original_form": "극장은 노노",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043165.1.20",
          "form": "에밀리인파리가 뭐야?",
          "original_form": "에밀리인파리가 뭐야?",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043165.1.21",
          "form": "최근에 나온 영화?",
          "original_form": "최근에 나온 영화?",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043165.1.22",
          "form": "아 넷플릭스에서 하는 시리즈야",
          "original_form": "아 넷플릭스에서 하는 시리즈야",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043165.1.23",
          "form": "작년에 나왔을걸?",
          "original_form": "작년에 나왔을걸?",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043165.1.24",
          "form": "넷플릭스 가족계정있는데",
          "original_form": "넷플릭스 가족계정있는데",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043165.1.25",
          "form": "어어 그거로 뭐봐?",
          "original_form": "어어 그거로 뭐봐?",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043165.1.26",
          "form": "휴대폰 용량 모자라서 삭제했거든ㅠ",
          "original_form": "휴대폰 용량 모자라서 삭제했거든ㅠ",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043165.1.27",
          "form": "다시 깔아야겠다",
          "original_form": "다시 깔아야겠다",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043165.1.28",
          "form": "아아그랬구나",
          "original_form": "아아그랬구나",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043165.1.29",
          "form": "넷플릭스 볼거리 추천해줘",
          "original_form": "넷플릭스 볼거리 추천해줘",
          "speaker_id": "2",
          "time": "20210907 18:05"
        }
      ]
    }
