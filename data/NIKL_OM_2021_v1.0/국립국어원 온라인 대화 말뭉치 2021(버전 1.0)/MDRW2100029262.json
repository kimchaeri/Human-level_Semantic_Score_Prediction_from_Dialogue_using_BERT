      "utterance": [
        {
          "id": "MDRW2100029262.1.1",
          "form": "사고싶은물건있으신가요",
          "original_form": "사고싶은물건있으신가요",
          "speaker_id": "1",
          "time": "20210903 00:49"
        },
        {
          "id": "MDRW2100029262.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210903 00:49"
        },
        {
          "id": "MDRW2100029262.1.3",
          "form": "지금 사고 싶은 물건은 가을옷",
          "original_form": "지금 사고 싶은 물건은 가을옷",
          "speaker_id": "2",
          "time": "20210903 00:50"
        },
        {
          "id": "MDRW2100029262.1.4",
          "form": "가을이 옷이 가장 이쁘죠",
          "original_form": "가을이 옷이 가장 이쁘죠",
          "speaker_id": "1",
          "time": "20210903 00:50"
        },
        {
          "id": "MDRW2100029262.1.5",
          "form": "인데 오늘 보러갔었다가 실패하고 왔어요",
          "original_form": "인데 오늘 보러갔었다가 실패하고 왔어요",
          "speaker_id": "2",
          "time": "20210903 00:50"
        },
        {
          "id": "MDRW2100029262.1.6",
          "form": "어떤거사고싶으세요 ?",
          "original_form": "어떤거사고싶으세요 ?",
          "speaker_id": "1",
          "time": "20210903 00:50"
        },
        {
          "id": "MDRW2100029262.1.7",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210903 00:50"
        },
        {
          "id": "MDRW2100029262.1.8",
          "form": "마땅한게 없었나요?",
          "original_form": "마땅한게 없었나요?",
          "speaker_id": "1",
          "time": "20210903 00:50"
        },
        {
          "id": "MDRW2100029262.1.9",
          "form": "유행하는 옷들이 안어울려요",
          "original_form": "유행하는 옷들이 안어울려요",
          "speaker_id": "2",
          "time": "20210903 00:50"
        },
        {
          "id": "MDRW2100029262.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:50"
        },
        {
          "id": "MDRW2100029262.1.11",
          "form": "이번 시즌은 글렀어요",
          "original_form": "이번 시즌은 글렀어요",
          "speaker_id": "2",
          "time": "20210903 00:50"
        },
        {
          "id": "MDRW2100029262.1.12",
          "form": "요즘 유행하는 옷이 뭔지도 몰라요 저,,",
          "original_form": "요즘 유행하는 옷이 뭔지도 몰라요 저,,",
          "speaker_id": "1",
          "time": "20210903 00:51"
        },
        {
          "id": "MDRW2100029262.1.13",
          "form": "그냥 사러 가보시면 느낌 오실거에요...",
          "original_form": "그냥 사러 가보시면 느낌 오실거에요...",
          "speaker_id": "2",
          "time": "20210903 00:51"
        },
        {
          "id": "MDRW2100029262.1.14",
          "form": "무난한거 찾아도 없어요",
          "original_form": "무난한거 찾아도 없어요",
          "speaker_id": "2",
          "time": "20210903 00:51"
        },
        {
          "id": "MDRW2100029262.1.15",
          "form": "한번 외출해볼게요,,,",
          "original_form": "한번 외출해볼게요,,,",
          "speaker_id": "1",
          "time": "20210903 00:51"
        },
        {
          "id": "MDRW2100029262.1.16",
          "form": "ㅋㅋㅋㅋ 가을옷도 밑둥이 잘렸어요",
          "original_form": "ㅋㅋㅋㅋ 가을옷도 밑둥이 잘렸어요",
          "speaker_id": "2",
          "time": "20210903 00:51"
        },
        {
          "id": "MDRW2100029262.1.17",
          "form": "아이고,..",
          "original_form": "아이고,..",
          "speaker_id": "1",
          "time": "20210903 00:51"
        },
        {
          "id": "MDRW2100029262.1.18",
          "form": "안갈게요,,,",
          "original_form": "안갈게요,,,",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100029262.1.19",
          "form": "여름옷에서 색이 변형됐답니다",
          "original_form": "여름옷에서 색이 변형됐답니다",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100029262.1.20",
          "form": "원단이 반이면 가격도 반이던가",
          "original_form": "원단이 반이면 가격도 반이던가",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100029262.1.21",
          "form": "팔은 길어졌는데",
          "original_form": "팔은 길어졌는데",
          "speaker_id": "2",
          "time": "20210903 00:52"
        }
      ]
