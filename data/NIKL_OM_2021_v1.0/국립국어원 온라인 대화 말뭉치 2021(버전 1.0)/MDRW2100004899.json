      "utterance": [
        {
          "id": "MDRW2100004899.1.1",
          "form": "나 있지, 지금 엄청 살까말까 고민하는게 있어",
          "original_form": "나 있지, 지금 엄청 살까말까 고민하는게 있어",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004899.1.2",
          "form": "금속 부품 조립해서 완성하는 상품인데",
          "original_form": "금속 부품 조립해서 완성하는 상품인데",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004899.1.3",
          "form": "그냥 다 만들어도 예쁘기만 한 장식품이거든?",
          "original_form": "그냥 다 만들어도 예쁘기만 한 장식품이거든?",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004899.1.4",
          "form": "가격이 저렴한 것도 아니고",
          "original_form": "가격이 저렴한 것도 아니고",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004899.1.5",
          "form": "뭐지",
          "original_form": "뭐지",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004899.1.6",
          "form": "그래도 사고싶어 어떡해...",
          "original_form": "그래도 사고싶어 어떡해...",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004899.1.7",
          "form": "그게 뭔데ㅋㅋ",
          "original_form": "그게 뭔데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004899.1.8",
          "form": "설명으론 모르겠어",
          "original_form": "설명으론 모르겠어",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004899.1.9",
          "form": "금속 드림캐쳐!",
          "original_form": "금속 드림캐쳐!",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004899.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004899.1.11",
          "form": "엄청 번쩍거려 예뻐",
          "original_form": "엄청 번쩍거려 예뻐",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004899.1.12",
          "form": "완성품이 아니라 조립이야?",
          "original_form": "완성품이 아니라 조립이야?",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004899.1.13",
          "form": "이런것도 있구나",
          "original_form": "이런것도 있구나",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004899.1.14",
          "form": "나중에 사진 보내줄게",
          "original_form": "나중에 사진 보내줄게",
          "speaker_id": "2",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004899.1.15",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004899.1.16",
          "form": "조립하는 또 그 과정이 재미있는거지",
          "original_form": "조립하는 또 그 과정이 재미있는거지",
          "speaker_id": "2",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004899.1.17",
          "form": "검색으로 보고있긴한데",
          "original_form": "검색으로 보고있긴한데",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004899.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004899.1.19",
          "form": "살까말까 고민중이야",
          "original_form": "살까말까 고민중이야",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004899.1.20",
          "form": "이뻐ㅠㅠ",
          "original_form": "이뻐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004899.1.21",
          "form": "고민될듯ㅠ",
          "original_form": "고민될듯ㅠ",
          "speaker_id": "1",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004899.1.22",
          "form": "응 너무 고민돼...가격이 만만치 않아서 더욱",
          "original_form": "응 너무 고민돼...가격이 만만치 않아서 더욱",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004899.1.23",
          "form": "5만원에 배송비 별도야",
          "original_form": "5만원에 배송비 별도야",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004899.1.24",
          "form": "5만원은 좀...고민이 돼",
          "original_form": "5만원은 좀...고민이 돼",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004899.1.25",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004899.1.26",
          "form": "비싸긴하다",
          "original_form": "비싸긴하다",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004899.1.27",
          "form": "생각보단",
          "original_form": "생각보단",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004899.1.28",
          "form": "그치, 내일까지 주문 받는데",
          "original_form": "그치, 내일까지 주문 받는데",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004899.1.29",
          "form": "아직까지 결정을 못했어",
          "original_form": "아직까지 결정을 못했어",
          "speaker_id": "2",
          "time": "20210821 19:21"
        }
      ]
    }
