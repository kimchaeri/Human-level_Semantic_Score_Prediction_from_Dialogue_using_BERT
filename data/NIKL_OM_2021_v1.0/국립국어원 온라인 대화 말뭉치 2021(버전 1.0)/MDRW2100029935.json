      "utterance": [
        {
          "id": "MDRW2100029935.1.1",
          "form": "최고시네여",
          "original_form": "최고시네여",
          "speaker_id": "1",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029935.1.2",
          "form": "주말부부 어떻게생각하심?",
          "original_form": "주말부부 어떻게생각하심?",
          "speaker_id": "2",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100029935.1.3",
          "form": "전싫어유",
          "original_form": "전싫어유",
          "speaker_id": "1",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100029935.1.4",
          "form": "외로움 많이 타시나 보네요",
          "original_form": "외로움 많이 타시나 보네요",
          "speaker_id": "2",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029935.1.5",
          "form": "왜냐면",
          "original_form": "왜냐면",
          "speaker_id": "1",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029935.1.6",
          "form": "남친이 장금이고 살림꾼이거등요",
          "original_form": "남친이 장금이고 살림꾼이거등요",
          "speaker_id": "1",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029935.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029935.1.8",
          "form": "제가 똥손이라 할줄아는게없어여",
          "original_form": "제가 똥손이라 할줄아는게없어여",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029935.1.9",
          "form": "이쁘신가봅니다",
          "original_form": "이쁘신가봅니다",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029935.1.10",
          "form": "그거면 됐죠",
          "original_form": "그거면 됐죠",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029935.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029935.1.12",
          "form": "넘치죠",
          "original_form": "넘치죠",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029935.1.13",
          "form": "넘칠정도인가요?",
          "original_form": "넘칠정도인가요?",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029935.1.14",
          "form": "그렇데요",
          "original_form": "그렇데요",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029935.1.15",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029935.1.16",
          "form": "무야호!네요 남친입장에선",
          "original_form": "무야호!네요 남친입장에선",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029935.1.17",
          "form": "성격도 좋으시고",
          "original_form": "성격도 좋으시고",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029935.1.18",
          "form": "스트레스 안받고",
          "original_form": "스트레스 안받고",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029935.1.19",
          "form": "1등 신부감 ",
          "original_form": "1등 신부감 ",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029935.1.20",
          "form": "자존감도 높은거 같아서 백점 만점에 만점드립닏다",
          "original_form": "자존감도 높은거 같아서 백점 만점에 만점드립닏다",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029935.1.21",
          "form": "감사해욤",
          "original_form": "감사해욤",
          "speaker_id": "1",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100029935.1.22",
          "form": "외모보다 마음은 더 이쁨요",
          "original_form": "외모보다 마음은 더 이쁨요",
          "speaker_id": "1",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100029935.1.23",
          "form": "식장은 어디로?",
          "original_form": "식장은 어디로?",
          "speaker_id": "2",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100029935.1.24",
          "form": "한옥웨딩 생각중이에요",
          "original_form": "한옥웨딩 생각중이에요",
          "speaker_id": "1",
          "time": "20210903 16:58"
        }
      ]
