      "utterance": [
        {
          "id": "MDRW2100002999.1.1",
          "form": "쇼핑 좋아하시나요",
          "original_form": "쇼핑 좋아하시나요",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002999.1.2",
          "form": "돈이 많이 없어서 쇼핑하면 다이소에서 해용",
          "original_form": "돈이 많이 없어서 쇼핑하면 다이소에서 해용",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002999.1.3",
          "form": "쇼핑 많이 하시나요?",
          "original_form": "쇼핑 많이 하시나요?",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002999.1.4",
          "form": "예 저는 인터넷쇼핑을 많이합니다",
          "original_form": "예 저는 인터넷쇼핑을 많이합니다",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002999.1.5",
          "form": "매일 접속합니다",
          "original_form": "매일 접속합니다",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002999.1.6",
          "form": "저는 부모님이랑 같이 살아서 엄마가 많이 쇼핑을 하세요",
          "original_form": "저는 부모님이랑 같이 살아서 엄마가 많이 쇼핑을 하세요",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002999.1.7",
          "form": "저는 부모님이 인터넷을 할 줄 몰라서",
          "original_form": "저는 부모님이 인터넷을 할 줄 몰라서",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002999.1.8",
          "form": "제가 대신 주문합니다",
          "original_form": "제가 대신 주문합니다",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002999.1.9",
          "form": "오호.. 저는 제가 식재료 같은거 사면 엄마가 되게 싫어하시더라구요 냉장고 관리 안된다고",
          "original_form": "오호.. 저는 제가 식재료 같은거 사면 엄마가 되게 싫어하시더라구요 냉장고 관리 안된다고",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002999.1.10",
          "form": "재료사면 버리는 일이 많아서 요리를 잘 안하게되더라구요",
          "original_form": "재료사면 버리는 일이 많아서 요리를 잘 안하게되더라구요",
          "speaker_id": "2",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100002999.1.11",
          "form": "쇼핑몰은 어디를 주로 이용하세요?",
          "original_form": "쇼핑몰은 어디를 주로 이용하세요?",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100002999.1.12",
          "form": "티몬과 지마켓을 이용합니다",
          "original_form": "티몬과 지마켓을 이용합니다",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100002999.1.13",
          "form": "항상 쓰는곳이 편합니다",
          "original_form": "항상 쓰는곳이 편합니다",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100002999.1.14",
          "form": "쿠팡은 이용 잘 안하시는군요~",
          "original_form": "쿠팡은 이용 잘 안하시는군요~",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100002999.1.15",
          "form": "쿠팡은 문제가 많다고해서 잘 안씁니다",
          "original_form": "쿠팡은 문제가 많다고해서 잘 안씁니다",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100002999.1.16",
          "form": "불매운동까지는 아니지만",
          "original_form": "불매운동까지는 아니지만",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100002999.1.17",
          "form": "맞아요 저도 쿠팡 정말 안좋아해요 사실",
          "original_form": "맞아요 저도 쿠팡 정말 안좋아해요 사실",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002999.1.18",
          "form": "남들보다 더 싫어합니다 쿠팡",
          "original_form": "남들보다 더 싫어합니다 쿠팡",
          "speaker_id": "1",
          "time": "20210820 17:33"
        }
