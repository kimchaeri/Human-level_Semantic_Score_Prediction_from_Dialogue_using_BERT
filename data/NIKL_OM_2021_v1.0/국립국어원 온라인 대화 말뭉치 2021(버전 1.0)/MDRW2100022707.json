      "utterance": [
        {
          "id": "MDRW2100022707.1.1",
          "form": "저는 여기 겉절이도 너무 맛있더라구요!!!!",
          "original_form": "저는 여기 겉절이도 너무 맛있더라구요!!!!",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022707.1.2",
          "form": "맞아요 등촌샤브..!!!",
          "original_form": "맞아요 등촌샤브..!!!",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022707.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022707.1.4",
          "form": "국물도 얼큰하니ㅠㅜ",
          "original_form": "국물도 얼큰하니ㅠㅜ",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022707.1.5",
          "form": "겉절이가 등촌샤브의 완성",
          "original_form": "겉절이가 등촌샤브의 완성",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022707.1.6",
          "form": "마자여",
          "original_form": "마자여",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022707.1.7",
          "form": "국물 ㅜ ㅜ",
          "original_form": "국물 ㅜ ㅜ",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022707.1.8",
          "form": "진짜여.. 맛잘알! ㅋㅋㅋㅋㅋ",
          "original_form": "진짜여.. 맛잘알! ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022707.1.9",
          "form": "갑자기 배고파지네여",
          "original_form": "갑자기 배고파지네여",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022707.1.10",
          "form": "뭐라도 얼른 배달시켜야겠어요",
          "original_form": "뭐라도 얼른 배달시켜야겠어요",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022707.1.11",
          "form": "아아ㅜㅜㅜ 꼬들꼬들한 볶음밥 먹고싶어요ㅠㅠ",
          "original_form": "아아ㅜㅜㅜ 꼬들꼬들한 볶음밥 먹고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022707.1.12",
          "form": "그 현대백화점이나 아울렛에 1인 샤브집 있거든요@!",
          "original_form": "그 현대백화점이나 아울렛에 1인 샤브집 있거든요@!",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022707.1.13",
          "form": "요기도 완전 맛집 ㅠㅠ",
          "original_form": "요기도 완전 맛집 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022707.1.14",
          "form": "아 볶음밥은 식후 볶음밥입니다..",
          "original_form": "아 볶음밥은 식후 볶음밥입니다..",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022707.1.15",
          "form": "저는 여기 안뒤로 여기만 가요.... 무조건 1인샤브입니다",
          "original_form": "저는 여기 안뒤로 여기만 가요.... 무조건 1인샤브입니다",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022707.1.16",
          "form": "1인샤브 진짜 기억하겠습니다..",
          "original_form": "1인샤브 진짜 기억하겠습니다..",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022707.1.17",
          "form": "꼭간다..",
          "original_form": "꼭간다..",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022707.1.18",
          "form": "코시국에 위생적이라고 해서 궁금했는데",
          "original_form": "코시국에 위생적이라고 해서 궁금했는데",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022707.1.19",
          "form": "좀 좁아서 아는사람이랑 가기엔 대화하기도 좀 애매하구 그런데..",
          "original_form": "좀 좁아서 아는사람이랑 가기엔 대화하기도 좀 애매하구 그런데..",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022707.1.20",
          "form": "혼자라면 무조건입니다",
          "original_form": "혼자라면 무조건입니다",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022707.1.21",
          "form": "1인 마라탕은 해봤어도 아직 1인샤브는 못해본 나..",
          "original_form": "1인 마라탕은 해봤어도 아직 1인샤브는 못해본 나..",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022707.1.22",
          "form": "꼭 도전하겠습니다",
          "original_form": "꼭 도전하겠습니다",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022707.1.23",
          "form": "여기 저는 멸치육수먹거든요",
          "original_form": "여기 저는 멸치육수먹거든요",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022707.1.24",
          "form": "근데 다른 육수도 인기많더라구요!",
          "original_form": "근데 다른 육수도 인기많더라구요!",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022707.1.25",
          "form": "얼큰해물육수.. 가쓰오부시 ...",
          "original_form": "얼큰해물육수.. 가쓰오부시 ...",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022707.1.26",
          "form": "아 먹고싶다ㅠㅜ",
          "original_form": "아 먹고싶다ㅠㅜ",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022707.1.27",
          "form": "샤브 국물에 빠진 배추ㅠㅠ",
          "original_form": "샤브 국물에 빠진 배추ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022707.1.28",
          "form": "청경채ㅠㅠ",
