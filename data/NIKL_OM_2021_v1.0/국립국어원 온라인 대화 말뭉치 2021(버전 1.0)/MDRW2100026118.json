        {
          "id": "MDRW2100026118.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "2",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026118.1.2",
          "form": "가끔해요",
          "original_form": "가끔해요",
          "speaker_id": "1",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026118.1.3",
          "form": "중고 거래 하세요?",
          "original_form": "중고 거래 하세요?",
          "speaker_id": "1",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026118.1.4",
          "form": "네 당근이요",
          "original_form": "네 당근이요",
          "speaker_id": "2",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026118.1.5",
          "form": "저도 당근만 몇 번 해봤어요",
          "original_form": "저도 당근만 몇 번 해봤어요",
          "speaker_id": "1",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026118.1.6",
          "form": "뭐 파셨어요?",
          "original_form": "뭐 파셨어요?",
          "speaker_id": "1",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100026118.1.7",
          "form": "최근에 공기청정기요",
          "original_form": "최근에 공기청정기요",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026118.1.8",
          "form": "왜 파셨나요?",
          "original_form": "왜 파셨나요?",
          "speaker_id": "1",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100026118.1.9",
          "form": "2개라서요",
          "original_form": "2개라서요",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026118.1.10",
          "form": "아 그러면 잘 파셨네요",
          "original_form": "아 그러면 잘 파셨네요",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026118.1.11",
          "form": "공청기 한 번 써보니까 꼭 있어야겠더라고요",
          "original_form": "공청기 한 번 써보니까 꼭 있어야겠더라고요",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026118.1.12",
          "form": "차량용이라소형이에요",
          "original_form": "차량용이라소형이에요",
          "speaker_id": "2",
          "time": "20210902 07:19"
        },
        {
          "id": "MDRW2100026118.1.13",
          "form": "주로 파시는 편인가요?",
          "original_form": "주로 파시는 편인가요?",
          "speaker_id": "1",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100026118.1.14",
          "form": "네 팔기만요",
          "original_form": "네 팔기만요",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100026118.1.15",
          "form": "주로 어떤 거 파셨나요?",
          "original_form": "주로 어떤 거 파셨나요?",
          "speaker_id": "1",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100026118.1.16",
          "form": "공기청정기요 최긎데",
          "original_form": "공기청정기요 최긎데",
