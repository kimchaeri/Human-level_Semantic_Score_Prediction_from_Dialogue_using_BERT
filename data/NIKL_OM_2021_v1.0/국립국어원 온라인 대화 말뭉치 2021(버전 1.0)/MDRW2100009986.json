      "utterance": [
        {
          "id": "MDRW2100009986.1.1",
          "form": "동아리 듦?",
          "original_form": "동아리 듦?",
          "speaker_id": "2",
          "time": "20210823 12:47"
        },
        {
          "id": "MDRW2100009986.1.2",
          "form": "동아리2개들었음",
          "original_form": "동아리2개들었음",
          "speaker_id": "1",
          "time": "20210823 12:47"
        },
        {
          "id": "MDRW2100009986.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210823 12:47"
        },
        {
          "id": "MDRW2100009986.1.4",
          "form": "2개나??? 오ㅑ???",
          "original_form": "2개나??? 오ㅑ???",
          "speaker_id": "2",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009986.1.5",
          "form": "나는 동아리 뭐 있는지도 모름",
          "original_form": "나는 동아리 뭐 있는지도 모름",
          "speaker_id": "2",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009986.1.6",
          "form": "하나는동아리랑모임사이에",
          "original_form": "하나는동아리랑모임사이에",
          "speaker_id": "1",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009986.1.7",
          "form": "있는거고",
          "original_form": "있는거고",
          "speaker_id": "1",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009986.1.8",
          "form": "나머지하나는",
          "original_form": "나머지하나는",
          "speaker_id": "1",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009986.1.9",
          "form": "좀어이없게들었음",
          "original_form": "좀어이없게들었음",
          "speaker_id": "1",
          "time": "20210823 12:49"
        },
        {
          "id": "MDRW2100009986.1.10",
          "form": "엥 왜왜",
          "original_form": "엥 왜왜",
          "speaker_id": "2",
          "time": "20210823 12:49"
        },
        {
          "id": "MDRW2100009986.1.11",
          "form": "우리학교는 모든 학과가 1학년 때 교수님하고 1학기에 2번정도 필수로 상담해야하거든",
          "original_form": "우리학교는 모든 학과가 1학년 때 교수님하고 1학기에 2번정도 필수로 상담해야하거든",
          "speaker_id": "1",
          "time": "20210823 12:49"
        },
        {
          "id": "MDRW2100009986.1.12",
          "form": "그래서 상담했는데 외국 수의사 이런 얘기가 나온거야",
          "original_form": "그래서 상담했는데 외국 수의사 이런 얘기가 나온거야",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009986.1.13",
          "form": "그래서외국으로나가는방법얘기하다가",
          "original_form": "그래서외국으로나가는방법얘기하다가",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009986.1.14",
          "form": "갑자기관심있으면동아리를하나소개해주겠대",
          "original_form": "갑자기관심있으면동아리를하나소개해주겠대",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009986.1.15",
          "form": "그래서알겠다고해서동아리장선배한테까지",
          "original_form": "그래서알겠다고해서동아리장선배한테까지",
          "speaker_id": "1",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009986.1.16",
          "form": "교수님이직접연락해준거야",
          "original_form": "교수님이직접연락해준거야",
          "speaker_id": "1",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009986.1.17",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009986.1.18",
          "form": "그래서바로들어갔는데",
          "original_form": "그래서바로들어갔는데",
          "speaker_id": "1",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009986.1.19",
          "form": "알고보니까그냥스터디동아리였음",
          "original_form": "알고보니까그냥스터디동아리였음",
          "speaker_id": "1",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009986.1.20",
          "form": "교수님이오해하심",
          "original_form": "교수님이오해하심",
          "speaker_id": "1",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009986.1.21",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009986.1.22",
          "form": "뭐여",
          "original_form": "뭐여",
          "speaker_id": "2",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009986.1.23",
          "form": "중간에그만한다고",
          "original_form": "중간에그만한다고",
          "speaker_id": "1",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009986.1.24",
          "form": "할수도없어서",
          "original_form": "할수도없어서",
          "speaker_id": "1",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009986.1.25",
          "form": "그냥동아리에있는상태임",
          "original_form": "그냥동아리에있는상태임",
          "speaker_id": "1",
          "time": "20210823 12:52"
        },
        {
          "id": "MDRW2100009986.1.26",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:52"
        },
        {
          "id": "MDRW2100009986.1.27",
          "form": "뭐야 뭔데 그 전개 당황스럽네",
          "original_form": "뭐야 뭔데 그 전개 당황스럽네",
          "speaker_id": "2",
          "time": "20210823 12:52"
        },
        {
          "id": "MDRW2100009986.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나도동아리들어가서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나도동아리들어가서",
          "speaker_id": "1",
          "time": "20210823 12:52"
        },
        {
          "id": "MDRW2100009986.1.29",
          "form": "활동내용을보는데",
          "original_form": "활동내용을보는데",
          "speaker_id": "1",
          "time": "20210823 12:52"
        },
        {
          "id": "MDRW2100009986.1.30",
          "form": "뭔가이상한거임",
          "original_form": "뭔가이상한거임",
          "speaker_id": "1",
          "time": "20210823 12:52"
        },
        {
          "id": "MDRW2100009986.1.31",
          "form": "나도당황스러웠음",
          "original_form": "나도당황스러웠음",
          "speaker_id": "1",
          "time": "20210823 12:52"
        },
        {
          "id": "MDRW2100009986.1.32",
          "form": "오ㅓㅋㅋㅋㅋㅋㅋㅋ 진짜 무슨 그런 일이 있냐",
          "original_form": "오ㅓㅋㅋㅋㅋㅋㅋㅋ 진짜 무슨 그런 일이 있냐",
          "speaker_id": "2",
          "time": "20210823 12:53"
        },
        {
          "id": "MDRW2100009986.1.33",
          "form": "이야 스펙타클 하네",
          "original_form": "이야 스펙타클 하네",
          "speaker_id": "2",
          "time": "20210823 12:53"
        },
        {
          "id": "MDRW2100009986.1.34",
          "form": "차라리모르면모른다고하시지",
          "original_form": "차라리모르면모른다고하시지",
          "speaker_id": "1",
          "time": "20210823 12:53"
        },
        {
          "id": "MDRW2100009986.1.35",
          "form": "그교수님은",
          "original_form": "그교수님은",
          "speaker_id": "1",
          "time": "20210823 12:53"
        },
        {
          "id": "MDRW2100009986.1.36",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "1",
          "time": "20210823 12:53"
        },
        {
          "id": "MDRW2100009986.1.37",
          "form": "너는왜동아리안하냐",
          "original_form": "너는왜동아리안하냐",
          "speaker_id": "1",
          "time": "20210823 12:53"
        },
        {
          "id": "MDRW2100009986.1.38",
          "form": "1학년 1학기 밖에 안나갔는데 분과하긴 시간이 안되고 동아리 찾기도 귀찮아서 안함",
          "original_form": "1학년 1학기 밖에 안나갔는데 분과하긴 시간이 안되고 동아리 찾기도 귀찮아서 안함",
          "speaker_id": "2",
          "time": "20210823 12:54"
        },
        {
          "id": "MDRW2100009986.1.39",
          "form": "그거단톡에알려주지않음?",
          "original_form": "그거단톡에알려주지않음?",
          "speaker_id": "1",
          "time": "20210823 12:55"
        },
        {
          "id": "MDRW2100009986.1.40",
          "form": "올해1학기에도새로뽑았을텐데",
          "original_form": "올해1학기에도새로뽑았을텐데",
