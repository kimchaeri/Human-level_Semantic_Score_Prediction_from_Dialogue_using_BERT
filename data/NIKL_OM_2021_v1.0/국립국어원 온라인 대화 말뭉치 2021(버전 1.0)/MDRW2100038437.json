      "utterance": [
        {
          "id": "MDRW2100038437.1.1",
          "form": "안녕하세요! 최근에 쇼핑하셨나요?",
          "original_form": "안녕하세요! 최근에 쇼핑하셨나요?",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100038437.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100038437.1.3",
          "form": "최근에 이어폰 구매했어요",
          "original_form": "최근에 이어폰 구매했어요",
          "speaker_id": "2",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100038437.1.4",
          "form": "블루투스 이어폰인가용?!",
          "original_form": "블루투스 이어폰인가용?!",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100038437.1.5",
          "form": "네 무선이어폰이에요",
          "original_form": "네 무선이어폰이에요",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100038437.1.6",
          "form": "당근마켓 자주 이용하시나요?",
          "original_form": "당근마켓 자주 이용하시나요?",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100038437.1.7",
          "form": "아니요ㅋㅋㅋㅋ 구경만 해요ㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋ 구경만 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100038437.1.8",
          "form": "딱한번 사봤어요!",
          "original_form": "딱한번 사봤어요!",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100038437.1.9",
          "form": "자주 이용하시나요?",
          "original_form": "자주 이용하시나요?",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100038437.1.10",
          "form": "어떤거 사셨어요?",
          "original_form": "어떤거 사셨어요?",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100038437.1.11",
          "form": "필요한 물건 있을때 은근 이용하게 되는거같아요",
          "original_form": "필요한 물건 있을때 은근 이용하게 되는거같아요",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100038437.1.12",
          "form": "컵 샀어요!!ㅎㅎ",
          "original_form": "컵 샀어요!!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100038437.1.13",
          "form": "주로 어떤거 사세요??",
          "original_form": "주로 어떤거 사세요??",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100038437.1.14",
          "form": "생필품 다양하게 구입하고 있어요",
          "original_form": "생필품 다양하게 구입하고 있어요",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038437.1.15",
          "form": "전에는 와이파이 공유기 샀어요",
          "original_form": "전에는 와이파이 공유기 샀어요",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038437.1.16",
          "form": "그쳐ㅋㅋㅋㅋ 당근 구경하면 오만거 다있어서 생필품 다 장만할수 있겠더라구요",
          "original_form": "그쳐ㅋㅋㅋㅋ 당근 구경하면 오만거 다있어서 생필품 다 장만할수 있겠더라구요",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100038437.1.17",
          "form": "맞아요 새거 사기는 좀 아깝고 걍 작동만 하면 되는 물건이 필요할 때 자주 보게돼요",
          "original_form": "맞아요 새거 사기는 좀 아깝고 걍 작동만 하면 되는 물건이 필요할 때 자주 보게돼요",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100038437.1.18",
          "form": "가격도 저렴해서 좋아요",
          "original_form": "가격도 저렴해서 좋아요",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100038437.1.19",
          "form": "맞아요ㅋㅋㅋ 중고나라보다 당근이 훨싸고 근처니까 바로 받을수도 있고 여러모로 편한거같아요ㅌㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ 중고나라보다 당근이 훨싸고 근처니까 바로 받을수도 있고 여러모로 편한거같아요ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038437.1.20",
          "form": "그쵸 비싼물건 사도 직거래니까 안심할 수 있는게 커요",
          "original_form": "그쵸 비싼물건 사도 직거래니까 안심할 수 있는게 커요",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100038437.1.21",
          "form": "그치만 가끔가다 진상도 있다고 하니까 조심해야해요ㅋㅋㅋ",
          "original_form": "그치만 가끔가다 진상도 있다고 하니까 조심해야해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100038437.1.22",
          "form": "그런거같아요 지난번엔 거래 약속하고 바람맞은적 있어요 ㅋㅋㅋㅋ ㅠ",
          "original_form": "그런거같아요 지난번엔 거래 약속하고 바람맞은적 있어요 ㅋㅋㅋㅋ ㅠ",
