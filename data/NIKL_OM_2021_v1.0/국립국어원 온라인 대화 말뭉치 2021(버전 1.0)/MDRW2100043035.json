      "utterance": [
        {
          "id": "MDRW2100043035.1.1",
          "form": "도시락 방부제 냄새 안나고 괜찮네",
          "original_form": "도시락 방부제 냄새 안나고 괜찮네",
          "speaker_id": "2",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043035.1.2",
          "form": "그러게 맛있당",
          "original_form": "그러게 맛있당",
          "speaker_id": "1",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100043035.1.3",
          "form": "내일도 또 배달오나?",
          "original_form": "내일도 또 배달오나?",
          "speaker_id": "2",
          "time": "20210907 18:45"
        },
        {
          "id": "MDRW2100043035.1.4",
          "form": "아니 모레!",
          "original_form": "아니 모레!",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043035.1.5",
          "form": "화목토유",
          "original_form": "화목토유",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043035.1.6",
          "form": "아 그렇구만유",
          "original_form": "아 그렇구만유",
          "speaker_id": "2",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043035.1.7",
          "form": "내일은 집밥",
          "original_form": "내일은 집밥",
          "speaker_id": "2",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043035.1.8",
          "form": "그러합니다",
          "original_form": "그러합니다",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100043035.1.9",
          "form": "코울슬로 이런거 먹지 않으려나",
          "original_form": "코울슬로 이런거 먹지 않으려나",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100043035.1.10",
          "form": "그래야지",
          "original_form": "그래야지",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043035.1.11",
          "form": "돼지국밥도 그립네",
          "original_form": "돼지국밥도 그립네",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043035.1.12",
          "form": "그러게 돼지 많아",
          "original_form": "그러게 돼지 많아",
          "speaker_id": "1",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043035.1.13",
          "form": "엄마가 안드셔서 글치",
          "original_form": "엄마가 안드셔서 글치",
          "speaker_id": "1",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043035.1.14",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043035.1.15",
          "form": "비오는날 돼지국밥 딱인디",
          "original_form": "비오는날 돼지국밥 딱인디",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043035.1.16",
          "form": "엄마용 추어탕이나 사야할듯",
          "original_form": "엄마용 추어탕이나 사야할듯",
          "speaker_id": "1",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043035.1.17",
          "form": "응 추어탕 사야지",
          "original_form": "응 추어탕 사야지",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043035.1.18",
          "form": "엄마 가을 양식",
          "original_form": "엄마 가을 양식",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043035.1.19",
          "form": "며칠 잘 드실거 아냐",
          "original_form": "며칠 잘 드실거 아냐",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043035.1.20",
          "form": "매일드시나?",
          "original_form": "매일드시나?",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043035.1.21",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043035.1.22",
          "form": "매일 아침식사야",
          "original_form": "매일 아침식사야",
