      "utterance": [
        {
          "id": "MDRW2100013947.1.1",
          "form": "점심에 뭐 드셧어요?",
          "original_form": "점심에 뭐 드셧어요?",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013947.1.2",
          "form": "저 비빔밥 먹었어요ㅎㅎ",
          "original_form": "저 비빔밥 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013947.1.3",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013947.1.4",
          "form": "저는 고기 먹었어요!",
          "original_form": "저는 고기 먹었어요!",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100013947.1.5",
          "form": "저녁 몇시에 드세요?",
          "original_form": "저녁 몇시에 드세요?",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100013947.1.6",
          "form": "저는 그냥 빵으로 먹으려구여ㅎㅎ 점심을 많이 먹어서..",
          "original_form": "저는 그냥 빵으로 먹으려구여ㅎㅎ 점심을 많이 먹어서..",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100013947.1.7",
          "form": "저녁 메뉴 정하셨어요?",
          "original_form": "저녁 메뉴 정하셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100013947.1.8",
          "form": "저는 회 먹을려고요!",
          "original_form": "저는 회 먹을려고요!",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100013947.1.9",
          "form": "회 좋아하세요?",
          "original_form": "회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100013947.1.10",
          "form": "아 저 진짜 좋아해요ㅠㅠ회먹구싶다",
          "original_form": "아 저 진짜 좋아해요ㅠㅠ회먹구싶다",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100013947.1.11",
          "form": "회 초장이랑 간장중에 머에 찍어먹는거 좋아하세요??",
          "original_form": "회 초장이랑 간장중에 머에 찍어먹는거 좋아하세요??",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100013947.1.12",
          "form": "아 저 초장파에요....ㅠㅠ",
          "original_form": "아 저 초장파에요....ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100013947.1.13",
          "form": "저도요!!! 간장은 심심해서 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도요!!! 간장은 심심해서 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100013947.1.14",
          "form": "소주랑 먹고싶어요 ㅠ",
          "original_form": "소주랑 먹고싶어요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100013947.1.15",
          "form": "저도요ㅠㅠㅠ아 소주까지ㅠㅠㅠ초장좋아하면 혼내는 분들이 많아서ㅋㅋ",
          "original_form": "저도요ㅠㅠㅠ아 소주까지ㅠㅠㅠ초장좋아하면 혼내는 분들이 많아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100013947.1.16",
          "form": "초장이 왜용???? 너무 맛있는데!",
          "original_form": "초장이 왜용???? 너무 맛있는데!",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100013947.1.17",
          "form": "회맛을 모른다구ㅎㅎㅜㅜㅜ그니까요 맛있는데",
          "original_form": "회맛을 모른다구ㅎㅎㅜㅜㅜ그니까요 맛있는데",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100013947.1.18",
          "form": "그러면 회맛을 알려면 생으로 먹어야 하나요?? ㅜㅜㅜ",
          "original_form": "그러면 회맛을 알려면 생으로 먹어야 하나요?? ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100013947.1.19",
          "form": "간장으로 먹으라고ㅋㅋㅋㅋ",
          "original_form": "간장으로 먹으라고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100013947.1.20",
          "form": "저는 그냥 초장이 좋아요ㅋㅋㅋ",
