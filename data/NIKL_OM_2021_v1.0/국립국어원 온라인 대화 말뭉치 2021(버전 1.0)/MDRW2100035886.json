      "utterance": [
        {
          "id": "MDRW2100035886.1.1",
          "form": "드셔보셨다니 부러워요!!ㅠㅠㅠㅠㅠ",
          "original_form": "드셔보셨다니 부러워요!!ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100035886.1.2",
          "form": "맞아요 빠네 맛있죠ㅠ",
          "original_form": "맞아요 빠네 맛있죠ㅠ",
          "speaker_id": "1",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100035886.1.3",
          "form": "힝",
          "original_form": "힝",
          "speaker_id": "1",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100035886.1.4",
          "form": "빠네스프가 진짜 맛있겠네요...우와",
          "original_form": "빠네스프가 진짜 맛있겠네요...우와",
          "speaker_id": "2",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100035886.1.5",
          "form": "저는 나중에",
          "original_form": "저는 나중에",
          "speaker_id": "1",
          "time": "20210830 10:31"
        },
        {
          "id": "MDRW2100035886.1.6",
          "form": "15만원 하는",
          "original_form": "15만원 하는",
          "speaker_id": "1",
          "time": "20210830 10:31"
        },
        {
          "id": "MDRW2100035886.1.7",
          "form": "호텔",
          "original_form": "호텔",
          "speaker_id": "1",
          "time": "20210830 10:31"
        },
        {
          "id": "MDRW2100035886.1.8",
          "form": "뷔페에서",
          "original_form": "뷔페에서",
          "speaker_id": "1",
          "time": "20210830 10:31"
        },
        {
          "id": "MDRW2100035886.1.9",
          "form": "신나게 먹고 오는게",
          "original_form": "신나게 먹고 오는게",
          "speaker_id": "1",
          "time": "20210830 10:31"
        },
        {
          "id": "MDRW2100035886.1.10",
          "form": "버킷리스트에용",
          "original_form": "버킷리스트에용",
          "speaker_id": "1",
          "time": "20210830 10:31"
        },
        {
          "id": "MDRW2100035886.1.11",
          "form": "하 저도요! 엄청 비싼호텔뷔페는 더 맛있겠죠?!",
          "original_form": "하 저도요! 엄청 비싼호텔뷔페는 더 맛있겠죠?!",
          "speaker_id": "2",
          "time": "20210830 10:32"
        },
        {
          "id": "MDRW2100035886.1.12",
          "form": "아직은 위가 작아서 한바퀴도 돌기 힘드니까 돈아까워서 못가고있어요ㅠㅠㅠㅋㅋㅋㅋ",
          "original_form": "아직은 위가 작아서 한바퀴도 돌기 힘드니까 돈아까워서 못가고있어요ㅠㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:32"
        },
        {
          "id": "MDRW2100035886.1.13",
          "form": "그럴만한 배포가 없는..",
          "original_form": "그럴만한 배포가 없는..",
          "speaker_id": "1",
          "time": "20210830 10:32"
        },
        {
          "id": "MDRW2100035886.1.14",
          "form": "겨울에 딸기시즌에 뷔페 축제여는곳 많던데",
          "original_form": "겨울에 딸기시즌에 뷔페 축제여는곳 많던데",
          "speaker_id": "1",
          "time": "20210830 10:32"
        },
        {
          "id": "MDRW2100035886.1.15",
          "form": "저 첫 월급받고 7-8만원하는 뷔페 간 적 있는데...진짜 너무 맛있는 거예요....",
          "original_form": "저 첫 월급받고 7-8만원하는 뷔페 간 적 있는데...진짜 너무 맛있는 거예요....",
          "speaker_id": "2",
          "time": "20210830 10:32"
        },
        {
          "id": "MDRW2100035886.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210830 10:32"
        },
        {
          "id": "MDRW2100035886.1.17",
          "form": "와ㅠㅠㅠ",
          "original_form": "와ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:32"
        },
        {
          "id": "MDRW2100035886.1.18",
          "form": "메뉴도 다양하고ㅠㅠㅠㅠㅠ 진짜 그때를 잊을 수가 없는데...",
          "original_form": "메뉴도 다양하고ㅠㅠㅠㅠㅠ 진짜 그때를 잊을 수가 없는데...",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.19",
          "form": "고생해서 번돈으로 드셔서",
          "original_form": "고생해서 번돈으로 드셔서",
          "speaker_id": "1",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.20",
          "form": "더 맛있으셨겠다",
          "original_form": "더 맛있으셨겠다",
          "speaker_id": "1",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.21",
          "form": "코로나로 폐업했더라고요....아놔......ㅠㅠ",
          "original_form": "코로나로 폐업했더라고요....아놔......ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.22",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.23",
          "form": "아아ㅠㅠㅠ",
          "original_form": "아아ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.24",
          "form": "뷔페가 요즘 폐업 많이하더라구요....",
          "original_form": "뷔페가 요즘 폐업 많이하더라구요....",
          "speaker_id": "1",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.25",
          "form": "첫 월급으로 부모님 모시고 갔었거든요!ㅎ.ㅎ 맞아요...",
          "original_form": "첫 월급으로 부모님 모시고 갔었거든요!ㅎ.ㅎ 맞아요...",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.26",
          "form": "애슐리 자연별곡... 쿠우쿠우...",
          "original_form": "애슐리 자연별곡... 쿠우쿠우...",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.27",
          "form": "다 없어지더라고요ㅠㅠ",
          "original_form": "다 없어지더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.28",
          "form": "와아 효녀시네용!!",
          "original_form": "와아 효녀시네용!!",
          "speaker_id": "1",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.29",
          "form": "저희동네 쿠우쿠우도 없어졌어용",
          "original_form": "저희동네 쿠우쿠우도 없어졌어용",
          "speaker_id": "1",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035886.1.30",
          "form": "대신 꽃게 무한리필이 생겨서",
          "original_form": "대신 꽃게 무한리필이 생겨서",
          "speaker_id": "1",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100035886.1.31",
          "form": "괜찮아용!ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "괜찮아용!ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100035886.1.32",
          "form": "아 거기가면",
          "original_form": "아 거기가면",
          "speaker_id": "1",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100035886.1.33",
          "form": "간장게장이랑",
          "original_form": "간장게장이랑",
          "speaker_id": "1",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100035886.1.34",
          "form": "양념게장이랑",
          "original_form": "양념게장이랑",
          "speaker_id": "1",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100035886.1.35",
          "form": "꽃게탕이 다 무한리필이에용",
          "original_form": "꽃게탕이 다 무한리필이에용",
          "speaker_id": "1",
          "time": "20210830 10:34"
        }
      ]
