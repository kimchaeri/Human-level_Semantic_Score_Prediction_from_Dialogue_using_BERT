      "utterance": [
        {
          "id": "MDRW2100041602.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ멋진 누나네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ멋진 누나네요",
          "speaker_id": "2",
          "time": "20210906 12:16"
        },
        {
          "id": "MDRW2100041602.1.2",
          "form": "난 어차피 까먹오",
          "original_form": "난 어차피 까먹오",
          "speaker_id": "1",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100041602.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100041602.1.4",
          "form": "나는 저번에 민초아이스크림통을 사서",
          "original_form": "나는 저번에 민초아이스크림통을 사서",
          "speaker_id": "2",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100041602.1.5",
          "form": "몇번 안 퍼먹었는데",
          "original_form": "몇번 안 퍼먹었는데",
          "speaker_id": "2",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100041602.1.6",
          "form": "텅텅 빈거에요",
          "original_form": "텅텅 빈거에요",
          "speaker_id": "2",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100041602.1.7",
          "form": "넘 슬펏답니다",
          "original_form": "넘 슬펏답니다",
          "speaker_id": "2",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.8",
          "form": "동생이 다 먹은 거시에요",
          "original_form": "동생이 다 먹은 거시에요",
          "speaker_id": "2",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.9",
          "form": "가족이ㅜ먹엇엉?",
          "original_form": "가족이ㅜ먹엇엉?",
          "speaker_id": "1",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.10",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.11",
          "form": "먹으라고 하긴 했지만",
          "original_form": "먹으라고 하긴 했지만",
          "speaker_id": "2",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.12",
          "form": "ㅋㅋㅋㅋㅋ ㅋㅋ ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ ㅋㅋ ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.13",
          "form": "뭔가...",
          "original_form": "뭔가...",
          "speaker_id": "2",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.14",
          "form": "나도 더 먹고싶엇답니다ㅠ",
          "original_form": "나도 더 먹고싶엇답니다ㅠ",
          "speaker_id": "2",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.15",
          "form": "남자동생 넘들이",
          "original_form": "남자동생 넘들이",
          "speaker_id": "1",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.16",
          "form": "양보하거나 생각해서 남기거나",
          "original_form": "양보하거나 생각해서 남기거나",
          "speaker_id": "1",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.17",
          "form": "하는게없어",
          "original_form": "하는게없어",
          "speaker_id": "1",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.18",
          "form": "그래보입니다",
          "original_form": "그래보입니다",
          "speaker_id": "2",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.19",
          "form": "ㅋㅋ-ㅋㅋㅋㅌ엉엉",
          "original_form": "ㅋㅋ-ㅋㅋㅋㅌ엉엉",
          "speaker_id": "2",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041602.1.20",
          "form": "이번에도",
          "original_form": "이번에도",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041602.1.21",
          "form": "슈크림 5개 들어있는",
          "original_form": "슈크림 5개 들어있는",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041602.1.22",
          "form": "봉..지를",
          "original_form": "봉..지를",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041602.1.23",
          "form": "네개 샀어",
          "original_form": "네개 샀어",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041602.1.24",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041602.1.25",
          "form": "그럼 나 두개 걔 두개 먹으면 되는데",
          "original_form": "그럼 나 두개 걔 두개 먹으면 되는데",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041602.1.26",
          "form": "이...이...",
          "original_form": "이...이...",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041602.1.27",
          "form": "나쁜애가",
          "original_form": "나쁜애가",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041602.1.28",
          "form": "총 10개 10개 먹으면 되는.것을",
          "original_form": "총 10개 10개 먹으면 되는.것을",
          "speaker_id": "2",
          "time": "20210906 12:20"
        },
        {
          "id": "MDRW2100041602.1.29",
          "form": "나 6개 먹고...",
          "original_form": "나 6개 먹고...",
          "speaker_id": "2",
          "time": "20210906 12:20"
        },
        {
          "id": "MDRW2100041602.1.30",
          "form": "그...렇개 된겁니다",
          "original_form": "그...렇개 된겁니다",
          "speaker_id": "2",
          "time": "20210906 12:20"
        },
        {
          "id": "MDRW2100041602.1.31",
          "form": "그런건 줘팸하고",
          "original_form": "그런건 줘팸하고",
          "speaker_id": "1",
          "time": "20210906 12:20"
        },
        {
          "id": "MDRW2100041602.1.32",
          "form": "두고두고 구박해야함",
          "original_form": "두고두고 구박해야함",
          "speaker_id": "1",
          "time": "20210906 12:20"
        },
        {
          "id": "MDRW2100041602.1.33",
          "form": "나쁩니다",
          "original_form": "나쁩니다",
          "speaker_id": "2",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041602.1.34",
          "form": "먹을 거로 이러는 거 서럽습니다",
          "original_form": "먹을 거로 이러는 거 서럽습니다",
          "speaker_id": "2",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041602.1.35",
          "form": "엉 그래서 난 구박해",
          "original_form": "엉 그래서 난 구박해",
          "speaker_id": "1",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041602.1.36",
          "form": "파파는 머먹어",
          "original_form": "파파는 머먹어",
          "speaker_id": "2",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041602.1.37",
          "form": "점심",
          "original_form": "점심",
          "speaker_id": "2",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041602.1.38",
          "form": "아니 얼마전에 아이스크림 사놔서 바는 따로 빼놓고",
          "original_form": "아니 얼마전에 아이스크림 사놔서 바는 따로 빼놓고",
          "speaker_id": "1",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041602.1.39",
          "form": "반쯤 숨겨둠",
          "original_form": "반쯤 숨겨둠",
          "speaker_id": "1",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041602.1.40",
          "form": "그 고드름은 먹으라고 놔뒀는데",
          "original_form": "그 고드름은 먹으라고 놔뒀는데",
          "speaker_id": "1",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.41",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.42",
          "form": "동생이 나 생각한답시고",
          "original_form": "동생이 나 생각한답시고",
          "speaker_id": "1",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.44",
          "form": "숨겨놓은 갈",
          "original_form": "숨겨놓은 갈",
          "speaker_id": "1",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.45",
          "form": "먹음ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "먹음ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.46",
          "form": "왜요 숨겨놓은건",
          "original_form": "왜요 숨겨놓은건",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.47",
          "form": "웃기는 애야 진짜",
          "original_form": "웃기는 애야 진짜",
          "speaker_id": "1",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.48",
          "form": "의미가 있는 건뎈ㅋㅋㅋ",
          "original_form": "의미가 있는 건뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.49",
          "form": "그냥 고드림이 비싸니까",
          "original_form": "그냥 고드림이 비싸니까",
          "speaker_id": "1",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.50",
          "form": "어째서...",
          "original_form": "어째서...",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.51",
          "form": "이거 누나 좋아하니까 생각해서 놔둔 거같은데",
          "original_form": "이거 누나 좋아하니까 생각해서 놔둔 거같은데",
          "speaker_id": "1",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041602.1.52",
          "form": "진짜 웃기는 애라니까",
          "original_form": "진짜 웃기는 애라니까",
          "speaker_id": "1",
          "time": "20210906 12:22"
        }
      ]
    }
