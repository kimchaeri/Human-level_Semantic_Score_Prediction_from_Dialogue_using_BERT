      "utterance": [
        {
          "id": "MDRW2100039080.1.1",
          "form": "장마가 지나갔네요",
          "original_form": "장마가 지나갔네요",
          "speaker_id": "2",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100039080.1.2",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100039080.1.3",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100039080.1.4",
          "form": "없어요ㅠ",
          "original_form": "없어요ㅠ",
          "speaker_id": "2",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100039080.1.5",
          "form": "키워본적도 없어요",
          "original_form": "키워본적도 없어요",
          "speaker_id": "2",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100039080.1.6",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100039080.1.7",
          "form": "키우고 싶은 친구는 있나요",
          "original_form": "키우고 싶은 친구는 있나요",
          "speaker_id": "2",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100039080.1.8",
          "form": "라쿤이요",
          "original_form": "라쿤이요",
          "speaker_id": "1",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100039080.1.9",
          "form": "와 라쿤 엄청 사고뭉치라던데",
          "original_form": "와 라쿤 엄청 사고뭉치라던데",
          "speaker_id": "2",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100039080.1.10",
          "form": "맞아요 두 발로 서고 손 잘 써요 ㅠㅋㅋ",
          "original_form": "맞아요 두 발로 서고 손 잘 써요 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100039080.1.11",
          "form": "라쿤이 지능이 워낙에 좋아서야 원ㅋㅋ",
          "original_form": "라쿤이 지능이 워낙에 좋아서야 원ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100039080.1.12",
          "form": "먹을건 꼭 씻어먹는대죠",
          "original_form": "먹을건 꼭 씻어먹는대죠",
          "speaker_id": "2",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100039080.1.13",
          "form": "그래도 넘 귀여워요 실물 가까이서 봤거든요",
          "original_form": "그래도 넘 귀여워요 실물 가까이서 봤거든요",
          "speaker_id": "1",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100039080.1.14",
          "form": "name2님은 키우시고 싶은 동물 있으세요?",
          "original_form": "&name2&님은 키우시고 싶은 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210903 09:30"
        },
        {
          "id": "MDRW2100039080.1.15",
          "form": "저는 웰시코기 키우고 싶어요",
          "original_form": "저는 웰시코기 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210903 09:30"
        },
        {
          "id": "MDRW2100039080.1.16",
          "form": "아 웰시코기도 귀엽죠 ㅎㅎ",
          "original_form": "아 웰시코기도 귀엽죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100039080.1.17",
          "form": "걸을 때 씰룩거리는 빵뎅이에 반했어요",
          "original_form": "걸을 때 씰룩거리는 빵뎅이에 반했어요",
          "speaker_id": "2",
          "time": "20210903 09:34"
        },
        {
          "id": "MDRW2100039080.1.18",
          "form": "코기 엉덩이 매력 포인트죠",
          "original_form": "코기 엉덩이 매력 포인트죠",
