      "utterance": [
        {
          "id": "MDRW2100004800.1.1",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004800.1.2",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004800.1.3",
          "form": "네 저 동물 좋아해요!",
          "original_form": "네 저 동물 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004800.1.4",
          "form": "반말 써도 되나요?",
          "original_form": "반말 써도 되나요?",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004800.1.5",
          "form": "네 가능해요 쓰세요 저도 반말쓸게요",
          "original_form": "네 가능해요 쓰세요 저도 반말쓸게요",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004800.1.6",
          "form": "응 고마워 근데 강아지가 좋아? 아님 고양이가 좋아?",
          "original_form": "응 고마워 근데 강아지가 좋아? 아님 고양이가 좋아?",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004800.1.7",
          "form": "난 강아지가 좋아",
          "original_form": "난 강아지가 좋아",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004800.1.8",
          "form": "나는 강아지가 좋아 왜냐하면 강아지가 귀여웡",
          "original_form": "나는 강아지가 좋아 왜냐하면 강아지가 귀여웡",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004800.1.9",
          "form": "아하 난 강아지 키워서 강아지 더 좋아해",
          "original_form": "아하 난 강아지 키워서 강아지 더 좋아해",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004800.1.10",
          "form": "근데 둘 다 좋아하긴 해 엄청",
          "original_form": "근데 둘 다 좋아하긴 해 엄청",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004800.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇구나ㅠ나도 강아지 키워서 강아지가 좋아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇구나ㅠ나도 강아지 키워서 강아지가 좋아",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004800.1.12",
          "form": "오 강아지 종이 혹시 치와와?",
          "original_form": "오 강아지 종이 혹시 치와와?",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004800.1.13",
          "form": "헐 맞아 장모치와와",
          "original_form": "헐 맞아 장모치와와",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004800.1.14",
          "form": "너도 치와와 키우니?",
          "original_form": "너도 치와와 키우니?",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004800.1.15",
          "form": "당연하지 같이 사니까",
          "original_form": "당연하지 같이 사니까",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004800.1.16",
          "form": "아하 그렇구나",
          "original_form": "아하 그렇구나",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004800.1.17",
          "form": "ㅋㅋㅋㅋ 그럼 무슨 동물이 젤 좋아",
          "original_form": "ㅋㅋㅋㅋ 그럼 무슨 동물이 젤 좋아",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004800.1.18",
          "form": "강아지가 좋아^^ 귀엽잖아^^",
          "original_form": "강아지가 좋아^^ 귀엽잖아^^",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004800.1.19",
          "form": "강아지 다음으론?",
          "original_form": "강아지 다음으론?",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004800.1.20",
          "form": "강아지 다음으론 고양이가 좋을 걸 아마도",
          "original_form": "강아지 다음으론 고양이가 좋을 걸 아마도",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004800.1.21",
          "form": "평범해",
          "original_form": "평범해",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004800.1.22",
          "form": "근데 토끼도 좋고 알파카도 좋아 근데 알파카는 반려공물이아니라슬포",
          "original_form": "근데 토끼도 좋고 알파카도 좋아 근데 알파카는 반려공물이아니라슬포",
          "speaker_id": "2",
          "time": "20210821 18:43"
        }
      ]
