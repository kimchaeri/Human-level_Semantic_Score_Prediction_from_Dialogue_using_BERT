      "utterance": [
        {
          "id": "MDRW2100046249.1.1",
          "form": "헬로",
          "original_form": "헬로",
          "speaker_id": "1",
          "time": "20211015 11:22"
        },
        {
          "id": "MDRW2100046249.1.2",
          "form": "ㅎㅇㅎㅇ!",
          "original_form": "ㅎㅇㅎㅇ!",
          "speaker_id": "2",
          "time": "20211015 11:24"
        },
        {
          "id": "MDRW2100046249.1.3",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20211015 11:24"
        },
        {
          "id": "MDRW2100046249.1.4",
          "form": "월요일에 쉬셨나요?",
          "original_form": "월요일에 쉬셨나요?",
          "speaker_id": "2",
          "time": "20211015 11:24"
        },
        {
          "id": "MDRW2100046249.1.5",
          "form": "요번주는 대체휴일이라 쉬엇지요",
          "original_form": "요번주는 대체휴일이라 쉬엇지요",
          "speaker_id": "1",
          "time": "20211015 11:24"
        },
        {
          "id": "MDRW2100046249.1.6",
          "form": "다행이네요 ! ",
          "original_form": "다행이네요 ! ",
          "speaker_id": "2",
          "time": "20211015 11:24"
        },
        {
          "id": "MDRW2100046249.1.7",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "1",
          "time": "20211015 11:24"
        },
        {
          "id": "MDRW2100046249.1.8",
          "form": "네 저도 쉬었어요 ! 2주연속 주4일 일했더니 다음 주가 걱정이에요 ㅋㅋ",
          "original_form": "네 저도 쉬었어요 ! 2주연속 주4일 일했더니 다음 주가 걱정이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 11:25"
        },
        {
          "id": "MDRW2100046249.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20211015 11:25"
        },
        {
          "id": "MDRW2100046249.1.10",
          "form": "주4일이 대박이긴해여",
          "original_form": "주4일이 대박이긴해여",
          "speaker_id": "1",
          "time": "20211015 11:25"
        },
        {
          "id": "MDRW2100046249.1.11",
          "form": "일주일이 빨리가고",
          "original_form": "일주일이 빨리가고",
          "speaker_id": "1",
          "time": "20211015 11:25"
        },
        {
          "id": "MDRW2100046249.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20211015 11:25"
        },
        {
          "id": "MDRW2100046249.1.13",
          "form": "주 4일로 수요일에 만약 쉬면 [어제 쉬었음 - 내일 쉼] 이게 반복되는 게 참 좋아보이던데",
          "original_form": "주 4일로 수요일에 만약 쉬면 [어제 쉬었음 - 내일 쉼] 이게 반복되는 게 참 좋아보이던데",
          "speaker_id": "2",
          "time": "20211015 11:26"
        },
        {
          "id": "MDRW2100046249.1.14",
          "form": "중간에 쉬면",
          "original_form": "중간에 쉬면",
          "speaker_id": "1",
          "time": "20211015 11:26"
        },
        {
          "id": "MDRW2100046249.1.15",
          "form": "리프레쉬되어 좋고",
          "original_form": "리프레쉬되어 좋고",
          "speaker_id": "1",
          "time": "20211015 11:26"
        },
        {
          "id": "MDRW2100046249.1.16",
          "form": "월,금에 쉬면 주말이길어 좋고",
          "original_form": "월,금에 쉬면 주말이길어 좋고",
          "speaker_id": "1",
          "time": "20211015 11:26"
        },
        {
          "id": "MDRW2100046249.1.17",
          "form": "맞아요 그것도 좋아요 ㅋㅋ",
          "original_form": "맞아요 그것도 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 11:27"
        },
        {
          "id": "MDRW2100046249.1.18",
          "form": "머가되엇든",
          "original_form": "머가되엇든",
          "speaker_id": "1",
          "time": "20211015 11:28"
        },
        {
          "id": "MDRW2100046249.1.19",
          "form": "주4일일하면 좋겟어요",
          "original_form": "주4일일하면 좋겟어요",
          "speaker_id": "1",
          "time": "20211015 11:28"
        },
        {
          "id": "MDRW2100046249.1.20",
          "form": "언제즘 되려나요",
          "original_form": "언제즘 되려나요",
          "speaker_id": "2",
          "time": "20211015 11:28"
        },
        {
          "id": "MDRW2100046249.1.21",
          "form": "아직 한참 멀엇죠 모",
          "original_form": "아직 한참 멀엇죠 모",
          "speaker_id": "1",
          "time": "20211015 11:28"
        },
        {
          "id": "MDRW2100046249.1.22",
          "form": "주5일도 안되는데가 엄청많은데",
          "original_form": "주5일도 안되는데가 엄청많은데",
          "speaker_id": "1",
          "time": "20211015 11:28"
        },
        {
          "id": "MDRW2100046249.1.23",
          "form": "그쵸 ㅠㅠ 아직 선진국 되려면 좀 먼 거 같아요",
          "original_form": "그쵸 ㅠㅠ 아직 선진국 되려면 좀 먼 거 같아요",
          "speaker_id": "2",
          "time": "20211015 11:29"
        }
      ]
