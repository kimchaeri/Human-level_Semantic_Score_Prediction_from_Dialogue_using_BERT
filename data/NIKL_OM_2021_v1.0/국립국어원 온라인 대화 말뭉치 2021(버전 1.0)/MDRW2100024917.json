      },
      "utterance": [
        {
          "id": "MDRW2100024917.1.1",
          "form": "요즘 너 살이 너무 빠졌어",
          "original_form": "요즘 너 살이 너무 빠졌어",
          "speaker_id": "1",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100024917.1.2",
          "form": "다이어트 중이야",
          "original_form": "다이어트 중이야",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100024917.1.3",
          "form": "3키로 뺐어",
          "original_form": "3키로 뺐어",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100024917.1.4",
          "form": "너무 아파보이게 뺐는디",
          "original_form": "너무 아파보이게 뺐는디",
          "speaker_id": "1",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100024917.1.5",
          "form": "그래서 오늘부터 운동 좀 열심히 하려구...",
          "original_form": "그래서 오늘부터 운동 좀 열심히 하려구...",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100024917.1.6",
          "form": "안먹으면서 뺏더니 탈모생겼어ㅠ",
          "original_form": "안먹으면서 뺏더니 탈모생겼어ㅠ",
          "speaker_id": "2",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100024917.1.7",
          "form": "아침에 검은콩 갈아줄게",
          "original_form": "아침에 검은콩 갈아줄게",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100024917.1.8",
          "form": "검은콩 먹으면 머리 자란대",
          "original_form": "검은콩 먹으면 머리 자란대",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100024917.1.9",
          "form": "낼 비오틴도 주문하려고...",
          "original_form": "낼 비오틴도 주문하려고...",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100024917.1.10",
          "form": "좀 심각해",
          "original_form": "좀 심각해",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100024917.1.11",
          "form": "그리고 안먹으면서 뺐더니 기운도 없어",
          "original_form": "그리고 안먹으면서 뺐더니 기운도 없어",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100024917.1.12",
          "form": "고기 좀 챙겨 먹어",
          "original_form": "고기 좀 챙겨 먹어",
          "speaker_id": "1",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100024917.1.13",
          "form": "헬스장을 좀 다니던지",
          "original_form": "헬스장을 좀 다니던지",
          "speaker_id": "1",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100024917.1.14",
          "form": "필라테스 하러가고 싶어",
          "original_form": "필라테스 하러가고 싶어",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100024917.1.15",
          "form": "그렇게 힘들다던대 건강해질거 같아",
          "original_form": "그렇게 힘들다던대 건강해질거 같아",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100024917.1.16",
          "form": "필라테스는 뭔데",
          "original_form": "필라테스는 뭔데",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100024917.1.17",
          "form": "요가 같은건데",
          "original_form": "요가 같은건데",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100024917.1.18",
          "form": "기구가지고 운동하는 요가보다 더 힘든거",
          "original_form": "기구가지고 운동하는 요가보다 더 힘든거",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100024917.1.19",
          "form": "저번에 한 운동은 뭔데??",
          "original_form": "저번에 한 운동은 뭔데??",
          "speaker_id": "1",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100024917.1.20",
          "form": "저번에 한 운동은 스피닝",
          "original_form": "저번에 한 운동은 스피닝",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100024917.1.21",
          "form": "재밌다더니 그건 왜 안해",
          "original_form": "재밌다더니 그건 왜 안해",
          "speaker_id": "1",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100024917.1.22",
          "form": "스피닝은 강사따라서 재미있냐 없냐가 갈리는데",
          "original_form": "스피닝은 강사따라서 재미있냐 없냐가 갈리는데",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100024917.1.23",
          "form": "재밌는 선생님이 다른데 갔어..",
          "original_form": "재밌는 선생님이 다른데 갔어..",
          "speaker_id": "2",
          "time": "20210902 00:30"
        }
      ]
    }
