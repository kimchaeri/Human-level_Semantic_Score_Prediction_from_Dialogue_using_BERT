      "utterance": [
        {
          "id": "MDRW2100033861.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100033861.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100033861.1.3",
          "form": "동물좋아하세요?",
          "original_form": "동물좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033861.1.4",
          "form": "네 좋아해요 name2님은요?",
          "original_form": "네 좋아해요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100033861.1.5",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033861.1.6",
          "form": "진돗개는 어떠세요?",
          "original_form": "진돗개는 어떠세요?",
          "speaker_id": "2",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033861.1.7",
          "form": "진돗개 귀여워요 ㅎㅎ",
          "original_form": "진돗개 귀여워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100033861.1.8",
          "form": "푸들은요?",
          "original_form": "푸들은요?",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100033861.1.9",
          "form": "푸들 괜찮네요 복실복실한게요 ㅋㅋ",
          "original_form": "푸들 괜찮네요 복실복실한게요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100033861.1.10",
          "form": "키워본 동물 있으세요?",
          "original_form": "키워본 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100033861.1.11",
          "form": "한번도요ㅠ",
          "original_form": "한번도요ㅠ",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100033861.1.12",
          "form": "그래서 포메를 키우고싶은거랍니다",
          "original_form": "그래서 포메를 키우고싶은거랍니다",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100033861.1.13",
          "form": "에구 그러시군요",
          "original_form": "에구 그러시군요",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100033861.1.14",
          "form": "포메가 귀엽긴 귀엽더라구요 털도 북실북실한게 촛감이좋아요",
          "original_form": "포메가 귀엽긴 귀엽더라구요 털도 북실북실한게 촛감이좋아요",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100033861.1.15",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100033861.1.16",
          "form": "전진짜",
          "original_form": "전진짜",
          "speaker_id": "2",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100033861.1.17",
          "form": "꼭키울거예요",
          "original_form": "꼭키울거예요",
          "speaker_id": "2",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100033861.1.18",
          "form": "응원합니다!",
          "original_form": "응원합니다!",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100033861.1.19",
          "form": "거북이가 돼지고기 먹는거아세요?",
          "original_form": "거북이가 돼지고기 먹는거아세요?",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100033861.1.20",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100033861.1.21",
          "form": "그래요ㅣ",
          "original_form": "그래요ㅣ",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100033861.1.22",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100033861.1.23",
          "form": "네 사료도 주고",
          "original_form": "네 사료도 주고",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100033861.1.24",
          "form": "가끔 돈까스하거나 할때 짜투리 고기 잘라서 줬어요",
          "original_form": "가끔 돈까스하거나 할때 짜투리 고기 잘라서 줬어요",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100033861.1.25",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033861.1.26",
          "form": "귀여울것같아요",
          "original_form": "귀여울것같아요",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033861.1.27",
          "form": "튀긴돈까스인가요?",
          "original_form": "튀긴돈까스인가요?",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033861.1.28",
          "form": "아뇨 그말이아니고",
          "original_form": "아뇨 그말이아니고",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100033861.1.29",
          "form": "돈까스 만들면서 나온 생고기요",
          "original_form": "돈까스 만들면서 나온 생고기요",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100033861.1.30",
          "form": "오ㅏ우",
          "original_form": "오ㅏ우",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100033861.1.31",
          "form": "거북이는 잠을",
          "original_form": "거북이는 잠을",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100033861.1.32",
          "form": "많이자나요?",
          "original_form": "많이자나요?",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100033861.1.33",
          "form": "아 잠 자주잤던거같아요",
          "original_form": "아 잠 자주잤던거같아요",
          "speaker_id": "1",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100033861.1.34",
          "form": "낮에도 돌위에 올라가서 자더라구요 ㅋㅋ",
          "original_form": "낮에도 돌위에 올라가서 자더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:21"
        }
      ]
    }
