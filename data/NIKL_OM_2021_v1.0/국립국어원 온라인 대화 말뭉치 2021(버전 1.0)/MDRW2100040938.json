      "utterance": [
        {
          "id": "MDRW2100040938.1.1",
          "form": "맞아여.. 진짜 소장이 백번 이득이다..",
          "original_form": "맞아여.. 진짜 소장이 백번 이득이다..",
          "speaker_id": "2",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040938.1.2",
          "form": "큐키를 한 500개쯤 날리고나서야",
          "original_form": "큐키를 한 500개쯤 날리고나서야",
          "speaker_id": "2",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040938.1.3",
          "form": "아.. 소장만이 답이구나..ㅎ",
          "original_form": "아.. 소장만이 답이구나..ㅎ",
          "speaker_id": "2",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040938.1.4",
          "form": "으악 그니까요!!!!!!!!!@@!!! 500개 넘... 아깝다",
          "original_form": "으악 그니까요!!!!!!!!!@@!!! 500개 넘... 아깝다",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040938.1.5",
          "form": "블루님 그럼 약간 장르 드라마??",
          "original_form": "블루님 그럼 약간 장르 드라마??",
          "speaker_id": "1",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100040938.1.6",
          "form": "도 자주 보세요??",
          "original_form": "도 자주 보세요??",
          "speaker_id": "1",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100040938.1.7",
          "form": "재혼황후... 드라마화 한다면서욧...!",
          "original_form": "재혼황후... 드라마화 한다면서욧...!",
          "speaker_id": "1",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100040938.1.8",
          "form": "장르드라마가 어떤거 말씀하시는",
          "original_form": "장르드라마가 어떤거 말씀하시는",
          "speaker_id": "2",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100040938.1.9",
          "form": "오우 이런",
          "original_form": "오우 이런",
          "speaker_id": "2",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100040938.1.10",
          "form": "아니 근데 저는 대체 누가 배역읗 따게될지..",
          "original_form": "아니 근데 저는 대체 누가 배역읗 따게될지..",
          "speaker_id": "2",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100040938.1.11",
          "form": "그게 가장 문제라고 생각합니다..",
          "original_form": "그게 가장 문제라고 생각합니다..",
          "speaker_id": "2",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100040938.1.12",
          "form": "아 인정합니다 ^.ㅠ 솔직히 황후는",
          "original_form": "아 인정합니다 ^.ㅠ 솔직히 황후는",
          "speaker_id": "1",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100040938.1.13",
          "form": "이사람 저사람 생각나는데",
          "original_form": "이사람 저사람 생각나는데",
          "speaker_id": "1",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100040938.1.14",
          "form": "하인리는 누가 할 거며....",
          "original_form": "하인리는 누가 할 거며....",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040938.1.15",
          "form": "^.^ 이상한 사람 데려다놓고 미남이라고",
          "original_form": "^.^ 이상한 사람 데려다놓고 미남이라고",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040938.1.16",
          "form": "올려치지만 않앗음 좋겠어요",
          "original_form": "올려치지만 않앗음 좋겠어요",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040938.1.17",
          "form": "아 제발요.. 제발..ㅎ",
          "original_form": "아 제발요.. 제발..ㅎ",
          "speaker_id": "2",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100040938.1.18",
          "form": "아니 드라마화가 된다니.. 저 몰랐는데 캐스팅 생각하니 반대하고싶어요..",
          "original_form": "아니 드라마화가 된다니.. 저 몰랐는데 캐스팅 생각하니 반대하고싶어요..",
          "speaker_id": "2",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100040938.1.19",
          "form": "저도요.. 전 그 네이버에서 광고한 거 봤을때도",
          "original_form": "저도요.. 전 그 네이버에서 광고한 거 봤을때도",
          "speaker_id": "1",
          "time": "20210905 13:26"
        },
        {
          "id": "MDRW2100040938.1.20",
          "form": "서예지는 걍 찰떡이었는데........ ... ...",
          "original_form": "서예지는 걍 찰떡이었는데........ ... ...",
          "speaker_id": "1",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100040938.1.21",
          "form": "물론 그건 하렘의 어쩌고들이긴한데",
          "original_form": "물론 그건 하렘의 어쩌고들이긴한데",
          "speaker_id": "1",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100040938.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100040938.1.23",
          "form": "제대로 안 할 거면 드라마화 안 했음 좋겠아요 ㅠ.",
          "original_form": "제대로 안 할 거면 드라마화 안 했음 좋겠아요 ㅠ.",
          "speaker_id": "1",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100040938.1.24",
          "form": "저 치인트도 넘 아쉬웟단 말이에요",
          "original_form": "저 치인트도 넘 아쉬웟단 말이에요",
          "speaker_id": "1",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100040938.1.25",
          "form": "전 서예지는 좋았는데 주지훈이 좀 별로였어요!",
          "original_form": "전 서예지는 좋았는데 주지훈이 좀 별로였어요!",
          "speaker_id": "2",
          "time": "20210905 13:28"
        },
        {
          "id": "MDRW2100040938.1.26",
          "form": "ㅠㅠ 넘 나이많구.. 흘러내리고있다구요!",
          "original_form": "ㅠㅠ 넘 나이많구.. 흘러내리고있다구요!",
          "speaker_id": "2",
          "time": "20210905 13:28"
        },
        {
          "id": "MDRW2100040938.1.27",
          "form": "오마이갓 저 사실 name2님이 주지훈 좋아하실까봐",
          "original_form": "오마이갓 저 사실 &name2&님이 주지훈 좋아하실까봐",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100040938.1.28",
          "form": "조용히 있었는데",
          "original_form": "조용히 있었는데",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100040938.1.29",
          "form": "아오 저도요!!!!!!!",
          "original_form": "아오 저도요!!!!!!!",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100040938.1.30",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100040938.1.31",
          "form": "아니... 여자배우들 세대교체는 그렇게 빠르묜소",
          "original_form": "아니... 여자배우들 세대교체는 그렇게 빠르묜소",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100040938.1.32",
          "form": "왜..왜!!! 남자배우들은 하정우가 수지 남편할 정도로",
          "original_form": "왜..왜!!! 남자배우들은 하정우가 수지 남편할 정도로",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100040938.1.33",
          "form": "세대교체를 안 하죠?!?!?!?",
          "original_form": "세대교체를 안 하죠?!?!?!?",
          "speaker_id": "1",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100040938.1.34",
          "form": "그러니까요.. 하정우 수지..? 솔직히 밸붕 미친거 아닌가요",
          "original_form": "그러니까요.. 하정우 수지..? 솔직히 밸붕 미친거 아닌가요",
          "speaker_id": "2",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100040938.1.35",
          "form": "ㅠ 아빠랑 딸 아니냐고요 ㅠ",
          "original_form": "ㅠ 아빠랑 딸 아니냐고요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100040938.1.36",
          "form": "아 제 말이요 -.- !!!!",
          "original_form": "아 제 말이요 -.- !!!!",
          "speaker_id": "1",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100040938.1.37",
          "form": "그냥 청춘시대처럼...",
          "original_form": "그냥 청춘시대처럼...",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100040938.1.38",
          "form": "매력적인 여배우들 많이 나오는",
          "original_form": "매력적인 여배우들 많이 나오는",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100040938.1.39",
          "form": "작품 또 있었음 좋겠아요!!! ㅠ.^",
          "original_form": "작품 또 있었음 좋겠아요!!! ㅠ.^",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100040938.1.40",
          "form": "물론... 시즌1은 이제 못 보지만 ^^..",
          "original_form": "물론... 시즌1은 이제 못 보지만 ^^..",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100040938.1.41",
          "form": "아니 청춘시대 ㅠ",
          "original_form": "아니 청춘시대 ㅠ",
          "speaker_id": "2",
          "time": "20210905 17:34"
