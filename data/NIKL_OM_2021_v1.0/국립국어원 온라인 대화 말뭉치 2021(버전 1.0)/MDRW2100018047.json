      "utterance": [
        {
          "id": "MDRW2100018047.1.1",
          "form": "지금 보는 특종세상 재미있으세요?",
          "original_form": "지금 보는 특종세상 재미있으세요?",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018047.1.2",
          "form": "지금 특종세상 하는데 저 사람 이상하네",
          "original_form": "지금 특종세상 하는데 저 사람 이상하네",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018047.1.3",
          "form": "알까기 도사가 나오시네요",
          "original_form": "알까기 도사가 나오시네요",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018047.1.4",
          "form": "부인이랑 따로 사는데 이상해",
          "original_form": "부인이랑 따로 사는데 이상해",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018047.1.5",
          "form": "방송 출연하기 참 힘들어보여요 대본대로 하는걸텐데요",
          "original_form": "방송 출연하기 참 힘들어보여요 대본대로 하는걸텐데요",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018047.1.6",
          "form": "역시 그부분에 꽂히셨구나",
          "original_form": "역시 그부분에 꽂히셨구나",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018047.1.7",
          "form": "저 부인이 남편 밥 안준다고 뭐라고 하셨잖아요",
          "original_form": "저 부인이 남편 밥 안준다고 뭐라고 하셨잖아요",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100018047.1.8",
          "form": "어머니도 출연하셨네",
          "original_form": "어머니도 출연하셨네",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100018047.1.9",
          "form": "이런 프로그램은 사돈의 팔촌까지 나와야 되나봐요",
          "original_form": "이런 프로그램은 사돈의 팔촌까지 나와야 되나봐요",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100018047.1.10",
          "form": "집을 사서 밑에 산다는거 보니  돈은 좀 있나봐",
          "original_form": "집을 사서 밑에 산다는거 보니  돈은 좀 있나봐",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100018047.1.11",
          "form": "돈이 있으면 방송출연의 욕심도 생기나봐요",
          "original_form": "돈이 있으면 방송출연의 욕심도 생기나봐요",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018047.1.12",
          "form": "지리산인가 보네 캠핑장도 운영하나봐",
          "original_form": "지리산인가 보네 캠핑장도 운영하나봐",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018047.1.13",
          "form": "부잔데 이제 할게 없으신가보다",
          "original_form": "부잔데 이제 할게 없으신가보다",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018047.1.14",
          "form": "광고일까요?",
          "original_form": "광고일까요?",
          "speaker_id": "2",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100018047.1.15",
          "form": "그런가봐 지리산이라 그런가 물이 엄청 맑네",
          "original_form": "그런가봐 지리산이라 그런가 물이 엄청 맑네",
          "speaker_id": "1",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100018047.1.16",
          "form": "요즘은 캠핑장도 많아서 홍보하기도 어렵네요",
          "original_form": "요즘은 캠핑장도 많아서 홍보하기도 어렵네요",
          "speaker_id": "2",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100018047.1.17",
          "form": "저 아저씨 바위위에서 알까기 해요",
          "original_form": "저 아저씨 바위위에서 알까기 해요",
          "speaker_id": "2",
          "time": "20210826 22:22"
        }
      ]
    }
