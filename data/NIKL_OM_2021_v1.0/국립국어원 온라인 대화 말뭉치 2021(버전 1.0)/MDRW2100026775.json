      "utterance": [
        {
          "id": "MDRW2100026775.1.1",
          "form": "반려동물 있으신가요?",
          "original_form": "반려동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026775.1.2",
          "form": "저는 없어요 ~ 반려동물 있으신가요?",
          "original_form": "저는 없어요 ~ 반려동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 08:32"
        },
        {
          "id": "MDRW2100026775.1.3",
          "form": "아뇨 실은 저도 없어요",
          "original_form": "아뇨 실은 저도 없어요",
          "speaker_id": "1",
          "time": "20210902 08:45"
        },
        {
          "id": "MDRW2100026775.1.4",
          "form": "부모님댁에 리트리버 한마리 있는데",
          "original_form": "부모님댁에 리트리버 한마리 있는데",
          "speaker_id": "1",
          "time": "20210902 08:45"
        },
        {
          "id": "MDRW2100026775.1.5",
          "form": "제가 기르는건 아니라서",
          "original_form": "제가 기르는건 아니라서",
          "speaker_id": "1",
          "time": "20210902 08:45"
        },
        {
          "id": "MDRW2100026775.1.6",
          "form": "혹시 동물 길러본 경험은 있으신가요?",
          "original_form": "혹시 동물 길러본 경험은 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 08:46"
        },
        {
          "id": "MDRW2100026775.1.7",
          "form": "한번 있어요 ~",
          "original_form": "한번 있어요 ~",
          "speaker_id": "2",
          "time": "20210902 08:48"
        },
        {
          "id": "MDRW2100026775.1.8",
          "form": "뭐 기르셨어요?",
          "original_form": "뭐 기르셨어요?",
          "speaker_id": "1",
          "time": "20210902 08:50"
        },
        {
          "id": "MDRW2100026775.1.9",
          "form": "래브라도 리트리버요",
          "original_form": "래브라도 리트리버요",
          "speaker_id": "2",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100026775.1.10",
          "form": "저희집도 레브라도 리트리버인데",
          "original_form": "저희집도 레브라도 리트리버인데",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100026775.1.11",
          "form": "레브라도 리트리버",
          "original_form": "레브라도 리트리버",
          "speaker_id": "1",
          "time": "20210902 08:54"
        },
        {
          "id": "MDRW2100026775.1.12",
          "form": "너무 사랑스러워요",
          "original_form": "너무 사랑스러워요",
          "speaker_id": "1",
          "time": "20210902 08:54"
        },
        {
          "id": "MDRW2100026775.1.13",
          "form": "귀엽고 사교성 좋고",
          "original_form": "귀엽고 사교성 좋고",
          "speaker_id": "1",
          "time": "20210902 08:54"
        },
        {
          "id": "MDRW2100026775.1.14",
          "form": "맞아요 리트리버 넘 예뻐요",
          "original_form": "맞아요 리트리버 넘 예뻐요",
          "speaker_id": "2",
          "time": "20210902 08:54"
        },
        {
          "id": "MDRW2100026775.1.15",
          "form": "그런데 힘이 너무 좋아서 부모님이 산책시키는걸 부담스러워 하세요",
          "original_form": "그런데 힘이 너무 좋아서 부모님이 산책시키는걸 부담스러워 하세요",
          "speaker_id": "1",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100026775.1.16",
          "form": "맞아여 ㅜㅜ 저두 그때 엄마도 끌려다니고 저도 끌려다니고 ㅋㅋ",
          "original_form": "맞아여 ㅜㅜ 저두 그때 엄마도 끌려다니고 저도 끌려다니고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 08:59"
        },
        {
          "id": "MDRW2100026775.1.17",
          "form": "정말 반려동물 기르려면 책임감도 중요하고 체력도 중요한것같아요",
          "original_form": "정말 반려동물 기르려면 책임감도 중요하고 체력도 중요한것같아요",
          "speaker_id": "1",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100026775.1.18",
          "form": "맞아요 좋아하는 마음만으로는 키울수없어요",
          "original_form": "맞아요 좋아하는 마음만으로는 키울수없어요",
          "speaker_id": "2",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100026775.1.19",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210902 15:38"
        }
      ]
