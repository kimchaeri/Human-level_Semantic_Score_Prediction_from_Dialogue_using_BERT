        },
        {
          "id": "MDRW2100021434.1.2",
          "form": "그럼 미역줄기도 좋아하시나요",
          "original_form": "그럼 미역줄기도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 19:00"
        },
        {
          "id": "MDRW2100021434.1.3",
          "form": "음 솔직히 별로 안좋아해요 그건",
          "original_form": "음 솔직히 별로 안좋아해요 그건",
          "speaker_id": "2",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021434.1.4",
          "form": "헉.. 그러시구나  미역국만 좋아하는걸로",
          "original_form": "헉.. 그러시구나  미역국만 좋아하는걸로",
          "speaker_id": "1",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021434.1.5",
          "form": "그럼 미역국에 후추뿌려드시나요",
          "original_form": "그럼 미역국에 후추뿌려드시나요",
          "speaker_id": "1",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021434.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021434.1.7",
          "form": "전 후추 무진장 뿌리거든요",
          "original_form": "전 후추 무진장 뿌리거든요",
          "speaker_id": "1",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021434.1.8",
          "form": "전 상관없이 먹어요!",
          "original_form": "전 상관없이 먹어요!",
          "speaker_id": "2",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021434.1.9",
          "form": "뿌려도 안뿌려도 맛있는 것 같아요",
          "original_form": "뿌려도 안뿌려도 맛있는 것 같아요",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021434.1.10",
          "form": "그럼 다 잘먹는걸로",
          "original_form": "그럼 다 잘먹는걸로",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021434.1.11",
          "form": "맞아요 사실 미역국은 다 맛있어요",
          "original_form": "맞아요 사실 미역국은 다 맛있어요",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021434.1.12",
          "form": "맞아요 사실",
          "original_form": "맞아요 사실",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021434.1.13",
          "form": "복숭아도 다 잘먹어요.",
          "original_form": "복숭아도 다 잘먹어요.",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021434.1.14",
          "form": "물복 딱복 따질 시간에 하나라도 더 먹어야죠",
          "original_form": "물복 딱복 따질 시간에 하나라도 더 먹어야죠",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021434.1.15",
          "form": "ㅋㅋ 오늘은 말복",
          "original_form": "ㅋㅋ 오늘은 말복",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021434.1.16",
          "form": "과육만드시는건가요",
          "original_form": "과육만드시는건가요",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021434.1.17",
          "form": "껍질까서 바로먹어요",
          "original_form": "껍질까서 바로먹어요",
          "speaker_id": "2",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100021434.1.18",
          "form": "너무좋다",
          "original_form": "너무좋다",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100021434.1.19",
          "form": "전 복숭아 껍질 정말,, 싫거든요",
          "original_form": "전 복숭아 껍질 정말,, 싫거든요",
          "speaker_id": "2",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100021434.1.20",
          "form": "말랑해서 먹기도변하고",
          "original_form": "말랑해서 먹기도변하고",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100021434.1.21",
          "form": "편하고",
          "original_form": "편하고",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100021434.1.22",
