      "utterance": [
        {
          "id": "MDRW2100029387.1.1",
          "form": "좋아하는 음식 잇으신가요~?",
          "original_form": "좋아하는 음식 잇으신가요~?",
          "speaker_id": "1",
          "time": "20210903 04:16"
        },
        {
          "id": "MDRW2100029387.1.2",
          "form": "맛있으면 다 좋아해요 호호",
          "original_form": "맛있으면 다 좋아해요 호호",
          "speaker_id": "2",
          "time": "20210903 04:16"
        },
        {
          "id": "MDRW2100029387.1.3",
          "form": "지금 먹고싶은 건 치킨이에요",
          "original_form": "지금 먹고싶은 건 치킨이에요",
          "speaker_id": "2",
          "time": "20210903 04:16"
        },
        {
          "id": "MDRW2100029387.1.4",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 04:17"
        },
        {
          "id": "MDRW2100029387.1.5",
          "form": "헐 치킨 넘 좋죠",
          "original_form": "헐 치킨 넘 좋죠",
          "speaker_id": "1",
          "time": "20210903 04:17"
        },
        {
          "id": "MDRW2100029387.1.6",
          "form": "저는 면 종류 엄청 좋아해요!",
          "original_form": "저는 면 종류 엄청 좋아해요!",
          "speaker_id": "1",
          "time": "20210903 04:17"
        },
        {
          "id": "MDRW2100029387.1.7",
          "form": "오 주로 어떤 면을 드시나요",
          "original_form": "오 주로 어떤 면을 드시나요",
          "speaker_id": "2",
          "time": "20210903 04:18"
        },
        {
          "id": "MDRW2100029387.1.8",
          "form": "냉면이나 잔치국수 같은 한식종류 좋아해요~~",
          "original_form": "냉면이나 잔치국수 같은 한식종류 좋아해요~~",
          "speaker_id": "1",
          "time": "20210903 04:19"
        },
        {
          "id": "MDRW2100029387.1.9",
          "form": "라면도 좋아하구...",
          "original_form": "라면도 좋아하구...",
          "speaker_id": "1",
          "time": "20210903 04:19"
        },
        {
          "id": "MDRW2100029387.1.10",
          "form": "아아 그럼",
          "original_form": "아아 그럼",
          "speaker_id": "2",
          "time": "20210903 04:22"
        },
        {
          "id": "MDRW2100029387.1.11",
          "form": "파스타쪽은 별로신가여?",
          "original_form": "파스타쪽은 별로신가여?",
          "speaker_id": "2",
          "time": "20210903 04:22"
        },
        {
          "id": "MDRW2100029387.1.12",
          "form": "별로까지는 아니지만 엄청 좋아하지도 않아요!",
          "original_form": "별로까지는 아니지만 엄청 좋아하지도 않아요!",
          "speaker_id": "1",
          "time": "20210903 04:23"
        },
        {
          "id": "MDRW2100029387.1.13",
          "form": "국수나무 국수가 그렇게 맛있던데ㅠ",
          "original_form": "국수나무 국수가 그렇게 맛있던데ㅠ",
          "speaker_id": "1",
          "time": "20210903 04:23"
        },
        {
          "id": "MDRW2100029387.1.14",
          "form": "오 이름값하나보네요",
          "original_form": "오 이름값하나보네요",
          "speaker_id": "2",
          "time": "20210903 04:24"
        },
        {
          "id": "MDRW2100029387.1.15",
          "form": "저는 볶음면종류가 좋더라구요",
          "original_form": "저는 볶음면종류가 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 04:24"
        },
        {
          "id": "MDRW2100029387.1.16",
          "form": "이름값ㅋㅋ 그쵸!!",
          "original_form": "이름값ㅋㅋ 그쵸!!",
          "speaker_id": "1",
          "time": "20210903 04:25"
        },
        {
          "id": "MDRW2100029387.1.17",
          "form": "거긴 면이 진짜 맛있어서",
          "original_form": "거긴 면이 진짜 맛있어서",
          "speaker_id": "1",
          "time": "20210903 04:25"
        },
        {
          "id": "MDRW2100029387.1.18",
          "form": "저도 볶음면 좋아해요ㅠ~~ 배고프다",
          "original_form": "저도 볶음면 좋아해요ㅠ~~ 배고프다",
          "speaker_id": "1",
          "time": "20210903 04:26"
        },
        {
          "id": "MDRW2100029387.1.19",
          "form": "ㅋㅋ야식은 잘 안 드시나요",
          "original_form": "ㅋㅋ야식은 잘 안 드시나요",
          "speaker_id": "2",
          "time": "20210903 04:27"
        },
        {
          "id": "MDRW2100029387.1.20",
          "form": "ㅠㅠ 요즘 살 빼려고 하고 있어서",
          "original_form": "ㅠㅠ 요즘 살 빼려고 하고 있어서",
          "speaker_id": "1",
          "time": "20210903 04:27"
        },
        {
          "id": "MDRW2100029387.1.21",
          "form": "닭가슴살 같은 거나 먹어요ㅠ...",
          "original_form": "닭가슴살 같은 거나 먹어요ㅠ...",
          "speaker_id": "1",
          "time": "20210903 04:27"
        },
        {
          "id": "MDRW2100029387.1.22",
          "form": "아앗 저도 빼야하는데",
          "original_form": "아앗 저도 빼야하는데",
          "speaker_id": "2",
          "time": "20210903 04:28"
        },
        {
          "id": "MDRW2100029387.1.23",
          "form": "전 숨쉬듯 먹을 게 들어가네요",
          "original_form": "전 숨쉬듯 먹을 게 들어가네요",
          "speaker_id": "2",
          "time": "20210903 04:28"
        },
        {
          "id": "MDRW2100029387.1.24",
          "form": "ㅋㅋㅋㅋㅠㅠ 그쵸... 뭔지 알아요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅠㅠ 그쵸... 뭔지 알아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 04:29"
        },
        {
          "id": "MDRW2100029387.1.25",
          "form": "전 사실 고기를 제일 좋아합니다",
          "original_form": "전 사실 고기를 제일 좋아합니다",
