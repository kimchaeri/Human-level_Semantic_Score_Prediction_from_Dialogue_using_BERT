      "utterance": [
        {
          "id": "MDRW2100039564.1.1",
          "form": "요즘 좋아하는 드라마는?",
          "original_form": "요즘 좋아하는 드라마는?",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100039564.1.2",
          "form": "요즘은 중국드라마랑일본드라마 많이 보는데",
          "original_form": "요즘은 중국드라마랑일본드라마 많이 보는데",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100039564.1.3",
          "form": "뭘로보노? 티비?",
          "original_form": "뭘로보노? 티비?",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100039564.1.4",
          "form": "아니 어플로 요즘은 다 봐진다 한달에 얼마내면 예능 영화도 다 볼 수 있다",
          "original_form": "아니 어플로 요즘은 다 봐진다 한달에 얼마내면 예능 영화도 다 볼 수 있다",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100039564.1.5",
          "form": "그러면 영화관에 안 가도 되겠네",
          "original_form": "그러면 영화관에 안 가도 되겠네",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100039564.1.6",
          "form": "요즘 코로나 때문도 있지만 오티티 때메 영화관 안가는 사람도 많지",
          "original_form": "요즘 코로나 때문도 있지만 오티티 때메 영화관 안가는 사람도 많지",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100039564.1.7",
          "form": "그래 영화값 올라서 비싸더라",
          "original_form": "그래 영화값 올라서 비싸더라",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100039564.1.8",
          "form": "ㅇㅇ 영화비 너무 비쌈 ㅡㅡ",
          "original_form": "ㅇㅇ 영화비 너무 비쌈 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100039564.1.9",
          "form": "딱 영화관에서 보고싶은거만 보고",
          "original_form": "딱 영화관에서 보고싶은거만 보고",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100039564.1.10",
          "form": "나머지는 어플로",
          "original_form": "나머지는 어플로",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100039564.1.11",
          "form": "나도 그거 틀어주라",
          "original_form": "나도 그거 틀어주라",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100039564.1.12",
          "form": "영화나 드라마 보고싶은거있나",
          "original_form": "영화나 드라마 보고싶은거있나",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100039564.1.13",
          "form": "펜트하우스 그거 안본지 좀 된다",
          "original_form": "펜트하우스 그거 안본지 좀 된다",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100039564.1.14",
          "form": "펜트하우스는 내가 보는 어플에는 없는데",
          "original_form": "펜트하우스는 내가 보는 어플에는 없는데",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100039564.1.15",
          "form": "다른거는",
          "original_form": "다른거는",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100039564.1.16",
          "form": "그러면 이제만나러갑니다",
          "original_form": "그러면 이제만나러갑니다",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100039564.1.17",
          "form": "그거 저번주안봤나",
          "original_form": "그거 저번주안봤나",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100039564.1.18",
          "form": "잔다고 못봤다",
          "original_form": "잔다고 못봤다",
          "speaker_id": "2",
          "time": "20210907 21:57"
        }
      ]
