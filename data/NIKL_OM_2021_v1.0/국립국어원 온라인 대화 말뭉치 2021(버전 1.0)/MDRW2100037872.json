      "utterance": [
        {
          "id": "MDRW2100037872.1.1",
          "form": "제주오늘날씨어때요 서울은 비안오네요",
          "original_form": "제주오늘날씨어때요 서울은 비안오네요",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100037872.1.2",
          "form": "지금 번개치고 난리요",
          "original_form": "지금 번개치고 난리요",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100037872.1.3",
          "form": "호고공",
          "original_form": "호고공",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037872.1.4",
          "form": "번개요?!",
          "original_form": "번개요?!",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037872.1.5",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037872.1.6",
          "form": "요며칠 날씨가 좋더라니",
          "original_form": "요며칠 날씨가 좋더라니",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037872.1.7",
          "form": "ㅜㅜㅜ무섭당 밖에 나가시면안되겠어용",
          "original_form": "ㅜㅜㅜ무섭당 밖에 나가시면안되겠어용",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037872.1.8",
          "form": "이게.맑은이라고 해도",
          "original_form": "이게.맑은이라고 해도",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037872.1.9",
          "form": "가끔 비가 와",
          "original_form": "가끔 비가 와",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037872.1.10",
          "form": "그리고 시침 뚝",
          "original_form": "그리고 시침 뚝",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037872.1.11",
          "form": "손님은 딱 좋은날씨에 살다갔네요ㅋㅋ외출도안하셨지만ㅋ",
          "original_form": "손님은 딱 좋은날씨에 살다갔네요ㅋㅋ외출도안하셨지만ㅋ",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037872.1.12",
          "form": "아 그러게",
          "original_form": "아 그러게",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037872.1.13",
          "form": "아근데 비오면 비행기가 안뜰수있어서 일찍간다던데",
          "original_form": "아근데 비오면 비행기가 안뜰수있어서 일찍간다던데",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037872.1.14",
          "form": "아침뱅기루 갔담소?",
          "original_form": "아침뱅기루 갔담소?",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037872.1.15",
          "form": "ㅇ 집에 계시겠군요 님은 우산좀 챙기쇼 감기걸려유",
          "original_form": "ㅇ 집에 계시겠군요 님은 우산좀 챙기쇼 감기걸려유",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037872.1.16",
          "form": "야스",
          "original_form": "야스",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100037872.1.17",
          "form": "name3씨는 일기예보 보면서 비행기표 끊으신 듯",
          "original_form": "&name3&씨는 일기예보 보면서 비행기표 끊으신 듯",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100037872.1.18",
          "form": "오 스마트해 아가씨",
          "original_form": "오 스마트해 아가씨",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037872.1.19",
          "form": "우산이나 우비같은거 딱 호주머니 넣고당기쇼",
          "original_form": "우산이나 우비같은거 딱 호주머니 넣고당기쇼",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037872.1.20",
          "form": "너는 비오면 우울하지 않남",
          "original_form": "너는 비오면 우울하지 않남",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037872.1.21",
          "form": "난 저혈압이라",
          "original_form": "난 저혈압이라",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037872.1.22",
          "form": "시름데",
          "original_form": "시름데",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037872.1.23",
          "form": "움..머리가 곱슬거려 우울하시",
          "original_form": "움..머리가 곱슬거려 우울하시",
