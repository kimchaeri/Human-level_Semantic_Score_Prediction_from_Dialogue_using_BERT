      "utterance": [
        {
          "id": "MDRW2100044836.1.1",
          "form": "일하시나요",
          "original_form": "일하시나요",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044836.1.2",
          "form": "아니요 현재 이직 준비중이에요",
          "original_form": "아니요 현재 이직 준비중이에요",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044836.1.3",
          "form": "일하시나요?",
          "original_form": "일하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044836.1.4",
          "form": "아니용 학생이고 졸업해서 취직해야져",
          "original_form": "아니용 학생이고 졸업해서 취직해야져",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044836.1.5",
          "form": "이직 왜요? 돈?",
          "original_form": "이직 왜요? 돈?",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044836.1.6",
          "form": "저는 해외 취업하고 귀국한거든요",
          "original_form": "저는 해외 취업하고 귀국한거든요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044836.1.7",
          "form": "헉 해외어디용",
          "original_form": "헉 해외어디용",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044836.1.8",
          "form": "저는 유럽에서 무역쪽 근무했었어요",
          "original_form": "저는 유럽에서 무역쪽 근무했었어요",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044836.1.9",
          "form": "와..인텔리시네용 ..",
          "original_form": "와..인텔리시네용 ..",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044836.1.10",
          "form": "부러워요유럽이라니",
          "original_form": "부러워요유럽이라니",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044836.1.11",
          "form": "한국서 일하시게요?",
          "original_form": "한국서 일하시게요?",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044836.1.12",
          "form": "네 한국에서 일해야겠죠?",
          "original_form": "네 한국에서 일해야겠죠?",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044836.1.13",
          "form": "한국 살고 싶으셔서요?",
          "original_form": "한국 살고 싶으셔서요?",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044836.1.14",
          "form": "부모님이 걱정하셔서 그런가봐요 그래도 나름 5년 나가서 있었거든요",
          "original_form": "부모님이 걱정하셔서 그런가봐요 그래도 나름 5년 나가서 있었거든요",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044836.1.15",
          "form": "어 오래잇으셧네요ㅠ 어떠셧나요",
          "original_form": "어 오래잇으셧네요ㅠ 어떠셧나요",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044836.1.16",
          "form": "그냥 한국 생활이랑 비슷해요",
          "original_form": "그냥 한국 생활이랑 비슷해요",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044836.1.17",
          "form": "안외로우셧어요?",
          "original_form": "안외로우셧어요?",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044836.1.18",
          "form": "딱히 외롭고 그리워하고 남 신경 쓰는 타입은 아니라서 괜찮아요",
          "original_form": "딱히 외롭고 그리워하고 남 신경 쓰는 타입은 아니라서 괜찮아요",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044836.1.19",
          "form": "멋지세영",
          "original_form": "멋지세영",
          "speaker_id": "1",
