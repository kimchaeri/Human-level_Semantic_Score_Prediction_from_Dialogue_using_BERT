      "utterance": [
        {
          "id": "MDRW2100047387.1.1",
          "form": "오늘 간만에 날씨가 맑네요",
          "original_form": "오늘 간만에 날씨가 맑네요",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100047387.1.2",
          "form": "네 해가 쨍쨍하네요",
          "original_form": "네 해가 쨍쨍하네요",
          "speaker_id": "2",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100047387.1.3",
          "form": "며칠 계속 비가와서 집이 눅눅햇어요 ㅠㅠ",
          "original_form": "며칠 계속 비가와서 집이 눅눅햇어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100047387.1.4",
          "form": "저는 제습기틀었어요",
          "original_form": "저는 제습기틀었어요",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100047387.1.5",
          "form": "저는 제습기가 없어서 에어컨 틀엇어요",
          "original_form": "저는 제습기가 없어서 에어컨 틀엇어요",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100047387.1.6",
          "form": "내일또 비소식 잇더라구요",
          "original_form": "내일또 비소식 잇더라구요",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100047387.1.7",
          "form": "또요??",
          "original_form": "또요??",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100047387.1.8",
          "form": "그만왔으면좋겠네요",
          "original_form": "그만왔으면좋겠네요",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100047387.1.9",
          "form": "네 이제 그만왔으면 좋겟어요",
          "original_form": "네 이제 그만왔으면 좋겟어요",
          "speaker_id": "1",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100047387.1.10",
          "form": "올해는 비가 많이오네요",
          "original_form": "올해는 비가 많이오네요",
          "speaker_id": "1",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100047387.1.11",
          "form": "기후가 이상해졌어요",
          "original_form": "기후가 이상해졌어요",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100047387.1.12",
          "form": "맞아요 외국에 산불나는것도 심상치않더라구요",
          "original_form": "맞아요 외국에 산불나는것도 심상치않더라구요",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100047387.1.13",
          "form": "부산에 홍수났대요",
          "original_form": "부산에 홍수났대요",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100047387.1.14",
          "form": "이번에 영남지방에 비가 많이왓나보더라구요",
          "original_form": "이번에 영남지방에 비가 많이왓나보더라구요",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100047387.1.15",
          "form": "원래는 잘 안오는 지역인데",
          "original_form": "원래는 잘 안오는 지역인데",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047387.1.16",
          "form": "10년뒤면 부산이 잠긴대요",
          "original_form": "10년뒤면 부산이 잠긴대요",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100047387.1.17",
          "form": "그정도예요?? 진짜 심각하네요",
