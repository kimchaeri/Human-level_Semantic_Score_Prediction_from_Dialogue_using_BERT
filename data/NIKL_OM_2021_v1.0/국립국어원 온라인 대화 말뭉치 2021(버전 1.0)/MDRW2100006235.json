      "utterance": [
        {
          "id": "MDRW2100006235.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006235.1.2",
          "form": "앗 아니요",
          "original_form": "앗 아니요",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006235.1.3",
          "form": "키우는 동물은 없어요",
          "original_form": "키우는 동물은 없어요",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006235.1.4",
          "form": "동물 안좋아하시는건가요??",
          "original_form": "동물 안좋아하시는건가요??",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006235.1.5",
          "form": "딱히 그런건 아닌데",
          "original_form": "딱히 그런건 아닌데",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100006235.1.6",
          "form": "잘 키울 자신은 없는 거 같아요",
          "original_form": "잘 키울 자신은 없는 거 같아요",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100006235.1.7",
          "form": "요즘은 키우진 않고 그냥 길고양이 보고 좋아하고 사진찍고 하는 정도에요",
          "original_form": "요즘은 키우진 않고 그냥 길고양이 보고 좋아하고 사진찍고 하는 정도에요",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100006235.1.8",
          "form": "헐 길고양이랑 친하신가요",
          "original_form": "헐 길고양이랑 친하신가요",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100006235.1.9",
          "form": "길고양이 있으면 다가가는 편이에요",
          "original_form": "길고양이 있으면 다가가는 편이에요",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006235.1.10",
          "form": "다가가면 도망가지않나요?ㅠㅜ",
          "original_form": "다가가면 도망가지않나요?ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006235.1.11",
          "form": "요즘 길고양이들은 대부분 사람한테 잘 다가가는 거 같더라고요",
          "original_form": "요즘 길고양이들은 대부분 사람한테 잘 다가가는 거 같더라고요",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100006235.1.12",
          "form": "전 동물 되게 좋아해서 유기견 봉사도 다니고 그랬는데",
          "original_form": "전 동물 되게 좋아해서 유기견 봉사도 다니고 그랬는데",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006235.1.13",
          "form": "길고양이랑 친해질수가없더라구요ㅠㅜ",
          "original_form": "길고양이랑 친해질수가없더라구요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006235.1.14",
          "form": "헉 그런가요",
          "original_form": "헉 그런가요",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006235.1.15",
          "form": "이것도 동네마다 조금씩 다른가보네요 ㅠㅠ",
          "original_form": "이것도 동네마다 조금씩 다른가보네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006235.1.16",
          "form": "그런가봐요ㅠㅜ",
          "original_form": "그런가봐요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006235.1.17",
          "form": "혹시 name1님은 반려동물 키우시나요??",
          "original_form": "혹시 &name1&님은 반려동물 키우시나요??",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006235.1.18",
          "form": "저는 못키우고있어요ㅠㅜ",
          "original_form": "저는 못키우고있어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006235.1.19",
          "form": "키우고 싶으신 동물 있으세요??",
          "original_form": "키우고 싶으신 동물 있으세요??",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006235.1.20",
          "form": "네 미어캣이랑 강아지 관심있어요ㅠㅜ",
          "original_form": "네 미어캣이랑 강아지 관심있어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006235.1.21",
          "form": "강아지도 완전 귀엽죠 ㅠㅠ",
          "original_form": "강아지도 완전 귀엽죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006235.1.22",
          "form": "강아지키우고싶어요ㅠㅜ",
          "original_form": "강아지키우고싶어요ㅠㅜ",
