      "utterance": [
        {
          "id": "MDRW2100047117.1.1",
          "form": "하고 계신 운동 있으세요?",
          "original_form": "하고 계신 운동 있으세요?",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100047117.1.2",
          "form": "저 헬스하다가 쉬고있어요",
          "original_form": "저 헬스하다가 쉬고있어요",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100047117.1.3",
          "form": "시국이 시국인지라..",
          "original_form": "시국이 시국인지라..",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100047117.1.4",
          "form": "아 ㅜㅜ 운동 못해서 답답하시겠어요",
          "original_form": "아 ㅜㅜ 운동 못해서 답답하시겠어요",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100047117.1.5",
          "form": "네ㅠㅠ 운동 하시는 것 있으세요?",
          "original_form": "네ㅠㅠ 운동 하시는 것 있으세요?",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100047117.1.6",
          "form": "저는 달리기 하고 있어요!",
          "original_form": "저는 달리기 하고 있어요!",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100047117.1.7",
          "form": "실외는 좀 괜찮을 것 같아서..",
          "original_form": "실외는 좀 괜찮을 것 같아서..",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100047117.1.8",
          "form": "아아 달리기 좋죠!! 저도 해야하는데...",
          "original_form": "아아 달리기 좋죠!! 저도 해야하는데...",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100047117.1.9",
          "form": "핼스는 꾸준히 해도 달리기가 은근 귀찮더라고요ㅋㅋㅋ",
          "original_form": "핼스는 꾸준히 해도 달리기가 은근 귀찮더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100047117.1.10",
          "form": "저는 헬스 하시는 분들이 진짜 대단하신 것 같아요!!",
          "original_form": "저는 헬스 하시는 분들이 진짜 대단하신 것 같아요!!",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100047117.1.11",
          "form": "참고 견디시는게 너무 멋진 것 같아요",
          "original_form": "참고 견디시는게 너무 멋진 것 같아요",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100047117.1.12",
          "form": "감사합니다! 나중에 잠잠해지면 한번 도전해보세요",
          "original_form": "감사합니다! 나중에 잠잠해지면 한번 도전해보세요",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100047117.1.13",
          "form": "한달 정도 지나면 은근 중독되더라고요!",
          "original_form": "한달 정도 지나면 은근 중독되더라고요!",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100047117.1.14",
          "form": "그렇군요!",
          "original_form": "그렇군요!",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100047117.1.15",
          "form": "피티받으시는 거예요?",
          "original_form": "피티받으시는 거예요?",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100047117.1.16",
          "form": "전 피티는 예전에 받았었고, 혼자 운동하고 있어요",
          "original_form": "전 피티는 예전에 받았었고, 혼자 운동하고 있어요",
          "speaker_id": "2",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100047117.1.17",
          "form": "오래하셨나보다!",
          "original_form": "오래하셨나보다!",
          "speaker_id": "1",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100047117.1.18",
          "form": "20대 치고는 오래한 것 같아요.",
          "original_form": "20대 치고는 오래한 것 같아요.",
          "speaker_id": "2",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100047117.1.19",
          "form": "4년 살짝 넘긴 것 같네요",
          "original_form": "4년 살짝 넘긴 것 같네요",
          "speaker_id": "2",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100047117.1.20",
          "form": "우와아아아",
          "original_form": "우와아아아",
          "speaker_id": "1",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100047117.1.21",
          "form": "한가지를 그렇게 꾸준히 할 수 있다는 건 진짜 멋진 것 같아요 ㅋㅋ",
          "original_form": "한가지를 그렇게 꾸준히 할 수 있다는 건 진짜 멋진 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100047117.1.22",
          "form": "감사합니다!!",
          "original_form": "감사합니다!!",
          "speaker_id": "2",
