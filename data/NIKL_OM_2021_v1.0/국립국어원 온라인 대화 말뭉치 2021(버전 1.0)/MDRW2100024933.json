      "utterance": [
        {
          "id": "MDRW2100024933.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100024933.1.2",
          "form": "안녕하세요 학생이세요?",
          "original_form": "안녕하세요 학생이세요?",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100024933.1.3",
          "form": "안녕하세요. 반직장인 반 학생이에요 ㅎㅎ",
          "original_form": "안녕하세요. 반직장인 반 학생이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100024933.1.4",
          "form": "와 직장과 학업을 동시에... 대단하시군요",
          "original_form": "와 직장과 학업을 동시에... 대단하시군요",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100024933.1.5",
          "form": "직장업무 관련된걸 배우시나요?",
          "original_form": "직장업무 관련된걸 배우시나요?",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100024933.1.6",
          "form": "아 대학원생 입니다.ㅠ",
          "original_form": "아 대학원생 입니다.ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100024933.1.7",
          "form": "일 하시나요?",
          "original_form": "일 하시나요?",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100024933.1.8",
          "form": "아니요 전 건강문제로 쉬고있어요",
          "original_form": "아니요 전 건강문제로 쉬고있어요",
          "speaker_id": "2",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100024933.1.9",
          "form": "아 제 친구 중에도 대학원생이 있어요 전공이 뭐예요?",
          "original_form": "아 제 친구 중에도 대학원생이 있어요 전공이 뭐예요?",
          "speaker_id": "2",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100024933.1.10",
          "form": "화학공학이요!!",
          "original_form": "화학공학이요!!",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100024933.1.11",
          "form": "와 공대생!! 부럽네요 왠지 공대에 대한 막연한 환상이 있어서요ㅋㅋ",
          "original_form": "와 공대생!! 부럽네요 왠지 공대에 대한 막연한 환상이 있어서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100024933.1.12",
          "form": "그런가여 ㅋㅋㅋ 별거 없어요 ㅠㅠㅠ",
          "original_form": "그런가여 ㅋㅋㅋ 별거 없어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100024933.1.13",
          "form": "어렸응 때 학교 다니는 거 좋아하는 학생이셨나요",
          "original_form": "어렸응 때 학교 다니는 거 좋아하는 학생이셨나요",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100024933.1.14",
          "form": "ㅋㅋ 아뇨 그냥 지겨웠죠 저희는 동아리도 그렇게 활성화돼있지 않았거든요. 님은요?",
          "original_form": "ㅋㅋ 아뇨 그냥 지겨웠죠 저희는 동아리도 그렇게 활성화돼있지 않았거든요. 님은요?",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100024933.1.15",
          "form": "전 중학교때까진 학교 다니는 거 좋아했어요 ㅎㅎ 급식은 맛있앗나요",
          "original_form": "전 중학교때까진 학교 다니는 거 좋아했어요 ㅎㅎ 급식은 맛있앗나요",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100024933.1.16",
          "form": "저는 뭐 그럭저럭 괜찮았는데 대부분 학생들은 싫어했어요ㅋㅋ",
          "original_form": "저는 뭐 그럭저럭 괜찮았는데 대부분 학생들은 싫어했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100024933.1.17",
          "form": "그래도 중학교때까지 학교다니는걸 좋아하셨다니 어떤 점이 좋았나요?",
          "original_form": "그래도 중학교때까지 학교다니는걸 좋아하셨다니 어떤 점이 좋았나요?",
          "speaker_id": "2",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100024933.1.18",
          "form": "음..친구들도 선생님들도 착하고 친절했어요. 운이 좋았죠",
          "original_form": "음..친구들도 선생님들도 착하고 친절했어요. 운이 좋았죠",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100024933.1.19",
          "form": "수학여행은 어디로 가셨나요",
          "original_form": "수학여행은 어디로 가셨나요",
          "speaker_id": "1",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100024933.1.20",
          "form": "초중고 다 제주도로 갔어요",
          "original_form": "초중고 다 제주도로 갔어요",
          "speaker_id": "2",
          "time": "20210903 20:33"
        }
      ]
