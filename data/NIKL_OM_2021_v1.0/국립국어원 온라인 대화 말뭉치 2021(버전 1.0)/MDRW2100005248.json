      "utterance": [
        {
          "id": "MDRW2100005248.1.1",
          "form": "치킨은언제나",
          "original_form": "치킨은언제나",
          "speaker_id": "1",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005248.1.2",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005248.1.3",
          "form": "치킨은 진리죠",
          "original_form": "치킨은 진리죠",
          "speaker_id": "2",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005248.1.4",
          "form": "내일도 치킨을 먹고 싶어요",
          "original_form": "내일도 치킨을 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005248.1.5",
          "form": "어제 짬뽕자장면 같이 먹는 영상 보았는데 참기 힘들더라구요",
          "original_form": "어제 짬뽕자장면 같이 먹는 영상 보았는데 참기 힘들더라구요",
          "speaker_id": "2",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005248.1.6",
          "form": "중국 음식도 엄청나죠",
          "original_form": "중국 음식도 엄청나죠",
          "speaker_id": "1",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005248.1.7",
          "form": "짬뽕은 비 오는 날에 먹으면 진짜 세상 부러울 거 없는듯",
          "original_form": "짬뽕은 비 오는 날에 먹으면 진짜 세상 부러울 거 없는듯",
          "speaker_id": "1",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005248.1.8",
          "form": "내일은 비가 안온대요",
          "original_form": "내일은 비가 안온대요",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005248.1.9",
          "form": "비오는 날엔 국물음식이나 기름진게 좋은것 같아요",
          "original_form": "비오는 날엔 국물음식이나 기름진게 좋은것 같아요",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005248.1.10",
          "form": "치킨 말고 어떤 음식 좋아하시나요",
          "original_form": "치킨 말고 어떤 음식 좋아하시나요",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005248.1.11",
          "form": "일주일 비 오는 거 아닌가요. 초밥요",
          "original_form": "일주일 비 오는 거 아닌가요. 초밥요",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005248.1.12",
          "form": "월요일부터 일주일 내내 온대요",
          "original_form": "월요일부터 일주일 내내 온대요",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005248.1.13",
          "form": "전 고기요",
          "original_form": "전 고기요",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005248.1.14",
          "form": "그러면 비올땐 파전",
          "original_form": "그러면 비올땐 파전",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005248.1.15",
          "form": "좋죠 막걸리 한잔",
          "original_form": "좋죠 막걸리 한잔",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005248.1.16",
          "form": "네 파전에 막걸리 좋죠",
          "original_form": "네 파전에 막걸리 좋죠",
          "speaker_id": "1",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005248.1.17",
          "form": "맛집 찾아다니시나요",
          "original_form": "맛집 찾아다니시나요",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005248.1.18",
          "form": "코로나라서요",
          "original_form": "코로나라서요",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005248.1.19",
          "form": "배달로 먹습니다",
          "original_form": "배달로 먹습니다",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005248.1.20",
          "form": "네예",
          "original_form": "네예",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005248.1.21",
          "form": "오이가 먹고싶네요",
          "original_form": "오이가 먹고싶네요",
