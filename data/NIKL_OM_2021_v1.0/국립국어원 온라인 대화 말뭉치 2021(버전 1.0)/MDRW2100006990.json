        },
        {
          "id": "MDRW2100006990.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006990.1.3",
          "form": "군대 다녀오셨나요?",
          "original_form": "군대 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006990.1.4",
          "form": "저는 군대 안 다녀왔어요ㅎㅎ",
          "original_form": "저는 군대 안 다녀왔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006990.1.5",
          "form": "네 여군이였거든요",
          "original_form": "네 여군이였거든요",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006990.1.6",
          "form": "지금은 전역했습니다",
          "original_form": "지금은 전역했습니다",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006990.1.7",
          "form": "군대에 대해서 궁금한점 있으신가요",
          "original_form": "군대에 대해서 궁금한점 있으신가요",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006990.1.8",
          "form": "여군 너무 멋있어요!!",
          "original_form": "여군 너무 멋있어요!!",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006990.1.9",
          "form": "군대 있으실 때 주말에는 쉬셨나요?",
          "original_form": "군대 있으실 때 주말에는 쉬셨나요?",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006990.1.10",
          "form": "네 해군이였지만 배타는 직별이 아니여서 주말 공휴일 다쉬었어요",
          "original_form": "네 해군이였지만 배타는 직별이 아니여서 주말 공휴일 다쉬었어요",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006990.1.11",
          "form": "우와 해군인데도 배를 안 타실 수 있구나",
          "original_form": "우와 해군인데도 배를 안 타실 수 있구나",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006990.1.12",
          "form": "여군 진짜 대단하고 멋있는 거 같아요",
          "original_form": "여군 진짜 대단하고 멋있는 거 같아요",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006990.1.13",
          "form": "요즘 특히 올림픽 보면서 열심히 하는 여자들 너무 멋있다고 생각했거든요",
          "original_form": "요즘 특히 올림픽 보면서 열심히 하는 여자들 너무 멋있다고 생각했거든요",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006990.1.14",
          "form": "저는 의무직별이라 병원에 있었거든요",
          "original_form": "저는 의무직별이라 병원에 있었거든요",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006990.1.15",
          "form": "여군도 마찬가지로 너무 대단하죠!",
          "original_form": "여군도 마찬가지로 너무 대단하죠!",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006990.1.16",
          "form": "남녀 군인들 다 보셨나요??",
          "original_form": "남녀 군인들 다 보셨나요??",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006990.1.17",
          "form": "네네 다봤어요",
          "original_form": "네네 다봤어요",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006990.1.18",
          "form": "전역을 한달만 늦게했어도 박보검을 볼수있었을텐데 아쉽네요",
          "original_form": "전역을 한달만 늦게했어도 박보검을 볼수있었을텐데 아쉽네요",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006990.1.19",
          "form": "아 헐ㅠㅠ 너무 아쉽겠어요!!!",
          "original_form": "아 헐ㅠㅠ 너무 아쉽겠어요!!!",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006990.1.20",
          "form": "해군 보검... 궁금하네요 저도 보고싶어요",
          "original_form": "해군 보검... 궁금하네요 저도 보고싶어요",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006990.1.21",
          "form": "해병대는 의무직별이 없어서 해군쪽으로 병원 오거나 저희가 해병대쪽으로 가서 일하거든요",
          "original_form": "해병대는 의무직별이 없어서 해군쪽으로 병원 오거나 저희가 해병대쪽으로 가서 일하거든요",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006990.1.22",
          "form": "샤이니 민호는 봤어요",
          "original_form": "샤이니 민호는 봤어요",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006990.1.23",
          "form": "치과진료하러 해군병원에 왔더라구요",
          "original_form": "치과진료하러 해군병원에 왔더라구요",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006990.1.24",
          "form": "우와 그렇게도 보게 되네요",
          "original_form": "우와 그렇게도 보게 되네요",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006990.1.25",
          "form": "군대에 그럼 얼마동안 계셨던 거예요?",
          "original_form": "군대에 그럼 얼마동안 계셨던 거예요?",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006990.1.26",
          "form": "5년정도 있었어요",
          "original_form": "5년정도 있었어요",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006990.1.27",
          "form": "5년이면 오래 계셨네요! 전역하실 때 시원섭섭하셨겠어요ㅠㅠ",
          "original_form": "5년이면 오래 계셨네요! 전역하실 때 시원섭섭하셨겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006990.1.28",
          "form": "아니요 군대가 뉴스보다시피 사건사고도 많고 고지식하고 그래서 안맞더라구요",
          "original_form": "아니요 군대가 뉴스보다시피 사건사고도 많고 고지식하고 그래서 안맞더라구요",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100006990.1.29",
          "form": "그냥 군대에서 좋은인연들 만난걸로 만족하고 전역했습니다",
          "original_form": "그냥 군대에서 좋은인연들 만난걸로 만족하고 전역했습니다",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100006990.1.30",
          "form": "아아ㅠㅠ 그래도 좋은 인연 만나셔서 너무 다행이에요!",
          "original_form": "아아ㅠㅠ 그래도 좋은 인연 만나셔서 너무 다행이에요!",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006990.1.31",
          "form": "맞아요 군대... 뉴스에도 많이 나오는데 안 나오는 거까지 하면 정말 사건 많을 거 같아요ㅠ",
          "original_form": "맞아요 군대... 뉴스에도 많이 나오는데 안 나오는 거까지 하면 정말 사건 많을 거 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006990.1.32",
          "form": "네 사실 큰사건은 뉴스나오고 작은사건은 쉬쉬하는 경우가 진짜많아요",
          "original_form": "네 사실 큰사건은 뉴스나오고 작은사건은 쉬쉬하는 경우가 진짜많아요",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006990.1.33",
          "form": "성관련사건같은경우도 보통 피해자들이 인사이동하구요ㅠ",
          "original_form": "성관련사건같은경우도 보통 피해자들이 인사이동하구요ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006990.1.34",
          "form": "주변에 저도 부대 괴롭힘으로 일찍 제대한 경우 있었어요ㅠ",
          "original_form": "주변에 저도 부대 괴롭힘으로 일찍 제대한 경우 있었어요ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006990.1.35",
          "form": "아 성문제 정말 요즘 너무 심각한 문제잖아요",
          "original_form": "아 성문제 정말 요즘 너무 심각한 문제잖아요",
          "speaker_id": "2",
          "time": "20210822 00:26"
        }
      ]
    }
