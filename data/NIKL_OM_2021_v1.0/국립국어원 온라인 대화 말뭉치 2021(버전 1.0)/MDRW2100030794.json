      "utterance": [
        {
          "id": "MDRW2100030794.1.1",
          "form": "나중에 같이 보러갈까?",
          "original_form": "나중에 같이 보러갈까?",
          "speaker_id": "2",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030794.1.2",
          "form": "언제오니",
          "original_form": "언제오니",
          "speaker_id": "2",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030794.1.3",
          "form": "종강하면 ㅜㅜ...",
          "original_form": "종강하면 ㅜㅜ...",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030794.1.4",
          "form": "아무튼 그 아저씨 무슨 불법수집이라던데",
          "original_form": "아무튼 그 아저씨 무슨 불법수집이라던데",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030794.1.5",
          "form": "장물수집",
          "original_form": "장물수집",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030794.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030794.1.7",
          "form": "근데 막 컬렉션이라고 띄워주는거 웃겨요",
          "original_form": "근데 막 컬렉션이라고 띄워주는거 웃겨요",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030794.1.8",
          "form": "유명한 사람들 많다더라",
          "original_form": "유명한 사람들 많다더라",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030794.1.9",
          "form": "그건 그래보였어요 함 가보면 좋을지두??",
          "original_form": "그건 그래보였어요 함 가보면 좋을지두??",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030794.1.10",
          "form": "삼성에서 내주는거니까 ㅎ",
          "original_form": "삼성에서 내주는거니까 ㅎ",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030794.1.11",
          "form": "그거 하고 죄 줄어들면 좋지",
          "original_form": "그거 하고 죄 줄어들면 좋지",
          "speaker_id": "2",
          "time": "20210904 12:39"
        },
        {
          "id": "MDRW2100030794.1.12",
          "form": "뭐 그렇긴 하지만요 사면해주고 ㅋㅋㅋ",
          "original_form": "뭐 그렇긴 하지만요 사면해주고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:39"
        },
        {
          "id": "MDRW2100030794.1.13",
          "form": "아무튼 다음에 가봐요!! ㅎㅎ",
          "original_form": "아무튼 다음에 가봐요!! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 12:39"
        },
        {
          "id": "MDRW2100030794.1.14",
          "form": "방학때 오면 많이 가자",
          "original_form": "방학때 오면 많이 가자",
          "speaker_id": "2",
          "time": "20210904 12:39"
        },
        {
          "id": "MDRW2100030794.1.15",
          "form": "코로나때문에 걱정이긴 하지만",
          "original_form": "코로나때문에 걱정이긴 하지만",
          "speaker_id": "2",
          "time": "20210904 12:39"
        },
        {
          "id": "MDRW2100030794.1.16",
          "form": "그때쯤이면 접종도 다 끝났을테니까요",
          "original_form": "그때쯤이면 접종도 다 끝났을테니까요",
          "speaker_id": "1",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100030794.1.17",
          "form": "2차 언제니?",
          "original_form": "2차 언제니?",
          "speaker_id": "2",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100030794.1.18",
          "form": "10월이에요 ㅋ 가요 종강후니까",
          "original_form": "10월이에요 ㅋ 가요 종강후니까",
          "speaker_id": "1",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100030794.1.19",
          "form": "그래 재밌겠다",
          "original_form": "그래 재밌겠다",
          "speaker_id": "2",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100030794.1.20",
          "form": "공짜니?",
          "original_form": "공짜니?",
          "speaker_id": "2",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100030794.1.21",
          "form": "그러지 않을까요?",
          "original_form": "그러지 않을까요?",
          "speaker_id": "1",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100030794.1.22",
          "form": "돈내고 보라고 하면 나쁜데",
          "original_form": "돈내고 보라고 하면 나쁜데",
          "speaker_id": "1",
          "time": "20210904 12:40"
        }
