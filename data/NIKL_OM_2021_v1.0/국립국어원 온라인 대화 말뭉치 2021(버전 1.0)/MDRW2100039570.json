      "utterance": [
        {
          "id": "MDRW2100039570.1.1",
          "form": "고등어랑 흰냥이에요 ㅎㅎㅎ",
          "original_form": "고등어랑 흰냥이에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039570.1.2",
          "form": "아빠가 고등어였나바요 ㅋㅋㅋㅋ 엄마가 흰냥이었어서",
          "original_form": "아빠가 고등어였나바요 ㅋㅋㅋㅋ 엄마가 흰냥이었어서",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039570.1.3",
          "form": "흐어어어어ㅓㅠㅠㅠ지금 상상하는 것만으로도",
          "original_form": "흐어어어어ㅓㅠㅠㅠ지금 상상하는 것만으로도",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039570.1.4",
          "form": "마음이 따땃해졌어요ㅠㅠㅠ",
          "original_form": "마음이 따땃해졌어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039570.1.5",
          "form": "최근에 뭐 구매하셨어용?!",
          "original_form": "최근에 뭐 구매하셨어용?!",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039570.1.6",
          "form": "저는 메론이랑 꽃게요 ㅎㅎ",
          "original_form": "저는 메론이랑 꽃게요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039570.1.7",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039570.1.8",
          "form": "ㅋㅋㅋ전 무선 키보드요!!",
          "original_form": "ㅋㅋㅋ전 무선 키보드요!!",
          "speaker_id": "2",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039570.1.9",
          "form": "노트북 거치대를 샀더니",
          "original_form": "노트북 거치대를 샀더니",
          "speaker_id": "2",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039570.1.10",
          "form": "키보드가 올라가서ㅋㅋㅋ",
          "original_form": "키보드가 올라가서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039570.1.11",
          "form": "ㅋㅋㅋ 좋은 노트북 사셨나요?!",
          "original_form": "ㅋㅋㅋ 좋은 노트북 사셨나요?!",
          "speaker_id": "1",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039570.1.12",
          "form": "저도 사고싶은데..ㅡ컴 고장나서",
          "original_form": "저도 사고싶은데..ㅡ컴 고장나서",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039570.1.13",
          "form": "고3때 자소서 쓰려구 샀어요ㅋㅋㅋㅋ",
          "original_form": "고3때 자소서 쓰려구 샀어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039570.1.14",
          "form": "샀을땐 손도 못댔는데",
          "original_form": "샀을땐 손도 못댔는데",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039570.1.15",
          "form": "이젠 너무 익숙해져서ㅋㅋㅋㅋ",
          "original_form": "이젠 너무 익숙해져서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039570.1.16",
          "form": "오오 열심히 쓰셔야겠네요 ㅜㅜ",
          "original_form": "오오 열심히 쓰셔야겠네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039570.1.17",
          "form": "저는 게임용 사고싶어요  ㅎㄹ",
          "original_form": "저는 게임용 사고싶어요  ㅎㄹ",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039570.1.18",
          "form": "와 저두요ㅋㅋㅋㅋ",
          "original_form": "와 저두요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039570.1.19",
          "form": "완전 꿈의 노트북이에요ㅋㅋㅋ",
          "original_form": "완전 꿈의 노트북이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039570.1.20",
          "form": "게이밍은 비싸더라구요 무겁고 ㅋㅋ",
          "original_form": "게이밍은 비싸더라구요 무겁고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039570.1.21",
          "form": "갖고다니기엔 좀 그렇죠ㅋㅋㅋ",
          "original_form": "갖고다니기엔 좀 그렇죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039570.1.22",
          "form": "전 갤탭 살까말까 고민중이에요!",
          "original_form": "전 갤탭 살까말까 고민중이에요!",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039570.1.23",
          "form": "오 무슨용도로요?!",
          "original_form": "오 무슨용도로요?!",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039570.1.24",
          "form": "강의 필기용이요!!",
          "original_form": "강의 필기용이요!!",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039570.1.25",
          "form": "물론 사게되면 유튜브만 보겠지만ㅋㅋㅋ",
          "original_form": "물론 사게되면 유튜브만 보겠지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039570.1.26",
          "form": "오 저도 유투브에서 보고 혹해서",
          "original_form": "오 저도 유투브에서 보고 혹해서",
          "speaker_id": "1",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039570.1.27",
          "form": "아이패드 샀어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아이패드 샀어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039570.1.28",
          "form": "애플펜슬이랑",
          "original_form": "애플펜슬이랑",
          "speaker_id": "1",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039570.1.29",
          "form": "ㅋㅋㅋㅋㅋ와 멋진데요?!?!",
          "original_form": "ㅋㅋㅋㅋㅋ와 멋진데요?!?!",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039570.1.30",
          "form": "전 애플은 한번도 안써봤어요!",
