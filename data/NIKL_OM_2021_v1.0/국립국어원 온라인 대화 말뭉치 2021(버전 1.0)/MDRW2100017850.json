      "utterance": [
        {
          "id": "MDRW2100017850.1.1",
          "form": "어떤 운동하셧어여?",
          "original_form": "어떤 운동하셧어여?",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017850.1.2",
          "form": "저 걷기랑 실내싸이클 해요",
          "original_form": "저 걷기랑 실내싸이클 해요",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017850.1.3",
          "form": "집에 사이클이 잇어여?",
          "original_form": "집에 사이클이 잇어여?",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017850.1.4",
          "form": "실내자전거요! 숀리가 광고하는 ㅋㅋ",
          "original_form": "실내자전거요! 숀리가 광고하는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017850.1.5",
          "form": "비싼거여?",
          "original_form": "비싼거여?",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017850.1.6",
          "form": "소음 괜찮은가요?",
          "original_form": "소음 괜찮은가요?",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017850.1.7",
          "form": "아뇨 ㅎㅎ 안 비싸요 소음도 없는편이에요",
          "original_form": "아뇨 ㅎㅎ 안 비싸요 소음도 없는편이에요",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017850.1.8",
          "form": "거리랑 그런거도 나와요?",
          "original_form": "거리랑 그런거도 나와요?",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017850.1.9",
          "form": "네네 속도 거리 뭐 이런거 다 나와요",
          "original_form": "네네 속도 거리 뭐 이런거 다 나와요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017850.1.10",
          "form": "한번에 몇마일씩 타세요?",
          "original_form": "한번에 몇마일씩 타세요?",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017850.1.11",
          "form": "몇마일은 모르겠구 인터발로 한시간정도 타요",
          "original_form": "몇마일은 모르겠구 인터발로 한시간정도 타요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017850.1.12",
          "form": "인터발 자전거 타면 엄청 힘들거든요",
          "original_form": "인터발 자전거 타면 엄청 힘들거든요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017850.1.13",
          "form": "오.. 뭘 아시네요...",
          "original_form": "오.. 뭘 아시네요...",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017850.1.14",
          "form": "네 그냥 타서는 운동이 안되더라구요 ㅎㅎ",
          "original_form": "네 그냥 타서는 운동이 안되더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017850.1.15",
          "form": "인타발이 진국이죠.. ㅋㅋㅋ시간대비 엄청 힘들죠..",
          "original_form": "인타발이 진국이죠.. ㅋㅋㅋ시간대비 엄청 힘들죠..",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017850.1.16",
          "form": "그냐우1시간보다 인터벌 20분이 효과적이죠",
          "original_form": "그냐우1시간보다 인터벌 20분이 효과적이죠",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017850.1.17",
          "form": "네네 어디서 보니까 짧은시간 집중해서 운동하라고 하더라구요",
          "original_form": "네네 어디서 보니까 짧은시간 집중해서 운동하라고 하더라구요",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017850.1.18",
          "form": "인터벌이... 빡세게하고 쉬는시간에도 뇌가 계속 운동중이라고 착각하는 원리라요..",
          "original_form": "인터벌이... 빡세게하고 쉬는시간에도 뇌가 계속 운동중이라고 착각하는 원리라요..",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017850.1.19",
          "form": "오 그렇군요 몰랐던 지식이예요! 신기하네요 ㅎㅎ",
          "original_form": "오 그렇군요 몰랐던 지식이예요! 신기하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017850.1.20",
          "form": "앞으로도 인터발 열심히 해야겠어요",
          "original_form": "앞으로도 인터발 열심히 해야겠어요",
          "speaker_id": "2",
          "time": "20210826 21:43"
        }
      ]
