      "utterance": [
        {
          "id": "MDRW2100007531.1.1",
          "form": "둘째가 밥도 잘먹고",
          "original_form": "둘째가 밥도 잘먹고",
          "speaker_id": "2",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100007531.1.2",
          "form": "아직출시는 안한거같고",
          "original_form": "아직출시는 안한거같고",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100007531.1.3",
          "form": "당뇨도요?",
          "original_form": "당뇨도요?",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100007531.1.4",
          "form": "그렇대요ㅎㅎ",
          "original_form": "그렇대요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100007531.1.5",
          "form": "어케측정하는지는 몰겠는데",
          "original_form": "어케측정하는지는 몰겠는데",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100007531.1.6",
          "form": "인바디는 손가락 대면측정되던데",
          "original_form": "인바디는 손가락 대면측정되던데",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100007531.1.7",
          "form": "당뇨는 피를 짜주는건가요?ㅎ",
          "original_form": "당뇨는 피를 짜주는건가요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100007531.1.8",
          "form": "설마요ㅎ",
          "original_form": "설마요ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100007531.1.9",
          "form": "바늘이있을까요?",
          "original_form": "바늘이있을까요?",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100007531.1.10",
          "form": "주기적으로 바늘이 나와서",
          "original_form": "주기적으로 바늘이 나와서",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100007531.1.11",
          "form": "찌르고 피를 가져가서 측정하는시스템?ㅋ",
          "original_form": "찌르고 피를 가져가서 측정하는시스템?ㅋ",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100007531.1.12",
          "form": "워치로하는거면 피부랑 몰통과시키려나요",
          "original_form": "워치로하는거면 피부랑 몰통과시키려나요",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100007531.1.13",
          "form": "아님 체지방측정처럼",
          "original_form": "아님 체지방측정처럼",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100007531.1.14",
          "form": "전류로?ㅋ",
          "original_form": "전류로?ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100007531.1.15",
          "form": "요즘 바이럴마케팅이라고",
          "original_form": "요즘 바이럴마케팅이라고",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100007531.1.16",
          "form": "워치로 백신주사맞고",
          "original_form": "워치로 백신주사맞고",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100007531.1.17",
          "form": "심장뛰는걸로 머 질병 알아냈데요",
          "original_form": "심장뛰는걸로 머 질병 알아냈데요",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100007531.1.18",
          "form": "아 아까아는언니가 그얘기하던데",
          "original_form": "아 아까아는언니가 그얘기하던데",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100007531.1.19",
          "form": "워치사야되나ㅋ",
          "original_form": "워치사야되나ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100007531.1.20",
          "form": "전 폰을 사고싶은데 비싸서 못사겠어요",
          "original_form": "전 폰을 사고싶은데 비싸서 못사겠어요",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100007531.1.21",
          "form": "폰은 고장나거나 용량문제아니면",
          "original_form": "폰은 고장나거나 용량문제아니면",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100007531.1.22",
          "form": "안바꿔도될듯요 ㅎ",
          "original_form": "안바꿔도될듯요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100007531.1.23",
          "form": "요즘 너무 비싸긴해요",
          "original_form": "요즘 너무 비싸긴해요",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100007531.1.24",
          "form": "잘쓰고있긴한데 남들 s21쓰고",
          "original_form": "잘쓰고있긴한데 남들 s21쓰고",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100007531.1.25",
          "form": "요번z플립인기가 많나봐요",
          "original_form": "요번z플립인기가 많나봐요",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100007531.1.26",
          "form": "대리가 제폰보더니 왜화면이 빨갛냐고ㅎㅎ",
          "original_form": "대리가 제폰보더니 왜화면이 빨갛냐고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100007531.1.27",
          "form": "플립 폴더 진짜비싸더라구요",
          "original_form": "플립 폴더 진짜비싸더라구요",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100007531.1.28",
          "form": "대단하대요ㅋ",
          "original_form": "대단하대요ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100007531.1.29",
          "form": "이런폰계속쓴다고",
          "original_form": "이런폰계속쓴다고",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100007531.1.30",
          "form": "그래서 사전예약으로 할인받아서 사는건가봐요",
          "original_form": "그래서 사전예약으로 할인받아서 사는건가봐요",
          "speaker_id": "2",
          "time": "20210825 20:01"
        }
      ]
