      "utterance": [
        {
          "id": "MDRW2100033175.1.1",
          "form": "내일 운동 갈꺼야?",
          "original_form": "내일 운동 갈꺼야?",
          "speaker_id": "1",
          "time": "20210907 02:49"
        },
        {
          "id": "MDRW2100033175.1.2",
          "form": "아니 내일은 못갈꺼 같은데?",
          "original_form": "아니 내일은 못갈꺼 같은데?",
          "speaker_id": "2",
          "time": "20210907 02:49"
        },
        {
          "id": "MDRW2100033175.1.3",
          "form": "왜???",
          "original_form": "왜???",
          "speaker_id": "1",
          "time": "20210907 02:49"
        },
        {
          "id": "MDRW2100033175.1.4",
          "form": "내일은 막내 병원가야할꺼 같아서.",
          "original_form": "내일은 막내 병원가야할꺼 같아서.",
          "speaker_id": "2",
          "time": "20210907 02:49"
        },
        {
          "id": "MDRW2100033175.1.5",
          "form": "병원에는 왜???",
          "original_form": "병원에는 왜???",
          "speaker_id": "1",
          "time": "20210907 02:49"
        },
        {
          "id": "MDRW2100033175.1.6",
          "form": "아 영유아 검진",
          "original_form": "아 영유아 검진",
          "speaker_id": "2",
          "time": "20210907 02:50"
        },
        {
          "id": "MDRW2100033175.1.7",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "1",
          "time": "20210907 03:09"
        },
        {
          "id": "MDRW2100033175.1.8",
          "form": "그리고 의사가 봤을깨 발육에 문제 있으면 말도 해주고.. 필요하긴 한듯??",
          "original_form": "그리고 의사가 봤을깨 발육에 문제 있으면 말도 해주고.. 필요하긴 한듯??",
          "speaker_id": "2",
          "time": "20210907 03:09"
        },
        {
          "id": "MDRW2100033175.1.9",
          "form": "그래?? 막내 별 문제 없겠지??",
          "original_form": "그래?? 막내 별 문제 없겠지??",
          "speaker_id": "1",
          "time": "20210907 03:09"
        },
        {
          "id": "MDRW2100033175.1.10",
          "form": "당연하지!!",
          "original_form": "당연하지!!",
          "speaker_id": "2",
          "time": "20210907 03:09"
        },
        {
          "id": "MDRW2100033175.1.11",
          "form": "그거 끝나고 가면 안되나 운동??",
          "original_form": "그거 끝나고 가면 안되나 운동??",
          "speaker_id": "1",
          "time": "20210907 03:09"
        },
        {
          "id": "MDRW2100033175.1.12",
          "form": "그럴까??요즘 부쩍 살 안빠지는듯",
          "original_form": "그럴까??요즘 부쩍 살 안빠지는듯",
          "speaker_id": "2",
          "time": "20210907 03:10"
        },
        {
          "id": "MDRW2100033175.1.13",
          "form": "아니야.. 살이 문제가 아니라.. 건강이 문제지.. 자꾸 이곳저곳 아프다고 하잖아",
          "original_form": "아니야.. 살이 문제가 아니라.. 건강이 문제지.. 자꾸 이곳저곳 아프다고 하잖아",
          "speaker_id": "1",
          "time": "20210907 03:10"
        },
        {
          "id": "MDRW2100033175.1.14",
          "form": "막내 맨날 코알라처럼 매달려 있으니가 그렇지.ㅠ",
          "original_form": "막내 맨날 코알라처럼 매달려 있으니가 그렇지.ㅠ",
          "speaker_id": "2",
          "time": "20210907 03:10"
        },
        {
          "id": "MDRW2100033175.1.15",
          "form": "도수치료 받는건 어때??",
          "original_form": "도수치료 받는건 어때??",
          "speaker_id": "1",
          "time": "20210907 03:10"
        },
        {
          "id": "MDRW2100033175.1.16",
          "form": "도수 치료 받아봤자 결국에는 내가 근육으로 버텨줘야하는 거라서 운동해야해.ㅠ",
          "original_form": "도수 치료 받아봤자 결국에는 내가 근육으로 버텨줘야하는 거라서 운동해야해.ㅠ",
          "speaker_id": "2",
          "time": "20210907 03:11"
        },
        {
          "id": "MDRW2100033175.1.17",
          "form": "그렇기는 한데. 그래도 들아플꺼 아니야.",
          "original_form": "그렇기는 한데. 그래도 들아플꺼 아니야.",
          "speaker_id": "1",
          "time": "20210907 03:11"
        },
        {
          "id": "MDRW2100033175.1.18",
          "form": "흠.... 한번 생각해 볼께..",
          "original_form": "흠.... 한번 생각해 볼께..",
          "speaker_id": "2",
          "time": "20210907 03:12"
        },
        {
          "id": "MDRW2100033175.1.19",
          "form": "여보는 아픈데 괜찮아??",
          "original_form": "여보는 아픈데 괜찮아??",
          "speaker_id": "2",
          "time": "20210907 03:12"
        },
        {
          "id": "MDRW2100033175.1.20",
          "form": "둘다 아프면 되겠냐...",
          "original_form": "둘다 아프면 되겠냐...",
          "speaker_id": "1",
