      "utterance": [
        {
          "id": "MDRW2100039302.1.1",
          "form": "내일 아는분이 여수로 여행간다고 하더라구요",
          "original_form": "내일 아는분이 여수로 여행간다고 하더라구요",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039302.1.2",
          "form": "너무 부러웠어요 ㅠㅠ",
          "original_form": "너무 부러웠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039302.1.3",
          "form": "우와, 좋겠네요",
          "original_form": "우와, 좋겠네요",
          "speaker_id": "1",
          "time": "20210903 19:42"
        },
        {
          "id": "MDRW2100039302.1.4",
          "form": "님은 혹시 계획이 있나요?",
          "original_form": "님은 혹시 계획이 있나요?",
          "speaker_id": "1",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100039302.1.5",
          "form": "아니요 저는 일단 안동을 가보고싶고 목포도 가고싶네요 ㅎㅎ",
          "original_form": "아니요 저는 일단 안동을 가보고싶고 목포도 가고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100039302.1.6",
          "form": "계획 있으신지요?",
          "original_form": "계획 있으신지요?",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100039302.1.7",
          "form": "여행은 아닌데 한달뒤에 부산으로 출장을 가기로 했어요",
          "original_form": "여행은 아닌데 한달뒤에 부산으로 출장을 가기로 했어요",
          "speaker_id": "1",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100039302.1.8",
          "form": "앗 출장으로 부산이라면 빡빡한 일정인가요?",
          "original_form": "앗 출장으로 부산이라면 빡빡한 일정인가요?",
          "speaker_id": "2",
          "time": "20210903 19:46"
        },
        {
          "id": "MDRW2100039302.1.9",
          "form": "부산 정말 좋은 곳이잖아요 ㅎㅎ",
          "original_form": "부산 정말 좋은 곳이잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:46"
        },
        {
          "id": "MDRW2100039302.1.10",
          "form": "갈때는 빡빡한데 금욜이라",
          "original_form": "갈때는 빡빡한데 금욜이라",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039302.1.11",
          "form": "하루정도는 제돈으로 더 머무를 생각이에요",
          "original_form": "하루정도는 제돈으로 더 머무를 생각이에요",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039302.1.12",
          "form": "그럼 더 좋을 것 같아요!",
          "original_form": "그럼 더 좋을 것 같아요!",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039302.1.13",
          "form": "자신만을 위한 시간도 필요하잖아요",
          "original_form": "자신만을 위한 시간도 필요하잖아요",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039302.1.14",
          "form": "ㅎㅎ 좋은 여행이었으면 좋겠네요",
          "original_form": "ㅎㅎ 좋은 여행이었으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039302.1.15",
          "form": "혼자 묵으시나요?",
          "original_form": "혼자 묵으시나요?",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039302.1.16",
          "form": "네. 평소에는 후배들도 데리고가곤 했는데",
          "original_form": "네. 평소에는 후배들도 데리고가곤 했는데",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039302.1.17",
          "form": "이시국이라 발표자만 가라고... ㅎㅎ",
          "original_form": "이시국이라 발표자만 가라고... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039302.1.18",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039302.1.19",
          "form": "발표라니 떨리지않으세요??",
          "original_form": "발표라니 떨리지않으세요??",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039302.1.20",
          "form": "응원하겠습니다!!정말 멋지시네요 !!",
          "original_form": "응원하겠습니다!!정말 멋지시네요 !!",
          "speaker_id": "2",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039302.1.21",
          "form": "떨려요. 엄청.",
          "original_form": "떨려요. 엄청.",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039302.1.22",
          "form": "그치만 간만에 멀리가서 맛있는것도 먹고 할 생각을 하니까 설레기도해요",
          "original_form": "그치만 간만에 멀리가서 맛있는것도 먹고 할 생각을 하니까 설레기도해요",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039302.1.23",
          "form": "저도 엄청 떠는 편인데 괜시리 더 응원하고 싶네요 ㅎㅎ",
          "original_form": "저도 엄청 떠는 편인데 괜시리 더 응원하고 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039302.1.24",
          "form": "고생한 기념으로 맛있는걸 드실건가봐요!!",
          "original_form": "고생한 기념으로 맛있는걸 드실건가봐요!!",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039302.1.25",
          "form": "여행 계획은 어떻게 짜셨나요??",
          "original_form": "여행 계획은 어떻게 짜셨나요??",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039302.1.26",
          "form": "사실 3년전에 이미 부산 여행을 하기도 해서",
          "original_form": "사실 3년전에 이미 부산 여행을 하기도 해서",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039302.1.27",
          "form": "시국도 시국이라 딱히 돌아다니지는 않을 생각이고",
          "original_form": "시국도 시국이라 딱히 돌아다니지는 않을 생각이고",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039302.1.28",
          "form": "오션뷰 숙소 잡아다 호캉스정도나 할 생각이에요",
          "original_form": "오션뷰 숙소 잡아다 호캉스정도나 할 생각이에요",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039302.1.29",
          "form": "글구 혹시 센텀시티에 있는 딤딤섬이라고 아세요?",
          "original_form": "글구 혹시 센텀시티에 있는 딤딤섬이라고 아세요?",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039302.1.30",
          "form": "전에 갔을때 가서 먹어봤는데 진짜 존맛",
          "original_form": "전에 갔을때 가서 먹어봤는데 진짜 존맛",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039302.1.31",
          "form": "거기는 꼭 다시 갈거에요 ㅎㅎ",
          "original_form": "거기는 꼭 다시 갈거에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039302.1.32",
          "form": "와...........오션뷰 라니 너무 좋을 것 같아요",
          "original_form": "와...........오션뷰 라니 너무 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100039302.1.33",
          "form": "세상에 저 딤딤섬 정말 좋아해요",
          "original_form": "세상에 저 딤딤섬 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100039302.1.34",
          "form": "일부러 찾아가는 곳인데 코로나로 인해 못간지 2년이 넘은것같아요 ㅠㅠㅠ",
          "original_form": "일부러 찾아가는 곳인데 코로나로 인해 못간지 2년이 넘은것같아요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100039302.1.35",
          "form": "전 대구에 처음들어왔다고 해서 먹어봤는데 와 너무 맛있어서 그뒤로 자주갔었어요",
          "original_form": "전 대구에 처음들어왔다고 해서 먹어봤는데 와 너무 맛있어서 그뒤로 자주갔었어요",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100039302.1.36",
          "form": "ㅠㅠ 너무 부럽네요",
          "original_form": "ㅠㅠ 너무 부럽네요",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100039302.1.37",
          "form": "여행계획이 너무 제취향이에요 호캉스에 딤딤섬이라니",
          "original_form": "여행계획이 너무 제취향이에요 호캉스에 딤딤섬이라니",
          "speaker_id": "2",
