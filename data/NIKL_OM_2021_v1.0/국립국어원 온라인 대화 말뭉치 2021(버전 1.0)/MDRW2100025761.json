      "utterance": [
        {
          "id": "MDRW2100025761.1.1",
          "form": "기이한 자세로 자다가 깼는데요",
          "original_form": "기이한 자세로 자다가 깼는데요",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100025761.1.2",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100025761.1.3",
          "form": "강아지도 잠이 많은가 봐요",
          "original_form": "강아지도 잠이 많은가 봐요",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100025761.1.4",
          "form": "새벽에 안자요 다들자는시간에 인형물고 뛰어다녀서 스트레스받네요",
          "original_form": "새벽에 안자요 다들자는시간에 인형물고 뛰어다녀서 스트레스받네요",
          "speaker_id": "2",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025761.1.5",
          "form": "애들은 어떻게 놀아주나요?",
          "original_form": "애들은 어떻게 놀아주나요?",
          "speaker_id": "2",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025761.1.6",
          "form": "왜 밤낮이 바뀐걸까요",
          "original_form": "왜 밤낮이 바뀐걸까요",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025761.1.7",
          "form": "고양이는 주로",
          "original_form": "고양이는 주로",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025761.1.8",
          "form": "낚시대로 놀아줘요",
          "original_form": "낚시대로 놀아줘요",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025761.1.9",
          "form": "강아지도 장난감으로 뺏기놀이해요",
          "original_form": "강아지도 장난감으로 뺏기놀이해요",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025761.1.10",
          "form": "어떤 간식 제일 좋아해요?",
          "original_form": "어떤 간식 제일 좋아해요?",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025761.1.11",
          "form": "아무고기면 다 환장합니다",
          "original_form": "아무고기면 다 환장합니다",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025761.1.12",
          "form": "ㅋㅋㅋ얘도요 그런데 금방질려해요",
          "original_form": "ㅋㅋㅋ얘도요 그런데 금방질려해요",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025761.1.13",
          "form": "입맛이 까다로운가요?",
          "original_form": "입맛이 까다로운가요?",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025761.1.14",
          "form": "주인을 닮는다는 말이 있던데...",
          "original_form": "주인을 닮는다는 말이 있던데...",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025761.1.15",
          "form": "그건아닐거에요 전 입맛이 까다롭지않아요",
          "original_form": "그건아닐거에요 전 입맛이 까다롭지않아요",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025761.1.16",
          "form": "입맛이 짧은 아이인가봐요",
          "original_form": "입맛이 짧은 아이인가봐요",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025761.1.17",
          "form": "우리집 애들은 너무 먹어서",
          "original_form": "우리집 애들은 너무 먹어서",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025761.1.18",
          "form": "돼지 될려고 해요",
          "original_form": "돼지 될려고 해요",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025761.1.19",
          "form": "그래요 얘는 찌우면 관절에 무리간다고해서 많이 주지 않으려고요",
          "original_form": "그래요 얘는 찌우면 관절에 무리간다고해서 많이 주지 않으려고요",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025761.1.20",
          "form": "강아지 종이 뭔가요?",
          "original_form": "강아지 종이 뭔가요?",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025761.1.21",
          "form": "푸들이에요 갈색",
          "original_form": "푸들이에요 갈색",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025761.1.22",
          "form": "오 갈색푸들 많이보이던데",
          "original_form": "오 갈색푸들 많이보이던데",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025761.1.23",
          "form": "다리가 약한가봐요",
          "original_form": "다리가 약한가봐요",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025761.1.24",
          "form": "가늘어서 그런가",
          "original_form": "가늘어서 그런가",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025761.1.25",
          "form": "관절을 조심해야 겠어요",
          "original_form": "관절을 조심해야 겠어요",
          "speaker_id": "1",
          "time": "20210901 17:39"
        }
      ]
    }
  ]
