      "utterance": [
        {
          "id": "MDRW2100037504.1.1",
          "form": "전 사실...목캔디를 좋아합니다",
          "original_form": "전 사실...목캔디를 좋아합니다",
          "speaker_id": "2",
          "time": "20210901 02:01"
        },
        {
          "id": "MDRW2100037504.1.2",
          "form": "박하사탕도ㅎ",
          "original_form": "박하사탕도ㅎ",
          "speaker_id": "2",
          "time": "20210901 02:02"
        },
        {
          "id": "MDRW2100037504.1.3",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210901 02:02"
        },
        {
          "id": "MDRW2100037504.1.4",
          "form": "저는 목캔디 분홍색!",
          "original_form": "저는 목캔디 분홍색!",
          "speaker_id": "1",
          "time": "20210901 02:02"
        },
        {
          "id": "MDRW2100037504.1.5",
          "form": "기본맛말고 색깔다른거 중에 맛있는거 있더라구요!!!",
          "original_form": "기본맛말고 색깔다른거 중에 맛있는거 있더라구요!!!",
          "speaker_id": "1",
          "time": "20210901 02:03"
        },
        {
          "id": "MDRW2100037504.1.6",
          "form": "엇ㅎ  어르신이라고 놀릴줄  알았는데ㅎㅎ",
          "original_form": "엇ㅎ  어르신이라고 놀릴줄  알았는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 02:03"
        },
        {
          "id": "MDRW2100037504.1.7",
          "form": "분홍은 무슨 맛인가요?",
          "original_form": "분홍은 무슨 맛인가요?",
          "speaker_id": "2",
          "time": "20210901 02:03"
        },
        {
          "id": "MDRW2100037504.1.8",
          "form": "필요성을 느껴요...",
          "original_form": "필요성을 느껴요...",
          "speaker_id": "1",
          "time": "20210901 02:03"
        },
        {
          "id": "MDRW2100037504.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:03"
        },
        {
          "id": "MDRW2100037504.1.10",
          "form": "전기본만 먹었는데ㅎ",
          "original_form": "전기본만 먹었는데ㅎ",
          "speaker_id": "2",
          "time": "20210901 02:03"
        },
        {
          "id": "MDRW2100037504.1.11",
          "form": "라즈베리? 뭐 이런맛이었던거 같아요!",
          "original_form": "라즈베리? 뭐 이런맛이었던거 같아요!",
          "speaker_id": "1",
          "time": "20210901 02:03"
        },
        {
          "id": "MDRW2100037504.1.12",
          "form": "베리류였던것 같은데... 확실하진 않아요ㅠ",
          "original_form": "베리류였던것 같은데... 확실하진 않아요ㅠ",
          "speaker_id": "1",
          "time": "20210901 02:03"
        },
        {
          "id": "MDRW2100037504.1.13",
          "form": "아니 목캔디 그 화한 와중에...베리맛을 냈다구요ㅎㅎㅎ",
          "original_form": "아니 목캔디 그 화한 와중에...베리맛을 냈다구요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 02:04"
        },
        {
          "id": "MDRW2100037504.1.14",
          "form": "원피스 입은 할아버지 느낌인데ㅎㅎ",
          "original_form": "원피스 입은 할아버지 느낌인데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 02:04"
        },
        {
          "id": "MDRW2100037504.1.15",
          "form": "맛 궁금하네요",
          "original_form": "맛 궁금하네요",
          "speaker_id": "2",
          "time": "20210901 02:04"
        },
        {
          "id": "MDRW2100037504.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:04"
        },
        {
          "id": "MDRW2100037504.1.17",
          "form": "원피스입은할아버짘ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "원피스입은할아버짘ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:04"
        },
        {
          "id": "MDRW2100037504.1.18",
          "form": "name1님 취향에 경의를 표합니다ㅎㅎ",
          "original_form": "&name1&님 취향에 경의를 표합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 02:05"
        },
        {
          "id": "MDRW2100037504.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:05"
        },
        {
          "id": "MDRW2100037504.1.20",
          "form": "저 아까 솔의눈 좋아한다는 분 봤거든요ㅎ",
          "original_form": "저 아까 솔의눈 좋아한다는 분 봤거든요ㅎ",
          "speaker_id": "2",
          "time": "20210901 02:06"
        },
        {
          "id": "MDRW2100037504.1.21",
          "form": "즐겨먹는건 아닌데 목이아프면 찾게되더라구여",
          "original_form": "즐겨먹는건 아닌데 목이아프면 찾게되더라구여",
          "speaker_id": "1",
          "time": "20210901 02:06"
        },
        {
          "id": "MDRW2100037504.1.22",
          "form": "저는 목캔디보단 호올스를 먹는편이긴해여!",
          "original_form": "저는 목캔디보단 호올스를 먹는편이긴해여!",
          "speaker_id": "1",
          "time": "20210901 02:06"
        },
        {
          "id": "MDRW2100037504.1.23",
          "form": "솔의눈...",
          "original_form": "솔의눈...",
          "speaker_id": "1",
          "time": "20210901 02:06"
        },
        {
          "id": "MDRW2100037504.1.24",
          "form": "제동생이 좋아합니다",
          "original_form": "제동생이 좋아합니다",
          "speaker_id": "1",
          "time": "20210901 02:06"
        },
        {
          "id": "MDRW2100037504.1.25",
          "form": "호올스도 맛있쥬ㅎ",
          "original_form": "호올스도 맛있쥬ㅎ",
          "speaker_id": "2",
          "time": "20210901 02:06"
        },
        {
          "id": "MDRW2100037504.1.26",
          "form": "호옥시 제가 name1님 동생을 봤나요ㅋㅋㅋ",
          "original_form": "호옥시 제가 &name1&님 동생을 봤나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 02:06"
        },
        {
          "id": "MDRW2100037504.1.27",
          "form": "암튼 아침햇살 민초 데자와 좋아하냐고 물었더니",
          "original_form": "암튼 아침햇살 민초 데자와 좋아하냐고 물었더니",
          "speaker_id": "2",
          "time": "20210901 02:07"
        },
        {
          "id": "MDRW2100037504.1.28",
          "form": "다 좋아하신대요ㅎ",
          "original_form": "다 좋아하신대요ㅎ",
          "speaker_id": "2",
          "time": "20210901 02:07"
        },
        {
          "id": "MDRW2100037504.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ옼ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ옼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:08"
        },
        {
          "id": "MDRW2100037504.1.30",
          "form": "민초 데자와는 모르겠고",
          "original_form": "민초 데자와는 모르겠고",
          "speaker_id": "1",
          "time": "20210901 02:08"
        },
        {
          "id": "MDRW2100037504.1.31",
          "form": "아침햇살도 좋아해요",
          "original_form": "아침햇살도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 02:08"
        },
        {
          "id": "MDRW2100037504.1.32",
          "form": "와... 그 맛의 결이 있나봐요",
          "original_form": "와... 그 맛의 결이 있나봐요",
          "speaker_id": "1",
          "time": "20210901 02:08"
        },
        {
          "id": "MDRW2100037504.1.33",
          "form": "나는 1도 모르겠는데",
          "original_form": "나는 1도 모르겠는데",
          "speaker_id": "1",
          "time": "20210901 02:08"
        },
        {
          "id": "MDRW2100037504.1.34",
          "form": "ㅎ 저는 이분과 소울메이트예요ㅎ",
          "original_form": "ㅎ 저는 이분과 소울메이트예요ㅎ",
          "speaker_id": "2",
          "time": "20210901 02:08"
        },
        {
          "id": "MDRW2100037504.1.35",
          "form": "아 맥콜을 물었어야 하는데",
          "original_form": "아 맥콜을 물었어야 하는데",
          "speaker_id": "2",
          "time": "20210901 02:09"
        },
        {
          "id": "MDRW2100037504.1.36",
          "form": "그분 맥콜도 취향이실듯ㅎ",
          "original_form": "그분 맥콜도 취향이실듯ㅎ",
          "speaker_id": "2",
