      "utterance": [
        {
          "id": "MDRW2100024171.1.1",
          "form": "기분탓일수도 있는데",
          "original_form": "기분탓일수도 있는데",
          "speaker_id": "2",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024171.1.2",
          "form": "제가 잔기침이 좀 많은 편인데",
          "original_form": "제가 잔기침이 좀 많은 편인데",
          "speaker_id": "2",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024171.1.3",
          "form": "그게 나아졌어요",
          "original_form": "그게 나아졌어요",
          "speaker_id": "2",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024171.1.4",
          "form": "아 면역력에 도움이 되나봐요",
          "original_form": "아 면역력에 도움이 되나봐요",
          "speaker_id": "1",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100024171.1.5",
          "form": "그렇다고 하대요.",
          "original_form": "그렇다고 하대요.",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024171.1.6",
          "form": "시국이 시국인지라 챙겨먹습니다",
          "original_form": "시국이 시국인지라 챙겨먹습니다",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024171.1.7",
          "form": "코로나는 못막아도",
          "original_form": "코로나는 못막아도",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024171.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024171.1.9",
          "form": "잔기침으로 인한 눈총?은 줄여주니까요 ㅎ",
          "original_form": "잔기침으로 인한 눈총?은 줄여주니까요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024171.1.10",
          "form": "그쵸 요즘은 기침하기도 눈치보이는 시국이라..",
          "original_form": "그쵸 요즘은 기침하기도 눈치보이는 시국이라..",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024171.1.11",
          "form": "혹시 루테인 드셔보셨나요?",
          "original_form": "혹시 루테인 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024171.1.12",
          "form": "아뇨, 아직",
          "original_form": "아뇨, 아직",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024171.1.13",
          "form": "눈이 좀 건조한거같아서 먹어보려는데",
          "original_form": "눈이 좀 건조한거같아서 먹어보려는데",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024171.1.14",
          "form": "효과 있다는 사람 반, 없다는 사람 반이어서 고민돼요",
          "original_form": "효과 있다는 사람 반, 없다는 사람 반이어서 고민돼요",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024171.1.15",
          "form": "안구건조는 루테인도 괜찮지만",
          "original_form": "안구건조는 루테인도 괜찮지만",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024171.1.16",
          "form": "눈마사지랑 눈운동이 가장 좋다고 하더라고요",
          "original_form": "눈마사지랑 눈운동이 가장 좋다고 하더라고요",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024171.1.17",
          "form": "아 맞아요 그런 얘기 들었는데",
          "original_form": "아 맞아요 그런 얘기 들었는데",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024171.1.18",
          "form": "반짝 하고 안하게 돼서...ㅋㅋㅋㅋㅋ ㅠ",
          "original_form": "반짝 하고 안하게 돼서...ㅋㅋㅋㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024171.1.19",
          "form": "그쵸ㅋㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024171.1.20",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "2",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100024171.1.21",
          "form": "차라리 자본의 힘을 빌리기로 했어요...",
          "original_form": "차라리 자본의 힘을 빌리기로 했어요...",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024171.1.22",
          "form": "아하! 네",
          "original_form": "아하! 네",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024171.1.23",
          "form": "효과가 있을거에요",
          "original_form": "효과가 있을거에요",
          "speaker_id": "2",
          "time": "20210830 23:44"
        }
      ]
    }
