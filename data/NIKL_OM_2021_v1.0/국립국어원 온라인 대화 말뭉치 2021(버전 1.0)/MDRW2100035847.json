      "utterance": [
        {
          "id": "MDRW2100035847.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210830 09:02"
        },
        {
          "id": "MDRW2100035847.1.2",
          "form": "아니요 졸업했어요",
          "original_form": "아니요 졸업했어요",
          "speaker_id": "1",
          "time": "20210830 09:03"
        },
        {
          "id": "MDRW2100035847.1.3",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210830 09:03"
        },
        {
          "id": "MDRW2100035847.1.4",
          "form": "저도 졸업했어요!",
          "original_form": "저도 졸업했어요!",
          "speaker_id": "2",
          "time": "20210830 09:07"
        },
        {
          "id": "MDRW2100035847.1.5",
          "form": "학교 다니실때 동아리 하셨어요?",
          "original_form": "학교 다니실때 동아리 하셨어요?",
          "speaker_id": "2",
          "time": "20210830 09:07"
        },
        {
          "id": "MDRW2100035847.1.6",
          "form": "저는 새내기때 배드민턴, 볼링, 잠깐했었어요",
          "original_form": "저는 새내기때 배드민턴, 볼링, 잠깐했었어요",
          "speaker_id": "1",
          "time": "20210830 09:08"
        },
        {
          "id": "MDRW2100035847.1.7",
          "form": "동아리하셨어요?",
          "original_form": "동아리하셨어요?",
          "speaker_id": "1",
          "time": "20210830 09:08"
        },
        {
          "id": "MDRW2100035847.1.8",
          "form": "오.. 저는 고등학교때 영상동아리 해봤어요!",
          "original_form": "오.. 저는 고등학교때 영상동아리 해봤어요!",
          "speaker_id": "2",
          "time": "20210830 09:39"
        },
        {
          "id": "MDRW2100035847.1.9",
          "form": "대학생때 못해본게 아쉬워요ㅠ",
          "original_form": "대학생때 못해본게 아쉬워요ㅠ",
          "speaker_id": "2",
          "time": "20210830 09:39"
        },
        {
          "id": "MDRW2100035847.1.10",
          "form": "저두요 활발하게 해볼껄 하는 생각 자주들어요",
          "original_form": "저두요 활발하게 해볼껄 하는 생각 자주들어요",
          "speaker_id": "1",
          "time": "20210830 09:58"
        },
        {
          "id": "MDRW2100035847.1.11",
          "form": "술을 싫어하다보니 동아리활동 안 맞아서 그냥 나왔거든요",
          "original_form": "술을 싫어하다보니 동아리활동 안 맞아서 그냥 나왔거든요",
          "speaker_id": "1",
          "time": "20210830 09:59"
        },
        {
          "id": "MDRW2100035847.1.12",
          "form": "아ㅠ 술동아리가 많다긴 하더라구요",
          "original_form": "아ㅠ 술동아리가 많다긴 하더라구요",
          "speaker_id": "2",
          "time": "20210830 10:16"
        },
        {
          "id": "MDRW2100035847.1.13",
          "form": "저도 그래서 안했는데 한때인데 할껄 그랬나 싶어요",
          "original_form": "저도 그래서 안했는데 한때인데 할껄 그랬나 싶어요",
          "speaker_id": "2",
          "time": "20210830 10:16"
        },
        {
          "id": "MDRW2100035847.1.14",
          "form": "그죠 그냥 술안마시는데 잘찾아볼걸 싶어요",
          "original_form": "그죠 그냥 술안마시는데 잘찾아볼걸 싶어요",
          "speaker_id": "1",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100035847.1.15",
          "form": "만약 하시게된다면 뭐 하시고 싶으세요?",
          "original_form": "만약 하시게된다면 뭐 하시고 싶으세요?",
          "speaker_id": "1",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100035847.1.16",
          "form": "천문학이나 기타요!",
          "original_form": "천문학이나 기타요!",
          "speaker_id": "2",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100035847.1.17",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100035847.1.18",
          "form": "저는 요즘은 배구요!",
          "original_form": "저는 요즘은 배구요!",
          "speaker_id": "1",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100035847.1.19",
          "form": "오오 운동파신가봐요!",
          "original_form": "오오 운동파신가봐요!",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100035847.1.20",
          "form": "몸 움직이는거 좋아하세요?",
          "original_form": "몸 움직이는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100035847.1.21",
          "form": "네 운동 좋아하는편이예요! 헬스같은건 싫어하지만요...",
          "original_form": "네 운동 좋아하는편이예요! 헬스같은건 싫어하지만요...",
          "speaker_id": "1",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100035847.1.22",
          "form": "이번올림픽보고 배구해보고싶어졌어요",
          "original_form": "이번올림픽보고 배구해보고싶어졌어요",
          "speaker_id": "1",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100035847.1.23",
          "form": "오오 대단하세용",
          "original_form": "오오 대단하세용",
          "speaker_id": "2",
          "time": "20210830 12:44"
        },
        {
          "id": "MDRW2100035847.1.24",
          "form": "이번 올림픽 완전 멋있었조!",
          "original_form": "이번 올림픽 완전 멋있었조!",
          "speaker_id": "2",
          "time": "20210830 12:44"
        }
      ]
    }
