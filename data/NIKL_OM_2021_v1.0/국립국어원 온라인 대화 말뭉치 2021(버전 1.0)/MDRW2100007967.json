      "utterance": [
        {
          "id": "MDRW2100007967.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007967.1.2",
          "form": "어제는 엄청왔어요 오늘은 꼭 운동해야져ㅜ",
          "original_form": "어제는 엄청왔어요 오늘은 꼭 운동해야져ㅜ",
          "speaker_id": "2",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007967.1.3",
          "form": "전 홈트요",
          "original_form": "전 홈트요",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007967.1.4",
          "form": "오 유튜브로 영상 보시나요",
          "original_form": "오 유튜브로 영상 보시나요",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007967.1.5",
          "form": "땅크봐요",
          "original_form": "땅크봐요",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007967.1.6",
          "form": "땅끄부부 말하시는것 맞나요 ㅎㅎ",
          "original_form": "땅끄부부 말하시는것 맞나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007967.1.7",
          "form": "네 캐미가",
          "original_form": "네 캐미가",
          "speaker_id": "1",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100007967.1.8",
          "form": "전 요새 2만보 걸어요",
          "original_form": "전 요새 2만보 걸어요",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007967.1.9",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100007967.1.10",
          "form": "대단요 만보도 쉽지가",
          "original_form": "대단요 만보도 쉽지가",
          "speaker_id": "1",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007967.1.11",
          "form": "시간이 오래걸려요 세시간?",
          "original_form": "시간이 오래걸려요 세시간?",
          "speaker_id": "2",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100007967.1.12",
          "form": "효과보나요?",
          "original_form": "효과보나요?",
          "speaker_id": "1",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100007967.1.13",
          "form": "대신 너무 잘먹어서ㅜ 더쪘어요ㅜㅜ",
          "original_form": "대신 너무 잘먹어서ㅜ 더쪘어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100007967.1.14",
          "form": "운동하면 땡기죠",
          "original_form": "운동하면 땡기죠",
          "speaker_id": "1",
          "time": "20210822 12:54"
        }
