      "utterance": [
        {
          "id": "MDRW2100028875.1.1",
          "form": "그룹유지하면서 소속사 나간거 치고는 최선의 루트같음",
          "original_form": "그룹유지하면서 소속사 나간거 치고는 최선의 루트같음",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028875.1.2",
          "form": "근데 탈 알비덥 잘한거같아...",
          "original_form": "근데 탈 알비덥 잘한거같아...",
          "speaker_id": "1",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028875.1.3",
          "form": "애초에 어떤 놈이 계약중계해...",
          "original_form": "애초에 어떤 놈이 계약중계해...",
          "speaker_id": "1",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028875.1.4",
          "form": "ㅇㅇ맞어....그거 솔직히 팬들 볼모로 잡고 협박하는거야",
          "original_form": "ㅇㅇ맞어....그거 솔직히 팬들 볼모로 잡고 협박하는거야",
          "speaker_id": "2",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028875.1.5",
          "form": "진짜 양아치여 그건",
          "original_form": "진짜 양아치여 그건",
          "speaker_id": "2",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028875.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ진짜 그때 어그로가 정말 말도아니어서...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ진짜 그때 어그로가 정말 말도아니어서...",
          "speaker_id": "1",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028875.1.7",
          "form": "계약 아직 안했다고 실시간 중계하고",
          "original_form": "계약 아직 안했다고 실시간 중계하고",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028875.1.8",
          "form": "진짜 못된겨 그건",
          "original_form": "진짜 못된겨 그건",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028875.1.9",
          "form": "스트레스진짜....",
          "original_form": "스트레스진짜....",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028875.1.10",
          "form": "팬들 조급하게",
          "original_form": "팬들 조급하게",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028875.1.11",
          "form": "내친구 피가마른다고",
          "original_form": "내친구 피가마른다고",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028875.1.12",
          "form": "ㄹㅇ...",
          "original_form": "ㄹㅇ...",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028875.1.13",
          "form": "막 해체처럼 몰아가면서ㅜㅜ",
          "original_form": "막 해체처럼 몰아가면서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028875.1.14",
          "form": "진짜 팬들 정병 걸리게",
          "original_form": "진짜 팬들 정병 걸리게",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028875.1.15",
          "form": "뭐만 뜨면",
          "original_form": "뭐만 뜨면",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028875.1.16",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028875.1.17",
          "form": "행회돌린답시고",
          "original_form": "행회돌린답시고",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028875.1.18",
          "form": "눈새들이 재계약됐나봐 ㅠㅠㅠㅠ",
          "original_form": "눈새들이 재계약됐나봐 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028875.1.19",
          "form": "이러고",
          "original_form": "이러고",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028875.1.20",
          "form": "어그로는 안됐다고 난리고",
          "original_form": "어그로는 안됐다고 난리고",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028875.1.21",
          "form": "제가봐도 안타까울지경...",
          "original_form": "제가봐도 안타까울지경...",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028875.1.22",
          "form": "소속사와 눈새 그리고 어그로의 대환장 삼박자 그 자체네ㅡ누",
          "original_form": "소속사와 눈새 그리고 어그로의 대환장 삼박자 그 자체네ㅡ누",
