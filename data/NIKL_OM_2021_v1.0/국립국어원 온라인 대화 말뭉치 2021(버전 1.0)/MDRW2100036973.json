      "utterance": [
        {
          "id": "MDRW2100036973.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100036973.1.2",
          "form": "안녕하세요. 요즘 사고 싶으신 거 있으신가요?",
          "original_form": "안녕하세요. 요즘 사고 싶으신 거 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100036973.1.3",
          "form": "저 요즘새로나온 갤 플립3이요",
          "original_form": "저 요즘새로나온 갤 플립3이요",
          "speaker_id": "1",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100036973.1.4",
          "form": "name2님은여",
          "original_form": "&name2&님은여",
          "speaker_id": "1",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100036973.1.5",
          "form": "그거 엄청 잘 팔리는 거 같더라구요",
          "original_form": "그거 엄청 잘 팔리는 거 같더라구요",
          "speaker_id": "2",
          "time": "20210831 15:57"
        },
        {
          "id": "MDRW2100036973.1.6",
          "form": "전 지금은 가디건 하나 살까 생각중이에요",
          "original_form": "전 지금은 가디건 하나 살까 생각중이에요",
          "speaker_id": "2",
          "time": "20210831 15:57"
        },
        {
          "id": "MDRW2100036973.1.7",
          "form": "맞아요 근데 폰바꾼지얼마안돼서ㅜ",
          "original_form": "맞아요 근데 폰바꾼지얼마안돼서ㅜ",
          "speaker_id": "1",
          "time": "20210831 15:58"
        },
        {
          "id": "MDRW2100036973.1.8",
          "form": "가디건 어떤가디건 사고싶으세요",
          "original_form": "가디건 어떤가디건 사고싶으세요",
          "speaker_id": "1",
          "time": "20210831 15:58"
        },
        {
          "id": "MDRW2100036973.1.9",
          "form": "아직 봐 둔 건 없긴한데 그냥 얇고 가벼운 기본 디자인이 좋더라구요",
          "original_form": "아직 봐 둔 건 없긴한데 그냥 얇고 가벼운 기본 디자인이 좋더라구요",
          "speaker_id": "2",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100036973.1.10",
          "form": "그쳐 이제 가을이니까요",
          "original_form": "그쳐 이제 가을이니까요",
          "speaker_id": "1",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100036973.1.11",
          "form": "색상은어떤색상으로 보통구매하시나요",
          "original_form": "색상은어떤색상으로 보통구매하시나요",
          "speaker_id": "1",
          "time": "20210831 16:01"
        },
        {
          "id": "MDRW2100036973.1.12",
          "form": "베이지색이 제일 무난하더라구요",
          "original_form": "베이지색이 제일 무난하더라구요",
          "speaker_id": "2",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100036973.1.13",
          "form": "그래도 버건디나 푸른색 계열도 사봤는데 그것도 나름 괜찮았어요",
          "original_form": "그래도 버건디나 푸른색 계열도 사봤는데 그것도 나름 괜찮았어요",
          "speaker_id": "2",
          "time": "20210831 16:08"
        },
        {
          "id": "MDRW2100036973.1.14",
          "form": "전 무조건 무채색으로사서",
          "original_form": "전 무조건 무채색으로사서",
          "speaker_id": "1",
          "time": "20210831 16:09"
        },
        {
          "id": "MDRW2100036973.1.15",
          "form": "옷은주로 어디서구매하세요",
          "original_form": "옷은주로 어디서구매하세요",
          "speaker_id": "1",
          "time": "20210831 16:10"
        },
        {
          "id": "MDRW2100036973.1.16",
          "form": "딱히 자주 가는 곳은 없고 그때그때 인터넷 검색하다 마음에 드는 걸로 사는 편이에요",
          "original_form": "딱히 자주 가는 곳은 없고 그때그때 인터넷 검색하다 마음에 드는 걸로 사는 편이에요",
          "speaker_id": "2",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100036973.1.17",
          "form": "저도 그러는편이에요 할인많이때리는곳있음 거기서사고",
          "original_form": "저도 그러는편이에요 할인많이때리는곳있음 거기서사고",
          "speaker_id": "1",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100036973.1.18",
          "form": "할인 중요하죠ㅋㅋ 세일할 때 구입하기",
          "original_form": "할인 중요하죠ㅋㅋ 세일할 때 구입하기",
          "speaker_id": "2",
          "time": "20210831 16:14"
        },
        {
          "id": "MDRW2100036973.1.19",
          "form": "네 온갖할인과 쿠폰끌어다 써요",
          "original_form": "네 온갖할인과 쿠폰끌어다 써요",
