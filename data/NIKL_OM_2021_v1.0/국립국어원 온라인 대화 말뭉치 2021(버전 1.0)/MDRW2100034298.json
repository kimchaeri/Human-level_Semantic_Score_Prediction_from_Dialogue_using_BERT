      "utterance": [
        {
          "id": "MDRW2100034298.1.1",
          "form": "오늘 저녁 뭐드셨나요??",
          "original_form": "오늘 저녁 뭐드셨나요??",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100034298.1.2",
          "form": "닭갈비요 ㅋㅋㅋ",
          "original_form": "닭갈비요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034298.1.3",
          "form": "뭐드셨어요",
          "original_form": "뭐드셨어요",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034298.1.4",
          "form": "저는 햄버거요 ㅋㅋ",
          "original_form": "저는 햄버거요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034298.1.5",
          "form": "닭갈비맛있죠 직접 요리해서드셨나요?",
          "original_form": "닭갈비맛있죠 직접 요리해서드셨나요?",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034298.1.6",
          "form": "유가네 시켰어요 ㅋㅋㅋ",
          "original_form": "유가네 시켰어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034298.1.7",
          "form": "아 유가네 진짜 맛있어요",
          "original_form": "아 유가네 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034298.1.8",
          "form": "닭갈비 다먹고",
          "original_form": "닭갈비 다먹고",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034298.1.9",
          "form": "밥볶아먹으면 최고죠",
          "original_form": "밥볶아먹으면 최고죠",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034298.1.10",
          "form": "필수죠",
          "original_form": "필수죠",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034298.1.11",
          "form": "배달시티면 볶음밥이랑 닭이랑 동시에 먹을수 있답니다",
          "original_form": "배달시티면 볶음밥이랑 닭이랑 동시에 먹을수 있답니다",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034298.1.12",
          "form": "와...미리 만들어져서 오는건가요?",
          "original_form": "와...미리 만들어져서 오는건가요?",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034298.1.13",
          "form": "볶음밥도요?",
          "original_form": "볶음밥도요?",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034298.1.14",
          "form": "네 너무 좋아요!",
          "original_form": "네 너무 좋아요!",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034298.1.15",
          "form": "추천해요",
          "original_form": "추천해요",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034298.1.16",
          "form": "꼭 먹어볼게요 ㅋㅋ",
          "original_form": "꼭 먹어볼게요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034298.1.17",
          "form": "무슨음식 좋아하시나요??",
          "original_form": "무슨음식 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034298.1.18",
          "form": "로제찜닭이요 ㅋㅋㅋ",
          "original_form": "로제찜닭이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034298.1.19",
          "form": "좋아하는게 넘 많네요",
          "original_form": "좋아하는게 넘 많네요",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034298.1.20",
          "form": "헉 ㅋㅋ한번도 안먹어봤는데",
          "original_form": "헉 ㅋㅋ한번도 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034298.1.21",
          "form": "맛있나요??",
          "original_form": "맛있나요??",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034298.1.22",
          "form": "네 두찜거 맛있어요!",
          "original_form": "네 두찜거 맛있어요!",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034298.1.23",
          "form": "당면이랑 고구마랑 먹으면 꿀맛이랍니다",
          "original_form": "당면이랑 고구마랑 먹으면 꿀맛이랍니다",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034298.1.24",
          "form": "당면 고구마는 추가해 먹는건가요?",
          "original_form": "당면 고구마는 추가해 먹는건가요?",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034298.1.25",
          "form": "기본으로 와요",
          "original_form": "기본으로 와요",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034298.1.26",
          "form": "심지어 당면은 엄청 많이와요 ㅋㅋㅋ",
          "original_form": "심지어 당면은 엄청 많이와요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034298.1.27",
          "form": "와 꼭 배달 시켜볼게요 대박",
          "original_form": "와 꼭 배달 시켜볼게요 대박",
          "speaker_id": "1",
          "time": "20210827 23:10"
        }
      ]
    }
  ]
