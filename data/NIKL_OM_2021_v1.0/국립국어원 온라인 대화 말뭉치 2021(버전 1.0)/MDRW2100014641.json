        {
          "id": "MDRW2100014641.1.1",
          "form": "애들은 뭐하나용??",
          "original_form": "애들은 뭐하나용??",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014641.1.2",
          "form": "씻고ㅋㅋ",
          "original_form": "씻고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014641.1.3",
          "form": "내일 개학이니 준비",
          "original_form": "내일 개학이니 준비",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014641.1.4",
          "form": "뭘들고왔는지 아닌지도 모르겄다",
          "original_form": "뭘들고왔는지 아닌지도 모르겄다",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014641.1.5",
          "form": "일단 내일 보내보고ㅋ",
          "original_form": "일단 내일 보내보고ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014641.1.6",
          "form": "언니 나는 그럴까봐 방학하고 온날 들고왔던 준비물 다 정비해가 가방에 그대로 넣어둠요 ㅋㅋㅋㅋ",
          "original_form": "언니 나는 그럴까봐 방학하고 온날 들고왔던 준비물 다 정비해가 가방에 그대로 넣어둠요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014641.1.7",
          "form": "낼 그냥 가기만하면 되여 ㅋㅋㅋ",
          "original_form": "낼 그냥 가기만하면 되여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014641.1.8",
          "form": "늠 신나요^^",
          "original_form": "늠 신나요^^",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014641.1.9",
          "form": "이야~~~",
          "original_form": "이야~~~",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014641.1.10",
          "form": "넌 계획이 다 있구나ㅋ",
          "original_form": "넌 계획이 다 있구나ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014641.1.11",
          "form": "피아노도 낼 하교하고 바로 오라네요 ㅋㅋㅋ",
          "original_form": "피아노도 낼 하교하고 바로 오라네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014641.1.12",
          "form": "낼 평소랑 똑같이 마친데요~쌤이 카페올렸네요^^",
          "original_form": "낼 평소랑 똑같이 마친데요~쌤이 카페올렸네요^^",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014641.1.13",
          "form": "그래ㅋㅋ",
          "original_form": "그래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014641.1.14",
          "form": "행복해요 온니 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "행복해요 온니 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014641.1.15",
          "form": "나도ㅋㅋ행복하다",
          "original_form": "나도ㅋㅋ행복하다",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014641.1.16",
          "form": "밥만안해도 에헤야디야",
          "original_form": "밥만안해도 에헤야디야",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014641.1.17",
          "form": "그니깐여~학교가 정말 큰역할을 한다 ㅋㅋㅋㅋㅋ",
          "original_form": "그니깐여~학교가 정말 큰역할을 한다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014641.1.18",
          "form": "방학때 결식아동은 진짜ㅠ",
          "original_form": "방학때 결식아동은 진짜ㅠ",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014641.1.19",
          "form": "ㅜㅜ 그래 코로나때도 그렇고ㅜㅜ",
          "original_form": "ㅜㅜ 그래 코로나때도 그렇고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014641.1.20",
          "form": "예전엔 미처 몰랐어요~정말 학교가 소중함 ㅋㅋㅋㅋㅋ",
          "original_form": "예전엔 미처 몰랐어요~정말 학교가 소중함 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014641.1.21",
          "form": "예전엔 학교서밥을안줬잖아ㅜ",
          "original_form": "예전엔 학교서밥을안줬잖아ㅜ",
