      "utterance": [
        {
          "id": "MDRW2100023351.1.1",
          "form": "요즘 사고싶은 것이 참 많아요",
          "original_form": "요즘 사고싶은 것이 참 많아요",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023351.1.2",
          "form": "오 어떤 게 사고싶으신가요?",
          "original_form": "오 어떤 게 사고싶으신가요?",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023351.1.3",
          "form": "좀 규모가 남다르긴한데",
          "original_form": "좀 규모가 남다르긴한데",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023351.1.4",
          "form": "한적한 시골에",
          "original_form": "한적한 시골에",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023351.1.5",
          "form": "커다란 집을 짓고싶어요",
          "original_form": "커다란 집을 짓고싶어요",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023351.1.6",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023351.1.7",
          "form": "그런 것도 상품이라고 할 수 있을까요? ㅎㅎ",
          "original_form": "그런 것도 상품이라고 할 수 있을까요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023351.1.8",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023351.1.9",
          "form": "그리고 요즘은 중고거래",
          "original_form": "그리고 요즘은 중고거래",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023351.1.10",
          "form": "잘 되어있어서",
          "original_form": "잘 되어있어서",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023351.1.11",
          "form": "살만한게 많더라구요",
          "original_form": "살만한게 많더라구요",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023351.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023351.1.13",
          "form": "당근이 아쉬운 건",
          "original_form": "당근이 아쉬운 건",
          "speaker_id": "1",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023351.1.14",
          "form": "우리 동네에 재밌는 게 없으니까...",
          "original_form": "우리 동네에 재밌는 게 없으니까...",
          "speaker_id": "1",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023351.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023351.1.16",
          "form": "서울에 살면",
          "original_form": "서울에 살면",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023351.1.17",
          "form": "동네에서",
          "original_form": "동네에서",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023351.1.18",
          "form": "갖고싶은 물건이 자주 나올거같아요",
          "original_form": "갖고싶은 물건이 자주 나올거같아요",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023351.1.19",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023351.1.20",
          "form": "그리고 백화점에 가면",
          "original_form": "그리고 백화점에 가면",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023351.1.21",
          "form": "모자를 사고싶어요",
          "original_form": "모자를 사고싶어요",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023351.1.22",
          "form": "모자!!!",
          "original_form": "모자!!!",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023351.1.23",
          "form": "모자를 좋아하시는군요!",
          "original_form": "모자를 좋아하시는군요!",
          "speaker_id": "1",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023351.1.24",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023351.1.25",
          "form": "나이가 드니까",
          "original_form": "나이가 드니까",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023351.1.26",
          "form": "두피가 휑해요~",
          "original_form": "두피가 휑해요~",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023351.1.27",
          "form": "그러면 모자가 꼭 필요하시겠어요 ㅠㅠ",
          "original_form": "그러면 모자가 꼭 필요하시겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023351.1.28",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023351.1.29",
          "form": "그리고 얼마전에는",
          "original_form": "그리고 얼마전에는",
          "speaker_id": "2",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023351.1.30",
          "form": "부엉이 모양 브로치를",
          "original_form": "부엉이 모양 브로치를",
          "speaker_id": "2",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023351.1.31",
          "form": "갖고싶어했답니다",
          "original_form": "갖고싶어했답니다",
          "speaker_id": "2",
          "time": "20210830 00:40"
        }
      ]
    }
  ]
