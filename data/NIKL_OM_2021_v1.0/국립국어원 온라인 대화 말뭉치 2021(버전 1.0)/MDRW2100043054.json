      "utterance": [
        {
          "id": "MDRW2100043054.1.1",
          "form": "슈키라 같은 청취자엿군요!!",
          "original_form": "슈키라 같은 청취자엿군요!!",
          "speaker_id": "1",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043054.1.2",
          "form": "둘이 입담 너무 좋아서",
          "original_form": "둘이 입담 너무 좋아서",
          "speaker_id": "1",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043054.1.3",
          "form": "진짜 배잡고 웃고 막 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 배잡고 웃고 막 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043054.1.4",
          "form": "맞아요 ㅋㅋㅋㅋ슈주는 진짜 ㅋㅋㅋ예능감이 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ슈주는 진짜 ㅋㅋㅋ예능감이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043054.1.5",
          "form": "막 몰래 듣다가 웃겨서 ㅋㅋㅋㅋㅋ웃음터지고 그랬어요 ㅋㅋㅋㅋㅋ",
          "original_form": "막 몰래 듣다가 웃겨서 ㅋㅋㅋㅋㅋ웃음터지고 그랬어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043054.1.6",
          "form": "그때는 줄이어폰이어서 ㅋㅋㅋ친구랑 하나씩 나눠서 끼고 듣고 막ㅋㅋㅋㅋ",
          "original_form": "그때는 줄이어폰이어서 ㅋㅋㅋ친구랑 하나씩 나눠서 끼고 듣고 막ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043054.1.7",
          "form": "마장요!!! ㅋㅋㅋㅋㅋ",
          "original_form": "마장요!!! ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043054.1.8",
          "form": "나중애는 그 두개 같이 낄수잇는",
          "original_form": "나중애는 그 두개 같이 낄수잇는",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043054.1.9",
          "form": "젠더 구해서 두개꽂아듣고",
          "original_form": "젠더 구해서 두개꽂아듣고",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043054.1.10",
          "form": "하 추억...",
          "original_form": "하 추억...",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043054.1.11",
          "form": "악ㅋㅋㅋㅋ그거도 뭔지 알아요 ㅋㅋㅋㅋㅋㅋㅋ아  정말ㅋㅋㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋㅋ그거도 뭔지 알아요 ㅋㅋㅋㅋㅋㅋㅋ아  정말ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043054.1.12",
          "form": "그거 녹음한다고 테이프에다가",
          "original_form": "그거 녹음한다고 테이프에다가",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043054.1.13",
          "form": "녹음하고그랫는뎈ㅋㅋㅋㅋㅋ",
          "original_form": "녹음하고그랫는뎈ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043054.1.14",
          "form": "세상추억",
          "original_form": "세상추억",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043054.1.15",
          "form": "악ㅋㅋㅋㅋㅋㅋㅋ name1님 당시에 좋아하는 아이돌이나 연예인 있었나요??",
          "original_form": "악ㅋㅋㅋㅋㅋㅋㅋ &name1&님 당시에 좋아하는 아이돌이나 연예인 있었나요??",
          "speaker_id": "2",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100043054.1.16",
          "form": "네 저 ...하...동방신기 진짜",
          "original_form": "네 저 ...하...동방신기 진짜",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100043054.1.17",
          "form": "사랑했거든 요...?",
          "original_form": "사랑했거든 요...?",
          "speaker_id": "1",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100043054.1.18",
          "form": "갈라지고 나서는 죄와죄 좋아했는데 세상에..",
          "original_form": "갈라지고 나서는 죄와죄 좋아했는데 세상에..",
          "speaker_id": "1",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100043054.1.19",
          "form": "와 저도 죄와죄 첫앨범나온거 이거 사줘야한다면서 샀거든요 5만원인가",
          "original_form": "와 저도 죄와죄 첫앨범나온거 이거 사줘야한다면서 샀거든요 5만원인가",
          "speaker_id": "2",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100043054.1.20",
          "form": "하.... ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "하.... ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100043054.1.21",
          "form": "그리고 최근까지도 그래도 윤호는 된사람이야 하면서 아이돌들 윤호같이 열정이 있어야지! 했는데 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그리고 최근까지도 그래도 윤호는 된사람이야 하면서 아이돌들 윤호같이 열정이 있어야지! 했는데 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043054.1.22",
          "form": "엉뚱한데 열정을 쏟고있을 줄이야...",
          "original_form": "엉뚱한데 열정을 쏟고있을 줄이야...",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043054.1.23",
          "form": "저 대학생때는 에셈콘서트 가봤어요....ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 대학생때는 에셈콘서트 가봤어요....ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043054.1.24",
          "form": "도주윤호 진짜 그렇게 안봣는데",
          "original_form": "도주윤호 진짜 그렇게 안봣는데",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043054.1.25",
          "form": "저 콘서트는 안가봠ㅅ어요 ㅋㅋㅋㅋ",
          "original_form": "저 콘서트는 안가봠ㅅ어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043054.1.26",
          "form": "근데 한번쯤 가볼걸 후회돼요",
          "original_form": "근데 한번쯤 가볼걸 후회돼요",
          "speaker_id": "1",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043054.1.27",
          "form": "아 인간들은 싫어졌지만 ㅋㅋㅋ 그때는 재밌게 놀았기도 하고ㅋㅋㅋ 재밌었던 추억이 됐네유  ㅋㅋㅋㅋ",
          "original_form": "아 인간들은 싫어졌지만 ㅋㅋㅋ 그때는 재밌게 놀았기도 하고ㅋㅋㅋ 재밌었던 추억이 됐네유  ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:10"
        },
        {
          "id": "MDRW2100043054.1.28",
          "form": "저 완전 지독한 스엠 ㅃr 여가지고",
          "original_form": "저 완전 지독한 스엠 ㅃr 여가지고",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043054.1.29",
          "form": "남돌여돌 안가리고 엄청 좋아했는데",
          "original_form": "남돌여돌 안가리고 엄청 좋아했는데",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043054.1.30",
          "form": "아 제 주위보면 한번 스엠은 계속 스엠 ㅃ \r\nㅏ 질하던데요ㅋㅋㅋㅋ",
          "original_form": "아 제 주위보면 한번 스엠은 계속 스엠 ㅃ \r\nㅏ 질하던데요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:17"
        },
        {
          "id": "MDRW2100043054.1.31",
          "form": "동방다음에 엑소 좋아하다가 ㅋㅋㅋ지금은 엔시티 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "동방다음에 엑소 좋아하다가 ㅋㅋㅋ지금은 엔시티 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
