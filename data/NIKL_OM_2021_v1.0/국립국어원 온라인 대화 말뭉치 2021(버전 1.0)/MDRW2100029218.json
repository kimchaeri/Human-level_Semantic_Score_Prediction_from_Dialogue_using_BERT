      "utterance": [
        {
          "id": "MDRW2100029218.1.1",
          "form": "여행 자주 가세요?",
          "original_form": "여행 자주 가세요?",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100029218.1.2",
          "form": "저는 이번 3월이 마지막이었어요ㅠㅠ",
          "original_form": "저는 이번 3월이 마지막이었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100029218.1.3",
          "form": "저는 코로나 이후로 간적이 없어요",
          "original_form": "저는 코로나 이후로 간적이 없어요",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100029218.1.4",
          "form": "정말요?? 제주도나 강원도 정도는 많이들 가던데",
          "original_form": "정말요?? 제주도나 강원도 정도는 많이들 가던데",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100029218.1.5",
          "form": "답답하셨겠다",
          "original_form": "답답하셨겠다",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100029218.1.6",
          "form": "작년에 수험생이어서 겸사겸사 참았어요",
          "original_form": "작년에 수험생이어서 겸사겸사 참았어요",
          "speaker_id": "2",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100029218.1.7",
          "form": "올해가 좀 문제네요...",
          "original_form": "올해가 좀 문제네요...",
          "speaker_id": "2",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100029218.1.8",
          "form": "그러셨군요ㅠㅠ",
          "original_form": "그러셨군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100029218.1.9",
          "form": "코로나 다끝나면 어디로 가보고 싶으세요??",
          "original_form": "코로나 다끝나면 어디로 가보고 싶으세요??",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100029218.1.10",
          "form": "제주도 다시 가고 싶어요",
          "original_form": "제주도 다시 가고 싶어요",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100029218.1.11",
          "form": "제주도 저도 3월에 가서 올레길걷고 돌아다녔는데",
          "original_form": "제주도 저도 3월에 가서 올레길걷고 돌아다녔는데",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100029218.1.12",
          "form": "!",
          "original_form": "!",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100029218.1.13",
          "form": "특히 기억에 남는 곳 여쭤봐도 될까요?",
          "original_form": "특히 기억에 남는 곳 여쭤봐도 될까요?",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100029218.1.14",
          "form": "어딘진 기억이 안나는데",
          "original_form": "어딘진 기억이 안나는데",
          "speaker_id": "2",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100029218.1.15",
          "form": "유채꽃이 많이 핀 곳이 좋았어요",
          "original_form": "유채꽃이 많이 핀 곳이 좋았어요",
          "speaker_id": "2",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100029218.1.16",
          "form": "3월에 갔을때도 조금 피어있던데",
          "original_form": "3월에 갔을때도 조금 피어있던데",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100029218.1.17",
          "form": "저는 되게 애매할때 가서 동백꽃은 한창 지고 없는데 벚꽃은 피기전에 가서",
          "original_form": "저는 되게 애매할때 가서 동백꽃은 한창 지고 없는데 벚꽃은 피기전에 가서",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100029218.1.18",
          "form": "아무것도 없었어요ㅠㅠ",
          "original_form": "아무것도 없었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100029218.1.19",
          "form": "심지어 귤도 이미다 수확해서 엄청 황량",
          "original_form": "심지어 귤도 이미다 수확해서 엄청 황량",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100029218.1.20",
          "form": "3월은 애매한 시기군요",
          "original_form": "3월은 애매한 시기군요",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100029218.1.21",
          "form": "3월초가 그러더라구요",
          "original_form": "3월초가 그러더라구요",
          "speaker_id": "1",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100029218.1.22",
          "form": "제주 도착 첫날에 눈도오고그랬어요",
          "original_form": "제주 도착 첫날에 눈도오고그랬어요",
          "speaker_id": "1",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100029218.1.23",
          "form": "신기한 경험 다 하셨겠어요",
          "original_form": "신기한 경험 다 하셨겠어요",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100029218.1.24",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100029218.1.25",
          "form": "눈오는거 볼일은 사실 잘없죠 제주에서",
          "original_form": "눈오는거 볼일은 사실 잘없죠 제주에서",
          "speaker_id": "1",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100029218.1.26",
          "form": "그쵸 제주는 아랫지방이다 보니까",
          "original_form": "그쵸 제주는 아랫지방이다 보니까",
          "speaker_id": "2",
          "time": "20210904 00:58"
        }
      ]
    }
