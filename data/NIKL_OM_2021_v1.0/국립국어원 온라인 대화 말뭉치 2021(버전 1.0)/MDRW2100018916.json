          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018916.1.7",
          "form": "웃기지마라 쫌",
          "original_form": "웃기지마라 쫌",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018916.1.8",
          "form": "아이 정말..",
          "original_form": "아이 정말..",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018916.1.9",
