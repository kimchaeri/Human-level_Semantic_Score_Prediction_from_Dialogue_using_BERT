        },
        {
          "id": "MDRW2100019591.1.2",
          "form": "중고거래 해본 적 있어요?",
          "original_form": "중고거래 해본 적 있어요?",
          "speaker_id": "1",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019591.1.3",
