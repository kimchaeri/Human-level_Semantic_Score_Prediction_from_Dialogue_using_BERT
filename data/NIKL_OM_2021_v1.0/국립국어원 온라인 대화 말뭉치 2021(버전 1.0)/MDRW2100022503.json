      "utterance": [
        {
          "id": "MDRW2100022503.1.1",
          "form": "ㅇㅇ보통 그런 소재는 원탁이지.....",
          "original_form": "ㅇㅇ보통 그런 소재는 원탁이지.....",
          "speaker_id": "2",
          "time": "20210829 15:10"
        },
        {
          "id": "MDRW2100022503.1.2",
          "form": "직사각형이면 그 자리가 말석이 아니니까 ㅋㅋㅋㅋ",
          "original_form": "직사각형이면 그 자리가 말석이 아니니까 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:10"
        },
        {
          "id": "MDRW2100022503.1.3",
          "form": "ㅇㅇ직사각형이면",
          "original_form": "ㅇㅇ직사각형이면",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022503.1.4",
          "form": "여튼 좀 충격적이다....",
          "original_form": "여튼 좀 충격적이다....",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022503.1.5",
          "form": "그치 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그치 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022503.1.6",
          "form": "내가 더러운 정부의 개라니....",
          "original_form": "내가 더러운 정부의 개라니....",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022503.1.7",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022503.1.8",
          "form": "그렇게 되냐고 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇게 되냐고 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022503.1.9",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022503.1.10",
          "form": "나 파란가챠로",
          "original_form": "나 파란가챠로",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022503.1.11",
          "form": "응광 뽑앗어",
          "original_form": "응광 뽑앗어",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022503.1.12",
          "form": "오 ㅊㅊ",
          "original_form": "오 ㅊㅊ",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022503.1.13",
          "form": "근데 쓸일 없겠지?",
          "original_form": "근데 쓸일 없겠지?",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022503.1.14",
          "form": "성능 어때",
          "original_form": "성능 어때",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022503.1.15",
          "form": "아직 안써봤어",
          "original_form": "아직 안써봤어",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022503.1.16",
          "form": "내가 좀 찾아보니까",
          "original_form": "내가 좀 찾아보니까",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022503.1.17",
          "form": "바위는 쓰려면",
          "original_form": "바위는 쓰려면",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022503.1.18",
          "form": "종려 필수더라....",
          "original_form": "종려 필수더라....",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022503.1.19",
          "form": "종려+여행자(바위)+노엘+응광 하면",
          "original_form": "종려+여행자(바위)+노엘+응광 하면",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022503.1.20",
          "form": "바위 캐릭터 다른거 쓰려고 해도 종려가 있어야 해?",
          "original_form": "바위 캐릭터 다른거 쓰려고 해도 종려가 있어야 해?",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022503.1.21",
          "form": "단단한 조합이래",
          "original_form": "단단한 조합이래",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022503.1.22",
          "form": "약간 그",
          "original_form": "약간 그",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022503.1.23",
          "form": "좋은 조합 짜려면?",
          "original_form": "좋은 조합 짜려면?",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022503.1.24",
          "form": "단단한 조합 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "단단한 조합 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100022503.1.25",
          "form": "피슬+행추+베넷 하면",
          "original_form": "피슬+행추+베넷 하면",
          "speaker_id": "2",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100022503.1.26",
          "form": "딜러 누구를 집어넣어도 상성좋은",
          "original_form": "딜러 누구를 집어넣어도 상성좋은",
          "speaker_id": "2",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100022503.1.27",
          "form": "서포터조합이래",
          "original_form": "서포터조합이래",
          "speaker_id": "2",
          "time": "20210829 15:14"
        }
      ]
    }
