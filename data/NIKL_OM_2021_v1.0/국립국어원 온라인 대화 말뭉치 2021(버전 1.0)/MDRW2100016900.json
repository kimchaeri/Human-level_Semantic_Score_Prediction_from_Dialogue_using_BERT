      "utterance": [
        {
          "id": "MDRW2100016900.1.1",
          "form": "1960년대 배경인데 재밌어 너도 보자",
          "original_form": "1960년대 배경인데 재밌어 너도 보자",
          "speaker_id": "2",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016900.1.2",
          "form": "넷플릭스면 함 볼 수있겠다",
          "original_form": "넷플릭스면 함 볼 수있겠다",
          "speaker_id": "1",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016900.1.3",
          "form": "조아용 조아용",
          "original_form": "조아용 조아용",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016900.1.4",
          "form": "매달 결제 해?",
          "original_form": "매달 결제 해?",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016900.1.5",
          "form": "동생이ㅋㅋㅋㅋ",
          "original_form": "동생이ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016900.1.6",
          "form": "난 티빙",
          "original_form": "난 티빙",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016900.1.7",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016900.1.8",
          "form": "좋네",
          "original_form": "좋네",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016900.1.9",
          "form": "엄마가 젤 잘 쓰는듯",
          "original_form": "엄마가 젤 잘 쓰는듯",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016900.1.10",
          "form": "나는 오빠가 결제했을 때 몇 달 보고 말았는데",
          "original_form": "나는 오빠가 결제했을 때 몇 달 보고 말았는데",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016900.1.11",
          "form": "우리 가족은 엄빠가 그런거 안봐..",
          "original_form": "우리 가족은 엄빠가 그런거 안봐..",
          "speaker_id": "2",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016900.1.12",
          "form": "ㅋㅋㅋ울 집도 아빠는 티비만 봄",
          "original_form": "ㅋㅋㅋ울 집도 아빠는 티비만 봄",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016900.1.13",
          "form": "힝구힝구",
          "original_form": "힝구힝구",
          "speaker_id": "2",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016900.1.14",
          "form": "넷플 둘만 보기엔 아까워",
          "original_form": "넷플 둘만 보기엔 아까워",
          "speaker_id": "2",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016900.1.15",
          "form": "그건 그렇지... 비싸구",
          "original_form": "그건 그렇지... 비싸구",
          "speaker_id": "1",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016900.1.16",
          "form": "애니도 좀 있더라",
          "original_form": "애니도 좀 있더라",
          "speaker_id": "1",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016900.1.17",
          "form": "애니는 라프텔 있어서 그런가 잘 안 보게 되는듯",
          "original_form": "애니는 라프텔 있어서 그런가 잘 안 보게 되는듯",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016900.1.18",
          "form": "라프텔 이제 안 해서",
          "original_form": "라프텔 이제 안 해서",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016900.1.19",
          "form": "애니만 보면 좀 질리긴 해",
          "original_form": "애니만 보면 좀 질리긴 해",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016900.1.20",
          "form": "넷플에 애니 예전거도 보구ㅋㅋ",
          "original_form": "넷플에 애니 예전거도 보구ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:15"
        }
      ]
    }
