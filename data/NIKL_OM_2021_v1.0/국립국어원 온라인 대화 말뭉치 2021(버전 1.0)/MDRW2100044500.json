      "utterance": [
        {
          "id": "MDRW2100044500.1.1",
          "form": "공연 좋아하세요??",
          "original_form": "공연 좋아하세요??",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044500.1.2",
          "form": "네 요즘 못가지만ㅠ",
          "original_form": "네 요즘 못가지만ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044500.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044500.1.4",
          "form": "저도 좋아했는데ㅋ쿠",
          "original_form": "저도 좋아했는데ㅋ쿠",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044500.1.5",
          "form": "주로 어떤 공연 보셨어요?",
          "original_form": "주로 어떤 공연 보셨어요?",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044500.1.6",
          "form": "저는 가수공연이요ㅎㅎ",
          "original_form": "저는 가수공연이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044500.1.7",
          "form": "포레스텔라랑 거미 가봤어요ㅎㅎ",
          "original_form": "포레스텔라랑 거미 가봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044500.1.8",
          "form": "아 저는 뮤지컬이랑 연극ㅋㅋ",
          "original_form": "아 저는 뮤지컬이랑 연극ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044500.1.9",
          "form": "오 저는 연극은 한번봤어요",
          "original_form": "오 저는 연극은 한번봤어요",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044500.1.10",
          "form": "라이어게임요",
          "original_form": "라이어게임요",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044500.1.11",
          "form": "뮤지컬 좋아하세요?",
          "original_form": "뮤지컬 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044500.1.12",
          "form": "그때 다니던 회사가 문화비 지원해쥬서ㅋㅋ",
          "original_form": "그때 다니던 회사가 문화비 지원해쥬서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044500.1.13",
          "form": "제돈으로 두어번 보고요",
          "original_form": "제돈으로 두어번 보고요",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044500.1.14",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044500.1.15",
          "form": "좋은회사에요ㅋㅋ",
          "original_form": "좋은회사에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044500.1.16",
          "form": "저는보통제돈주고ㅋㅋ",
          "original_form": "저는보통제돈주고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044500.1.17",
          "form": "저도 퇴사후엔 내돈내산ㅋㅋ",
          "original_form": "저도 퇴사후엔 내돈내산ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044500.1.18",
          "form": "티케팅이 힘들더라고욬ㅋ",
          "original_form": "티케팅이 힘들더라고욬ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044500.1.19",
          "form": "제돈주기엔 진짜 비싸요",
          "original_form": "제돈주기엔 진짜 비싸요",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044500.1.20",
          "form": "맞아요 큰맘먹고 질러야해요",
          "original_form": "맞아요 큰맘먹고 질러야해요",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044500.1.21",
          "form": "회전문도시는 분들은 진짜 대단하신거같아요",
          "original_form": "회전문도시는 분들은 진짜 대단하신거같아요",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044500.1.22",
          "form": "그니까요 한번에 십만원!!!",
          "original_form": "그니까요 한번에 십만원!!!",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
