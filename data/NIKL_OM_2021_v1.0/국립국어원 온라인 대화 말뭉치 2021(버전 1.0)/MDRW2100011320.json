      "utterance": [
        {
          "id": "MDRW2100011320.1.1",
          "form": "구매하신 상품 중에 마음에 드는 게 있나요",
          "original_form": "구매하신 상품 중에 마음에 드는 게 있나요",
          "speaker_id": "1",
          "time": "20210824 11:04"
        },
        {
          "id": "MDRW2100011320.1.2",
          "form": "글쎄요. 다 나름 만족하고 쓰는거 같아요",
          "original_form": "글쎄요. 다 나름 만족하고 쓰는거 같아요",
          "speaker_id": "2",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100011320.1.3",
          "form": "최근엔 우양산을 샀어요",
          "original_form": "최근엔 우양산을 샀어요",
          "speaker_id": "2",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100011320.1.4",
          "form": "아무래도 자외선이 따가 우니까요",
          "original_form": "아무래도 자외선이 따가 우니까요",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100011320.1.5",
          "form": "양산은 어때요",
          "original_form": "양산은 어때요",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100011320.1.6",
          "form": "자동으로 펼쳐지는데 편하더라고요.",
          "original_form": "자동으로 펼쳐지는데 편하더라고요.",
          "speaker_id": "2",
          "time": "20210824 11:07"
        },
        {
          "id": "MDRW2100011320.1.7",
          "form": "디자인도 귀엽구요",
          "original_form": "디자인도 귀엽구요",
          "speaker_id": "2",
          "time": "20210824 11:07"
        },
        {
          "id": "MDRW2100011320.1.8",
          "form": "어떤 디자인의 양산을 사셨나요",
          "original_form": "어떤 디자인의 양산을 사셨나요",
          "speaker_id": "1",
          "time": "20210824 11:07"
        },
        {
          "id": "MDRW2100011320.1.9",
          "form": "고양이랑 귤이 그려져있는 양산이에요",
          "original_form": "고양이랑 귤이 그려져있는 양산이에요",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100011320.1.10",
          "form": "너무 귀엽겠다",
          "original_form": "너무 귀엽겠다",
          "speaker_id": "1",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100011320.1.11",
          "form": "ㅋㅋ커버에도 작게 그려져서 귀여워요",
          "original_form": "ㅋㅋ커버에도 작게 그려져서 귀여워요",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100011320.1.12",
          "form": "우양산 이면 우산도 되고 양산도 되는 거죠",
          "original_form": "우양산 이면 우산도 되고 양산도 되는 거죠",
          "speaker_id": "1",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011320.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011320.1.14",
          "form": "오늘처럼 비가 많을땐 쓰기 좀 그렇네요. 작은 크기라ㅋㅋ",
          "original_form": "오늘처럼 비가 많을땐 쓰기 좀 그렇네요. 작은 크기라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011320.1.15",
          "form": "앗 아무래도 그럴 거 같아요ㅎ",
          "original_form": "앗 아무래도 그럴 거 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011320.1.16",
          "form": "좀 적게 오는 날 쓰기 좋을 거 같아요",
          "original_form": "좀 적게 오는 날 쓰기 좋을 거 같아요",
          "speaker_id": "1",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100011320.1.17",
          "form": "갑작스런 소나기땐 괜찮더라고요.",
          "original_form": "갑작스런 소나기땐 괜찮더라고요.",
          "speaker_id": "2",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100011320.1.18",
          "form": "양산으론 좋았으니 만족하고있어요",
          "original_form": "양산으론 좋았으니 만족하고있어요",
          "speaker_id": "2",
          "time": "20210824 11:11"
        },
        {
          "id": "MDRW2100011320.1.19",
          "form": "만족하시면 된 거죠",
          "original_form": "만족하시면 된 거죠",
          "speaker_id": "1",
          "time": "20210824 11:11"
        },
        {
          "id": "MDRW2100011320.1.20",
          "form": "뭐가 중요한가요",
          "original_form": "뭐가 중요한가요",
          "speaker_id": "1",
          "time": "20210824 11:11"
        }
      ]
