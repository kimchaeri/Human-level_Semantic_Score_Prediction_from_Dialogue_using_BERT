        {
          "id": "MDRW2100014122.1.2",
          "form": "잇츠미",
          "original_form": "잇츠미",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014122.1.3",
          "form": "나 여행 가고 싶어",
          "original_form": "나 여행 가고 싶어",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014122.1.4",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014122.1.5",
          "form": "나",
          "original_form": "나",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014122.1.6",
          "form": "동남아",
          "original_form": "동남아",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014122.1.7",
          "form": "돈 모아 나랑 같이 여행 가자",
          "original_form": "돈 모아 나랑 같이 여행 가자",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014122.1.8",
          "form": "근데 안 갈 거잖아",
          "original_form": "근데 안 갈 거잖아",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014122.1.9",
          "form": "다 알아",
          "original_form": "다 알아",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014122.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014122.1.11",
          "form": "너 안가잖아",
          "original_form": "너 안가잖아",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014122.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 갈 거야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 갈 거야",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014122.1.13",
          "form": "시간만 있으면 갈 거야",
          "original_form": "시간만 있으면 갈 거야",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014122.1.14",
          "form": "어어",
          "original_form": "어어",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014122.1.15",
          "form": "이래놓고 안가기",
          "original_form": "이래놓고 안가기",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014122.1.16",
          "form": "뭐말해야하지",
          "original_form": "뭐말해야하지",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014122.1.17",
          "form": "여행경험",
          "original_form": "여행경험",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014122.1.18",
          "form": "썰을 풀어보자",
          "original_form": "썰을 풀어보자",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014122.1.19",
          "form": "나 여행 그 거기",
          "original_form": "나 여행 그 거기",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014122.1.20",
          "form": "베트남 다녀왔는데",
          "original_form": "베트남 다녀왔는데",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014122.1.21",
          "form": "사기당해서 35만원 잃어버리고 엄마 핸드폰 잃어버리고",
          "original_form": "사기당해서 35만원 잃어버리고 엄마 핸드폰 잃어버리고",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014122.1.22",
          "form": "돌아오는 날 비행기도 지연됐어",
          "original_form": "돌아오는 날 비행기도 지연됐어",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014122.1.23",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014122.1.24",
          "form": "전혀 긍정적인 방향이 아니쥬",
          "original_form": "전혀 긍정적인 방향이 아니쥬",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014122.1.25",
          "form": "난 제주도가 내 마지막 여행지였다",
          "original_form": "난 제주도가 내 마지막 여행지였다",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014122.1.26",
          "form": "비 많이 내렸던것밖에 기억이안남",
          "original_form": "비 많이 내렸던것밖에 기억이안남",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014122.1.27",
          "form": "나도 제주도 갈ㄹ",
          "original_form": "나도 제주도 갈ㄹ",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014122.1.28",
          "form": "내 마지막 제주도는 수학여행 때야",
          "original_form": "내 마지막 제주도는 수학여행 때야",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014122.1.29",
          "form": "까마득하네",
          "original_form": "까마득하네",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014122.1.30",
          "form": "진짜 할 얘기 없다",
          "original_form": "진짜 할 얘기 없다",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014122.1.31",
          "form": "제주도에서 흑돼지 먹었던 거 같아",
          "original_form": "제주도에서 흑돼지 먹었던 거 같아",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014122.1.32",
          "form": "맛있어",
          "original_form": "맛있어",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014122.1.33",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014122.1.34",
          "form": "제주도 그 백종원 골목식당 했던곳",
          "original_form": "제주도 그 백종원 골목식당 했던곳",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014122.1.35",
          "form": "돈가스집 가야되는데",
          "original_form": "돈가스집 가야되는데",
          "speaker_id": "2",
          "time": "20210825 17:37"
        }
      ]
