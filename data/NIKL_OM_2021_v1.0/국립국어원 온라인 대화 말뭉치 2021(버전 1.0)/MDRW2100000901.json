      "utterance": [
        {
          "id": "MDRW2100000901.1.1",
          "form": "뮤지컬 보러가고싶다",
          "original_form": "뮤지컬 보러가고싶다",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000901.1.2",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000901.1.3",
          "form": "요즘 유투브에 시카고",
          "original_form": "요즘 유투브에 시카고",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.4",
          "form": "우리 팬텀 너무 아깝다",
          "original_form": "우리 팬텀 너무 아깝다",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.5",
          "form": "클립 엄청 많이뜨던데",
          "original_form": "클립 엄청 많이뜨던데",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.6",
          "form": "코로나때문에 못갔잖아ㅠㅠ",
          "original_form": "코로나때문에 못갔잖아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.7",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.8",
          "form": "아진짜?",
          "original_form": "아진짜?",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.9",
          "form": "나는 그거 말고 헤드윅? 헤드웍? 조승우 나온 거 보고싶다",
          "original_form": "나는 그거 말고 헤드윅? 헤드웍? 조승우 나온 거 보고싶다",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.10",
          "form": "근데 그 배우한명 코로나 확진떴대",
          "original_form": "근데 그 배우한명 코로나 확진떴대",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.11",
          "form": "아 팬텀이 아니라 오페라의유령",
          "original_form": "아 팬텀이 아니라 오페라의유령",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.12",
          "form": "아 맞아 그거 근데 트젠? 이야긴거 알고있었음?",
          "original_form": "아 맞아 그거 근데 트젠? 이야긴거 알고있었음?",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.13",
          "form": "뭐 그게 그거지만",
          "original_form": "뭐 그게 그거지만",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.14",
          "form": "어어 머리기르고 나오잖어",
          "original_form": "어어 머리기르고 나오잖어",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.15",
          "form": "옷도 좀 그렇고",
          "original_form": "옷도 좀 그렇고",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.16",
          "form": "나는 몰랐어서 영상으로보고",
          "original_form": "나는 몰랐어서 영상으로보고",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.17",
          "form": "깜짝놀랐잖아",
          "original_form": "깜짝놀랐잖아",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.18",
          "form": "뮤지컬 좋아하눈 애들은",
          "original_form": "뮤지컬 좋아하눈 애들은",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000901.1.19",
          "form": "그 책도 많이 사도라",
          "original_form": "그 책도 많이 사도라",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000901.1.20",
          "form": "나도 뮤지컬 엠디도 갖고싶고 일단 뮤지컬 보러가고싶다",
          "original_form": "나도 뮤지컬 엠디도 갖고싶고 일단 뮤지컬 보러가고싶다",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000901.1.21",
          "form": "맞아 뮤지컬 넘버도 좋대",
          "original_form": "맞아 뮤지컬 넘버도 좋대",
          "speaker_id": "2",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000901.1.22",
          "form": "일단 돈을 벌어야...",
          "original_form": "일단 돈을 벌어야...",
          "speaker_id": "2",
          "time": "20210819 20:31"
        }
      ]
