      "utterance": [
        {
          "id": "MDRW2100012395.1.1",
          "form": "아이는 요즘 말 잘 듣나?",
          "original_form": "아이는 요즘 말 잘 듣나?",
          "speaker_id": "1",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100012395.1.2",
          "form": "말도이쁘게하고 도와주고",
          "original_form": "말도이쁘게하고 도와주고",
          "speaker_id": "2",
          "time": "20210824 21:49"
        },
        {
          "id": "MDRW2100012395.1.3",
          "form": "어휴 장하네",
          "original_form": "어휴 장하네",
          "speaker_id": "1",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100012395.1.4",
          "form": "자기를 믿어주고 화안내면 엄청 착한아이다",
          "original_form": "자기를 믿어주고 화안내면 엄청 착한아이다",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012395.1.5",
          "form": "스스로 알아서 할려고 한다",
          "original_form": "스스로 알아서 할려고 한다",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012395.1.6",
          "form": "그래 믿어라~자꾸 다그치지말고",
          "original_form": "그래 믿어라~자꾸 다그치지말고",
          "speaker_id": "1",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012395.1.7",
          "form": "멋진 아들이네",
          "original_form": "멋진 아들이네",
          "speaker_id": "1",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012395.1.8",
          "form": "어 근데 그게 안쉽다",
          "original_form": "어 근데 그게 안쉽다",
          "speaker_id": "2",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100012395.1.9",
          "form": "이렇게만 키우면 잘클거같기는 한데",
          "original_form": "이렇게만 키우면 잘클거같기는 한데",
          "speaker_id": "2",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100012395.1.10",
          "form": "니가 걱정이 많아서 칸다",
          "original_form": "니가 걱정이 많아서 칸다",
          "speaker_id": "1",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100012395.1.11",
          "form": "잘 크는 아이다",
          "original_form": "잘 크는 아이다",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012395.1.12",
          "form": "ㅇㅇ예민하고 너무 앞서서 걱정한다 항상",
          "original_form": "ㅇㅇ예민하고 너무 앞서서 걱정한다 항상",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012395.1.13",
          "form": "상처가 많아서 그렇다 ㅜ",
          "original_form": "상처가 많아서 그렇다 ㅜ",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012395.1.14",
          "form": "그런가보다ㅜ",
          "original_form": "그런가보다ㅜ",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012395.1.15",
          "form": "니가 이겨내야지",
          "original_form": "니가 이겨내야지",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012395.1.16",
          "form": "사람이 너무 앞이 보이는것도 안좋다",
          "original_form": "사람이 너무 앞이 보이는것도 안좋다",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012395.1.17",
          "form": "무뎌지고 싶네",
          "original_form": "무뎌지고 싶네",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012395.1.18",
          "form": "그니깐 말이다ㅜ",
          "original_form": "그니깐 말이다ㅜ",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012395.1.19",
          "form": "예견이되니ㅜ좋은것도 있고 안 좋은것도 있고",
          "original_form": "예견이되니ㅜ좋은것도 있고 안 좋은것도 있고",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012395.1.20",
          "form": "어 뭐든 그런거같다 장.단점이",
          "original_form": "어 뭐든 그런거같다 장.단점이",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012395.1.21",
          "form": "애키우는게어렵다",
          "original_form": "애키우는게어렵다",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012395.1.22",
          "form": "형제는 어떤데?",
          "original_form": "형제는 어떤데?",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012395.1.23",
          "form": "형제에 아빠까지 싸우니 대략 난감이다",
          "original_form": "형제에 아빠까지 싸우니 대략 난감이다",
          "speaker_id": "1",
          "time": "20210824 22:22"
        }
      ]
