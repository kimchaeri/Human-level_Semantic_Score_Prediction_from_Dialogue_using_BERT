      "utterance": [
        {
          "id": "MDRW2100021189.1.1",
          "form": "아 너무 고민되는데?",
          "original_form": "아 너무 고민되는데?",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100021189.1.2",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "1",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100021189.1.3",
          "form": "얼라이브?",
          "original_form": "얼라이브?",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021189.1.4",
          "form": "아 얼라이브 좋지..",
          "original_form": "아 얼라이브 좋지..",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021189.1.5",
          "form": "아니면 더 웨이백..",
          "original_form": "아니면 더 웨이백..",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021189.1.6",
          "form": "웨이백 못잃지..",
          "original_form": "웨이백 못잃지..",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021189.1.7",
          "form": "웨이백이 진짜 숨은 명곡이잖아",
          "original_form": "웨이백이 진짜 숨은 명곡이잖아",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021189.1.8",
          "form": "사실 내 픽은 컨프롱이었어..",
          "original_form": "사실 내 픽은 컨프롱이었어..",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021189.1.9",
          "form": "컨프롱도 좋지....아휴 컨프롱 좋지..",
          "original_form": "컨프롱도 좋지....아휴 컨프롱 좋지..",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021189.1.10",
          "form": "컨프롱은 정말... 그거보러 지킬보러 가는 거 같아",
          "original_form": "컨프롱은 정말... 그거보러 지킬보러 가는 거 같아",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021189.1.11",
          "form": "약간 마지막 피날레 느낌이잖아. 그거 잘하면 그냥 공연 찢은거야",
          "original_form": "약간 마지막 피날레 느낌이잖아. 그거 잘하면 그냥 공연 찢은거야",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021189.1.12",
          "form": "맞아.. 나 아직도 처음에 그 안면근육 다르게 쓴 거 잊지못해",
          "original_form": "맞아.. 나 아직도 처음에 그 안면근육 다르게 쓴 거 잊지못해",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021189.1.13",
          "form": "나도 처음에 은지킬 컨프롱 보고 충격먹었잖아",
          "original_form": "나도 처음에 은지킬 컨프롱 보고 충격먹었잖아",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021189.1.14",
          "form": "그러니까.. 다 그랬을걸",
          "original_form": "그러니까.. 다 그랬을걸",
          "speaker_id": "1",
