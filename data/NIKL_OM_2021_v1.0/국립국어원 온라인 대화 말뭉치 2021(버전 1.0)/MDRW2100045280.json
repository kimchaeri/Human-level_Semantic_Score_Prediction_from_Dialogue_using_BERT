        },
        {
          "id": "MDRW2100045280.1.2",
          "form": "완곡으로???",
          "original_form": "완곡으로???",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045280.1.3",
          "form": "아닠ㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045280.1.4",
          "form": "자기 가사모른다고",
          "original_form": "자기 가사모른다고",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045280.1.5",
          "form": "중간에 멈추긴했는데",
          "original_form": "중간에 멈추긴했는데",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045280.1.6",
          "form": "거의 1절 반주까지깔고 노래부르던ㄷㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "거의 1절 반주까지깔고 노래부르던ㄷㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045280.1.7",
          "form": "그래도 반주 깐거면",
          "original_form": "그래도 반주 깐거면",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045280.1.8",
          "form": "본격적인뎈ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "본격적인뎈ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045280.1.9",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045280.1.10",
          "form": "원래 그냥",
          "original_form": "원래 그냥",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045280.1.11",
          "form": "겟세마네 알아???",
          "original_form": "겟세마네 알아???",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045280.1.12",
          "form": "하면서 한두소절 부르는게 다인데",
          "original_form": "하면서 한두소절 부르는게 다인데",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045280.1.13",
          "form": "이번거는",
          "original_form": "이번거는",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045280.1.14",
          "form": "유다부분 ㅋㅋㅋㅋㅋ 반주 준비했나봐 깔고부름",
          "original_form": "유다부분 ㅋㅋㅋㅋㅋ 반주 준비했나봐 깔고부름",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045280.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045280.1.16",
          "form": "ㅋㅋㅋㅋㅋ오늘 뭔가 신나보였어",
          "original_form": "ㅋㅋㅋㅋㅋ오늘 뭔가 신나보였어",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045280.1.17",
          "form": "텐션 좋았나보네",
          "original_form": "텐션 좋았나보네",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045280.1.18",
          "form": "갈수록",
          "original_form": "갈수록",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045280.1.19",
          "form": "그 감정 치닫는 부분에서",
          "original_form": "그 감정 치닫는 부분에서",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045280.1.20",
          "form": "잘울더라고여",
          "original_form": "잘울더라고여",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045280.1.21",
          "form": "역시 연기잘하네...",
          "original_form": "역시 연기잘하네...",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045280.1.22",
          "form": "저런거보다가 민기꺼보니까....",
          "original_form": "저런거보다가 민기꺼보니까....",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045280.1.23",
          "form": "배우들 그런 연기 하는 거 보면 신기해",
          "original_form": "배우들 그런 연기 하는 거 보면 신기해",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045280.1.24",
          "form": "냉정하지만 감정의 깊이가 다르다...",
          "original_form": "냉정하지만 감정의 깊이가 다르다...",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045280.1.25",
          "form": "민기야미아내...",
          "original_form": "민기야미아내...",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045280.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜...",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045280.1.27",
          "form": "연기로 감정을 그렇게 끌어올리는 것도 그렇고",
          "original_form": "연기로 감정을 그렇게 끌어올리는 것도 그렇고",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045280.1.28",
          "form": "매 회차마다 같은 연기 그렇게 하는것도..",
          "original_form": "매 회차마다 같은 연기 그렇게 하는것도..",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045280.1.29",
          "form": "그 토미가 자기를 떠나는 부분",
          "original_form": "그 토미가 자기를 떠나는 부분",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045280.1.30",
          "form": "두명부분을 혼자서 모노드라마\r\n하듯히 하거든",
          "original_form": "두명부분을 혼자서 모노드라마\r\n하듯히 하거든",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045280.1.31",
          "form": "근데 거기서 진짜 울컥하고",
          "original_form": "근데 거기서 진짜 울컥하고",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045280.1.32",
          "form": "눈물흘리느너도 너무",
          "original_form": "눈물흘리느너도 너무",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045280.1.33",
          "form": "사람한테 진짜 느껴지게",
          "original_form": "사람한테 진짜 느껴지게",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045280.1.34",
          "form": "감정전달이되는거보면",
          "original_form": "감정전달이되는거보면",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045280.1.35",
          "form": "너무신기...",
          "original_form": "너무신기...",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045280.1.36",
          "form": "그리고 마지막에 토미가",
          "original_form": "그리고 마지막에 토미가",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045280.1.37",
          "form": "미안하다고 하는거보고",
          "original_form": "미안하다고 하는거보고",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045280.1.38",
          "form": "응어리풀릴때 집어던진 가발 끌어안는 부분이있는데",
          "original_form": "응어리풀릴때 집어던진 가발 끌어안는 부분이있는데",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045280.1.39",
          "form": "그 간단한 몸짓하나에서도",
          "original_form": "그 간단한 몸짓하나에서도",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045280.1.40",
          "form": "얼마나서러웠는지 그동안 힘들었는지 ㄹ이ㅣ런게 다 느껴지더군여",
          "original_form": "얼마나서러웠는지 그동안 힘들었는지 ㄹ이ㅣ런게 다 느껴지더군여",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045280.1.41",
          "form": "대단...",
          "original_form": "대단...",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045280.1.42",
          "form": "그런게 진짜 신기한듯",
          "original_form": "그런게 진짜 신기한듯",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045280.1.43",
          "form": "그부분은 뀨가 더 좋다고생각했는데",
          "original_form": "그부분은 뀨가 더 좋다고생각했는데",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045280.1.44",
          "form": "최근 본두회차에서는",
          "original_form": "최근 본두회차에서는",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045280.1.45",
          "form": "조도 좋았어서",
          "original_form": "조도 좋았어서",
          "speaker_id": "1",
          "time": "20210908 01:45"
        }
      ]
    }
