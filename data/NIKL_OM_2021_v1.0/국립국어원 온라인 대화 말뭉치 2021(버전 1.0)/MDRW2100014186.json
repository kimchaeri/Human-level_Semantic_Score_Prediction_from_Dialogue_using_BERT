        },
        {
          "id": "MDRW2100014186.1.3",
          "form": "name2은 유투브 많이보시나요?",
          "original_form": "&name2&은 유투브 많이보시나요?",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014186.1.4",
          "form": "네 자주 봐요ㅋㅋ",
          "original_form": "네 자주 봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014186.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014186.1.6",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014186.1.7",
          "form": "저는 고양이 많이 찾아봐요",
          "original_form": "저는 고양이 많이 찾아봐요",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014186.1.8",
          "form": "어 저도 고양이좋아해요",
          "original_form": "어 저도 고양이좋아해요",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014186.1.9",
          "form": "어떤채널 많이보세요?",
          "original_form": "어떤채널 많이보세요?",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014186.1.10",
          "form": "고양이와 댕댕이었남",
          "original_form": "고양이와 댕댕이었남",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014186.1.11",
          "form": "강아지 먹방도 좋아해요 별님은요",
          "original_form": "강아지 먹방도 좋아해요 별님은요",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014186.1.12",
          "form": "아항 제가보는건 냥큼한친구들이라고 고양이 많이 돌보시는 분 채널있어요",
          "original_form": "아항 제가보는건 냥큼한친구들이라고 고양이 많이 돌보시는 분 채널있어요",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014186.1.13",
          "form": "오  함구경가봐야겠네요",
          "original_form": "오  함구경가봐야겠네요",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014186.1.14",
          "form": "고양이들 다 귀여워요ㅎㅎ",
          "original_form": "고양이들 다 귀여워요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014186.1.15",
          "form": "고양이 많이 키우는분이 나오시나봐요",
          "original_form": "고양이 많이 키우는분이 나오시나봐요",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014186.1.16",
          "form": "네 길고양이 구조도 자주 하시는 분이세요",
          "original_form": "네 길고양이 구조도 자주 하시는 분이세요",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014186.1.17",
          "form": "오오 의로운분이시네요 구독해야겠어요",
          "original_form": "오오 의로운분이시네요 구독해야겠어요",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014186.1.18",
          "form": "좀만보셔도 빠지실거에요ㅎㅎ",
          "original_form": "좀만보셔도 빠지실거에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
