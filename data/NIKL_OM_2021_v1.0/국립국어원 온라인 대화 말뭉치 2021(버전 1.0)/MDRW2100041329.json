      "utterance": [
        {
          "id": "MDRW2100041329.1.1",
          "form": "name2님은 매운거 잘 드시나요?",
          "original_form": "&name2&님은 매운거 잘 드시나요?",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041329.1.2",
          "form": "저는 맵찔이랍니다^^ㅎㅎ",
          "original_form": "저는 맵찔이랍니다^^ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100041329.1.3",
          "form": "앗 그러시구나",
          "original_form": "앗 그러시구나",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100041329.1.4",
          "form": "저도 잘 먹는 편 아니에요",
          "original_form": "저도 잘 먹는 편 아니에요",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100041329.1.5",
          "form": "엽떡... 착한 맛만 좋아해요 ㅎ",
          "original_form": "엽떡... 착한 맛만 좋아해요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100041329.1.6",
          "form": "ㅋㅋㅋㅋ 저는 신전좋아하는데 순한맛만 먹어요",
          "original_form": "ㅋㅋㅋㅋ 저는 신전좋아하는데 순한맛만 먹어요",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100041329.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100041329.1.8",
          "form": "예전에 엽떡 무서운줄 모르고",
          "original_form": "예전에 엽떡 무서운줄 모르고",
          "speaker_id": "2",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100041329.1.9",
          "form": "먹었다가 크게 데이고 그 다음부턴 못먹겠어요 ㅎ",
          "original_form": "먹었다가 크게 데이고 그 다음부턴 못먹겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100041329.1.10",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100041329.1.11",
          "form": "악",
          "original_form": "악",
          "speaker_id": "1",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100041329.1.12",
          "form": "그러면 응꼬의 심판을 받으셨겠군요....ㅠ",
          "original_form": "그러면 응꼬의 심판을 받으셨겠군요....ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100041329.1.13",
          "form": "..ㅠㅠㅠㅠ 저 진짜 매워서 울었어요 ㅋㅋㅋㅋㅋ",
          "original_form": "..ㅠㅠㅠㅠ 저 진짜 매워서 울었어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041329.1.14",
          "form": "아니 무슨 분명히 처음 입에 넣을땐 괜찮았어요",
          "original_form": "아니 무슨 분명히 처음 입에 넣을땐 괜찮았어요",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041329.1.15",
          "form": "분명히!",
          "original_form": "분명히!",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041329.1.16",
          "form": "근데 몇초 지나고 삼키니까 시작되더라고요?",
          "original_form": "근데 몇초 지나고 삼키니까 시작되더라고요?",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041329.1.17",
          "form": "악마의 음식이에용",
          "original_form": "악마의 음식이에용",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041329.1.18",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041329.1.19",
          "form": "아니진짜 먹고 씹어야하지만",
          "original_form": "아니진짜 먹고 씹어야하지만",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041329.1.20",
          "form": "진정한 무서움(?)이 드러나는,,,,",
          "original_form": "진정한 무서움(?)이 드러나는,,,,",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041329.1.21",
          "form": "뭔지 너무 알 것 같아요...",
          "original_form": "뭔지 너무 알 것 같아요...",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041329.1.22",
          "form": "상상만해도 고개가 절레절레 후 무서워요 ㅋㅋㅋ",
          "original_form": "상상만해도 고개가 절레절레 후 무서워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100041329.1.23",
          "form": "그쵸그쵸....",
          "original_form": "그쵸그쵸....",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041329.1.24",
          "form": "저는 제일 화나는건",
          "original_form": "저는 제일 화나는건",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041329.1.25",
          "form": "맛 별로 없는데 맵기만 한거...",
          "original_form": "맛 별로 없는데 맵기만 한거...",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041329.1.26",
          "form": "ㅋㅋ 못참아요!!ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ 못참아요!!ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100041329.1.27",
          "form": "들디님 내일 뭐드실거에용",
          "original_form": "들디님 내일 뭐드실거에용",
          "speaker_id": "2",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100041329.1.28",
          "form": "헤헤 내일은",
          "original_form": "헤헤 내일은",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100041329.1.29",
          "form": "이제 다시 일상이니까....",
          "original_form": "이제 다시 일상이니까....",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100041329.1.30",
