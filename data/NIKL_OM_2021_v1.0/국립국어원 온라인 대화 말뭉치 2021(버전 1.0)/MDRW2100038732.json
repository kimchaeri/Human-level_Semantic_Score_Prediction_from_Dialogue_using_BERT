        {
          "id": "MDRW2100038732.1.1",
          "form": "학교다니시나여?",
          "original_form": "학교다니시나여?",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100038732.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100038732.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100038732.1.4",
          "form": "저는 직장 다녀요 ㅎ",
          "original_form": "저는 직장 다녀요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100038732.1.5",
          "form": "저는 학교 다녀요",
          "original_form": "저는 학교 다녀요",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100038732.1.6",
          "form": "학생 때 수강신청 잘 하셨나요?",
          "original_form": "학생 때 수강신청 잘 하셨나요?",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100038732.1.7",
          "form": "저는 수강신청 어렵지 않게 했었어요 ㅎ 대부분 전공에 맞는거 들어가지공",
          "original_form": "저는 수강신청 어렵지 않게 했었어요 ㅎ 대부분 전공에 맞는거 들어가지공",
          "speaker_id": "1",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038732.1.8",
          "form": "친구들은 거의 티켓팅 수준으로 하던데",
          "original_form": "친구들은 거의 티켓팅 수준으로 하던데",
          "speaker_id": "1",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038732.1.9",
          "form": "오 특수전공이셨나봐요",
          "original_form": "오 특수전공이셨나봐요",
          "speaker_id": "2",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038732.1.10",
          "form": "맞아요!  특수전공이었어요",
          "original_form": "맞아요!  특수전공이었어요",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038732.1.11",
          "form": "그럼 동아리에 가입하셨던 적도 있으신가요?",
          "original_form": "그럼 동아리에 가입하셨던 적도 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100038732.1.12",
          "form": "아뇨ㅠ ㅠ그냥 학교 집 학교 집 만 다녔어요",
          "original_form": "아뇨ㅠ ㅠ그냥 학교 집 학교 집 만 다녔어요",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100038732.1.13",
          "form": "집순이라ㅠ",
          "original_form": "집순이라ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100038732.1.14",
          "form": "중 고딩땐 가입했었는데",
          "original_form": "중 고딩땐 가입했었는데",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100038732.1.15",
          "form": "대학땐 땡기는게 없더라고요",
          "original_form": "대학땐 땡기는게 없더라고요",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100038732.1.16",
          "form": "그럼 학업만족도는 높으셨나요?",
          "original_form": "그럼 학업만족도는 높으셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100038732.1.17",
          "form": "음....그냥 열심히 했어요ㅠ",
          "original_form": "음....그냥 열심히 했어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038732.1.18",
          "form": "좋았던건 아니고ㅠ",
          "original_form": "좋았던건 아니고ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038732.1.19",
          "form": "취업하려고요",
          "original_form": "취업하려고요",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038732.1.20",
          "form": "그러면 지금 전공과 관련된 일을 하고 계시나요?",
          "original_form": "그러면 지금 전공과 관련된 일을 하고 계시나요?",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100038732.1.21",
          "form": "네네 ㅎ",
          "original_form": "네네 ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100038732.1.22",
          "form": "전공 관련이에요 ㅎ때려치고싶지만요",
          "original_form": "전공 관련이에요 ㅎ때려치고싶지만요",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038732.1.23",
          "form": "뭐든지 일하는 건 어렵죠ㅠㅠ 학교 생활할 때도 그렀겠지만",
          "original_form": "뭐든지 일하는 건 어렵죠ㅠㅠ 학교 생활할 때도 그렀겠지만",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100038732.1.24",
          "form": "학교 다닐땐 그때의 문제땜에 힘들었어요",
          "original_form": "학교 다닐땐 그때의 문제땜에 힘들었어요",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038732.1.25",
          "form": "그냥 다 그때마다 힘든게 있는듯요",
          "original_form": "그냥 다 그때마다 힘든게 있는듯요",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038732.1.26",
          "form": "공감합니다",
          "original_form": "공감합니다",
