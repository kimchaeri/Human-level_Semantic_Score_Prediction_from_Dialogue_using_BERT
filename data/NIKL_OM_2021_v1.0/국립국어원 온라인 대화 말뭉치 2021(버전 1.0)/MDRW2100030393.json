      "utterance": [
        {
          "id": "MDRW2100030393.1.1",
          "form": "결혼하셧나요",
          "original_form": "결혼하셧나요",
          "speaker_id": "2",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030393.1.2",
          "form": "아니요 연애중입니다",
          "original_form": "아니요 연애중입니다",
          "speaker_id": "1",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030393.1.3",
          "form": "님은여",
          "original_form": "님은여",
          "speaker_id": "1",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030393.1.4",
          "form": "저도 안함",
          "original_form": "저도 안함",
          "speaker_id": "2",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030393.1.5",
          "form": "결혼은.. 무서움..",
          "original_form": "결혼은.. 무서움..",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030393.1.6",
          "form": "희생의 연속이라는..",
          "original_form": "희생의 연속이라는..",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030393.1.7",
          "form": "애인이랑 결혼은 안하기로 합의하셧나요?",
          "original_form": "애인이랑 결혼은 안하기로 합의하셧나요?",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030393.1.8",
          "form": "비밀입니다",
          "original_form": "비밀입니다",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030393.1.9",
          "form": "머리아픈얘기라.ㅈ",
          "original_form": "머리아픈얘기라.ㅈ",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030393.1.10",
          "form": "연애중인가요",
          "original_form": "연애중인가요",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030393.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저는 ㄴㄴ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저는 ㄴㄴ",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030393.1.12",
          "form": "좋은분 만나길 바랍니다",
          "original_form": "좋은분 만나길 바랍니다",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030393.1.13",
          "form": "감사합니다 ㅋㅋㅋㅋ",
          "original_form": "감사합니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030393.1.14",
          "form": "실례지만 연배가..",
          "original_form": "실례지만 연배가..",
          "speaker_id": "1",
          "time": "20210904 03:04"
        },
        {
          "id": "MDRW2100030393.1.15",
          "form": "전 32입니다",
          "original_form": "전 32입니다",
          "speaker_id": "1",
          "time": "20210904 03:04"
        },
        {
          "id": "MDRW2100030393.1.16",
          "form": "31입니다",
          "original_form": "31입니다",
          "speaker_id": "2",
          "time": "20210904 09:27"
        },
        {
          "id": "MDRW2100030393.1.17",
          "form": "비슷하네요",
          "original_form": "비슷하네요",
          "speaker_id": "1",
          "time": "20210904 09:27"
        },
        {
          "id": "MDRW2100030393.1.18",
          "form": "푹잤습니까",
          "original_form": "푹잤습니까",
          "speaker_id": "1",
          "time": "20210904 09:27"
        },
        {
          "id": "MDRW2100030393.1.19",
          "form": "주말의 시작입니다",
          "original_form": "주말의 시작입니다",
          "speaker_id": "1",
          "time": "20210904 09:27"
        },
        {
          "id": "MDRW2100030393.1.20",
          "form": "아니요 잠을 좀 설쳤네요",
          "original_form": "아니요 잠을 좀 설쳤네요",
          "speaker_id": "2",
          "time": "20210904 09:28"
        },
        {
          "id": "MDRW2100030393.1.21",
          "form": "꿈에 사랑하는 그가 나왔습니까",
          "original_form": "꿈에 사랑하는 그가 나왔습니까",
          "speaker_id": "1",
          "time": "20210904 09:29"
        },
        {
          "id": "MDRW2100030393.1.22",
          "form": "아니요 ㅋㅋㅋㅋ",
          "original_form": "아니요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100030393.1.23",
          "form": "낮에 커피를 많이 마셧더니ㅋㅋㅋ",
          "original_form": "낮에 커피를 많이 마셧더니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 09:45"
        },
        {
          "id": "MDRW2100030393.1.24",
          "form": "전 카페인 중독이라 지금도 한잔 마시고있습니다",
