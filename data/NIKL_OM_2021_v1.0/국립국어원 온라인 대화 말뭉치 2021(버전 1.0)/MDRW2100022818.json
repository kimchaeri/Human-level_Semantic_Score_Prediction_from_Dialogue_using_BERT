      "utterance": [
        {
          "id": "MDRW2100022818.1.1",
          "form": "저녁 뭐 먹니",
          "original_form": "저녁 뭐 먹니",
          "speaker_id": "1",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022818.1.2",
          "form": "요즘 밥 먹기 귀찮아 너무너무",
          "original_form": "요즘 밥 먹기 귀찮아 너무너무",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022818.1.3",
          "form": "그래서 진짜 대충먹음",
          "original_form": "그래서 진짜 대충먹음",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022818.1.4",
          "form": "대충 뭐해서ㅋㅋㅋㅋ",
          "original_form": "대충 뭐해서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022818.1.5",
          "form": "그냥 토스트도 먹고 비빔면도 먹고",
          "original_form": "그냥 토스트도 먹고 비빔면도 먹고",
          "speaker_id": "2",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022818.1.6",
          "form": "뭐라도 만드는거 너무 귀찮",
          "original_form": "뭐라도 만드는거 너무 귀찮",
          "speaker_id": "2",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022818.1.7",
          "form": "비빔면 좋네ㅋㅋ",
          "original_form": "비빔면 좋네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022818.1.8",
          "form": "나도 그냥 집에 있는거 대충 먹는다ㅋㅋ",
          "original_form": "나도 그냥 집에 있는거 대충 먹는다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022818.1.9",
          "form": "장본지도 오래 ㅋㅋ",
          "original_form": "장본지도 오래 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022818.1.10",
          "form": "집에 그냥 기본 찬도 거의 없음 ㅋㅋ",
          "original_form": "집에 그냥 기본 찬도 거의 없음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022818.1.11",
          "form": "내일 마트라도 가라ㅋㅋㅋ",
          "original_form": "내일 마트라도 가라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022818.1.12",
          "form": "먹는 즐거움이 을매나 큰디ㅋㅋ",
          "original_form": "먹는 즐거움이 을매나 큰디ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022818.1.13",
          "form": "홈플 배송 시킬라고 마트 가기 귀찮",
          "original_form": "홈플 배송 시킬라고 마트 가기 귀찮",
          "speaker_id": "2",
          "time": "20210829 18:27"
        },
        {
          "id": "MDRW2100022818.1.14",
          "form": "장바구니에 뭐 담아놨농ㅋㅋ",
          "original_form": "장바구니에 뭐 담아놨농ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100022818.1.15",
          "form": "삼겹살 짜장라면.. 또 뭐있드라...",
          "original_form": "삼겹살 짜장라면.. 또 뭐있드라...",
          "speaker_id": "2",
          "time": "20210829 18:30"
        },
        {
          "id": "MDRW2100022818.1.16",
          "form": "오 삼겹살ㅋㅋ 김치랑 같이 구워먹고싶네ㅋ",
          "original_form": "오 삼겹살ㅋㅋ 김치랑 같이 구워먹고싶네ㅋ",
          "speaker_id": "1",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100022818.1.17",
          "form": "김냉에서 꺼내오기도 귀찮 ㅋㅋㅋㅋ",
          "original_form": "김냉에서 꺼내오기도 귀찮 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:33"
        },
        {
          "id": "MDRW2100022818.1.18",
          "form": "숨도 귀찮아서 안 쉬는거 아니제ㅋㅋㅋ",
          "original_form": "숨도 귀찮아서 안 쉬는거 아니제ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022818.1.19",
          "form": "백신 부작용 무기력증이라고 우겨본다 ㅋㅋㅋㅋㅋ",
          "original_form": "백신 부작용 무기력증이라고 우겨본다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
