        {
          "id": "MDRW2100030358.1.1",
          "form": "미대는 수업이 주로 어떤식으로 이루어지나요?",
          "original_form": "미대는 수업이 주로 어떤식으로 이루어지나요?",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030358.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030358.1.3",
          "form": "대면수업이고 그냥 작업해요",
          "original_form": "대면수업이고 그냥 작업해요",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030358.1.4",
          "form": "교수님들이 풀어놓으시는? 편이에요ㅋㅋ",
          "original_form": "교수님들이 풀어놓으시는? 편이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030358.1.5",
          "form": "아 그래도 대면하는군요",
          "original_form": "아 그래도 대면하는군요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030358.1.6",
          "form": "실습이 주로라서 그런가봐요",
          "original_form": "실습이 주로라서 그런가봐요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030358.1.7",
          "form": "아무래도 그렇죠ㅜㅜ",
          "original_form": "아무래도 그렇죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100030358.1.8",
          "form": "재밌을거같아요 ㅋㅋㅋ",
          "original_form": "재밌을거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100030358.1.9",
          "form": "학교생활 안해본지 4년 지나서..",
          "original_form": "학교생활 안해본지 4년 지나서..",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100030358.1.10",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100030358.1.11",
          "form": "벌써 곧 4학년이라니 안믿기네여",
          "original_form": "벌써 곧 4학년이라니 안믿기네여",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100030358.1.12",
          "form": "졸업하셨나요??",
          "original_form": "졸업하셨나요??",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100030358.1.13",
          "form": "이제 곧 졸업반이죠 ㅋㅋㅋ",
          "original_form": "이제 곧 졸업반이죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100030358.1.14",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100030358.1.15",
          "form": "멋있어요!!",
          "original_form": "멋있어요!!",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100030358.1.16",
