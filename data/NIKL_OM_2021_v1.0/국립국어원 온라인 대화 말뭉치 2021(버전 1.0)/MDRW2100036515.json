      "utterance": [
        {
          "id": "MDRW2100036515.1.1",
          "form": "그게 뭐야",
          "original_form": "그게 뭐야",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100036515.1.2",
          "form": "구글은 참 많은걸 하는구나",
          "original_form": "구글은 참 많은걸 하는구나",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.3",
          "form": "그것도 시간관리애 좋은 시계같더라",
          "original_form": "그것도 시간관리애 좋은 시계같더라",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.4",
          "form": "근데 예쁘게 생겼어",
          "original_form": "근데 예쁘게 생겼어",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.5",
          "form": "구글에서 만든 제품일까?",
          "original_form": "구글에서 만든 제품일까?",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.6",
          "form": "근데 엄청 비싸",
          "original_form": "근데 엄청 비싸",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.7",
          "form": "감성값같아",
          "original_form": "감성값같아",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.8",
          "form": "흠 어던 제품인지 예상이안가",
          "original_form": "흠 어던 제품인지 예상이안가",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.9",
          "form": "얼마인데??",
          "original_form": "얼마인데??",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.10",
          "form": "몇만원씩하더라",
          "original_form": "몇만원씩하더라",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.11",
          "form": "그냥 시계를 도로록 돌려거 쓰는거야",
          "original_form": "그냥 시계를 도로록 돌려거 쓰는거야",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.12",
          "form": "한 사오만원 하는가?",
          "original_form": "한 사오만원 하는가?",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.13",
          "form": "스탑워치같기도해",
          "original_form": "스탑워치같기도해",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.14",
          "form": "차라리 회중시계 큰걸사서",
          "original_form": "차라리 회중시계 큰걸사서",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.15",
          "form": "응 그거보다 비쌌던거같기도스하고",
          "original_form": "응 그거보다 비쌌던거같기도스하고",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.16",
          "form": "눈앞에 두고 공부하는건 어때",
          "original_form": "눈앞에 두고 공부하는건 어때",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.17",
          "form": "요즘 사람들에겐 갬성이 중요하잖아",
          "original_form": "요즘 사람들에겐 갬성이 중요하잖아",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.18",
          "form": "걔가 나에게 최면을 걸묜 어떡해",
          "original_form": "걔가 나에게 최면을 걸묜 어떡해",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.19",
          "form": "정각마다 종이 울리면 좀 조급해질꺼야",
          "original_form": "정각마다 종이 울리면 좀 조급해질꺼야",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.20",
          "form": "그건 견뎌.",
          "original_form": "그건 견뎌.",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.21",
          "form": "하지만 난 어플로 해결하기로 했어",
          "original_form": "하지만 난 어플로 해결하기로 했어",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036515.1.22",
          "form": "충고 안고마워",
          "original_form": "충고 안고마워",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100036515.1.23",
          "form": "아아 말실수야",
          "original_form": "아아 말실수야",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100036515.1.24",
          "form": "맞아 스마트폰을 놀리면 안되지",
          "original_form": "맞아 스마트폰을 놀리면 안되지",
          "speaker_id": "2",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100036515.1.25",
          "form": "고맙지만 안고마워",
          "original_form": "고맙지만 안고마워",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100036515.1.26",
          "form": "앗 그래!",
          "original_form": "앗 그래!",
          "speaker_id": "2",
          "time": "20210831 00:00"
        }
      ]
    }
  ]
