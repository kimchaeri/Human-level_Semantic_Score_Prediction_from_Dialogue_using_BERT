        {
          "id": "MDRW2100017437.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017437.1.2",
          "form": "안녕하세요 네넵 저녁 덮밥으로 먹었습니다",
          "original_form": "안녕하세요 네넵 저녁 덮밥으로 먹었습니다",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017437.1.3",
          "form": "name1님은 저녁 뭐드셨나요?",
          "original_form": "&name1&님은 저녁 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017437.1.4",
          "form": "저는 닭갈비볶음밥이요",
          "original_form": "저는 닭갈비볶음밥이요",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017437.1.5",
          "form": "무슨 덮밥 드셨나요?",
          "original_form": "무슨 덮밥 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017437.1.6",
          "form": "육.회.",
          "original_form": "육.회.",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017437.1.7",
          "form": "아 맛있는거 드셨다",
          "original_form": "아 맛있는거 드셨다",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017437.1.8",
          "form": "직접 하셨나요",
          "original_form": "직접 하셨나요",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017437.1.9",
          "form": "아뇨 유가네 닭갈비에서 시켜먹었어요",
          "original_form": "아뇨 유가네 닭갈비에서 시켜먹었어요",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017437.1.10",
          "form": "wow",
          "original_form": "wow",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017437.1.11",
          "form": "자본의 맛이었군요",
          "original_form": "자본의 맛이었군요",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017437.1.12",
          "form": "유가네 맛있나요?!!",
          "original_form": "유가네 맛있나요?!!",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017437.1.13",
          "form": "맛있어요!! JMT",
          "original_form": "맛있어요!! JMT",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017437.1.14",
          "form": "닭갈비보다 볶음밥이 찐이에요",
          "original_form": "닭갈비보다 볶음밥이 찐이에요",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017437.1.15",
          "form": "오케이",
          "original_form": "오케이",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017437.1.16",
          "form": "name1님 찐을 드셨군요 오늘",
          "original_form": "&name1&님 찐을 드셨군요 오늘",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017437.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ육회덮밥은 직접 하신건가요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ육회덮밥은 직접 하신건가요??",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017437.1.18",
          "form": "아뇨 저도 자본의 맛",
          "original_form": "아뇨 저도 자본의 맛",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017437.1.19",
          "form": "뭔..무슨 궁 비빔밥 집에서 시켰는데",
          "original_form": "뭔..무슨 궁 비빔밥 집에서 시켰는데",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017437.1.20",
          "form": "이틀째 같은거거든요",
          "original_form": "이틀째 같은거거든요",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017437.1.21",
          "form": "JMT라는 뜻",
          "original_form": "JMT라는 뜻",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017437.1.22",
          "form": "오 이틀연속이면 정말 JMT인가봐요",
          "original_form": "오 이틀연속이면 정말 JMT인가봐요",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017437.1.23",
          "form": "먹여주고싶어요",
          "original_form": "먹여주고싶어요",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017437.1.24",
          "form": "정말..",
          "original_form": "정말..",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017437.1.25",
          "form": "남들도 다 알았으믄 좋겠다..",
          "original_form": "남들도 다 알았으믄 좋겠다..",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017437.1.26",
          "form": "소스가 고추장에 뭘 섞었는데",
          "original_form": "소스가 고추장에 뭘 섞었는데",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017437.1.27",
          "form": "비법 안다면 며느리가 되고싶을 정도예요",
          "original_form": "비법 안다면 며느리가 되고싶을 정도예요",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017437.1.28",
          "form": "그정돈가요?",
          "original_form": "그정돈가요?",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017437.1.29",
          "form": "정확한 가게이름이 필요합니다",
          "original_form": "정확한 가게이름이 필요합니다",
          "speaker_id": "1",
