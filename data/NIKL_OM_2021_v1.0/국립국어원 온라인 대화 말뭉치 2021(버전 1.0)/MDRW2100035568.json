      "utterance": [
        {
          "id": "MDRW2100035568.1.1",
          "form": "요즘엔 단호박이요!",
          "original_form": "요즘엔 단호박이요!",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035568.1.2",
          "form": "맛있더라구요~",
          "original_form": "맛있더라구요~",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035568.1.3",
          "form": "아 단호박 맛있죠 ㅜㅜ 밤호박도 드시나요?",
          "original_form": "아 단호박 맛있죠 ㅜㅜ 밤호박도 드시나요?",
          "speaker_id": "1",
          "time": "20210829 20:46"
        },
        {
          "id": "MDRW2100035568.1.4",
          "form": "앗 밤호박은 모르겠네요ㅠ",
          "original_form": "앗 밤호박은 모르겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100035568.1.5",
          "form": "단호박이랑 다른건가요?",
          "original_form": "단호박이랑 다른건가요?",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100035568.1.6",
          "form": "작은 사이즈의 단호박이에요!",
          "original_form": "작은 사이즈의 단호박이에요!",
          "speaker_id": "1",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100035568.1.7",
          "form": "당도가 더 좋아요 ㅎㅎㅎ",
          "original_form": "당도가 더 좋아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100035568.1.8",
          "form": "오.. 신기하네용ㅋㅋ",
          "original_form": "오.. 신기하네용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035568.1.9",
          "form": "주먹정도일까나요?",
          "original_form": "주먹정도일까나요?",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035568.1.10",
          "form": "네 딱 주먹사이즈에요 ㅎㅎ",
          "original_form": "네 딱 주먹사이즈에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100035568.1.11",
          "form": "하나 먹으면 딱 좋아요 ! 근데 단호박에 비해 좀 비싸긴해요ㅎㅎ",
          "original_form": "하나 먹으면 딱 좋아요 ! 근데 단호박에 비해 좀 비싸긴해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035568.1.12",
          "form": "오.. 담에 장볼때 눈여겨봐야겧어요ㅋㅋ",
          "original_form": "오.. 담에 장볼때 눈여겨봐야겧어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035568.1.13",
          "form": "네네 꼭 맛있는걸 선택하시길 ㅎㅎ 바랍니다",
          "original_form": "네네 꼭 맛있는걸 선택하시길 ㅎㅎ 바랍니다",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035568.1.14",
          "form": "고구마는 안좋아하세요?",
          "original_form": "고구마는 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035568.1.15",
          "form": "고구마 좋아해요!",
          "original_form": "고구마 좋아해요!",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035568.1.16",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035568.1.17",
          "form": "네 저도 고구마 좋아해요 !!",
          "original_form": "네 저도 고구마 좋아해요 !!",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035568.1.18",
          "form": "어떤고구마 좋아하세요?",
          "original_form": "어떤고구마 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035568.1.19",
          "form": "전 심만 많지않으면 ㅎㅎ 고구마는 안가리지만",
          "original_form": "전 심만 많지않으면 ㅎㅎ 고구마는 안가리지만",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100035568.1.20",
          "form": "밤고구마 좋아해요!",
          "original_form": "밤고구마 좋아해요!",
