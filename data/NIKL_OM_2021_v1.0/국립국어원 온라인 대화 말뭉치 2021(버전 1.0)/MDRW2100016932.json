      "utterance": [
        {
          "id": "MDRW2100016932.1.1",
          "form": "요새는 다들 배곧이에요 이쪽은",
          "original_form": "요새는 다들 배곧이에요 이쪽은",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016932.1.2",
          "form": "회나 조개구이 이런거도 걍 동네에서 드시는게",
          "original_form": "회나 조개구이 이런거도 걍 동네에서 드시는게",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016932.1.3",
          "form": "이득",
          "original_form": "이득",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016932.1.4",
          "form": "가려면 아예 대부도 제부도로 나가야ㅋㅋㅋ",
          "original_form": "가려면 아예 대부도 제부도로 나가야ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016932.1.5",
          "form": "아 날거 좋아하신댔지",
          "original_form": "아 날거 좋아하신댔지",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016932.1.6",
          "form": "서해는 이제안갑니다~~ㅋㅋㅋ무조건 남해동해로갑니다",
          "original_form": "서해는 이제안갑니다~~ㅋㅋㅋ무조건 남해동해로갑니다",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016932.1.7",
          "form": "맑은물이좋아요",
          "original_form": "맑은물이좋아요",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016932.1.8",
          "form": "ㅋㅋㅋㅋㅋ인정",
          "original_form": "ㅋㅋㅋㅋㅋ인정",
          "speaker_id": "2",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016932.1.9",
          "form": "뭐하러 고생해서 서쪽을..",
          "original_form": "뭐하러 고생해서 서쪽을..",
          "speaker_id": "2",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016932.1.10",
          "form": "저 내일 반차인데 이번주 반차쓰는거 설레서 일 1개도안했어요",
          "original_form": "저 내일 반차인데 이번주 반차쓰는거 설레서 일 1개도안했어요",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016932.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016932.1.12",
          "form": "업무일지 뭐라쓰냐 할말없네",
          "original_form": "업무일지 뭐라쓰냐 할말없네",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016932.1.13",
          "form": "지어내는거야 뭐 제 전문입니다",
          "original_form": "지어내는거야 뭐 제 전문입니다",
          "speaker_id": "1",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016932.1.14",
          "form": "이번주에 서울가는데 회먹고올거에요",
          "original_form": "이번주에 서울가는데 회먹고올거에요",
          "speaker_id": "1",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100016932.1.15",
          "form": "회 회 하니까 숭어회 먹고싶네요",
          "original_form": "회 회 하니까 숭어회 먹고싶네요",
          "speaker_id": "2",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100016932.1.16",
          "form": "회중에 젤 좋아하는회",
          "original_form": "회중에 젤 좋아하는회",
          "speaker_id": "2",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100016932.1.17",
          "form": "그게 연어처럼 빨간살인가요?",
          "original_form": "그게 연어처럼 빨간살인가요?",
          "speaker_id": "1",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100016932.1.18",
          "form": "아뇨 고건 송어",
          "original_form": "아뇨 고건 송어",
          "speaker_id": "2",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016932.1.19",
          "form": "님 회 좋아하는거 맞죠?",
          "original_form": "님 회 좋아하는거 맞죠?",
          "speaker_id": "2",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016932.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016932.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016932.1.22",
          "form": "회라면 뭐든먹어서 뭔지도모르고먹어요",
          "original_form": "회라면 뭐든먹어서 뭔지도모르고먹어요",
          "speaker_id": "1",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016932.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016932.1.24",
          "form": "와 어케그러지",
          "original_form": "와 어케그러지",
          "speaker_id": "2",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016932.1.25",
          "form": "먹으면 이게 뭐여 이러지 않아요?",
          "original_form": "먹으면 이게 뭐여 이러지 않아요?",
          "speaker_id": "2",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016932.1.26",
          "form": "물어볼새없이 먹기바쁜데 뭣하러물어요",
          "original_form": "물어볼새없이 먹기바쁜데 뭣하러물어요",
          "speaker_id": "1",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016932.1.27",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "2",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100016932.1.28",
          "form": "저는 간장파입니다",
          "original_form": "저는 간장파입니다",
          "speaker_id": "1",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100016932.1.29",
          "form": "와사비 노노",
          "original_form": "와사비 노노",
          "speaker_id": "1",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100016932.1.30",
          "form": "문세윤 선생님 말씀이 떠오르네요",
          "original_form": "문세윤 선생님 말씀이 떠오르네요",
          "speaker_id": "2",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100016932.1.31",
          "form": "부먹찍먹 생각할 시간에 하나라도 더 먹으라던",
          "original_form": "부먹찍먹 생각할 시간에 하나라도 더 먹으라던",
          "speaker_id": "2",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100016932.1.32",
          "form": "아 어디 송어회 잘파는곳잇어서 거기자주가는데",
          "original_form": "아 어디 송어회 잘파는곳잇어서 거기자주가는데",
          "speaker_id": "1",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100016932.1.33",
          "form": "대청호근처인가",
          "original_form": "대청호근처인가",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016932.1.34",
          "form": "민물 바다 안가리시는구나",
          "original_form": "민물 바다 안가리시는구나",
          "speaker_id": "2",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016932.1.35",
          "form": "송어 흙내나지않아요??",
          "original_form": "송어 흙내나지않아요??",
          "speaker_id": "2",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016932.1.36",
          "form": "안나요",
          "original_form": "안나요",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016932.1.37",
          "form": "예????",
          "original_form": "예????",
          "speaker_id": "2",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016932.1.38",
          "form": "흙내라니,, 이해못하겟네요",
          "original_form": "흙내라니,, 이해못하겟네요",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016932.1.39",
          "form": "미각을 잃으신건 아니죠?",
          "original_form": "미각을 잃으신건 아니죠?",
          "speaker_id": "2",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016932.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋ아니그거보통 야채랑 같이 샐러드처럼 먹든데요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니그거보통 야채랑 같이 샐러드처럼 먹든데요?",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016932.1.41",
          "form": "풍미니까 코가 막히신건가",
          "original_form": "풍미니까 코가 막히신건가",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016932.1.42",
          "form": "안나던데 연어랑 맛 비슷하던데요?",
          "original_form": "안나던데 연어랑 맛 비슷하던데요?",
          "speaker_id": "1",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016932.1.43",
          "form": "그 야채에 초고추장 범벅해서?",
          "original_form": "그 야채에 초고추장 범벅해서?",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016932.1.44",
          "form": "아 진짜 재밌는 분이시네 ^^",
          "original_form": "아 진짜 재밌는 분이시네 ^^",
          "speaker_id": "1",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016932.1.45",
          "form": "네 근데저는 간장범벅해요",
          "original_form": "네 근데저는 간장범벅해요",
          "speaker_id": "1",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016932.1.46",
          "form": "죄송합니다 째꼼 친해졌다고 무리수를",
          "original_form": "죄송합니다 째꼼 친해졌다고 무리수를",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016932.1.47",
          "form": "간장을 비벼드시다니ㄷㄷㄷ",
          "original_form": "간장을 비벼드시다니ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016932.1.48",
          "form": "연어의 느낌이긴한데",
          "original_form": "연어의 느낌이긴한데",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016932.1.49",
          "form": "재밌고 좋아요 ^^",
          "original_form": "재밌고 좋아요 ^^",
          "speaker_id": "1",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016932.1.50",
          "form": "뭐라그럴까",
          "original_form": "뭐라그럴까",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016932.1.51",
          "form": "기름이 쫌 덜하고 참치느낌",
          "original_form": "기름이 쫌 덜하고 참치느낌",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016932.1.52",
          "form": "다 채소에 비벼드셨어요?",
          "original_form": "다 채소에 비벼드셨어요?",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016932.1.53",
          "form": "회얘기하니까 또 회먹고싶네요 ㅠ",
          "original_form": "회얘기하니까 또 회먹고싶네요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016932.1.54",
          "form": "하나 딱 넣으면 흙내나는데",
          "original_form": "하나 딱 넣으면 흙내나는데",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016932.1.55",
          "form": "아뇨 저는 비벼먹는건 거의없고",
          "original_form": "아뇨 저는 비벼먹는건 거의없고",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016932.1.56",
          "form": "날거 간장찍어먹어요",
          "original_form": "날거 간장찍어먹어요",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016932.1.57",
          "form": "와 근데 모르시면",
          "original_form": "와 근데 모르시면",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016932.1.58",
          "form": "송어 맛집인가 뭐지",
          "original_form": "송어 맛집인가 뭐지",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016932.1.59",
          "form": "버근가",
          "original_form": "버근가",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016932.1.60",
          "form": "거기진짜맛집인데",
          "original_form": "거기진짜맛집인데",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016932.1.61",
          "form": "어딘데요?",
          "original_form": "어딘데요?",
