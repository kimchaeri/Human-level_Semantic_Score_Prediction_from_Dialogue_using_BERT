      "utterance": [
        {
          "id": "MDRW2100047212.1.1",
          "form": "자기전에 스트레칭을 하라고함",
          "original_form": "자기전에 스트레칭을 하라고함",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100047212.1.2",
          "form": "그건 꾸준히하는데 1번이 안돼겠어",
          "original_form": "그건 꾸준히하는데 1번이 안돼겠어",
          "speaker_id": "2",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100047212.1.3",
          "form": "아니 뒹굴뒹굴 하다가 자는데",
          "original_form": "아니 뒹굴뒹굴 하다가 자는데",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100047212.1.4",
          "form": "폰보다보면 ㅋㅋ",
          "original_form": "폰보다보면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100047212.1.5",
          "form": "스트레칭 안함ㅋㅋ",
          "original_form": "스트레칭 안함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100047212.1.6",
          "form": "젊은 사람도 맞는구나",
          "original_form": "젊은 사람도 맞는구나",
          "speaker_id": "1",
          "time": "20210826 09:30"
        },
        {
          "id": "MDRW2100047212.1.7",
          "form": "공연은 솔직히 안본지 10년 넘었음",
          "original_form": "공연은 솔직히 안본지 10년 넘었음",
          "speaker_id": "1",
          "time": "20210826 09:33"
        },
        {
          "id": "MDRW2100047212.1.8",
          "form": "부산에 좋은공연많든데",
          "original_form": "부산에 좋은공연많든데",
          "speaker_id": "2",
          "time": "20210826 09:36"
        },
        {
          "id": "MDRW2100047212.1.9",
          "form": "어떤 공연?",
          "original_form": "어떤 공연?",
          "speaker_id": "1",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100047212.1.10",
          "form": "눈치껏 열리는듯",
          "original_form": "눈치껏 열리는듯",
          "speaker_id": "2",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100047212.1.11",
          "form": "최근 뭐 본거 있어요?",
          "original_form": "최근 뭐 본거 있어요?",
          "speaker_id": "1",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100047212.1.12",
          "form": "창가인가 전에 봤다고한듭",
          "original_form": "창가인가 전에 봤다고한듭",
          "speaker_id": "1",
          "time": "20210826 09:58"
        },
        {
          "id": "MDRW2100047212.1.13",
          "form": "무료표가 생겨서 피카소전 보러간듯",
          "original_form": "무료표가 생겨서 피카소전 보러간듯",
          "speaker_id": "2",
          "time": "20210826 09:59"
        },
        {
          "id": "MDRW2100047212.1.14",
          "form": "뮤지컬이 아니라",
          "original_form": "뮤지컬이 아니라",
          "speaker_id": "1",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100047212.1.15",
          "form": "전시회는 사실 괜찮을듯?",
          "original_form": "전시회는 사실 괜찮을듯?",
          "speaker_id": "1",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100047212.1.16",
          "form": "생각해보니",
          "original_form": "생각해보니",
          "speaker_id": "1",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100047212.1.17",
          "form": "아 전시회는",
          "original_form": "아 전시회는",
          "speaker_id": "2",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100047212.1.18",
          "form": "안에 사람 제한두더라",
          "original_form": "안에 사람 제한두더라",
          "speaker_id": "2",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100047212.1.19",
          "form": "들어가고 한명 나오면 들어가고 그러는듯",
          "original_form": "들어가고 한명 나오면 들어가고 그러는듯",
          "speaker_id": "2",
          "time": "20210826 10:01"
        },
        {
          "id": "MDRW2100047212.1.20",
          "form": "거리두기도 철저히하고?",
          "original_form": "거리두기도 철저히하고?",
          "speaker_id": "1",
          "time": "20210826 10:04"
        },
        {
          "id": "MDRW2100047212.1.21",
          "form": "그런거면괜찮을듯",
          "original_form": "그런거면괜찮을듯",
          "speaker_id": "1",
          "time": "20210826 10:05"
        },
        {
          "id": "MDRW2100047212.1.22",
          "form": "근데 일행이랑가면?",
          "original_form": "근데 일행이랑가면?",
          "speaker_id": "1",
          "time": "20210826 10:05"
        },
        {
          "id": "MDRW2100047212.1.23",
          "form": "그럼 앞에 일행만큼 나와야햄",
          "original_form": "그럼 앞에 일행만큼 나와야햄",
          "speaker_id": "2",
          "time": "20210826 10:07"
        },
        {
          "id": "MDRW2100047212.1.24",
          "form": "아하 다행히 일행을 떼놓지는 않군요",
          "original_form": "아하 다행히 일행을 떼놓지는 않군요",
          "speaker_id": "1",
          "time": "20210826 10:08"
        },
        {
          "id": "MDRW2100047212.1.25",
          "form": "간만의 대형전시니까 사람 넘 많았음ㅋㅋㅋ",
          "original_form": "간만의 대형전시니까 사람 넘 많았음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:08"
        }
      ]
    }
