      "utterance": [
        {
          "id": "MDRW2100018027.1.1",
          "form": "데미안 읽어봤어?",
          "original_form": "데미안 읽어봤어?",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100018027.1.2",
          "form": "읽어봤지만  너무오래됐지",
          "original_form": "읽어봤지만  너무오래됐지",
          "speaker_id": "2",
          "time": "20210827 07:05"
        },
        {
          "id": "MDRW2100018027.1.3",
          "form": "고등학교때 읽은거같은데",
          "original_form": "고등학교때 읽은거같은데",
          "speaker_id": "2",
          "time": "20210827 07:05"
        },
        {
          "id": "MDRW2100018027.1.4",
          "form": "나는 데미안 처음 읽었을 때, 많은 충격을 받았어",
          "original_form": "나는 데미안 처음 읽었을 때, 많은 충격을 받았어",
          "speaker_id": "1",
          "time": "20210827 08:24"
        },
        {
          "id": "MDRW2100018027.1.5",
          "form": "감명을 받았어",
          "original_form": "감명을 받았어",
          "speaker_id": "1",
          "time": "20210827 08:28"
        },
        {
          "id": "MDRW2100018027.1.6",
          "form": "최근에 부의 인문학 읽었어",
          "original_form": "최근에 부의 인문학 읽었어",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018027.1.7",
          "form": "오늘 일지하이솔루스 환불",
          "original_form": "오늘 일지하이솔루스 환불",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018027.1.8",
          "form": "몇개받았어?",
          "original_form": "몇개받았어?",
          "speaker_id": "2",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100018027.1.9",
          "form": "나는 주식대신 달러를 산다",
          "original_form": "나는 주식대신 달러를 산다",
          "speaker_id": "2",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100018027.1.10",
          "form": "이책이 아주 도움이 많이되고있어",
          "original_form": "이책이 아주 도움이 많이되고있어",
          "speaker_id": "2",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100018027.1.11",
          "form": "그렇고만",
          "original_form": "그렇고만",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018027.1.12",
          "form": "달러 좋지",
          "original_form": "달러 좋지",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018027.1.13",
          "form": "name2",
          "original_form": "&name2&",
          "speaker_id": "1",
          "time": "20210830 08:32"
        },
        {
          "id": "MDRW2100018027.1.14",
          "form": "출근했숴?",
          "original_form": "출근했숴?",
          "speaker_id": "1",
          "time": "20210830 08:32"
        },
        {
          "id": "MDRW2100018027.1.15",
          "form": "엉  출근했엉 ㅎ",
          "original_form": "엉  출근했엉 ㅎ",
          "speaker_id": "2",
          "time": "20210830 08:41"
        },
        {
          "id": "MDRW2100018027.1.16",
          "form": "재태크 책 많이 봤어?",
          "original_form": "재태크 책 많이 봤어?",
          "speaker_id": "1",
          "time": "20210830 08:42"
        },
        {
          "id": "MDRW2100018027.1.17",
          "form": "아니 재테크 말고 다른책 봤엉",
          "original_form": "아니 재테크 말고 다른책 봤엉",
          "speaker_id": "2",
          "time": "20210830 08:51"
        },
        {
          "id": "MDRW2100018027.1.18",
          "form": "무슨책?",
          "original_form": "무슨책?",
          "speaker_id": "1",
          "time": "20210830 08:51"
        },
        {
          "id": "MDRW2100018027.1.19",
          "form": "아이진 수익률 어때?",
          "original_form": "아이진 수익률 어때?",
          "speaker_id": "1",
          "time": "20210830 09:05"
        },
        {
          "id": "MDRW2100018027.1.20",
          "form": "나는 뭐 2만 중반에 샀으니까 항상 이득이지",
          "original_form": "나는 뭐 2만 중반에 샀으니까 항상 이득이지",
          "speaker_id": "2",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100018027.1.21",
          "form": "직이네",
          "original_form": "직이네",
          "speaker_id": "1",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100018027.1.22",
          "form": "평단가 2만중반이야?",
          "original_form": "평단가 2만중반이야?",
          "speaker_id": "1",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100018027.1.23",
          "form": "어  지금은 계속 4만~4만5천  사이에 왔다갔다 하면서 개미털기 하고있어",
          "original_form": "어  지금은 계속 4만~4만5천  사이에 왔다갔다 하면서 개미털기 하고있어",
          "speaker_id": "2",
          "time": "20210830 09:29"
        },
        {
          "id": "MDRW2100018027.1.24",
          "form": "20주까지 늘렸엉",
          "original_form": "20주까지 늘렸엉",
          "speaker_id": "2",
          "time": "20210830 09:30"
        },
        {
          "id": "MDRW2100018027.1.25",
          "form": "목표주가가 얼마야?",
          "original_form": "목표주가가 얼마야?",
          "speaker_id": "1",
          "time": "20210830 09:35"
        },
        {
          "id": "MDRW2100018027.1.26",
          "form": "임상승인나서 8가면 털거야",
          "original_form": "임상승인나서 8가면 털거야",
          "speaker_id": "2",
          "time": "20210830 10:04"
        },
        {
          "id": "MDRW2100018027.1.27",
          "form": "굳",
          "original_form": "굳",
          "speaker_id": "1",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100018027.1.28",
          "form": "대박나라",
          "original_form": "대박나라",
          "speaker_id": "1",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100018027.1.29",
          "form": "너는 요새 무슨 책 읽고있엉?",
          "original_form": "너는 요새 무슨 책 읽고있엉?",
          "speaker_id": "2",
          "time": "20210830 11:22"
        },
        {
          "id": "MDRW2100018027.1.30",
          "form": "나는 책 안읽어",
          "original_form": "나는 책 안읽어",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100018027.1.31",
          "form": "요즘 읽은게 없어",
          "original_form": "요즘 읽은게 없어",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100018027.1.32",
          "form": "아빠준비하느라바쁘구나....ㅋㅋ",
          "original_form": "아빠준비하느라바쁘구나....ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100018027.1.33",
          "form": "나는 아북리더기하나사려고",
          "original_form": "나는 아북리더기하나사려고",
          "speaker_id": "2",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100018027.1.34",
          "form": "아북리더기가 뭐야?",
          "original_form": "아북리더기가 뭐야?",
          "speaker_id": "1",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100018027.1.35",
          "form": "전자책?",
          "original_form": "전자책?",
          "speaker_id": "1",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100018027.1.36",
          "form": "ㅇㅇ이북리더기 ㅋ",
          "original_form": "ㅇㅇ이북리더기 ㅋ",
          "speaker_id": "2",
          "time": "20210830 13:29"
        }
      ]
