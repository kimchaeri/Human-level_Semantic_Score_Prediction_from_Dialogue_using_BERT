      "utterance": [
        {
          "id": "MDRW2100009334.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009334.1.2",
          "form": "지금은 안 키워요",
          "original_form": "지금은 안 키워요",
          "speaker_id": "2",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009334.1.3",
          "form": "원래는 머 키웠나여?",
          "original_form": "원래는 머 키웠나여?",
          "speaker_id": "1",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100009334.1.4",
          "form": "햄스터를 키웠어요!",
          "original_form": "햄스터를 키웠어요!",
          "speaker_id": "2",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009334.1.5",
          "form": "어렵지 않아요?",
          "original_form": "어렵지 않아요?",
          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009334.1.6",
          "form": "어릴 때라 부모님이 거진 다 키우셨어요ㅋㅋ",
          "original_form": "어릴 때라 부모님이 거진 다 키우셨어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009334.1.7",
          "form": "아 그런가요",
          "original_form": "아 그런가요",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009334.1.8",
          "form": "아마 부모님이 힘드셨을 것 같아요ㅋㅋ",
          "original_form": "아마 부모님이 힘드셨을 것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100009334.1.9",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009334.1.10",
          "form": "이제는 생명에 대한 책임을 제가 져야 하는 나이라 엄두가 안 나네요",
          "original_form": "이제는 생명에 대한 책임을 제가 져야 하는 나이라 엄두가 안 나네요",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009334.1.11",
          "form": "시간도 없잖아요",
          "original_form": "시간도 없잖아요",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009334.1.12",
          "form": "그렇죠ㅋㅋ 시간도 없어서 문제죠ㅠ",
          "original_form": "그렇죠ㅋㅋ 시간도 없어서 문제죠ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009334.1.13",
          "form": "저도 여건이",
          "original_form": "저도 여건이",
