      "utterance": [
        {
          "id": "MDRW2100030195.1.1",
          "form": "돈을준대도 안만드는건 대체 무슨심본지 모르겠네요ㅠ.ㅠ",
          "original_form": "돈을준대도 안만드는건 대체 무슨심본지 모르겠네요ㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030195.1.2",
          "form": "옷이 사람에 맞추는게 아니라 사람이 옷에 맞춰지는 세상이에요ㅠㅋㅋ",
          "original_form": "옷이 사람에 맞추는게 아니라 사람이 옷에 맞춰지는 세상이에요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030195.1.3",
          "form": "으 차라리 푸대자루가 편하겠어요",
          "original_form": "으 차라리 푸대자루가 편하겠어요",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030195.1.4",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030195.1.5",
          "form": "다이어트 자주하세요?",
          "original_form": "다이어트 자주하세요?",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030195.1.6",
          "form": "20대땐 열심히했는데 ㅋㅋ 요즘은 그냥 숨쉬기운동만 해요",
          "original_form": "20대땐 열심히했는데 ㅋㅋ 요즘은 그냥 숨쉬기운동만 해요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030195.1.7",
          "form": "전 진짜 아무것도 안하다가 ㅋㅋㅋ 최근에 시작했어요 ㅎ",
          "original_form": "전 진짜 아무것도 안하다가 ㅋㅋㅋ 최근에 시작했어요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030195.1.8",
          "form": "오 어떤방법하세요??",
          "original_form": "오 어떤방법하세요??",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030195.1.9",
          "form": "욱긴다 저 졸려서 정신이없나봐요 ㅋㅋㅋㅋ",
          "original_form": "욱긴다 저 졸려서 정신이없나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030195.1.10",
          "form": "다이어트는 안하고 필라테스 운동만 조금 해요",
          "original_form": "다이어트는 안하고 필라테스 운동만 조금 해요",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030195.1.11",
          "form": "과식을 자주해서 살은 안빠져요 ㅋㅋㅋㅋ",
          "original_form": "과식을 자주해서 살은 안빠져요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030195.1.12",
          "form": "아하 필라테스 좋죠!",
          "original_form": "아하 필라테스 좋죠!",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030195.1.13",
          "form": "재밌어요!",
          "original_form": "재밌어요!",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030195.1.14",
          "form": "운동은 그냥 건강위해할때가 좋은거같아요 살뺄생각으로 하면 끝도없고ㅠㅠㅋㅋ",
          "original_form": "운동은 그냥 건강위해할때가 좋은거같아요 살뺄생각으로 하면 끝도없고ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030195.1.15",
          "form": "아 지금 딱 졸릴시간이죠 누워야해요",
          "original_form": "아 지금 딱 졸릴시간이죠 누워야해요",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030195.1.16",
          "form": "제가 진짜 몸이 안쑤신데가 없어서 시작했는데 ㅋㅋㅋㅋ",
          "original_form": "제가 진짜 몸이 안쑤신데가 없어서 시작했는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030195.1.17",
          "form": "재밌지만 너무 못따라가서 속상해요 ㅎㅎ",
          "original_form": "재밌지만 너무 못따라가서 속상해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030195.1.18",
          "form": "일찍 자는것도 건강에 좋다는데 맨날 늦게자네요 ㅎ",
          "original_form": "일찍 자는것도 건강에 좋다는데 맨날 늦게자네요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030195.1.19",
          "form": "자기만의 속도가 있기나름이니까요 ㅎㅎ 강사님도 이해해주실거에요",
          "original_form": "자기만의 속도가 있기나름이니까요 ㅎㅎ 강사님도 이해해주실거에요",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030195.1.20",
          "form": "특히 주말전날이면 더 일찍자기싫죠 ㅋㅋㅋ",
          "original_form": "특히 주말전날이면 더 일찍자기싫죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030195.1.21",
          "form": "강사님을 좋은 분으로 만나서 다행이에요 ㅎㅎ",
          "original_form": "강사님을 좋은 분으로 만나서 다행이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030195.1.22",
          "form": "평소에 몇시쯤 주무세요?",
          "original_form": "평소에 몇시쯤 주무세요?",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030195.1.23",
          "form": "저는 보통 열두시면 누워서 눈은감으랴고해요!",
          "original_form": "저는 보통 열두시면 누워서 눈은감으랴고해요!",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030195.1.24",
          "form": "일찍 주무시는 편이네요!",
          "original_form": "일찍 주무시는 편이네요!",
          "speaker_id": "1",
          "time": "20210903 23:28"
        }
      ]
    }
  ]
