      "utterance": [
        {
          "id": "MDRW2100007673.1.1",
          "form": "키워보고싶은 동물 있나요?",
          "original_form": "키워보고싶은 동물 있나요?",
          "speaker_id": "1",
          "time": "20210822 09:24"
        },
        {
          "id": "MDRW2100007673.1.2",
          "form": "저는 길냥이를 데려오고 싶어요",
          "original_form": "저는 길냥이를 데려오고 싶어요",
          "speaker_id": "2",
          "time": "20210822 09:24"
        },
        {
          "id": "MDRW2100007673.1.3",
          "form": "호호이님은요?",
          "original_form": "호호이님은요?",
          "speaker_id": "2",
          "time": "20210822 09:25"
        },
        {
          "id": "MDRW2100007673.1.4",
          "form": "저도요! 길냥이가 최고인 것 같아요",
          "original_form": "저도요! 길냥이가 최고인 것 같아요",
          "speaker_id": "1",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007673.1.5",
          "form": "아니면 시고르자브종 강아지요",
          "original_form": "아니면 시고르자브종 강아지요",
          "speaker_id": "1",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007673.1.6",
          "form": "저희 집에 자주오는 길냥이 있는데 정말 귀엽습니다",
          "original_form": "저희 집에 자주오는 길냥이 있는데 정말 귀엽습니다",
          "speaker_id": "1",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007673.1.7",
          "form": "자주온다니 길냥이 밥챙겨주시나요?",
          "original_form": "자주온다니 길냥이 밥챙겨주시나요?",
          "speaker_id": "2",
          "time": "20210822 09:31"
        },
        {
          "id": "MDRW2100007673.1.8",
          "form": "네 맞아요. 하지만 부벼주진 않습니다",
          "original_form": "네 맞아요. 하지만 부벼주진 않습니다",
          "speaker_id": "1",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100007673.1.9",
          "form": "중성화는 어디선가 하고 왔더라구요",
          "original_form": "중성화는 어디선가 하고 왔더라구요",
          "speaker_id": "1",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100007673.1.10",
          "form": "너무 귀엽다...",
          "original_form": "너무 귀엽다...",
          "speaker_id": "2",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100007673.1.11",
          "form": "다행이네요 저도 사료랑 물만 챙긴적 몇번있는데",
          "original_form": "다행이네요 저도 사료랑 물만 챙긴적 몇번있는데",
          "speaker_id": "2",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007673.1.12",
          "form": "가까이 온적이 없어요 멀리서 지켜만봤어요",
          "original_form": "가까이 온적이 없어요 멀리서 지켜만봤어요",
          "speaker_id": "2",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007673.1.13",
          "form": "제 친구는 거북이 키우는데",
          "original_form": "제 친구는 거북이 키우는데",
          "speaker_id": "1",
          "time": "20210822 09:34"
        },
        {
          "id": "MDRW2100007673.1.14",
          "form": "파충류도 귀엽더라구요",
          "original_form": "파충류도 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210822 09:34"
        },
        {
          "id": "MDRW2100007673.1.15",
          "form": "맞아요!파충류 도마뱀종류도 너무 귀여워요",
          "original_form": "맞아요!파충류 도마뱀종류도 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210822 09:37"
        },
        {
          "id": "MDRW2100007673.1.16",
          "form": "하지만 물컹한 친구 조금 무서워쇼",
          "original_form": "하지만 물컹한 친구 조금 무서워쇼",
          "speaker_id": "1",
          "time": "20210822 09:37"
        },
        {
          "id": "MDRW2100007673.1.17",
          "form": "공감해요ㅋㅋㅋㅋ 반려동물 이름 생각해두신거 있나요?",
          "original_form": "공감해요ㅋㅋㅋㅋ 반려동물 이름 생각해두신거 있나요?",
          "speaker_id": "2",
          "time": "20210822 09:40"
        },
        {
          "id": "MDRW2100007673.1.18",
          "form": "구름이요~",
          "original_form": "구름이요~",
          "speaker_id": "1",
          "time": "20210822 09:45"
        },
        {
          "id": "MDRW2100007673.1.19",
          "form": "구름이! 폭신폭신하고 귀여워요",
          "original_form": "구름이! 폭신폭신하고 귀여워요",
          "speaker_id": "2",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007673.1.20",
          "form": "고양이나 강아지한테 어울리는 이름 같습니다",
          "original_form": "고양이나 강아지한테 어울리는 이름 같습니다",
          "speaker_id": "1",
          "time": "20210822 10:11"
        }
      ]
