      "utterance": [
        {
          "id": "MDRW2100029952.1.1",
          "form": "딱 그때 끝났어야했는데",
          "original_form": "딱 그때 끝났어야했는데",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100029952.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 카제하야 흑화 이러니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 카제하야 흑화 이러니까",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100029952.1.3",
          "form": "반응 나빠지잖나",
          "original_form": "반응 나빠지잖나",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100029952.1.4",
          "form": "갸는 상큼 상쾌인데",
          "original_form": "갸는 상큼 상쾌인데",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100029952.1.5",
          "form": "장기간으로 가면 질투 독점욕이 안나올 수가 없어서",
          "original_form": "장기간으로 가면 질투 독점욕이 안나올 수가 없어서",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100029952.1.6",
          "form": "상큼풋풋에서 끝내죠",
          "original_form": "상큼풋풋에서 끝내죠",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100029952.1.7",
          "form": "너는 그것을위해태어났잖오",
          "original_form": "너는 그것을위해태어났잖오",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100029952.1.8",
          "form": "갸는 그게 독보적이라",
          "original_form": "갸는 그게 독보적이라",
          "speaker_id": "2",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100029952.1.9",
          "form": "셀링이 된건데",
          "original_form": "셀링이 된건데",
          "speaker_id": "2",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100029952.1.10",
          "form": "흔한 남주가 됐지",
          "original_form": "흔한 남주가 됐지",
          "speaker_id": "2",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100029952.1.11",
          "form": "순정만화공식...",
          "original_form": "순정만화공식...",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100029952.1.12",
          "form": "적당히 치고빠지자...",
          "original_form": "적당히 치고빠지자...",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100029952.1.13",
          "form": "이게",
          "original_form": "이게",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100029952.1.14",
          "form": "순정만화는 길어지면",
          "original_form": "순정만화는 길어지면",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100029952.1.15",
          "form": "주인공 외 커플들이",
          "original_form": "주인공 외 커플들이",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100029952.1.16",
          "form": "생겨서",
          "original_form": "생겨서",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100029952.1.17",
          "form": "주인공만 관심있는 나는 급노잼이 됨",
          "original_form": "주인공만 관심있는 나는 급노잼이 됨",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100029952.1.18",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100029952.1.19",
          "form": "가끔은 그런거잇자나",
          "original_form": "가끔은 그런거잇자나",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100029952.1.20",
          "form": "서브컾이 더 매력적인 경우는",
          "original_form": "서브컾이 더 매력적인 경우는",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100029952.1.21",
          "form": "차라리 빼서 내줘 ㅠ",
          "original_form": "차라리 빼서 내줘 ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100029952.1.22",
          "form": "그거만보게 ㅠ 이럴때도잏고",
          "original_form": "그거만보게 ㅠ 이럴때도잏고",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100029952.1.23",
          "form": "아 오타...",
          "original_form": "아 오타...",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100029952.1.24",
          "form": "서브컾이 더 매력적이면 작품에 근본적인",
          "original_form": "서브컾이 더 매력적이면 작품에 근본적인",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100029952.1.25",
          "form": "문제가 존재한다고 생각한다",
          "original_form": "문제가 존재한다고 생각한다",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100029952.1.26",
          "form": "ㅋㅋㅋㅋ일작품 중에서는",
          "original_form": "ㅋㅋㅋㅋ일작품 중에서는",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100029952.1.27",
          "form": "짧은 벨작품",
          "original_form": "짧은 벨작품",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100029952.1.28",
          "form": "시리즈 처음에 나온 메인커플은 1권인데",
          "original_form": "시리즈 처음에 나온 메인커플은 1권인데",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100029952.1.29",
          "form": "서브커플이 메인화되어서 4권 넘에 나오는 것도 있자나",
          "original_form": "서브커플이 메인화되어서 4권 넘에 나오는 것도 있자나",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100029952.1.30",
          "form": "그건 사실 사기아닌갘ㅋㅋㅋㅋ",
          "original_form": "그건 사실 사기아닌갘ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 03:14"
        },
        {
          "id": "MDRW2100029952.1.31",
          "form": "1권 커플은 결국 세계관설명에.이용당했을 뿐",
          "original_form": "1권 커플은 결국 세계관설명에.이용당했을 뿐",
          "speaker_id": "1",
          "time": "20210904 08:35"
        },
        {
          "id": "MDRW2100029952.1.32",
          "form": "정말 싫엇",
          "original_form": "정말 싫엇",
          "speaker_id": "2",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100029952.1.33",
          "form": "내가 뒷커플을 좋아하면 진짜 최고지만ㅌㅌㅌ",
          "original_form": "내가 뒷커플을 좋아하면 진짜 최고지만ㅌㅌㅌ",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100029952.1.34",
          "form": "근데 그런 경우가 잇긴해",
          "original_form": "근데 그런 경우가 잇긴해",
          "speaker_id": "1",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100029952.1.35",
          "form": "앞에 낸 컾은 메이저고",
          "original_form": "앞에 낸 컾은 메이저고",
          "speaker_id": "1",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100029952.1.36",
          "form": "뒤에 낸 커플은 그럭저럭 메이너-마이너라서",
          "original_form": "뒤에 낸 커플은 그럭저럭 메이너-마이너라서",
          "speaker_id": "1",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100029952.1.37",
          "form": "서브 나오는 작품은 서브쪽이 작가 취향일거라고",
          "original_form": "서브 나오는 작품은 서브쪽이 작가 취향일거라고",
          "speaker_id": "1",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100029952.1.38",
          "form": "여기는 사람들도 있더라",
          "original_form": "여기는 사람들도 있더라",
          "speaker_id": "1",
          "time": "20210904 12:40"
        }
      ]
    }
