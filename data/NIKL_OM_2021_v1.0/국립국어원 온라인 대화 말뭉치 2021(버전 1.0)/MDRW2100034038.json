      "utterance": [
        {
          "id": "MDRW2100034038.1.1",
          "form": "점심 뭐 드셔요?",
          "original_form": "점심 뭐 드셔요?",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100034038.1.2",
          "form": "저 점심 육개장이요~",
          "original_form": "저 점심 육개장이요~",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100034038.1.3",
          "form": "오 맛있겠어요",
          "original_form": "오 맛있겠어요",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100034038.1.4",
          "form": "반찬은요?",
          "original_form": "반찬은요?",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100034038.1.5",
          "form": "반찬으로는 장조림 먹었어요~",
          "original_form": "반찬으로는 장조림 먹었어요~",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100034038.1.6",
          "form": "장조림 만들었나요??",
          "original_form": "장조림 만들었나요??",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100034038.1.7",
          "form": "네~ 수제임돠 ㅋㅋㅋ",
          "original_form": "네~ 수제임돠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100034038.1.8",
          "form": "왕 금손",
          "original_form": "왕 금손",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100034038.1.9",
          "form": "ㅋㅋㅋㅋ만들기 쉽나요",
          "original_form": "ㅋㅋㅋㅋ만들기 쉽나요",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100034038.1.10",
          "form": "전 유투브참고해서 젤쉬운레시피 따라만들고있어요~",
          "original_form": "전 유투브참고해서 젤쉬운레시피 따라만들고있어요~",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100034038.1.11",
          "form": "쉬워서 놀란 요리중에 하나에요 장조림!",
          "original_form": "쉬워서 놀란 요리중에 하나에요 장조림!",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100034038.1.12",
          "form": "ㅋㅋ ㅋㅋ 혹시 어떤분꺼 봤나요",
          "original_form": "ㅋㅋ ㅋㅋ 혹시 어떤분꺼 봤나요",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100034038.1.13",
          "form": "저도 해볼까해요",
          "original_form": "저도 해볼까해요",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100034038.1.14",
          "form": "그냥주부 님거였나? 정확히기억은안나는데 대충 이런느낌의이름이었어요!",
          "original_form": "그냥주부 님거였나? 정확히기억은안나는데 대충 이런느낌의이름이었어요!",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100034038.1.15",
          "form": "백종원선생님꺼도 괜찮아요~",
          "original_form": "백종원선생님꺼도 괜찮아요~",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100034038.1.16",
          "form": "ㅎ ㅎ 백주부꺼 봐야겠어요",
          "original_form": "ㅎ ㅎ 백주부꺼 봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100034038.1.17",
          "form": "맞아요 백주부가 재믺어요 ㅋㅋ",
          "original_form": "맞아요 백주부가 재믺어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100034038.1.18",
          "form": "ㅋㅋㅋㅋㅋ 쉽게",
          "original_form": "ㅋㅋㅋㅋㅋ 쉽게",
          "speaker_id": "2",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100034038.1.19",
          "form": "뚝딱 알려주죠",
          "original_form": "뚝딱 알려주죠",
          "speaker_id": "2",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100034038.1.20",
          "form": "맞아요 ㅋㅋ 말투도 재밌고 웃겨용",
          "original_form": "맞아요 ㅋㅋ 말투도 재밌고 웃겨용",
          "speaker_id": "1",
          "time": "20210827 12:47"
        }
