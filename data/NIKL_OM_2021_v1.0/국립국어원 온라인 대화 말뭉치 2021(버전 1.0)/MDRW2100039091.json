      "utterance": [
        {
          "id": "MDRW2100039091.1.1",
          "form": "먹는게 낙이긴하죠",
          "original_form": "먹는게 낙이긴하죠",
          "speaker_id": "1",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100039091.1.2",
          "form": "먹는 재미가 없었으면 어떻게 살았을까 싶어요",
          "original_form": "먹는 재미가 없었으면 어떻게 살았을까 싶어요",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100039091.1.3",
          "form": "맛아요 전 아침먹으면서 점심모먹을까 생각하는데ㅋ",
          "original_form": "맛아요 전 아침먹으면서 점심모먹을까 생각하는데ㅋ",
          "speaker_id": "1",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100039091.1.4",
          "form": "모든 사람이 그러지 않나요ㅋㅋ",
          "original_form": "모든 사람이 그러지 않나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100039091.1.5",
          "form": "오늘 점심은 모드세요?ㅋ",
          "original_form": "오늘 점심은 모드세요?ㅋ",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100039091.1.6",
          "form": "짜장면 먹을까 생각중이에요",
          "original_form": "짜장면 먹을까 생각중이에요",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100039091.1.7",
          "form": "좋네요 저도 면이 땡겨서",
          "original_form": "좋네요 저도 면이 땡겨서",
          "speaker_id": "1",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100039091.1.8",
          "form": "비빔국수갈까 해요",
          "original_form": "비빔국수갈까 해요",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100039091.1.9",
          "form": "비빔국수도 좋죠",
          "original_form": "비빔국수도 좋죠",
          "speaker_id": "2",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100039091.1.10",
          "form": "면은 질리지가 않아요",
          "original_form": "면은 질리지가 않아요",
          "speaker_id": "1",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100039091.1.11",
          "form": "맞아요 면만 먹고도 살수 있을 정도로요",
          "original_form": "맞아요 면만 먹고도 살수 있을 정도로요",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100039091.1.12",
          "form": "네 요즘워낙 다양하게나와서",
          "original_form": "네 요즘워낙 다양하게나와서",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100039091.1.13",
          "form": "신라면볶음면 광고 나와서 사봤어요",
          "original_form": "신라면볶음면 광고 나와서 사봤어요",
          "speaker_id": "1",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100039091.1.14",
          "form": "아직 못먹어봤는데 어떤가요?",
          "original_form": "아직 못먹어봤는데 어떤가요?",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100039091.1.15",
          "form": "저도 내일 먹어보려고요ㅋㅋ\r\n불닭만큼 맵진않겟져",
          "original_form": "저도 내일 먹어보려고요ㅋㅋ\r\n불닭만큼 맵진않겟져",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100039091.1.16",
          "form": "그렇겠죠?ㅎㅎ 그러면 신라면이 아니니까요",
          "original_form": "그렇겠죠?ㅎㅎ 그러면 신라면이 아니니까요",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100039091.1.17",
          "form": "신라면옛날엔 매웟는데 지금은 진라면이 더매운거같아여",
          "original_form": "신라면옛날엔 매웟는데 지금은 진라면이 더매운거같아여",
          "speaker_id": "1",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100039091.1.18",
          "form": "진라면 매운맛이 좀 세죠",
          "original_form": "진라면 매운맛이 좀 세죠",
          "speaker_id": "2",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100039091.1.19",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210903 10:17"
        }
      ]
