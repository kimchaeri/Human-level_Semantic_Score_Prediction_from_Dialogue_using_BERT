      "utterance": [
        {
          "id": "MDRW2100006288.1.1",
          "form": "야구얘기하니까",
          "original_form": "야구얘기하니까",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006288.1.2",
          "form": "금방금방 채우네요",
          "original_form": "금방금방 채우네요",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006288.1.3",
          "form": "ㅋㅋㅋ 요새는 kbs 해설진이 좋더라구요. 김태균 박용택 영입 성공적ㅋㅋ",
          "original_form": "ㅋㅋㅋ 요새는 kbs 해설진이 좋더라구요. 김태균 박용택 영입 성공적ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006288.1.4",
          "form": "sbs는 이순철 이승엽인가요?",
          "original_form": "sbs는 이순철 이승엽인가요?",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100006288.1.5",
          "form": "안경현 이동현까지..",
          "original_form": "안경현 이동현까지..",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100006288.1.6",
          "form": "이순철 아저씨는 자꾸 화내서..ㅋㅋ",
          "original_form": "이순철 아저씨는 자꾸 화내서..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100006288.1.7",
          "form": "그게 매력이죠",
          "original_form": "그게 매력이죠",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006288.1.8",
          "form": "이순철의 매력",
          "original_form": "이순철의 매력",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006288.1.9",
          "form": "엠비씨는",
          "original_form": "엠비씨는",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006288.1.10",
          "form": "허구연?ㅋㅋ",
          "original_form": "허구연?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006288.1.11",
          "form": "김선우 이상훈 말고 또 있는데 기억이 안 나네요ㅋㅋ",
          "original_form": "김선우 이상훈 말고 또 있는데 기억이 안 나네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006288.1.12",
          "form": "심수창",
          "original_form": "심수창",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006288.1.13",
          "form": "양준혁",
          "original_form": "양준혁",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006288.1.14",
          "form": "심재학",
          "original_form": "심재학",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006288.1.15",
          "form": "이정도인가요?",
          "original_form": "이정도인가요?",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006288.1.16",
          "form": "ㅋㅋ 많네요. 근데 심수창은 중계 진짜 몇 번 안 시켜주더라구요",
          "original_form": "ㅋㅋ 많네요. 근데 심수창은 중계 진짜 몇 번 안 시켜주더라구요",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006288.1.17",
          "form": "수습사원같은 느낌",
          "original_form": "수습사원같은 느낌",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006288.1.18",
          "form": "ㅋㅋ 딱 맞는 비유네요",
          "original_form": "ㅋㅋ 딱 맞는 비유네요",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100006288.1.19",
          "form": "엠스플 유튜브에 자주 나오던데",
          "original_form": "엠스플 유튜브에 자주 나오던데",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006288.1.20",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006288.1.21",
          "form": "이종범 정민철때가 좋았는데",
          "original_form": "이종범 정민철때가 좋았는데",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006288.1.22",
          "form": "정민철이야 단장 잘 하고 있으니까요ㅋㅋ",
          "original_form": "정민철이야 단장 잘 하고 있으니까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006288.1.23",
          "form": "이종범이 감독하는 날도 올까요?",
          "original_form": "이종범이 감독하는 날도 올까요?",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100006288.1.24",
          "form": "언젠가 하지 않을까요?",
          "original_form": "언젠가 하지 않을까요?",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006288.1.25",
          "form": "정민철은 감독은 생각 없는거 같구",
          "original_form": "정민철은 감독은 생각 없는거 같구",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006288.1.26",
          "form": "기아에서 하게 된다면 기아팬들은 안 좋아할 것 같긴 하네요. 선동렬 때 생각하면",
          "original_form": "기아에서 하게 된다면 기아팬들은 안 좋아할 것 같긴 하네요. 선동렬 때 생각하면",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006288.1.27",
          "form": "선동렬은 감독을 못하네요",
          "original_form": "선동렬은 감독을 못하네요",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006288.1.28",
          "form": "슥에서 데려올라다가 윗선 반대로",
          "original_form": "슥에서 데려올라다가 윗선 반대로",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006288.1.29",
          "form": "김원형  데려오고",
          "original_form": "김원형  데려오고",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006288.1.30",
          "form": "공부 열심히 하시나 보던데 다시 할 수 있을지는 모르겠네요. 젊은 감독으로 가는 흐름이라",
          "original_form": "공부 열심히 하시나 보던데 다시 할 수 있을지는 모르겠네요. 젊은 감독으로 가는 흐름이라",
