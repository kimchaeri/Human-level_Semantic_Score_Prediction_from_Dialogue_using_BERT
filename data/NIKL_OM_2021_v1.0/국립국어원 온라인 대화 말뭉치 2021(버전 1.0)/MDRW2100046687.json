        },
        {
          "id": "MDRW2100046687.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211105 00:06"
        },
        {
          "id": "MDRW2100046687.1.3",
          "form": "최근에 즐기고 계시는 취미가 있으신가요?",
          "original_form": "최근에 즐기고 계시는 취미가 있으신가요?",
          "speaker_id": "2",
          "time": "20211105 00:06"
        },
        {
          "id": "MDRW2100046687.1.4",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20211105 00:06"
        },
        {
          "id": "MDRW2100046687.1.5",
          "form": "음악듣기요",
          "original_form": "음악듣기요",
          "speaker_id": "1",
          "time": "20211105 00:07"
        },
        {
          "id": "MDRW2100046687.1.6",
          "form": "오 저도 음악 듣는거 엄청 좋아해요!",
          "original_form": "오 저도 음악 듣는거 엄청 좋아해요!",
          "speaker_id": "2",
          "time": "20211105 00:07"
        },
        {
          "id": "MDRW2100046687.1.7",
          "form": "가수누구좋아해요?",
          "original_form": "가수누구좋아해요?",
          "speaker_id": "1",
          "time": "20211105 00:07"
        },
        {
          "id": "MDRW2100046687.1.8",
          "form": "최근에 들으셨던 노래 중에 추천해주실 만한 노래가 있을까요?",
          "original_form": "최근에 들으셨던 노래 중에 추천해주실 만한 노래가 있을까요?",
          "speaker_id": "2",
          "time": "20211105 00:07"
        },
        {
          "id": "MDRW2100046687.1.9",
          "form": "저는 막 엄청 팬인 가수는 없는데",
          "original_form": "저는 막 엄청 팬인 가수는 없는데",
          "speaker_id": "2",
          "time": "20211105 00:07"
        },
        {
          "id": "MDRW2100046687.1.10",
          "form": "저는 국민가수보고있어서",
          "original_form": "저는 국민가수보고있어서",
          "speaker_id": "1",
          "time": "20211105 00:07"
        },
        {
          "id": "MDRW2100046687.1.11",
          "form": "요즘은 플라이투더스카이랑 화요비 노래를 많이 들어요~",
          "original_form": "요즘은 플라이투더스카이랑 화요비 노래를 많이 들어요~",
          "speaker_id": "2",
          "time": "20211105 00:07"
        },
        {
          "id": "MDRW2100046687.1.12",
          "form": "아 국민가수!!",
          "original_form": "아 국민가수!!",
          "speaker_id": "2",
          "time": "20211105 00:08"
        },
        {
          "id": "MDRW2100046687.1.13",
          "form": "국민가수보시나요?",
          "original_form": "국민가수보시나요?",
          "speaker_id": "1",
          "time": "20211105 00:08"
        },
        {
          "id": "MDRW2100046687.1.14",
          "form": "앗 아니요 ㅠ_ㅠ",
          "original_form": "앗 아니요 ㅠ_ㅠ",
          "speaker_id": "2",
          "time": "20211105 00:08"
        },
        {
          "id": "MDRW2100046687.1.15",
          "form": "근데 그것도 트로트 부르는 프로그램인가요?",
          "original_form": "근데 그것도 트로트 부르는 프로그램인가요?",
          "speaker_id": "2",
          "time": "20211105 00:08"
        },
        {
          "id": "MDRW2100046687.1.16",
          "form": "전체적인장르묘",
          "original_form": "전체적인장르묘",
          "speaker_id": "1",
          "time": "20211105 00:08"
        },
        {
          "id": "MDRW2100046687.1.17",
          "form": "점심은드셨어요?",
          "original_form": "점심은드셨어요?",
          "speaker_id": "1",
          "time": "20211105 00:10"
        },
        {
          "id": "MDRW2100046687.1.18",
          "form": "아 그건 딱히 장르가 정해져있는게 아니군요",
          "original_form": "아 그건 딱히 장르가 정해져있는게 아니군요",
          "speaker_id": "2",
          "time": "20211105 00:11"
        },
        {
          "id": "MDRW2100046687.1.19",
          "form": "그러면 한 번 봐야겠어요!",
          "original_form": "그러면 한 번 봐야겠어요!",
          "speaker_id": "2",
          "time": "20211105 00:11"
        },
        {
          "id": "MDRW2100046687.1.20",
          "form": "아직 안먹었습니다 ㅎ.ㅎ",
          "original_form": "아직 안먹었습니다 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20211105 00:11"
        },
        {
          "id": "MDRW2100046687.1.21",
          "form": "국민가수 보면서",
          "original_form": "국민가수 보면서",
          "speaker_id": "2",
          "time": "20211105 00:11"
        },
        {
          "id": "MDRW2100046687.1.22",
          "form": "특별하게",
          "original_form": "특별하게",
          "speaker_id": "2",
          "time": "20211105 00:11"
        },
        {
          "id": "MDRW2100046687.1.23",
          "form": "응원하고 계신 분 있으세요?",
          "original_form": "응원하고 계신 분 있으세요?",
          "speaker_id": "2",
          "time": "20211105 00:11"
        },
        {
          "id": "MDRW2100046687.1.24",
          "form": "이솔로몬이",
          "original_form": "이솔로몬이",
          "speaker_id": "1",
          "time": "20211105 00:12"
        },
        {
          "id": "MDRW2100046687.1.25",
          "form": "잘하더라구요",
          "original_form": "잘하더라구요",
          "speaker_id": "1",
          "time": "20211105 00:12"
        },
        {
          "id": "MDRW2100046687.1.26",
          "form": "독실한크리스찬같아서",
          "original_form": "독실한크리스찬같아서",
          "speaker_id": "1",
          "time": "20211105 00:12"
        },
        {
          "id": "MDRW2100046687.1.27",
          "form": "시인이면서",
          "original_form": "시인이면서",
          "speaker_id": "1",
          "time": "20211105 00:12"
        },
        {
          "id": "MDRW2100046687.1.28",
          "form": "부모님들이 좋아하죠",
          "original_form": "부모님들이 좋아하죠",
          "speaker_id": "1",
          "time": "20211105 00:12"
        },
        {
          "id": "MDRW2100046687.1.29",
          "form": "와 시인인데 노래도 잘하는구나",
          "original_form": "와 시인인데 노래도 잘하는구나",
          "speaker_id": "2",
          "time": "20211105 00:13"
        },
        {
          "id": "MDRW2100046687.1.30",
          "form": "사기캐릭터네요 ㅎㅎ",
          "original_form": "사기캐릭터네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 00:13"
        },
        {
          "id": "MDRW2100046687.1.31",
          "form": "저도 한 번 찾아봐야겠어요!\r\n",
          "original_form": "저도 한 번 찾아봐야겠어요!\r\n",
          "speaker_id": "2",
          "time": "20211105 00:13"
        }
      ]
    }
