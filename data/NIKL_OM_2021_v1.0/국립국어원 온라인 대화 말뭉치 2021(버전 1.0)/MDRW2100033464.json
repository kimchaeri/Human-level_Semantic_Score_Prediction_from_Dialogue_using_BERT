      "utterance": [
        {
          "id": "MDRW2100033464.1.1",
          "form": "GRN 얼마예요?",
          "original_form": "GRN 얼마예요?",
          "speaker_id": "1",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100033464.1.2",
          "form": "거기 회원가입하면싸요",
          "original_form": "거기 회원가입하면싸요",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100033464.1.3",
          "form": "출석하년 100원씩 줘서 모아서 사요",
          "original_form": "출석하년 100원씩 줘서 모아서 사요",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100033464.1.4",
          "form": "오 그런것도 있어요?",
          "original_form": "오 그런것도 있어요?",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100033464.1.5",
          "form": "3만원대에 두병요",
          "original_form": "3만원대에 두병요",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100033464.1.6",
          "form": "근데 세일을 자뢔요",
          "original_form": "근데 세일을 자뢔요",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100033464.1.7",
          "form": "잘해요",
          "original_form": "잘해요",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100033464.1.8",
          "form": "올리븡영은 사지 마세요",
          "original_form": "올리븡영은 사지 마세요",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100033464.1.9",
          "form": "비싸요",
          "original_form": "비싸요",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100033464.1.10",
          "form": "아하..ㅅ",
          "original_form": "아하..ㅅ",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033464.1.11",
          "form": "접근성이 좋잖아요",
          "original_form": "접근성이 좋잖아요",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033464.1.12",
          "form": "올리브영",
          "original_form": "올리브영",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033464.1.13",
          "form": "이런거 쉽게 사기엔 올영 가죠 ㅎㅎ",
          "original_form": "이런거 쉽게 사기엔 올영 가죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033464.1.14",
          "form": "너무바가지요",
          "original_form": "너무바가지요",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033464.1.15",
          "form": "진짜 비싸요",
          "original_form": "진짜 비싸요",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033464.1.16",
          "form": "그정도예요?",
          "original_form": "그정도예요?",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033464.1.17",
          "form": "회원가입하고",
          "original_form": "회원가입하고",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033464.1.18",
          "form": "사기당한 기분이네요",
          "original_form": "사기당한 기분이네요",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033464.1.19",
          "form": "출석하고주말에 사세요",
          "original_form": "출석하고주말에 사세요",
          "speaker_id": "2",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100033464.1.20",
          "form": "하루면 와요",
          "original_form": "하루면 와요",
          "speaker_id": "2",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100033464.1.21",
          "form": "분홍이 초록이 어떤거드세요?",
          "original_form": "분홍이 초록이 어떤거드세요?",
          "speaker_id": "1",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100033464.1.22",
          "form": "고건 분홍이 먹는데",
          "original_form": "고건 분홍이 먹는데",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100033464.1.23",
          "form": "전 천둥이만 요",
          "original_form": "전 천둥이만 요",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100033464.1.24",
          "form": "분홍초록 효과도 없고요",
          "original_form": "분홍초록 효과도 없고요",
          "speaker_id": "2",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100033464.1.25",
          "form": "아...종류가 엄청많네요",
          "original_form": "아...종류가 엄청많네요",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100033464.1.26",
          "form": "하늘이 천둥이",
          "original_form": "하늘이 천둥이",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100033464.1.27",
          "form": "네 변비에 천둥이만 효과 있어요",
          "original_form": "네 변비에 천둥이만 효과 있어요",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100033464.1.28",
          "form": "천둥이요 ㅎㅎ",
          "original_form": "천둥이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100033464.1.29",
          "form": "이름도 참 다양하네요",
          "original_form": "이름도 참 다양하네요",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100033464.1.30",
          "form": "지알앤 많이 들어봤는데",
          "original_form": "지알앤 많이 들어봤는데",
          "speaker_id": "1",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100033464.1.31",
          "form": "사이트는 첨들어가보네요",
          "original_form": "사이트는 첨들어가보네요",
          "speaker_id": "1",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100033464.1.32",
          "form": "출석 꼭하세요",
          "original_form": "출석 꼭하세요",
          "speaker_id": "2",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100033464.1.33",
          "form": "언제살지 모르니",
          "original_form": "언제살지 모르니",
          "speaker_id": "2",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100033464.1.34",
          "form": "100원줘요 하루에",
          "original_form": "100원줘요 하루에",
          "speaker_id": "2",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100033464.1.35",
          "form": "한번 시작해보려구요 ㅎ",
          "original_form": "한번 시작해보려구요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100033464.1.36",
          "form": "네 홧팅요",
          "original_form": "네 홧팅요",
          "speaker_id": "2",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100033464.1.37",
          "form": "보고 결정하세요",
          "original_form": "보고 결정하세요",
          "speaker_id": "2",
          "time": "20210823 23:46"
        }
      ]
    }
  ]
