      "utterance": [
        {
          "id": "MDRW2100020647.1.1",
          "form": "저도 그부분은 잘 모르겠는데 제일 문제가 되었던 부분은",
          "original_form": "저도 그부분은 잘 모르겠는데 제일 문제가 되었던 부분은",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020647.1.2",
          "form": "진짜ㅜㅜ 팬들한테 잘 좀 하지",
          "original_form": "진짜ㅜㅜ 팬들한테 잘 좀 하지",
          "speaker_id": "1",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020647.1.3",
          "form": "여자친구가 있었음에도 불구하고 여캠한테 별풍을 몇백씩 쏜거랑",
          "original_form": "여자친구가 있었음에도 불구하고 여캠한테 별풍을 몇백씩 쏜거랑",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020647.1.4",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020647.1.5",
          "form": "와 그런거 안볼것같은데",
          "original_form": "와 그런거 안볼것같은데",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020647.1.6",
          "form": "그 인스타인가에 동영상 올렸었거든요 살ㅈ닥 사랑멘트 이쪽으로 ??",
          "original_form": "그 인스타인가에 동영상 올렸었거든요 살ㅈ닥 사랑멘트 이쪽으로 ??",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020647.1.7",
          "form": "인스타에는 팬들 겨냥해서 그렇게 한것처럼 올렸는데",
          "original_form": "인스타에는 팬들 겨냥해서 그렇게 한것처럼 올렸는데",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020647.1.8",
          "form": "그걸 그대로 여자친구한테 보냈더라구요",
          "original_form": "그걸 그대로 여자친구한테 보냈더라구요",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020647.1.9",
          "form": "아이구",
          "original_form": "아이구",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020647.1.10",
          "form": "여자친구 사귈수는 있다쳐도",
          "original_form": "여자친구 사귈수는 있다쳐도",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.11",
          "form": "그니까요 저도 진짜 너무 충격받아서 ...",
          "original_form": "그니까요 저도 진짜 너무 충격받아서 ...",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.12",
          "form": "팬들한테 티 안냈으면 좋겠어요",
          "original_form": "팬들한테 티 안냈으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.13",
          "form": "개인적으로는ㅠ",
          "original_form": "개인적으로는ㅠ",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.14",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.15",
          "form": "저도 사귀는건 그렇다치는데",
          "original_form": "저도 사귀는건 그렇다치는데",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.16",
          "form": "적어도 예의는 지켜야죠",
          "original_form": "적어도 예의는 지켜야죠",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.17",
          "form": "막 그거 가지고 지들끼리 티내고",
          "original_form": "막 그거 가지고 지들끼리 티내고",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.18",
          "form": "박지훈 노래도 좋아했는데",
          "original_form": "박지훈 노래도 좋아했는데",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.19",
          "form": "공연 초대하고 막... 비계파고... 그런건 ㅠㅠ",
          "original_form": "공연 초대하고 막... 비계파고... 그런건 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.20",
          "form": "데뷔곡이 진짜 지훈이만의 느낌이 읶었더든요",
          "original_form": "데뷔곡이 진짜 지훈이만의 느낌이 읶었더든요",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.21",
          "form": "맞아요 노래 좋더라구요",
          "original_form": "맞아요 노래 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.22",
          "form": "와 진짜 .... 싫어요 .....",
          "original_form": "와 진짜 .... 싫어요 .....",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020647.1.23",
          "form": "네네 근데 노래도 이제 맘편히 못듣겠네요 ..",
          "original_form": "네네 근데 노래도 이제 맘편히 못듣겠네요 ..",
