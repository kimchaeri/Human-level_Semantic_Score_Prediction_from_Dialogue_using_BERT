      "utterance": [
        {
          "id": "MDRW2100039518.1.1",
          "form": "바삭한식당 리뉴얼하고...",
          "original_form": "바삭한식당 리뉴얼하고...",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039518.1.2",
          "form": "더좁아진거같은너낌이에여",
          "original_form": "더좁아진거같은너낌이에여",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039518.1.3",
          "form": "주방이커지고",
          "original_form": "주방이커지고",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039518.1.4",
          "form": "좌석은적어졌네",
          "original_form": "좌석은적어졌네",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039518.1.5",
          "form": "아니 거기 배달도 안되는데",
          "original_form": "아니 거기 배달도 안되는데",
          "speaker_id": "2",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100039518.1.6",
          "form": "좌석이 좁아지면",
          "original_form": "좌석이 좁아지면",
          "speaker_id": "2",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100039518.1.7",
          "form": "웨이팅 지옥일텐데",
          "original_form": "웨이팅 지옥일텐데",
          "speaker_id": "2",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100039518.1.8",
          "form": "이제 배달 하시려는건가...?",
          "original_form": "이제 배달 하시려는건가...?",
          "speaker_id": "2",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100039518.1.9",
          "form": "배달 안할거같아",
          "original_form": "배달 안할거같아",
          "speaker_id": "1",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039518.1.10",
          "form": "곱빼기도 자기가",
          "original_form": "곱빼기도 자기가",
          "speaker_id": "1",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039518.1.11",
          "form": "감당되면하겠대",
          "original_form": "감당되면하겠대",
          "speaker_id": "1",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039518.1.12",
          "form": "으어",
          "original_form": "으어",
          "speaker_id": "2",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039518.1.13",
          "form": "도전 난이도가 더 어려운 식당이 되어버렷네",
          "original_form": "도전 난이도가 더 어려운 식당이 되어버렷네",
          "speaker_id": "2",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039518.1.14",
          "form": "도전할 가치가 있는 식당이긴 하지만..",
          "original_form": "도전할 가치가 있는 식당이긴 하지만..",
          "speaker_id": "2",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039518.1.15",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039518.1.16",
          "form": "감당되면하겠다는거보디",
          "original_form": "감당되면하겠다는거보디",
          "speaker_id": "1",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039518.1.17",
          "form": "더많은 고기를 준비하기어려우신가봐",
          "original_form": "더많은 고기를 준비하기어려우신가봐",
          "speaker_id": "1",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039518.1.18",
          "form": "본인이 감당할수있는양껏하겠다는",
          "original_form": "본인이 감당할수있는양껏하겠다는",
          "speaker_id": "1",
          "time": "20210903 19:00"
        },
        {
          "id": "MDRW2100039518.1.19",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "2",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039518.1.20",
          "form": "주방이더커지긴했어",
          "original_form": "주방이더커지긴했어",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039518.1.21",
          "form": "퀄리티 유지하겠다는 뜻이 있으신거같아서",
          "original_form": "퀄리티 유지하겠다는 뜻이 있으신거같아서",
          "speaker_id": "2",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039518.1.22",
          "form": "여전히혼자하시더라구여",
          "original_form": "여전히혼자하시더라구여",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039518.1.23",
          "form": "신뢰는 무너지지 않네여..",
          "original_form": "신뢰는 무너지지 않네여..",
          "speaker_id": "2",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039518.1.24",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039518.1.25",
          "form": "안심등심시켰는ㄷ77ㅣ",
          "original_form": "안심등심시켰는ㄷ77ㅣ",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039518.1.26",
          "form": "안심이 180그람",
          "original_form": "안심이 180그람",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039518.1.27",
          "form": "남았다고",
          "original_form": "남았다고",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039518.1.28",
          "form": "원래200인데",
          "original_form": "원래200인데",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039518.1.29",
          "form": "그래도시키겠다하니까",
          "original_form": "그래도시키겠다하니까",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039518.1.30",
          "form": "그럼 등심을더드리겠다고 아니면치킨이나",
          "original_form": "그럼 등심을더드리겠다고 아니면치킨이나",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039518.1.31",
          "form": "그래서치킨으로달라했더니",
          "original_form": "그래서치킨으로달라했더니",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039518.1.32",
          "form": "0.5인분을더주심",
          "original_form": "0.5인분을더주심",
          "speaker_id": "1",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100039518.1.33",
          "form": "와 진짜 사장님....",
          "original_form": "와 진짜 사장님....",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100039518.1.34",
          "form": "자선사업 하시는 거 맞다니까.....",
          "original_form": "자선사업 하시는 거 맞다니까.....",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100039518.1.35",
          "form": "ㄹㅇ.....",
          "original_form": "ㄹㅇ.....",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100039518.1.36",
          "form": "등심도 맛있었고",
          "original_form": "등심도 맛있었고",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100039518.1.37",
          "form": "안심도맛있는데",
          "original_form": "안심도맛있는데",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100039518.1.38",
          "form": "우리둘이가면선택지가",
          "original_form": "우리둘이가면선택지가",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039518.1.39",
          "form": "세개가되어벌엿",
          "original_form": "세개가되어벌엿",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039518.1.40",
          "form": "그닉가",
          "original_form": "그닉가",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039518.1.41",
          "form": "하나를 포기해야 하는 상황이라니",
          "original_form": "하나를 포기해야 하는 상황이라니",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039518.1.42",
          "form": "눈물난다",
          "original_form": "눈물난다",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039518.1.43",
          "form": "아니 근데다맛있어",
          "original_form": "아니 근데다맛있어",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039518.1.44",
          "form": "또르륵",
          "original_form": "또르륵",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039518.1.45",
          "form": "아니 등심이 안심만큼부드럽다니",
          "original_form": "아니 등심이 안심만큼부드럽다니",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039518.1.46",
          "form": "신세계",
          "original_form": "신세계",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039518.1.47",
          "form": "그게",
          "original_form": "그게",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039518.1.48",
          "form": "가능해...?",
          "original_form": "가능해...?",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039518.1.49",
          "form": "쉬면서 또 연구 엄청 해오셧나보네...",
          "original_form": "쉬면서 또 연구 엄청 해오셧나보네...",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039518.1.50",
          "form": "ㄹㅇ 넘맛있었어",
          "original_form": "ㄹㅇ 넘맛있었어",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039518.1.51",
          "form": "빠른시일내로함가여",
          "original_form": "빠른시일내로함가여",
          "speaker_id": "1",
          "time": "20210903 19:16"
        }
      ]
