      "utterance": [
        {
          "id": "MDRW2100007343.1.1",
          "form": "어디로 여행 가봤어?",
          "original_form": "어디로 여행 가봤어?",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007343.1.2",
          "form": "가장 마지막 여행은 제주도!",
          "original_form": "가장 마지막 여행은 제주도!",
          "speaker_id": "1",
          "time": "20210822 06:07"
        },
        {
          "id": "MDRW2100007343.1.3",
          "form": "제주도? 멋있겠다 난 어릴때 수학여행 간게 전부라서",
          "original_form": "제주도? 멋있겠다 난 어릴때 수학여행 간게 전부라서",
          "speaker_id": "2",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100007343.1.4",
          "form": "나머지는 해외야 제주도 어땠어?",
          "original_form": "나머지는 해외야 제주도 어땠어?",
          "speaker_id": "2",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100007343.1.5",
          "form": "그 마저도 작년이라 아련하다ㅠ",
          "original_form": "그 마저도 작년이라 아련하다ㅠ",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100007343.1.6",
          "form": "엄청 재밌었어",
          "original_form": "엄청 재밌었어",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100007343.1.7",
          "form": "작년에 갔구나 뭐 봤어?",
          "original_form": "작년에 갔구나 뭐 봤어?",
          "speaker_id": "2",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100007343.1.8",
          "form": "거의 호캉스였어",
          "original_form": "거의 호캉스였어",
          "speaker_id": "1",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100007343.1.9",
          "form": "여유롭게 저녁만 나가서 먹고 드라이브하고 정도",
          "original_form": "여유롭게 저녁만 나가서 먹고 드라이브하고 정도",
          "speaker_id": "1",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100007343.1.10",
          "form": "힐링하기 좋았겠다",
          "original_form": "힐링하기 좋았겠다",
          "speaker_id": "2",
          "time": "20210822 20:01"
        },
        {
          "id": "MDRW2100007343.1.11",
          "form": "응 호캉스 가봤어?",
          "original_form": "응 호캉스 가봤어?",
          "speaker_id": "1",
          "time": "20210822 20:02"
        },
        {
          "id": "MDRW2100007343.1.12",
          "form": "해외 있을때 가봤어 호캉스라 부르긴 그랬지만 ㅋㅋ",
          "original_form": "해외 있을때 가봤어 호캉스라 부르긴 그랬지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:05"
        },
        {
          "id": "MDRW2100007343.1.13",
          "form": "해외는 호캉스 뭔가 아쉽지",
          "original_form": "해외는 호캉스 뭔가 아쉽지",
          "speaker_id": "1",
          "time": "20210822 20:07"
        },
        {
          "id": "MDRW2100007343.1.14",
          "form": "응 맞아 제대로된 게 없더라",
          "original_form": "응 맞아 제대로된 게 없더라",
          "speaker_id": "2",
          "time": "20210822 20:12"
        },
        {
          "id": "MDRW2100007343.1.15",
          "form": "거기다 이왕 출국이니까 왠지 더 구경하고싶고",
          "original_form": "거기다 이왕 출국이니까 왠지 더 구경하고싶고",
          "speaker_id": "1",
          "time": "20210822 20:16"
        },
        {
          "id": "MDRW2100007343.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
