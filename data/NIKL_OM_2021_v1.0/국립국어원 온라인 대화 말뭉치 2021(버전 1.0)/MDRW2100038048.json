          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100038048.1.4",
          "form": "바쁘나ㅋ",
          "original_form": "바쁘나ㅋ",
          "speaker_id": "2",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100038048.1.5",
          "form": "오늘 계속 손님있어서ㅠ",
          "original_form": "오늘 계속 손님있어서ㅠ",
          "speaker_id": "1",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100038048.1.6",
          "form": "힘들었다ㅠ",
          "original_form": "힘들었다ㅠ",
          "speaker_id": "1",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100038048.1.7",
          "form": "의자에 한번 못앉아봤네이;;",
          "original_form": "의자에 한번 못앉아봤네이;;",
          "speaker_id": "1",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100038048.1.8",
          "form": "아이고 ㅋㅋ 안그래도 톡없는거 보니 바쁜가보다 싶더라",
          "original_form": "아이고 ㅋㅋ 안그래도 톡없는거 보니 바쁜가보다 싶더라",
          "speaker_id": "2",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038048.1.9",
          "form": "고생했네",
          "original_form": "고생했네",
          "speaker_id": "2",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038048.1.10",
          "form": "이제 끝났어?",
          "original_form": "이제 끝났어?",
          "speaker_id": "2",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038048.1.11",
          "form": "집에 왔는데 와따 녹초네이;;",
          "original_form": "집에 왔는데 와따 녹초네이;;",
          "speaker_id": "1",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038048.1.12",
          "form": "울 큰애는 왜 집에 없노 ㅋㅋㅋㅋ",
          "original_form": "울 큰애는 왜 집에 없노 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038048.1.13",
          "form": "그러게 진짜 녹초겠다 ㅠㅠ",
          "original_form": "그러게 진짜 녹초겠다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038048.1.14",
          "form": "아고 ㅋㅋ 어디갔나?",
          "original_form": "아고 ㅋㅋ 어디갔나?",
          "speaker_id": "2",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038048.1.15",
          "form": "전화도 안받너..쩝..",
          "original_form": "전화도 안받너..쩝..",
          "speaker_id": "1",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100038048.1.16",
          "form": "갈수록 더하구먼 ㅋㅋㅋ",
          "original_form": "갈수록 더하구먼 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100038048.1.17",
          "form": "아고 ㅠㅠㅠㅠㅠㅠㅠ전화라두 받아야지; ㅠㅠㅋㅋㅋ",
          "original_form": "아고 ㅠㅠㅠㅠㅠㅠㅠ전화라두 받아야지; ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100038048.1.18",
          "form": "연락안되면 걱정된다이",
          "original_form": "연락안되면 걱정된다이",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100038048.1.19",
          "form": "무음해놓으니 지가 전화할때 아님",
          "original_form": "무음해놓으니 지가 전화할때 아님",
          "speaker_id": "1",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100038048.1.20",
          "form": "맞다 맞다",
          "original_form": "맞다 맞다",
          "speaker_id": "2",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100038048.1.21",
          "form": "진동해놓지 ㅠ",
          "original_form": "진동해놓지 ㅠ",
          "speaker_id": "2",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100038048.1.22",
          "form": "전화를 받지를.않넵",
          "original_form": "전화를 받지를.않넵",
          "speaker_id": "1",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100038048.1.23",
          "form": "방금 전화왔네 이제 온다고;;",
          "original_form": "방금 전화왔네 이제 온다고;;",
          "speaker_id": "1",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100038048.1.24",
          "form": "학원을 1시 전에 갔는데 ㅋㅋㅋㅋ",
          "original_form": "학원을 1시 전에 갔는데 ㅋㅋㅋㅋ",
