      "utterance": [
        {
          "id": "MDRW2100042626.1.1",
          "form": "닐하시는 건 어더세요?",
          "original_form": "닐하시는 건 어더세요?",
          "speaker_id": "2",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042626.1.2",
          "form": "일 힘들지만 좋아요",
          "original_form": "일 힘들지만 좋아요",
          "speaker_id": "1",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042626.1.3",
          "form": "다행이네요!!!ㅎㅎ 저는 일하다가 몸이아파서 쉬고 있답니다!",
          "original_form": "다행이네요!!!ㅎㅎ 저는 일하다가 몸이아파서 쉬고 있답니다!",
          "speaker_id": "2",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042626.1.4",
          "form": "그럼 아예 쉬시는 중인가요? 몸은 괜창ㅎ으신지..",
          "original_form": "그럼 아예 쉬시는 중인가요? 몸은 괜창ㅎ으신지..",
          "speaker_id": "1",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042626.1.5",
          "form": "네 ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ 몸은 괜찮고 이젠 취준중이네용",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ 몸은 괜찮고 이젠 취준중이네용",
          "speaker_id": "2",
          "time": "20210906 23:59"
        },
        {
          "id": "MDRW2100042626.1.6",
          "form": "어디러 취업하실 계획이세요",
          "original_form": "어디러 취업하실 계획이세요",
          "speaker_id": "1",
          "time": "20210906 23:59"
        },
        {
          "id": "MDRW2100042626.1.7",
          "form": "저 공시준비중에용!ㅜㅜ",
          "original_form": "저 공시준비중에용!ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042626.1.8",
          "form": "공시 힘들다던데 대단하시네요",
          "original_form": "공시 힘들다던데 대단하시네요",
          "speaker_id": "1",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042626.1.9",
          "form": "네ㅜㅜ 다들 열심히하고있으니 저도 더 힘내야죠",
          "original_form": "네ㅜㅜ 다들 열심히하고있으니 저도 더 힘내야죠",
          "speaker_id": "2",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042626.1.10",
          "form": "내년에 붙으시면 좋겠네요",
          "original_form": "내년에 붙으시면 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042626.1.11",
          "form": "그러게요ㅜ 꼭 붙으면 좋겠어요",
          "original_form": "그러게요ㅜ 꼭 붙으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042626.1.12",
          "form": "저는 힘들어도 이 일이 넘  하고싶었거든요",
          "original_form": "저는 힘들어도 이 일이 넘  하고싶었거든요",
          "speaker_id": "2",
          "time": "20210907 00:01"
        },
        {
          "id": "MDRW2100042626.1.13",
          "form": "직렬도 정하셨나요?",
          "original_form": "직렬도 정하셨나요?",
          "speaker_id": "1",
          "time": "20210907 00:01"
        },
        {
          "id": "MDRW2100042626.1.14",
          "form": "검찰직이요 너무 하고싶어요ㅠ ㅠ 진짜로요",
          "original_form": "검찰직이요 너무 하고싶어요ㅠ ㅠ 진짜로요",
          "speaker_id": "2",
