      "utterance": [
        {
          "id": "MDRW2100015644.1.1",
          "form": "또 무슨연예인이있을까요ㅎㅎ",
          "original_form": "또 무슨연예인이있을까요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015644.1.2",
          "form": "이상민하면 탁재훈인가요?ㅎㅎ",
          "original_form": "이상민하면 탁재훈인가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015644.1.3",
          "form": "음 ~~~~~~",
          "original_form": "음 ~~~~~~",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015644.1.4",
          "form": "탁재훈은 왠지... 비호감은 아닌데",
          "original_form": "탁재훈은 왠지... 비호감은 아닌데",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015644.1.5",
          "form": "음...ㅎㅎㅎㅎㅎ",
          "original_form": "음...ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015644.1.6",
          "form": "탁재훈하니 컨추리꼬꼬가",
          "original_form": "탁재훈하니 컨추리꼬꼬가",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015644.1.7",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015644.1.8",
          "form": "진짜,대단했어요ㅎㅎ",
          "original_form": "진짜,대단했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015644.1.9",
          "form": "크리스마스~",
          "original_form": "크리스마스~",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015644.1.10",
          "form": "ㅋㅋㅋㅋ 오해피 붐붐붐",
          "original_form": "ㅋㅋㅋㅋ 오해피 붐붐붐",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015644.1.11",
          "form": "ㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015644.1.12",
          "form": "너무 좋았져",
          "original_form": "너무 좋았져",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015644.1.13",
          "form": "아마 컨추리꼬꼬가 그당시아이돌들 다제쳤을껄요?",
          "original_form": "아마 컨추리꼬꼬가 그당시아이돌들 다제쳤을껄요?",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015644.1.14",
          "form": "말빨이 ㅋㅋ",
          "original_form": "말빨이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015644.1.15",
          "form": "둘이 정말 재밌었져ㅎㅎ",
          "original_form": "둘이 정말 재밌었져ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015644.1.16",
          "form": "근데 끝이 다들 아쉽네요 ㅎ",
          "original_form": "근데 끝이 다들 아쉽네요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015644.1.17",
          "form": "신정환도ㅎㅎ 탁재훈도ㅎ",
          "original_form": "신정환도ㅎㅎ 탁재훈도ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015644.1.18",
          "form": "이궁 잘못된선탯 ㅜㅜ",
          "original_form": "이궁 잘못된선탯 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015644.1.19",
          "form": "안탑깝졍",
          "original_form": "안탑깝졍",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015644.1.20",
          "form": "ㅎㅎ 본인의 선택이니깐요ㅎ",
          "original_form": "ㅎㅎ 본인의 선택이니깐요ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015644.1.21",
          "form": "둘다 도박을... ㅍ",
          "original_form": "둘다 도박을... ㅍ",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015644.1.22",
          "form": "틱재훈은 이수근이랑 스포츠 도박",
          "original_form": "틱재훈은 이수근이랑 스포츠 도박",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015644.1.23",
          "form": "신정환은 원정도박에ㅎ",
          "original_form": "신정환은 원정도박에ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015644.1.24",
          "form": "흑흑 ㅜㅜ",
          "original_form": "흑흑 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015644.1.25",
          "form": "아쉬운 일이에요",
          "original_form": "아쉬운 일이에요",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015644.1.26",
          "form": "다른것도 많은데 하필",
          "original_form": "다른것도 많은데 하필",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015644.1.27",
          "form": "그니깐요ㅍ",
          "original_form": "그니깐요ㅍ",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015644.1.28",
          "form": "도박은 못끊ㅇ어ㅛㅎ",
          "original_form": "도박은 못끊ㅇ어ㅛㅎ",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015644.1.29",
          "form": "지금은 그래도 정신차려서 다행인거 같아요",
          "original_form": "지금은 그래도 정신차려서 다행인거 같아요",
          "speaker_id": "1",
          "time": "20210826 00:06"
        }
      ]
