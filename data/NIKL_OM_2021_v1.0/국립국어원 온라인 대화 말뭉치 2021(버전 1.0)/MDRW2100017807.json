      "utterance": [
        {
          "id": "MDRW2100017807.1.1",
          "form": "곰표 콜라보 한 음식 보셨나요?",
          "original_form": "곰표 콜라보 한 음식 보셨나요?",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017807.1.2",
          "form": "곰표 맥주는 마셔봤어요!",
          "original_form": "곰표 맥주는 마셔봤어요!",
          "speaker_id": "2",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017807.1.3",
          "form": "앗 전 아직 곰표 맥주는 마셔보지 못했는데",
          "original_form": "앗 전 아직 곰표 맥주는 마셔보지 못했는데",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100017807.1.4",
          "form": "맛이 어떤가용??",
          "original_form": "맛이 어떤가용??",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100017807.1.5",
          "form": "일반 맥주보다 산뜻하고 끝 맛이 좋았어여!",
          "original_form": "일반 맥주보다 산뜻하고 끝 맛이 좋았어여!",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100017807.1.6",
          "form": "오옹 한번 마셔봐야겠어요",
          "original_form": "오옹 한번 마셔봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100017807.1.7",
          "form": "곰표 나쵸도 드셔보셨나요??",
          "original_form": "곰표 나쵸도 드셔보셨나요??",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100017807.1.8",
          "form": "그냥 과자였나ㅎㅎ",
          "original_form": "그냥 과자였나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100017807.1.9",
          "form": "곰표는 맥주만 마셔봤어요ㅠ",
          "original_form": "곰표는 맥주만 마셔봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100017807.1.10",
          "form": "아ㅏ",
          "original_form": "아ㅏ",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100017807.1.11",
          "form": "전 친구가 가지고 있길래 먹어봤는데",
          "original_form": "전 친구가 가지고 있길래 먹어봤는데",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100017807.1.12",
          "form": "맛이 괜찮더라구요",
          "original_form": "맛이 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100017807.1.13",
          "form": "팝콘 보다는 나쵸를 먹어봐야겠네요ㅎㅎ",
          "original_form": "팝콘 보다는 나쵸를 먹어봐야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100017807.1.14",
          "form": "아 곰표 팝콘이 있었나요??",
          "original_form": "아 곰표 팝콘이 있었나요??",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100017807.1.15",
          "form": "곰표 팝콘도 있대요!",
          "original_form": "곰표 팝콘도 있대요!",
          "speaker_id": "2",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100017807.1.16",
          "form": "우왕 오리지널 맛인가요?",
          "original_form": "우왕 오리지널 맛인가요?",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100017807.1.17",
          "form": "그런가봐요!",
          "original_form": "그런가봐요!",
          "speaker_id": "2",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100017807.1.18",
          "form": "두껍 진로는 마셔보셨나용?",
          "original_form": "두껍 진로는 마셔보셨나용?",
          "speaker_id": "1",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100017807.1.19",
          "form": "네네! 근데 소주 맛은 아직 잘 모르겠더라구요ㅠ",
          "original_form": "네네! 근데 소주 맛은 아직 잘 모르겠더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 18:28"
        }
      ]
