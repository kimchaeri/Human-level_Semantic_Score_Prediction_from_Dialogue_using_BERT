      "utterance": [
        {
          "id": "MDRW2100042135.1.1",
          "form": "스팸좋아하세요?",
          "original_form": "스팸좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100042135.1.2",
          "form": "먹는 햄 말하시는거죠?",
          "original_form": "먹는 햄 말하시는거죠?",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100042135.1.3",
          "form": "네네 통조림햄요!",
          "original_form": "네네 통조림햄요!",
          "speaker_id": "1",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100042135.1.4",
          "form": "맛있잖아요 좀 짜긴하지만",
          "original_form": "맛있잖아요 좀 짜긴하지만",
          "speaker_id": "2",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100042135.1.5",
          "form": "밥반찬으로 좋지요!",
          "original_form": "밥반찬으로 좋지요!",
          "speaker_id": "1",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100042135.1.6",
          "form": "스팸 좋아하시나봐요",
          "original_form": "스팸 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100042135.1.7",
          "form": "네 저는 부대찌개에 넣는 거 좋아해요",
          "original_form": "네 저는 부대찌개에 넣는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100042135.1.8",
          "form": "부대찌개에 넣으면 맛있죠 밥 한그릇 뚝딱",
          "original_form": "부대찌개에 넣으면 맛있죠 밥 한그릇 뚝딱",
          "speaker_id": "2",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100042135.1.9",
          "form": "아 오늘 날씨가 꿉꿉해서 부대찌개 땡겨요ㅠ",
          "original_form": "아 오늘 날씨가 꿉꿉해서 부대찌개 땡겨요ㅠ",
          "speaker_id": "1",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100042135.1.10",
          "form": "요즘 날이 흐려서 뜨끈하고 매운음식이 먹고싶긴해요.",
          "original_form": "요즘 날이 흐려서 뜨끈하고 매운음식이 먹고싶긴해요.",
          "speaker_id": "2",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100042135.1.11",
          "form": "저는 부대찌개에 스팸도 넣고 비엔나도 넣어요",
          "original_form": "저는 부대찌개에 스팸도 넣고 비엔나도 넣어요",
          "speaker_id": "1",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100042135.1.12",
          "form": "완전 햄찌개네요!!",
          "original_form": "완전 햄찌개네요!!",
          "speaker_id": "2",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100042135.1.13",
          "form": "그렇긴한데 맛있어용ㅎ",
          "original_form": "그렇긴한데 맛있어용ㅎ",
          "speaker_id": "1",
          "time": "20210907 15:36"
        }
