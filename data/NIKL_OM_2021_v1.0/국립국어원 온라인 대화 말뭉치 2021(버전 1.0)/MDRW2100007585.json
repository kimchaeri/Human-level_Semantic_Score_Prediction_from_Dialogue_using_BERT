      "utterance": [
        {
          "id": "MDRW2100007585.1.1",
          "form": "타코야키 재료는 뭘 쓰세요?",
          "original_form": "타코야키 재료는 뭘 쓰세요?",
          "speaker_id": "2",
          "time": "20210824 01:48"
        },
        {
          "id": "MDRW2100007585.1.2",
          "form": "타코야끼는 양배추랑 문어랑",
          "original_form": "타코야끼는 양배추랑 문어랑",
          "speaker_id": "1",
          "time": "20210824 01:54"
        },
        {
          "id": "MDRW2100007585.1.3",
          "form": "옥수수콘이랑 반죽이용",
          "original_form": "옥수수콘이랑 반죽이용",
          "speaker_id": "1",
          "time": "20210824 01:54"
        },
        {
          "id": "MDRW2100007585.1.4",
          "form": "양배추랑 뮨어...메모...",
          "original_form": "양배추랑 뮨어...메모...",
          "speaker_id": "2",
          "time": "20210824 01:54"
        },
        {
          "id": "MDRW2100007585.1.5",
          "form": "그래서 양배추 샀어요",
          "original_form": "그래서 양배추 샀어요",
          "speaker_id": "1",
          "time": "20210824 01:54"
        },
        {
          "id": "MDRW2100007585.1.6",
          "form": "우와 타코야끼 만드세요?",
          "original_form": "우와 타코야끼 만드세요?",
          "speaker_id": "2",
          "time": "20210824 01:55"
        },
        {
          "id": "MDRW2100007585.1.7",
          "form": "타코야끼 소스 꼭 뿌리고",
          "original_form": "타코야끼 소스 꼭 뿌리고",
          "speaker_id": "1",
          "time": "20210824 01:55"
        },
        {
          "id": "MDRW2100007585.1.8",
          "form": "소스필요하죠",
          "original_form": "소스필요하죠",
          "speaker_id": "2",
          "time": "20210824 01:55"
        },
        {
          "id": "MDRW2100007585.1.9",
          "form": "마요네즈는 선택",
          "original_form": "마요네즈는 선택",
          "speaker_id": "1",
          "time": "20210824 01:55"
        },
        {
          "id": "MDRW2100007585.1.10",
          "form": "가스오부시는 꼭꼭마니마니",
          "original_form": "가스오부시는 꼭꼭마니마니",
          "speaker_id": "1",
          "time": "20210824 01:55"
        },
        {
          "id": "MDRW2100007585.1.11",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210824 01:55"
        },
        {
          "id": "MDRW2100007585.1.12",
          "form": "가스오부시X 참다랑어포O",
          "original_form": "가스오부시X 참다랑어포O",
          "speaker_id": "1",
          "time": "20210824 01:55"
        },
        {
          "id": "MDRW2100007585.1.13",
          "form": "저 일본놈 아닙니다",
          "original_form": "저 일본놈 아닙니다",
          "speaker_id": "1",
          "time": "20210824 01:55"
        },
        {
          "id": "MDRW2100007585.1.14",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "2",
          "time": "20210824 01:55"
        },
        {
          "id": "MDRW2100007585.1.15",
          "form": "명심할게요",
          "original_form": "명심할게요",
          "speaker_id": "2",
          "time": "20210824 01:56"
        },
        {
          "id": "MDRW2100007585.1.16",
          "form": "네네 일본 물건도 안산답니다",
          "original_form": "네네 일본 물건도 안산답니다",
          "speaker_id": "1",
          "time": "20210824 01:56"
        },
        {
          "id": "MDRW2100007585.1.17",
          "form": "요즘 뭐 사고싶으세요",
          "original_form": "요즘 뭐 사고싶으세요",
          "speaker_id": "1",
          "time": "20210824 01:56"
        },
        {
          "id": "MDRW2100007585.1.18",
          "form": "저는 옷사려구요",
          "original_form": "저는 옷사려구요",
          "speaker_id": "2",
          "time": "20210824 01:57"
        },
        {
          "id": "MDRW2100007585.1.19",
          "form": "쿠폰써야해요",
          "original_form": "쿠폰써야해요",
          "speaker_id": "2",
          "time": "20210824 01:57"
        },
        {
          "id": "MDRW2100007585.1.20",
          "form": "무슨쿠폰이요??",
          "original_form": "무슨쿠폰이요??",
          "speaker_id": "1",
          "time": "20210824 02:01"
        },
        {
          "id": "MDRW2100007585.1.21",
          "form": "무신사 20퍼 쿠폰이욥",
          "original_form": "무신사 20퍼 쿠폰이욥",
          "speaker_id": "2",
          "time": "20210824 02:03"
        },
        {
          "id": "MDRW2100007585.1.22",
          "form": "아아 무신사에서 구매하세요??",
          "original_form": "아아 무신사에서 구매하세요??",
          "speaker_id": "1",
          "time": "20210824 02:04"
        },
        {
          "id": "MDRW2100007585.1.23",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210824 02:04"
        },
        {
          "id": "MDRW2100007585.1.24",
          "form": "무신사가 옷사기는 좋죠",
          "original_form": "무신사가 옷사기는 좋죠",
