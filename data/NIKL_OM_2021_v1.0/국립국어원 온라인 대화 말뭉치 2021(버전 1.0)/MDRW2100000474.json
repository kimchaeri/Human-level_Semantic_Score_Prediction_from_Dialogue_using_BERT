      "utterance": [
        {
          "id": "MDRW2100000474.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000474.1.2",
          "form": "반가워여",
          "original_form": "반가워여",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000474.1.3",
          "form": "여름휴가 다녀오셨나요",
          "original_form": "여름휴가 다녀오셨나요",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000474.1.4",
          "form": "아뇨ㅠㅠ 코로나가 심해져서 집에서 열심히 여름 보냈어요ㅎㅎ",
          "original_form": "아뇨ㅠㅠ 코로나가 심해져서 집에서 열심히 여름 보냈어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000474.1.5",
          "form": "휴가 다녀오신 곳 있으세요?",
          "original_form": "휴가 다녀오신 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000474.1.6",
          "form": "저두 계속 집.. ㅋㅋ",
          "original_form": "저두 계속 집.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000474.1.7",
          "form": "친구가 통영사는데 이번주말에 놀러갈거긴해요 ㅎㅎ",
          "original_form": "친구가 통영사는데 이번주말에 놀러갈거긴해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000474.1.8",
          "form": "통영 여행 가보신적있나요",
          "original_form": "통영 여행 가보신적있나요",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000474.1.9",
          "form": "헛 통영!! 전 한번도 가본적 없어요ㅠㅠㅠ",
          "original_form": "헛 통영!! 전 한번도 가본적 없어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000474.1.10",
          "form": "친구가 통영 동피랑마을이 예쁘대서",
          "original_form": "친구가 통영 동피랑마을이 예쁘대서",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000474.1.11",
          "form": "가보고싶어요",
          "original_form": "가보고싶어요",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000474.1.12",
          "form": "제가 재밌게 본 영화 촬영지 이기도하고ㅎㅎ",
          "original_form": "제가 재밌게 본 영화 촬영지 이기도하고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000474.1.13",
          "form": "우와ㅠㅠㅠ 이쁘겠다...ㅠㅠ 나중에 꼭 가셨으면 좋겠어요!",
          "original_form": "우와ㅠㅠㅠ 이쁘겠다...ㅠㅠ 나중에 꼭 가셨으면 좋겠어요!",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000474.1.14",
          "form": "저는 태어나서 서울을 한번도 안가봐서 여행을 꼭 간다면 서울 먼저 가보고싶어요",
          "original_form": "저는 태어나서 서울을 한번도 안가봐서 여행을 꼭 간다면 서울 먼저 가보고싶어요",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000474.1.15",
          "form": "저도 지방에 살아서 서울은 다 커서 가봤어요ㅋㅋㅋㅋ",
          "original_form": "저도 지방에 살아서 서울은 다 커서 가봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000474.1.16",
          "form": "티비에서만 보던곳 가보니까 넘나 신기하구ㅋㅋㅋ",
          "original_form": "티비에서만 보던곳 가보니까 넘나 신기하구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000474.1.17",
          "form": "아 그쵸ㅠㅠㅠ 롯데타워 생기기 전 꺼지는 남산타워 가는게 소원이고 그랬는데...ㅠ 꼭 여행 가보고싶더라구요..",
          "original_form": "아 그쵸ㅠㅠㅠ 롯데타워 생기기 전 꺼지는 남산타워 가는게 소원이고 그랬는데...ㅠ 꼭 여행 가보고싶더라구요..",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000474.1.18",
          "form": "롯데타워 남산타워도 좋구",
          "original_form": "롯데타워 남산타워도 좋구",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000474.1.19",
          "form": "서울은 구석구석 예쁜동네도 많아서",
          "original_form": "서울은 구석구석 예쁜동네도 많아서",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000474.1.20",
          "form": "저도 또 가고싶어욤ㅎㅎ",
          "original_form": "저도 또 가고싶어욤ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000474.1.21",
          "form": "맞아요... 약간 고층 건물이 많은 길을 나도 한번 걸어보고싶다 하는 환상이 있어서ㅋㅎㅋㅎㅋ 서울 거리를 너무 좋아해요..",
          "original_form": "맞아요... 약간 고층 건물이 많은 길을 나도 한번 걸어보고싶다 하는 환상이 있어서ㅋㅎㅋㅎㅋ 서울 거리를 너무 좋아해요..",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000474.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그맘알아요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그맘알아요!!",
          "speaker_id": "2",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000474.1.23",
          "form": "앟ㅋㅋㅋㅋ 그쵸ㅠ 진짜 꼭 가보는게 소원이에요...",
          "original_form": "앟ㅋㅋㅋㅋ 그쵸ㅠ 진짜 꼭 가보는게 소원이에요...",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000474.1.24",
          "form": "높은곳 가보고싶어서 63빌딩 전망대랑 아쿠아리움도 갔었어요 ㅋㅋㅋㅋㅋ",
          "original_form": "높은곳 가보고싶어서 63빌딩 전망대랑 아쿠아리움도 갔었어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000474.1.25",
          "form": "허어어 부러워라... 저도 63빌딩이랑 롯데타워 전망대 꼭 가고싶었거든요..ㅠ",
          "original_form": "허어어 부러워라... 저도 63빌딩이랑 롯데타워 전망대 꼭 가고싶었거든요..ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000474.1.26",
          "form": "딱 노을질 때 바라보면... 크흐..",
          "original_form": "딱 노을질 때 바라보면... 크흐..",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000474.1.27",
          "form": "전망대 입장료있어서",
          "original_form": "전망대 입장료있어서",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000474.1.28",
          "form": "갔을때 쪼끔 당황했어요 사실ㅋㅋㅋㅋㅋ\r\n입장료있는지 몰랐거든요...",
          "original_form": "갔을때 쪼끔 당황했어요 사실ㅋㅋㅋㅋㅋ\r\n입장료있는지 몰랐거든요...",
