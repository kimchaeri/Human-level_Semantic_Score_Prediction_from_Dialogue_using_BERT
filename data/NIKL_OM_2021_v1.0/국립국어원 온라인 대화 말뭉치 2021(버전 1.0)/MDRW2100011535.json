      "utterance": [
        {
          "id": "MDRW2100011535.1.1",
          "form": "언니~나 폰을 바꿨는데~",
          "original_form": "언니~나 폰을 바꿨는데~",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011535.1.2",
          "form": "폰에 홈트 어플이 있더라구요~",
          "original_form": "폰에 홈트 어플이 있더라구요~",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011535.1.3",
          "form": "세상 신기했네요~",
          "original_form": "세상 신기했네요~",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011535.1.4",
          "form": "홈트좋지ㅋㅋ",
          "original_form": "홈트좋지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100011535.1.5",
          "form": "나도 온라인으로 해볼까?",
          "original_form": "나도 온라인으로 해볼까?",
          "speaker_id": "2",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100011535.1.6",
          "form": "내핸드폰도 되는가?",
          "original_form": "내핸드폰도 되는가?",
          "speaker_id": "2",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100011535.1.7",
          "form": "모르겠네요ㅋㅋㅋ",
          "original_form": "모르겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011535.1.8",
          "form": "나 유튜브로 요가 했었는데",
          "original_form": "나 유튜브로 요가 했었는데",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011535.1.9",
          "form": "이거 좀만 힘들면 안하게 되서 ㅋㅋㅋㅋ",
          "original_form": "이거 좀만 힘들면 안하게 되서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011535.1.10",
          "form": "유튜브는 몇개나 봤다 한국꺼 외국꺼",
          "original_form": "유튜브는 몇개나 봤다 한국꺼 외국꺼",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011535.1.11",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011535.1.12",
          "form": "잘안되드라",
          "original_form": "잘안되드라",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011535.1.13",
          "form": "그러니깐요~이게 내 자세도 교정해 주고 해야되는데 ㅋㅋㅋㅋ",
          "original_form": "그러니깐요~이게 내 자세도 교정해 주고 해야되는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011535.1.14",
          "form": "땅끄부부??",
          "original_form": "땅끄부부??",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011535.1.15",
          "form": "그거는 걍 대충따라해도 땀 쭉 나데요~",
          "original_form": "그거는 걍 대충따라해도 땀 쭉 나데요~",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011535.1.16",
          "form": "땅끄부부도 하고",
          "original_form": "땅끄부부도 하고",
          "speaker_id": "2",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011535.1.17",
          "form": "다해봄",
          "original_form": "다해봄",
          "speaker_id": "2",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011535.1.18",
          "form": "ㅋㅋㅋ언니 나름 노력했네요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ언니 나름 노력했네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011535.1.19",
          "form": "요가 필라테스 요런 섬세한거는 가서 해야겠더라구요 ㅋㅋㅋ",
          "original_form": "요가 필라테스 요런 섬세한거는 가서 해야겠더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011535.1.20",
          "form": "그래 자세잘못하면 큰일난다",
          "original_form": "그래 자세잘못하면 큰일난다",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011535.1.21",
          "form": "나이먹고 잘못하면 돈더든다",
          "original_form": "나이먹고 잘못하면 돈더든다",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011535.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요~뼈 돌아간다며 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요~뼈 돌아간다며 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011535.1.23",
          "form": "요가하고픈데ㅠ",
          "original_form": "요가하고픈데ㅠ",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011535.1.24",
          "form": "땅끄부부나 해야겠어요 ㅋㅋㅋ",
          "original_form": "땅끄부부나 해야겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011535.1.25",
          "form": "그래 땅끄부부ㅋㅋ",
          "original_form": "그래 땅끄부부ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011535.1.26",
          "form": "난 나자신과의 싸움에 매번진자ㅋ",
          "original_form": "난 나자신과의 싸움에 매번진자ㅋ",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011535.1.27",
          "form": "ㅋㅋㅋㅋㅋ자신과는 싸우는거 아녀요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ자신과는 싸우는거 아녀요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100011535.1.28",
          "form": "자신과는 타협만이 ㅋㅋㅋㅋ",
          "original_form": "자신과는 타협만이 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100011535.1.29",
          "form": "ㅋㅋ 그래 타협ㅋㅋ",
          "original_form": "ㅋㅋ 그래 타협ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100011535.1.30",
          "form": "우리 함께 타협합시당^^",
          "original_form": "우리 함께 타협합시당^^",
          "speaker_id": "1",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100011535.1.31",
          "form": "온라인으로라도 한번 해보아요 ㅋㅋㅋ",
          "original_form": "온라인으로라도 한번 해보아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:27"
        },
        {
          "id": "MDRW2100011535.1.32",
          "form": "그래 뭐 ㅎ 뭐",
          "original_form": "그래 뭐 ㅎ 뭐",
          "speaker_id": "2",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100011535.1.33",
          "form": "노력해볼께ㅋㅋ",
          "original_form": "노력해볼께ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100011535.1.34",
