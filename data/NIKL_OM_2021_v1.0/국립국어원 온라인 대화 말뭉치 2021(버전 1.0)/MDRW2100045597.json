      "utterance": [
        {
          "id": "MDRW2100045597.1.1",
          "form": "name2님 영화는 좋아하세요?",
          "original_form": "&name2&님 영화는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045597.1.2",
          "form": "영화는 보고 싶은게 생기면 보러가는 편이에요!",
          "original_form": "영화는 보고 싶은게 생기면 보러가는 편이에요!",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045597.1.3",
          "form": "name1님은 좋아하시나요?",
          "original_form": "&name1&님은 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045597.1.4",
          "form": "네 영화 좋아해요",
          "original_form": "네 영화 좋아해요",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045597.1.5",
          "form": "인생영화잇으세요?",
          "original_form": "인생영화잇으세요?",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045597.1.6",
          "form": "아뇨ㅠㅠ 아직 없어요ㅠㅠ",
          "original_form": "아뇨ㅠㅠ 아직 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045597.1.7",
          "form": "그럼 재밋게 본 영화는요? ㅋㅋ",
          "original_form": "그럼 재밋게 본 영화는요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045597.1.8",
          "form": "전에 바람의 검신이랑 해리포터요!",
          "original_form": "전에 바람의 검신이랑 해리포터요!",
          "speaker_id": "2",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100045597.1.9",
          "form": "해리포터는 불의잔에서 보다 말았지만욬ㅋㅋㅋ",
          "original_form": "해리포터는 불의잔에서 보다 말았지만욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100045597.1.10",
          "form": "해리포터 재밋죠 ㅋㅋㅋㅋ",
          "original_form": "해리포터 재밋죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100045597.1.11",
          "form": "마블은 보시나요?",
          "original_form": "마블은 보시나요?",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045597.1.12",
          "form": "마블은 스파이더맨만 봤어요",
          "original_form": "마블은 스파이더맨만 봤어요",
          "speaker_id": "2",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045597.1.13",
          "form": "마블인지도 모르고 봤네욬ㅋㅋㅋㅋ",
          "original_form": "마블인지도 모르고 봤네욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:08"
        },
        {
          "id": "MDRW2100045597.1.14",
          "form": "아 스파이더맨 보셧군요 ㅋㅋㅋㅋ",
          "original_form": "아 스파이더맨 보셧군요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:08"
        },
        {
          "id": "MDRW2100045597.1.15",
          "form": "저는 반지의제왕 시리즈 좋아햇어요",
          "original_form": "저는 반지의제왕 시리즈 좋아햇어요",
          "speaker_id": "1",
          "time": "20210908 04:08"
        },
        {
          "id": "MDRW2100045597.1.16",
          "form": "네! 스파이더맨은 배우 바뀐고 다 본 것같아욬ㅋㅋㅋ",
          "original_form": "네! 스파이더맨은 배우 바뀐고 다 본 것같아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:12"
        },
        {
          "id": "MDRW2100045597.1.17",
          "form": "어쩌다보닠ㅋㅋㅋ",
          "original_form": "어쩌다보닠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:12"
        },
        {
          "id": "MDRW2100045597.1.18",
          "form": "반지의 제왕이 그렇게 명작이라고 들었어요!!",
          "original_form": "반지의 제왕이 그렇게 명작이라고 들었어요!!",
          "speaker_id": "2",
          "time": "20210908 04:12"
        },
        {
          "id": "MDRW2100045597.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ전 거의 안봣어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ전 거의 안봣어요",
          "speaker_id": "1",
          "time": "20210908 04:12"
        },
        {
          "id": "MDRW2100045597.1.20",
          "form": "네 반지의제왕 명작이에요 ㅠㅠ",
          "original_form": "네 반지의제왕 명작이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 04:31"
        },
        {
          "id": "MDRW2100045597.1.21",
          "form": "추천추천",
          "original_form": "추천추천",
          "speaker_id": "1",
          "time": "20210908 04:31"
        },
        {
          "id": "MDRW2100045597.1.22",
          "form": "나중에 시간내서 각잡고 꼭 볼게요ㅠㅠ",
          "original_form": "나중에 시간내서 각잡고 꼭 볼게요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045597.1.23",
          "form": "어떻길래 다들 그렇게 명작이라고 하는지 궁금해지기작했어요ㅠㅠ",
          "original_form": "어떻길래 다들 그렇게 명작이라고 하는지 궁금해지기작했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045597.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ네 추천해요 후회안하실거예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ네 추천해요 후회안하실거예요",
          "speaker_id": "1",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045597.1.25",
          "form": "보면서 먹을 팝콘이랑 음료 준비해두고 블라인드치고 봐야겠어요ㅠㅠ 갑저기 엄청 당기네욬ㅋㅋㅋ",
          "original_form": "보면서 먹을 팝콘이랑 음료 준비해두고 블라인드치고 봐야겠어요ㅠㅠ 갑저기 엄청 당기네욬ㅋㅋㅋ",
