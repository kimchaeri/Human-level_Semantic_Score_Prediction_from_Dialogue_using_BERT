      "utterance": [
        {
          "id": "MDRW2100006487.1.1",
          "form": "마저 진짜",
          "original_form": "마저 진짜",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006487.1.2",
          "form": "봐야될게 너무 많아서",
          "original_form": "봐야될게 너무 많아서",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006487.1.3",
          "form": "저 절대 못 죽어요ㅜ",
          "original_form": "저 절대 못 죽어요ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006487.1.4",
          "form": "돌려보내줘요ㅜㅜ 해야할판ㅋㅋㅋ",
          "original_form": "돌려보내줘요ㅜㅜ 해야할판ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006487.1.5",
          "form": "저 그럼 못해도 결말 스포라도 해줘요",
          "original_form": "저 그럼 못해도 결말 스포라도 해줘요",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006487.1.6",
          "form": "내가 담아논거 다 알려주기 전까진 나 못 가니까 다 해줘야해요",
          "original_form": "내가 담아논거 다 알려주기 전까진 나 못 가니까 다 해줘야해요",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100006487.1.7",
          "form": "ㅇㅇ... 다 알려줘야함 속편 외전까지 싹다",
          "original_form": "ㅇㅇ... 다 알려줘야함 속편 외전까지 싹다",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100006487.1.8",
          "form": "숨겨진 설정들도 포기못해 ㅋㅋㅋ",
          "original_form": "숨겨진 설정들도 포기못해 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100006487.1.9",
          "form": "ㅇㅇㅇ 다 내놔 절대 하나도 포기 못해",
          "original_form": "ㅇㅇㅇ 다 내놔 절대 하나도 포기 못해",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100006487.1.10",
          "form": "무족권",
          "original_form": "무족권",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100006487.1.11",
          "form": "양손에 가득 쥐고도 모자라다",
          "original_form": "양손에 가득 쥐고도 모자라다",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100006487.1.12",
          "form": "지금 양팔이 터지게 안ㄴ고 있는걸",
          "original_form": "지금 양팔이 터지게 안ㄴ고 있는걸",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100006487.1.13",
          "form": "이미 줄줄 흘러넘치고 있음ㅋㅋㅋ",
          "original_form": "이미 줄줄 흘러넘치고 있음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100006487.1.14",
          "form": "나랑 놀자",
          "original_form": "나랑 놀자",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100006487.1.15",
          "form": "이리와아아",
          "original_form": "이리와아아",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100006487.1.16",
          "form": "부에에에ㅔㅔㅇ",
          "original_form": "부에에에ㅔㅔㅇ",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100006487.1.17",
          "form": "아 그게 뭐에요...ㅋㅋㅋ",
          "original_form": "아 그게 뭐에요...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100006487.1.18",
          "form": "오늘도 기무는 물건너갔다며",
          "original_form": "오늘도 기무는 물건너갔다며",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100006487.1.19",
          "form": "아 내 기무..",
          "original_form": "아 내 기무..",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100006487.1.20",
          "form": "ㅇㅇ.. 완전 망했어 ㅠㅠ",
          "original_form": "ㅇㅇ.. 완전 망했어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100006487.1.21",
