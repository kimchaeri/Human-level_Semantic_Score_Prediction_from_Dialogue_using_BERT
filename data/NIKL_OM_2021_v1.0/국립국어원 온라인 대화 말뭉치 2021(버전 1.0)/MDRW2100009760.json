        },
        {
          "id": "MDRW2100009760.1.2",
          "form": "요즘 무슨영화가 핫한가요?",
          "original_form": "요즘 무슨영화가 핫한가요?",
          "speaker_id": "1",
          "time": "20210823 09:33"
        },
        {
          "id": "MDRW2100009760.1.3",
          "form": "싱크홀 재밌더라구요 ㅎㅎ",
          "original_form": "싱크홀 재밌더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 09:52"
        },
        {
          "id": "MDRW2100009760.1.4",
          "form": "아 새로개봉한거군요 저는 그거말고 모가디슈봤어요",
          "original_form": "아 새로개봉한거군요 저는 그거말고 모가디슈봤어요",
          "speaker_id": "1",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009760.1.5",
          "form": "아직 못봤네요 거거는",
          "original_form": "아직 못봤네요 거거는",
          "speaker_id": "1",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009760.1.6",
          "form": "싱크홀에 어떤배우가 나오나요",
          "original_form": "싱크홀에 어떤배우가 나오나요",
          "speaker_id": "1",
          "time": "20210823 09:54"
        },
        {
          "id": "MDRW2100009760.1.7",
          "form": "차승원이랑 이광수 나와요 ㅎㅎ",
          "original_form": "차승원이랑 이광수 나와요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009760.1.8",
          "form": "최근 개봉 맞아요 코믹 소재입니다",
          "original_form": "최근 개봉 맞아요 코믹 소재입니다",
          "speaker_id": "2",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009760.1.9",
          "form": "재밌더라구요",
          "original_form": "재밌더라구요",
          "speaker_id": "2",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009760.1.10",
          "form": "와우 재밌겠다",
          "original_form": "와우 재밌겠다",
          "speaker_id": "1",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009760.1.11",
          "form": "헌혈하면 영화티켓 두장씩주는데",
          "original_form": "헌혈하면 영화티켓 두장씩주는데",
          "speaker_id": "1",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009760.1.12",
          "form": "봐야겠네요",
          "original_form": "봐야겠네요",
          "speaker_id": "1",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100009760.1.13",
          "form": "개그소재좋아하세요?",
          "original_form": "개그소재좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100009760.1.14",
          "form": "네 ㅋㅋㅋㅋ 좀 코믹이 잘 맞아요",
          "original_form": "네 ㅋㅋㅋㅋ 좀 코믹이 잘 맞아요",
          "speaker_id": "2",
          "time": "20210823 12:42"
        },
        {
          "id": "MDRW2100009760.1.15",
          "form": "영화는 대부분 코믹이나 액션위주로 보는거같아요",
          "original_form": "영화는 대부분 코믹이나 액션위주로 보는거같아요",
          "speaker_id": "2",
          "time": "20210823 12:42"
        },
        {
          "id": "MDRW2100009760.1.16",
          "form": "저도 마블이 좋아요 ㅎㅎ",
          "original_form": "저도 마블이 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 12:45"
        },
        {
          "id": "MDRW2100009760.1.17",
          "form": "극한직업하고 엑시트도 좋았어요",
          "original_form": "극한직업하고 엑시트도 좋았어요",
          "speaker_id": "1",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009760.1.18",
          "form": "저 또한 마블팬이랍니다 ㅎㅎ",
          "original_form": "저 또한 마블팬이랍니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100009760.1.19",
          "form": "늘 챙겨봐요 최근 블랙위도우도요",
          "original_form": "늘 챙겨봐요 최근 블랙위도우도요",
          "speaker_id": "2",
          "time": "20210823 14:22"
        },
        {
          "id": "MDRW2100009760.1.20",
          "form": "블랙위도우 넘 재밌었어요",
          "original_form": "블랙위도우 넘 재밌었어요",
          "speaker_id": "1",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100009760.1.21",
          "form": "맞아요..... 다음편 너무 기다려지게 ㅠㅠ",
          "original_form": "맞아요..... 다음편 너무 기다려지게 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100009760.1.22",
          "form": "2편 나오겠죠 당연히??",
          "original_form": "2편 나오겠죠 당연히??",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100009760.1.23",
          "form": "ㅜㅜㅜ저도 희망하고있는데 스칼렛요한슨은 계약만기라고하네요ㅜ",
          "original_form": "ㅜㅜㅜ저도 희망하고있는데 스칼렛요한슨은 계약만기라고하네요ㅜ",
          "speaker_id": "1",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100009760.1.24",
          "form": "캐릭터들 다 훌륭했는데",
          "original_form": "캐릭터들 다 훌륭했는데",
          "speaker_id": "1",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100009760.1.25",
          "form": "헐.....ㅠㅠ  죽음과 동시에 계약종료인건지...",
          "original_form": "헐.....ㅠㅠ  죽음과 동시에 계약종료인건지...",
          "speaker_id": "2",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100009760.1.26",
          "form": "어벤져스 엔드게임 보셨나요?",
          "original_form": "어벤져스 엔드게임 보셨나요?",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100009760.1.27",
          "form": "동생이 호크아이 찾아나서는걸로 나올거같은데",
          "original_form": "동생이 호크아이 찾아나서는걸로 나올거같은데",
          "speaker_id": "2",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100009760.1.28",
          "form": "네네 엔드게임... 봤죠 ㅠㅠ",
          "original_form": "네네 엔드게임... 봤죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 18:57"
        },
        {
          "id": "MDRW2100009760.1.29",
          "form": "네 그런 스토리로 가나봐요ㅜ",
          "original_form": "네 그런 스토리로 가나봐요ㅜ",
          "speaker_id": "1",
          "time": "20210823 18:57"
        },
        {
          "id": "MDRW2100009760.1.30",
          "form": "플로렌스 퓨가 나오는걸 위로 삼아야죠ㅜㅜ",
