      "utterance": [
        {
          "id": "MDRW2100039573.1.1",
          "form": "에궁 어쩌시다가..",
          "original_form": "에궁 어쩌시다가..",
          "speaker_id": "2",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039573.1.2",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039573.1.3",
          "form": "날이 안 좋아서 그런가?",
          "original_form": "날이 안 좋아서 그런가?",
          "speaker_id": "1",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039573.1.4",
          "form": "그럴 수도 있겠어요",
          "original_form": "그럴 수도 있겠어요",
          "speaker_id": "2",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039573.1.5",
          "form": "네 그래서 낮에 파스 붙였어요",
          "original_form": "네 그래서 낮에 파스 붙였어요",
          "speaker_id": "1",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039573.1.6",
          "form": "병원은 안가도 돼요?",
          "original_form": "병원은 안가도 돼요?",
          "speaker_id": "2",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039573.1.7",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "1",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039573.1.8",
          "form": "물리치료",
          "original_form": "물리치료",
          "speaker_id": "1",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039573.1.9",
          "form": "만 했어요",
          "original_form": "만 했어요",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039573.1.10",
          "form": "집에서여?",
          "original_form": "집에서여?",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039573.1.11",
          "form": "네 저주파 안마기로요 ㅎㅎ",
          "original_form": "네 저주파 안마기로요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039573.1.12",
          "form": "아 클럭??",
          "original_form": "아 클럭??",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039573.1.13",
          "form": "아뇽 그 비슷한 짝퉁이요 ㅋㅋ",
          "original_form": "아뇽 그 비슷한 짝퉁이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039573.1.14",
          "form": "좋은가요?",
          "original_form": "좋은가요?",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039573.1.15",
          "form": "오늘은 안 아프더라고요",
          "original_form": "오늘은 안 아프더라고요",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039573.1.16",
          "form": "오 좋은가봐요",
          "original_form": "오 좋은가봐요",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039573.1.17",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039573.1.18",
          "form": "쓸만하네요",
          "original_form": "쓸만하네요",
          "speaker_id": "1",
          "time": "20210903 19:14"
        }
      ]
