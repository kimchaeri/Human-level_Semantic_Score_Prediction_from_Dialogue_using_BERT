      "utterance": [
        {
          "id": "MDRW2100036394.1.1",
          "form": "코로나라 여행못가서 넘아쉬워요ㅠ",
          "original_form": "코로나라 여행못가서 넘아쉬워요ㅠ",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100036394.1.2",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100036394.1.3",
          "form": "좋아하시나요? 여행하는거?",
          "original_form": "좋아하시나요? 여행하는거?",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100036394.1.4",
          "form": "엄청 좋아해요!",
          "original_form": "엄청 좋아해요!",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100036394.1.5",
          "form": "해외여행도좋아하고\n국내 돌아다니는것도 좋아해요",
          "original_form": "해외여행도좋아하고\n국내 돌아다니는것도 좋아해요",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036394.1.6",
          "form": "가장 기억에 남는 여행장소는 어디세요?!",
          "original_form": "가장 기억에 남는 여행장소는 어디세요?!",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036394.1.7",
          "form": "다 비슷비슷하게 기억에 남은거같아요~~~!!ㅋㅋㅋ \n그치만 또 가고싶은곳근 루르드나 피렌체",
          "original_form": "다 비슷비슷하게 기억에 남은거같아요~~~!!ㅋㅋㅋ \n그치만 또 가고싶은곳근 루르드나 피렌체",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036394.1.8",
          "form": "오 루르드는 프랑스인가요 ??!",
          "original_form": "오 루르드는 프랑스인가요 ??!",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100036394.1.9",
          "form": "네 헉 아세요???",
          "original_form": "네 헉 아세요???",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100036394.1.10",
          "form": "잘 알지는 못하지만 인터넷에서 본거같아요 !",
          "original_form": "잘 알지는 못하지만 인터넷에서 본거같아요 !",
          "speaker_id": "2",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100036394.1.11",
          "form": "어떤 느낌이에요?",
          "original_form": "어떤 느낌이에요?",
          "speaker_id": "2",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100036394.1.12",
          "form": "가면 힐링되는것같아요.",
          "original_form": "가면 힐링되는것같아요.",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100036394.1.13",
          "form": "그곳에가면 기적수라는게 있는데",
          "original_form": "그곳에가면 기적수라는게 있는데",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100036394.1.14",
          "form": "네네 !",
          "original_form": "네네 !",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100036394.1.15",
          "form": "저희가족이 그 기적수 덕을 본 것 같아서",
          "original_form": "저희가족이 그 기적수 덕을 본 것 같아서",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100036394.1.16",
          "form": "오 진짜요?",
          "original_form": "오 진짜요?",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100036394.1.17",
          "form": "기회되면 한번 더 가서 기적수를 떠오고싶어요.",
          "original_form": "기회되면 한번 더 가서 기적수를 떠오고싶어요.",
          "speaker_id": "1",
          "time": "20210830 22:43"
        }
      ]
