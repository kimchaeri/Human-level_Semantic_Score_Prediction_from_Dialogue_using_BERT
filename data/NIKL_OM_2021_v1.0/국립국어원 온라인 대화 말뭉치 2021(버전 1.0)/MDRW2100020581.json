      "utterance": [
        {
          "id": "MDRW2100020581.1.1",
          "form": "안되면 피스타치오라도 넣어야해요",
          "original_form": "안되면 피스타치오라도 넣어야해요",
          "speaker_id": "2",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020581.1.2",
          "form": "그럼 파인트에 3가지맛을 담는다면?",
          "original_form": "그럼 파인트에 3가지맛을 담는다면?",
          "speaker_id": "1",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020581.1.3",
          "form": "하 세가지..",
          "original_form": "하 세가지..",
          "speaker_id": "2",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020581.1.4",
          "form": "파인트는 취급안하는데 고민해볼게요",
          "original_form": "파인트는 취급안하는데 고민해볼게요",
          "speaker_id": "2",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020581.1.5",
          "form": "이상한나라의 솜사탕 애플민트 체리쥬빌레",
          "original_form": "이상한나라의 솜사탕 애플민트 체리쥬빌레",
          "speaker_id": "2",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020581.1.6",
          "form": "피스타치오랑 ㅋㅋ 민초는 \n색   \n만 \n같\n지\n\n결이 전혀 다르잖아요ㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "피스타치오랑 ㅋㅋ 민초는 \n색   \n만 \n같\n지\n\n결이 전혀 다르잖아요ㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020581.1.7",
          "form": "체쥬를 드시는군요",
          "original_form": "체쥬를 드시는군요",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020581.1.8",
          "form": "쳇 그렇게 강조를 하시다니",
          "original_form": "쳇 그렇게 강조를 하시다니",
          "speaker_id": "2",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020581.1.9",
          "form": "맛잘알이시네요",
          "original_form": "맛잘알이시네요",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020581.1.10",
          "form": "저는 첨에 구분을 못했다구요",
          "original_form": "저는 첨에 구분을 못했다구요",
          "speaker_id": "2",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020581.1.11",
          "form": "세사메",
          "original_form": "세사메",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020581.1.12",
          "form": "피스타치오도 약간 화하지 않나요",
          "original_form": "피스타치오도 약간 화하지 않나요",
          "speaker_id": "2",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020581.1.13",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020581.1.14",
          "form": "피스타가요?",
          "original_form": "피스타가요?",
          "speaker_id": "1",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020581.1.15",
          "form": "전혀요",
          "original_form": "전혀요",
          "speaker_id": "1",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020581.1.16",
          "form": "화한가? 아닌데",
          "original_form": "화한가? 아닌데",
          "speaker_id": "1",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020581.1.17",
          "form": "네 약간 입안이 시원한",
          "original_form": "네 약간 입안이 시원한",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020581.1.18",
          "form": "느낌이 들었는데..!",
          "original_form": "느낌이 들었는데..!",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020581.1.19",
          "form": "사실 저만 그런거같긴 해요 ㅎㅁㅎ",
          "original_form": "사실 저만 그런거같긴 해요 ㅎㅁㅎ",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020581.1.20",
          "form": "아 느낌은 있어요",
          "original_form": "아 느낌은 있어요",
          "speaker_id": "1",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020581.1.21",
          "form": "근데 민트처럼",
          "original_form": "근데 민트처럼",
          "speaker_id": "1",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020581.1.22",
          "form": "너의 입을 청소하겠다",
          "original_form": "너의 입을 청소하겠다",
          "speaker_id": "1",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020581.1.23",
          "form": "이런느낌은 아니라ㅋㅋ",
          "original_form": "이런느낌은 아니라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020581.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸 있죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸 있죠",
          "speaker_id": "2",
          "time": "20210828 10:07"
        }
