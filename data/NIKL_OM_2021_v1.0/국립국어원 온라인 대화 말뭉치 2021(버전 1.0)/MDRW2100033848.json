      "utterance": [
        {
          "id": "MDRW2100033848.1.1",
          "form": "녹차vs홍차 어떤가요?",
          "original_form": "녹차vs홍차 어떤가요?",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100033848.1.2",
          "form": "오 저는 홍차요",
          "original_form": "오 저는 홍차요",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100033848.1.3",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100033848.1.4",
          "form": "ㅋㅋㅋ저도요 둘중에 고르자면",
          "original_form": "ㅋㅋㅋ저도요 둘중에 고르자면",
          "speaker_id": "2",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100033848.1.5",
          "form": "떡볶이는 좋아하세요?",
          "original_form": "떡볶이는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100033848.1.6",
          "form": "떡볶이 좋죠 엄청 자주 먹어요ㅋㅋㅋㅋ",
          "original_form": "떡볶이 좋죠 엄청 자주 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:36"
        },
        {
          "id": "MDRW2100033848.1.7",
          "form": "어떤브랜드 좋아하세요?",
          "original_form": "어떤브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100033848.1.8",
          "form": "전 신전 좋아해요!",
          "original_form": "전 신전 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 12:45"
        },
        {
          "id": "MDRW2100033848.1.9",
          "form": "name2 님은 떡볶이 좋아하세요?",
          "original_form": "&name2& 님은 떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 12:45"
        },
        {
          "id": "MDRW2100033848.1.10",
          "form": "전 너무좋아해요ㅋㅋㅋㅋ",
          "original_form": "전 너무좋아해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:55"
        },
        {
          "id": "MDRW2100033848.1.11",
          "form": "오ㅋㅋㅋ 어디 브랜드 좋아하세요?",
          "original_form": "오ㅋㅋㅋ 어디 브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:01"
        },
        {
          "id": "MDRW2100033848.1.12",
          "form": "ㅋㅋㅋ전다좋아해요",
          "original_form": "ㅋㅋㅋ전다좋아해요",
          "speaker_id": "2",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100033848.1.13",
          "form": "로제좋아하세요?",
          "original_form": "로제좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100033848.1.14",
          "form": "와 당연히 좋아하죠!!! 로제 떡볶이 너무 맛있죠",
          "original_form": "와 당연히 좋아하죠!!! 로제 떡볶이 너무 맛있죠",
          "speaker_id": "1",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100033848.1.15",
          "form": "배떡드셔보셨어요?",
          "original_form": "배떡드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100033848.1.16",
          "form": "아뇨 배떡은 안 먹어봤어요! 맛있나요?",
          "original_form": "아뇨 배떡은 안 먹어봤어요! 맛있나요?",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100033848.1.17",
          "form": "ㅋㅋㅋ 생각보다별로입니다",
          "original_form": "ㅋㅋㅋ 생각보다별로입니다",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033848.1.18",
          "form": "떡군이네가 존맛이더라고요",
          "original_form": "떡군이네가 존맛이더라고요",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033848.1.19",
          "form": "아 떡군이네에서 배떡이 나왔다고 했나...? 그랬다고 들었어요",
          "original_form": "아 떡군이네에서 배떡이 나왔다고 했나...? 그랬다고 들었어요",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100033848.1.20",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033848.1.21",
