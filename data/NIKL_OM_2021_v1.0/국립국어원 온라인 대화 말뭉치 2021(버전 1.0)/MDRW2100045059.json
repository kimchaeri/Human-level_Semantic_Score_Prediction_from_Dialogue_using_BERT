      "utterance": [
        {
          "id": "MDRW2100045059.1.1",
          "form": "기본 초코쿠키도 짱맛이던데",
          "original_form": "기본 초코쿠키도 짱맛이던데",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045059.1.2",
          "form": "민트도 그렇군요!",
          "original_form": "민트도 그렇군요!",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045059.1.3",
          "form": "ㅋㅋㅋ네네 완전 시원달달 ㅋㅋㅋ최고입니다 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ네네 완전 시원달달 ㅋㅋㅋ최고입니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045059.1.4",
          "form": "과자 좋아하세요?",
          "original_form": "과자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045059.1.5",
          "form": "너무 좋아해서 문제ㅜ",
          "original_form": "너무 좋아해서 문제ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045059.1.6",
          "form": "ㄴㅋㅋㅋㅋㅋ저두요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㄴㅋㅋㅋㅋㅋ저두요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045059.1.7",
          "form": "너무많이먹어요",
          "original_form": "너무많이먹어요",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045059.1.8",
          "form": "차도 좋아하시죠?",
          "original_form": "차도 좋아하시죠?",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045059.1.9",
          "form": "ㅋㅋㅋ넹 차는 페퍼민트 조아해용",
          "original_form": "ㅋㅋㅋ넹 차는 페퍼민트 조아해용",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045059.1.10",
          "form": "그럼 해독됩니다ㄱㄲ",
          "original_form": "그럼 해독됩니다ㄱㄲ",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045059.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 역시",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 역시",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045059.1.12",
          "form": "아메리카노 법칙에 이은",
          "original_form": "아메리카노 법칙에 이은",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045059.1.13",
          "form": "차 법칙 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "차 법칙 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045059.1.14",
          "form": "ㅋㅋ마카롱이랑 홍차랑 궁합이",
          "original_form": "ㅋㅋ마카롱이랑 홍차랑 궁합이",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045059.1.15",
          "form": "ㅋㅋㅋ와우 미치죠 그거진짜",
          "original_form": "ㅋㅋㅋ와우 미치죠 그거진짜",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045059.1.16",
          "form": "ㅋㅋ 전 그때 홍차로 넘어갈뻔했는데ㅋㅋ",
          "original_form": "ㅋㅋ 전 그때 홍차로 넘어갈뻔했는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045059.1.17",
          "form": "아메...",
          "original_form": "아메...",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045059.1.18",
          "form": "하 저는 카페인이 약해서",
          "original_form": "하 저는 카페인이 약해서",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045059.1.19",
          "form": "아니면 아메 0.5샷으로 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니면 아메 0.5샷으로 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045059.1.20",
