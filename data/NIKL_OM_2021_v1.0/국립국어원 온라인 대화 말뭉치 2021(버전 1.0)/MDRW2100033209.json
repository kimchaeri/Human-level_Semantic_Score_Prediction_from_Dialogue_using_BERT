      "utterance": [
        {
          "id": "MDRW2100033209.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210906 19:01"
        },
        {
          "id": "MDRW2100033209.1.2",
