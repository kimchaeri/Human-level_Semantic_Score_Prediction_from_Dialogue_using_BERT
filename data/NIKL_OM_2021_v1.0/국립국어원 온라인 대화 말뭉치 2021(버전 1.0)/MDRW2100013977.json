      "utterance": [
        {
          "id": "MDRW2100013977.1.1",
          "form": "저녁 뭐 먹을거야?",
          "original_form": "저녁 뭐 먹을거야?",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100013977.1.2",
          "form": "오~벌써 저녁시간이네",
          "original_form": "오~벌써 저녁시간이네",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100013977.1.3",
          "form": "ㅇㅇ저녁 뭐 먹으려고?",
          "original_form": "ㅇㅇ저녁 뭐 먹으려고?",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013977.1.4",
          "form": "난 검은콩국수 생각중ㅋ",
          "original_form": "난 검은콩국수 생각중ㅋ",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013977.1.5",
          "form": "오 회사식단?",
          "original_form": "오 회사식단?",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013977.1.6",
          "form": "너는 정했어?",
          "original_form": "너는 정했어?",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013977.1.7",
          "form": "난 뭐 먹어야될지 모르겠네",
          "original_form": "난 뭐 먹어야될지 모르겠네",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013977.1.8",
          "form": "응ㅋ 치킨도 나오는데\r\n콩물 먹을라고ㅋㅋ",
          "original_form": "응ㅋ 치킨도 나오는데\r\n콩물 먹을라고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013977.1.9",
          "form": "와우 치킨 대박",
          "original_form": "와우 치킨 대박",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013977.1.10",
          "form": "간단하게 먹을거야?",
          "original_form": "간단하게 먹을거야?",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013977.1.11",
          "form": "나 치킨먹고싶었는데",
          "original_form": "나 치킨먹고싶었는데",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013977.1.12",
          "form": "힝",
          "original_form": "힝",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100013977.1.13",
          "form": "밥 먹을건데 카레 남은거 먹을까싶당",
          "original_form": "밥 먹을건데 카레 남은거 먹을까싶당",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100013977.1.14",
          "form": "치킨 할인 없어?",
          "original_form": "치킨 할인 없어?",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100013977.1.15",
          "form": "할인 있음 좋을텐데",
          "original_form": "할인 있음 좋을텐데",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100013977.1.16",
          "form": "요기요나 배민 할인 시덥지않더라",
          "original_form": "요기요나 배민 할인 시덥지않더라",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100013977.1.17",
          "form": "카레라니 치킨보다 맛있는건데~~",
          "original_form": "카레라니 치킨보다 맛있는건데~~",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100013977.1.18",
          "form": "몇주전부터 할인이 별루야",
          "original_form": "몇주전부터 할인이 별루야",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100013977.1.19",
          "form": "카레 찐맛 카레 먹은지 오래된듯ㅜㅜㅋ",
          "original_form": "카레 찐맛 카레 먹은지 오래된듯ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013977.1.20",
          "form": "카레가 치킨보다 맛있어?",
          "original_form": "카레가 치킨보다 맛있어?",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013977.1.21",
          "form": "카레 들으니 먹고싶ㅋ",
          "original_form": "카레 들으니 먹고싶ㅋ",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013977.1.22",
          "form": "아 먹은지 오래되서그런듯",
          "original_form": "아 먹은지 오래되서그런듯",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013977.1.23",
          "form": "삼분카레도 괜찮던데",
          "original_form": "삼분카레도 괜찮던데",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013977.1.24",
          "form": "편의점 소시지랑 같이먹어봐",
          "original_form": "편의점 소시지랑 같이먹어봐",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013977.1.25",
          "form": "작년에 먹었던듯",
          "original_form": "작년에 먹었던듯",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013977.1.26",
          "form": "소시지랑?",
          "original_form": "소시지랑?",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013977.1.27",
          "form": "삼분카레는 건더기가 적어서",
          "original_form": "삼분카레는 건더기가 적어서",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013977.1.28",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013977.1.29",
          "form": "카레는 고기맛이지ㅋㅋ",
          "original_form": "카레는 고기맛이지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013977.1.30",
          "form": "뭐라도 같이먹음 맛있옹",
          "original_form": "뭐라도 같이먹음 맛있옹",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013977.1.31",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013977.1.32",
          "form": "고기 골라먹는거ㅋㅋㅋ",
          "original_form": "고기 골라먹는거ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013977.1.33",
          "form": "닭슴살 넣어서 카레했당",
          "original_form": "닭슴살 넣어서 카레했당",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013977.1.34",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013977.1.35",
          "form": "아이디어다",
          "original_form": "아이디어다",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013977.1.36",
          "form": "그냥 돼지고기 카레랑",
          "original_form": "그냥 돼지고기 카레랑",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100013977.1.37",
          "form": "맛 비교 어때?",
          "original_form": "맛 비교 어때?",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100013977.1.38",
          "form": "돼지고기가 맛있어....",
          "original_form": "돼지고기가 맛있어....",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100013977.1.39",
          "form": "걍 닭이 있으니까 닭 씀 ㅋㅋ",
          "original_form": "걍 닭이 있으니까 닭 씀 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100013977.1.40",
          "form": "그래도 당근 많이 넣어서 맛있어",
          "original_form": "그래도 당근 많이 넣어서 맛있어",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100013977.1.41",
          "form": "난 카레에 감자ㅋ",
          "original_form": "난 카레에 감자ㅋ",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100013977.1.42",
          "form": "저녁 먹었나",
          "original_form": "저녁 먹었나",
