        {
          "id": "MDRW2100013535.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013535.1.2",
          "form": "저 답을 못봤는데",
          "original_form": "저 답을 못봤는데",
          "speaker_id": "2",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013535.1.3",
          "form": "어떻게 된거죠?",
          "original_form": "어떻게 된거죠?",
          "speaker_id": "2",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013535.1.4",
          "form": "ㅋㅋ 지나간 시간에 미련을 가지지마세요",
          "original_form": "ㅋㅋ 지나간 시간에 미련을 가지지마세요",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013535.1.5",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013535.1.6",
          "form": "안녕하세요 name1님",
          "original_form": "안녕하세요 &name1&님",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013535.1.7",
          "form": "배달음식 많이 드시나요?",
          "original_form": "배달음식 많이 드시나요?",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013535.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013535.1.9",
          "form": "제가 그 유명한",
          "original_form": "제가 그 유명한",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013535.1.10",
          "form": "마라탕 킬러죠",
          "original_form": "마라탕 킬러죠",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013535.1.11",
          "form": "아니 저기요",
          "original_form": "아니 저기요",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013535.1.12",
          "form": "마라탕 좋아하시나봐요",
          "original_form": "마라탕 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013535.1.13",
          "form": "제가 질문 할꺼라구요",
          "original_form": "제가 질문 할꺼라구요",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013535.1.14",
          "form": "네 하세요",
          "original_form": "네 하세요",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013535.1.15",
          "form": "면접관님",
          "original_form": "면접관님",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013535.1.16",
          "form": "오늘의 점심은",
          "original_form": "오늘의 점심은",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013535.1.17",
          "form": "무엇?",
          "original_form": "무엇?",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013535.1.18",
          "form": "이죠?",
          "original_form": "이죠?",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013535.1.19",
          "form": "저는 오늘 비가와서",
          "original_form": "저는 오늘 비가와서",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013535.1.20",
          "form": "아 거기는 비가 오나봐요",
          "original_form": "아 거기는 비가 오나봐요",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013535.1.21",
          "form": "제일 가까운 롯데리아를 갔다 왔어요",
          "original_form": "제일 가까운 롯데리아를 갔다 왔어요",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013535.1.22",
          "form": "여기는 쪄죽겠어요",
          "original_form": "여기는 쪄죽겠어요",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013535.1.23",
          "form": "비오는날에",
          "original_form": "비오는날에",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013535.1.24",
          "form": "네 비가 왔다 안왔다 하네요",
          "original_form": "네 비가 왔다 안왔다 하네요",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013535.1.25",
          "form": "롯데리아라..",
          "original_form": "롯데리아라..",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013535.1.26",
          "form": "특이하시네요",
          "original_form": "특이하시네요",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013535.1.27",
          "form": "어제도 비가와서 칼국수를 먹었답니다",
          "original_form": "어제도 비가와서 칼국수를 먹었답니다",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013535.1.28",
          "form": "칼국수는 인정해드립니다",
          "original_form": "칼국수는 인정해드립니다",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013535.1.29",
          "form": "롯데리아는 가까워서 간거예요",
          "original_form": "롯데리아는 가까워서 간거예요",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013535.1.30",
          "form": "저는 질문에 소질이 없는거 같아요",
          "original_form": "저는 질문에 소질이 없는거 같아요",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013535.1.31",
          "form": "햄버거는 맛이 없구요",
          "original_form": "햄버거는 맛이 없구요",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013535.1.32",
          "form": "하던대로 합시다",
          "original_form": "하던대로 합시다",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013535.1.33",
          "form": "감자튀김이 최고예요",
          "original_form": "감자튀김이 최고예요",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013535.1.34",
          "form": "감자튀김은",
          "original_form": "감자튀김은",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013535.1.35",
          "form": "어디냐?",
          "original_form": "어디냐?",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013535.1.36",
          "form": "이름이 기억이 안나네요",
          "original_form": "이름이 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013535.1.37",
          "form": "아 저거 반말 아니고 혼잣말입니다",
          "original_form": "아 저거 반말 아니고 혼잣말입니다",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013535.1.38",
          "form": "그래서 어디냐??",
          "original_form": "그래서 어디냐??",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.39",
          "form": "혼잣말을 채팅으로 하시네요",
          "original_form": "혼잣말을 채팅으로 하시네요",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.40",
          "form": "아!!!kfc가",
          "original_form": "아!!!kfc가",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.41",
          "form": "근데 반말해도 되는데요?",
          "original_form": "근데 반말해도 되는데요?",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.42",
          "form": "맛있다고 생각해요",
          "original_form": "맛있다고 생각해요",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.43",
          "form": "감튀는 인정이지~~",
          "original_form": "감튀는 인정이지~~",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.44",
          "form": "아진짜?",
          "original_form": "아진짜?",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.45",
          "form": "친한척좀할게",
          "original_form": "친한척좀할게",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.46",
          "form": "난 왜 자꾸 존댓말을 하지?",
          "original_form": "난 왜 자꾸 존댓말을 하지?",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.47",
          "form": "컨셉인가?",
          "original_form": "컨셉인가?",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.48",
          "form": "컨셉이 더 재밌는데",
          "original_form": "컨셉이 더 재밌는데",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.49",
          "form": "존대로 할래",
          "original_form": "존대로 할래",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.50",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013535.1.51",
          "form": "지금 햄버거가 안땡겨서 그러는데",
          "original_form": "지금 햄버거가 안땡겨서 그러는데",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013535.1.52",
          "form": "다른걸로 넘어갈게요",
          "original_form": "다른걸로 넘어갈게요",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013535.1.53",
          "form": "네 마라탕은 제가 안먹어봐서요",
          "original_form": "네 마라탕은 제가 안먹어봐서요",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013535.1.54",
          "form": "그래서 질문이 뭐죠?",
          "original_form": "그래서 질문이 뭐죠?",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013535.1.55",
          "form": "카페 좋아하세요?",
          "original_form": "카페 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013535.1.56",
          "form": "마라탕",
          "original_form": "마라탕",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013535.1.57",
          "form": "아 카페 ㅈㆍㄷㅎ죠",
          "original_form": "아 카페 ㅈㆍㄷㅎ죠",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013535.1.58",
          "form": "너무 좋아서 오타가 나네요",
          "original_form": "너무 좋아서 오타가 나네요",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013535.1.59",
          "form": "임티쓰면 좀 아줌마같다",
          "original_form": "임티쓰면 좀 아줌마같다",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013535.1.60",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013535.1.61",
          "form": "아줌마 쓰셔도 괜찮아요",
          "original_form": "아줌마 쓰셔도 괜찮아요",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013535.1.62",
          "form": "저 겜좀하고올게요",
          "original_form": "저 겜좀하고올게요",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013535.1.63",
          "form": "어느 카페 자주 가세요?",
          "original_form": "어느 카페 자주 가세요?",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013535.1.64",
          "form": "저 개인시간",
          "original_form": "저 개인시간",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013535.1.65",
          "form": "존중",
          "original_form": "존중",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013535.1.66",
          "form": "알죠",
          "original_form": "알죠",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013535.1.67",
          "form": "대화하다가 게임하러 가시는게",
          "original_form": "대화하다가 게임하러 가시는게",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013535.1.68",
          "form": "10분뒤에봐요",
          "original_form": "10분뒤에봐요",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013535.1.69",
          "form": "더 존중이 없는거 아녜요?",
          "original_form": "더 존중이 없는거 아녜요?",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013535.1.70",
          "form": "허참 이상한 사람이네!",
          "original_form": "허참 이상한 사람이네!",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013535.1.71",
          "form": "그 이상한사람",
          "original_form": "그 이상한사람",
          "speaker_id": "2",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013535.1.72",
          "form": "6시에 봅시다",
          "original_form": "6시에 봅시다",
          "speaker_id": "2",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013535.1.73",
          "form": "마라탕 왜 맛있는지 설명해주고 가세요",
