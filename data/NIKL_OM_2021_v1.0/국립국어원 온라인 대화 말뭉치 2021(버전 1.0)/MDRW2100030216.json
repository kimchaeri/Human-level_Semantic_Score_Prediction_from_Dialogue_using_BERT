      "utterance": [
        {
          "id": "MDRW2100030216.1.1",
          "form": "무뼈인가요",
          "original_form": "무뼈인가요",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030216.1.2",
          "form": "그는 좋은닭발이었죠",
          "original_form": "그는 좋은닭발이었죠",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030216.1.3",
          "form": "매웠나요",
          "original_form": "매웠나요",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030216.1.4",
          "form": "무뼈입니다 불맛",
          "original_form": "무뼈입니다 불맛",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030216.1.5",
          "form": "닭은 발이 두개입니다",
          "original_form": "닭은 발이 두개입니다",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030216.1.6",
          "form": "몇개드셨나요",
          "original_form": "몇개드셨나요",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030216.1.7",
          "form": "매운게 짱이져",
          "original_form": "매운게 짱이져",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030216.1.8",
          "form": "소금맛도  있나요",
          "original_form": "소금맛도  있나요",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030216.1.9",
          "form": "스트레스해소되지요",
          "original_form": "스트레스해소되지요",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030216.1.10",
          "form": "닭은 발이 세개가 아닙니다",
          "original_form": "닭은 발이 세개가 아닙니다",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030216.1.11",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030216.1.12",
          "form": "양념만먹어봤어야",
          "original_form": "양념만먹어봤어야",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030216.1.13",
          "form": "네개도 아니지요",
          "original_form": "네개도 아니지요",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030216.1.14",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030216.1.15",
          "form": "맞아요 두개에요",
          "original_form": "맞아요 두개에요",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030216.1.16",
          "form": "몇마리치드셨나요",
          "original_form": "몇마리치드셨나요",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030216.1.17",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030216.1.18",
          "form": "많이요 ㅜㅜ",
          "original_form": "많이요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030216.1.19",
          "form": "전 닭목도 좋아합니다",
          "original_form": "전 닭목도 좋아합니다",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030216.1.20",
          "form": "많이 드셨군요",
          "original_form": "많이 드셨군요",
          "speaker_id": "2",
          "time": "20210903 23:01"
        }
