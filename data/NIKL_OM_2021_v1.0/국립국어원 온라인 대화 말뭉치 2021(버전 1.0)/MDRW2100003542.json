      "utterance": [
        {
          "id": "MDRW2100003542.1.1",
          "form": "괜찮은 회사 구인공고 올라왔는데,",
          "original_form": "괜찮은 회사 구인공고 올라왔는데,",
          "speaker_id": "1",
          "time": "20210821 00:50"
        },
        {
          "id": "MDRW2100003542.1.2",
          "form": "지원해볼까요?",
          "original_form": "지원해볼까요?",
          "speaker_id": "1",
          "time": "20210821 00:50"
        },
        {
          "id": "MDRW2100003542.1.3",
          "form": "뭐가 괜찮은데?",
          "original_form": "뭐가 괜찮은데?",
          "speaker_id": "2",
          "time": "20210821 00:50"
        },
        {
          "id": "MDRW2100003542.1.4",
          "form": "일단 입사율에 비해 퇴사율이 적고",
          "original_form": "일단 입사율에 비해 퇴사율이 적고",
          "speaker_id": "1",
          "time": "20210821 00:50"
        },
        {
          "id": "MDRW2100003542.1.5",
          "form": "사내 분위기도 좋은 거 같고",
          "original_form": "사내 분위기도 좋은 거 같고",
          "speaker_id": "1",
          "time": "20210821 00:50"
        },
        {
          "id": "MDRW2100003542.1.6",
          "form": "체계도 잘 잡혀 잇는 거 같아요~",
          "original_form": "체계도 잘 잡혀 잇는 거 같아요~",
          "speaker_id": "1",
          "time": "20210821 00:50"
        },
        {
          "id": "MDRW2100003542.1.7",
          "form": "그래? 다른건?",
          "original_form": "그래? 다른건?",
          "speaker_id": "2",
          "time": "20210821 00:51"
        },
        {
          "id": "MDRW2100003542.1.8",
          "form": "여기저기 알아봤는데,",
          "original_form": "여기저기 알아봤는데,",
          "speaker_id": "1",
          "time": "20210821 00:51"
        },
        {
          "id": "MDRW2100003542.1.9",
          "form": "스타트업이라 정보는 많이 없지만",
          "original_form": "스타트업이라 정보는 많이 없지만",
          "speaker_id": "1",
          "time": "20210821 00:51"
        },
        {
          "id": "MDRW2100003542.1.10",
          "form": "나름 평들이 좋아요~",
          "original_form": "나름 평들이 좋아요~",
          "speaker_id": "1",
          "time": "20210821 00:51"
        },
        {
          "id": "MDRW2100003542.1.11",
          "form": "지역이 어딘데?",
          "original_form": "지역이 어딘데?",
          "speaker_id": "2",
          "time": "20210821 00:51"
        },
        {
          "id": "MDRW2100003542.1.12",
          "form": "강남역이요~",
          "original_form": "강남역이요~",
          "speaker_id": "1",
          "time": "20210821 00:51"
        },
        {
          "id": "MDRW2100003542.1.13",
          "form": "강남역 1번 출궄ㅋㅋㅋ",
          "original_form": "강남역 1번 출궄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 00:51"
        },
        {
          "id": "MDRW2100003542.1.14",
          "form": "또 강남역이네~",
          "original_form": "또 강남역이네~",
          "speaker_id": "2",
          "time": "20210821 00:52"
        },
        {
          "id": "MDRW2100003542.1.15",
          "form": "뭐.. 이제 익숙하죠~ㅎㅎ",
          "original_form": "뭐.. 이제 익숙하죠~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 00:52"
        },
        {
          "id": "MDRW2100003542.1.16",
          "form": "딸이 괜찮다고 생각되면",
          "original_form": "딸이 괜찮다고 생각되면",
          "speaker_id": "2",
          "time": "20210821 00:52"
        },
        {
          "id": "MDRW2100003542.1.17",
          "form": "한번해봐~",
          "original_form": "한번해봐~",
          "speaker_id": "2",
          "time": "20210821 00:52"
        },
        {
          "id": "MDRW2100003542.1.18",
          "form": "뭐~ 안되면 말고니까~",
          "original_form": "뭐~ 안되면 말고니까~",
          "speaker_id": "1",
          "time": "20210821 00:52"
        },
        {
          "id": "MDRW2100003542.1.19",
          "form": "되면 좋은 거고~~",
          "original_form": "되면 좋은 거고~~",
          "speaker_id": "1",
          "time": "20210821 00:52"
        },
        {
          "id": "MDRW2100003542.1.20",
          "form": "언제까진데?",
          "original_form": "언제까진데?",
          "speaker_id": "2",
          "time": "20210821 00:52"
        },
        {
          "id": "MDRW2100003542.1.21",
          "form": "다다음주까지요~",
          "original_form": "다다음주까지요~",
          "speaker_id": "1",
          "time": "20210821 00:52"
        },
        {
          "id": "MDRW2100003542.1.22",
          "form": "미리미리준비해야겠네~",
          "original_form": "미리미리준비해야겠네~",
          "speaker_id": "2",
          "time": "20210821 00:53"
        },
        {
          "id": "MDRW2100003542.1.23",
          "form": "미리 만들어놓은 것도 있어서 조금만 고치면 될 거 같아요~",
          "original_form": "미리 만들어놓은 것도 있어서 조금만 고치면 될 거 같아요~",
          "speaker_id": "1",
          "time": "20210821 00:53"
        }
