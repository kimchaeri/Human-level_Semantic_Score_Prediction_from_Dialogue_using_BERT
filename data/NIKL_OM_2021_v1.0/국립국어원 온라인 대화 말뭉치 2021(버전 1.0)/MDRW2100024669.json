      "utterance": [
        {
          "id": "MDRW2100024669.1.1",
          "form": "그리고 일본여행 내내 느낀 건데",
          "original_form": "그리고 일본여행 내내 느낀 건데",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024669.1.2",
          "form": "그리고 여행선물 1위 일본과자",
          "original_form": "그리고 여행선물 1위 일본과자",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024669.1.3",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024669.1.4",
          "form": "디저트가 진짜 다양해",
          "original_form": "디저트가 진짜 다양해",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024669.1.5",
          "form": "인절미과자 사람들이 정말 많이 사가더라",
          "original_form": "인절미과자 사람들이 정말 많이 사가더라",
          "speaker_id": "1",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024669.1.6",
          "form": "둘째날 저녁은 뭐먹었어?",
          "original_form": "둘째날 저녁은 뭐먹었어?",
          "speaker_id": "2",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024669.1.7",
          "form": "저녁으로 규카츠 먹었어!!!!!!!!!!!!!!",
          "original_form": "저녁으로 규카츠 먹었어!!!!!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100024669.1.8",
          "form": "규카츠  맛있냐??",
          "original_form": "규카츠  맛있냐??",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100024669.1.9",
          "form": "응 맛있어 ㅋㅋㅋㅋ해외에서도 고기는 늘 옳더라",
          "original_form": "응 맛있어 ㅋㅋㅋㅋ해외에서도 고기는 늘 옳더라",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100024669.1.10",
          "form": "돈카츠랑 약간 달랐어",
          "original_form": "돈카츠랑 약간 달랐어",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100024669.1.11",
          "form": "가격은 비싸??",
          "original_form": "가격은 비싸??",
          "speaker_id": "2",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100024669.1.12",
          "form": "우리나라보다 물가가 비싸다던디",
          "original_form": "우리나라보다 물가가 비싸다던디",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100024669.1.13",
          "form": "엄청 비쌀 줄 알았는데 '생각보다는' 쌌어 만원 중반대",
          "original_form": "엄청 비쌀 줄 알았는데 '생각보다는' 쌌어 만원 중반대",
          "speaker_id": "1",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100024669.1.14",
          "form": "난 이만원대쯤 할 줄 알았거든",
          "original_form": "난 이만원대쯤 할 줄 알았거든",
          "speaker_id": "1",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100024669.1.15",
          "form": "ㅅㅓ울이랑 비슷하네",
          "original_form": "ㅅㅓ울이랑 비슷하네",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100024669.1.16",
          "form": "솔찍",
          "original_form": "솔찍",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100024669.1.17",
          "form": "맞아 첫여행이라 그나라 물가 수준을 몰라서",
          "original_form": "맞아 첫여행이라 그나라 물가 수준을 몰라서",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024669.1.18",
          "form": "서울도 물가가 많이 올랐구먼",
          "original_form": "서울도 물가가 많이 올랐구먼",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024669.1.19",
          "form": "여행치라 비싼게 좀 있을텐디",
          "original_form": "여행치라 비싼게 좀 있을텐디",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024669.1.20",
          "form": "돈 모자랄까봐 걱정했는데 한국 서울수준이었어ㅋㅋ",
          "original_form": "돈 모자랄까봐 걱정했는데 한국 서울수준이었어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024669.1.21",
          "form": "만 원 이내로 하는 텐동도 있고",
          "original_form": "만 원 이내로 하는 텐동도 있고",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024669.1.22",
          "form": "오코노미야끼는 부침개 주제에 만원대라 좀 비싼 감이 있어",
          "original_form": "오코노미야끼는 부침개 주제에 만원대라 좀 비싼 감이 있어",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024669.1.23",
          "form": "오코노미야끼 무슨 맛으로 먹었냐",
          "original_form": "오코노미야끼 무슨 맛으로 먹었냐",
          "speaker_id": "2",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024669.1.24",
          "form": "해물맛이었어 근데 한국 해물부침개랑은 좀 향이 달랐어",
          "original_form": "해물맛이었어 근데 한국 해물부침개랑은 좀 향이 달랐어",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024669.1.25",
          "form": "그리고 겨자가 들어가",
          "original_form": "그리고 겨자가 들어가",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024669.1.26",
          "form": "여행가면 날씨 걱정되지않냐",
          "original_form": "여행가면 날씨 걱정되지않냐",
          "speaker_id": "2",
          "time": "20210831 17:03"
        }
      ]
