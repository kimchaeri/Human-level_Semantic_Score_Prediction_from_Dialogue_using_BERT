      "utterance": [
        {
          "id": "MDRW2100028714.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028714.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028714.1.3",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028714.1.4",
          "form": "아뇨 세례는 받았는데 지금은 무교에요",
          "original_form": "아뇨 세례는 받았는데 지금은 무교에요",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028714.1.5",
          "form": "저도 원래 종교가 있었는데 지금은 무교에요",
          "original_form": "저도 원래 종교가 있었는데 지금은 무교에요",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028714.1.6",
          "form": "안믿게 된 이유가 뭔가요?",
          "original_form": "안믿게 된 이유가 뭔가요?",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028714.1.7",
          "form": "세례 받은 것도 그냥 어머니가 천주교여서 받은 거고 별로 신을 믿지는 않았어요ㅎㅎ 별로 신이 있는 거 같진 않아서요",
          "original_form": "세례 받은 것도 그냥 어머니가 천주교여서 받은 거고 별로 신을 믿지는 않았어요ㅎㅎ 별로 신이 있는 거 같진 않아서요",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028714.1.8",
          "form": "사후세계를 믿지도 않고...",
          "original_form": "사후세계를 믿지도 않고...",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028714.1.9",
          "form": "저도요... 종교에서 하는 얘기는 다 판타지 같아요",
          "original_form": "저도요... 종교에서 하는 얘기는 다 판타지 같아요",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028714.1.10",
          "form": "그걸 믿으라고 하는것도 좀 어이없고...",
          "original_form": "그걸 믿으라고 하는것도 좀 어이없고...",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028714.1.11",
          "form": "믿어야ㅠ천국 간다는게 좀 웃겨요",
          "original_form": "믿어야ㅠ천국 간다는게 좀 웃겨요",
          "speaker_id": "2",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028714.1.12",
          "form": "착하고 의로운 사람이 교회 안 다니면 지옥 가야 하나요...ㅎㅎ",
          "original_form": "착하고 의로운 사람이 교회 안 다니면 지옥 가야 하나요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028714.1.13",
          "form": "맞아요 너무 일방적이죠",
          "original_form": "맞아요 너무 일방적이죠",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028714.1.14",
          "form": "사이비도 너무 많고...",
          "original_form": "사이비도 너무 많고...",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028714.1.15",
          "form": "헌금한 것도 교회나 시설 운영하는 것보다 목사 사리사욕 채우는 데 쓴다는 얘기도 너무 많이 들었고",
          "original_form": "헌금한 것도 교회나 시설 운영하는 것보다 목사 사리사욕 채우는 데 쓴다는 얘기도 너무 많이 들었고",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028714.1.16",
          "form": "헌금이 제일 이해안돼요 진짜",
          "original_form": "헌금이 제일 이해안돼요 진짜",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028714.1.17",
          "form": "그러니까요... 헌금한다고 집 팔고 이러는 사람도 있대요...",
          "original_form": "그러니까요... 헌금한다고 집 팔고 이러는 사람도 있대요...",
          "speaker_id": "2",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028714.1.18",
          "form": "그건 진짜 선 넘었네요",
          "original_form": "그건 진짜 선 넘었네요",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028714.1.19",
          "form": "가족은 어쩌라고...",
          "original_form": "가족은 어쩌라고...",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028714.1.20",
          "form": "교회에 미치면 그렇게 되나봐요...",
          "original_form": "교회에 미치면 그렇게 되나봐요...",
          "speaker_id": "2",
