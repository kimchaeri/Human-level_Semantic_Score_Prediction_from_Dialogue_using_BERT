      "utterance": [
        {
          "id": "MDRW2100023405.1.1",
          "form": "안녕하세요!  혹시 즐겨보시는 프로그램 있으신가용",
          "original_form": "안녕하세요!  혹시 즐겨보시는 프로그램 있으신가용",
          "speaker_id": "1",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023405.1.2",
          "form": "티비를 잘안봐서요",
          "original_form": "티비를 잘안봐서요",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023405.1.3",
          "form": "넷플릭스나 왓챠 이런거 봐요",
          "original_form": "넷플릭스나 왓챠 이런거 봐요",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023405.1.4",
          "form": "아 유튜브는요?",
          "original_form": "아 유튜브는요?",
          "speaker_id": "1",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023405.1.5",
          "form": "어제는 디피봤어요",
          "original_form": "어제는 디피봤어요",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023405.1.6",
          "form": "요즘 디피가 진짜인기가많나봐요",
          "original_form": "요즘 디피가 진짜인기가많나봐요",
          "speaker_id": "1",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.7",
          "form": "박호두라고",
          "original_form": "박호두라고",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.8",
          "form": "다들그거보시던데",
          "original_form": "다들그거보시던데",
          "speaker_id": "1",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.9",
          "form": "코인하는애있는데",
          "original_form": "코인하는애있는데",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.12",
          "form": "박호두방송봐요",
          "original_form": "박호두방송봐요",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.13",
          "form": "하루에 2500날리고",
          "original_form": "하루에 2500날리고",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.14",
          "form": "좀 웃겨서요",
          "original_form": "좀 웃겨서요",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.15",
          "form": "와ㅋㅋ스케일다르네요..",
          "original_form": "와ㅋㅋ스케일다르네요..",
          "speaker_id": "1",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.16",
          "form": "코인마진거래하는사람인데",
          "original_form": "코인마진거래하는사람인데",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.17",
          "form": "돈을 1천씩넣고",
          "original_form": "돈을 1천씩넣고",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023405.1.18",
          "form": "따는걸 못봤어요",
          "original_form": "따는걸 못봤어요",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023405.1.19",
          "form": "맨날 잃고있더라구요",
          "original_form": "맨날 잃고있더라구요",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023405.1.20",
          "form": "돈잃는게 컨텐츠인듯",
          "original_form": "돈잃는게 컨텐츠인듯",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023405.1.21",
          "form": "돈잃는컨텐츠ㅋㅋㅋㄱ신박하네요",
          "original_form": "돈잃는컨텐츠ㅋㅋㅋㄱ신박하네요",
          "speaker_id": "1",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023405.1.22",
          "form": "디피 재밌어요",
          "original_form": "디피 재밌어요",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023405.1.23",
          "form": "넷플릭스에 있어요",
          "original_form": "넷플릭스에 있어요",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023405.1.24",
          "form": "여자분이세요?",
          "original_form": "여자분이세요?",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023405.1.25",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023405.1.26",
          "form": "그러면 군대얘기라서 좀 싫으실수도",
          "original_form": "그러면 군대얘기라서 좀 싫으실수도",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023405.1.27",
          "form": "헌병대 얘기거든요",
          "original_form": "헌병대 얘기거든요",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023405.1.28",
          "form": "넷플은 킹덤 볼때만 구독했다가 취소했는데",
          "original_form": "넷플은 킹덤 볼때만 구독했다가 취소했는데",
          "speaker_id": "1",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023405.1.29",
          "form": "유튭에서 디피예고편보니 재밌어보이더라구요",
          "original_form": "유튭에서 디피예고편보니 재밌어보이더라구요",
          "speaker_id": "1",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023405.1.30",
          "form": "재밌긴해요",
          "original_form": "재밌긴해요",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023405.1.31",
          "form": "정해인이 군대입대해서",
          "original_form": "정해인이 군대입대해서",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023405.1.32",
          "form": "헌병대로 차출되고",
          "original_form": "헌병대로 차출되고",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023405.1.33",
          "form": "거기서 dp병이 되는데",
          "original_form": "거기서 dp병이 되는데",
          "speaker_id": "2",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023405.1.34",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023405.1.35",
          "form": "머리기르고 일반인처럼 해서 탈영병 잡는 보직이거든요",
          "original_form": "머리기르고 일반인처럼 해서 탈영병 잡는 보직이거든요",
          "speaker_id": "2",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023405.1.36",
          "form": "그내용이예요",
          "original_form": "그내용이예요",
          "speaker_id": "2",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023405.1.37",
          "form": "재밌을거같더라구요 모든에피소드 다올라온상태인가요?",
          "original_form": "재밌을거같더라구요 모든에피소드 다올라온상태인가요?",
          "speaker_id": "1",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023405.1.38",
          "form": "거기나오는 구교환님? 그분도 요새좋은거같구ㅎㅎ",
          "original_form": "거기나오는 구교환님? 그분도 요새좋은거같구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:44"
        }
      ]
