      "utterance": [
        {
          "id": "MDRW2100001938.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001938.1.2",
          "form": "안녕하세요\r\n우아 군대 ㅜㅜㅋ",
          "original_form": "안녕하세요\r\n우아 군대 ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001938.1.3",
          "form": "이제 곧 가겠네요...",
          "original_form": "이제 곧 가겠네요...",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001938.1.4",
          "form": "씁쓸하네요",
          "original_form": "씁쓸하네요",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001938.1.5",
          "form": "앗 전 군대를 안가지만\r\n집안이 군인집안이라  \r\n대략은 알고 있죠",
          "original_form": "앗 전 군대를 안가지만\r\n집안이 군인집안이라  \r\n대략은 알고 있죠",
          "speaker_id": "2",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100001938.1.6",
          "form": "부모님도 군인이세요?",
          "original_form": "부모님도 군인이세요?",
          "speaker_id": "1",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100001938.1.7",
          "form": "부모님은 아니구\r\n외가쪽으로요",
          "original_form": "부모님은 아니구\r\n외가쪽으로요",
          "speaker_id": "2",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100001938.1.8",
          "form": "저도 아빠가 군인이신데",
          "original_form": "저도 아빠가 군인이신데",
          "speaker_id": "1",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100001938.1.9",
          "form": "할아버지부터  군이이에요\r\n삼촌들도",
          "original_form": "할아버지부터  군이이에요\r\n삼촌들도",
          "speaker_id": "2",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100001938.1.10",
          "form": "앗 그럼 더 잘알겠네요",
          "original_form": "앗 그럼 더 잘알겠네요",
          "speaker_id": "2",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100001938.1.11",
          "form": "할아버지 연세가 어떻게 되세요?",
          "original_form": "할아버지 연세가 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100001938.1.12",
          "form": "625때 계셨나",
          "original_form": "625때 계셨나",
          "speaker_id": "1",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100001938.1.13",
          "form": "아 할아버지는 91세 세요",
          "original_form": "아 할아버지는 91세 세요",
          "speaker_id": "2",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100001938.1.14",
          "form": "부사관 출신이시구요 그땐  명칭이 하사관이였죠 ㅎㅎ",
          "original_form": "부사관 출신이시구요 그땐  명칭이 하사관이였죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:18"
        },
        {
          "id": "MDRW2100001938.1.15",
          "form": "7~80년대때는 군대 정말 힘들었다던데",
          "original_form": "7~80년대때는 군대 정말 힘들었다던데",
          "speaker_id": "1",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100001938.1.16",
          "form": "그래도 1년 6개월만 가서 참 다행인거같아요",
          "original_form": "그래도 1년 6개월만 가서 참 다행인거같아요",
          "speaker_id": "1",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100001938.1.17",
          "form": "북한은 10년간다던네",
          "original_form": "북한은 10년간다던네",
          "speaker_id": "1",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100001938.1.18",
          "form": "아마두  더 힘들었을거 같긴해요",
          "original_form": "아마두  더 힘들었을거 같긴해요",
          "speaker_id": "2",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100001938.1.19",
          "form": "근데 군대  몇개월 남았어요?",
          "original_form": "근데 군대  몇개월 남았어요?",
          "speaker_id": "2",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100001938.1.20",
          "form": "저 아직 안갔어요..",
          "original_form": "저 아직 안갔어요..",
          "speaker_id": "1",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100001938.1.21",
          "form": "언제 입대 하냐구여 ㅎㅎ",
          "original_form": "언제 입대 하냐구여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:21"
        },
        {
          "id": "MDRW2100001938.1.22",
          "form": "가려면 몇개월 남았는지 궁금해서요",
          "original_form": "가려면 몇개월 남았는지 궁금해서요",
          "speaker_id": "2",
          "time": "20210819 23:21"
        },
        {
          "id": "MDRW2100001938.1.23",
          "form": "한 1~2년 있으면 가지 않을까요?",
          "original_form": "한 1~2년 있으면 가지 않을까요?",
          "speaker_id": "1",
          "time": "20210819 23:23"
        }
      ]
    }
