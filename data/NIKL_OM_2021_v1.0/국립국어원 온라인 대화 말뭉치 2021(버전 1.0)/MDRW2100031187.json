      "utterance": [
        {
          "id": "MDRW2100031187.1.1",
          "form": "맞아욬ㅋㅋㅋㅋ 돈냄새나는 거 너무 좋아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋ 돈냄새나는 거 너무 좋아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100031187.1.2",
          "form": "자본의 맛이란 너무 좋아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "자본의 맛이란 너무 좋아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100031187.1.3",
          "form": "그래서 제가 넷플을 좋아하죠ㅋㅋㅋ",
          "original_form": "그래서 제가 넷플을 좋아하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100031187.1.4",
          "form": "ㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031187.1.5",
          "form": "혹시 프로젝트 파워라는 영화 아시나요",
          "original_form": "혹시 프로젝트 파워라는 영화 아시나요",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031187.1.6",
          "form": "작년엔가 넷플에서 만든 영화인데",
          "original_form": "작년엔가 넷플에서 만든 영화인데",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031187.1.7",
          "form": "진짜 내용 구리긴 한데",
          "original_form": "진짜 내용 구리긴 한데",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031187.1.8",
          "form": "와중에 돈쓴티는 팍팍 나더라구요 ㅋㅋㅋㅋ",
          "original_form": "와중에 돈쓴티는 팍팍 나더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031187.1.9",
          "form": "완전 처음듣네요ㅋㅋㅋㅋ",
          "original_form": "완전 처음듣네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031187.1.10",
          "form": "그쵸 ㅎㅎ 영화 재미없어서 망한것같아요 ㅋㅋㅋ",
          "original_form": "그쵸 ㅎㅎ 영화 재미없어서 망한것같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031187.1.11",
          "form": "아 혹시 퀸즈갬빗 보셨어요?",
          "original_form": "아 혹시 퀸즈갬빗 보셨어요?",
          "speaker_id": "1",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031187.1.12",
          "form": "이것도 재미있더라고요",
          "original_form": "이것도 재미있더라고요",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031187.1.13",
          "form": "아 봤죠 ㅎㅎ",
          "original_form": "아 봤죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031187.1.14",
          "form": "주인공이 똑똑해서 좋았어요",
          "original_form": "주인공이 똑똑해서 좋았어요",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031187.1.15",
          "form": "근데 뭔가 제 취향은 아니더라구요 ㅋㅋㅋ",
          "original_form": "근데 뭔가 제 취향은 아니더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031187.1.16",
          "form": "저는 그거 1회 틀었다가 앉은자리에서 다봤어요ㅋㅋㅋ",
          "original_form": "저는 그거 1회 틀었다가 앉은자리에서 다봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:31"
        },
        {
          "id": "MDRW2100031187.1.17",
          "form": "너무 예전이긴하진만 돈쓴영화로 치면 아바타 빼놓을 수 없죠ㅋㅋㅋㅋㅋ",
          "original_form": "너무 예전이긴하진만 돈쓴영화로 치면 아바타 빼놓을 수 없죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031187.1.18",
          "form": "퀸즈갬빗 대체로 평이 좋더라구요 ㅎㅎ",
          "original_form": "퀸즈갬빗 대체로 평이 좋더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031187.1.19",
          "form": "아바타 ㅋㅋㅋㅋㅋㅋㅋ 추억의 영화네요",
          "original_form": "아바타 ㅋㅋㅋㅋㅋㅋㅋ 추억의 영화네요",
          "speaker_id": "2",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031187.1.20",
          "form": "그때 정말 센세이션했는데 ㅋㅋㅋㅋ",
          "original_form": "그때 정말 센세이션했는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031187.1.21",
          "form": "진짜 아바타는 지금 생각해도 대자본의 영화에요ㅋㅋㅋㅋ",
          "original_form": "진짜 아바타는 지금 생각해도 대자본의 영화에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100031187.1.22",
          "form": "ㅋㅋㅋㅋ 생각난김에 보고싶네요",
          "original_form": "ㅋㅋㅋㅋ 생각난김에 보고싶네요",
          "speaker_id": "2",
          "time": "20210905 12:54"
        },
        {
          "id": "MDRW2100031187.1.23",
          "form": "내용도 좋았는데",
          "original_form": "내용도 좋았는데",
          "speaker_id": "2",
          "time": "20210905 12:54"
        },
        {
          "id": "MDRW2100031187.1.24",
          "form": "그거 시리즈 또 제작된다고 하지 않았어요??",
          "original_form": "그거 시리즈 또 제작된다고 하지 않았어요??",
