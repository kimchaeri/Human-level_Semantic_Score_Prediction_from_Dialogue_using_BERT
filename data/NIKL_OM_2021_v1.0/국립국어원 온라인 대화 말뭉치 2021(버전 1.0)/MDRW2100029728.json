      "utterance": [
        {
          "id": "MDRW2100029728.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210903 12:23"
        },
        {
          "id": "MDRW2100029728.1.2",
          "form": "안녕하세요! 점심 드셨나요?",
          "original_form": "안녕하세요! 점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210903 12:24"
        },
        {
          "id": "MDRW2100029728.1.3",
          "form": "점심은 아직이지만 늦은 아침은 먹었어요!",
          "original_form": "점심은 아직이지만 늦은 아침은 먹었어요!",
          "speaker_id": "1",
          "time": "20210903 12:24"
        },
        {
          "id": "MDRW2100029728.1.4",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210903 12:24"
        },
        {
          "id": "MDRW2100029728.1.5",
          "form": "저도 오늘 늦게 아침을 먹어서 안먹고 있어요!",
          "original_form": "저도 오늘 늦게 아침을 먹어서 안먹고 있어요!",
          "speaker_id": "2",
          "time": "20210903 12:25"
        },
        {
          "id": "MDRW2100029728.1.6",
          "form": "아침 뭐드셨나요??",
          "original_form": "아침 뭐드셨나요??",
          "speaker_id": "2",
          "time": "20210903 12:25"
        },
        {
          "id": "MDRW2100029728.1.7",
          "form": "주먹밥 먹었어요 김이랑 멸치랑...",
          "original_form": "주먹밥 먹었어요 김이랑 멸치랑...",
          "speaker_id": "1",
          "time": "20210903 12:27"
        },
        {
          "id": "MDRW2100029728.1.8",
          "form": "맛있었겠네요! 아침엔 간단하게 먹은게 좋은것같아요ㅎㅎ",
          "original_form": "맛있었겠네요! 아침엔 간단하게 먹은게 좋은것같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100029728.1.9",
          "form": "어떤 음식 제일 좋아하세요?",
          "original_form": "어떤 음식 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100029728.1.10",
          "form": "어... 설렁탕?",
          "original_form": "어... 설렁탕?",
          "speaker_id": "1",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100029728.1.11",
          "form": "국밥 같은거 좋아해요ㅋㅋ",
          "original_form": "국밥 같은거 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100029728.1.12",
          "form": "저도 국밥 좋아해요~~",
          "original_form": "저도 국밥 좋아해요~~",
          "speaker_id": "2",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100029728.1.13",
          "form": "전 콩나물 국밥 젤 좋아요ㅎㅎ",
          "original_form": "전 콩나물 국밥 젤 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100029728.1.14",
          "form": "헐 맛있죠 콩나물국밥...",
          "original_form": "헐 맛있죠 콩나물국밥...",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100029728.1.15",
          "form": "안그래도 저희동네 콩나물 국밥집이 사라졌더라구요 눈물날뻔",
          "original_form": "안그래도 저희동네 콩나물 국밥집이 사라졌더라구요 눈물날뻔",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100029728.1.16",
          "form": "맞아요 어렸을땐 국밥 이런거 맛있는지 모르겠던데 요즘엔 너무 맛있어요",
          "original_form": "맞아요 어렸을땐 국밥 이런거 맛있는지 모르겠던데 요즘엔 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100029728.1.17",
          "form": "헐ㅜㅜㅜ 진짜 집근처에 잘 가는 식당 없어질때 눈물나죠",
          "original_form": "헐ㅜㅜㅜ 진짜 집근처에 잘 가는 식당 없어질때 눈물나죠",
          "speaker_id": "2",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100029728.1.18",
          "form": "ㅋㅋㅋㅋㅠ name2은 무슨 음식 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋㅠ &name2&은 무슨 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029728.1.19",
          "form": "저는 한식 대체적으로 좋아해요!",
          "original_form": "저는 한식 대체적으로 좋아해요!",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029728.1.20",
          "form": "특히 제육볶음이 제 소울푸드입니다",
          "original_form": "특히 제육볶음이 제 소울푸드입니다",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029728.1.21",
          "form": "헉 저도 제육볶음 좋아해요",
          "original_form": "헉 저도 제육볶음 좋아해요",
          "speaker_id": "1",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100029728.1.22",
          "form": "한식도 다 대체적으로 좋아하는편...말하다보니 배고프네요",
          "original_form": "한식도 다 대체적으로 좋아하는편...말하다보니 배고프네요",
          "speaker_id": "1",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100029728.1.23",
          "form": "맞아요ㅠ 한식 너무 맛있죠",
          "original_form": "맞아요ㅠ 한식 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029728.1.24",
          "form": "그래서 해외여행같은데 가도 한국음식이 제일 맛있는거같더라구요",
          "original_form": "그래서 해외여행같은데 가도 한국음식이 제일 맛있는거같더라구요",
          "speaker_id": "2",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029728.1.25",
          "form": "음식때문에 이민을 못가요ㅋㅋㅋㅋ",
          "original_form": "음식때문에 이민을 못가요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:44"
        }
      ]
