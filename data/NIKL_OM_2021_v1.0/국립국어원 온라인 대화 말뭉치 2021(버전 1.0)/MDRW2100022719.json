      "utterance": [
        {
          "id": "MDRW2100022719.1.1",
          "form": "우리집 맨날 식탁 의자 넣고 다니고",
          "original_form": "우리집 맨날 식탁 의자 넣고 다니고",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022719.1.2",
          "form": "나도 그런거 감당하기 어려울거같아서ㅋㅋㅋ",
          "original_form": "나도 그런거 감당하기 어려울거같아서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022719.1.3",
          "form": "방문 다 안닫으면",
          "original_form": "방문 다 안닫으면",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022719.1.4",
          "form": "외출 못함",
          "original_form": "외출 못함",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022719.1.5",
          "form": "애가 많이 활발했었네",
          "original_form": "애가 많이 활발했었네",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022719.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022719.1.7",
          "form": "근데 어떻게 보면",
          "original_form": "근데 어떻게 보면",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022719.1.8",
          "form": "우리가 10시간 넘게 집을 비워두고 그랬으니까",
          "original_form": "우리가 10시간 넘게 집을 비워두고 그랬으니까",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022719.1.9",
          "form": "할말이 없다",
          "original_form": "할말이 없다",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022719.1.10",
          "form": "미안했지..",
          "original_form": "미안했지..",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.11",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.12",
          "form": "갑자기 슬퍼지네 흑흑",
          "original_form": "갑자기 슬퍼지네 흑흑",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.13",
          "form": "나도 그래서",
          "original_form": "나도 그래서",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.14",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.15",
          "form": "중년쯤에 입양하고싶음",
          "original_form": "중년쯤에 입양하고싶음",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.16",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.17",
          "form": "ㄹㅇ경제적으로도 여유있고",
          "original_form": "ㄹㅇ경제적으로도 여유있고",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.18",
          "form": "시간도많을때",
          "original_form": "시간도많을때",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.19",
          "form": "맞음맞음",
          "original_form": "맞음맞음",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.20",
          "form": "좋은집사서 입양하고싶다",
          "original_form": "좋은집사서 입양하고싶다",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.21",
          "form": "그래야함",
          "original_form": "그래야함",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022719.1.22",
          "form": "그 보더로운 생활",
          "original_form": "그 보더로운 생활",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
