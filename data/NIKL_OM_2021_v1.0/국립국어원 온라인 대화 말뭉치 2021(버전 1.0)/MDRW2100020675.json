      "utterance": [
        {
          "id": "MDRW2100020675.1.1",
          "form": "맞아요 남들이 추천하는덴 다 이유가 있더라구요",
          "original_form": "맞아요 남들이 추천하는덴 다 이유가 있더라구요",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020675.1.2",
          "form": "그럼 그럼요",
          "original_form": "그럼 그럼요",
          "speaker_id": "1",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020675.1.3",
          "form": "노브랜드 버거 먹어봤어요?",
          "original_form": "노브랜드 버거 먹어봤어요?",
          "speaker_id": "1",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020675.1.4",
          "form": "전 안먹어봤는데 괜찮다고들 해서 궁금해요",
          "original_form": "전 안먹어봤는데 괜찮다고들 해서 궁금해요",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020675.1.5",
          "form": "아뇨 주변에 생겼는데 아직 못먹어봤어요ㅠ",
          "original_form": "아뇨 주변에 생겼는데 아직 못먹어봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020675.1.6",
          "form": "하필 이사를 와서...",
          "original_form": "하필 이사를 와서...",
          "speaker_id": "2",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020675.1.7",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020675.1.8",
          "form": "띠로리",
          "original_form": "띠로리",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020675.1.9",
          "form": "이번에 이삭버거도 생겼다는데",
          "original_form": "이번에 이삭버거도 생겼다는데",
          "speaker_id": "2",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020675.1.10",
          "form": "저희 지역은 언제 들어올까 궁금하네요",
          "original_form": "저희 지역은 언제 들어올까 궁금하네요",
          "speaker_id": "2",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020675.1.11",
          "form": "헐 너무맛있겠다",
          "original_form": "헐 너무맛있겠다",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020675.1.12",
          "form": "토스트도 맛있는데",
          "original_form": "토스트도 맛있는데",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020675.1.13",
          "form": "이삭토스트 맛있으니 버거도 짱맛있겠지..",
          "original_form": "이삭토스트 맛있으니 버거도 짱맛있겠지..",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020675.1.14",
          "form": "버거까지 나온다니",
          "original_form": "버거까지 나온다니",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020675.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020675.1.16",
          "form": "아주그냥 통장을 거덜내려고",
          "original_form": "아주그냥 통장을 거덜내려고",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020675.1.17",
          "form": "급 토스트도 끌리네요",
          "original_form": "급 토스트도 끌리네요",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020675.1.18",
          "form": "작정을했구나",
          "original_form": "작정을했구나",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020675.1.19",
          "form": "이삭가면 뭐 먹어요",
          "original_form": "이삭가면 뭐 먹어요",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020675.1.20",
          "form": "저는 일단 뭐가 됐든",
          "original_form": "저는 일단 뭐가 됐든",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020675.1.21",
          "form": "감자가 들어가야해요",
          "original_form": "감자가 들어가야해요",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020675.1.22",
          "form": "그 감자 아시죠",
          "original_form": "그 감자 아시죠",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020675.1.23",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020675.1.24",
          "form": "해쉬브라운",
          "original_form": "해쉬브라운",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020675.1.25",
          "form": "해시브라운같은거",
          "original_form": "해시브라운같은거",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020675.1.26",
          "form": "올ㅋ",
          "original_form": "올ㅋ",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020675.1.27",
          "form": "정답",
          "original_form": "정답",
          "speaker_id": "2",
          "time": "20210828 11:06"
        }
      ]
    }
