      "utterance": [
        {
          "id": "MDRW2100044566.1.1",
          "form": "일한지 얼마나 되셨어요?",
          "original_form": "일한지 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044566.1.2",
          "form": "전 알바 처음 시작한지 2년 반 정도고",
          "original_form": "전 알바 처음 시작한지 2년 반 정도고",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044566.1.3",
          "form": "와 오래 일하셨네요",
          "original_form": "와 오래 일하셨네요",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044566.1.4",
          "form": "이 일을 시작하고는 2년 다 되어가는 거 같아요",
          "original_form": "이 일을 시작하고는 2년 다 되어가는 거 같아요",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044566.1.5",
          "form": "오래 하다보니까 쉽게 다른 일을 못하겠더라구요",
          "original_form": "오래 하다보니까 쉽게 다른 일을 못하겠더라구요",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044566.1.6",
          "form": "아 그런게 있죠 ㅠ",
          "original_form": "아 그런게 있죠 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044566.1.7",
          "form": "저도 여기 만2년 채워가는데",
          "original_form": "저도 여기 만2년 채워가는데",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044566.1.8",
          "form": "스트레스 받지만 딴데 옮기려니까 약간 무섭더라구요 ㅠ",
          "original_form": "스트레스 받지만 딴데 옮기려니까 약간 무섭더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044566.1.9",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044566.1.10",
          "form": "선뜻 엄두가 안나요..ㅋㅋㅋㅋ",
          "original_form": "선뜻 엄두가 안나요..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044566.1.11",
          "form": "또 새로 적응하기도 두렵고 엄두도 안나죠",
          "original_form": "또 새로 적응하기도 두렵고 엄두도 안나죠",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044566.1.12",
          "form": "ㅋㅋㅋㅋ다들 똑같네요",
          "original_form": "ㅋㅋㅋㅋ다들 똑같네요",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044566.1.13",
          "form": "그쵸 ㅋㅋㅋ 벌써부터 이러면 안되는데",
          "original_form": "그쵸 ㅋㅋㅋ 벌써부터 이러면 안되는데",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044566.1.14",
          "form": "사무직이면 계약직?같은 건 아닌거죠??",
          "original_form": "사무직이면 계약직?같은 건 아닌거죠??",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044566.1.15",
          "form": "네 계약직은 아니에요",
          "original_form": "네 계약직은 아니에요",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044566.1.16",
          "form": "근데 페이가 너무 낮고 컴플레인 받는 부서라..ㅠ",
          "original_form": "근데 페이가 너무 낮고 컴플레인 받는 부서라..ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044566.1.17",
          "form": "아 정말요.?.?",
          "original_form": "아 정말요.?.?",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044566.1.18",
          "form": "혹시 페이가 어느정도 되는지 물어봐도 돼요?",
          "original_form": "혹시 페이가 어느정도 되는지 물어봐도 돼요?",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044566.1.19",
          "form": "초과근무 따라서 다른데 200초중반정도예요",
          "original_form": "초과근무 따라서 다른데 200초중반정도예요",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044566.1.20",
          "form": "아하 좋은편은 아니네요,,",
          "original_form": "아하 좋은편은 아니네요,,",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044566.1.21",
          "form": "컴플레인 받는 부서면 엄청 힘드실텐데",
          "original_form": "컴플레인 받는 부서면 엄청 힘드실텐데",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044566.1.22",
          "form": "네네 ㅋㅋㅋ ㅠ 솔직히",
          "original_form": "네네 ㅋㅋㅋ ㅠ 솔직히",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044566.1.23",
          "form": "제가 왜 이런 하소연을 듣고있어야 하는지 현타와요",
          "original_form": "제가 왜 이런 하소연을 듣고있어야 하는지 현타와요",
          "speaker_id": "1",
          "time": "20210907 23:29"
        }
      ]
    }
