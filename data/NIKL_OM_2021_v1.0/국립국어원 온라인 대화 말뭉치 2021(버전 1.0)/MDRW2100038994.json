      "utterance": [
        {
          "id": "MDRW2100038994.1.1",
          "form": "취업성공 글을 올렸네",
          "original_form": "취업성공 글을 올렸네",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100038994.1.2",
          "form": "그러게 댓글에 축하글이 올라왔다",
          "original_form": "그러게 댓글에 축하글이 올라왔다",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100038994.1.3",
          "form": "아직 세상은 따뜻한가봐",
          "original_form": "아직 세상은 따뜻한가봐",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100038994.1.4",
          "form": "하늘에 계신 어머니가 보낸 편지처럼 적은 댓글이 있었다네",
          "original_form": "하늘에 계신 어머니가 보낸 편지처럼 적은 댓글이 있었다네",
          "speaker_id": "1",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100038994.1.5",
          "form": "감동적이다",
          "original_form": "감동적이다",
          "speaker_id": "2",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100038994.1.6",
          "form": "그러게 말이야",
          "original_form": "그러게 말이야",
          "speaker_id": "1",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100038994.1.7",
          "form": "변호사님도 엄마가 일찍 돌아가셨네",
          "original_form": "변호사님도 엄마가 일찍 돌아가셨네",
          "speaker_id": "2",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100038994.1.8",
          "form": "그러게 그래서 이문제에 눈시울이 불어졋나보다",
          "original_form": "그러게 그래서 이문제에 눈시울이 불어졋나보다",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100038994.1.9",
          "form": "가출방황하고 다시 집으로 돌아갔다네",
          "original_form": "가출방황하고 다시 집으로 돌아갔다네",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100038994.1.10",
          "form": "가출청소년에서 사법시험으로",
          "original_form": "가출청소년에서 사법시험으로",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100038994.1.11",
          "form": "인생반전이다",
          "original_form": "인생반전이다",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100038994.1.12",
          "form": "그러게 자기도 공부에 자신이 잇었다고 하니까",
          "original_form": "그러게 자기도 공부에 자신이 잇었다고 하니까",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100038994.1.13",
          "form": "그래도 사법시험이 싶지않지",
          "original_form": "그래도 사법시험이 싶지않지",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100038994.1.14",
          "form": "엉 그건 맞다",
          "original_form": "엉 그건 맞다",
          "speaker_id": "1",
