      "utterance": [
        {
          "id": "MDRW2100022231.1.1",
          "form": "근데 또 데려올 엄두는 안나서",
          "original_form": "근데 또 데려올 엄두는 안나서",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022231.1.2",
          "form": "그냥 대리만족하기로...",
          "original_form": "그냥 대리만족하기로...",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022231.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022231.1.4",
          "form": "저는 출퇴근 하는 동안 반려동물이 집에서 혼자 외로울까봐",
          "original_form": "저는 출퇴근 하는 동안 반려동물이 집에서 혼자 외로울까봐",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100022231.1.5",
          "form": "아 혼자사세요?",
          "original_form": "아 혼자사세요?",
          "speaker_id": "2",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100022231.1.6",
          "form": "선뜻 입양을 못하겠더라고요",
          "original_form": "선뜻 입양을 못하겠더라고요",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100022231.1.7",
          "form": "가족들도 출퇴근해요",
          "original_form": "가족들도 출퇴근해요",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100022231.1.8",
          "form": "학교 가거나",
          "original_form": "학교 가거나",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100022231.1.9",
          "form": "강아지는 혼자 냅두면 불안해하더라구여",
          "original_form": "강아지는 혼자 냅두면 불안해하더라구여",
          "speaker_id": "2",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100022231.1.10",
          "form": "아 그러면",
          "original_form": "아 그러면",
          "speaker_id": "2",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100022231.1.11",
          "form": "혼자있게 되겠네요",
          "original_form": "혼자있게 되겠네요",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022231.1.12",
          "form": "네 그래서 강아지 입양은 힘들 듯해요",
          "original_form": "네 그래서 강아지 입양은 힘들 듯해요",
          "speaker_id": "1",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022231.1.13",
          "form": "그럼 고양이가 딱인데",
          "original_form": "그럼 고양이가 딱인데",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022231.1.14",
          "form": "고양이는 털이....",
          "original_form": "고양이는 털이....",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022231.1.15",
          "form": "나중에 은퇴하고?는 한 마리 키워볼까 싶어요",
          "original_form": "나중에 은퇴하고?는 한 마리 키워볼까 싶어요",
          "speaker_id": "1",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022231.1.16",
          "form": "전 나중에 주택살게되면",
          "original_form": "전 나중에 주택살게되면",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022231.1.17",
          "form": "마당에 두마리 키워볼까 싶어요",
          "original_form": "마당에 두마리 키워볼까 싶어요",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022231.1.18",
          "form": "외롭지않게",
          "original_form": "외롭지않게",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022231.1.19",
          "form": "아무래도 아파트는 키우는 데 신경쓸 게 많죠",
          "original_form": "아무래도 아파트는 키우는 데 신경쓸 게 많죠",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022231.1.20",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022231.1.21",
          "form": "네 반려동물도 2마리 이상이면 어울려 지내니 더 좋은 것 같아요",
          "original_form": "네 반려동물도 2마리 이상이면 어울려 지내니 더 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022231.1.22",
          "form": "강아지는 두마리 키우면 서로 잘 놀더라구여",
          "original_form": "강아지는 두마리 키우면 서로 잘 놀더라구여",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022231.1.23",
          "form": "좋네요ㅎㅎ",
          "original_form": "좋네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:27"
        }
      ]
