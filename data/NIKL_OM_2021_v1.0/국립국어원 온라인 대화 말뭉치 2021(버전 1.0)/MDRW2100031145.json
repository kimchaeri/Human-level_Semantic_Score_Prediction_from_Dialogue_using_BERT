      "utterance": [
        {
          "id": "MDRW2100031145.1.1",
          "form": "예전에는 강아지 밥을 그냥 밥으로 줬어요",
          "original_form": "예전에는 강아지 밥을 그냥 밥으로 줬어요",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031145.1.2",
          "form": "요즘은 그럼 어떤 걸 줘요",
          "original_form": "요즘은 그럼 어떤 걸 줘요",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031145.1.3",
          "form": "자료도 먹지만 간식까지 주더라구요",
          "original_form": "자료도 먹지만 간식까지 주더라구요",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031145.1.4",
          "form": "수제간식 시장이 엄청 커졌죠",
          "original_form": "수제간식 시장이 엄청 커졌죠",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031145.1.5",
          "form": "사람이 직접 만들어 주기도 해요 집에서",
          "original_form": "사람이 직접 만들어 주기도 해요 집에서",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031145.1.6",
          "form": "돈이 많이 가겠네요",
          "original_form": "돈이 많이 가겠네요",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031145.1.7",
          "form": "그런데 강아지가 잘 먹는 모습을 보면",
          "original_form": "그런데 강아지가 잘 먹는 모습을 보면",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031145.1.8",
          "form": "노력이 헛되지 않아요",
          "original_form": "노력이 헛되지 않아요",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031145.1.9",
          "form": "저는 귀찮아서 못 만들어 줄 것 같아요",
          "original_form": "저는 귀찮아서 못 만들어 줄 것 같아요",
          "speaker_id": "1",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031145.1.10",
          "form": "막상 키워 보면 해 주게 될 걸요",
          "original_form": "막상 키워 보면 해 주게 될 걸요",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031145.1.11",
          "form": "얼마나 잘 먹는데요",
          "original_form": "얼마나 잘 먹는데요",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031145.1.12",
          "form": "일단 강아지를 키우고 싶네요",
          "original_form": "일단 강아지를 키우고 싶네요",
          "speaker_id": "1",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031145.1.13",
          "form": "저도요 다 해 줄 수 있을 거 같아요",
          "original_form": "저도요 다 해 줄 수 있을 거 같아요",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031145.1.14",
          "form": "아프면 어쩌죠",
          "original_form": "아프면 어쩌죠",
          "speaker_id": "1",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031145.1.15",
          "form": "병원비가 제법 될 거예요",
          "original_form": "병원비가 제법 될 거예요",
          "speaker_id": "2",
          "time": "20210904 17:29"
        }
