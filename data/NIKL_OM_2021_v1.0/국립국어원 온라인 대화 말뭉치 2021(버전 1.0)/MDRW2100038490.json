      "utterance": [
        {
          "id": "MDRW2100038490.1.1",
          "form": "ㅇㅇ아니 대체 이럴거면",
          "original_form": "ㅇㅇ아니 대체 이럴거면",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100038490.1.2",
          "form": "대체 뭘 한건데;;;",
          "original_form": "대체 뭘 한건데;;;",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100038490.1.3",
          "form": "펀딩은 왜받은거임",
          "original_form": "펀딩은 왜받은거임",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100038490.1.4",
          "form": "펀딩이 무료나눔이야?",
          "original_form": "펀딩이 무료나눔이야?",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100038490.1.5",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038490.1.6",
          "form": "사기죄로 다 고소때려야대 진짜",
          "original_form": "사기죄로 다 고소때려야대 진짜",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038490.1.7",
          "form": "펀딩은 후원이 아니고 사전 구매인데",
          "original_form": "펀딩은 후원이 아니고 사전 구매인데",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038490.1.8",
          "form": "누가보면 기부한줄;;",
          "original_form": "누가보면 기부한줄;;",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038490.1.9",
          "form": "게임 상태나 일러 보면",
          "original_form": "게임 상태나 일러 보면",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038490.1.10",
          "form": "뭐 팀 내부에서 마찰이 있거나 했을것같긴 해",
          "original_form": "뭐 팀 내부에서 마찰이 있거나 했을것같긴 해",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038490.1.11",
          "form": "그렇지만 투자 받았으면",
          "original_form": "그렇지만 투자 받았으면",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100038490.1.12",
          "form": "받은만큼은 내놔야지",
          "original_form": "받은만큼은 내놔야지",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100038490.1.13",
          "form": "뭐하는짓인지 원",
          "original_form": "뭐하는짓인지 원",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100038490.1.14",
          "form": "ㅇㅇ 게임 만드는 만화? 이것도",
          "original_form": "ㅇㅇ 게임 만드는 만화? 이것도",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100038490.1.15",
          "form": "작년 8월에 애매하게 올라오다 만거 보면",
          "original_form": "작년 8월에 애매하게 올라오다 만거 보면",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100038490.1.16",
          "form": "그쯤에서 팀 내에 마찰이 생긴거같은데 ...",
          "original_form": "그쯤에서 팀 내에 마찰이 생긴거같은데 ...",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100038490.1.17",
          "form": "그건 그거고 이건 이거지;;",
          "original_form": "그건 그거고 이건 이거지;;",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100038490.1.18",
          "form": "ㅇㅇ이게 뭔",
          "original_form": "ㅇㅇ이게 뭔",
          "speaker_id": "2",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100038490.1.19",
          "form": "대학생 모임 프로젝트도 아니고",
          "original_form": "대학생 모임 프로젝트도 아니고",
          "speaker_id": "2",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100038490.1.20",
          "form": "다른 일러레를 구하던 스작을 구하던 서버 관리자를 구하던",
          "original_form": "다른 일러레를 구하던 스작을 구하던 서버 관리자를 구하던",
          "speaker_id": "2",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100038490.1.21",
          "form": "ㅇㅇ 그렇다고 찐 무과금이 플레이하기 쉬운 구조도 아니고",
          "original_form": "ㅇㅇ 그렇다고 찐 무과금이 플레이하기 쉬운 구조도 아니고",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100038490.1.22",
          "form": "뭔가 해서 돌렸어야지",
          "original_form": "뭔가 해서 돌렸어야지",
          "speaker_id": "2",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100038490.1.23",
          "form": "어떻게든 과금을 하게 만들었으면서 내용물이 이모양이면..ㅋㅋㅋ....",
          "original_form": "어떻게든 과금을 하게 만들었으면서 내용물이 이모양이면..ㅋㅋㅋ....",
          "speaker_id": "1",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100038490.1.24",
          "form": "ㅇㅇ성우는 진짜",
          "original_form": "ㅇㅇ성우는 진짜",
          "speaker_id": "2",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100038490.1.25",
          "form": "왜 고용한거임",
          "original_form": "왜 고용한거임",
          "speaker_id": "2",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100038490.1.26",
          "form": "노이즈마켓팅?",
          "original_form": "노이즈마켓팅?",
          "speaker_id": "2",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100038490.1.27",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100038490.1.28",
          "form": "걍",
          "original_form": "걍",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100038490.1.29",
          "form": "성우 있으면 잘팔리겠지??",
          "original_form": "성우 있으면 잘팔리겠지??",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100038490.1.30",
          "form": "생각만 하고",
          "original_form": "생각만 하고",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100038490.1.31",
          "form": "비용이나 수익 생각은 하나도 안한듯 ㅋㅋㅋ",
          "original_form": "비용이나 수익 생각은 하나도 안한듯 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100038490.1.32",
          "form": "ㅇㅇㄹㅇ운영이나 게임에대해서는",
          "original_form": "ㅇㅇㄹㅇ운영이나 게임에대해서는",
          "speaker_id": "2",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100038490.1.33",
          "form": "전혀 생각안한거",
          "original_form": "전혀 생각안한거",
          "speaker_id": "2",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100038490.1.34",
          "form": "게임에서 다티나",
          "original_form": "게임에서 다티나",
          "speaker_id": "2",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100038490.1.35",
          "form": "이 게임은 걍 대학생 조별과제로",
          "original_form": "이 게임은 걍 대학생 조별과제로",
          "speaker_id": "2",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100038490.1.36",
          "form": "끝났어야했다",
          "original_form": "끝났어야했다",
          "speaker_id": "2",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100038490.1.37",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100038490.1.38",
          "form": "ㅇㅇㅋㅋㅋㅋ 진짜...",
          "original_form": "ㅇㅇㅋㅋㅋㅋ 진짜...",
