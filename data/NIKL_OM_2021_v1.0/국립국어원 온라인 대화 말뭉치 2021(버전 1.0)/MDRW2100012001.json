      "utterance": [
        {
          "id": "MDRW2100012001.1.1",
          "form": "동물 좋아하세요",
          "original_form": "동물 좋아하세요",
          "speaker_id": "1",
          "time": "20210824 18:22"
        },
        {
          "id": "MDRW2100012001.1.2",
          "form": "막 엄청 예뻐하는 편은 아닌데 평범히 좋아해요ㅎㅎ",
          "original_form": "막 엄청 예뻐하는 편은 아닌데 평범히 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:22"
        },
        {
          "id": "MDRW2100012001.1.3",
          "form": "동물 많이 좋아하세요?",
          "original_form": "동물 많이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 18:22"
        },
        {
          "id": "MDRW2100012001.1.4",
          "form": "저는 좋아하는 편이에요",
          "original_form": "저는 좋아하는 편이에요",
          "speaker_id": "1",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100012001.1.5",
          "form": "좋아하는 동물 있어요?",
          "original_form": "좋아하는 동물 있어요?",
          "speaker_id": "1",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100012001.1.6",
          "form": "강아지...?!",
          "original_form": "강아지...?!",
          "speaker_id": "2",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100012001.1.7",
          "form": "지원님은요?",
          "original_form": "지원님은요?",
          "speaker_id": "2",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100012001.1.8",
          "form": "운명이다",
          "original_form": "운명이다",
          "speaker_id": "1",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100012001.1.9",
          "form": "저 진돗개 키워요",
          "original_form": "저 진돗개 키워요",
          "speaker_id": "1",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100012001.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100012001.1.11",
          "form": "와!!!",
          "original_form": "와!!!",
          "speaker_id": "2",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100012001.1.12",
          "form": "진돗개 어때요",
          "original_form": "진돗개 어때요",
          "speaker_id": "1",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100012001.1.13",
          "form": "어.. 좋아히지않는다고 하셔도 이해합니다",
          "original_form": "어.. 좋아히지않는다고 하셔도 이해합니다",
          "speaker_id": "1",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100012001.1.14",
          "form": "진돗개사진보고싶어요ㅎㅎ",
          "original_form": "진돗개사진보고싶어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100012001.1.15",
          "form": "실제로 보면 조금",
          "original_form": "실제로 보면 조금",
          "speaker_id": "2",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100012001.1.16",
          "form": "무서워할수도있어요",
          "original_form": "무서워할수도있어요",
          "speaker_id": "2",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100012001.1.17",
          "form": "이해합니다",
          "original_form": "이해합니다",
          "speaker_id": "1",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100012001.1.18",
          "form": "그럴 수 있어요",
          "original_form": "그럴 수 있어요",
          "speaker_id": "1",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100012001.1.19",
          "form": "누구나 다 진돗개를 좋아할 수는 없으니까",
          "original_form": "누구나 다 진돗개를 좋아할 수는 없으니까",
          "speaker_id": "1",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100012001.1.20",
          "form": "제가 쫄보여서 그래요ㅎㅠ",
          "original_form": "제가 쫄보여서 그래요ㅎㅠ",
          "speaker_id": "2",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100012001.1.21",
          "form": "아니에요 사람마다 다른거니까",
          "original_form": "아니에요 사람마다 다른거니까",
          "speaker_id": "1",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100012001.1.22",
          "form": "이름 물어봐줄래요",
          "original_form": "이름 물어봐줄래요",
          "speaker_id": "1",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100012001.1.23",
          "form": "감사합니다ㅎㅎ안그래도 진돗개 이름 뭐냐고 치고있었어요",
          "original_form": "감사합니다ㅎㅎ안그래도 진돗개 이름 뭐냐고 치고있었어요",
          "speaker_id": "2",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100012001.1.24",
          "form": "name3이에요",
          "original_form": "&name3&이에요",
          "speaker_id": "1",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100012001.1.25",
          "form": "으악",
          "original_form": "으악",
          "speaker_id": "2",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100012001.1.26",
          "form": "귀여워요ㅠㅜ",
          "original_form": "귀여워요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100012001.1.27",
          "form": "순하지는..않은데 아빠가 순하게 자라라고 지어줬어요",
          "original_form": "순하지는..않은데 아빠가 순하게 자라라고 지어줬어요",
