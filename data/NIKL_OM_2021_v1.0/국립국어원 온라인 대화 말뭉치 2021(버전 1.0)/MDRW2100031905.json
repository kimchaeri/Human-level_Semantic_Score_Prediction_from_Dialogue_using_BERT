        {
          "id": "MDRW2100031905.1.1",
          "form": "영화 좋아하시나요",
          "original_form": "영화 좋아하시나요",
          "speaker_id": "1",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031905.1.2",
          "form": "넹넹 종종 시간 나면 보려고 노력해요",
          "original_form": "넹넹 종종 시간 나면 보려고 노력해요",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031905.1.3",
          "form": "넷플릭스 하시나요",
          "original_form": "넷플릭스 하시나요",
          "speaker_id": "1",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100031905.1.4",
          "form": "하죠! 잘 보진 못해도 구독은 해놔야 든든하더라구요",
          "original_form": "하죠! 잘 보진 못해도 구독은 해놔야 든든하더라구요",
          "speaker_id": "2",
          "time": "20210905 09:31"
        },
        {
          "id": "MDRW2100031905.1.5",
          "form": "넷플릭스 보세요?",
          "original_form": "넷플릭스 보세요?",
          "speaker_id": "2",
          "time": "20210905 09:31"
        },
        {
          "id": "MDRW2100031905.1.6",
          "form": "네 요새 디피 보고 있어요 ㅎㅎ",
          "original_form": "네 요새 디피 보고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:01"
        },
        {
          "id": "MDRW2100031905.1.7",
          "form": "되게 재밌다면서요 ㅋㅋㅋㅋ",
          "original_form": "되게 재밌다면서요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:09"
        },
        {
          "id": "MDRW2100031905.1.8",
          "form": "남친이랑 만나서 봐야지하고 아껴두고있어요",
          "original_form": "남친이랑 만나서 봐야지하고 아껴두고있어요",
          "speaker_id": "2",
          "time": "20210905 10:09"
        },
        {
          "id": "MDRW2100031905.1.9",
          "form": "네 진짜 핵잼입니다",
          "original_form": "네 진짜 핵잼입니다",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031905.1.10",
          "form": "2021 최고인듯요",
          "original_form": "2021 최고인듯요",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031905.1.11",
          "form": "오오 그정도로! 꼭 봐야겠어요",
          "original_form": "오오 그정도로! 꼭 봐야겠어요",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031905.1.12",
          "form": "네 연기가 후덜덜",
          "original_form": "네 연기가 후덜덜",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031905.1.13",
          "form": "아내분과 같이 보셨나옹",
          "original_form": "아내분과 같이 보셨나옹",
          "speaker_id": "2",
          "time": "20210905 10:53"
        },
        {
          "id": "MDRW2100031905.1.14",
          "form": "네 ㅋㅋ 여자들도",
          "original_form": "네 ㅋㅋ 여자들도",
          "speaker_id": "1",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100031905.1.15",
          "form": "재밌게 보더라구요",
          "original_form": "재밌게 보더라구요",
          "speaker_id": "1",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100031905.1.16",
          "form": "오오 보는데 한 반나절 걸리나요?",
          "original_form": "오오 보는데 한 반나절 걸리나요?",
          "speaker_id": "2",
          "time": "20210905 11:00"
        },
        {
