      "utterance": [
        {
          "id": "MDRW2100037645.1.1",
          "form": "하는 운동 있으세요?",
          "original_form": "하는 운동 있으세요?",
          "speaker_id": "1",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100037645.1.2",
          "form": "홈트 가끔해요",
          "original_form": "홈트 가끔해요",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100037645.1.3",
          "form": "꾸준히 해야하는데 그게 어렵더라구요",
          "original_form": "꾸준히 해야하는데 그게 어렵더라구요",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100037645.1.4",
          "form": "오 홈트 안지겨우세요?",
          "original_form": "오 홈트 안지겨우세요?",
          "speaker_id": "1",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100037645.1.5",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100037645.1.6",
          "form": "지겨워요ㅋㅋㅋ",
          "original_form": "지겨워요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100037645.1.7",
          "form": "그래서그런가 잘 안하게되더라구요",
          "original_form": "그래서그런가 잘 안하게되더라구요",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100037645.1.8",
          "form": "좀 귀찮기도하고",
          "original_form": "좀 귀찮기도하고",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100037645.1.9",
          "form": "저도 홈트 제의지로 절대못해요 ㅋㅋㅋ",
          "original_form": "저도 홈트 제의지로 절대못해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100037645.1.10",
          "form": "홈트로 근력운동 하시나요??",
          "original_form": "홈트로 근력운동 하시나요??",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100037645.1.11",
          "form": "근력운동은 팔굽혀펴기 정도?",
          "original_form": "근력운동은 팔굽혀펴기 정도?",
          "speaker_id": "2",
          "time": "20210901 14:57"
        },
        {
          "id": "MDRW2100037645.1.12",
          "form": "딱히 유산소운동, 근력운동 이렇게 자세히 하진않아요",
          "original_form": "딱히 유산소운동, 근력운동 이렇게 자세히 하진않아요",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100037645.1.13",
          "form": "하긴 저도 그런거같아요 저도 시키는대로..",
          "original_form": "하긴 저도 그런거같아요 저도 시키는대로..",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037645.1.14",
          "form": "저도 집에서는 요가 햇었는데 아무 효과도 없는거같아서 중단햇어요",
          "original_form": "저도 집에서는 요가 햇었는데 아무 효과도 없는거같아서 중단햇어요",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037645.1.15",
          "form": "그래서 다들 피티하고 그러나봐요",
          "original_form": "그래서 다들 피티하고 그러나봐요",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100037645.1.16",
          "form": "친구가 피티 하고있는데 효과가 있는것같더라구요",
          "original_form": "친구가 피티 하고있는데 효과가 있는것같더라구요",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100037645.1.17",
          "form": "몸이 확실히 좀 탄탄해졌어요",
          "original_form": "몸이 확실히 좀 탄탄해졌어요",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100037645.1.18",
          "form": "와 탄탄한 몸 진짜 가지고싶어요",
          "original_form": "와 탄탄한 몸 진짜 가지고싶어요",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100037645.1.19",
          "form": "피티 비싸지만 않으면 진짜 좋을거같아요 제 몸 개인관리사가 있는거니까",
          "original_form": "피티 비싸지만 않으면 진짜 좋을거같아요 제 몸 개인관리사가 있는거니까",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100037645.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100037645.1.21",
          "form": "이 친구도 피티 이번에 받고는",
          "original_form": "이 친구도 피티 이번에 받고는",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100037645.1.22",
          "form": "어느정도 되면 자기 스스로 할거라고 햇어요",
          "original_form": "어느정도 되면 자기 스스로 할거라고 햇어요",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100037645.1.23",
          "form": "그러명서 추천하더라구요",
          "original_form": "그러명서 추천하더라구요",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100037645.1.24",
          "form": "혼자할때랑은 확실히 다르긴 다르다고",
          "original_form": "혼자할때랑은 확실히 다르긴 다르다고",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100037645.1.25",
          "form": "맞아요 자세교정이나 재활같이 목적이 잇으면 더 효과볼거같아요",
          "original_form": "맞아요 자세교정이나 재활같이 목적이 잇으면 더 효과볼거같아요",
          "speaker_id": "1",
          "time": "20210902 12:10"
        },
        {
          "id": "MDRW2100037645.1.26",
          "form": "멎아요 잘못된 자세로만 하면 다른곳 다칠수도 있어서",
          "original_form": "멎아요 잘못된 자세로만 하면 다른곳 다칠수도 있어서",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100037645.1.27",
          "form": "저도 시간 많으면 받아볼까 고민되더라구요",
          "original_form": "저도 시간 많으면 받아볼까 고민되더라구요",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100037645.1.28",
          "form": "오 피티 진짜 효율적인거같아요 가격은 안 효율적이지만  ㅜ",
          "original_form": "오 피티 진짜 효율적인거같아요 가격은 안 효율적이지만  ㅜ",
          "speaker_id": "1",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100037645.1.29",
          "form": "저도 피티 가격만 괜찮으면 받아보고싶어요!",
          "original_form": "저도 피티 가격만 괜찮으면 받아보고싶어요!",
          "speaker_id": "1",
          "time": "20210902 17:28"
        }
      ]
    }
