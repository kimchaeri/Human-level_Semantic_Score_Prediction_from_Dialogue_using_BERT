      "utterance": [
        {
          "id": "MDRW2100026233.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026233.1.2",
          "form": "뭐 보시는거 있으세요?",
          "original_form": "뭐 보시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026233.1.3",
          "form": "스우파랑 런닝맨 일바이일 보네요ㅎㅎ 가벼운거 봐요",
          "original_form": "스우파랑 런닝맨 일바이일 보네요ㅎㅎ 가벼운거 봐요",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026233.1.4",
          "form": "런닝맨빼고 모르는거네요ㅋㅋ",
          "original_form": "런닝맨빼고 모르는거네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026233.1.5",
          "form": "오락프로죠?",
          "original_form": "오락프로죠?",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026233.1.6",
          "form": "스우파 지난주부터 했대요 여자댄서들 춤대결이에요",
          "original_form": "스우파 지난주부터 했대요 여자댄서들 춤대결이에요",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026233.1.7",
          "form": "아맞다",
          "original_form": "아맞다",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026233.1.8",
          "form": "그 핫한프로그램 유트브에서 짤 몇개본것같아요",
          "original_form": "그 핫한프로그램 유트브에서 짤 몇개본것같아요",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026233.1.9",
          "form": "다들 강추하더라고요",
          "original_form": "다들 강추하더라고요",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026233.1.10",
          "form": "네 저도 추천에 봤는데 완전 혼이 나가더라고요",
          "original_form": "네 저도 추천에 봤는데 완전 혼이 나가더라고요",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026233.1.11",
          "form": "오 그정도에요?",
          "original_form": "오 그정도에요?",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026233.1.12",
          "form": "주말에 몰아봐야겠어여",
          "original_form": "주말에 몰아봐야겠어여",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026233.1.13",
          "form": "ㅋㅋㅈ근데 심사위원이랑 엠씨가.... ㅋㅋ",
          "original_form": "ㅋㅋㅈ근데 심사위원이랑 엠씨가.... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026233.1.14",
          "form": "평가할 수준이 아닌데 평가하니까 민망합니다ㅋㅋ",
          "original_form": "평가할 수준이 아닌데 평가하니까 민망합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026233.1.15",
          "form": "아 보아랑",
          "original_form": "아 보아랑",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026233.1.16",
          "form": "nct 태용인가?",
          "original_form": "nct 태용인가?",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026233.1.17",
          "form": "ㅋㅋㅋ말많더라고요",
          "original_form": "ㅋㅋㅋ말많더라고요",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026233.1.18",
          "form": "누가누굴평가하느냐고요",
          "original_form": "누가누굴평가하느냐고요",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026233.1.19",
          "form": "네 제가 볼때도 감탄만하고있더라고요 제가 가서 있어도 ㅋㅋ",
          "original_form": "네 제가 볼때도 감탄만하고있더라고요 제가 가서 있어도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026233.1.20",
          "form": "춤배워보셨어여?",
          "original_form": "춤배워보셨어여?",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026233.1.21",
          "form": "진짜타고나는거아닌면 무지노력들했을거에요",
          "original_form": "진짜타고나는거아닌면 무지노력들했을거에요",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026233.1.22",
          "form": "방송댄스배워봤는데...으 보는건만큼 쉽지않아여",
          "original_form": "방송댄스배워봤는데...으 보는건만큼 쉽지않아여",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026233.1.23",
          "form": "아뇨? ㅋㅋ안배워봤으니 할 수 있는 망언ㅋㅋ",
          "original_form": "아뇨? ㅋㅋ안배워봤으니 할 수 있는 망언ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026233.1.24",
          "form": "ㅋㅋ 방송댄스 완전 멋져요! 전 몸치ㅋㅋ",
          "original_form": "ㅋㅋ 방송댄스 완전 멋져요! 전 몸치ㅋㅋ",
