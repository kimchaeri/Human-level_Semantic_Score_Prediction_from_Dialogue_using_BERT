      "utterance": [
        {
          "id": "MDRW2100017118.1.1",
          "form": "거기에 인테리어 좋으면...",
          "original_form": "거기에 인테리어 좋으면...",
          "speaker_id": "2",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017118.1.2",
          "form": "가고싶은 여행지가 있나요",
          "original_form": "가고싶은 여행지가 있나요",
          "speaker_id": "1",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017118.1.3",
          "form": "전 발리요!",
          "original_form": "전 발리요!",
          "speaker_id": "2",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017118.1.4",
          "form": "바다 좋아해요",
          "original_form": "바다 좋아해요",
          "speaker_id": "2",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017118.1.5",
          "form": "바다좋아하시는구나",
          "original_form": "바다좋아하시는구나",
          "speaker_id": "1",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017118.1.6",
          "form": "코로나 끝나면 진짜 가려구요",
          "original_form": "코로나 끝나면 진짜 가려구요",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017118.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017118.1.8",
          "form": "해외 자주가세요?",
          "original_form": "해외 자주가세요?",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017118.1.9",
          "form": "전 아직 해외 가본적이 없어서",
          "original_form": "전 아직 해외 가본적이 없어서",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017118.1.10",
          "form": "전 스페인가고싶어요",
          "original_form": "전 스페인가고싶어요",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017118.1.11",
          "form": "예전에 자주 갔었어요",
          "original_form": "예전에 자주 갔었어요",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017118.1.12",
          "form": "승무원이었거든요",
          "original_form": "승무원이었거든요",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017118.1.13",
          "form": "하지만 코로나로.... ㅠㅠ 여행 끝 직장 끝",
          "original_form": "하지만 코로나로.... ㅠㅠ 여행 끝 직장 끝",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017118.1.14",
          "form": "스페인 좋죠",
          "original_form": "스페인 좋죠",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017118.1.15",
          "form": "승무원이셨구나..앗.. ㅠㅠ",
          "original_form": "승무원이셨구나..앗.. ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017118.1.16",
          "form": "근데 스페인 소매치기 되게 많대요",
          "original_form": "근데 스페인 소매치기 되게 많대요",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017118.1.17",
          "form": "특히 바르셀로나!",
          "original_form": "특히 바르셀로나!",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017118.1.18",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017118.1.19",
          "form": "전 안가봤는데",
          "original_form": "전 안가봤는데",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017118.1.20",
          "form": "조심해야겠어요",
          "original_form": "조심해야겠어요",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017118.1.21",
          "form": "동기들이 소매치기 많이 당했대요",
          "original_form": "동기들이 소매치기 많이 당했대요",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017118.1.22",
          "form": "유럽이 좀 그런게 심하다더라구요",
          "original_form": "유럽이 좀 그런게 심하다더라구요",
          "speaker_id": "1",
          "time": "20210826 17:34"
        }
