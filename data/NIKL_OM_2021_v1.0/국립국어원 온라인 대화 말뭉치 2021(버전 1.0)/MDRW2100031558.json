          "original_form": "인데",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100031558.1.19",
          "form": "내남자가",
          "original_form": "내남자가",
          "speaker_id": "2",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100031558.1.20",
