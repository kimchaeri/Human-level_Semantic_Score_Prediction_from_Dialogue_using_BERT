          "speaker_id": "1",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100031558.1.29",
          "form": "벨빙의인가 ㄷㄷ햇엄",
          "original_form": "벨빙의인가 ㄷㄷ햇엄",
          "speaker_id": "2",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100031558.1.30",
          "form": "막 시작부터 결혼하진 않고",
          "original_form": "막 시작부터 결혼하진 않고",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100031558.1.31",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100031558.1.32",
          "form": "제목은 엔딩에서 이루어질듯",
          "original_form": "제목은 엔딩에서 이루어질듯",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100031558.1.33",
          "form": "남주는 호감뿜뿜인데",
          "original_form": "남주는 호감뿜뿜인데",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100031558.1.34",
          "form": "여주는 제 자식 보듯하는 정도라",
          "original_form": "여주는 제 자식 보듯하는 정도라",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100031558.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ구ㅏ엽다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ구ㅏ엽다",
          "speaker_id": "2",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100031558.1.36",
          "form": "근데 좋게 말하면 쿨인데",
          "original_form": "근데 좋게 말하면 쿨인데",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100031558.1.37",
          "form": "어디만화여",
          "original_form": "어디만화여",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100031558.1.38",
          "form": "나쁘게 말하면 정이 없어",
          "original_form": "나쁘게 말하면 정이 없어",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100031558.1.39",
          "form": "요즘엠 일본한국",
          "original_form": "요즘엠 일본한국",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100031558.1.40",
          "form": "카카페",
          "original_form": "카카페",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100031558.1.41",
          "form": "다거기서거기라",
          "original_form": "다거기서거기라",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100031558.1.42",
          "form": "국적을 추측할수가없다",
          "original_form": "국적을 추측할수가없다",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100031558.1.43",
          "form": "로판 이야기 구성은 한국이 나은데",
          "original_form": "로판 이야기 구성은 한국이 나은데",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100031558.1.44",
          "form": "소재가 비슷한건 동의2233",
          "original_form": "소재가 비슷한건 동의2233",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100031558.1.45",
          "form": "오리지널이야?",
          "original_form": "오리지널이야?",
          "speaker_id": "2",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100031558.1.46",
          "form": "아님 소설원작?",
          "original_form": "아님 소설원작?",
          "speaker_id": "2",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100031558.1.47",
          "form": "소설원작",
          "original_form": "소설원작",
          "speaker_id": "1",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100031558.1.48",
          "form": "카카페를 안쓰니 아쉽네",
          "original_form": "카카페를 안쓰니 아쉽네",
          "speaker_id": "2",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100031558.1.49",
          "form": "굳이 볼 그건 아닌 거 같아",
          "original_form": "굳이 볼 그건 아닌 거 같아",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100031558.1.50",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100031558.1.51",
          "form": "고양이상 남주 넘 좋아",
          "original_form": "고양이상 남주 넘 좋아",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100031558.1.52",
          "form": "영원히 흥했으면",
          "original_form": "영원히 흥했으면",
          "speaker_id": "2",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100031558.1.53",
          "form": "근데 다들 고양이 남주상 아냐?",
          "original_form": "근데 다들 고양이 남주상 아냐?",
          "speaker_id": "1",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100031558.1.54",
          "form": "구니까",
          "original_form": "구니까",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100031558.1.55",
          "form": "영원히 흥하라고",
          "original_form": "영원히 흥하라고",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100031558.1.56",
          "form": "벨에는 없어",
          "original_form": "벨에는 없어",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100031558.1.57",
          "form": "고양이상공...",
          "original_form": "고양이상공...",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100031558.1.58",
          "form": "고양이는 모두 수를 위한거로",
          "original_form": "고양이는 모두 수를 위한거로",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100031558.1.59",
          "form": "나와 ㅠ",
          "original_form": "나와 ㅠ",
          "speaker_id": "2",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100031558.1.60",
          "form": "난 수는 강아지상이 좋은데",
          "original_form": "난 수는 강아지상이 좋은데",
          "speaker_id": "1",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100031558.1.61",
          "form": "나둥 강아지 늑대 여우 토끼",
          "original_form": "나둥 강아지 늑대 여우 토끼",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100031558.1.62",
          "form": "공은 무조건 고양이...호랑이나 흑표범 ㄴㄴ 고양이",
          "original_form": "공은 무조건 고양이...호랑이나 흑표범 ㄴㄴ 고양이",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100031558.1.63",
          "form": "나는 공 폭은 넓지만...",
          "original_form": "나는 공 폭은 넓지만...",
          "speaker_id": "1",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100031558.1.64",
          "form": "능글공은 아웃",
          "original_form": "능글공은 아웃",
          "speaker_id": "2",
          "time": "20210905 13:59"
        },
        {
          "id": "MDRW2100031558.1.65",
          "form": "수나 여주도 까칠무심은 아웃",
          "original_form": "수나 여주도 까칠무심은 아웃",
          "speaker_id": "2",
          "time": "20210905 13:59"
        },
        {
          "id": "MDRW2100031558.1.66",
          "form": "나는 그것도 상관없지만",
          "original_form": "나는 그것도 상관없지만",
          "speaker_id": "1",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100031558.1.67",
          "form": "역시 키워드보다는",
          "original_form": "역시 키워드보다는",
          "speaker_id": "1",
          "time": "20210905 14:04"
        },
