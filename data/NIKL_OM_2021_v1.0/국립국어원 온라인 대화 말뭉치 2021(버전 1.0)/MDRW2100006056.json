      "utterance": [
        {
          "id": "MDRW2100006056.1.1",
          "form": "손흥민말고 또 누구있어",
          "original_form": "손흥민말고 또 누구있어",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100006056.1.2",
          "form": "국대에?",
          "original_form": "국대에?",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100006056.1.3",
          "form": "의조 알지 소명이랑 소개팅..",
          "original_form": "의조 알지 소명이랑 소개팅..",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100006056.1.4",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100006056.1.5",
          "form": "언급해버였네",
          "original_form": "언급해버였네",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100006056.1.6",
          "form": "헐 그랬나",
          "original_form": "헐 그랬나",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100006056.1.7",
          "form": "걔가 걔였군",
          "original_form": "걔가 걔였군",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100006056.1.8",
          "form": "ㅇㅇ 근데 오늘은 선발아님",
          "original_form": "ㅇㅇ 근데 오늘은 선발아님",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100006056.1.9",
          "form": "오늘 라인업 좀 갈고 나왔어",
          "original_form": "오늘 라인업 좀 갈고 나왔어",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100006056.1.10",
          "form": "엄마는 축구 좋아하는데",
          "original_form": "엄마는 축구 좋아하는데",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100006056.1.11",
          "form": "우리엄마늠 요새안봐..",
          "original_form": "우리엄마늠 요새안봐..",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100006056.1.12",
          "form": "보면 진대",
          "original_form": "보면 진대",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100006056.1.13",
          "form": "방금 알려줬다 한다고",
          "original_form": "방금 알려줬다 한다고",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100006056.1.14",
          "form": "올 ㅋㅋㅋ",
          "original_form": "올 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100006056.1.15",
          "form": "아버지는 안좋아하셔??",
          "original_form": "아버지는 안좋아하셔??",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100006056.1.16",
          "form": "아빠는 그냥 틀어져있으면 다 봐",
          "original_form": "아빠는 그냥 틀어져있으면 다 봐",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100006056.1.17",
          "form": "ㅋㅋㅋㅋㅋ웃기다",
          "original_form": "ㅋㅋㅋㅋㅋ웃기다",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100006056.1.18",
          "form": "우리아빤 좋아해 나와의 유일한 공통점..",
          "original_form": "우리아빤 좋아해 나와의 유일한 공통점..",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100006056.1.19",
          "form": "광고도 웃으면서 봄",
          "original_form": "광고도 웃으면서 봄",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100006056.1.20",
          "form": "곧 시작임..",
          "original_form": "곧 시작임..",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100006056.1.21",
          "form": "오 치킨먹어야지",
          "original_form": "오 치킨먹어야지",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100006056.1.22",
          "form": "뱌불러요...",
          "original_form": "뱌불러요...",
          "speaker_id": "2",
          "time": "20210907 19:53"
        }
      ]
    }
