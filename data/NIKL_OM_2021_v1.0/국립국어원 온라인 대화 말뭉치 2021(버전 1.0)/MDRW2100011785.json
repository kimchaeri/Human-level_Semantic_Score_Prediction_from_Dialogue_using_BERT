        {
          "id": "MDRW2100011785.1.1",
          "form": "사실상 조선시대랑 달라진게 없네요",
          "original_form": "사실상 조선시대랑 달라진게 없네요",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011785.1.2",
          "form": "어쩜 이렇게 여자가 돌봄노동을 전담하는 구조가 되었는지",
          "original_form": "어쩜 이렇게 여자가 돌봄노동을 전담하는 구조가 되었는지",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011785.1.3",
          "form": "기이해요",
          "original_form": "기이해요",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011785.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011785.1.5",
          "form": "맞아요 조선시댘ㅋㅋㅋ",
          "original_form": "맞아요 조선시댘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011785.1.6",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011785.1.7",
          "form": "오래전부터 이어진 것들에",
          "original_form": "오래전부터 이어진 것들에",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011785.1.8",
          "form": "의문을 표하고 바꾸려 하니",
          "original_form": "의문을 표하고 바꾸려 하니",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011785.1.9",
          "form": "맞아요 어려워요 ㅜㅜ",
          "original_form": "맞아요 어려워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011785.1.10",
          "form": "반응이 거센 것 같아요",
          "original_form": "반응이 거센 것 같아요",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011785.1.11",
          "form": "비혼한다하면 이기적이다 그렇고",
          "original_form": "비혼한다하면 이기적이다 그렇고",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011785.1.12",
          "form": "약간 한국이 래디컬 생성국 아닌가요",
          "original_form": "약간 한국이 래디컬 생성국 아닌가요",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011785.1.13",
          "form": "극한의 환경",
          "original_form": "극한의 환경",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011785.1.14",
          "form": "맞아욬ㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011785.1.15",
          "form": "사실 자라면서 어느정도는",
          "original_form": "사실 자라면서 어느정도는",
          "speaker_id": "2",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011785.1.16",
          "form": "그런 의문을 갖게 되는 것 같아요",
          "original_form": "그런 의문을 갖게 되는 것 같아요",
          "speaker_id": "2",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011785.1.17",
          "form": "그냥 넘기고 살면 결혼하고 그런거고",
          "original_form": "그냥 넘기고 살면 결혼하고 그런거고",
          "speaker_id": "2",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011785.1.18",
          "form": "맞아요 너무 기괴하니까 안볼수가없어요",
          "original_form": "맞아요 너무 기괴하니까 안볼수가없어요",
          "speaker_id": "1",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011785.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011785.1.20",
          "form": "그냥 살고있는데",
          "original_form": "그냥 살고있는데",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011785.1.21",
          "form": "살아남으려다 보니",
          "original_form": "살아남으려다 보니",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011785.1.22",
          "form": "이렇게 됐어욬ㅋㅋㅋㅋ",
