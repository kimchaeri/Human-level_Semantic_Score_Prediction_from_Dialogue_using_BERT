      "utterance": [
        {
          "id": "MDRW2100034966.1.1",
          "form": "날씨너무덥죠",
          "original_form": "날씨너무덥죠",
          "speaker_id": "1",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100034966.1.2",
          "form": "저녁되니 선선하네요",
          "original_form": "저녁되니 선선하네요",
          "speaker_id": "2",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100034966.1.3",
          "form": "더우신가요",
          "original_form": "더우신가요",
          "speaker_id": "2",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100034966.1.4",
          "form": "네 덥습니다 선풍기앞에서 떠나질않아요",
          "original_form": "네 덥습니다 선풍기앞에서 떠나질않아요",
          "speaker_id": "1",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034966.1.5",
          "form": "ㅋㅋㅋㅋ찬물로 샤워햇더니 시원하니요",
          "original_form": "ㅋㅋㅋㅋ찬물로 샤워햇더니 시원하니요",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034966.1.6",
          "form": "네요",
          "original_form": "네요",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034966.1.7",
          "form": "와 찬물샤워가 가능하신가요",
          "original_form": "와 찬물샤워가 가능하신가요",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100034966.1.8",
          "form": "운동하고 왔더니 더워서욬ㅋㅋㅋ",
          "original_form": "운동하고 왔더니 더워서욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100034966.1.9",
          "form": "더운날씨에도 운동을하시다니 부지런해요",
          "original_form": "더운날씨에도 운동을하시다니 부지런해요",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100034966.1.10",
          "form": "땀복입으셨나요",
          "original_form": "땀복입으셨나요",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100034966.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100034966.1.12",
          "form": "30대 되니 살빼려고 노력해야 유지정도가 되네여",
          "original_form": "30대 되니 살빼려고 노력해야 유지정도가 되네여",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100034966.1.13",
          "form": "name2님이 30대되니 이말하니 왤케어색하게느겨질까요",
          "original_form": "&name2&님이 30대되니 이말하니 왤케어색하게느겨질까요",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100034966.1.14",
          "form": "우리중엔 신세대라서 그런걸까요",
          "original_form": "우리중엔 신세대라서 그런걸까요",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100034966.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ왜죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ왜죠",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100034966.1.16",
          "form": "신세대라도 30대는 30대인것을",
          "original_form": "신세대라도 30대는 30대인것을",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100034966.1.17",
          "form": "ㅋㅋㅋㅋㅋ휴",
          "original_form": "ㅋㅋㅋㅋㅋ휴",
          "speaker_id": "1",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100034966.1.18",
          "form": "다음주엔비예정이던데 우비입으시나요",
          "original_form": "다음주엔비예정이던데 우비입으시나요",
          "speaker_id": "1",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100034966.1.19",
          "form": "제 우비 어디갔을까요",
          "original_form": "제 우비 어디갔을까요",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034966.1.20",
          "form": "야심차게 샀지만 옷장 어디론가 사라진",
          "original_form": "야심차게 샀지만 옷장 어디론가 사라진",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034966.1.21",
          "form": "오잉 반전인데요?!  우비가사라지다니",
          "original_form": "오잉 반전인데요?!  우비가사라지다니",
          "speaker_id": "1",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034966.1.22",
          "form": "ㅋㅋㅋㅋㅋ찾으면 분명 나오긴 할텐데",
          "original_form": "ㅋㅋㅋㅋㅋ찾으면 분명 나오긴 할텐데",
