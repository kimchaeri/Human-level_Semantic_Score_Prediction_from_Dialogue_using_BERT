      "utterance": [
        {
          "id": "MDRW2100042042.1.1",
          "form": "배고파...",
          "original_form": "배고파...",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042042.1.2",
          "form": "언니는 뭐먹어...",
          "original_form": "언니는 뭐먹어...",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042042.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100042042.1.4",
          "form": "난 컵누들 먹엇더",
          "original_form": "난 컵누들 먹엇더",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100042042.1.5",
          "form": "저녁 뭐먹을거야?",
          "original_form": "저녁 뭐먹을거야?",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100042042.1.6",
          "form": "컵누들 맛있겠다..",
          "original_form": "컵누들 맛있겠다..",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042042.1.7",
          "form": "나는 어제 엄마가 외갓집 갔다와서",
          "original_form": "나는 어제 엄마가 외갓집 갔다와서",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042042.1.8",
          "form": "생선탕? 지리탕?",
          "original_form": "생선탕? 지리탕?",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042042.1.9",
          "form": "맑은탕?",
          "original_form": "맑은탕?",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042042.1.10",
          "form": "먹으려고",
          "original_form": "먹으려고",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042042.1.11",
          "form": "오 맛있겠다",
          "original_form": "오 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100042042.1.12",
          "form": "오늘 날씨랑 잘 어울리는 음식이네",
          "original_form": "오늘 날씨랑 잘 어울리는 음식이네",
          "speaker_id": "1",
          "time": "20210906 21:01"
        },
        {
          "id": "MDRW2100042042.1.13",
          "form": "ㅇㅇ 국물 딱 좋았어",
          "original_form": "ㅇㅇ 국물 딱 좋았어",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042042.1.14",
          "form": "약간 국수같은 것도 먹고 싶긴 했는데 밥 열심히 먹었당",
          "original_form": "약간 국수같은 것도 먹고 싶긴 했는데 밥 열심히 먹었당",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042042.1.15",
          "form": "후식? 아닌 후식으로 치즈 먹었는데 너무 짜서 아예 쓰더라고.. 빵이랑 같이 먹어야 하나싶고",
          "original_form": "후식? 아닌 후식으로 치즈 먹었는데 너무 짜서 아예 쓰더라고.. 빵이랑 같이 먹어야 하나싶고",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042042.1.16",
          "form": "오 국수",
          "original_form": "오 국수",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042042.1.17",
          "form": "그치만 맑은 탕에는 밥이지",
          "original_form": "그치만 맑은 탕에는 밥이지",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042042.1.18",
          "form": "치즈?",
          "original_form": "치즈?",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042042.1.19",
          "form": "엄마가 일하는 곳에서 할머니 아들이 사다줬다고 하는데",
          "original_form": "엄마가 일하는 곳에서 할머니 아들이 사다줬다고 하는데",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042042.1.20",
          "form": "겁나 짜서 못 먹겠다고 해서 가져왔대",
          "original_form": "겁나 짜서 못 먹겠다고 해서 가져왔대",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042042.1.21",
          "form": "근데 진짜 치즈가 너무 짜... 어케 치즈가 짜겠어 싶었는데 소금에 절인것처럼 짜..짜다못해 써...",
          "original_form": "근데 진짜 치즈가 너무 짜... 어케 치즈가 짜겠어 싶었는데 소금에 절인것처럼 짜..짜다못해 써...",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042042.1.22",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042042.1.23",
          "form": "어떻게 생겼는데?",
          "original_form": "어떻게 생겼는데?",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042042.1.24",
          "form": "보통 그러면",
          "original_form": "보통 그러면",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042042.1.25",
          "form": "아이비같은 크래커 위에 올려서",
          "original_form": "아이비같은 크래커 위에 올려서",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042042.1.26",
          "form": "위에 꿀 뿌려먹으면 좀 나아",
          "original_form": "위에 꿀 뿌려먹으면 좀 나아",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042042.1.27",
          "form": "그냥 네모난 치즈 우리가 흔히 보는?",
          "original_form": "그냥 네모난 치즈 우리가 흔히 보는?",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042042.1.28",
          "form": "? 빵같은거랑 같이 먹고",
          "original_form": "? 빵같은거랑 같이 먹고",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042042.1.29",
          "form": "쨈도 먹고",
          "original_form": "쨈도 먹고",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042042.1.30",
          "form": "엥 근데 그렇게 짜?",
          "original_form": "엥 근데 그렇게 짜?",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042042.1.31",
          "form": "어케 설명이 안되는데",
          "original_form": "어케 설명이 안되는데",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042042.1.32",
          "form": "넘 짜서 나중에 쓰고 떫은 것 같은 맛이 나더라고",
          "original_form": "넘 짜서 나중에 쓰고 떫은 것 같은 맛이 나더라고",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042042.1.33",
          "form": "혀가 아렸어",
          "original_form": "혀가 아렸어",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042042.1.34",
          "form": "헐;;;",
          "original_form": "헐;;;",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042042.1.35",
          "form": "그정도면 먹으면 안될거같은데",
          "original_form": "그정도면 먹으면 안될거같은데",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042042.1.36",
          "form": "진짜.. 치즈가 짜면 얼마나 짜겠어 했다가...",
          "original_form": "진짜.. 치즈가 짜면 얼마나 짜겠어 했다가...",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042042.1.37",
          "form": "세상 이런 치즈가 잇단 말이야? 하고 놀랐음...",
          "original_form": "세상 이런 치즈가 잇단 말이야? 하고 놀랐음...",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042042.1.38",
          "form": "식빵 크게 먹고 치즈 조금 먹는다구 됐니",
          "original_form": "식빵 크게 먹고 치즈 조금 먹는다구 됐니",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042042.1.39",
          "form": "코스트코껀가?",
          "original_form": "코스트코껀가?",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042042.1.40",
          "form": "겉에 상표 잇어?",
          "original_form": "겉에 상표 잇어?",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042042.1.41",
          "form": "아니 없다고 하시던데 그냥 비닐? 그런것만 있다고",
          "original_form": "아니 없다고 하시던데 그냥 비닐? 그런것만 있다고",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042042.1.42",
          "form": "나도 궁금해서 물어봣던",
          "original_form": "나도 궁금해서 물어봣던",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042042.1.43",
          "form": "하얀색이야?",
          "original_form": "하얀색이야?",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042042.1.44",
          "form": "이건 도전할만한게 아냐... 아니 그 노란색",
          "original_form": "이건 도전할만한게 아냐... 아니 그 노란색",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042042.1.45",
          "form": "체다치즈처럼 노란색이었어",
          "original_form": "체다치즈처럼 노란색이었어",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042042.1.46",
          "form": "ㅇㅎ...",
          "original_form": "ㅇㅎ...",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042042.1.47",
          "form": "대체 뭔 치즈지 ㅋㅋㅋㅋ",
          "original_form": "대체 뭔 치즈지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042042.1.48",
          "form": "아니면 무염 모짜렐라를 사서",
          "original_form": "아니면 무염 모짜렐라를 사서",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042042.1.49",
          "form": "반반 섞어버려...",
          "original_form": "반반 섞어버려...",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042042.1.50",
          "form": "ㅋㅋㅋㅋㅋㅋ 그런 방법도 있겠넼ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그런 방법도 있겠넼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042042.1.51",
          "form": "진짜 암맛 안나는 거랑 반죽해서 섞고 빵을 만들어버릴까...",
          "original_form": "진짜 암맛 안나는 거랑 반죽해서 섞고 빵을 만들어버릴까...",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042042.1.52",
          "form": "빵에 소금 안쳐도 될듯 ㅋㅋㅋㅋㅋ",
          "original_form": "빵에 소금 안쳐도 될듯 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042042.1.53",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042042.1.54",
          "form": "아니면 계란말이할때",
          "original_form": "아니면 계란말이할때",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042042.1.55",
          "form": "계란에 소금넣지말고 그거 넣어 ㅋㅋㅋ",
          "original_form": "계란에 소금넣지말고 그거 넣어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042042.1.56",
          "form": "뭐 어때",
          "original_form": "뭐 어때",
          "speaker_id": "1",
