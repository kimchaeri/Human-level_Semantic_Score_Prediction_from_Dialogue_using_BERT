      "utterance": [
        {
          "id": "MDRW2100020782.1.1",
          "form": "칵테일 좋아하세요?",
          "original_form": "칵테일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020782.1.2",
          "form": "잘 모르는데 마시는걸 다 좋아합니다 !",
          "original_form": "잘 모르는데 마시는걸 다 좋아합니다 !",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020782.1.3",
          "form": "최근에 한 가게를 가서 진토닉을 마셨는데 역대급 맛있었어요",
          "original_form": "최근에 한 가게를 가서 진토닉을 마셨는데 역대급 맛있었어요",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020782.1.4",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020782.1.5",
          "form": "저는 칵테일 첫잔을 꼭 진토닉으로 먹어요",
          "original_form": "저는 칵테일 첫잔을 꼭 진토닉으로 먹어요",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020782.1.6",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020782.1.7",
          "form": "맛잘알!",
          "original_form": "맛잘알!",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020782.1.8",
          "form": "ㅋㅋㅋ 유튜브 무슨 술 채널을 봤는데 거기서 진토닉 잘하는 집이 칵테일 맛집이라 그래서요",
          "original_form": "ㅋㅋㅋ 유튜브 무슨 술 채널을 봤는데 거기서 진토닉 잘하는 집이 칵테일 맛집이라 그래서요",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020782.1.9",
          "form": "세상에 !! 칵테일 좋아하세요?!",
          "original_form": "세상에 !! 칵테일 좋아하세요?!",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020782.1.10",
          "form": "그 후로 진토닉으로 시작하게됐어요ㅋㅋ",
          "original_form": "그 후로 진토닉으로 시작하게됐어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020782.1.11",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020782.1.12",
          "form": "어떤거 좋아하세요?!",
          "original_form": "어떤거 좋아하세요?!",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020782.1.13",
          "form": "종류 불문 좋아해요",
          "original_form": "종류 불문 좋아해요",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020782.1.14",
          "form": "추천해주시면 저 나중에 먹어볼게요 !",
          "original_form": "추천해주시면 저 나중에 먹어볼게요 !",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020782.1.15",
          "form": "저렴하게는 소닉워터요!",
          "original_form": "저렴하게는 소닉워터요!",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020782.1.16",
          "form": "토닉워터에 소주!!",
          "original_form": "토닉워터에 소주!!",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020782.1.17",
          "form": "너무맛있죠",
          "original_form": "너무맛있죠",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020782.1.18",
          "form": "크!",
          "original_form": "크!",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020782.1.19",
          "form": "쏘토닉 최고죠",
          "original_form": "쏘토닉 최고죠",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020782.1.20",
          "form": "흐미 ,,",
          "original_form": "흐미 ,,",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020782.1.21",
          "form": "상큼상큼하죠ㅠㅠ",
          "original_form": "상큼상큼하죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020782.1.22",
          "form": "칵테일 나가서 먹으면 맛은 있는데 양껏먹으려면",
          "original_form": "칵테일 나가서 먹으면 맛은 있는데 양껏먹으려면",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020782.1.23",
          "form": "너무 비싸서",
