      "utterance": [
        {
          "id": "MDRW2100034747.1.1",
          "form": "미용실 자주 다니시나요?",
          "original_form": "미용실 자주 다니시나요?",
          "speaker_id": "1",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100034747.1.2",
          "form": "네 며칠전에도 볼륨펌했어요ㅋ",
          "original_form": "네 며칠전에도 볼륨펌했어요ㅋ",
          "speaker_id": "2",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100034747.1.3",
          "form": "미용실 자주가세요?",
          "original_form": "미용실 자주가세요?",
          "speaker_id": "2",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100034747.1.4",
          "form": "이쁘게 잘나왔나요?\n전 미용실 아주 어쩌다 한 번 가네요.ㅋ",
          "original_form": "이쁘게 잘나왔나요?\n전 미용실 아주 어쩌다 한 번 가네요.ㅋ",
          "speaker_id": "1",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100034747.1.5",
          "form": "네 이번에는 만족해요",
          "original_form": "네 이번에는 만족해요",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100034747.1.6",
          "form": "지난번에는 파마했다가",
          "original_form": "지난번에는 파마했다가",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100034747.1.7",
          "form": "머리 다 타서ㅜㅜ",
          "original_form": "머리 다 타서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100034747.1.8",
          "form": "이번에 거의 잘랐거든요",
          "original_form": "이번에 거의 잘랐거든요",
          "speaker_id": "2",
          "time": "20210829 10:37"
        },
        {
          "id": "MDRW2100034747.1.9",
          "form": "엥; 미용실도 잘 골라야 겠어요..",
          "original_form": "엥; 미용실도 잘 골라야 겠어요..",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100034747.1.10",
          "form": "네ㅠㅠ정말ㅋㅋㅋ",
          "original_form": "네ㅠㅠ정말ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100034747.1.11",
          "form": "저렴해서갔다가 화만났네요ㅋ",
          "original_form": "저렴해서갔다가 화만났네요ㅋ",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100034747.1.12",
          "form": "미용실은 지인들의 후기를 믿는 게 가장 좋겠더라구요..",
          "original_form": "미용실은 지인들의 후기를 믿는 게 가장 좋겠더라구요..",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100034747.1.13",
          "form": "네 그게맞는것같아요",
          "original_form": "네 그게맞는것같아요",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100034747.1.14",
          "form": "저도 오픈해서 50퍼하는 미용실이 있길래 가볼까 고민되네요.ㅋ",
          "original_form": "저도 오픈해서 50퍼하는 미용실이 있길래 가볼까 고민되네요.ㅋ",
          "speaker_id": "1",
          "time": "20210829 14:51"
        },
        {
          "id": "MDRW2100034747.1.15",
          "form": "거기는 제가했는곳보다 낫겠죠ㅋ",
          "original_form": "거기는 제가했는곳보다 낫겠죠ㅋ",
          "speaker_id": "2",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100034747.1.16",
          "form": "너무 걱정마셔요ㅋ",
          "original_form": "너무 걱정마셔요ㅋ",
          "speaker_id": "2",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100034747.1.17",
          "form": "ㅎㅎ\n그럼 정보를 더 수집해서 이쁜 스타일을 만들어봐야겠어요.",
          "original_form": "ㅎㅎ\n그럼 정보를 더 수집해서 이쁜 스타일을 만들어봐야겠어요.",
          "speaker_id": "1",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100034747.1.18",
          "form": "맞아요. 후기 꼭 보고 가셔요ㅋ",
          "original_form": "맞아요. 후기 꼭 보고 가셔요ㅋ",
          "speaker_id": "2",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100034747.1.19",
          "form": "자주 하는 헤어스타일은 어떤거에요?",
          "original_form": "자주 하는 헤어스타일은 어떤거에요?",
          "speaker_id": "1",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100034747.1.20",
          "form": "볼륨펌이요 매직ㅋㅋ",
          "original_form": "볼륨펌이요 매직ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:34"
        }
      ]
