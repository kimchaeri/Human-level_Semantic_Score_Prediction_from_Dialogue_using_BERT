      "utterance": [
        {
          "id": "MDRW2100030640.1.1",
          "form": "ㅋㅋ 와디즈 같은거요?",
          "original_form": "ㅋㅋ 와디즈 같은거요?",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030640.1.2",
          "form": "네 와디즈",
          "original_form": "네 와디즈",
          "speaker_id": "1",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100030640.1.3",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100030640.1.4",
          "form": "거기 신기한거 많이 팔더라구요 눈팅만 자주해요",
          "original_form": "거기 신기한거 많이 팔더라구요 눈팅만 자주해요",
          "speaker_id": "2",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030640.1.5",
          "form": "전 와디즈에서 펀딩 자주해요 ㅋㅋㅋ",
          "original_form": "전 와디즈에서 펀딩 자주해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100030640.1.6",
          "form": "까먹고있다가 택배오면",
          "original_form": "까먹고있다가 택배오면",
          "speaker_id": "1",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100030640.1.7",
          "form": "선물온 느낌이랄까",
          "original_form": "선물온 느낌이랄까",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100030640.1.8",
          "form": "은근 비싸더라구요",
          "original_form": "은근 비싸더라구요",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100030640.1.9",
          "form": "괜찮은게 좀 있어요",
          "original_form": "괜찮은게 좀 있어요",
          "speaker_id": "1",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100030640.1.10",
          "form": "득템하신거있으세요?",
          "original_form": "득템하신거있으세요?",
          "speaker_id": "2",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100030640.1.11",
          "form": "트래킹화?",
          "original_form": "트래킹화?",
          "speaker_id": "1",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100030640.1.12",
          "form": "블랙야크껀데 공동구매같이 싸게 팔아서",
          "original_form": "블랙야크껀데 공동구매같이 싸게 팔아서",
          "speaker_id": "1",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100030640.1.13",
          "form": "시중에 나오기전에 먼저 와디즈에서 펀딩하더라구요",
          "original_form": "시중에 나오기전에 먼저 와디즈에서 펀딩하더라구요",
          "speaker_id": "1",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100030640.1.14",
          "form": "오 ~브랜드도 와디즈에 나오나봐요 ? 첨알았어요",
          "original_form": "오 ~브랜드도 와디즈에 나오나봐요 ? 첨알았어요",
          "speaker_id": "2",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100030640.1.15",
          "form": "스타트업 중소기업제품만 있는줄",
          "original_form": "스타트업 중소기업제품만 있는줄",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100030640.1.16",
          "form": "네 나와요 ㅎㅎ",
          "original_form": "네 나와요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100030640.1.17",
          "form": "오~ 꼼꼼히 봐야겄어요",
          "original_form": "오~ 꼼꼼히 봐야겄어요",
          "speaker_id": "2",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100030640.1.18",
          "form": "네 찾아보면 할만한게 있어요",
          "original_form": "네 찾아보면 할만한게 있어요",
          "speaker_id": "1",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100030640.1.19",
          "form": "그래야겠어요. 어플부터 깔아야겠어요",
          "original_form": "그래야겠어요. 어플부터 깔아야겠어요",
          "speaker_id": "2",
          "time": "20210905 08:58"
        },
        {
          "id": "MDRW2100030640.1.20",
          "form": "지난번에 아이즈원 재결합 펀딩도 했다면서요? 와디즈에서",
          "original_form": "지난번에 아이즈원 재결합 펀딩도 했다면서요? 와디즈에서",
          "speaker_id": "2",
          "time": "20210905 08:59"
        },
        {
          "id": "MDRW2100030640.1.21",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "1",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100030640.1.22",
          "form": "그건 몰랐어요ㅋㅋㅋ",
          "original_form": "그건 몰랐어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100030640.1.23",
          "form": "네 근데 실제로 돈내는거 아니니까 막 금액올려서 펀딩하고했다고 하더라구요",
          "original_form": "네 근데 실제로 돈내는거 아니니까 막 금액올려서 펀딩하고했다고 하더라구요",
          "speaker_id": "2",
          "time": "20210905 09:47"
        }
      ]
