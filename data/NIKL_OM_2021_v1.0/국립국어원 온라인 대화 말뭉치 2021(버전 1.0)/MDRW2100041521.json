      "utterance": [
        {
          "id": "MDRW2100041521.1.1",
          "form": "먹는게 고생이겠네요",
          "original_form": "먹는게 고생이겠네요",
          "speaker_id": "2",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100041521.1.2",
          "form": "근데 암 한번 걸린 사람은 또 걸리기 쉽다고 하더라구요",
          "original_form": "근데 암 한번 걸린 사람은 또 걸리기 쉽다고 하더라구요",
          "speaker_id": "1",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100041521.1.3",
          "form": "저는 국가검진 2년마다 하는 거에서 발견했어요",
          "original_form": "저는 국가검진 2년마다 하는 거에서 발견했어요",
          "speaker_id": "1",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100041521.1.4",
          "form": "그럴지도요.",
          "original_form": "그럴지도요.",
          "speaker_id": "2",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100041521.1.5",
          "form": "다행이 1기일때 발견했죠",
          "original_form": "다행이 1기일때 발견했죠",
          "speaker_id": "1",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100041521.1.6",
          "form": "빨리 발견하셨네요",
          "original_form": "빨리 발견하셨네요",
          "speaker_id": "2",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100041521.1.7",
          "form": "그래도 위를 3분의 2를 잘라냈어요",
          "original_form": "그래도 위를 3분의 2를 잘라냈어요",
          "speaker_id": "1",
          "time": "20210906 08:47"
        },
        {
          "id": "MDRW2100041521.1.8",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "2",
          "time": "20210906 08:47"
        },
        {
          "id": "MDRW2100041521.1.9",
          "form": "아이고...",
          "original_form": "아이고...",
          "speaker_id": "2",
          "time": "20210906 08:47"
        },
        {
          "id": "MDRW2100041521.1.10",
          "form": "처음엔 잘 못먹다가",
          "original_form": "처음엔 잘 못먹다가",
          "speaker_id": "1",
          "time": "20210906 08:47"
        },
        {
          "id": "MDRW2100041521.1.11",
          "form": "지금은 수술전만큼 양이 돌아왔어요",
          "original_form": "지금은 수술전만큼 양이 돌아왔어요",
          "speaker_id": "1",
          "time": "20210906 08:47"
        },
        {
          "id": "MDRW2100041521.1.12",
          "form": "그게 위험한 암이라 그때 발견 못했으면 큰일날 뻔 했다 하더라구요",
          "original_form": "그게 위험한 암이라 그때 발견 못했으면 큰일날 뻔 했다 하더라구요",
          "speaker_id": "1",
          "time": "20210906 08:48"
        },
        {
          "id": "MDRW2100041521.1.13",
          "form": "1기여도 얼른 수술했어야 했다고",
          "original_form": "1기여도 얼른 수술했어야 했다고",
          "speaker_id": "1",
          "time": "20210906 08:48"
        },
        {
          "id": "MDRW2100041521.1.14",
          "form": "저희어머니는 3기....",
          "original_form": "저희어머니는 3기....",
          "speaker_id": "2",
          "time": "20210906 08:48"
        },
        {
          "id": "MDRW2100041521.1.15",
          "form": "위 다 잘라냈죠, 뭐...",
          "original_form": "위 다 잘라냈죠, 뭐...",
          "speaker_id": "2",
          "time": "20210906 08:48"
        },
        {
          "id": "MDRW2100041521.1.16",
          "form": "19년 수술인데 아직 죽 반그릇정도만 드세요",
          "original_form": "19년 수술인데 아직 죽 반그릇정도만 드세요",
          "speaker_id": "2",
          "time": "20210906 08:49"
        },
        {
          "id": "MDRW2100041521.1.17",
          "form": "다 제거한거면 저보다 더 오래 걸리시겠네요",
          "original_form": "다 제거한거면 저보다 더 오래 걸리시겠네요",
          "speaker_id": "1",
          "time": "20210906 08:50"
        },
        {
          "id": "MDRW2100041521.1.18",
          "form": "저도 처음엔 진짜 조금씩 먹었거든요",
          "original_form": "저도 처음엔 진짜 조금씩 먹었거든요",
          "speaker_id": "1",
          "time": "20210906 08:50"
        },
        {
          "id": "MDRW2100041521.1.19",
          "form": "조금씩 자주",
          "original_form": "조금씩 자주",
          "speaker_id": "1",
          "time": "20210906 08:50"
        },
        {
          "id": "MDRW2100041521.1.20",
          "form": "입맛은 어떠신가요?",
          "original_form": "입맛은 어떠신가요?",
          "speaker_id": "2",
          "time": "20210906 08:53"
        },
        {
          "id": "MDRW2100041521.1.21",
          "form": "저희 어머니는 입맛이 극도로 짧아졌달까...",
          "original_form": "저희 어머니는 입맛이 극도로 짧아졌달까...",
          "speaker_id": "2",
          "time": "20210906 08:54"
        },
        {
          "id": "MDRW2100041521.1.22",
          "form": "그나마도 잘 못드세요...",
          "original_form": "그나마도 잘 못드세요...",
          "speaker_id": "2",
          "time": "20210906 08:54"
        },
        {
          "id": "MDRW2100041521.1.23",
          "form": "아 저도 엄청 까다로왔죠",
          "original_form": "아 저도 엄청 까다로왔죠",
          "speaker_id": "1",
          "time": "20210906 08:58"
        },
        {
          "id": "MDRW2100041521.1.24",
          "form": "원래 진짜 아무거나 잘 먹거든요",
          "original_form": "원래 진짜 아무거나 잘 먹거든요",
          "speaker_id": "1",
          "time": "20210906 08:58"
        },
        {
          "id": "MDRW2100041521.1.25",
          "form": "지금도 아무거나 잘 먹는데",
          "original_form": "지금도 아무거나 잘 먹는데",
          "speaker_id": "1",
          "time": "20210906 08:58"
        },
        {
          "id": "MDRW2100041521.1.26",
          "form": "수술 직후  항암 치료기간에는",
          "original_form": "수술 직후  항암 치료기간에는",
          "speaker_id": "1",
          "time": "20210906 08:59"
        },
        {
          "id": "MDRW2100041521.1.27",
          "form": "정말 입맛 까다로워지더라구요",
          "original_form": "정말 입맛 까다로워지더라구요",
          "speaker_id": "1",
          "time": "20210906 08:59"
        },
        {
          "id": "MDRW2100041521.1.28",
          "form": "우리 딸이 고생했죠",
          "original_form": "우리 딸이 고생했죠",
          "speaker_id": "1",
          "time": "20210906 08:59"
        },
        {
          "id": "MDRW2100041521.1.29",
          "form": "저희어머니도 아프니까 억지로라도 먹어야지 하면서",
          "original_form": "저희어머니도 아프니까 억지로라도 먹어야지 하면서",
          "speaker_id": "2",
          "time": "20210906 09:01"
        },
        {
          "id": "MDRW2100041521.1.30",
          "form": "정말 열나고 아파도 끼니 꼭꼭 챙기던 분이었는데",
          "original_form": "정말 열나고 아파도 끼니 꼭꼭 챙기던 분이었는데",
          "speaker_id": "2",
          "time": "20210906 09:02"
        },
        {
          "id": "MDRW2100041521.1.31",
          "form": "유방암때도 진짜 밥 다드셨는데",
          "original_form": "유방암때도 진짜 밥 다드셨는데",
          "speaker_id": "2",
          "time": "20210906 09:02"
        },
        {
          "id": "MDRW2100041521.1.32",
          "form": "이제 1년 좀 지나신 거니까 아직은 힘드실 거에요",
          "original_form": "이제 1년 좀 지나신 거니까 아직은 힘드실 거에요",
          "speaker_id": "1",
          "time": "20210906 09:02"
        },
        {
          "id": "MDRW2100041521.1.33",
          "form": "위암은 정말 음식이 먹기 힘들더라구요",
          "original_form": "위암은 정말 음식이 먹기 힘들더라구요",
          "speaker_id": "1",
          "time": "20210906 09:02"
        },
        {
          "id": "MDRW2100041521.1.34",
          "form": "그럴까요... 지금도 뭘 드시면서 게워내는 소리를 내세요",
          "original_form": "그럴까요... 지금도 뭘 드시면서 게워내는 소리를 내세요",
          "speaker_id": "2",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100041521.1.35",
          "form": "진짜 게워내는건 아닌데",
          "original_form": "진짜 게워내는건 아닌데",
          "speaker_id": "2",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100041521.1.36",
          "form": "잘 안넘어가는건지....",
          "original_form": "잘 안넘어가는건지....",
          "speaker_id": "2",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100041521.1.37",
          "form": "저도 잘 안넘거갔어요",
          "original_form": "저도 잘 안넘거갔어요",
          "speaker_id": "1",
          "time": "20210906 09:22"
        },
        {
          "id": "MDRW2100041521.1.38",
          "form": "한숟갈 먹고 한숨 쉬고 이렇게 먹었어요",
          "original_form": "한숟갈 먹고 한숨 쉬고 이렇게 먹었어요",
          "speaker_id": "1",
          "time": "20210906 09:22"
        },
        {
          "id": "MDRW2100041521.1.39",
          "form": "꼭 너무 부데껴서 나는 한숨 있잖아요",
          "original_form": "꼭 너무 부데껴서 나는 한숨 있잖아요",
          "speaker_id": "1",
          "time": "20210906 09:22"
        },
        {
          "id": "MDRW2100041521.1.40",
          "form": "아, 한숨도 많이 쉬세요.",
          "original_form": "아, 한숨도 많이 쉬세요.",
          "speaker_id": "2",
          "time": "20210906 09:46"
        },
        {
          "id": "MDRW2100041521.1.41",
          "form": "한숨에서 좀 더 발전해서 그웩 거리시는건가봐요...",
          "original_form": "한숨에서 좀 더 발전해서 그웩 거리시는건가봐요...",
          "speaker_id": "2",
          "time": "20210906 09:47"
        }
      ]
    }
  ]
