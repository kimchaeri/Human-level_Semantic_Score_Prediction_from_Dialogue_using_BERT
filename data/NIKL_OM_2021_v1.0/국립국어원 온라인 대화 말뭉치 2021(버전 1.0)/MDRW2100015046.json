      "utterance": [
        {
          "id": "MDRW2100015046.1.1",
          "form": "옥주현님 영접 이야기 더듣고싶어요...",
          "original_form": "옥주현님 영접 이야기 더듣고싶어요...",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015046.1.2",
          "form": "또 다른 극도 보신적 있나요?",
          "original_form": "또 다른 극도 보신적 있나요?",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015046.1.3",
          "form": "앜ㅋㅋㅋ옥주현님 세번봣어요",
          "original_form": "앜ㅋㅋㅋ옥주현님 세번봣어요",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015046.1.4",
          "form": "레베카 스위니토드 최근에 위키드!",
          "original_form": "레베카 스위니토드 최근에 위키드!",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015046.1.5",
          "form": "뮤지컬 덕후 시군요.... 짬이 느껴져요",
          "original_form": "뮤지컬 덕후 시군요.... 짬이 느껴져요",
          "speaker_id": "1",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015046.1.6",
          "form": "아니에요....똥손이라 티케팅도 어렵고",
          "original_form": "아니에요....똥손이라 티케팅도 어렵고",
          "speaker_id": "2",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015046.1.7",
          "form": "금액도너무비싸서 잘 못봅니다ㅠ",
          "original_form": "금액도너무비싸서 잘 못봅니다ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015046.1.8",
          "form": "저는 최근에 시카고 본게 다인걸요",
          "original_form": "저는 최근에 시카고 본게 다인걸요",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015046.1.9",
          "form": "뮤지컬덕후하려면 재정이 제일 먼저인거 같아요",
          "original_form": "뮤지컬덕후하려면 재정이 제일 먼저인거 같아요",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015046.1.10",
          "form": "너무 비싸...ㅠ",
          "original_form": "너무 비싸...ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015046.1.11",
          "form": "오 시카고 누구배우로 보셨어요??",
          "original_form": "오 시카고 누구배우로 보셨어요??",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015046.1.12",
          "form": "이번에 최재림님 엄청 유명해지셨잖아요",
          "original_form": "이번에 최재림님 엄청 유명해지셨잖아요",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015046.1.13",
          "form": "티파니씨요",
          "original_form": "티파니씨요",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015046.1.14",
          "form": "저도보고싶엇는데",
          "original_form": "저도보고싶엇는데",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015046.1.15",
          "form": "제가 소녀시대 팬이였어가지구...",
          "original_form": "제가 소녀시대 팬이였어가지구...",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015046.1.16",
          "form": "어머 대박",
          "original_form": "어머 대박",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015046.1.17",
          "form": "왜 과거형이죠?",
          "original_form": "왜 과거형이죠?",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015046.1.18",
          "form": "요즘은 팬이라고하기엔 조금 서먹해서요",
          "original_form": "요즘은 팬이라고하기엔 조금 서먹해서요",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015046.1.19",
          "form": "ㅋㅋㄱㅋㅋㄱㄲ",
          "original_form": "ㅋㅋㄱㅋㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015046.1.20",
          "form": "여전히 좋지만 어색한느낌...",
          "original_form": "여전히 좋지만 어색한느낌...",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015046.1.21",
          "form": "ㅋㅋㅋㅋㅋ곧 완전체 컴백한다는 소리가 있던데",
          "original_form": "ㅋㅋㅋㅋㅋ곧 완전체 컴백한다는 소리가 있던데",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015046.1.22",
          "form": "유큐ㅣ즈도 나온다고 하고",
          "original_form": "유큐ㅣ즈도 나온다고 하고",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015046.1.23",
          "form": "활동이 많아지면 다시 가까워질까요? ㅋㅋ",
          "original_form": "활동이 많아지면 다시 가까워질까요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015046.1.24",
          "form": "많이 나오시면 여전히 좋지만",
          "original_form": "많이 나오시면 여전히 좋지만",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015046.1.25",
          "form": "예전처럼 팬질할시간이 있거나 하질 않아서요",
          "original_form": "예전처럼 팬질할시간이 있거나 하질 않아서요",
          "speaker_id": "1",
          "time": "20210825 21:51"
        }
      ]
    }
