      "utterance": [
        {
          "id": "MDRW2100042582.1.1",
          "form": "오늘 name2님 영업하려고 심심이 했어요",
          "original_form": "오늘 &name2&님 영업하려고 심심이 했어요",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042582.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042582.1.3",
          "form": "ㅋㅋㅋㅋ오늘 영업력이 쩌시네욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ오늘 영업력이 쩌시네욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042582.1.4",
          "form": "오늘 심심이는 성공적이었군여",
          "original_form": "오늘 심심이는 성공적이었군여",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042582.1.5",
          "form": "제 입맛을 자극하셨어요 ㅋㅋㅋㅋㅋ",
          "original_form": "제 입맛을 자극하셨어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042582.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042582.1.7",
          "form": "아니 새로운 정보 서로 알아가면 좋으니까요",
          "original_form": "아니 새로운 정보 서로 알아가면 좋으니까요",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042582.1.8",
          "form": "저 간식 마음속에 저장했답니다",
          "original_form": "저 간식 마음속에 저장했답니다",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042582.1.9",
          "form": "ㅎㅎ 저도요",
          "original_form": "ㅎㅎ 저도요",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042582.1.10",
          "form": "이렇게 유용한 심심이라니 ㅎㅎ",
          "original_form": "이렇게 유용한 심심이라니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042582.1.11",
          "form": "ㅋㅋㅋㅋ 복숭아빵 약과",
          "original_form": "ㅋㅋㅋㅋ 복숭아빵 약과",
          "speaker_id": "1",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042582.1.12",
          "form": "저장했어요 아 녹차꽃빵도 ㅋㅋㅋ",
          "original_form": "저장했어요 아 녹차꽃빵도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042582.1.13",
          "form": "ㅋㅋㅋ저도 수박이랑",
          "original_form": "ㅋㅋㅋ저도 수박이랑",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042582.1.14",
          "form": "앗 체리는 탈락이라 수박 저장했어욬ㅋㅋㅋㅋ",
          "original_form": "앗 체리는 탈락이라 수박 저장했어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042582.1.15",
          "form": "아 트러플오일도!",
          "original_form": "아 트러플오일도!",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042582.1.16",
          "form": "ㅋㅋㅋㅋ 나중에 기회되시면 드세여",
          "original_form": "ㅋㅋㅋㅋ 나중에 기회되시면 드세여",
          "speaker_id": "1",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042582.1.17",
          "form": "수박은 정말 대추천",
          "original_form": "수박은 정말 대추천",
          "speaker_id": "1",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042582.1.18",
          "form": "정말 먹어볼겁니닼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "정말 먹어볼겁니닼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042582.1.19",
          "form": "트러플오일은 그정도 가격이면 맛있어야될거 같아요ㅋㅋㅋㅋ",
          "original_form": "트러플오일은 그정도 가격이면 맛있어야될거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042582.1.20",
          "form": "으아",
          "original_form": "으아",
          "speaker_id": "2",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100042582.1.21",
