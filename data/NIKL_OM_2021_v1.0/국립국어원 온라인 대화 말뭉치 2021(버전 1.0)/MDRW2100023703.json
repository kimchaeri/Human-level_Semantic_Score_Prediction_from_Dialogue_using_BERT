        },
        {
          "id": "MDRW2100023703.1.2",
          "form": "아 그치 양이 많으니까",
          "original_form": "아 그치 양이 많으니까",
          "speaker_id": "1",
          "time": "20210830 15:02"
        },
        {
          "id": "MDRW2100023703.1.3",
          "form": "영상만 유툽있어서 그거 한창 보고ㅋㅋ",
          "original_form": "영상만 유툽있어서 그거 한창 보고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:02"
        },
        {
          "id": "MDRW2100023703.1.4",
          "form": "유튜브에 영상 올렸었나?",
          "original_form": "유튜브에 영상 올렸었나?",
          "speaker_id": "1",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023703.1.5",
          "form": "응 name3가 올렸었어!",
          "original_form": "응 &name3&가 올렸었어!",
          "speaker_id": "2",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023703.1.6",
          "form": "아하!\r\n그럼 너 혼자서 어디 갔어?",
          "original_form": "아하!\r\n그럼 너 혼자서 어디 갔어?",
          "speaker_id": "1",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023703.1.7",
          "form": "나 그냥 그주변 돌아다녔어",
          "original_form": "나 그냥 그주변 돌아다녔어",
          "speaker_id": "2",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023703.1.8",
          "form": "볼 거 많았어?",
          "original_form": "볼 거 많았어?",
          "speaker_id": "1",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023703.1.9",
          "form": "아니ㅋㅋ 그냥 쭉 걸어다녔어",
