      "utterance": [
        {
          "id": "MDRW2100013369.1.1",
          "form": "와 세척사과 상태 엄청 좋네",
          "original_form": "와 세척사과 상태 엄청 좋네",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013369.1.2",
          "form": "도착했어?",
          "original_form": "도착했어?",
          "speaker_id": "1",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013369.1.3",
          "form": "ㅇㅇ 완전 아기자기해 ㅋㅋ",
          "original_form": "ㅇㅇ 완전 아기자기해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013369.1.4",
          "form": "썩은 건 없고?",
          "original_form": "썩은 건 없고?",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013369.1.5",
          "form": "겉도 깨끗하고 싱싱하네~ 아마 못난이 사과 같은 건가봐 크기 때문에 ㅎㅎ",
          "original_form": "겉도 깨끗하고 싱싱하네~ 아마 못난이 사과 같은 건가봐 크기 때문에 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013369.1.6",
          "form": "아침에 잘라먹으면 되겠다 ㅎㅎ",
          "original_form": "아침에 잘라먹으면 되겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013369.1.7",
          "form": "완전 득템인데? ㅋ",
          "original_form": "완전 득템인데? ㅋ",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013369.1.8",
          "form": "행사 또 하면 좋겠네 ㅎㅎ",
          "original_form": "행사 또 하면 좋겠네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013369.1.9",
          "form": "먹어보고 좋으면 또 사야겠다~",
          "original_form": "먹어보고 좋으면 또 사야겠다~",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013369.1.10",
          "form": "ㅇㅇ 하나 까서 먹어봐요",
          "original_form": "ㅇㅇ 하나 까서 먹어봐요",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013369.1.11",
          "form": "애들 게임 끝나면 같이 먹어보고 맛이 어떤지 얘기해줄께 ㅎㅎ",
          "original_form": "애들 게임 끝나면 같이 먹어보고 맛이 어떤지 얘기해줄께 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013369.1.12",
          "form": "궁금하다 ㅎㅎ 어떤지 얘기해줘",
          "original_form": "궁금하다 ㅎㅎ 어떤지 얘기해줘",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013369.1.13",
          "form": "한번에 두개씩은 먹어야 할 것 같아",
          "original_form": "한번에 두개씩은 먹어야 할 것 같아",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013369.1.14",
          "form": "꼬마 사과 크기야?",
          "original_form": "꼬마 사과 크기야?",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013369.1.15",
          "form": "그 정도는 아닌데 일반 사과의 3분의2 정도",
          "original_form": "그 정도는 아닌데 일반 사과의 3분의2 정도",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013369.1.16",
          "form": "작긴 작네 ㅎㅎ",
          "original_form": "작긴 작네 ㅎㅎ",
