      "utterance": [
        {
          "id": "MDRW2100003296.1.1",
          "form": "배달음식 좋아해?",
          "original_form": "배달음식 좋아해?",
          "speaker_id": "2",
          "time": "20210820 21:30"
        },
        {
          "id": "MDRW2100003296.1.2",
          "form": "응 좋아해",
          "original_form": "응 좋아해",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003296.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003296.1.4",
          "form": "나는 배달음식보다 집밥 선호해",
          "original_form": "나는 배달음식보다 집밥 선호해",
          "speaker_id": "2",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003296.1.5",
          "form": "하긴 건강에 좋지 집밥이",
          "original_form": "하긴 건강에 좋지 집밥이",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003296.1.6",
          "form": "하지만 귀찮을 때는 무조건 배달이지",
          "original_form": "하지만 귀찮을 때는 무조건 배달이지",
          "speaker_id": "2",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003296.1.7",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003296.1.8",
          "form": "코로나라 외식 못하니까",
          "original_form": "코로나라 외식 못하니까",
          "speaker_id": "2",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003296.1.9",
          "form": "배달 맛있지",
          "original_form": "배달 맛있지",
          "speaker_id": "1",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003296.1.10",
          "form": "요즘에는 배달 안되는 게 거의 없잖아",
          "original_form": "요즘에는 배달 안되는 게 거의 없잖아",
          "speaker_id": "2",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003296.1.11",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003296.1.12",
          "form": "코로나로 배달 매출 엄청 올랐다더라",
          "original_form": "코로나로 배달 매출 엄청 올랐다더라",
          "speaker_id": "1",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003296.1.13",
          "form": "근데 플라스틱 쓰레기 문제가 심각하더라고",
          "original_form": "근데 플라스틱 쓰레기 문제가 심각하더라고",
          "speaker_id": "2",
          "time": "20210820 21:33"
        },
        {
          "id": "MDRW2100003296.1.14",
          "form": "맞아 자기 용기로 포장하는 거 좋은 거 같아",
          "original_form": "맞아 자기 용기로 포장하는 거 좋은 거 같아",
          "speaker_id": "1",
          "time": "20210820 21:33"
        },
        {
          "id": "MDRW2100003296.1.15",
          "form": "용기 내기가 힘들지만",
          "original_form": "용기 내기가 힘들지만",
          "speaker_id": "2",
          "time": "20210820 21:33"
        },
        {
          "id": "MDRW2100003296.1.16",
          "form": "환경 생각하면",
          "original_form": "환경 생각하면",
          "speaker_id": "1",
          "time": "20210820 21:33"
        },
        {
          "id": "MDRW2100003296.1.17",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210820 21:33"
        }
      ]
