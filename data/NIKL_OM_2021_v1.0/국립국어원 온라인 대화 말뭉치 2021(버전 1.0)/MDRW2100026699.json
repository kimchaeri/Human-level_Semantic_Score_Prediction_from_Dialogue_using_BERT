      "utterance": [
        {
          "id": "MDRW2100026699.1.1",
          "form": "결혼 하셨나요?",
          "original_form": "결혼 하셨나요?",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026699.1.2",
          "form": "네~ 근데 너무졸려서",
          "original_form": "네~ 근데 너무졸려서",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026699.1.3",
          "form": "안녕히 주무세요!",
          "original_form": "안녕히 주무세요!",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026699.1.4",
          "form": "다음날 뵈어요!",
          "original_form": "다음날 뵈어요!",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026699.1.5",
          "form": "결혼한지 6년?",
          "original_form": "결혼한지 6년?",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100026699.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100026699.1.7",
          "form": "배우자 분이 뭐라고 부르시나요?",
          "original_form": "배우자 분이 뭐라고 부르시나요?",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100026699.1.8",
          "form": "자기야?",
          "original_form": "자기야?",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100026699.1.9",
          "form": "주로 부부 애칭? 부르는말은",
          "original_form": "주로 부부 애칭? 부르는말은",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100026699.1.10",
          "form": "애칭은 없어요ㅋ",
          "original_form": "애칭은 없어요ㅋ",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100026699.1.11",
          "form": "여보 or 자기 or 이름 아닌가요?",
          "original_form": "여보 or 자기 or 이름 아닌가요?",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100026699.1.12",
          "form": "글쵸 맨날안싸우면 다행",
          "original_form": "글쵸 맨날안싸우면 다행",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100026699.1.13",
          "form": "기본적으로 다들 뭐 오빠라던지 누나라던지ㅋ",
          "original_form": "기본적으로 다들 뭐 오빠라던지 누나라던지ㅋ",
          "speaker_id": "1",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100026699.1.14",
          "form": "애칭있으세요?",
          "original_form": "애칭있으세요?",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100026699.1.15",
          "form": "엄마 또는 아빠라고 부르는걸 어떻게 생각하시나요?",
          "original_form": "엄마 또는 아빠라고 부르는걸 어떻게 생각하시나요?",
          "speaker_id": "1",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100026699.1.16",
          "form": "앞에 애 이름 없이 그냥..",
          "original_form": "앞에 애 이름 없이 그냥..",
          "speaker_id": "1",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100026699.1.17",
          "form": "그건 별로",
          "original_form": "그건 별로",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100026699.1.18",
          "form": "애가 뛰면 다짜고짜 아빠!!",
          "original_form": "애가 뛰면 다짜고짜 아빠!!",
          "speaker_id": "1",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100026699.1.19",
          "form": "애가 부르면 다짜고짜 엄마!!",
          "original_form": "애가 부르면 다짜고짜 엄마!!",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100026699.1.20",
          "form": "아니 내가 아이의 아빠또는 엄마지\r\n배우자의 아빠엄마 아니잖아요",
          "original_form": "아니 내가 아이의 아빠또는 엄마지\r\n배우자의 아빠엄마 아니잖아요",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100026699.1.21",
          "form": "다시 또 이야기해요",
          "original_form": "다시 또 이야기해요",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100026699.1.22",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
