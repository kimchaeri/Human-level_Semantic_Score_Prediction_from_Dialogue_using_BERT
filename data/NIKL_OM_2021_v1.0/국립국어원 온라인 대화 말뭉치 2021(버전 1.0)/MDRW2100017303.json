      "utterance": [
        {
          "id": "MDRW2100017303.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017303.1.2",
          "form": "모임 하는거 있으세요?",
          "original_form": "모임 하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017303.1.3",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017303.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017303.1.5",
          "form": "낯을 많이 가려서 모임은 안 하고",
          "original_form": "낯을 많이 가려서 모임은 안 하고",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017303.1.6",
          "form": "봉사활동해요! 얼마전부터",
          "original_form": "봉사활동해요! 얼마전부터",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017303.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100017303.1.8",
          "form": "뭐하시나요?",
          "original_form": "뭐하시나요?",
          "speaker_id": "2",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100017303.1.9",
          "form": "아름다운 가게라고 아세요?",
          "original_form": "아름다운 가게라고 아세요?",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017303.1.10",
          "form": "물건 기부받아서 재판매하고 순환할 수 있도록 돕는 곳에서 봉사해요",
          "original_form": "물건 기부받아서 재판매하고 순환할 수 있도록 돕는 곳에서 봉사해요",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017303.1.11",
          "form": "오우 알죠",
          "original_form": "오우 알죠",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017303.1.12",
          "form": "와 좋은일하시네요",
          "original_form": "와 좋은일하시네요",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017303.1.13",
          "form": "아니에요ㅎㅎㅎ",
          "original_form": "아니에요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017303.1.14",
          "form": "아이들 관련 기관에서 봉사해보고 싶은데 요즘은 안 되더라고요ㅠ",
          "original_form": "아이들 관련 기관에서 봉사해보고 싶은데 요즘은 안 되더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017303.1.15",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017303.1.16",
          "form": "아예 무급이죠?",
          "original_form": "아예 무급이죠?",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017303.1.17",
          "form": "네네 봉사시간 인증정도?",
          "original_form": "네네 봉사시간 인증정도?",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017303.1.18",
          "form": "기관마다 다르긴한데 전 오전에 일해서 점심 챙겨주는걸로 식사해결되는거 정도?!",
          "original_form": "기관마다 다르긴한데 전 오전에 일해서 점심 챙겨주는걸로 식사해결되는거 정도?!",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017303.1.19",
          "form": "아 봉사시간인증이",
          "original_form": "아 봉사시간인증이",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017303.1.20",
          "form": "필요하신가요?!",
          "original_form": "필요하신가요?!",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017303.1.21",
          "form": "딱히 쓸데는 없는데 그냥 모으고 싶어서ㅋㅋㅋㅋㅋ",
          "original_form": "딱히 쓸데는 없는데 그냥 모으고 싶어서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017303.1.22",
          "form": "성취감때문에요 어렵지도 않고ㅎㅎ",
          "original_form": "성취감때문에요 어렵지도 않고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017303.1.23",
          "form": "와우..",
          "original_form": "와우..",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017303.1.24",
          "form": "진정한 봉사왕이시네요",
          "original_form": "진정한 봉사왕이시네요",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017303.1.25",
          "form": "아니에요ㅎㅎ대단한 일 아니고 그냥 가게 정리하는게 전부인걸요ㅠ",
          "original_form": "아니에요ㅎㅎ대단한 일 아니고 그냥 가게 정리하는게 전부인걸요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:30"
        }
      ]
    }
  ]
