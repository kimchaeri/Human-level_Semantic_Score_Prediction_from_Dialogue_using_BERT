        },
        {
          "id": "MDRW2100030032.1.3",
          "form": "여행 가고 싶은 곳 있어요?",
          "original_form": "여행 가고 싶은 곳 있어요?",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030032.1.4",
          "form": "음 난 부산 가보고 싶어!",
          "original_form": "음 난 부산 가보고 싶어!",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030032.1.5",
          "form": "우와 부산 좋죠",
          "original_form": "우와 부산 좋죠",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030032.1.6",
          "form": "name1이는 여행 가고 싶은 곳 있어?",
          "original_form": "&name1&이는 여행 가고 싶은 곳 있어?",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030032.1.7",
          "form": "부산 바다 가고 싶어요",
          "original_form": "부산 바다 가고 싶어요",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030032.1.8",
          "form": "저는 호주!",
          "original_form": "저는 호주!",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030032.1.9",
          "form": "호주 자연 보고 싶어요!!",
          "original_form": "호주 자연 보고 싶어요!!",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030032.1.10",
          "form": "부산 바다 가고 싶어 흑흑",
          "original_form": "부산 바다 가고 싶어 흑흑",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030032.1.11",
          "form": "오오 호주자연!",
          "original_form": "오오 호주자연!",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030032.1.12",
          "form": "그쵸ㅠㅠ 코로롱이라 못가고 있는...",
          "original_form": "그쵸ㅠㅠ 코로롱이라 못가고 있는...",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030032.1.13",
          "form": "호주에서 샌드보딩? 그것도 유명하던데!",
          "original_form": "호주에서 샌드보딩? 그것도 유명하던데!",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030032.1.14",
          "form": "어떤 자연 보고싶어??",
          "original_form": "어떤 자연 보고싶어??",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030032.1.15",
          "form": "오오 그것도 하고 싶어요",
          "original_form": "오오 그것도 하고 싶어요",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030032.1.16",
          "form": "저 오빠가 호주 다녀와서 자랑했었거든요 ㅋㅋㅋ",
          "original_form": "저 오빠가 호주 다녀와서 자랑했었거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030032.1.17",
          "form": "그래서 가고 싶어요!",
          "original_form": "그래서 가고 싶어요!",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030032.1.18",
          "form": "우와 오빠는 최근에 가신거야??",
          "original_form": "우와 오빠는 최근에 가신거야??",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100030032.1.19",
          "form": "완전 부럽다ㅠㅠㅠ",
          "original_form": "완전 부럽다ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100030032.1.20",
          "form": "아뇨 오빠는 진짜 예전에 결혼전ㅋㅋㅋㅋ",
          "original_form": "아뇨 오빠는 진짜 예전에 결혼전ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100030032.1.21",
          "form": "한 6년 전이에요!",
          "original_form": "한 6년 전이에요!",
          "speaker_id": "2",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100030032.1.22",
          "form": "앗 ㅋㅋㅋ 예전에었구나..!!",
          "original_form": "앗 ㅋㅋㅋ 예전에었구나..!!",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100030032.1.23",
          "form": "코로롱 전이었어ㅠㅠ 흑 그때 갔어야했나봐 진짜",
          "original_form": "코로롱 전이었어ㅠㅠ 흑 그때 갔어야했나봐 진짜",
