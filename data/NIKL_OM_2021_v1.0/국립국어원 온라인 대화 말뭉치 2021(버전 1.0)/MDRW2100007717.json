      "utterance": [
        {
          "id": "MDRW2100007717.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007717.1.2",
          "form": "네 고양이 키워요",
          "original_form": "네 고양이 키워요",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007717.1.3",
          "form": "name1님은 동물좋아하시나요?",
          "original_form": "&name1&님은 동물좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007717.1.4",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "1",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007717.1.5",
          "form": "집사시군요",
          "original_form": "집사시군요",
          "speaker_id": "1",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007717.1.6",
          "form": "고양이 이름은 뭔가요?",
          "original_form": "고양이 이름은 뭔가요?",
          "speaker_id": "1",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007717.1.7",
          "form": "고양이 이름은 감자에요",
          "original_form": "고양이 이름은 감자에요",
          "speaker_id": "2",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007717.1.8",
          "form": "귀엽네요!",
          "original_form": "귀엽네요!",
          "speaker_id": "1",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007717.1.9",
          "form": "주로 간식은 뭘 먹나요?",
          "original_form": "주로 간식은 뭘 먹나요?",
          "speaker_id": "1",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007717.1.10",
          "form": "아무래도 추르 가장 좋아하더라구요",
          "original_form": "아무래도 추르 가장 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007717.1.11",
          "form": "고양이들의 소울푸드랄까",
          "original_form": "고양이들의 소울푸드랄까",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007717.1.12",
          "form": "저는 진짜 키우고 싶은데 털 알러지 때문에 못키워요ㅠㅠ",
          "original_form": "저는 진짜 키우고 싶은데 털 알러지 때문에 못키워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007717.1.13",
          "form": "사진으로만 보고 있습니다",
          "original_form": "사진으로만 보고 있습니다",
          "speaker_id": "1",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007717.1.14",
          "form": "아이고.. 알러지 너무 힘들죠ㅠㅠ",
          "original_form": "아이고.. 알러지 너무 힘들죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007717.1.15",
          "form": "랜선집사하시는구나",
          "original_form": "랜선집사하시는구나",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007717.1.16",
          "form": "어머 맞아요 랜선집사ㅋㅋ",
          "original_form": "어머 맞아요 랜선집사ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007717.1.17",
          "form": "장난감도 많이 있으신가요?",
          "original_form": "장난감도 많이 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007717.1.18",
          "form": "아무래도 낚시놀이 좋아하니까 비슷한 종류로 많이 있어요",
          "original_form": "아무래도 낚시놀이 좋아하니까 비슷한 종류로 많이 있어요",
          "speaker_id": "2",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007717.1.19",
          "form": "동물 좋아하는데 털 알러지 있으시면 많이 힘드시겠어요",
          "original_form": "동물 좋아하는데 털 알러지 있으시면 많이 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007717.1.20",
          "form": "네ㅠㅠ사진으로 앓는답니다ㅎㅎ",
          "original_form": "네ㅠㅠ사진으로 앓는답니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007717.1.21",
          "form": "간식이랑 사료비로 많이 나가나요?",
          "original_form": "간식이랑 사료비로 많이 나가나요?",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007717.1.22",
          "form": "달에 크게 나가는건 없어요 한번 사면 한달~한달 반 정도? 버티니까 네..",
          "original_form": "달에 크게 나가는건 없어요 한번 사면 한달~한달 반 정도? 버티니까 네..",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007717.1.23",
          "form": "대신 병원비같은게 좀 비싸더라구요",
          "original_form": "대신 병원비같은게 좀 비싸더라구요",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007717.1.24",
          "form": "맞아요 예방접종비? 이런것도 비싸더라구요",
          "original_form": "맞아요 예방접종비? 이런것도 비싸더라구요",
          "speaker_id": "1",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100007717.1.25",
          "form": "어디 아프면 또 더 나가고",
          "original_form": "어디 아프면 또 더 나가고",
          "speaker_id": "1",
          "time": "20210822 10:07"
        }
      ]
