      "utterance": [
        {
          "id": "MDRW2100043183.1.1",
          "form": "최근 쇼핑하셨나용~?",
          "original_form": "최근 쇼핑하셨나용~?",
          "speaker_id": "1",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100043183.1.2",
          "form": "지원금하나로 12번 턴은 힘들었네요 ㅎㅎ..",
          "original_form": "지원금하나로 12번 턴은 힘들었네요 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100043183.1.3",
          "form": "ㅋㅋㅋㅋ네네 최근에 해초비빔밥샀어요",
          "original_form": "ㅋㅋㅋㅋ네네 최근에 해초비빔밥샀어요",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043183.1.4",
          "form": "오오 어디서 사셨죠?",
          "original_form": "오오 어디서 사셨죠?",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043183.1.5",
          "form": "맛있나용?",
          "original_form": "맛있나용?",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043183.1.6",
          "form": "인터넷으로 오늘오전에샀어요",
          "original_form": "인터넷으로 오늘오전에샀어요",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043183.1.7",
          "form": "ㅋㅋㅋㅋ추천으로산건데맛있대요",
          "original_form": "ㅋㅋㅋㅋ추천으로산건데맛있대요",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043183.1.8",
          "form": "엄마가 좋아하실 것 같아요 ㅎㅎㅎㅎ",
          "original_form": "엄마가 좋아하실 것 같아요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043183.1.9",
          "form": "하나에 얼마정도 했나용? ㅎㅎ",
          "original_form": "하나에 얼마정도 했나용? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043183.1.10",
          "form": "ㅋㅋㅋ마켓컬리에서 2500원이더라구요",
          "original_form": "ㅋㅋㅋ마켓컬리에서 2500원이더라구요",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043183.1.11",
          "form": "저도 먹어보진못했어여ㅋㅋㅋ 소스가맛있대여",
          "original_form": "저도 먹어보진못했어여ㅋㅋㅋ 소스가맛있대여",
          "speaker_id": "2",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043183.1.12",
          "form": "아정말요??",
          "original_form": "아정말요??",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043183.1.13",
          "form": "엄청 저렴한데요??",
          "original_form": "엄청 저렴한데요??",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043183.1.14",
          "form": "ㅋㅋㅋ네 그렇더라구요 ㅋㅋㅋ 기대중이에요 최근에 머 사셨나요",
          "original_form": "ㅋㅋㅋ네 그렇더라구요 ㅋㅋㅋ 기대중이에요 최근에 머 사셨나요",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100043183.1.15",
          "form": "저는 올리브영세일에서 이것저것이요 ㅎㅎㅎ",
          "original_form": "저는 올리브영세일에서 이것저것이요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043183.1.16",
          "form": "고카쥰 로션? 이 좋다더라구요",
          "original_form": "고카쥰 로션? 이 좋다더라구요",
          "speaker_id": "1",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043183.1.17",
          "form": "써보셨나요?",
          "original_form": "써보셨나요?",
          "speaker_id": "1",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043183.1.18",
          "form": "엄청 예전에 썼던거같은데...고쿠하준? 이건가여?ㅋㅋㅋ",
          "original_form": "엄청 예전에 썼던거같은데...고쿠하준? 이건가여?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043183.1.19",
          "form": "아아 하다라보 ㅋㅋㅋ 맞아여 이름을 잘못알고있었네요",
          "original_form": "아아 하다라보 ㅋㅋㅋ 맞아여 이름을 잘못알고있었네요",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043183.1.20",
          "form": "써봤어요 ㅋㅋ",
          "original_form": "써봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100043183.1.21",
          "form": "아 정말요??",
          "original_form": "아 정말요??",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043183.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043183.1.23",
          "form": "어때용???",
          "original_form": "어때용???",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043183.1.24",
          "form": "저도 이번에 구매했어요!",
          "original_form": "저도 이번에 구매했어요!",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043183.1.25",
          "form": "무난해서 괜찮았던거같아요~!",
          "original_form": "무난해서 괜찮았던거같아요~!",
          "speaker_id": "2",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043183.1.26",
          "form": "다행이네요!!",
          "original_form": "다행이네요!!",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043183.1.27",
          "form": "당근도 하시나요?",
          "original_form": "당근도 하시나요?",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043183.1.28",
          "form": "저는 안해요~! 저빼고 다하는거같아요 ㅋㅋ",
          "original_form": "저는 안해요~! 저빼고 다하는거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043183.1.29",
          "form": "쇼핑하고싶네요!!",
          "original_form": "쇼핑하고싶네요!!",
