      "utterance": [
        {
          "id": "MDRW2100001716.1.1",
          "form": "오늘 저녁에 뭐 먹을거니?",
          "original_form": "오늘 저녁에 뭐 먹을거니?",
          "speaker_id": "1",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001716.1.2",
          "form": "나 어제 두부김치 먹고 남은거..",
          "original_form": "나 어제 두부김치 먹고 남은거..",
          "speaker_id": "2",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001716.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001716.1.4",
          "form": "근데 김치 볶는거말고 지지는거 알아? 지짐 맛있더라",
          "original_form": "근데 김치 볶는거말고 지지는거 알아? 지짐 맛있더라",
          "speaker_id": "2",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001716.1.5",
          "form": "오 두부김치맛있겠다",
          "original_form": "오 두부김치맛있겠다",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001716.1.6",
          "form": "지지는거? 모르겠네",
          "original_form": "지지는거? 모르겠네",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001716.1.7",
          "form": "어제 두부 한모를 먹었어서 그렇게 땡기진않네",
          "original_form": "어제 두부 한모를 먹었어서 그렇게 땡기진않네",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001716.1.8",
          "form": "근데 반모가 남아서 먹어야해.. 걍 드레싱뿌려서 먹을까",
          "original_form": "근데 반모가 남아서 먹어야해.. 걍 드레싱뿌려서 먹을까",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001716.1.9",
          "form": "큰거한모?",
          "original_form": "큰거한모?",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001716.1.10",
          "form": "반포기 만들어놨으니까 내일 먹어봐ㅋㅋ",
          "original_form": "반포기 만들어놨으니까 내일 먹어봐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001716.1.11",
          "form": "큰거가 300g이니? 그거야",
          "original_form": "큰거가 300g이니? 그거야",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001716.1.12",
          "form": "그랭! 먹어볼게",
          "original_form": "그랭! 먹어볼게",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001716.1.13",
          "form": "적당히 크네",
          "original_form": "적당히 크네",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001716.1.14",
          "form": "감자도 많이 있어서 그걸로 간장조림을 할까 빨갛게 짜글이를 할까 고민중이야",
          "original_form": "감자도 많이 있어서 그걸로 간장조림을 할까 빨갛게 짜글이를 할까 고민중이야",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001716.1.15",
          "form": "적당히 크지ㅋㅋㅋ",
          "original_form": "적당히 크지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001716.1.16",
          "form": "너네집도 감자 많구나ㅋㅋ",
          "original_form": "너네집도 감자 많구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001716.1.17",
          "form": "응ㅋㅋ 넌 뭐해먹었어?",
          "original_form": "응ㅋㅋ 넌 뭐해먹었어?",
          "speaker_id": "2",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001716.1.18",
          "form": "나는 감자옹심이랑 짜글이ㅋㅋ",
          "original_form": "나는 감자옹심이랑 짜글이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:28"
        },
        {
          "id": "MDRW2100001716.1.19",
          "form": "스팸짜글이 했는데 짜서 그냥 그랬어",
          "original_form": "스팸짜글이 했는데 짜서 그냥 그랬어",
          "speaker_id": "1",
          "time": "20210819 18:28"
        },
        {
          "id": "MDRW2100001716.1.20",
          "form": "아진짜? 백종원? 나 그거할라했는데..",
          "original_form": "아진짜? 백종원? 나 그거할라했는데..",
          "speaker_id": "2",
          "time": "20210819 18:29"
        },
        {
          "id": "MDRW2100001716.1.21",
          "form": "응 백종원유튜브!양세형 나오는거",
          "original_form": "응 백종원유튜브!양세형 나오는거",
          "speaker_id": "1",
          "time": "20210819 18:29"
        },
        {
          "id": "MDRW2100001716.1.22",
          "form": "근데 내가 짜게한걸수도있어",
          "original_form": "근데 내가 짜게한걸수도있어",
          "speaker_id": "1",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001716.1.23",
          "form": "그리고 제대로  안따라해서ㅋㅋ",
          "original_form": "그리고 제대로  안따라해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001716.1.24",
          "form": "ㅋㅋㅋ 맞아 그거 봐도 내식대로 하게돼ㅋㅋ 그럼 맛도 다 비슷해지는거같음ㅋㅋ",
          "original_form": "ㅋㅋㅋ 맞아 그거 봐도 내식대로 하게돼ㅋㅋ 그럼 맛도 다 비슷해지는거같음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:34"
        },
        {
          "id": "MDRW2100001716.1.25",
          "form": "마자ㅋㅋㅋ대충 이거이거 넣으라는거지",
          "original_form": "마자ㅋㅋㅋ대충 이거이거 넣으라는거지",
          "speaker_id": "1",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100001716.1.26",
          "form": "이러고 만들게돼ㅋㅋ",
          "original_form": "이러고 만들게돼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100001716.1.27",
          "form": "나도ㅋㅋ 설탕 조금넣고",
          "original_form": "나도ㅋㅋ 설탕 조금넣고",
          "speaker_id": "2",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001716.1.28",
          "form": "우리 토요일에는 뭐먹어?",
          "original_form": "우리 토요일에는 뭐먹어?",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100001716.1.29",
          "form": "토요일은 샤브샤브고",
          "original_form": "토요일은 샤브샤브고",
          "speaker_id": "2",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100001716.1.30",
          "form": "금요일을 안정했어!",
          "original_form": "금요일을 안정했어!",
          "speaker_id": "2",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100001716.1.31",
          "form": "아니 토요일 낮에 먹을거",
          "original_form": "아니 토요일 낮에 먹을거",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100001716.1.32",
          "form": "거기는 배떡 없지?",
          "original_form": "거기는 배떡 없지?",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100001716.1.33",
          "form": "요새 로제떡볶이가 너무 맛있어",
          "original_form": "요새 로제떡볶이가 너무 맛있어",
          "speaker_id": "1",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100001716.1.34",
          "form": "배떡있어! 바로 코앞이야ㅋㅋ 근데 배떡이랑 맛 똑같다고 태리로제 먹어봤는데 거기도 괜찮더라고?",
          "original_form": "배떡있어! 바로 코앞이야ㅋㅋ 근데 배떡이랑 맛 똑같다고 태리로제 먹어봤는데 거기도 괜찮더라고?",
          "speaker_id": "2",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100001716.1.35",
          "form": "아 토요일 낮에는 홍대라서 홍대엔 모르겠네",
          "original_form": "아 토요일 낮에는 홍대라서 홍대엔 모르겠네",
          "speaker_id": "2",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100001716.1.36",
          "form": "마자 태리로제도 유명한거같더라",
          "original_form": "마자 태리로제도 유명한거같더라",
          "speaker_id": "1",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100001716.1.37",
          "form": "가서 생각하지뭐",
          "original_form": "가서 생각하지뭐",
          "speaker_id": "1",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100001716.1.38",
          "form": "집에서 먹어도됑ㅋㅋ",
          "original_form": "집에서 먹어도됑ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:26"
        },
        {
          "id": "MDRW2100001716.1.39",
          "form": "태리로제?",
          "original_form": "태리로제?",
          "speaker_id": "1",
          "time": "20210819 19:27"
        },
        {
          "id": "MDRW2100001716.1.40",
          "form": "응 태리로제도 있어! 배달비가 붙긴하지만ㅋㅋ",
          "original_form": "응 태리로제도 있어! 배달비가 붙긴하지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:28"
        }
      ]
