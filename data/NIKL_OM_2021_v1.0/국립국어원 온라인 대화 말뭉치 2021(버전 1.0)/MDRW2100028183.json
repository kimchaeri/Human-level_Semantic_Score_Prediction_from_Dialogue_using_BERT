        {
          "id": "MDRW2100028183.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028183.1.2",
          "form": "보통 팔때 많이 해요!",
          "original_form": "보통 팔때 많이 해요!",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028183.1.3",
          "form": "전 당근",
          "original_form": "전 당근",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100028183.1.4",
          "form": "최근에 뭐 거래하셨어요ㅋㅋ",
          "original_form": "최근에 뭐 거래하셨어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028183.1.5",
          "form": "공기청정기요",
          "original_form": "공기청정기요",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100028183.1.6",
          "form": "오 필수템 구하셨구나",
          "original_form": "오 필수템 구하셨구나",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028183.1.7",
          "form": "팔기만요",
          "original_form": "팔기만요",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028183.1.8",
          "form": "는 파신거구나..",
          "original_form": "는 파신거구나..",
          "speaker_id": "2",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100028183.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100028183.1.10",
          "form": "저는 먹어보고 싶었던 홍차티백 소분 샀었어요ㅋㅋ",
          "original_form": "저는 먹어보고 싶었던 홍차티백 소분 샀었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:39"
        },
        {
          "id": "MDRW2100028183.1.11",
          "form": "맛나겠네녀",
          "original_form": "맛나겠네녀",
          "speaker_id": "1",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100028183.1.12",
          "form": "우유에 담궈먹으면 맛있더라구요",
