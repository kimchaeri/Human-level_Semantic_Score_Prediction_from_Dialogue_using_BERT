      "utterance": [
        {
          "id": "MDRW2100008881.1.1",
          "form": "앗 알겠습니다!",
          "original_form": "앗 알겠습니다!",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008881.1.2",
          "form": "동물 키우시는거 있으세요?",
          "original_form": "동물 키우시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008881.1.3",
          "form": "저는 구피를 키웁니닷ㅋㅋ",
          "original_form": "저는 구피를 키웁니닷ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008881.1.4",
          "form": "식물하구ㅎㅎ",
          "original_form": "식물하구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008881.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008881.1.6",
          "form": "앗 저도 구피하고 새우 예전에 키웠는데 지금은 아니에요",
          "original_form": "앗 저도 구피하고 새우 예전에 키웠는데 지금은 아니에요",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008881.1.7",
          "form": "구피가 새우를 먹었나요?",
          "original_form": "구피가 새우를 먹었나요?",
          "speaker_id": "1",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008881.1.8",
          "form": "먹기도 했는데ㅋㅋㅋㅋ",
          "original_form": "먹기도 했는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008881.1.9",
          "form": "관리가너무힘들기도 하고",
          "original_form": "관리가너무힘들기도 하고",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008881.1.10",
          "form": "아하ㅜ",
          "original_form": "아하ㅜ",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008881.1.11",
          "form": "오래하다보니 지쳐서?ㅋㅋ다분양하고",
          "original_form": "오래하다보니 지쳐서?ㅋㅋ다분양하고",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008881.1.12",
          "form": "손털었어요",
          "original_form": "손털었어요",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008881.1.13",
          "form": "저는 작은 어항이라 아직 유지하고있어요",
          "original_form": "저는 작은 어항이라 아직 유지하고있어요",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008881.1.14",
          "form": "물고기 물갈아주는거 은근 번거롭지요",
          "original_form": "물고기 물갈아주는거 은근 번거롭지요",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008881.1.15",
          "form": "네ㅠㅠ이끼 딱고 똥 치우고",
          "original_form": "네ㅠㅠ이끼 딱고 똥 치우고",
          "speaker_id": "2",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008881.1.16",
          "form": "ㅋㅋ 똥치우고 222",
          "original_form": "ㅋㅋ 똥치우고 222",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008881.1.17",
          "form": "물한번 갈면 시간이...",
          "original_form": "물한번 갈면 시간이...",
          "speaker_id": "2",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008881.1.18",
          "form": "와..",
          "original_form": "와..",
          "speaker_id": "2",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008881.1.19",
          "form": "스포이드로 뽑아내고",
          "original_form": "스포이드로 뽑아내고",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008881.1.20",
          "form": "그래도 다른동물보단 손이덜가죠",
          "original_form": "그래도 다른동물보단 손이덜가죠",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008881.1.21",
          "form": "ㅋㅋㅋㅋㅋ 헬",
          "original_form": "ㅋㅋㅋㅋㅋ 헬",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008881.1.22",
          "form": "맞아요 물멍도하고",
          "original_form": "맞아요 물멍도하고",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008881.1.23",
          "form": "ㅋㅋㅋ 헬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 헬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008881.1.24",
          "form": "새우가참 기여웠는데",
          "original_form": "새우가참 기여웠는데",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008881.1.25",
          "form": "치.비도키우고",
          "original_form": "치.비도키우고",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008881.1.26",
          "form": "많이 키우셨네요",
          "original_form": "많이 키우셨네요",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008881.1.27",
          "form": "진짜 키우기 힘들더라구요 쬐깐 한게",
          "original_form": "진짜 키우기 힘들더라구요 쬐깐 한게",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008881.1.28",
          "form": "치비가먼가요",
          "original_form": "치비가먼가요",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008881.1.29",
          "form": "새우의 아기ㅋ",
          "original_form": "새우의 아기ㅋ",
          "speaker_id": "2",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008881.1.30",
          "form": "씨몽키인가요?",
          "original_form": "씨몽키인가요?",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008881.1.31",
          "form": "아니요~",
          "original_form": "아니요~",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008881.1.32",
          "form": "달라요~ ㅋㅋㅋ",
          "original_form": "달라요~ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008881.1.33",
          "form": "체리새우가 알 품고다니거든요",
          "original_form": "체리새우가 알 품고다니거든요",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008881.1.34",
          "form": "안키워본티가 팍파나네 나 ㅋㅋㅋ",
          "original_form": "안키워본티가 팍파나네 나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008881.1.35",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008881.1.36",
          "form": "찾아봐야겠어요",
          "original_form": "찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008881.1.37",
          "form": "다리밑에서 알을 품고 다니면서 다리로 알을 굴려서",
          "original_form": "다리밑에서 알을 품고 다니면서 다리로 알을 굴려서",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008881.1.38",
          "form": "데리고다니다가 한달쯤뒤에 부화하는데 공기그것도?있어야하고",
          "original_form": "데리고다니다가 한달쯤뒤에 부화하는데 공기그것도?있어야하고",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008881.1.39",
          "form": "새우는 키우기가 힘들더라구요",
          "original_form": "새우는 키우기가 힘들더라구요",
          "speaker_id": "2",
          "time": "20210822 19:31"
        }
      ]
