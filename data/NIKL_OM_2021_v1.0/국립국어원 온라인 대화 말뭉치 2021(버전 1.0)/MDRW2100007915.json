      "utterance": [
        {
          "id": "MDRW2100007915.1.1",
          "form": "염색도 해보고 싶었는데",
          "original_form": "염색도 해보고 싶었는데",
          "speaker_id": "2",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007915.1.2",
          "form": "고민만 하다 결국 못했어",
          "original_form": "고민만 하다 결국 못했어",
          "speaker_id": "2",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007915.1.3",
          "form": "옹",
          "original_form": "옹",
          "speaker_id": "1",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007915.1.4",
          "form": "염색 해본적없지않아?",
          "original_form": "염색 해본적없지않아?",
          "speaker_id": "1",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007915.1.5",
          "form": "응 한번도 없었지",
          "original_form": "응 한번도 없었지",
          "speaker_id": "2",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007915.1.6",
          "form": "한다면 무슨 색?",
          "original_form": "한다면 무슨 색?",
          "speaker_id": "1",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007915.1.7",
          "form": "탈색 하고 핑크브라운ㅎㅎ",
          "original_form": "탈색 하고 핑크브라운ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:51"
        },
        {
          "id": "MDRW2100007915.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 11:54"
        },
        {
          "id": "MDRW2100007915.1.9",
          "form": "단발로해서?",
          "original_form": "단발로해서?",
          "speaker_id": "1",
          "time": "20210822 11:55"
        },
        {
          "id": "MDRW2100007915.1.10",
          "form": "긴머리만 봐서 그런가 님 단발도 어울릴것같음ㅋㅋ",
          "original_form": "긴머리만 봐서 그런가 님 단발도 어울릴것같음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:55"
        },
        {
          "id": "MDRW2100007915.1.11",
          "form": "아, 단발...하고싶긴한데 나 머리카락이 너무 억세서",
          "original_form": "아, 단발...하고싶긴한데 나 머리카락이 너무 억세서",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007915.1.12",
          "form": "매일 매일 세팅을 엄청 빡세게 해야해",
          "original_form": "매일 매일 세팅을 엄청 빡세게 해야해",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007915.1.13",
          "form": "아아아ㅠㅠ",
          "original_form": "아아아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007915.1.14",
          "form": "나 이번 겨울에 얼굴 고칠거야",
          "original_form": "나 이번 겨울에 얼굴 고칠거야",
          "speaker_id": "1",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007915.1.15",
          "form": "쌍술은 당연히할거고",
          "original_form": "쌍술은 당연히할거고",
          "speaker_id": "1",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007915.1.16",
          "form": "코는 상담받고 할거야",
          "original_form": "코는 상담받고 할거야",
          "speaker_id": "1",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007915.1.17",
          "form": "코는 아프다는 얘기가있어서ㅠ",
          "original_form": "코는 아프다는 얘기가있어서ㅠ",
          "speaker_id": "1",
          "time": "20210822 11:58"
        },
        {
          "id": "MDRW2100007915.1.18",
          "form": "코는 신중해야 해ㅠ코는 진짜로...",
          "original_form": "코는 신중해야 해ㅠ코는 진짜로...",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007915.1.19",
          "form": "쌍수는 안말리는데 코는 한번쯤 말리라는 글도 봤음",
          "original_form": "쌍수는 안말리는데 코는 한번쯤 말리라는 글도 봤음",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007915.1.20",
          "form": "맞아ㅠ",
          "original_form": "맞아ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007915.1.21",
          "form": "근데 코가 날렵하면 좋겠어ㅠ",
          "original_form": "근데 코가 날렵하면 좋겠어ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007915.1.22",
          "form": "맞아, 코가 예쁘면 좋지",
          "original_form": "맞아, 코가 예쁘면 좋지",
          "speaker_id": "2",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007915.1.23",
          "form": "코 이쁘면 옆모습이 이뻐지는것같아",
          "original_form": "코 이쁘면 옆모습이 이뻐지는것같아",
          "speaker_id": "1",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007915.1.24",
          "form": "아~~~맞아맞아맞아",
          "original_form": "아~~~맞아맞아맞아",
          "speaker_id": "2",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007915.1.25",
          "form": "좀 입체적이 되는것같가?",
          "original_form": "좀 입체적이 되는것같가?",
          "speaker_id": "2",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007915.1.26",
          "form": "고?",
          "original_form": "고?",
          "speaker_id": "2",
          "time": "20210822 12:04"
        }
      ]
