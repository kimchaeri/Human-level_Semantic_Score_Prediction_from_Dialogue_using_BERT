      "utterance": [
        {
          "id": "MDRW2100005140.1.1",
          "form": "요즘 갖고싶은거 있어?",
          "original_form": "요즘 갖고싶은거 있어?",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005140.1.2",
          "form": "태블릿....",
          "original_form": "태블릿....",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005140.1.3",
          "form": "태블릿이너무갖고싶어요...",
          "original_form": "태블릿이너무갖고싶어요...",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005140.1.4",
          "form": "name1님만아세요",
          "original_form": "&name1&님만아세요",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005140.1.5",
          "form": "저 내일 중고거래하러가여",
          "original_form": "저 내일 중고거래하러가여",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005140.1.6",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.7",
          "form": "태블릿 좋으시겠어요",
          "original_form": "태블릿 좋으시겠어요",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.8",
          "form": "비밀이에요",
          "original_form": "비밀이에요",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.9",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.10",
          "form": "무슨 태블릿인가요?",
          "original_form": "무슨 태블릿인가요?",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.11",
          "form": "갤s5e",
          "original_form": "갤s5e",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.12",
          "form": "당근마켓 거래인가요?",
          "original_form": "당근마켓 거래인가요?",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.13",
          "form": "예...",
          "original_form": "예...",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.14",
          "form": "와아 좋은건가요?",
          "original_form": "와아 좋은건가요?",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.15",
          "form": "내일 상태보고 괜찮으면 거래하기로했어요",
          "original_form": "내일 상태보고 괜찮으면 거래하기로했어요",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.16",
          "form": "음...그냥 그럭저럭??",
          "original_form": "음...그냥 그럭저럭??",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.17",
          "form": "27만원....",
          "original_form": "27만원....",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.18",
          "form": "좋아요 내일 거래때 비안오면 좋겠네요",
          "original_form": "좋아요 내일 거래때 비안오면 좋겠네요",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005140.1.19",
          "form": "헥 꽤 가격이 있네요",
          "original_form": "헥 꽤 가격이 있네요",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005140.1.20",
          "form": "ㅠ..ㅜ..",
          "original_form": "ㅠ..ㅜ..",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005140.1.21",
          "form": "누가 공짜로줬으면 좋겠어요",
          "original_form": "누가 공짜로줬으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005140.1.22",
          "form": "빨리 싸다고 말해주세요",
          "original_form": "빨리 싸다고 말해주세요",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005140.1.23",
          "form": "합리적인가격ㄱ이라고말해주세요",
          "original_form": "합리적인가격ㄱ이라고말해주세요",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005140.1.24",
          "form": "빨리",
          "original_form": "빨리",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005140.1.25",
          "form": "합리적인 가격같아요...",
          "original_form": "합리적인 가격같아요...",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005140.1.26",
          "form": "고마워요...",
          "original_form": "고마워요...",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005140.1.27",
          "form": "비싼거같기도해요",
          "original_form": "비싼거같기도해요",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005140.1.28",
          "form": "하지만...!!!새로사면",
          "original_form": "하지만...!!!새로사면",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005140.1.29",
          "form": "50만원이라고요...",
          "original_form": "50만원이라고요...",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005140.1.30",
          "form": "합리적이라고말해요",
          "original_form": "합리적이라고말해요",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005140.1.31",
          "form": "날 슬프게하지말란말야...",
          "original_form": "날 슬프게하지말란말야...",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005140.1.32",
          "form": "너무 저렴해요",
          "original_form": "너무 저렴해요",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005140.1.33",
          "form": "현명하신 선택을하셨어요",
          "original_form": "현명하신 선택을하셨어요",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005140.1.34",
          "form": "저도 좋은 거래하고싶어요",
          "original_form": "저도 좋은 거래하고싶어요",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005140.1.35",
          "form": "당근마켓에서 진상들을 많이봤어요",
          "original_form": "당근마켓에서 진상들을 많이봤어요",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005140.1.36",
          "form": "고망워요....",
          "original_form": "고망워요....",
          "speaker_id": "2",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005140.1.37",
          "form": "마음이..한결...나아졌어요....",
          "original_form": "마음이..한결...나아졌어요....",
          "speaker_id": "2",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005140.1.38",
          "form": "굿이에요..",
          "original_form": "굿이에요..",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005140.1.39",
          "form": "히믈내세요",
          "original_form": "히믈내세요",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005140.1.40",
          "form": "덜덜덜 살해당하지않게 도끼라도",
          "original_form": "덜덜덜 살해당하지않게 도끼라도",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005140.1.41",
          "form": "챙겨가야할까요",
          "original_form": "챙겨가야할까요",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005140.1.42",
          "form": "그럴필욘 없을거같아요",
          "original_form": "그럴필욘 없을거같아요",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005140.1.43",
          "form": "내일 좋은거래하고오세요~!~!",
          "original_form": "내일 좋은거래하고오세요~!~!",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005140.1.44",
          "form": "곰아워요",
          "original_form": "곰아워요",
          "speaker_id": "2",
          "time": "20210821 20:58"
        }
      ]
    }
