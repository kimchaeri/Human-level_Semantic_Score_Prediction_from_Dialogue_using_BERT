      "utterance": [
        {
          "id": "MDRW2100005892.1.1",
          "form": "점심은 빵 먹었어요",
          "original_form": "점심은 빵 먹었어요",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005892.1.2",
          "form": "코로나 이후로",
          "original_form": "코로나 이후로",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005892.1.3",
          "form": "진짜 간단하게 드셨네요 배 안고프시나요",
          "original_form": "진짜 간단하게 드셨네요 배 안고프시나요",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005892.1.4",
          "form": "각자 자리에서",
          "original_form": "각자 자리에서",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005892.1.5",
          "form": "싸온 거 먹는지라",
          "original_form": "싸온 거 먹는지라",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005892.1.6",
          "form": "그래서 아침을",
          "original_form": "그래서 아침을",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005892.1.7",
          "form": "꼭 먹습니다",
          "original_form": "꼭 먹습니다",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005892.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005892.1.9",
          "form": "저녁을 잘 챙겨드셔야겠어요",
          "original_form": "저녁을 잘 챙겨드셔야겠어요",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100005892.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100005892.1.11",
          "form": "아침과 저녁이",
          "original_form": "아침과 저녁이",
          "speaker_id": "1",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100005892.1.12",
          "form": "풍족한 편이랍니다",
          "original_form": "풍족한 편이랍니다",
          "speaker_id": "1",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100005892.1.13",
          "form": "오늘 저녁 메뉴는 생각해보셨나요",
          "original_form": "오늘 저녁 메뉴는 생각해보셨나요",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005892.1.14",
          "form": "그...러게요",
          "original_form": "그...러게요",
          "speaker_id": "1",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005892.1.15",
          "form": "지금 고민 중입니다",
          "original_form": "지금 고민 중입니다",
          "speaker_id": "1",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005892.1.16",
          "form": "고등어나 구워먹을까 싶기도 하고...",
          "original_form": "고등어나 구워먹을까 싶기도 하고...",
          "speaker_id": "1",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005892.1.17",
          "form": "저는 배달반찬으로 먹었습니다ㅎㅎ",
          "original_form": "저는 배달반찬으로 먹었습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100005892.1.18",
          "form": "결국 고등어 먹었습니다",
          "original_form": "결국 고등어 먹었습니다",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100005892.1.19",
          "form": "배달 반찬은",
          "original_form": "배달 반찬은",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100005892.1.20",
          "form": "뭐 드서나요",
          "original_form": "뭐 드서나요",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100005892.1.21",
          "form": "비지찌개요..ㅎㅎ",
          "original_form": "비지찌개요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100005892.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100005892.1.23",
          "form": "비지찌개가",
          "original_form": "비지찌개가",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100005892.1.24",
          "form": "배달이 돼요?",
          "original_form": "배달이 돼요?",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100005892.1.25",
          "form": "반찬배달이요~",
          "original_form": "반찬배달이요~",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100005892.1.26",
          "form": "오...",
