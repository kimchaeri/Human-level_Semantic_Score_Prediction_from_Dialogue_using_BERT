      "utterance": [
        {
          "id": "MDRW2100026028.1.1",
          "form": "요즘 보는 드라마 있으신가요",
          "original_form": "요즘 보는 드라마 있으신가요",
          "speaker_id": "1",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100026028.1.2",
          "form": "부모님때문에 평일은 빨강구두보고 주말은 오케이광자매봐요",
          "original_form": "부모님때문에 평일은 빨강구두보고 주말은 오케이광자매봐요",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100026028.1.3",
          "form": "저는 그저께",
          "original_form": "저는 그저께",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100026028.1.4",
          "form": "신드라마 하길래 봤어요",
          "original_form": "신드라마 하길래 봤어요",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100026028.1.5",
          "form": "약간 퓨전 사극",
          "original_form": "약간 퓨전 사극",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100026028.1.6",
          "form": "홍천기인가 그거요?",
          "original_form": "홍천기인가 그거요?",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026028.1.7",
          "form": "아 그거요",
          "original_form": "아 그거요",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026028.1.8",
          "form": "2화는 못봤는데",
          "original_form": "2화는 못봤는데",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026028.1.9",
          "form": "1화는 애매",
          "original_form": "1화는 애매",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026028.1.10",
          "form": "저도 1화 약간보다말았어요 재밌다고하면 보려고요",
          "original_form": "저도 1화 약간보다말았어요 재밌다고하면 보려고요",
          "speaker_id": "2",
          "time": "20210901 19:47"
        },
        {
          "id": "MDRW2100026028.1.11",
          "form": "그러게요 요즘 이거다 하는 재밌는게 없네요",
          "original_form": "그러게요 요즘 이거다 하는 재밌는게 없네요",
          "speaker_id": "1",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100026028.1.12",
          "form": "예능은 뭐보세요",
          "original_form": "예능은 뭐보세요",
          "speaker_id": "2",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026028.1.13",
          "form": "토요일에 놀뭐 뉴스앵커보는 유재석님 봤어요",
          "original_form": "토요일에 놀뭐 뉴스앵커보는 유재석님 봤어요",
          "speaker_id": "1",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026028.1.14",
          "form": "저도잠깐봤어요 잘하시더라구요",
          "original_form": "저도잠깐봤어요 잘하시더라구요",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026028.1.15",
          "form": "낚시같긴했죠?",
          "original_form": "낚시같긴했죠?",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026028.1.16",
          "form": "네 런닝맨 보고싶었는데 깜빡하고 못봤어요ㅜ",
          "original_form": "네 런닝맨 보고싶었는데 깜빡하고 못봤어요ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026028.1.17",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026028.1.18",
          "form": "괜찮게봤는데",
          "original_form": "괜찮게봤는데",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026028.1.19",
