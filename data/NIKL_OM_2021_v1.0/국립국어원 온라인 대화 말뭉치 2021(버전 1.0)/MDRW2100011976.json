      "utterance": [
        {
          "id": "MDRW2100011976.1.1",
          "form": "아하 그렇군용",
          "original_form": "아하 그렇군용",
          "speaker_id": "2",
          "time": "20210824 18:00"
        },
        {
          "id": "MDRW2100011976.1.2",
          "form": "매콤하고 중독성있는 맛이에요",
          "original_form": "매콤하고 중독성있는 맛이에요",
          "speaker_id": "2",
          "time": "20210824 18:00"
        },
        {
          "id": "MDRW2100011976.1.3",
          "form": "맵찔이인데 그래도 맛있어요ㅎ ㅎ",
          "original_form": "맵찔이인데 그래도 맛있어요ㅎ ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.4",
          "form": "매콤하고 중독성있다라..",
          "original_form": "매콤하고 중독성있다라..",
          "speaker_id": "1",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.5",
          "form": "떡볶이같은거죠..",
          "original_form": "떡볶이같은거죠..",
          "speaker_id": "2",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.6",
          "form": "저도 분명 좋아할 것 같아요",
          "original_form": "저도 분명 좋아할 것 같아요",
          "speaker_id": "1",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.7",
          "form": "소울푸드..",
          "original_form": "소울푸드..",
          "speaker_id": "2",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.8",
          "form": "떡볶이 같은거라니",
          "original_form": "떡볶이 같은거라니",
          "speaker_id": "1",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.9",
          "form": "제 생각에도요!",
          "original_form": "제 생각에도요!",
          "speaker_id": "2",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.10",
          "form": "완벽하다",
          "original_form": "완벽하다",
          "speaker_id": "1",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.11",
          "form": "다음에 도전해보세요ㅎㅎㅎ",
          "original_form": "다음에 도전해보세요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.12",
          "form": "재료 많이 들어가던데",
          "original_form": "재료 많이 들어가던데",
          "speaker_id": "1",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.13",
          "form": "뭐 좋아하세요",
          "original_form": "뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.14",
          "form": "안에 재료요",
          "original_form": "안에 재료요",
          "speaker_id": "1",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.15",
          "form": "고기 꼭 넣고 유부랑 중국당면이랑",
          "original_form": "고기 꼭 넣고 유부랑 중국당면이랑",
          "speaker_id": "2",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011976.1.16",
          "form": "나머지는 그냥 적당히요ㅎㅎ",
          "original_form": "나머지는 그냥 적당히요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011976.1.17",
          "form": "그거면 됐어요",
          "original_form": "그거면 됐어요",
          "speaker_id": "1",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011976.1.18",
          "form": "고기 유부 당면",
          "original_form": "고기 유부 당면",
          "speaker_id": "1",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011976.1.19",
          "form": "ㅋㅋㅋㅋㄱㄲㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㄱㄲㅋ맞아요",
          "speaker_id": "2",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011976.1.20",
          "form": "뭐가 더 필요해",
          "original_form": "뭐가 더 필요해",
          "speaker_id": "1",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011976.1.21",
          "form": "그럼됐죠.",
          "original_form": "그럼됐죠.",
          "speaker_id": "2",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011976.1.22",
          "form": "완벽해 탄단지",
          "original_form": "완벽해 탄단지",
          "speaker_id": "1",
          "time": "20210824 18:02"
        }
      ]
