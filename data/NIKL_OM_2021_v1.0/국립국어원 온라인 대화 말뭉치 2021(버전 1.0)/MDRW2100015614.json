      "utterance": [
        {
          "id": "MDRW2100015614.1.1",
          "form": "넷플릭스 구독하시나요?",
          "original_form": "넷플릭스 구독하시나요?",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015614.1.2",
          "form": "지금은 안하는데 예전에 자주봤었어요",
          "original_form": "지금은 안하는데 예전에 자주봤었어요",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015614.1.3",
          "form": "요즘 추천해주실 작품 있으신가요?",
          "original_form": "요즘 추천해주실 작품 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015614.1.4",
          "form": "곧 기묘한 이야기 4가 나온다고 하더라구요",
          "original_form": "곧 기묘한 이야기 4가 나온다고 하더라구요",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015614.1.5",
          "form": "기묘한 이야기 재밌다고 들었어요",
          "original_form": "기묘한 이야기 재밌다고 들었어요",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015614.1.6",
          "form": "저는 다큐도 종류가 많아서 재밌더라구요",
          "original_form": "저는 다큐도 종류가 많아서 재밌더라구요",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015614.1.7",
          "form": "저도 다큐 좋아해요!",
          "original_form": "저도 다큐 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015614.1.8",
          "form": "동물 다큐도 재밌더라구요",
          "original_form": "동물 다큐도 재밌더라구요",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015614.1.9",
          "form": "동물관련 다큐들 좋아해요",
          "original_form": "동물관련 다큐들 좋아해요",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015614.1.10",
          "form": "스파이펭귄같은",
          "original_form": "스파이펭귄같은",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015614.1.11",
          "form": "요새 동물 다큐가 많이 올라왔나요?",
          "original_form": "요새 동물 다큐가 많이 올라왔나요?",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015614.1.12",
          "form": "스파이 펭귄 궁금하네요",
          "original_form": "스파이 펭귄 궁금하네요",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015614.1.13",
          "form": "펭귄들무리에 펭귄모양 로봇넣고 관찰하는 다큐에요",
          "original_form": "펭귄들무리에 펭귄모양 로봇넣고 관찰하는 다큐에요",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015614.1.14",
          "form": "아 움짤로 본 기억은 나요  무척귀여울것같아요",
          "original_form": "아 움짤로 본 기억은 나요  무척귀여울것같아요",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015614.1.15",
          "form": "매우 귀엽습니다 동물 다큐 좋아하시면 추천드립니다",
          "original_form": "매우 귀엽습니다 동물 다큐 좋아하시면 추천드립니다",
          "speaker_id": "2",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015614.1.16",
          "form": "ㅎㅎㅎ꼭 볼게요. 감사해요",
          "original_form": "ㅎㅎㅎ꼭 볼게요. 감사해요",
          "speaker_id": "1",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015614.1.17",
          "form": "넷플릭스를 한 번 다시 들어가봐야겠어요",
          "original_form": "넷플릭스를 한 번 다시 들어가봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 01:21"
        },
        {
          "id": "MDRW2100015614.1.18",
          "form": "넷플릭스에도 있는지는 모르겠어요 전 다른데서봐서",
          "original_form": "넷플릭스에도 있는지는 모르겠어요 전 다른데서봐서",
          "speaker_id": "2",
          "time": "20210826 01:26"
        }
      ]
    }
