      "utterance": [
        {
          "id": "MDRW2100010529.1.1",
          "form": "책 좋아하세용?",
          "original_form": "책 좋아하세용?",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100010529.1.2",
          "form": "네 시간 있을 때마다 틈틈히 봐요!",
          "original_form": "네 시간 있을 때마다 틈틈히 봐요!",
          "speaker_id": "2",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100010529.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100010529.1.4",
          "form": "오 저는 요즘사놓고 안읽는중요 ..",
          "original_form": "오 저는 요즘사놓고 안읽는중요 ..",
          "speaker_id": "1",
          "time": "20210823 20:01"
        },
        {
          "id": "MDRW2100010529.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:01"
        },
        {
          "id": "MDRW2100010529.1.6",
          "form": "저도 사둔 거 자주 방치해요ㅋㅋㅋ",
          "original_form": "저도 사둔 거 자주 방치해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010529.1.7",
          "form": "한번 꽂히면 재밌게보는데 ..",
          "original_form": "한번 꽂히면 재밌게보는데 ..",
          "speaker_id": "1",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010529.1.8",
          "form": "요즘 왤케 다귀찮나요 ㅜㅜ흑",
          "original_form": "요즘 왤케 다귀찮나요 ㅜㅜ흑",
          "speaker_id": "1",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010529.1.9",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010529.1.10",
          "form": "한 번 집중하면 쭈욱 보는데",
          "original_form": "한 번 집중하면 쭈욱 보는데",
          "speaker_id": "2",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010529.1.11",
          "form": "요즘 집중이 잘 안 돼요",
          "original_form": "요즘 집중이 잘 안 돼요",
          "speaker_id": "2",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010529.1.12",
          "form": "전 요즘 웹툰에 빠졋어요 ..",
          "original_form": "전 요즘 웹툰에 빠졋어요 ..",
          "speaker_id": "1",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010529.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010529.1.14",
          "form": "원래도 만화책 엄청좋아햇거든요 ㅋㅋ",
          "original_form": "원래도 만화책 엄청좋아햇거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010529.1.15",
          "form": "저도 종이책 보다 웹툰이나 웹소설이 이제 익숙하더라고요",
          "original_form": "저도 종이책 보다 웹툰이나 웹소설이 이제 익숙하더라고요",
          "speaker_id": "2",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010529.1.16",
          "form": "웹툰 재밌게 보시는 거 있으세요?",
          "original_form": "웹툰 재밌게 보시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010529.1.17",
          "form": "너무 많이봐서요 ㅠㅠㅋㅋ 네이버웹툰만 봐용 인기순위는 거의다보는듯용",
          "original_form": "너무 많이봐서요 ㅠㅠㅋㅋ 네이버웹툰만 봐용 인기순위는 거의다보는듯용",
          "speaker_id": "1",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010529.1.18",
          "form": "아 저도 거의 요일 별로 1~2개 씩은 보는 거 같아요",
          "original_form": "아 저도 거의 요일 별로 1~2개 씩은 보는 거 같아요",
          "speaker_id": "2",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010529.1.19",
          "form": "재밌는 작품이 너무 많죠",
          "original_form": "재밌는 작품이 너무 많죠",
          "speaker_id": "2",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010529.1.20",
          "form": "ㅋㅋㅋㅋ 책은 어떤류 좋아하세요? 전 소설 좋아해요",
          "original_form": "ㅋㅋㅋㅋ 책은 어떤류 좋아하세요? 전 소설 좋아해요",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010529.1.21",
          "form": "소설 좋아하는데 최근에는 거의 안 읽었어요",
          "original_form": "소설 좋아하는데 최근에는 거의 안 읽었어요",
          "speaker_id": "2",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010529.1.22",
          "form": "최근에는 자연과학이랑 인문학 도서 읽은 거 같아요",
          "original_form": "최근에는 자연과학이랑 인문학 도서 읽은 거 같아요",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010529.1.23",
          "form": "채밌게 본 소설 있으세요?",
          "original_form": "채밌게 본 소설 있으세요?",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010529.1.24",
          "form": "재밌게 본!",
          "original_form": "재밌게 본!",
          "speaker_id": "2",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010529.1.25",
          "form": "달러구트 꿈백화점이용",
          "original_form": "달러구트 꿈백화점이용",
          "speaker_id": "1",
          "time": "20210823 20:31"
        }
      ]
    }
