        },
        {
          "id": "MDRW2100020169.1.2",
          "form": "프로듀스 원오원 시즌 투 봤어?",
          "original_form": "프로듀스 원오원 시즌 투 봤어?",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020169.1.3",
          "form": "당연히 봤지 내 친구들 중에 그거 안 본 사람 없어",
          "original_form": "당연히 봤지 내 친구들 중에 그거 안 본 사람 없어",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020169.1.4",
          "form": "너도 워너원 좋아했어?",
          "original_form": "너도 워너원 좋아했어?",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020169.1.5",
          "form": "나도 좋아했어",
          "original_form": "나도 좋아했어",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020169.1.6",
          "form": "어떤 멤버 제일 좋아했어요",
          "original_form": "어떤 멤버 제일 좋아했어요",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020169.1.7",
          "form": "나는 열한 명을 모두 좋아했지",
          "original_form": "나는 열한 명을 모두 좋아했지",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020169.1.8",
          "form": "인정한 워너비 네",
          "original_form": "인정한 워너비 네",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020169.1.9",
          "form": "진정한 팬을 자세라 할 수 있지",
          "original_form": "진정한 팬을 자세라 할 수 있지",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020169.1.10",
          "form": "올 팽이 제일 좋긴 해",
          "original_form": "올 팽이 제일 좋긴 해",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020169.1.11",
          "form": "올팬",
          "original_form": "올팬",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020169.1.12",
          "form": "그래서 지금도 가끔 워너원 근황을 찾아봐",
          "original_form": "그래서 지금도 가끔 워너원 근황을 찾아봐",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020169.1.13",
          "form": "에너제틱 노래가 제일 재밌긴 해",
          "original_form": "에너제틱 노래가 제일 재밌긴 해",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020169.1.14",
          "form": "박지훈은 박지훈은 가끔 연기를 해",
          "original_form": "박지훈은 박지훈은 가끔 연기를 해",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020169.1.15",
          "form": "요즘 배진영은 뭐해",
          "original_form": "요즘 배진영은 뭐해",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020169.1.16",
          "form": "연예계 활동 잘 할 거야",
          "original_form": "연예계 활동 잘 할 거야",
          "speaker_id": "1",
