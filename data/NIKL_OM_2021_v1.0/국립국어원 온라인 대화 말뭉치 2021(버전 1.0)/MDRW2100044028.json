      "utterance": [
        {
          "id": "MDRW2100044028.1.1",
          "form": "옆집은 상호명이 뭔가요?",
          "original_form": "옆집은 상호명이 뭔가요?",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044028.1.2",
          "form": "중앙이래요ㅋㅋ",
          "original_form": "중앙이래요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044028.1.3",
          "form": "아아 맞아요 중앙도",
          "original_form": "아아 맞아요 중앙도",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044028.1.4",
          "form": "유명하다고 들은 것 같아요",
          "original_form": "유명하다고 들은 것 같아요",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044028.1.5",
          "form": "뭐든 기대는 하지말고 드셔야 맛나요!",
          "original_form": "뭐든 기대는 하지말고 드셔야 맛나요!",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044028.1.6",
          "form": "근데 중앙도 강릉에 있나요??",
          "original_form": "근데 중앙도 강릉에 있나요??",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044028.1.7",
          "form": "ㅋㅋ속초에 있다는데 평이 갈리네요",
          "original_form": "ㅋㅋ속초에 있다는데 평이 갈리네요",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044028.1.8",
          "form": "저는 거의 십년전쯤 ㅋㅋ 만석 먹었어요ㅋㅋ",
          "original_form": "저는 거의 십년전쯤 ㅋㅋ 만석 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044028.1.9",
          "form": "근데 닭강정이 맛없긴 어렵죠?? ㅋㅋ",
          "original_form": "근데 닭강정이 맛없긴 어렵죠?? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044028.1.10",
          "form": "아니에요..",
          "original_form": "아니에요..",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044028.1.11",
          "form": "맛없을수가 있더라고요. .. ..",
          "original_form": "맛없을수가 있더라고요. .. ..",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044028.1.12",
          "form": "정말 .매우...",
          "original_form": "정말 .매우...",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044028.1.13",
          "form": "놀랍게도.....",
          "original_form": "놀랍게도.....",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044028.1.14",
          "form": "ㅋㅋ 하긴.. 그런데 바닷가를 헤매고 물놀이 하고 드심 맛있을거예요ㅋㅋ",
          "original_form": "ㅋㅋ 하긴.. 그런데 바닷가를 헤매고 물놀이 하고 드심 맛있을거예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044028.1.15",
          "form": "지금 날씨엔",
          "original_form": "지금 날씨엔",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044028.1.16",
          "form": "물놀이는 안될것같아요ㅠㅠㅠ",
          "original_form": "물놀이는 안될것같아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044028.1.17",
          "form": "중앙 뻑뻑하다는 평도 있고 현지인은 중앙만 간다하고ㅋㅋ",
          "original_form": "중앙 뻑뻑하다는 평도 있고 현지인은 중앙만 간다하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044028.1.18",
          "form": "전 강릉에서 짬뽕순두부먹었어요ㅋㅋ",
          "original_form": "전 강릉에서 짬뽕순두부먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044028.1.19",
          "form": "오 맛있으셨나요?",
          "original_form": "오 맛있으셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044028.1.20",
          "form": "맛있었는데ㅋㅋ 한시간인가 기다렸다 먹은거라... ㅋㅋ",
          "original_form": "맛있었는데ㅋㅋ 한시간인가 기다렸다 먹은거라... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044028.1.21",
          "form": "기다림이ㅋㅋ",
          "original_form": "기다림이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044028.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044028.1.23",
          "form": "유명한 집에서 드셖나봐요",
          "original_form": "유명한 집에서 드셖나봐요",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044028.1.24",
          "form": "그래두 맛있으면 됐죠!!",
          "original_form": "그래두 맛있으면 됐죠!!",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044028.1.25",
          "form": "네 가장 유명한 곳으로ㅋㅋ",
          "original_form": "네 가장 유명한 곳으로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044028.1.26",
          "form": "그옆에 두부 아이스크림도 팔아서 대기하면서 먹음 돼요ㅋㅋ",
          "original_form": "그옆에 두부 아이스크림도 팔아서 대기하면서 먹음 돼요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044028.1.27",
          "form": "앗 맞아요 강릉",
          "original_form": "앗 맞아요 강릉",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044028.1.28",
          "form": "두부젤라또도 유명하더라고요! 그거드신건가요?",
          "original_form": "두부젤라또도 유명하더라고요! 그거드신건가요?",
          "speaker_id": "1",
          "time": "20210907 21:41"
        }
      ]
