      "utterance": [
        {
          "id": "MDRW2100039393.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210903 17:06"
        },
        {
          "id": "MDRW2100039393.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100039393.1.3",
          "form": "게임 좋아하시나 봐요",
          "original_form": "게임 좋아하시나 봐요",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100039393.1.4",
          "form": "게임 엄청 좋아하지는 않고 게임 방송 보는 걸 주로 좋아합니다",
          "original_form": "게임 엄청 좋아하지는 않고 게임 방송 보는 걸 주로 좋아합니다",
          "speaker_id": "1",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039393.1.5",
          "form": "저는 게임을 하지는 않고",
          "original_form": "저는 게임을 하지는 않고",
          "speaker_id": "2",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100039393.1.6",
          "form": "제 아들이 여덟 살인데",
          "original_form": "제 아들이 여덟 살인데",
          "speaker_id": "2",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100039393.1.7",
          "form": "요즘 게임을 엄청 좋아하더라구요",
          "original_form": "요즘 게임을 엄청 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100039393.1.8",
          "form": "어떤 게임이 요즘 인기가 많나요? 아이들이 게임을 곧잘 잘하더라구요",
          "original_form": "어떤 게임이 요즘 인기가 많나요? 아이들이 게임을 곧잘 잘하더라구요",
          "speaker_id": "1",
          "time": "20210903 17:15"
        },
        {
          "id": "MDRW2100039393.1.9",
          "form": "무한의계단 제일 좋아해요",
          "original_form": "무한의계단 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100039393.1.10",
          "form": "브롤스타즈도 좋아하구요",
          "original_form": "브롤스타즈도 좋아하구요",
          "speaker_id": "2",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100039393.1.11",
          "form": "와 다양한 게임이 많군요",
          "original_form": "와 다양한 게임이 많군요",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100039393.1.12",
          "form": "다 모바일 게임인가요?",
          "original_form": "다 모바일 게임인가요?",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100039393.1.13",
          "form": "네 ㅎㅎ 모바일이요",
          "original_form": "네 ㅎㅎ 모바일이요",
          "speaker_id": "2",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100039393.1.14",
          "form": "폰으로만 게임을 해서",
          "original_form": "폰으로만 게임을 해서",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039393.1.15",
          "form": "컴퓨터로는 안하구요",
          "original_form": "컴퓨터로는 안하구요",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039393.1.16",
          "form": "저도 모바일 게임 하기는 하는데  진득하게 하는 성격이 아니라 일찍 그만둬요ㅠㅠㅎㅎ",
          "original_form": "저도 모바일 게임 하기는 하는데  진득하게 하는 성격이 아니라 일찍 그만둬요ㅠㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039393.1.17",
          "form": "맞아요 요즘 다들 모바일 게임 하죠",
          "original_form": "맞아요 요즘 다들 모바일 게임 하죠",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039393.1.18",
          "form": "네 ㅎㅎ정말 열심히 하더라구요",
          "original_form": "네 ㅎㅎ정말 열심히 하더라구요",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039393.1.19",
          "form": "유튜브 도 게임 유튜브를 보고",
          "original_form": "유튜브 도 게임 유튜브를 보고",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039393.1.20",
          "form": "저는 게임 유툽을 좋아해서 즐겨봐요",
          "original_form": "저는 게임 유툽을 좋아해서 즐겨봐요",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039393.1.21",
          "form": "뭔가 저는 잘 못해서 남이 하는거 보면 재밌더라구요ㅎ",
          "original_form": "뭔가 저는 잘 못해서 남이 하는거 보면 재밌더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039393.1.22",
          "form": "ㅎㅎ 저희애도",
          "original_form": "ㅎㅎ 저희애도",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100039393.1.23",
          "form": "잘하지는 못해서",
          "original_form": "잘하지는 못해서",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100039393.1.24",
          "form": "게임 유투 버가 하는 거 보면 재밌어 보이나 보더라구요",
          "original_form": "게임 유투 버가 하는 거 보면 재밌어 보이나 보더라구요",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100039393.1.25",
          "form": "스무 살 되면 꿈이 현 질하는 거예요",
          "original_form": "스무 살 되면 꿈이 현 질하는 거예요",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100039393.1.26",
          "form": "너무 웃기더라구요",
          "original_form": "너무 웃기더라구요",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100039393.1.27",
          "form": "앗 너무 귀여운 꿈이에요ㅎㅎ",
          "original_form": "앗 너무 귀여운 꿈이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039393.1.28",
          "form": "몰래 현질하는 아이들도 있는데 넘넘 착한 아이같아요",
          "original_form": "몰래 현질하는 아이들도 있는데 넘넘 착한 아이같아요",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039393.1.29",
          "form": "아직 8살이라 ㅎㅎ 할줄을 몰라요 \r\n다 막아놔서 ㅎㅎ",
          "original_form": "아직 8살이라 ㅎㅎ 할줄을 몰라요 \r\n다 막아놔서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039393.1.30",
          "form": "스무 살 되면 군대 가야 되냐 길래",
          "original_form": "스무 살 되면 군대 가야 되냐 길래",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039393.1.31",
          "form": "군대에 가야 된다고 했더니",
          "original_form": "군대에 가야 된다고 했더니",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039393.1.32",
          "form": "군대 가면 월급 주냐고",
          "original_form": "군대 가면 월급 주냐고",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039393.1.33",
          "form": "훈련하면 훈련비 나온다고 했더니",
          "original_form": "훈련하면 훈련비 나온다고 했더니",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039393.1.34",
          "form": "그럼 그 돈으로 현 질해야지 하더라구요",
          "original_form": "그럼 그 돈으로 현 질해야지 하더라구요",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039393.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039393.1.36",
          "form": "앗ㅋㅋㅋ넘 귀여운ㅠㅠㅠㅠ아ㅠㅠ귀여워ㅠㅠㅠ",
          "original_form": "앗ㅋㅋㅋ넘 귀여운ㅠㅠㅠㅠ아ㅠㅠ귀여워ㅠㅠㅠ",
