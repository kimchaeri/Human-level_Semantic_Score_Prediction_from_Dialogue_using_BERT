      "utterance": [
        {
          "id": "MDRW2100032794.1.1",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "1",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032794.1.2",
          "form": "헬로",
          "original_form": "헬로",
          "speaker_id": "2",
          "time": "20210905 23:47"
        },
        {
          "id": "MDRW2100032794.1.3",
          "form": "저녁 먹었제?",
          "original_form": "저녁 먹었제?",
          "speaker_id": "2",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100032794.1.4",
          "form": "저녁 비스무리하게 먹었지..",
          "original_form": "저녁 비스무리하게 먹었지..",
          "speaker_id": "1",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032794.1.5",
          "form": "먹으면 먹은거고 안먹으면 안먹은거지 ㅋㅋ\n비스므리는 모꼬 ㅋㅋ",
          "original_form": "먹으면 먹은거고 안먹으면 안먹은거지 ㅋㅋ\n비스므리는 모꼬 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032794.1.6",
          "form": "시장가서 튀김사와서 먹고 끝",
          "original_form": "시장가서 튀김사와서 먹고 끝",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032794.1.7",
          "form": "그걸로 밥이 되나???\n난 밥먹고 과자 먹고 .. 좀전에 커피도 사뭇는데..",
          "original_form": "그걸로 밥이 되나???\n난 밥먹고 과자 먹고 .. 좀전에 커피도 사뭇는데..",
          "speaker_id": "2",
          "time": "20210906 00:03"
        },
        {
          "id": "MDRW2100032794.1.8",
          "form": "점심을4시에먹음 ㅎㅎ",
          "original_form": "점심을4시에먹음 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 00:04"
        },
        {
          "id": "MDRW2100032794.1.9",
          "form": "그럼 저녁을 몇시에 먹었단 말이고?",
          "original_form": "그럼 저녁을 몇시에 먹었단 말이고?",
          "speaker_id": "2",
          "time": "20210906 00:05"
        },
        {
          "id": "MDRW2100032794.1.10",
          "form": "그니까 저녁을 안먹고 튀김만 사서 ㅋㅋ",
          "original_form": "그니까 저녁을 안먹고 튀김만 사서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:05"
        },
        {
          "id": "MDRW2100032794.1.11",
          "form": "그 튀김을 몇시에 묵은건딩.\n무슨 튀김 먹었어?",
          "original_form": "그 튀김을 몇시에 묵은건딩.\n무슨 튀김 먹었어?",
          "speaker_id": "2",
          "time": "20210906 00:09"
        },
        {
          "id": "MDRW2100032794.1.12",
          "form": "고.추.튀김ㅋㅋ",
          "original_form": "고.추.튀김ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:10"
        },
        {
          "id": "MDRW2100032794.1.13",
          "form": "튀김은..한8시?",
          "original_form": "튀김은..한8시?",
          "speaker_id": "1",
          "time": "20210906 00:10"
        },
        {
          "id": "MDRW2100032794.1.14",
          "form": "월배시장에 위에 떡볶이집 튀김 맛있도라",
          "original_form": "월배시장에 위에 떡볶이집 튀김 맛있도라",
          "speaker_id": "1",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100032794.1.15",
          "form": "위에가 어디쪽인데?\n아파트쪽?\n입구쪽?`",
          "original_form": "위에가 어디쪽인데?\n아파트쪽?\n입구쪽?`",
          "speaker_id": "2",
          "time": "20210906 00:18"
        },
        {
          "id": "MDRW2100032794.1.16",
          "form": "입구쪽ㅋㅋ 빨간오뎅에서 조금만 밑으로",
          "original_form": "입구쪽ㅋㅋ 빨간오뎅에서 조금만 밑으로",
          "speaker_id": "1",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100032794.1.17",
          "form": "아.. 떡볶이 또 먹고 싶노 ㅋㅋㅋㅋ",
          "original_form": "아.. 떡볶이 또 먹고 싶노 ㅋㅋㅋㅋ",
