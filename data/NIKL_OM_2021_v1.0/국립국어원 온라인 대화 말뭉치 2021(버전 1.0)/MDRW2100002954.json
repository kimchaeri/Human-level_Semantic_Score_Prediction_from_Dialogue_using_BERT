        },
        {
          "id": "MDRW2100002954.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002954.1.3",
          "form": "연애중이신가요~",
          "original_form": "연애중이신가요~",
          "speaker_id": "1",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002954.1.4",
          "form": "음 아뇨? name1님은요?",
          "original_form": "음 아뇨? &name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002954.1.5",
          "form": "아하 저는 연애중이에요~",
          "original_form": "아하 저는 연애중이에요~",
          "speaker_id": "1",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002954.1.6",
          "form": "아하 연애중이시구나 어때요?",
          "original_form": "아하 연애중이시구나 어때요?",
          "speaker_id": "2",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002954.1.7",
          "form": "이상형이 어떻게 되시나요?",
          "original_form": "이상형이 어떻게 되시나요?",
          "speaker_id": "1",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002954.1.8",
          "form": "음 내면이 단단한 사람이요",
          "original_form": "음 내면이 단단한 사람이요",
          "speaker_id": "2",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002954.1.9",
          "form": "저는 3년반째 연애중",
          "original_form": "저는 3년반째 연애중",
          "speaker_id": "1",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002954.1.10",
          "form": "오.. 대단하세요",
          "original_form": "오.. 대단하세요",
          "speaker_id": "2",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002954.1.11",
          "form": "저는 뭔가 사람을 오래 못만나서",
          "original_form": "저는 뭔가 사람을 오래 못만나서",
          "speaker_id": "2",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002954.1.12",
          "form": "그런가요 ㅜ.ㅜ",
          "original_form": "그런가요 ㅜ.ㅜ",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002954.1.13",
          "form": "이상순같은 사람이 좋은거같아요~",
          "original_form": "이상순같은 사람이 좋은거같아요~",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002954.1.14",
          "form": "아 어떤 느낌일지 알거같아요",
          "original_form": "아 어떤 느낌일지 알거같아요",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002954.1.15",
          "form": "안정을 주는 사람",
          "original_form": "안정을 주는 사람",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002954.1.16",
          "form": "이상순은 부모님이 싸우는걸 본적이 없대요",
          "original_form": "이상순은 부모님이 싸우는걸 본적이 없대요",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002954.1.17",
          "form": "좋은 부모님이시네요",
          "original_form": "좋은 부모님이시네요",
          "speaker_id": "2",
          "time": "20210820 17:05"
