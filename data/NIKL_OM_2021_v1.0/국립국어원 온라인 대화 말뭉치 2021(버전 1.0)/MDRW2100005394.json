      "utterance": [
        {
          "id": "MDRW2100005394.1.1",
          "form": "오늘 알바 어땠어?",
          "original_form": "오늘 알바 어땠어?",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005394.1.2",
          "form": "원래 가게 마감하고 저녁 시켜먹는데",
          "original_form": "원래 가게 마감하고 저녁 시켜먹는데",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005394.1.3",
          "form": "저녁 생각 없다니까 조기 퇴근 당했어",
          "original_form": "저녁 생각 없다니까 조기 퇴근 당했어",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005394.1.4",
          "form": "앗ㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005394.1.5",
          "form": "나도 다음주부터 다시 2시간만 수업한다",
          "original_form": "나도 다음주부터 다시 2시간만 수업한다",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005394.1.6",
          "form": "벌써 개학이더라고",
          "original_form": "벌써 개학이더라고",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005394.1.7",
          "form": "헐 대박 ㅠ",
          "original_form": "헐 대박 ㅠ",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005394.1.8",
          "form": "수강신청도 끝난 겸",
          "original_form": "수강신청도 끝난 겸",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005394.1.9",
          "form": "알바 구해야 되는디",
          "original_form": "알바 구해야 되는디",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005394.1.10",
          "form": "어머님 도와드리는 건 방학 때만 해?",
          "original_form": "어머님 도와드리는 건 방학 때만 해?",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005394.1.11",
          "form": "응 그렇게 말했었는데",
          "original_form": "응 그렇게 말했었는데",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005394.1.12",
          "form": "개강하고는 어찌 될지 모르겠네",
          "original_form": "개강하고는 어찌 될지 모르겠네",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005394.1.13",
          "form": "주말 알바 자리 있으면 좋을 거 같은데",
          "original_form": "주말 알바 자리 있으면 좋을 거 같은데",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005394.1.14",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005394.1.15",
          "form": "아니면 수목",
          "original_form": "아니면 수목",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005394.1.16",
          "form": "다 녹강이라",
          "original_form": "다 녹강이라",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005394.1.17",
          "form": "실시간 화상강의 있는지 잘 봐...",
          "original_form": "실시간 화상강의 있는지 잘 봐...",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005394.1.18",
          "form": "나 뒤통수 맞은 강의 있음",
          "original_form": "나 뒤통수 맞은 강의 있음",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005394.1.19",
          "form": "오마갓...",
          "original_form": "오마갓...",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005394.1.20",
          "form": "강의 계획서 꼭 찾아봐...",
          "original_form": "강의 계획서 꼭 찾아봐...",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005394.1.21",
          "form": "어제 봤을 때는 다 괜찮았던 거 같은데",
          "original_form": "어제 봤을 때는 다 괜찮았던 거 같은데",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005394.1.22",
          "form": "아 모르겠다...",
          "original_form": "아 모르겠다...",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005394.1.23",
          "form": "23일이랑 정정 때",
          "original_form": "23일이랑 정정 때",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005394.1.24",
          "form": "또 바꾸거나 교양 잡으면",
          "original_form": "또 바꾸거나 교양 잡으면",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005394.1.25",
          "form": "달라지니께",
          "original_form": "달라지니께",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005394.1.26",
          "form": "지금 주택 청약 알아보는중... 가계부도 정리하고",
          "original_form": "지금 주택 청약 알아보는중... 가계부도 정리하고",
          "speaker_id": "2",
          "time": "20210829 20:55"
        }
      ]
    }
  ]
