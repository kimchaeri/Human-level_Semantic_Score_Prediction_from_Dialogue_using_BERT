      "utterance": [
        {
          "id": "MDRW2100010092.1.1",
          "form": "강아지 키우세요?",
          "original_form": "강아지 키우세요?",
          "speaker_id": "1",
          "time": "20210823 14:11"
        },
        {
          "id": "MDRW2100010092.1.2",
          "form": "아니요ㅠㅠ 키우곤싶은데 아직 여건이 안되네요ㅠㅠ",
          "original_form": "아니요ㅠㅠ 키우곤싶은데 아직 여건이 안되네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100010092.1.3",
          "form": "강아지 키우세요?",
          "original_form": "강아지 키우세요?",
          "speaker_id": "2",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100010092.1.4",
          "form": "아뇨!! 저는 고양이 키워요!",
          "original_form": "아뇨!! 저는 고양이 키워요!",
          "speaker_id": "1",
          "time": "20210823 14:13"
        },
        {
          "id": "MDRW2100010092.1.5",
          "form": "헉 고양이라니ㅠㅠ",
          "original_form": "헉 고양이라니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 14:13"
        },
        {
          "id": "MDRW2100010092.1.6",
          "form": "집앞에 계속 울고있어서 밥주고하다보니...",
          "original_form": "집앞에 계속 울고있어서 밥주고하다보니...",
          "speaker_id": "1",
          "time": "20210823 14:13"
        },
        {
          "id": "MDRW2100010092.1.7",
          "form": "어느새 제가 키우고있네요ㅋㅋ",
          "original_form": "어느새 제가 키우고있네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100010092.1.8",
          "form": "헐 그 유명한 간택...",
          "original_form": "헐 그 유명한 간택...",
          "speaker_id": "2",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100010092.1.9",
          "form": "엄청 개냥이라 귀여워요 제보물 ㅠㅠ",
          "original_form": "엄청 개냥이라 귀여워요 제보물 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100010092.1.10",
          "form": "너무 부럽습니다...",
          "original_form": "너무 부럽습니다...",
          "speaker_id": "2",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100010092.1.11",
          "form": "사실 냥줍안했으면 유기견입양할까했는데 어쩌다보니 지금 고양이를 키우고있네요ㅋㅋ",
          "original_form": "사실 냥줍안했으면 유기견입양할까했는데 어쩌다보니 지금 고양이를 키우고있네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100010092.1.12",
          "form": "유기견입양 너무 힘들지않나요? 절차가 복잡하다던데",
          "original_form": "유기견입양 너무 힘들지않나요? 절차가 복잡하다던데",
          "speaker_id": "2",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100010092.1.13",
          "form": "혼자 살면 더 안해준다고하더라고요",
          "original_form": "혼자 살면 더 안해준다고하더라고요",
          "speaker_id": "2",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100010092.1.14",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100010092.1.15",
          "form": "자취하는 사람은 제2보호자가 없으니까 힘들대요",
          "original_form": "자취하는 사람은 제2보호자가 없으니까 힘들대요",
          "speaker_id": "1",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100010092.1.16",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100010092.1.17",
          "form": "고양이 알러지는 없으셨나요?",
          "original_form": "고양이 알러지는 없으셨나요?",
          "speaker_id": "2",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010092.1.18",
          "form": "전 그것도 걱정이에요 막상 키웠는데 알러지있을까봐",
          "original_form": "전 그것도 걱정이에요 막상 키웠는데 알러지있을까봐",
          "speaker_id": "2",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010092.1.19",
          "form": "제가 바로 그래요ㅋㅋ",
          "original_form": "제가 바로 그래요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010092.1.20",
          "form": "고양이 알러지있어서 약먹고 병원다니다보니",
          "original_form": "고양이 알러지있어서 약먹고 병원다니다보니",
          "speaker_id": "1",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010092.1.21",
          "form": "이제 괜찮더라구요",
          "original_form": "이제 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010092.1.22",
          "form": "정말 사랑으로 키우시네요...",
          "original_form": "정말 사랑으로 키우시네요...",
          "speaker_id": "2",
          "time": "20210823 14:18"
        }
      ]
    }
