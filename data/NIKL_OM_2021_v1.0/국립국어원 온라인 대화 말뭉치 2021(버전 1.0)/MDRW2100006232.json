        },
        {
          "id": "MDRW2100006232.1.2",
          "form": "여행지 어디가 좋나요?",
          "original_form": "여행지 어디가 좋나요?",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006232.1.3",
          "form": "스위스요",
          "original_form": "스위스요",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006232.1.4",
          "form": "추천해주세요",
          "original_form": "추천해주세요",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006232.1.5",
          "form": "가보셨어요",
          "original_form": "가보셨어요",
          "speaker_id": "1",
