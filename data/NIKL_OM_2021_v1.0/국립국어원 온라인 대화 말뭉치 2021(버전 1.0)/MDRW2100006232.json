        },
        {
          "id": "MDRW2100006232.1.6",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006232.1.7",
          "form": "베트남도",
          "original_form": "베트남도",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006232.1.8",
          "form": "네..가봤네요",
          "original_form": "네..가봤네요",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006232.1.9",
          "form": "오 저는 비행기라고는 제주도밖에",
          "original_form": "오 저는 비행기라고는 제주도밖에",
