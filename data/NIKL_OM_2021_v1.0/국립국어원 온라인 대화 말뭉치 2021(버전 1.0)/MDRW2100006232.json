          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006232.1.10",
          "form": "안터봤어요",
          "original_form": "안터봤어요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006232.1.11",
          "form": "아..그렇군요",
          "original_form": "아..그렇군요",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006232.1.12",
          "form": "저도 해외 가보고 싶네요",
          "original_form": "저도 해외 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006232.1.13",
          "form": "나중에 가보세요..",
