      "utterance": [
        {
          "id": "MDRW2100046822.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20211104 20:44"
        },
        {
          "id": "MDRW2100046822.1.2",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 20:58"
        },
        {
          "id": "MDRW2100046822.1.3",
          "form": "운동 좋아하세용",
          "original_form": "운동 좋아하세용",
          "speaker_id": "2",
          "time": "20211104 20:59"
        },
        {
          "id": "MDRW2100046822.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20211104 20:59"
        },
        {
          "id": "MDRW2100046822.1.5",
          "form": "아주 좋아하진않구요 건강생각해서 조금씩은 하는편입니다",
          "original_form": "아주 좋아하진않구요 건강생각해서 조금씩은 하는편입니다",
          "speaker_id": "1",
          "time": "20211104 21:06"
        },
        {
          "id": "MDRW2100046822.1.6",
          "form": "그 정도면 충분하죠 ㅎㅎ",
          "original_form": "그 정도면 충분하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:06"
        },
        {
          "id": "MDRW2100046822.1.7",
          "form": "어떤 운동 하시나요!",
          "original_form": "어떤 운동 하시나요!",
          "speaker_id": "2",
          "time": "20211104 21:06"
        },
        {
          "id": "MDRW2100046822.1.8",
          "form": "음.. 그냥 집에서 무게조절되는 아령으로 이두운동하구요\r\n매트에서 플랭크 정도 하는것같습니자",
          "original_form": "음.. 그냥 집에서 무게조절되는 아령으로 이두운동하구요\r\n매트에서 플랭크 정도 하는것같습니자",
          "speaker_id": "1",
          "time": "20211104 21:07"
        },
        {
          "id": "MDRW2100046822.1.9",
          "form": "다",
          "original_form": "다",
          "speaker_id": "1",
          "time": "20211104 21:07"
        },
        {
          "id": "MDRW2100046822.1.10",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20211104 21:08"
        },
        {
          "id": "MDRW2100046822.1.11",
          "form": "전 진짜 플랭크가 너무 싫어요....",
          "original_form": "전 진짜 플랭크가 너무 싫어요....",
          "speaker_id": "2",
          "time": "20211104 21:09"
        },
        {
          "id": "MDRW2100046822.1.12",
          "form": "플랭크 몇 분 정도까지 가능하신가요 ㅎㅎ",
          "original_form": "플랭크 몇 분 정도까지 가능하신가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:09"
        },
        {
          "id": "MDRW2100046822.1.13",
          "form": "저도 싫긴한데요 말그대로 건강생각해서 합니다",
          "original_form": "저도 싫긴한데요 말그대로 건강생각해서 합니다",
          "speaker_id": "1",
          "time": "20211104 21:09"
        },
        {
          "id": "MDRW2100046822.1.14",
          "form": "몇분까진 아니고 길어야 1~2분하고 쉬고 그런식으로요",
          "original_form": "몇분까진 아니고 길어야 1~2분하고 쉬고 그런식으로요",
          "speaker_id": "1",
          "time": "20211104 21:10"
        },
        {
          "id": "MDRW2100046822.1.15",
          "form": "2분이라니",
          "original_form": "2분이라니",
          "speaker_id": "2",
          "time": "20211104 21:10"
        },
        {
          "id": "MDRW2100046822.1.16",
          "form": "전 30초도 못버티고 픽 쓰러지는데 ㅎㅎㅎ",
          "original_form": "전 30초도 못버티고 픽 쓰러지는데 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:11"
        },
        {
          "id": "MDRW2100046822.1.17",
          "form": "대단하세용",
          "original_form": "대단하세용",
          "speaker_id": "2",
          "time": "20211104 21:11"
        },
        {
          "id": "MDRW2100046822.1.18",
          "form": "아뇨아뇨 저도 최대로해봐야 그정도이고 부들부들떨면서 간신히 하는정도입니다",
          "original_form": "아뇨아뇨 저도 최대로해봐야 그정도이고 부들부들떨면서 간신히 하는정도입니다",
          "speaker_id": "1",
          "time": "20211104 21:11"
        },
        {
          "id": "MDRW2100046822.1.19",
          "form": "ㅎㅎ 겸손하시네용",
          "original_form": "ㅎㅎ 겸손하시네용",
          "speaker_id": "2",
          "time": "20211104 21:11"
        },
        {
          "id": "MDRW2100046822.1.20",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20211104 21:12"
        },
        {
          "id": "MDRW2100046822.1.21",
          "form": "코어 근육이 참 중요하다고 하더라구요 ㅜ",
          "original_form": "코어 근육이 참 중요하다고 하더라구요 ㅜ",
          "speaker_id": "2",
          "time": "20211104 21:12"
        },
        {
          "id": "MDRW2100046822.1.22",
          "form": "네 맞습니다 이게 하나씩 아프기시작하면 끝도없는것같아요",
          "original_form": "네 맞습니다 이게 하나씩 아프기시작하면 끝도없는것같아요",
          "speaker_id": "1",
          "time": "20211104 21:12"
        },
        {
          "id": "MDRW2100046822.1.23",
          "form": "맞아요 ㅜㅜ 건강이 최고의 자산인데",
          "original_form": "맞아요 ㅜㅜ 건강이 최고의 자산인데",
          "speaker_id": "2",
          "time": "20211104 21:14"
        },
        {
          "id": "MDRW2100046822.1.24",
          "form": "저도 님처럼",
          "original_form": "저도 님처럼",
          "speaker_id": "2",
          "time": "20211104 21:14"
        },
        {
          "id": "MDRW2100046822.1.25",
          "form": "조금씩이라도",
          "original_form": "조금씩이라도",
          "speaker_id": "2",
          "time": "20211104 21:14"
        },
        {
          "id": "MDRW2100046822.1.26",
          "form": "꾸준히 운동해서 건강 좀 챙겨야겠어요\r\n",
          "original_form": "꾸준히 운동해서 건강 좀 챙겨야겠어요\r\n",
          "speaker_id": "2",
          "time": "20211104 21:15"
        }
      ]
