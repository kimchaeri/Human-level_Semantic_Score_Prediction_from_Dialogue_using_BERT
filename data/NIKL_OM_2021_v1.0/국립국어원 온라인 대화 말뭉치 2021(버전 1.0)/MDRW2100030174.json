      "utterance": [
        {
          "id": "MDRW2100030174.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030174.1.2",
          "form": "안녕하세요! 여행좋아하세요?",
          "original_form": "안녕하세요! 여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030174.1.3",
          "form": "네네 좋아해요 name2님은요?",
          "original_form": "네네 좋아해요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030174.1.4",
          "form": "저도 좋아해요!",
          "original_form": "저도 좋아해요!",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030174.1.5",
          "form": "어디가 가장 좋으셨나요?",
          "original_form": "어디가 가장 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030174.1.6",
          "form": "싱가폴이랑 제주도요 ㅎㅎ",
          "original_form": "싱가폴이랑 제주도요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030174.1.7",
          "form": "제주도는 최근에 다녀와서 기억에 남네요",
          "original_form": "제주도는 최근에 다녀와서 기억에 남네요",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030174.1.8",
          "form": "싱가폴 진짜 좋다고하더라고요",
          "original_form": "싱가폴 진짜 좋다고하더라고요",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030174.1.9",
          "form": "제주 가서 마음샌드사셨나요?",
          "original_form": "제주 가서 마음샌드사셨나요?",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030174.1.10",
          "form": "헉 그게 뭔가요",
          "original_form": "헉 그게 뭔가요",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030174.1.11",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030174.1.12",
          "form": "우도땅콩이 들어간 파바가 제주공항에서만 판다는 건데 맛나더라고요",
          "original_form": "우도땅콩이 들어간 파바가 제주공항에서만 판다는 건데 맛나더라고요",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030174.1.13",
          "form": "후속작으로 가평잣마음샌드도 나왔대요",
          "original_form": "후속작으로 가평잣마음샌드도 나왔대요",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030174.1.14",
          "form": "히익",
          "original_form": "히익",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030174.1.15",
          "form": "들어본것 같아요",
          "original_form": "들어본것 같아요",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030174.1.16",
          "form": "와 왜 못먹었지...와 감사합니다 알려주셔서",
          "original_form": "와 왜 못먹었지...와 감사합니다 알려주셔서",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030174.1.17",
          "form": "담엔 꼭 먹어봐야겟어요",
          "original_form": "담엔 꼭 먹어봐야겟어요",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030174.1.18",
          "form": "맛있어요! 기대없이 드셔보세요!",
          "original_form": "맛있어요! 기대없이 드셔보세요!",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030174.1.19",
          "form": "ㅋㅋㅋㅋ name2님은 어디 여행지가 인상깊엇나요",
          "original_form": "ㅋㅋㅋㅋ &name2&님은 어디 여행지가 인상깊엇나요",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030174.1.20",
          "form": "전 캐나다하고 강릉이요!",
          "original_form": "전 캐나다하고 강릉이요!",
          "speaker_id": "2",
          "time": "20210903 22:55"
        }
      ]
