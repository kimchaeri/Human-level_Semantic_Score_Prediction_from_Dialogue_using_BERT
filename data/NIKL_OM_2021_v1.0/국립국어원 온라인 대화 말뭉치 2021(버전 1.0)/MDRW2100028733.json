      "utterance": [
        {
          "id": "MDRW2100028733.1.1",
          "form": "점심 맛난거 드셨나요~~~?",
          "original_form": "점심 맛난거 드셨나요~~~?",
          "speaker_id": "2",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100028733.1.2",
          "form": "우동먹었어요",
          "original_form": "우동먹었어요",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100028733.1.3",
          "form": "면이 탱글탱글 맛있었어요",
          "original_form": "면이 탱글탱글 맛있었어요",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100028733.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100028733.1.5",
          "form": "맛있었겠네요!!",
          "original_form": "맛있었겠네요!!",
          "speaker_id": "2",
          "time": "20210904 20:21"
        },
        {
          "id": "MDRW2100028733.1.6",
          "form": "저는 점심은 별로였어요 ㅎ",
          "original_form": "저는 점심은 별로였어요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100028733.1.7",
          "form": "퇴근하고 유부초밥 해먹었어용",
          "original_form": "퇴근하고 유부초밥 해먹었어용",
          "speaker_id": "2",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100028733.1.8",
          "form": "오~ 저도 어제유부초밥먹었는데. 도제",
          "original_form": "오~ 저도 어제유부초밥먹었는데. 도제",
          "speaker_id": "1",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100028733.1.9",
          "form": "크래미랑 같이 먹었어요!",
          "original_form": "크래미랑 같이 먹었어요!",
          "speaker_id": "2",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100028733.1.10",
          "form": "갑자기 과자같은거 씹고 싶어서 생라면 먹었네요 ㅎ 먹고 후회했지만ㅠ",
          "original_form": "갑자기 과자같은거 씹고 싶어서 생라면 먹었네요 ㅎ 먹고 후회했지만ㅠ",
          "speaker_id": "2",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100028733.1.11",
          "form": "쌩라 안먹은지 오래됐는데",
          "original_form": "쌩라 안먹은지 오래됐는데",
          "speaker_id": "1",
          "time": "20210905 08:59"
        },
        {
          "id": "MDRW2100028733.1.12",
          "form": "어떤 라면으로요?",
          "original_form": "어떤 라면으로요?",
          "speaker_id": "1",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100028733.1.13",
          "form": "저희집은 진라면 먹어서 그거 먹었어요!!!!!",
          "original_form": "저희집은 진라면 먹어서 그거 먹었어요!!!!!",
          "speaker_id": "2",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100028733.1.14",
          "form": "한번씩 생라면 먹으면 맛있더라고용",
          "original_form": "한번씩 생라면 먹으면 맛있더라고용",
          "speaker_id": "2",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100028733.1.15",
          "form": "에~이 쌩라는 스낵면이죠 ㅋㅋ",
          "original_form": "에~이 쌩라는 스낵면이죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100028733.1.16",
          "form": "그런가요? ㅋㅋ그냥 집에있는 라면만 먹어봤어요",
          "original_form": "그런가요? ㅋㅋ그냥 집에있는 라면만 먹어봤어요",
          "speaker_id": "2",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100028733.1.17",
          "form": "스낵면 사먹어봐야겠네요ㅋㅋㅋ예전에 밥말아먹어보려고 먹어봤는데",
          "original_form": "스낵면 사먹어봐야겠네요ㅋㅋㅋ예전에 밥말아먹어보려고 먹어봤는데",
          "speaker_id": "2",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100028733.1.18",
          "form": "면이 얇아서 맛있어요",
          "original_form": "면이 얇아서 맛있어요",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100028733.1.19",
          "form": "면 얇은게 더맛있는거같아요 ㅎ",
          "original_form": "면 얇은게 더맛있는거같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100028733.1.20",
          "form": "컵라면은 뭐 좋아하세요?",
          "original_form": "컵라면은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100028733.1.21",
          "form": "작은육개장이나 왕뚜껑김치요",
          "original_form": "작은육개장이나 왕뚜껑김치요",
          "speaker_id": "1",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100028733.1.22",
          "form": "불닭미트소스맛도 좋아해요",
          "original_form": "불닭미트소스맛도 좋아해요",
          "speaker_id": "1",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100028733.1.23",
          "form": "저도 그거 좋아요 작은육개장!!!!!! 그 면 느낌이 좋아요",
          "original_form": "저도 그거 좋아요 작은육개장!!!!!! 그 면 느낌이 좋아요",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100028733.1.24",
          "form": "불닭 미트소스요? 오 궁금하네요",
          "original_form": "불닭 미트소스요? 오 궁금하네요",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100028733.1.25",
          "form": "작은 육개장을 쌩라로 먹는 사람도 있더라구요",
          "original_form": "작은 육개장을 쌩라로 먹는 사람도 있더라구요",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100028733.1.26",
          "form": "매운 파스타맛이예요 맛있어요",
          "original_form": "매운 파스타맛이예요 맛있어요",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100028733.1.27",
          "form": "오우 진짜요? 컵라는 생라로 도전안했는데",
          "original_form": "오우 진짜요? 컵라는 생라로 도전안했는데",
          "speaker_id": "2",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100028733.1.28",
          "form": "담에 먹어봐야겠네요",
          "original_form": "담에 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100028733.1.29",
          "form": "오 맛있겠다 매운파스타라니~~~!",
          "original_form": "오 맛있겠다 매운파스타라니~~~!",
          "speaker_id": "2",
          "time": "20210905 21:21"
        }
      ]
