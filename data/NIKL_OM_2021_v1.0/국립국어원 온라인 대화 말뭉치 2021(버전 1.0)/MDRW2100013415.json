      "utterance": [
        {
          "id": "MDRW2100013415.1.1",
          "form": "요즘 날씨가 좀 오락가락하네요!",
          "original_form": "요즘 날씨가 좀 오락가락하네요!",
          "speaker_id": "1",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013415.1.2",
          "form": "네 오락가락하고있어요!",
          "original_form": "네 오락가락하고있어요!",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013415.1.3",
          "form": "오전엔 칙칙했는데",
          "original_form": "오전엔 칙칙했는데",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013415.1.4",
          "form": "또 금새 해가 비치네요~",
          "original_form": "또 금새 해가 비치네요~",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013415.1.5",
          "form": "지금 여기 비오고 있어요",
          "original_form": "지금 여기 비오고 있어요",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013415.1.6",
          "form": "헉 어디쪽이시길래요?!",
          "original_form": "헉 어디쪽이시길래요?!",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013415.1.7",
          "form": "여긴 수도권인데",
          "original_form": "여긴 수도권인데",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013415.1.8",
          "form": "여긴 경북이요",
          "original_form": "여긴 경북이요",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013415.1.9",
          "form": "아직 태풍 영향권인가요?!",
          "original_form": "아직 태풍 영향권인가요?!",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013415.1.10",
          "form": "장마라서....ㅜㅜ",
          "original_form": "장마라서....ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013415.1.11",
          "form": "맞아요 요즘 비가 잦아서 보니 2차장마래요",
          "original_form": "맞아요 요즘 비가 잦아서 보니 2차장마래요",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013415.1.12",
          "form": "그니깐요ㅠㅠ",
          "original_form": "그니깐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013415.1.13",
          "form": "이번 태풍때 피해는 없으셨어요?",
          "original_form": "이번 태풍때 피해는 없으셨어요?",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013415.1.14",
          "form": "옷다젖었어요ㅠㅠ",
          "original_form": "옷다젖었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013415.1.15",
          "form": "나갈때면",
          "original_form": "나갈때면",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013415.1.16",
          "form": "비바람과싸워야돼요",
          "original_form": "비바람과싸워야돼요",
          "speaker_id": "2",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100013415.1.17",
          "form": "아이고ㅜㅜ 비 맞아서 눅눅하면 너무 찝찝하던데..",
          "original_form": "아이고ㅜㅜ 비 맞아서 눅눅하면 너무 찝찝하던데..",
          "speaker_id": "1",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100013415.1.18",
          "form": "우산을 써도 쓴게 아닌가봐요",
          "original_form": "우산을 써도 쓴게 아닌가봐요",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100013415.1.19",
          "form": "우산써도 다 젖어요ㅠㅠ",
          "original_form": "우산써도 다 젖어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:46"
        }
      ]
