      "utterance": [
        {
          "id": "MDRW2100035767.1.1",
          "form": "안녕하세용 주말에 뭐 드셨어용~?",
          "original_form": "안녕하세용 주말에 뭐 드셨어용~?",
          "speaker_id": "1",
          "time": "20210830 01:06"
        },
        {
          "id": "MDRW2100035767.1.2",
          "form": "주말에 간단하게먹었어요 라면이랑 삼겹살 등등 ㅎㅎ",
          "original_form": "주말에 간단하게먹었어요 라면이랑 삼겹살 등등 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100035767.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100035767.1.4",
          "form": "저는 카레 수제버거 거봉 등등 먹었어요ㅋㅋ",
          "original_form": "저는 카레 수제버거 거봉 등등 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:18"
        },
        {
          "id": "MDRW2100035767.1.5",
          "form": "라면은 무슨 라면 좋아하세요?",
          "original_form": "라면은 무슨 라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 02:18"
        },
        {
          "id": "MDRW2100035767.1.6",
          "form": "오 맛있는거 드셨네요",
          "original_form": "오 맛있는거 드셨네요",
          "speaker_id": "2",
          "time": "20210831 02:18"
        },
        {
          "id": "MDRW2100035767.1.7",
          "form": "전 요즘 진라면순한맛에꽂혔어요",
          "original_form": "전 요즘 진라면순한맛에꽂혔어요",
          "speaker_id": "2",
          "time": "20210831 02:18"
        },
        {
          "id": "MDRW2100035767.1.8",
          "form": "버거는 수제버거 사드신거에요 만드신거에요",
          "original_form": "버거는 수제버거 사드신거에요 만드신거에요",
          "speaker_id": "2",
          "time": "20210831 02:19"
        },
        {
          "id": "MDRW2100035767.1.9",
          "form": "오 좀 순한 라면 좋아하시나봐요!",
          "original_form": "오 좀 순한 라면 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210831 02:19"
        },
        {
          "id": "MDRW2100035767.1.10",
          "form": "사먹었어요ㅋㅋㅋㅋ 전 요리 솜씨가 없어서요",
          "original_form": "사먹었어요ㅋㅋㅋㅋ 전 요리 솜씨가 없어서요",
          "speaker_id": "1",
          "time": "20210831 02:19"
        },
        {
          "id": "MDRW2100035767.1.11",
          "form": "ㅎㅎ네요즘 순한맛이 좋더라구여",
          "original_form": "ㅎㅎ네요즘 순한맛이 좋더라구여",
          "speaker_id": "2",
          "time": "20210831 02:19"
        },
        {
          "id": "MDRW2100035767.1.12",
          "form": "순두부 열라면이라고 아세요?",
          "original_form": "순두부 열라면이라고 아세요?",
          "speaker_id": "1",
          "time": "20210831 02:19"
        },
        {
          "id": "MDRW2100035767.1.13",
          "form": "장이안좋아서 ㅎㅎ...",
          "original_form": "장이안좋아서 ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210831 02:19"
        },
        {
          "id": "MDRW2100035767.1.14",
          "form": "아하ㅠㅠ 매운건 좀 자극적이죠...",
          "original_form": "아하ㅠㅠ 매운건 좀 자극적이죠...",
          "speaker_id": "1",
          "time": "20210831 02:19"
        },
        {
          "id": "MDRW2100035767.1.15",
          "form": "네 알아요 전 신라면보다매우면 배가탈이나서ㅜㅜ",
          "original_form": "네 알아요 전 신라면보다매우면 배가탈이나서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100035767.1.16",
          "form": "사실신라면부터 문제에요",
          "original_form": "사실신라면부터 문제에요",
          "speaker_id": "2",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100035767.1.17",
          "form": "이거 열라면으로 안하셔도 되고",
          "original_form": "이거 열라면으로 안하셔도 되고",
          "speaker_id": "1",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100035767.1.18",
          "form": "진라면으로 하셔도 되는데",
          "original_form": "진라면으로 하셔도 되는데",
          "speaker_id": "1",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100035767.1.19",
          "form": "근데 순두부는 좀맵고얼큰해야 맛있잖아여",
          "original_form": "근데 순두부는 좀맵고얼큰해야 맛있잖아여",
          "speaker_id": "2",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100035767.1.20",
          "form": "라면에 순두부 넣으면 소화도 잘되고 고소해져요!",
          "original_form": "라면에 순두부 넣으면 소화도 잘되고 고소해져요!",
          "speaker_id": "1",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100035767.1.21",
          "form": "오 한번도전해봐야겠네요",
          "original_form": "오 한번도전해봐야겠네요",
          "speaker_id": "2",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100035767.1.22",
          "form": "라면스프에 msg 때문인지 안매워도 맛있어요ㅎㅎ",
          "original_form": "라면스프에 msg 때문인지 안매워도 맛있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100035767.1.23",
          "form": "애초에 순한맛으로 먹는거라ㅋㅋㅋ",
          "original_form": "애초에 순한맛으로 먹는거라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100035767.1.24",
          "form": "계란풀어서고소하게 먹는거좋아하거든여",
          "original_form": "계란풀어서고소하게 먹는거좋아하거든여",
          "speaker_id": "2",
          "time": "20210831 02:21"
        },
        {
          "id": "MDRW2100035767.1.25",
          "form": "그럼 괜찮은것같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "그럼 괜찮은것같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:21"
        },
        {
          "id": "MDRW2100035767.1.26",
          "form": "저도 계란 노른자까지 풀어서 먹어요ㅋㅋ",
          "original_form": "저도 계란 노른자까지 풀어서 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:21"
        },
        {
          "id": "MDRW2100035767.1.27",
          "form": "제가 가장 좋아하는건",
          "original_form": "제가 가장 좋아하는건",
          "speaker_id": "1",
          "time": "20210831 02:21"
        },
        {
          "id": "MDRW2100035767.1.28",
          "form": "참깨라면이에요",
          "original_form": "참깨라면이에요",
          "speaker_id": "1",
          "time": "20210831 02:21"
        }
      ]
