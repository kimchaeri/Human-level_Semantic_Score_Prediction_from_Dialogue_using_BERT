      "utterance": [
        {
          "id": "MDRW2100011536.1.1",
          "form": "좋아하는 연예인 있으세요?",
          "original_form": "좋아하는 연예인 있으세요?",
          "speaker_id": "2",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100011536.1.2",
          "form": "저 있죠!",
          "original_form": "저 있죠!",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100011536.1.3",
          "form": "누구 좋아하시나요",
          "original_form": "누구 좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100011536.1.4",
          "form": "유재석 좋아합니다!",
          "original_form": "유재석 좋아합니다!",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100011536.1.5",
          "form": "유느님! 무도시즌2ㅜ보고싶네요",
          "original_form": "유느님! 무도시즌2ㅜ보고싶네요",
          "speaker_id": "2",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100011536.1.6",
          "form": "무도때부터 봤었어서 유난히 좋아하는거같아요 ㅠㅠ",
          "original_form": "무도때부터 봤었어서 유난히 좋아하는거같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100011536.1.7",
          "form": "유퀴즈도 보시나요",
          "original_form": "유퀴즈도 보시나요",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100011536.1.8",
          "form": "유퀴즈도 보긴하죠",
          "original_form": "유퀴즈도 보긴하죠",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100011536.1.9",
          "form": "유투브에서 하이라이트 정리해서 올린거 보곤해용",
          "original_form": "유투브에서 하이라이트 정리해서 올린거 보곤해용",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100011536.1.10",
          "form": "예전 코로나전이 훨씬 재밌었는데ㅠ",
          "original_form": "예전 코로나전이 훨씬 재밌었는데ㅠ",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100011536.1.11",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100011536.1.12",
          "form": "코로나 이후로 뭔가 축소되는 그런게 참..ㅠㅠ",
          "original_form": "코로나 이후로 뭔가 축소되는 그런게 참..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100011536.1.13",
          "form": "맨날 초대만해서 하니까ㅠ",
          "original_form": "맨날 초대만해서 하니까ㅠ",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100011536.1.14",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100011536.1.15",
