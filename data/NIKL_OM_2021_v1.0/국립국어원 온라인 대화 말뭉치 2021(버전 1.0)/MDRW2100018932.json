      "utterance": [
        {
          "id": "MDRW2100018932.1.1",
          "form": "제말 보셧나요",
          "original_form": "제말 보셧나요",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018932.1.2",
          "form": "걍 보여드림",
          "original_form": "걍 보여드림",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018932.1.3",
          "form": "휴가시니까",
          "original_form": "휴가시니까",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018932.1.4",
          "form": "이제 이 얘긴 그만하죠",
          "original_form": "이제 이 얘긴 그만하죠",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018932.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018932.1.6",
          "form": "박씨가",
          "original_form": "박씨가",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018932.1.7",
          "form": "대화에 끼질못해서",
          "original_form": "대화에 끼질못해서",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018932.1.8",
          "form": "바로 선긋는것 같네요",
          "original_form": "바로 선긋는것 같네요",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018932.1.9",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018932.1.10",
          "form": "아닙니다ㅋㅋㅋ",
          "original_form": "아닙니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018932.1.11",
          "form": "같은 직장인데",
          "original_form": "같은 직장인데",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018932.1.12",
          "form": "얘기하려니",
          "original_form": "얘기하려니",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018932.1.13",
          "form": "이벤트 관련 얘기뿐이넹",
          "original_form": "이벤트 관련 얘기뿐이넹",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018932.1.14",
          "form": "박씨는 왜 휴가 안쓰고",
          "original_form": "박씨는 왜 휴가 안쓰고",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018932.1.15",
          "form": "반차만 쓰시나요",
          "original_form": "반차만 쓰시나요",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018932.1.16",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018932.1.17",
          "form": "이번엔 휴가쓰려고요",
          "original_form": "이번엔 휴가쓰려고요",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018932.1.18",
          "form": "이사할때ㅠ",
          "original_form": "이사할때ㅠ",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018932.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018932.1.20",
          "form": "9일날 쓰시나요",
          "original_form": "9일날 쓰시나요",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018932.1.21",
          "form": "넵 맞습니다",
          "original_form": "넵 맞습니다",
          "speaker_id": "2",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018932.1.22",
          "form": "이제 박씨 이사날짜도 외우고",
          "original_form": "이제 박씨 이사날짜도 외우고",
          "speaker_id": "1",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018932.1.23",
          "form": "세뇌당햇네요",
          "original_form": "세뇌당햇네요",
          "speaker_id": "1",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018932.1.24",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018932.1.25",
          "form": "잘 기억하시네ㅋㅋ",
          "original_form": "잘 기억하시네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018932.1.26",
          "form": "당황",
          "original_form": "당황",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018932.1.27",
          "form": "제가원래쓸데없는거 잘 기억해요",
          "original_form": "제가원래쓸데없는거 잘 기억해요",
          "speaker_id": "1",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018932.1.28",
          "form": "name2님 생일은 모르는데",
          "original_form": "&name2&님 생일은 모르는데",
          "speaker_id": "1",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018932.1.29",
          "form": "name2님 이사날짜는 외우고 잇는것 처럼..",
          "original_form": "&name2&님 이사날짜는 외우고 잇는것 처럼..",
          "speaker_id": "1",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018932.1.30",
          "form": "쩝",
          "original_form": "쩝",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018932.1.31",
          "form": "저희 사무용품",
          "original_form": "저희 사무용품",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018932.1.32",
          "form": "공구하나요?",
          "original_form": "공구하나요?",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018932.1.33",
          "form": "뭐 공구하고 싶으신가요",
          "original_form": "뭐 공구하고 싶으신가요",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018932.1.34",
          "form": "합당하면 같이 구매함",
          "original_form": "합당하면 같이 구매함",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018932.1.35",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018932.1.36",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018932.1.37",
