        {
          "id": "MDRW2100013732.1.1",
          "form": "치즈케익 먹고싶다",
          "original_form": "치즈케익 먹고싶다",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013732.1.2",
          "form": "사 먹어라",
          "original_form": "사 먹어라",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013732.1.3",
          "form": "엄마도 먹을래?",
          "original_form": "엄마도 먹을래?",
          "speaker_id": "1",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013732.1.4",
          "form": "나는 됐다",
          "original_form": "나는 됐다",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013732.1.5",
          "form": "스타벅스 가서 사올까",
          "original_form": "스타벅스 가서 사올까",
          "speaker_id": "1",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013732.1.6",
          "form": "너무 멀다",
          "original_form": "너무 멀다",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013732.1.7",
          "form": "금방 갔다올수 있다",
          "original_form": "금방 갔다올수 있다",
          "speaker_id": "1",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013732.1.8",
          "form": "그러면 갔다 오던지",
          "original_form": "그러면 갔다 오던지",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013732.1.9",
          "form": "그를까 ㅋ 근데 일하고있어서..",
          "original_form": "그를까 ㅋ 근데 일하고있어서..",
          "speaker_id": "1",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013732.1.10",
          "form": "잠시 갔다 온다 해라",
          "original_form": "잠시 갔다 온다 해라",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013732.1.11",
          "form": "음 생각해보공 ㅋ",
          "original_form": "음 생각해보공 ㅋ",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013732.1.12",
          "form": "이따 같이 가든지",
          "original_form": "이따 같이 가든지",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013732.1.13",
          "form": "오옷!좋아용",
          "original_form": "오옷!좋아용",
