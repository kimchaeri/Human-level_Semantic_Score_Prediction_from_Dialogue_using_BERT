      "utterance": [
        {
          "id": "MDRW2100040504.1.1",
          "form": "그러게ㅡㅡ",
          "original_form": "그러게ㅡㅡ",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040504.1.2",
          "form": "하는일없이바쁘노",
          "original_form": "하는일없이바쁘노",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040504.1.3",
          "form": "설거지도 못했다이",
          "original_form": "설거지도 못했다이",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040504.1.4",
          "form": "언니 오늘 왜 이래 바쁘노??",
          "original_form": "언니 오늘 왜 이래 바쁘노??",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040504.1.5",
          "form": "설거지끝ㅋ",
          "original_form": "설거지끝ㅋ",
          "speaker_id": "2",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100040504.1.6",
          "form": "상담잘했나?",
          "original_form": "상담잘했나?",
          "speaker_id": "2",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100040504.1.7",
          "form": "엉 상담 잘했지",
          "original_form": "엉 상담 잘했지",
          "speaker_id": "1",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100040504.1.8",
          "form": "잘이라기 보다는 그냥 쏘쏘",
          "original_form": "잘이라기 보다는 그냥 쏘쏘",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100040504.1.9",
          "form": "개인적으로 하는게 나은데",
          "original_form": "개인적으로 하는게 나은데",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100040504.1.10",
          "form": "그래도 선생님이 애들 파악을 잘하고 있는 거 같더라",
          "original_form": "그래도 선생님이 애들 파악을 잘하고 있는 거 같더라",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100040504.1.11",
          "form": "ㅋ 그렇구만ㅋㅋ",
          "original_form": "ㅋ 그렇구만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100040504.1.12",
          "form": "ㅡ다음엔 일대일로한다고해라ㅎ",
          "original_form": "ㅡ다음엔 일대일로한다고해라ㅎ",
          "speaker_id": "2",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100040504.1.13",
          "form": "그것도 괜찮겠다이",
          "original_form": "그것도 괜찮겠다이",
          "speaker_id": "2",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100040504.1.14",
          "form": "아 원래 같았으면",
          "original_form": "아 원래 같았으면",
          "speaker_id": "1",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100040504.1.15",
          "form": "수 언니랑 가도 괜찮았는데",
          "original_form": "수 언니랑 가도 괜찮았는데",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040504.1.16",
          "form": "지금 우리원이랑 수랑 사이가 안 좋거든",
          "original_form": "지금 우리원이랑 수랑 사이가 안 좋거든",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040504.1.17",
          "form": "어른들도 좀 그렇고",
          "original_form": "어른들도 좀 그렇고",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040504.1.18",
          "form": "그래서 같이 얘기할라니 좀 그렇대",
          "original_form": "그래서 같이 얘기할라니 좀 그렇대",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040504.1.19",
          "form": "잉?",
          "original_form": "잉?",
          "speaker_id": "2",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040504.1.20",
          "form": "어른들이왜?",
          "original_form": "어른들이왜?",
          "speaker_id": "2",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040504.1.21",
          "form": "무슨일있엏나?",
          "original_form": "무슨일있엏나?",
          "speaker_id": "2",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040504.1.22",
          "form": "아 길다 언니",
          "original_form": "아 길다 언니",
          "speaker_id": "1",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040504.1.23",
          "form": "오늘 얘기해줄랬는데",
          "original_form": "오늘 얘기해줄랬는데",
          "speaker_id": "1",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040504.1.24",
          "form": "만나서 얘기해야됨",
          "original_form": "만나서 얘기해야됨",
          "speaker_id": "1",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040504.1.25",
          "form": "너무 길다 ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "너무 길다 ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040504.1.26",
          "form": "아하;;;;;;",
          "original_form": "아하;;;;;;",
          "speaker_id": "2",
          "time": "20210905 00:10"
        },
        {
          "id": "MDRW2100040504.1.27",
          "form": "캠핑이후?",
          "original_form": "캠핑이후?",
          "speaker_id": "2",
          "time": "20210905 00:10"
        },
        {
          "id": "MDRW2100040504.1.28",
          "form": "어 캠핑 갔다 와서",
          "original_form": "어 캠핑 갔다 와서",
          "speaker_id": "1",
          "time": "20210905 00:10"
        },
        {
          "id": "MDRW2100040504.1.29",
          "form": "이제 같이 캠핑 가겠나 싶다",
          "original_form": "이제 같이 캠핑 가겠나 싶다",
          "speaker_id": "1",
          "time": "20210905 00:10"
        },
        {
          "id": "MDRW2100040504.1.30",
          "form": "최근이구먼ㅜ",
          "original_form": "최근이구먼ㅜ",
          "speaker_id": "2",
          "time": "20210905 00:10"
        },
        {
          "id": "MDRW2100040504.1.31",
          "form": "헐 궁금ㅡㅡ",
          "original_form": "헐 궁금{censored}ㅡㅡ",
          "speaker_id": "2",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040504.1.32",
          "form": "언니 화요일날 커피 마실 수 있지??",
          "original_form": "언니 화요일날 커피 마실 수 있지??",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040504.1.33",
          "form": "밥 먹을까??",
          "original_form": "밥 먹을까??",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040504.1.34",
          "form": "나 월성동가잖애ㅡㅡ",
          "original_form": "나 월성동가잖애ㅡㅡ",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040504.1.35",
          "form": "갔다오면 오후지ㅜ",
          "original_form": "갔다오면 오후지ㅜ",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040504.1.36",
          "form": "아 언니 그날 못 본다 그랬지",
          "original_form": "아 언니 그날 못 본다 그랬지",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040504.1.37",
          "form": "씁",
          "original_form": "씁",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040504.1.38",
          "form": "안되겠네",
          "original_form": "안되겠네",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040504.1.39",
          "form": "언니한테는 말 안 해줘야지 그냥",
          "original_form": "언니한테는 말 안 해줘야지 그냥",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040504.1.40",
          "form": "헐ㅋㅋ",
          "original_form": "헐ㅋㅋ",
          "speaker_id": "2",
