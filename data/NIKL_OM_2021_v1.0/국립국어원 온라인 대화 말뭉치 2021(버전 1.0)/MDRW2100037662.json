      "utterance": [
        {
          "id": "MDRW2100037662.1.1",
          "form": "못난 엄마들 ㅋㅋㅋㅋㅋ",
          "original_form": "못난 엄마들 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100037662.1.2",
          "form": "아까 원이 전화왔거든",
          "original_form": "아까 원이 전화왔거든",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100037662.1.3",
          "form": "학교 마쳤는데 name3한테 전화해도 되냐고",
          "original_form": "학교 마쳤는데 &name3&한테 전화해도 되냐고",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100037662.1.4",
          "form": "전화했더니 name3는 집에 가고 있다고 해서 자기는 영어학원으로 바로 간다대",
          "original_form": "전화했더니 &name3&는 집에 가고 있다고 해서 자기는 영어학원으로 바로 간다대",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100037662.1.5",
          "form": "그래서 우산 잘 챙겼냐고 다시 전화를 하니까",
          "original_form": "그래서 우산 잘 챙겼냐고 다시 전화를 하니까",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100037662.1.6",
          "form": "name3집에 가다가 name4이 영어학원에 있다고 하니까 바로 영어 학원으로 와서 같이 놀고 있다고",
          "original_form": "&name3&집에 가다가 &name4&이 영어학원에 있다고 하니까 바로 영어 학원으로 와서 같이 놀고 있다고",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100037662.1.7",
          "form": "애들은 애들이다 정마?",
          "original_form": "애들은 애들이다 정마?",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100037662.1.8",
          "form": "정말",
          "original_form": "정말",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100037662.1.9",
          "form": "ㅋㅋㅋㅋㄱ맞어",
          "original_form": "ㅋㅋㅋㅋㄱ맞어",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100037662.1.10",
          "form": "그렇게된다니깐ㅋㅋ",
          "original_form": "그렇게된다니깐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100037662.1.11",
          "form": "웃겨ㅋㅋㅋㅋㅋ",
          "original_form": "웃겨ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100037662.1.12",
          "form": "어 지네끼리는 다시 사이 좋아졌고",
          "original_form": "어 지네끼리는 다시 사이 좋아졌고",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100037662.1.13",
          "form": "어른들만 불편해졌음",
          "original_form": "어른들만 불편해졌음",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100037662.1.14",
          "form": "어른이 애보다 못하다 정말",
          "original_form": "어른이 애보다 못하다 정말",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100037662.1.15",
          "form": "그러니깐ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100037662.1.16",
          "form": "그럴때있지ㅋㅋ아 웃겨ㅋ",
          "original_form": "그럴때있지ㅋㅋ아 웃겨ㅋ",
          "speaker_id": "2",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100037662.1.17",
          "form": "남일같지않다 다 그럴걸?ㅋㅋ",
          "original_form": "남일같지않다 다 그럴걸?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:19"
        },
        {
          "id": "MDRW2100037662.1.18",
          "form": "그래 다 그렇겠지??",
          "original_form": "그래 다 그렇겠지??",
          "speaker_id": "1",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100037662.1.19",
          "form": "지금 같이 캠핑을 갈지 말지도 고민이다 다음 준데",
          "original_form": "지금 같이 캠핑을 갈지 말지도 고민이다 다음 준데",
          "speaker_id": "1",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100037662.1.20",
          "form": "캠핑 어떻게 하지??",
          "original_form": "캠핑 어떻게 하지??",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100037662.1.21",
          "form": "그렇지ㅋㅋㅋㅋ",
          "original_form": "그렇지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100037662.1.22",
          "form": "난감하긴하다이",
          "original_form": "난감하긴하다이",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100037662.1.23",
          "form": "언니랑 신랑이 기분풀리고 그러면 가도되지않겠나",
          "original_form": "언니랑 신랑이 기분풀리고 그러면 가도되지않겠나",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100037662.1.24",
          "form": "나는 기분이 안 좋아서 안 가기보다는",
          "original_form": "나는 기분이 안 좋아서 안 가기보다는",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100037662.1.25",
          "form": "애들이 서로 조심하는 차원에서",
          "original_form": "애들이 서로 조심하는 차원에서",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100037662.1.26",
          "form": "안 가는 게 나을 거 같다고 생각을 하거든",
          "original_form": "안 가는 게 나을 거 같다고 생각을 하거든",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100037662.1.27",
          "form": "붙어있으면 분명히 또 싸울 건데",
          "original_form": "붙어있으면 분명히 또 싸울 건데",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100037662.1.28",
          "form": "그러면 왠지 안면 받힐것같아서",
          "original_form": "그러면 왠지 안면 받힐것같아서",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100037662.1.29",
          "form": "같이 있는데 싸우면 좀 그렇잖아",
          "original_form": "같이 있는데 싸우면 좀 그렇잖아",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100037662.1.30",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100037662.1.31",
          "form": "이번에는 그럼 쿨하게 패스해뿌라 시간지나면 또 괜찮아지더라",
          "original_form": "이번에는 그럼 쿨하게 패스해뿌라 시간지나면 또 괜찮아지더라",
          "speaker_id": "2",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100037662.1.32",
          "form": "애들 좀 괜찮아지면 같이 가자고 하는 게  나을 거 같다",
          "original_form": "애들 좀 괜찮아지면 같이 가자고 하는 게  나을 거 같다",
          "speaker_id": "1",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100037662.1.33",
          "form": "알았으",
          "original_form": "알았으",
          "speaker_id": "1",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100037662.1.34",
          "form": "투 왔다",
          "original_form": "투 왔다",
          "speaker_id": "1",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100037662.1.35",
          "form": "육출 ㅋㅋㅋㅋ",
          "original_form": "육출 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100037662.1.36",
          "form": "웅웅 ㅋㅋ",
          "original_form": "웅웅 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:07"
        },
        {
          "id": "MDRW2100037662.1.37",
          "form": "나도 투가 왔네ㅋ",
          "original_form": "나도 투가 왔네ㅋ",
          "speaker_id": "2",
          "time": "20210901 16:07"
        },
        {
          "id": "MDRW2100037662.1.38",
          "form": "이제 곧 name4도 온다",
          "original_form": "이제 곧 &name4&도 온다",
          "speaker_id": "1",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100037662.1.39",
          "form": "오늘은 과연 무슨 일이 있었을까?",
          "original_form": "오늘은 과연 무슨 일이 있었을까?",
          "speaker_id": "1",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100037662.1.40",
          "form": "별일없었을거야 언니ㅋㅋㄱㅋㅋ",
          "original_form": "별일없었을거야 언니ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100037662.1.41",
          "form": "그래 오늘은 별 일 없었다 네",
          "original_form": "그래 오늘은 별 일 없었다 네",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037662.1.42",
          "form": "하루 잘 넘어가는군",
          "original_form": "하루 잘 넘어가는군",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037662.1.43",
          "form": "ㅋㅋ웃겨 신경쓰인다ㅋ",
          "original_form": "ㅋㅋ웃겨 신경쓰인다ㅋ",
          "speaker_id": "2",
          "time": "20210901 17:06"
        }
