      "utterance": [
        {
          "id": "MDRW2100003959.1.1",
          "form": "신을 믿나요?",
          "original_form": "신을 믿나요?",
          "speaker_id": "1",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003959.1.2",
          "form": "음 그냥 있다고는 생각해요",
          "original_form": "음 그냥 있다고는 생각해요",
          "speaker_id": "2",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003959.1.3",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003959.1.4",
          "form": "name1님은 믿으시나요??",
          "original_form": "&name1&님은 믿으시나요??",
          "speaker_id": "2",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003959.1.5",
          "form": "저는 없다고 생각하는 편이라",
          "original_form": "저는 없다고 생각하는 편이라",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003959.1.6",
          "form": "종교는 있으세요?",
          "original_form": "종교는 있으세요?",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003959.1.7",
          "form": "엌ㅋㅋㅋ 종교는 없어요~!",
          "original_form": "엌ㅋㅋㅋ 종교는 없어요~!",
          "speaker_id": "2",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003959.1.8",
          "form": "예전에",
          "original_form": "예전에",
          "speaker_id": "2",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003959.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003959.1.10",
          "form": "친구가 교회가면 맛있는거 준다해서 따라간거가 마지막이네요!",
          "original_form": "친구가 교회가면 맛있는거 준다해서 따라간거가 마지막이네요!",
          "speaker_id": "2",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003959.1.11",
          "form": "그때 아이스크림도 얻어먹고 문상도 받고 그랬었는데",
          "original_form": "그때 아이스크림도 얻어먹고 문상도 받고 그랬었는데",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003959.1.12",
          "form": "대부분 시작은 친구나 가족으로 시작하는거같아요ㅋㅋ",
          "original_form": "대부분 시작은 친구나 가족으로 시작하는거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003959.1.13",
          "form": "선물준다해서 따라가거나",
          "original_form": "선물준다해서 따라가거나",
          "speaker_id": "1",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003959.1.14",
          "form": "맞아욬ㅋㅋㅋㅋ 선물에 홀려서ㅠㅠ",
          "original_form": "맞아욬ㅋㅋㅋㅋ 선물에 홀려서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003959.1.15",
          "form": "가서 들어보면 그냥 잠오고 뭐라는지도 모르겠고ㅠ",
          "original_form": "가서 들어보면 그냥 잠오고 뭐라는지도 모르겠고ㅠ",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003959.1.16",
          "form": "맞아요 지루하죠ㅠ",
          "original_form": "맞아요 지루하죠ㅠ",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003959.1.17",
          "form": "사실 노래 부르는 것도",
          "original_form": "사실 노래 부르는 것도",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003959.1.18",
          "form": "별로..안좋아하고",
          "original_form": "별로..안좋아하고",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003959.1.19",
          "form": "그니까요ㅠ",
          "original_form": "그니까요ㅠ",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003959.1.20",
          "form": "다른것 보다 헌금제도가 이해가 안되서",
          "original_form": "다른것 보다 헌금제도가 이해가 안되서",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003959.1.21",
          "form": "마지막에 무슨 통들고 다니는데 거기에 돈도 내야하더라구요",
          "original_form": "마지막에 무슨 통들고 다니는데 거기에 돈도 내야하더라구요",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003959.1.22",
          "form": "안내면 눈치주고",
          "original_form": "안내면 눈치주고",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003959.1.23",
          "form": "그니까요!",
          "original_form": "그니까요!",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003959.1.24",
          "form": "좀 그런 것 때문에",
          "original_form": "좀 그런 것 때문에",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003959.1.25",
          "form": "그래서 싫었어요",
          "original_form": "그래서 싫었어요",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003959.1.26",
          "form": "반감이 들죠 아무래도",
          "original_form": "반감이 들죠 아무래도",
          "speaker_id": "1",
          "time": "20210821 14:14"
        }
      ]
    }
