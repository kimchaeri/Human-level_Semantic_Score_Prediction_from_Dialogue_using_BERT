      "utterance": [
        {
          "id": "MDRW2100009788.1.1",
          "form": "저도 진짜 지웠는지 봐야겠네요ㅎㅎ",
          "original_form": "저도 진짜 지웠는지 봐야겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100009788.1.2",
          "form": "지금은 학교생활 안하시죠?",
          "original_form": "지금은 학교생활 안하시죠?",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100009788.1.3",
          "form": "네ㅎㅎㅎ학교생활하시나요?",
          "original_form": "네ㅎㅎㅎ학교생활하시나요?",
          "speaker_id": "1",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100009788.1.4",
          "form": "저도 졸업한지 오래 되었습니다 ㅎㅎㅎ",
          "original_form": "저도 졸업한지 오래 되었습니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100009788.1.5",
          "form": "학교다닐때가 그리워요ㅎㅎ",
          "original_form": "학교다닐때가 그리워요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100009788.1.6",
          "form": "그쵸? 다시 가면 더 열심히 놀거예요 ㅋ",
          "original_form": "그쵸? 다시 가면 더 열심히 놀거예요 ㅋ",
          "speaker_id": "2",
          "time": "20210823 21:32"
        },
        {
          "id": "MDRW2100009788.1.7",
          "form": "맞아요ㅋㅋㅋ공부말고 그시절 즐기기ㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ공부말고 그시절 즐기기ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:32"
        },
        {
          "id": "MDRW2100009788.1.8",
          "form": "동아리도 한개 더 했어야 했엉ㅅ",
          "original_form": "동아리도 한개 더 했어야 했엉ㅅ",
          "speaker_id": "2",
          "time": "20210823 21:34"
        },
        {
          "id": "MDRW2100009788.1.9",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210823 21:34"
        },
        {
          "id": "MDRW2100009788.1.10",
          "form": "맞아요..저는 취업동아리만 해서 더 아쉬워요 ㅠ",
          "original_form": "맞아요..저는 취업동아리만 해서 더 아쉬워요 ㅠ",
          "speaker_id": "1",
          "time": "20210823 21:34"
        },
        {
          "id": "MDRW2100009788.1.11",
          "form": "악!! 정말 못 노셨는데요...",
          "original_form": "악!! 정말 못 노셨는데요...",
          "speaker_id": "2",
          "time": "20210823 21:36"
        },
        {
          "id": "MDRW2100009788.1.12",
          "form": "너무 아쉽구...ㅋㅋㅋ잘놀아야 취업도 잘하던데ㅠㅠㅜ",
          "original_form": "너무 아쉽구...ㅋㅋㅋ잘놀아야 취업도 잘하던데ㅠㅠㅜ",
          "speaker_id": "1",
          "time": "20210823 21:37"
        },
        {
          "id": "MDRW2100009788.1.13",
          "form": "전 그래도 놀만큼 논것 같은데 다른 재미있는 동아리를 더 했어야 했다는 후회가 남아요",
          "original_form": "전 그래도 놀만큼 논것 같은데 다른 재미있는 동아리를 더 했어야 했다는 후회가 남아요",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100009788.1.14",
          "form": "저도 그래요 많이할걸...ㅎㅎ",
          "original_form": "저도 그래요 많이할걸...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100009788.1.15",
          "form": "다시 돌아가면 어떤 동아리 하시고 싶으세요?",
          "original_form": "다시 돌아가면 어떤 동아리 하시고 싶으세요?",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100009788.1.16",
          "form": "저는 악기ㅎㅎ 하고 싶어요",
          "original_form": "저는 악기ㅎㅎ 하고 싶어요",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100009788.1.17",
          "form": "무슨 악기요?",
          "original_form": "무슨 악기요?",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100009788.1.18",
          "form": "밴드동아리가 있었는데ㅋㅋ건반해보고 싶었어요ㅎㅎ",
          "original_form": "밴드동아리가 있었는데ㅋㅋ건반해보고 싶었어요ㅎㅎ",
