      "utterance": [
        {
          "id": "MDRW2100011041.1.1",
          "form": "반려동물 키우세여?",
          "original_form": "반려동물 키우세여?",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011041.1.2",
          "form": "아니요 ㅠㅠ 키우고 싶어요",
          "original_form": "아니요 ㅠㅠ 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011041.1.3",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011041.1.4",
          "form": "아니요ㅜ 저도 키우고 싶어요!",
          "original_form": "아니요ㅜ 저도 키우고 싶어요!",
          "speaker_id": "1",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011041.1.5",
          "form": "어떤 동물 좋아하세여?",
          "original_form": "어떤 동물 좋아하세여?",
          "speaker_id": "1",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011041.1.6",
          "form": "저는 강아지를 좋아해요!",
          "original_form": "저는 강아지를 좋아해요!",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011041.1.7",
          "form": "강아지 키우고 싶어요 ㅠㅠ",
          "original_form": "강아지 키우고 싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011041.1.8",
          "form": "저도요! 강아지랑 산책 다니고 싶어요ㅋㅋ",
          "original_form": "저도요! 강아지랑 산책 다니고 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011041.1.9",
          "form": "근데 현실적으로 키우기 쉽지 않은것 같아요..",
          "original_form": "근데 현실적으로 키우기 쉽지 않은것 같아요..",
          "speaker_id": "1",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011041.1.10",
          "form": "맞아요 ㅠㅠ 그래서 망설여져요",
          "original_form": "맞아요 ㅠㅠ 그래서 망설여져요",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011041.1.11",
          "form": "독립하면 키우고 싶은데 무서워요 ㅠㅠ",
          "original_form": "독립하면 키우고 싶은데 무서워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011041.1.12",
          "form": "아프면 돈도 많이 들고 외롭지 않게 챙겨야하니까 쉽지 않은것 같아요ㅜㅜ",
          "original_form": "아프면 돈도 많이 들고 외롭지 않게 챙겨야하니까 쉽지 않은것 같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011041.1.13",
          "form": "가족들도 강아지 좋아해요?",
          "original_form": "가족들도 강아지 좋아해요?",
          "speaker_id": "1",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011041.1.14",
          "form": "맞아요ㅜㅜ 동생은 좋아하는데 부모님이 안좋아하셔 못키워요 ㅠㅠ",
          "original_form": "맞아요ㅜㅜ 동생은 좋아하는데 부모님이 안좋아하셔 못키워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 00:06"
        },
        {
          "id": "MDRW2100011041.1.15",
          "form": "저도 엄마가 반대하셔서 못키웠는데..한명이라도 반대하면 어려운것 같아요ㅜㅜ",
          "original_form": "저도 엄마가 반대하셔서 못키웠는데..한명이라도 반대하면 어려운것 같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011041.1.16",
          "form": "맞아요. 가족들이 다 좋아해야 하는 것 같아요.",
          "original_form": "맞아요. 가족들이 다 좋아해야 하는 것 같아요.",
          "speaker_id": "2",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011041.1.17",
          "form": "인터넷에 싫어하던 가족들이 결국 반려동물이랑 친해지는거 보면 부러워요 ㅠㅠ",
          "original_form": "인터넷에 싫어하던 가족들이 결국 반려동물이랑 친해지는거 보면 부러워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100011041.1.18",
          "form": "일단 키우면 다들 좋아하시는 것 같아요ㅋㅋㅋ",
          "original_form": "일단 키우면 다들 좋아하시는 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011041.1.19",
          "form": "강아지 어떤 종류 키우고 싶어요?",
          "original_form": "강아지 어떤 종류 키우고 싶어요?",
          "speaker_id": "1",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011041.1.20",
          "form": "저는 포메나 말티즈? 근데 사실 강아지는 다 귀여워서",
          "original_form": "저는 포메나 말티즈? 근데 사실 강아지는 다 귀여워서",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011041.1.21",
          "form": "요건만 된다면 다 키우고 싶어요 큰강아지도 ㅋㅋ",
          "original_form": "요건만 된다면 다 키우고 싶어요 큰강아지도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011041.1.22",
          "form": "저는 큰 강아지 있으면 든든할것 같아요 ㅋㅋㅋ",
          "original_form": "저는 큰 강아지 있으면 든든할것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100011041.1.23",
          "form": "맞아요. 리트리버 같은!!",
          "original_form": "맞아요. 리트리버 같은!!",
          "speaker_id": "2",
