      "utterance": [
        {
          "id": "MDRW2100019995.1.1",
          "form": "나중에 코로나 끝나면 전시회에서 그림 보고싶어요",
          "original_form": "나중에 코로나 끝나면 전시회에서 그림 보고싶어요",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019995.1.2",
          "form": "저도 전시회 가고 싶네요~",
          "original_form": "저도 전시회 가고 싶네요~",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019995.1.3",
          "form": "이건희 회장이 기중 특별 전시회하더라구요 요즘",
          "original_form": "이건희 회장이 기중 특별 전시회하더라구요 요즘",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019995.1.4",
          "form": "이건희 회장이 기증한 작품 전시회요ㅎㅎ",
          "original_form": "이건희 회장이 기증한 작품 전시회요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019995.1.5",
          "form": "오 그거 들어봤어요 저도 한번 가보고싶네요",
          "original_form": "오 그거 들어봤어요 저도 한번 가보고싶네요",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019995.1.6",
          "form": "영화관람 좋아하세요?",
          "original_form": "영화관람 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019995.1.7",
          "form": "네 좋아해요. 최근에 극장에서 보신 영화 있나요?",
          "original_form": "네 좋아해요. 최근에 극장에서 보신 영화 있나요?",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019995.1.8",
          "form": "코로나때문에 못간지 오래되었어요 1917이 가장 최근이예요",
          "original_form": "코로나때문에 못간지 오래되었어요 1917이 가장 최근이예요",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019995.1.9",
          "form": "저도 그 영화 극장에서 재밌게 봤어요",
          "original_form": "저도 그 영화 극장에서 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100019995.1.10",
          "form": "주인공이 멋있었어요",
          "original_form": "주인공이 멋있었어요",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019995.1.11",
          "form": "12월달엔 스파이더맨 보러가려구요",
          "original_form": "12월달엔 스파이더맨 보러가려구요",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019995.1.12",
          "form": "저도 그 작품 기대 중이에요",
          "original_form": "저도 그 작품 기대 중이에요",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100019995.1.13",
          "form": "마블영화는 극장에서 큰 화면으로 봐야겠더라구요",
          "original_form": "마블영화는 극장에서 큰 화면으로 봐야겠더라구요",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100019995.1.14",
          "form": "맞아요 cg가 더 와닿아요",
          "original_form": "맞아요 cg가 더 와닿아요",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100019995.1.15",
          "form": "샹치나 이터널스는 관람 계획 없으세요?",
          "original_form": "샹치나 이터널스는 관람 계획 없으세요?",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100019995.1.16",
          "form": "별점 보고 보려고요",
          "original_form": "별점 보고 보려고요",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100019995.1.17",
          "form": "하긴 요새 영화 티켓도 비싸다보니 진짜 보고 싶었던 영화 아니면 극장에 잘 안 가게 되네요",
          "original_form": "하긴 요새 영화 티켓도 비싸다보니 진짜 보고 싶었던 영화 아니면 극장에 잘 안 가게 되네요",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100019995.1.18",
          "form": "전 그래서 조조가격으로 봐요",
          "original_form": "전 그래서 조조가격으로 봐요",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100019995.1.19",
          "form": "팝콘도 안사먹게되었죠",
          "original_form": "팝콘도 안사먹게되었죠",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100019995.1.20",
          "form": "네 저두요. 요즘엔 뮤지컬에 관심이 생겨서 보러갈까 생각 중이에요",
          "original_form": "네 저두요. 요즘엔 뮤지컬에 관심이 생겨서 보러갈까 생각 중이에요",
          "speaker_id": "2",
          "time": "20210827 23:48"
        }
      ]
