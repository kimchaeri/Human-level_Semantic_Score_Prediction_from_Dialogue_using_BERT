      "utterance": [
        {
          "id": "MDRW2100012591.1.1",
          "form": "오늘 뭐 시켜먹을까?",
          "original_form": "오늘 뭐 시켜먹을까?",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012591.1.2",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012591.1.3",
          "form": "치킨?",
          "original_form": "치킨?",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012591.1.4",
          "form": "냄새나는건 안돼",
          "original_form": "냄새나는건 안돼",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012591.1.5",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012591.1.6",
          "form": "에어컨에 냄새 베",
          "original_form": "에어컨에 냄새 베",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012591.1.7",
          "form": "너무 까다롭네",
          "original_form": "너무 까다롭네",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012591.1.8",
          "form": "어쩔수없어",
          "original_form": "어쩔수없어",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012591.1.9",
          "form": "그럼 뭐먹지?",
          "original_form": "그럼 뭐먹지?",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012591.1.10",
          "form": "도시락류 어때",
          "original_form": "도시락류 어때",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012591.1.11",
          "form": "이근처에 맛있는집 없어",
          "original_form": "이근처에 맛있는집 없어",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012591.1.12",
          "form": "30분 거리는 배달 안오나?",
          "original_form": "30분 거리는 배달 안오나?",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012591.1.13",
          "form": "안오지",
          "original_form": "안오지",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012591.1.14",
          "form": "와도 배달팁 엄청 비싸",
          "original_form": "와도 배달팁 엄청 비싸",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012591.1.15",
          "form": "얼마정도인데?",
          "original_form": "얼마정도인데?",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012591.1.16",
          "form": "엄청 비싼대는 8천원까지도 봤어",
          "original_form": "엄청 비싼대는 8천원까지도 봤어",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012591.1.17",
          "form": "음식값보다 더 나가네",
          "original_form": "음식값보다 더 나가네",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012591.1.18",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012591.1.19",
          "form": "배달팁은 다 있는거야?",
          "original_form": "배달팁은 다 있는거야?",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012591.1.20",
          "form": "응 그렇지",
          "original_form": "응 그렇지",
          "speaker_id": "2",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012591.1.21",
          "form": "그럼 싼대는 얼마야",
          "original_form": "그럼 싼대는 얼마야",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012591.1.22",
          "form": "2천원은 엄청싼거고",
          "original_form": "2천원은 엄청싼거고",
          "speaker_id": "2",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012591.1.23",
          "form": "보통 3천원정도 하는것 같아",
          "original_form": "보통 3천원정도 하는것 같아",
          "speaker_id": "2",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012591.1.24",
          "form": "비싸네",
          "original_form": "비싸네",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012591.1.25",
          "form": "이왕이면 제일 배달팁 싼대로해",
          "original_form": "이왕이면 제일 배달팁 싼대로해",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012591.1.26",
          "form": "근데 비싼대가 더 맛있긴해",
          "original_form": "근데 비싼대가 더 맛있긴해",
          "speaker_id": "2",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012591.1.27",
          "form": "이근방에는 잘하는집이 없나보네",
          "original_form": "이근방에는 잘하는집이 없나보네",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012591.1.28",
          "form": "촌동네라그래",
          "original_form": "촌동네라그래",
          "speaker_id": "2",
          "time": "20210824 23:19"
        }
      ]
    }
  ]
