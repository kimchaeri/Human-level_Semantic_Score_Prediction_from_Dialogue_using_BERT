      "utterance": [
        {
          "id": "MDRW2100038038.1.1",
          "form": "이것도 남편의 아이디어일까",
          "original_form": "이것도 남편의 아이디어일까",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100038038.1.2",
          "form": "아닐거 같다ㅋ",
          "original_form": "아닐거 같다ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100038038.1.3",
          "form": "킹덤도 자기 아이디어라는데 ㅋㅋ",
          "original_form": "킹덤도 자기 아이디어라는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038038.1.4",
          "form": "진짜예요?  아닐거 같은데요",
          "original_form": "진짜예요?  아닐거 같은데요",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038038.1.5",
          "form": "김은희 작가 아이디어 아닐까요",
          "original_form": "김은희 작가 아이디어 아닐까요",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100038038.1.6",
          "form": "학교2021도 하나봐 여기서 또 스타들 많이 배출되겠다",
          "original_form": "학교2021도 하나봐 여기서 또 스타들 많이 배출되겠다",
          "speaker_id": "2",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100038038.1.7",
          "form": "가수 김요한이 나오네 학교에",
          "original_form": "가수 김요한이 나오네 학교에",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100038038.1.8",
          "form": "아 김요한 얘도 연기하네요",
          "original_form": "아 김요한 얘도 연기하네요",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100038038.1.9",
          "form": "가수자나요",
          "original_form": "가수자나요",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100038038.1.10",
          "form": "응 학교라는 드라마가 원래 연기력을 요하지는 안잫아",
          "original_form": "응 학교라는 드라마가 원래 연기력을 요하지는 안잫아",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100038038.1.11",
          "form": "그쵸.  그래도 나왔던 애들 다 뜬거 같은데요",
          "original_form": "그쵸.  그래도 나왔던 애들 다 뜬거 같은데요",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100038038.1.12",
          "form": "경찰수업 박민규역으로 나오는 추영우도 학교에 나옴",
          "original_form": "경찰수업 박민규역으로 나오는 추영우도 학교에 나옴",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100038038.1.13",
          "form": "추영우는 처음 들어요",
          "original_form": "추영우는 처음 들어요",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100038038.1.14",
          "form": "추영우 검색해 보께요",
          "original_form": "추영우 검색해 보께요",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100038038.1.15",
          "form": "거기다 슬기로운 의사생활에서 장윤복역의 조이현도 나오고",
          "original_form": "거기다 슬기로운 의사생활에서 장윤복역의 조이현도 나오고",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100038038.1.16",
          "form": "낯선 이름의 연기자들이 많네요",
          "original_form": "낯선 이름의 연기자들이 많네요",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100038038.1.17",
          "form": "추영우는 신인이다 유메이크미댄스랑 경찰수업 밖에 없다",
          "original_form": "추영우는 신인이다 유메이크미댄스랑 경찰수업 밖에 없다",
          "speaker_id": "2",
          "time": "20210901 22:50"
        }
