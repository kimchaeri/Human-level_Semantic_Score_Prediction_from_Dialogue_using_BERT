      "utterance": [
        {
          "id": "MDRW2100028631.1.1",
          "form": "최근에 여행 다녀오셨나요?",
          "original_form": "최근에 여행 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028631.1.2",
          "form": "아니요ㅠㅠ",
          "original_form": "아니요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028631.1.3",
          "form": "코시국때문에..",
          "original_form": "코시국때문에..",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028631.1.4",
          "form": "앗..ㅠㅠㅠ",
          "original_form": "앗..ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028631.1.5",
          "form": "저는 8월초에 부산에 다녀왔어요",
          "original_form": "저는 8월초에 부산에 다녀왔어요",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028631.1.6",
          "form": "와 부산 좋죠!",
          "original_form": "와 부산 좋죠!",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028631.1.7",
          "form": "저는 어렸을때 딱 한 번 가본 것 같아요",
          "original_form": "저는 어렸을때 딱 한 번 가본 것 같아요",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028631.1.8",
          "form": "저는 이번이 3번째에요!",
          "original_form": "저는 이번이 3번째에요!",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028631.1.9",
          "form": "와 가사 뭐하셨나요?",
          "original_form": "와 가사 뭐하셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028631.1.10",
          "form": "가서",
          "original_form": "가서",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028631.1.11",
          "form": "코로나때문에 사람많은 해운대는 안가고",
          "original_form": "코로나때문에 사람많은 해운대는 안가고",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028631.1.12",
          "form": "거의 호캉스처럼 방에만 있었어요",
          "original_form": "거의 호캉스처럼 방에만 있었어요",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028631.1.13",
          "form": "그래도 바다는 보고 오셨죠?",
          "original_form": "그래도 바다는 보고 오셨죠?",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028631.1.14",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028631.1.15",
          "form": "비록 케이블카를 타고 봤지만..",
          "original_form": "비록 케이블카를 타고 봤지만..",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028631.1.16",
          "form": "오 재밌었겠네요ㅋㅋㅋㅋ",
          "original_form": "오 재밌었겠네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028631.1.17",
          "form": "그리고 자갈치시장에 가서 맛있는 것도 먹었어요",
          "original_form": "그리고 자갈치시장에 가서 맛있는 것도 먹었어요",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028631.1.18",
          "form": "저도 코로나가 잠잠해지면 여행 가고 싶네요",
          "original_form": "저도 코로나가 잠잠해지면 여행 가고 싶네요",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028631.1.19",
          "form": "저도 다음에는 꼭 해외여행을 가고 싶어요~",
          "original_form": "저도 다음에는 꼭 해외여행을 가고 싶어요~",
          "speaker_id": "2",
          "time": "20210902 21:13"
        }
      ]
