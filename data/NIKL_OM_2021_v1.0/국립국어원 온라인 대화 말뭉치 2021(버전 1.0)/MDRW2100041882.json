      "utterance": [
        {
          "id": "MDRW2100041882.1.1",
          "form": "저녁 뭐 먹을지",
          "original_form": "저녁 뭐 먹을지",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041882.1.2",
          "form": "정하셨나요",
          "original_form": "정하셨나요",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041882.1.3",
          "form": "아뇨 아직.. 그냥 있는 거 먹으려고요",
          "original_form": "아뇨 아직.. 그냥 있는 거 먹으려고요",
          "speaker_id": "2",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041882.1.4",
          "form": "아합 집에 반찬",
          "original_form": "아합 집에 반찬",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041882.1.5",
          "form": "괜찮은 거 있나요",
          "original_form": "괜찮은 거 있나요",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041882.1.6",
          "form": "그냥 만두 구워먹으려고요",
          "original_form": "그냥 만두 구워먹으려고요",
          "speaker_id": "2",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041882.1.7",
          "form": "오 만두 맛있죠",
          "original_form": "오 만두 맛있죠",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041882.1.8",
          "form": "만두 좋아하세요?",
          "original_form": "만두 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041882.1.9",
          "form": "넵 저 군만두 찐만두 다 좋아해요",
          "original_form": "넵 저 군만두 찐만두 다 좋아해요",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041882.1.10",
          "form": "헉 어디 만두 좋아하세요?",
          "original_form": "헉 어디 만두 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100041882.1.11",
          "form": "음 ... 풀무원이었나? ..",
          "original_form": "음 ... 풀무원이었나? ..",
          "speaker_id": "2",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100041882.1.12",
          "form": "사실 종류 무관하게 잘 먹죠",
          "original_form": "사실 종류 무관하게 잘 먹죠",
          "speaker_id": "2",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100041882.1.13",
          "form": "오홍 그렇군요",
          "original_form": "오홍 그렇군요",
          "speaker_id": "1",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041882.1.14",
          "form": "만두는 비비고꺼",
          "original_form": "만두는 비비고꺼",
          "speaker_id": "1",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041882.1.15",
          "form": "좋아하는 사람 많더라구요",
          "original_form": "좋아하는 사람 많더라구요",
          "speaker_id": "1",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041882.1.16",
          "form": "비비고 .. 막 해외에서도 유명해졌던데!!!!",
          "original_form": "비비고 .. 막 해외에서도 유명해졌던데!!!!",
          "speaker_id": "2",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041882.1.17",
          "form": "네 비비고 만두는",
          "original_form": "네 비비고 만두는",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100041882.1.18",
          "form": "진짜 유명하더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 유명하더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100041882.1.19",
          "form": "ㅠㅠㅠㅋㅋ 뭔가 급성장한 기분이라 신기해요",
          "original_form": "ㅠㅠㅠㅋㅋ 뭔가 급성장한 기분이라 신기해요",
          "speaker_id": "2",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100041882.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ 그렇죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그렇죠",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100041882.1.21",
