      "utterance": [
        {
          "id": "MDRW2100023103.1.1",
          "form": "네 집에는 걍 저두 폼롤러정도",
          "original_form": "네 집에는 걍 저두 폼롤러정도",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023103.1.2",
          "form": "필라테스링이랑",
          "original_form": "필라테스링이랑",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023103.1.3",
          "form": "오 ㅎㅎ 필라테스링이랑 요가링이랑 같은거에요?",
          "original_form": "오 ㅎㅎ 필라테스링이랑 요가링이랑 같은거에요?",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023103.1.4",
          "form": "요가링은 그 종아리에",
          "original_form": "요가링은 그 종아리에",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023103.1.5",
          "form": "하는거 아니에요?",
          "original_form": "하는거 아니에요?",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023103.1.6",
          "form": "어 맞아요 저 이거 첨에 사용할 줄 몰라서",
          "original_form": "어 맞아요 저 이거 첨에 사용할 줄 몰라서",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023103.1.7",
          "form": "낑낑거리다가 남자라서 안맞나보다 하고 포기했었어요",
          "original_form": "낑낑거리다가 남자라서 안맞나보다 하고 포기했었어요",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023103.1.8",
          "form": "필라테스링은 좀 핸들처럼 생겼는데",
          "original_form": "필라테스링은 좀 핸들처럼 생겼는데",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023103.1.9",
          "form": "분명 친구가 발 집어넣는거랬는데 ㅡㅡ;;",
          "original_form": "분명 친구가 발 집어넣는거랬는데 ㅡㅡ;;",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023103.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023103.1.11",
          "form": "발 집어넣긴 커녕 안넣어도 되더라구요;",
          "original_form": "발 집어넣긴 커녕 안넣어도 되더라구요;",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023103.1.12",
          "form": "첨에는 잘 안 들어간다고 하더라구요 ㅋㅋㅋㅋ",
          "original_form": "첨에는 잘 안 들어간다고 하더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023103.1.13",
          "form": "저 되게 다리가 얇은편이거든요",
          "original_form": "저 되게 다리가 얇은편이거든요",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023103.1.14",
          "form": "오 그런데도 안들어가요?",
          "original_form": "오 그런데도 안들어가요?",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023103.1.15",
          "form": "그래서 다행히도 딱 맞아요!",
          "original_form": "그래서 다행히도 딱 맞아요!",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023103.1.16",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023103.1.17",
          "form": "근데 요가링도 이게 사이즈가 있는거에요?",
          "original_form": "근데 요가링도 이게 사이즈가 있는거에요?",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023103.1.18",
          "form": "사이즈 몇개 있었던 거 같아요",
          "original_form": "사이즈 몇개 있었던 거 같아요",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023103.1.19",
          "form": "사이즈 어떻게 확인해요?",
          "original_form": "사이즈 어떻게 확인해요?",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023103.1.20",
          "form": "ㅋㅋㅋㅋ당첨되어서 원사이즈만 받으셨군여",
          "original_form": "ㅋㅋㅋㅋ당첨되어서 원사이즈만 받으셨군여",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023103.1.21",
          "form": "ㅋㅋㅋ네 맞아요...",
          "original_form": "ㅋㅋㅋ네 맞아요...",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023103.1.22",
          "form": "그냥 살때 스몰 라지 이런거 있었던 거같은대",
          "original_form": "그냥 살때 스몰 라지 이런거 있었던 거같은대",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023103.1.23",
          "form": "정확히는 모르겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "정확히는 모르겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:46"
        }
      ]
