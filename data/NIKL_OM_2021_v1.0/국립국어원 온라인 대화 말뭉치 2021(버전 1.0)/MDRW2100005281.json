      "utterance": [
        {
          "id": "MDRW2100005281.1.1",
          "form": "어째서...",
          "original_form": "어째서...",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005281.1.2",
          "form": "여기에 사람이갇혔어요..!!!",
          "original_form": "여기에 사람이갇혔어요..!!!",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005281.1.3",
          "form": "킄킄킄",
          "original_form": "킄킄킄",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005281.1.4",
          "form": "킼킼킼",
          "original_form": "킼킼킼",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005281.1.5",
          "form": "졸개처럼웃지말고대화해요",
          "original_form": "졸개처럼웃지말고대화해요",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005281.1.6",
          "form": "하하하",
          "original_form": "하하하",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005281.1.7",
          "form": "해외여행 갓다온적 잇나요",
          "original_form": "해외여행 갓다온적 잇나요",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005281.1.8",
          "form": "썰풀어주세요",
          "original_form": "썰풀어주세요",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005281.1.9",
          "form": "아니...아까..!!했잖아요...!!!",
          "original_form": "아니...아까..!!했잖아요...!!!",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005281.1.10",
          "form": "이번엔 그쪽썰풀어주세요",
          "original_form": "이번엔 그쪽썰풀어주세요",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005281.1.11",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005281.1.12",
          "form": "전 캄보댜 갓다왓는데",
          "original_form": "전 캄보댜 갓다왓는데",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005281.1.13",
          "form": "강박적 뽕뽑이 유적돌기에",
          "original_form": "강박적 뽕뽑이 유적돌기에",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005281.1.14",
          "form": "진심이었어요",
          "original_form": "진심이었어요",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005281.1.15",
          "form": "과제에 쓰려고 사진 오천장 찍고",
          "original_form": "과제에 쓰려고 사진 오천장 찍고",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005281.1.16",
          "form": "리터럴리 오천장",
          "original_form": "리터럴리 오천장",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005281.1.17",
          "form": "첫날은 하루종일 국립박물관에서 안 나왔어요",
          "original_form": "첫날은 하루종일 국립박물관에서 안 나왔어요",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005281.1.18",
          "form": "와.......무서워요",
          "original_form": "와.......무서워요",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005281.1.19",
          "form": "각.오.하세요",
          "original_form": "각.오.하세요",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005281.1.20",
          "form": "여향이아니라 답사갔다오셧나요..?",
          "original_form": "여향이아니라 답사갔다오셧나요..?",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005281.1.21",
          "form": "아 그치그치 완전 그거였어요",
          "original_form": "아 그치그치 완전 그거였어요",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.22",
          "form": "마침 같이 간 친구 중에",
          "original_form": "마침 같이 간 친구 중에",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.23",
          "form": "사학과가 있어서",
          "original_form": "사학과가 있어서",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.24",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.25",
          "form": "더더욱 본격적인",
          "original_form": "더더욱 본격적인",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.26",
          "form": "답사..",
          "original_form": "답사..",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.27",
          "form": "너무 본격적이에요....",
          "original_form": "너무 본격적이에요....",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.28",
          "form": "재밌는썰풀어주새요",
          "original_form": "재밌는썰풀어주새요",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.29",
          "form": "사진찍고 메모하고 그림그리고",
          "original_form": "사진찍고 메모하고 그림그리고",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.30",
          "form": "눈물겨운설말고",
          "original_form": "눈물겨운설말고",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.32",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.33",
          "form": "아니...?",
          "original_form": "아니...?",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.34",
          "form": "지금 되짚어보니",
          "original_form": "지금 되짚어보니",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005281.1.35",
          "form": "첫날부터 막날까지 답사",
          "original_form": "첫날부터 막날까지 답사",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.36",
          "form": "뿐이네요",
          "original_form": "뿐이네요",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.37",
          "form": "그..건..그..건...",
          "original_form": "그..건..그..건...",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.38",
          "form": "먼근거라도",
          "original_form": "먼근거라도",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.39",
          "form": "먹ㅇ은거라도",
          "original_form": "먹ㅇ은거라도",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.40",
          "form": "말해주새요",
          "original_form": "말해주새요",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.41",
          "form": "마지막날까지도 앙코르와트에",
          "original_form": "마지막날까지도 앙코르와트에",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.42",
          "form": "먹은거...?",
          "original_form": "먹은거...?",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.43",
          "form": "그냥..대충... 끼니 때웠어요",
          "original_form": "그냥..대충... 끼니 때웠어요",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.44",
          "form": "유적 돌아야해서",
          "original_form": "유적 돌아야해서",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.45",
          "form": "너무..너무..너무....",
          "original_form": "너무..너무..너무....",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.46",
          "form": "당신은미쳤어...!",
          "original_form": "당신은미쳤어...!",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.47",
          "form": "사학과 친구가 전통음식에 진심이어서",
          "original_form": "사학과 친구가 전통음식에 진심이어서",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.48",
          "form": "한번 실패하고도",
          "original_form": "한번 실패하고도",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.49",
          "form": "당신들은...",
          "original_form": "당신들은...",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005281.1.50",
          "form": "또 시켰는데",
          "original_form": "또 시켰는데",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005281.1.51",
          "form": "전통음식무슨맛이에요",
          "original_form": "전통음식무슨맛이에요",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005281.1.52",
          "form": "아 진심 식탁에서 음식물스레기 냄새가..",
          "original_form": "아 진심 식탁에서 음식물스레기 냄새가..",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005281.1.53",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005281.1.54",
          "form": "심지어 젤 비싼거였단 말임",
          "original_form": "심지어 젤 비싼거였단 말임",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005281.1.55",
          "form": "둘 다 그랫던 거 보면",
          "original_form": "둘 다 그랫던 거 보면",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005281.1.56",
          "form": "원래 그런 향... 맛....",
          "original_form": "원래 그런 향... 맛....",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005281.1.57",
          "form": "인가봄...",
          "original_form": "인가봄...",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005281.1.58",
          "form": "그렇구나.....",
          "original_form": "그렇구나.....",
          "speaker_id": "2",
          "time": "20210821 21:42"
        }
      ]
