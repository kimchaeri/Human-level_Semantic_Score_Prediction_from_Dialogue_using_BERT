      "utterance": [
        {
          "id": "MDRW2100025003.1.1",
          "form": "땅땅 그 허브후라이드 존맛탱",
          "original_form": "땅땅 그 허브후라이드 존맛탱",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100025003.1.2",
          "form": "허브후라이드?",
          "original_form": "허브후라이드?",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100025003.1.3",
          "form": "사실 안먹은지 몇년 됐어 ㅋㅋㅋ",
          "original_form": "사실 안먹은지 몇년 됐어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100025003.1.4",
          "form": "기억에 남은 건 3번 뿐",
          "original_form": "기억에 남은 건 3번 뿐",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100025003.1.5",
          "form": "ㅋㅋㅋㅋㅋ나도 쫌 된것 같다",
          "original_form": "ㅋㅋㅋㅋㅋ나도 쫌 된것 같다",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025003.1.6",
          "form": "땅땅으로 치팅데이하자",
          "original_form": "땅땅으로 치팅데이하자",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100025003.1.7",
          "form": "ㅋㅋㅋ토요일 치팅각",
          "original_form": "ㅋㅋㅋ토요일 치팅각",
          "speaker_id": "2",
          "time": "20210901 07:45"
        },
        {
          "id": "MDRW2100025003.1.8",
          "form": "아 눈뜨자마자 배고픈디요",
          "original_form": "아 눈뜨자마자 배고픈디요",
          "speaker_id": "2",
          "time": "20210901 07:45"
        },
        {
          "id": "MDRW2100025003.1.9",
          "form": "ㅋㅋㅋㅋㅋ아침 뭐먹엇어",
          "original_form": "ㅋㅋㅋㅋㅋ아침 뭐먹엇어",
          "speaker_id": "1",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025003.1.10",
          "form": "매일두유요",
          "original_form": "매일두유요",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025003.1.11",
          "form": "배고파쥬금요...",
          "original_form": "배고파쥬금요...",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025003.1.12",
          "form": "점심은 뭐먹음ㅋㅋㅋㅋ",
          "original_form": "점심은 뭐먹음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100025003.1.13",
          "form": "회사 사료요...",
          "original_form": "회사 사료요...",
          "speaker_id": "2",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100025003.1.14",
          "form": "국물 비쥬얼은 밍밍 그자체인데",
          "original_form": "국물 비쥬얼은 밍밍 그자체인데",
          "speaker_id": "2",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100025003.1.15",
          "form": "맛이 아주 자극적이었던 것 ㅋㅋㅋ",
          "original_form": "맛이 아주 자극적이었던 것 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100025003.1.16",
          "form": "아주 조미료를 넣엇나보네 ㅋㅋㅋㅋ",
          "original_form": "아주 조미료를 넣엇나보네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100025003.1.17",
          "form": "어어 ㅋㅋㅋ업체가 그거다",
          "original_form": "어어 ㅋㅋㅋ업체가 그거다",
          "speaker_id": "2",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100025003.1.18",
          "form": "본푸드라고 백종원네",
          "original_form": "본푸드라고 백종원네",
          "speaker_id": "2",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100025003.1.19",
          "form": "아 거기 조미료로 유명해?",
          "original_form": "아 거기 조미료로 유명해?",
          "speaker_id": "1",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100025003.1.20",
          "form": "조미료는 잘 모르겠는데..",
          "original_form": "조미료는 잘 모르겠는데..",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025003.1.21",
          "form": "일단 백종원네 브랜드 좀 전체적으로 자극적이지 않나 ㅋㅋㅋ",
          "original_form": "일단 백종원네 브랜드 좀 전체적으로 자극적이지 않나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025003.1.22",
          "form": "그렇군 ㅋㅋㅋㅋ",
          "original_form": "그렇군 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100025003.1.23",
          "form": "나는 점심 만두국먹어써",
          "original_form": "나는 점심 만두국먹어써",
          "speaker_id": "1",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100025003.1.24",
          "form": "ㅋㅋㅋ만두 그 얇은피 인가여",
          "original_form": "ㅋㅋㅋ만두 그 얇은피 인가여",
          "speaker_id": "2",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100025003.1.25",
          "form": "만두국 뜨끈하게 맛잇겟다",
          "original_form": "만두국 뜨끈하게 맛잇겟다",
          "speaker_id": "2",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100025003.1.26",
          "form": "맞음 김치랑 만두 둘다 넣어봣음 ㅋㅋㅋㅋ",
          "original_form": "맞음 김치랑 만두 둘다 넣어봣음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025003.1.27",
          "form": "생각보다 맛잇조러",
          "original_form": "생각보다 맛잇조러",
          "speaker_id": "1",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025003.1.28",
          "form": "ㅋㅋㅋ컈ㅣ난 이따가 땅땅먹어",
          "original_form": "ㅋㅋㅋ컈ㅣ난 이따가 땅땅먹어",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025003.1.29",
          "form": "급발진햇어",
          "original_form": "급발진햇어",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025003.1.30",
          "form": "땅땅 맛있겠다ㅋㅋㅋㅋㅋ",
          "original_form": "땅땅 맛있겠다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
