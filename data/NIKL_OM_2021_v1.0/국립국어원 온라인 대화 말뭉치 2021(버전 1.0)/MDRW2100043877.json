      "utterance": [
        {
          "id": "MDRW2100043877.1.1",
          "form": "ㅋㅋ그러고 보면 귤주스도 맛있었어요..",
          "original_form": "ㅋㅋ그러고 보면 귤주스도 맛있었어요..",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043877.1.2",
          "form": "역시 여행은 식도락인가봅니당",
          "original_form": "역시 여행은 식도락인가봅니당",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043877.1.3",
          "form": "맞아여 ㅋㅋㅋㅋㅋ제주도 맛집어디어디가셨어요",
          "original_form": "맞아여 ㅋㅋㅋㅋㅋ제주도 맛집어디어디가셨어요",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043877.1.4",
          "form": "음..저는 연돈 앞에 있는 삼겹살집이랑 우미노식탁?",
          "original_form": "음..저는 연돈 앞에 있는 삼겹살집이랑 우미노식탁?",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043877.1.5",
          "form": "그리고 특색음식으로는 말고기 먹었어요",
          "original_form": "그리고 특색음식으로는 말고기 먹었어요",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043877.1.6",
          "form": "ㅎㅎ어디어디가셧어요?",
          "original_form": "ㅎㅎ어디어디가셧어요?",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043877.1.7",
          "form": "오 말고기특이하네여",
          "original_form": "오 말고기특이하네여",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043877.1.8",
          "form": "전 칠돈가갔었고 그 딱새우파스타?그거먹으러가고 젤기억남은건 동문시장에서먹은 고등어회여ㅜㅜㅜㅜㅜ",
          "original_form": "전 칠돈가갔었고 그 딱새우파스타?그거먹으러가고 젤기억남은건 동문시장에서먹은 고등어회여ㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043877.1.9",
          "form": "ㅎㅎ네 육회 맛있었어요",
          "original_form": "ㅎㅎ네 육회 맛있었어요",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043877.1.10",
          "form": "오 고등어회는 어떤맛인가요?",
          "original_form": "오 고등어회는 어떤맛인가요?",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043877.1.11",
          "form": "전어랑 비슷한가요?:)",
          "original_form": "전어랑 비슷한가요?:)",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043877.1.12",
          "form": "육회 신기하네여",
          "original_form": "육회 신기하네여",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043877.1.13",
          "form": "전어는 꼬도독꼬도독하잖아여 고등어는 부드러워요",
          "original_form": "전어는 꼬도독꼬도독하잖아여 고등어는 부드러워요",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043877.1.14",
          "form": "고소하고",
          "original_form": "고소하고",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043877.1.15",
          "form": "오 ㅎㅎㅎ",
          "original_form": "오 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100043877.1.16",
          "form": "고등어회 먹어보고 올걸 좀 후회되네요",
          "original_form": "고등어회 먹어보고 올걸 좀 후회되네요",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100043877.1.17",
          "form": "네 좀비린맛나는데 신선해서 그비린맛도맛있었어요",
          "original_form": "네 좀비린맛나는데 신선해서 그비린맛도맛있었어요",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100043877.1.18",
          "form": "아 ㅎㅎㅎ겨울에 한번 도전해봐야겠어요",
          "original_form": "아 ㅎㅎㅎ겨울에 한번 도전해봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100043877.1.19",
          "form": "저는 이제 가을이라 가을전어만 생각했는데",
          "original_form": "저는 이제 가을이라 가을전어만 생각했는데",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100043877.1.20",
          "form": "ㅎㅎㅎ계획이 생겼네요",
          "original_form": "ㅎㅎㅎ계획이 생겼네요",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100043877.1.21",
          "form": "네 아 가을이니까 가을전어 크 먹어줘야져",
          "original_form": "네 아 가을이니까 가을전어 크 먹어줘야져",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100043877.1.22",
          "form": "ㅋㅋㅋㅋㅋㅎㅎ네",
          "original_form": "ㅋㅋㅋㅋㅋㅎㅎ네",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100043877.1.23",
          "form": "사람없는 바닷가 가서 ㅋㅋㅋ먹어야겠어열",
          "original_form": "사람없는 바닷가 가서 ㅋㅋㅋ먹어야겠어열",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100043877.1.24",
          "form": "꼭 바닷가에서 고등어는 신선도가생명이에여",
          "original_form": "꼭 바닷가에서 고등어는 신선도가생명이에여",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100043877.1.25",
          "form": "ㅋㅋㅋ맞아요!",
          "original_form": "ㅋㅋㅋ맞아요!",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100043877.1.26",
          "form": "회는 신선해야죷ㅎㅎ",
          "original_form": "회는 신선해야죷ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:45"
        }
      ]
