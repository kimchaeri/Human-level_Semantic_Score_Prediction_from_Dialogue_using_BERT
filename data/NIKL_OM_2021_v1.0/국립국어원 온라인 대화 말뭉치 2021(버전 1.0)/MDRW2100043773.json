      "utterance": [
        {
          "id": "MDRW2100043773.1.1",
          "form": "좋아하는 연예인 잇으세요?",
          "original_form": "좋아하는 연예인 잇으세요?",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043773.1.2",
          "form": "없어요ㅎㅎ",
          "original_form": "없어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043773.1.3",
          "form": "잇으신가욤",
          "original_form": "잇으신가욤",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043773.1.4",
          "form": "저는 영화덕후에여",
          "original_form": "저는 영화덕후에여",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043773.1.5",
          "form": "오 영화좋아하세요?",
          "original_form": "오 영화좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043773.1.6",
          "form": "인생영화 뭐예요?",
          "original_form": "인생영화 뭐예요?",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043773.1.7",
          "form": "어려운질문이에용",
          "original_form": "어려운질문이에용",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043773.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043773.1.9",
          "form": "꼽기힘드러여",
          "original_form": "꼽기힘드러여",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043773.1.10",
          "form": "ㅋㅋㅋㅋ그럼 최근에 재밋게 본 영화는요?",
          "original_form": "ㅋㅋㅋㅋ그럼 최근에 재밋게 본 영화는요?",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043773.1.11",
          "form": "그거슨",
          "original_form": "그거슨",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043773.1.12",
          "form": "모가디슈임다!!",
          "original_form": "모가디슈임다!!",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043773.1.13",
          "form": "이번주말엔 샹치볼려구욥",
          "original_form": "이번주말엔 샹치볼려구욥",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043773.1.14",
          "form": "아 모가디슈 보셧구나 저는 아직 못봣어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 모가디슈 보셧구나 저는 아직 못봣어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043773.1.15",
          "form": "마블 좋아하세요?",
          "original_form": "마블 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043773.1.16",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043773.1.17",
          "form": "저도 마블좋아해요 ㅎㅎ",
          "original_form": "저도 마블좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043773.1.18",
          "form": "샹치 보셧나욤",
          "original_form": "샹치 보셧나욤",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043773.1.19",
          "form": "아니요 ㅠㅠ 최근에 바빠서 블랙위도우도 아직 못봣어요",
          "original_form": "아니요 ㅠㅠ 최근에 바빠서 블랙위도우도 아직 못봣어요",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043773.1.20",
          "form": "코로나도 잇고 ㅠㅠ",
          "original_form": "코로나도 잇고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043773.1.21",
          "form": "원래 개봉첫날에 기를쓰고 보러갓엇는뎈ㅋㅋㅋㅋㅋ",
          "original_form": "원래 개봉첫날에 기를쓰고 보러갓엇는뎈ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043773.1.22",
          "form": "블랙위도우 진짜 기대안햇는데",
          "original_form": "블랙위도우 진짜 기대안햇는데",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043773.1.23",
          "form": "너무너무너무 재밋엇어요",
          "original_form": "너무너무너무 재밋엇어요",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043773.1.24",
          "form": "ㅠㅠ저도 얼른 봐야겟어요",
          "original_form": "ㅠㅠ저도 얼른 봐야겟어요",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043773.1.25",
          "form": "원래 어떤캐릭터를 좋아하세요?",
          "original_form": "원래 어떤캐릭터를 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043773.1.26",
          "form": "최애는",
          "original_form": "최애는",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043773.1.27",
          "form": "토니요ㅜㅜㅜㅜ",
          "original_form": "토니요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043773.1.28",
          "form": "저 엔드게임때 울엇어유",
          "original_form": "저 엔드게임때 울엇어유",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043773.1.29",
          "form": "저는 토니 최애 아닌데도 엔드게임 보면서 오열햇어욬ㅋㅋㅋㅋ",
          "original_form": "저는 토니 최애 아닌데도 엔드게임 보면서 오열햇어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043773.1.30",
          "form": "아흑ㅜㅜㅜ",
          "original_form": "아흑ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043773.1.31",
          "form": "최애 누구인가욥!!!!!!",
          "original_form": "최애 누구인가욥!!!!!!",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043773.1.32",
          "form": "저는 가오갤시리즈를 제일 좋아해요",
          "original_form": "저는 가오갤시리즈를 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043773.1.33",
          "form": "와씨",
          "original_form": "와씨",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043773.1.34",
          "form": "가오갤3",
          "original_form": "가오갤3",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043773.1.35",
          "form": "레전드아닌가요",
          "original_form": "레전드아닌가요",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043773.1.36",
          "form": "최곱니다",
          "original_form": "최곱니다",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043773.1.37",
          "form": "최애마블영화 가오갤이에요",
          "original_form": "최애마블영화 가오갤이에요",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043773.1.38",
          "form": "가오갤3이요?",
          "original_form": "가오갤3이요?",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043773.1.39",
          "form": "그 욘두죽는편이",
          "original_form": "그 욘두죽는편이",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043773.1.40",
          "form": "2인가요",
          "original_form": "2인가요",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043773.1.41",
          "form": "그편이 최애에여",
          "original_form": "그편이 최애에여",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043773.1.42",
          "form": "ㅠㅠ",
