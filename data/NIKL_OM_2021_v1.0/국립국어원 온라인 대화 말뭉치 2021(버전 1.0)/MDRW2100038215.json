      "utterance": [
        {
          "id": "MDRW2100038215.1.1",
          "form": "게임 하시는거 있으신가요?",
          "original_form": "게임 하시는거 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100038215.1.2",
          "form": "아니요 게임 무식자예요",
          "original_form": "아니요 게임 무식자예요",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038215.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100038215.1.4",
          "form": "폰게임도 안하세요?",
          "original_form": "폰게임도 안하세요?",
          "speaker_id": "1",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100038215.1.5",
          "form": "지금은 안하고 잇어요 ㅋㅋㅋ",
          "original_form": "지금은 안하고 잇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038215.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038215.1.7",
          "form": "아 저는 지금하는건 쿠키런킹덤이요!",
          "original_form": "아 저는 지금하는건 쿠키런킹덤이요!",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038215.1.8",
          "form": "아ㅋㅋㅋㅋ 쿠키들이 귀엽더라구요",
          "original_form": "아ㅋㅋㅋㅋ 쿠키들이 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100038215.1.9",
          "form": "친구가 하는거 구경만 함 ㅋㅋㅋㅋ",
          "original_form": "친구가 하는거 구경만 함 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100038215.1.10",
          "form": "맞아요! 근데 좀... 패턴이 똑같아서 질리더라구요",
          "original_form": "맞아요! 근데 좀... 패턴이 똑같아서 질리더라구요",
          "speaker_id": "1",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100038215.1.11",
          "form": "예전에는 무슨 게임 하셨어요?",
          "original_form": "예전에는 무슨 게임 하셨어요?",
          "speaker_id": "1",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100038215.1.12",
          "form": "요리중독? 재밋게 햇엇던 거 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "요리중독? 재밋게 햇엇던 거 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100038215.1.13",
          "form": "오 저도 그거 했었어요!! 진짜 중독됐었는데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 저도 그거 했었어요!! 진짜 중독됐었는데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100038215.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저도 중독됏엇어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저도 중독됏엇어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100038215.1.15",
          "form": "요즘은 그렇게 모두에게 핫하고 유행하는 그런 게임이 없는거같아요",
          "original_form": "요즘은 그렇게 모두에게 핫하고 유행하는 그런 게임이 없는거같아요",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100038215.1.16",
          "form": "쿠키런 인기많지않아요? 꽤 핫했던것같은데 ㅋㅋㅋ",
          "original_form": "쿠키런 인기많지않아요? 꽤 핫했던것같은데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100038215.1.17",
          "form": "쿠키런은 인기많았는데 킹덤은 반짝하고 말더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "쿠키런은 인기많았는데 킹덤은 반짝하고 말더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100038215.1.18",
          "form": "유저들이 떠나가는게 눈에 보일정도로ㅠㅠ",
          "original_form": "유저들이 떠나가는게 눈에 보일정도로ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100038215.1.19",
          "form": "아 그랫구나 ㅠㅠ",
          "original_form": "아 그랫구나 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100038215.1.20",
          "form": "오래 흥행하는 게임이 되는 길이 쉽지 않은가봐요 ㅠㅠ",
