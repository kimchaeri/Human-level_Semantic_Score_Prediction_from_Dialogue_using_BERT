      "utterance": [
        {
          "id": "MDRW2100024136.1.1",
          "form": "누구 좋아해요 님은",
          "original_form": "누구 좋아해요 님은",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024136.1.2",
          "form": "어디가 좋아요?",
          "original_form": "어디가 좋아요?",
          "speaker_id": "1",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024136.1.3",
          "form": "송중기는 너무 잘생겼으니까요",
          "original_form": "송중기는 너무 잘생겼으니까요",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024136.1.4",
          "form": "잘생긴거 최고",
          "original_form": "잘생긴거 최고",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024136.1.5",
          "form": "송중기는 송혜교꺼 아니였어요?",
          "original_form": "송중기는 송혜교꺼 아니였어요?",
          "speaker_id": "1",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024136.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024136.1.7",
          "form": "이혼한지가 언젠데",
          "original_form": "이혼한지가 언젠데",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024136.1.8",
          "form": "어머 그래요?",
          "original_form": "어머 그래요?",
          "speaker_id": "1",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024136.1.9",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024136.1.10",
          "form": "그 누구지 안재현도 이혼했던데",
          "original_form": "그 누구지 안재현도 이혼했던데",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.11",
          "form": "충격의 이혼이었죠",
          "original_form": "충격의 이혼이었죠",
          "speaker_id": "2",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.12",
          "form": "잘생긴사람이 이혼하는게 유행인가봐요",
          "original_form": "잘생긴사람이 이혼하는게 유행인가봐요",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.13",
          "form": "그니까요 그즈음에 다 이혼해서 충격",
          "original_form": "그니까요 그즈음에 다 이혼해서 충격",
          "speaker_id": "2",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.14",
          "form": "원래 송송커플로 유명하지 않았어요?",
          "original_form": "원래 송송커플로 유명하지 않았어요?",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.15",
          "form": "송혜교는 아무말없이 이혼햇고",
          "original_form": "송혜교는 아무말없이 이혼햇고",
          "speaker_id": "2",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.16",
          "form": "저 그때 자녀 낳으면 되게 예쁜 아기 나올거라고 엄청 부러웠는데 ㅜㅜ",
          "original_form": "저 그때 자녀 낳으면 되게 예쁜 아기 나올거라고 엄청 부러웠는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.17",
          "form": "구혜선은 뭔기 많이 떠들어서 좀 그랬고",
          "original_form": "구혜선은 뭔기 많이 떠들어서 좀 그랬고",
          "speaker_id": "2",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.18",
          "form": "비교가 됐었죠...",
          "original_form": "비교가 됐었죠...",
          "speaker_id": "2",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.19",
          "form": "구혜선 누나 왜그래요 자꾸?",
          "original_form": "구혜선 누나 왜그래요 자꾸?",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.20",
          "form": "ㅋㅋㅋㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋㅋㅋ그러게요",
          "speaker_id": "2",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.21",
          "form": "원래 구혜선 좋아했는데",
          "original_form": "원래 구혜선 좋아했는데",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.22",
          "form": "그 일로 인해서 좀 정떨어졌어요",
          "original_form": "그 일로 인해서 좀 정떨어졌어요",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024136.1.23",
          "form": "송혜교가 뭔가 딩크족이었을거같아요",
          "original_form": "송혜교가 뭔가 딩크족이었을거같아요",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024136.1.24",
          "form": "아~ ㅜㅜ 왜요?",
          "original_form": "아~ ㅜㅜ 왜요?",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024136.1.25",
          "form": "ㅋㅋㅋㅋ뭔가 듣기로는 그런 류의 이혼같더라구여",
          "original_form": "ㅋㅋㅋㅋ뭔가 듣기로는 그런 류의 이혼같더라구여",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024136.1.26",
          "form": "출산과 육아에 대한 가치관 갈등",
          "original_form": "출산과 육아에 대한 가치관 갈등",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024136.1.27",
          "form": "그래도 미성숙한 부모가 되는 것보다 딩크족이 더 낫다고 생각해요!",
          "original_form": "그래도 미성숙한 부모가 되는 것보다 딩크족이 더 낫다고 생각해요!",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024136.1.28",
          "form": "박수 짝짝",
          "original_form": "박수 짝짝",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024136.1.29",
          "form": "저 연예인 중에 좋아하는 사람 있어요",
          "original_form": "저 연예인 중에 좋아하는 사람 있어요",
          "speaker_id": "1",
          "time": "20210830 22:35"
        }
      ]
    }
  ]
