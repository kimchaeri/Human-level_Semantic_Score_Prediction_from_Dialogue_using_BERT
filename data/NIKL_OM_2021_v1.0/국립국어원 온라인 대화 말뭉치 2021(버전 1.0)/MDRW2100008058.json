      "utterance": [
        {
          "id": "MDRW2100008058.1.1",
          "form": "잘 놀다 왔아여??",
          "original_form": "잘 놀다 왔아여??",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008058.1.2",
          "form": "아니 튕겼다",
          "original_form": "아니 튕겼다",
          "speaker_id": "1",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008058.1.3",
          "form": "그집 원이 캠핑가고 싶다해서",
          "original_form": "그집 원이 캠핑가고 싶다해서",
          "speaker_id": "1",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008058.1.4",
          "form": "반도가는길에 튕김....",
          "original_form": "반도가는길에 튕김....",
          "speaker_id": "1",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008058.1.5",
          "form": "아..ㅋㅋㅋㅋ반도 가도 몬했네여 ㅋㅋㅋㅋ",
          "original_form": "아..ㅋㅋㅋㅋ반도 가도 몬했네여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008058.1.6",
          "form": "캠핑가믄 뭐하고 놀아여??",
          "original_form": "캠핑가믄 뭐하고 놀아여??",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008058.1.7",
          "form": "반도갔다가 놀이터에서 자전거타다 큰놈 넘어져가지고",
          "original_form": "반도갔다가 놀이터에서 자전거타다 큰놈 넘어져가지고",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008058.1.8",
          "form": "밴드붙이고 다시 과학관 감",
          "original_form": "밴드붙이고 다시 과학관 감",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008058.1.9",
          "form": "캠핑가믄........\r\n일단 아무것도 없는 자연만 있는곳은 안됨 ㅋㅋㅋ",
          "original_form": "캠핑가믄........\r\n일단 아무것도 없는 자연만 있는곳은 안됨 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008058.1.10",
          "form": "방방이든 놀이터든 수영장이든 동물이든 뭐든 있어야됨",
          "original_form": "방방이든 놀이터든 수영장이든 동물이든 뭐든 있어야됨",
          "speaker_id": "1",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100008058.1.11",
          "form": "맞죠?? 우리도 한번 갔는데 심심허다캐서 걍 왔으요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞죠?? 우리도 한번 갔는데 심심허다캐서 걍 왔으요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008058.1.12",
          "form": "지금 과학관가요??",
          "original_form": "지금 과학관가요??",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008058.1.13",
          "form": "하루가 길구만요 ㅋㅋ",
          "original_form": "하루가 길구만요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008058.1.14",
          "form": "ㅋㅋㅋㅋ 아니 나는 안가고",
          "original_form": "ㅋㅋㅋㅋ 아니 나는 안가고",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008058.1.15",
          "form": "신랑이 애들 데리고 나갔다",
          "original_form": "신랑이 애들 데리고 나갔다",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008058.1.16",
          "form": "신랑 사무실 부장님이 애들 데려나온다해가지고",
          "original_form": "신랑 사무실 부장님이 애들 데려나온다해가지고",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008058.1.17",
          "form": "110동에 튕기고 방황했는데",
          "original_form": "110동에 튕기고 방황했는데",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008058.1.18",
          "form": "덕분에 자유부인됨 ㅋㅋㄱㄱㄱㅋㅋ",
          "original_form": "덕분에 자유부인됨 ㅋㅋㄱㄱㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008058.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ오~잘됐구만요~",
          "original_form": "ㅋㅋㅋㅋㅋㅋ오~잘됐구만요~",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008058.1.20",
          "form": "덕분에 자유부인됨 ㅋㅋㅋㅋ",
          "original_form": "덕분에 자유부인됨 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008058.1.21",
          "form": "엉 ㅋㅋㅋㅋㅋㅋㅋ 밥먹고",
          "original_form": "엉 ㅋㅋㅋㅋㅋㅋㅋ 밥먹고",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008058.1.22",
          "form": "장보러 가야긋다",
          "original_form": "장보러 가야긋다",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008058.1.23",
          "form": "저녁 메뉴는 뭔데여??",
          "original_form": "저녁 메뉴는 뭔데여??",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008058.1.24",
          "form": "하아....저녁.........",
          "original_form": "하아....저녁.........",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008058.1.25",
          "form": "고등어??",
          "original_form": "고등어??",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008058.1.26",
          "form": "생선이 제일 만만하지 ㅋㅋㅋㅋㅋ",
          "original_form": "생선이 제일 만만하지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008058.1.27",
          "form": "내가 야채를 안먹으니",
          "original_form": "내가 야채를 안먹으니",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008058.1.28",
          "form": "애들도 다 야채를 안 좋아함....",
          "original_form": "애들도 다 야채를 안 좋아함....",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008058.1.29",
          "form": "나물 이런것도 천지 안하고",
          "original_form": "나물 이런것도 천지 안하고",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008058.1.30",
          "form": "맨날 고기 아니면 생선 ㅋㅋㅋㄱ",
          "original_form": "맨날 고기 아니면 생선 ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008058.1.31",
          "form": "진짜여?난 생선을 안먹어서ㅠ 고기가 젤 만만함 ㅋㅋㅋㅋ",
          "original_form": "진짜여?난 생선을 안먹어서ㅠ 고기가 젤 만만함 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008058.1.32",
          "form": "다룬반찬 짜다라 필요없고~",
          "original_form": "다룬반찬 짜다라 필요없고~",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008058.1.33",
          "form": "ㅋㅋㅋㅋㄱ 그래 고기가 최고다",
          "original_form": "ㅋㅋㅋㅋㄱ 그래 고기가 최고다",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008058.1.34",
          "form": "고기 어디서 사노??",
          "original_form": "고기 어디서 사노??",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008058.1.35",
          "form": "돼지는 address정육점 가고 소는",
          "original_form": "돼지는 &address&정육점 가고 소는",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008058.1.36",
          "form": "거기 ic에서 오는길에 소고기파는데있는데",
          "original_form": "거기 ic에서 오는길에 소고기파는데있는데",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008058.1.37",
          "form": "address이 엠스퀘어제??",
          "original_form": "&address&이 엠스퀘어제??",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008058.1.38",
          "form": "맛있고 쌈 ㅋㅋㅋㅋ",
          "original_form": "맛있고 쌈 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008058.1.39",
          "form": "얍얍~",
          "original_form": "얍얍~",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008058.1.40",
          "form": "어제 삼겹살 질겨서 맛없드라",
          "original_form": "어제 삼겹살 질겨서 맛없드라",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008058.1.41",
          "form": "우리도 address에서 사는데",
          "original_form": "우리도 &address&에서 사는데",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008058.1.42",
          "form": "정육점 바꿀까 하고 있음",
          "original_form": "정육점 바꿀까 하고 있음",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008058.1.43",
          "form": "언니 삼겹 잘 안사고 대패만사요~",
          "original_form": "언니 삼겹 잘 안사고 대패만사요~",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008058.1.44",
          "form": "삼겹 거기 어느순간 맛없어짐~",
          "original_form": "삼겹 거기 어느순간 맛없어짐~",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008058.1.45",
          "form": "아 그래",
          "original_form": "아 그래",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008058.1.46",
          "form": "대패가 금방 익고 애들도 먹기편해가~",
          "original_form": "대패가 금방 익고 애들도 먹기편해가~",
          "speaker_id": "2",
