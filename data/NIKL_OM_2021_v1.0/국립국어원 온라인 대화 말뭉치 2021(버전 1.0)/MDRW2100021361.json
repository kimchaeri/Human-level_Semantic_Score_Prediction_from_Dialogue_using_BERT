      "utterance": [
        {
          "id": "MDRW2100021361.1.1",
          "form": "일단 부산역으로 보통 오시잖아요?",
          "original_form": "일단 부산역으로 보통 오시잖아요?",
          "speaker_id": "1",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100021361.1.2",
          "form": "부산역에 그집 이라고 곱도리탕 맛집이 있어요",
          "original_form": "부산역에 그집 이라고 곱도리탕 맛집이 있어요",
          "speaker_id": "1",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100021361.1.3",
          "form": "앗 제가 곱창을 못먹어요 ",
          "original_form": "앗 제가 곱창을 못먹어요 ",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100021361.1.4",
          "form": "아하 그럼 바로다음으로",
          "original_form": "아하 그럼 바로다음으로",
          "speaker_id": "1",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100021361.1.5",
          "form": "ㅠㅠ네네",
          "original_form": "ㅠㅠ네네",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100021361.1.6",
          "form": "부산역에서 버스를 타고 조금만가시면 초량 이바구길이 있는데요",
          "original_form": "부산역에서 버스를 타고 조금만가시면 초량 이바구길이 있는데요",
          "speaker_id": "1",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100021361.1.7",
          "form": "거기에 아이유 뮤비에 나왔던",
          "original_form": "거기에 아이유 뮤비에 나왔던",
          "speaker_id": "1",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100021361.1.8",
          "form": "정란각이 있구요",
          "original_form": "정란각이 있구요",
          "speaker_id": "1",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100021361.1.9",
          "form": "와 진짜 첨들어봐요",
          "original_form": "와 진짜 첨들어봐요",
          "speaker_id": "2",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100021361.1.10",
          "form": "그옆에 초량1941이라는 우유 맛집 카페가 있는데",
          "original_form": "그옆에 초량1941이라는 우유 맛집 카페가 있는데",
          "speaker_id": "1",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100021361.1.11",
          "form": "캡쳐할게요..",
          "original_form": "캡쳐할게요..",
          "speaker_id": "2",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100021361.1.12",
          "form": "저는 여기가 더 좋았어요",
          "original_form": "저는 여기가 더 좋았어요",
          "speaker_id": "1",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100021361.1.13",
          "form": "사진이 중요하시면 전자",
          "original_form": "사진이 중요하시면 전자",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021361.1.14",
          "form": "아니면 후자 추천드려요",
          "original_form": "아니면 후자 추천드려요",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021361.1.15",
          "form": "전 맛ㅁ",
          "original_form": "전 맛ㅁ",
          "speaker_id": "2",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021361.1.16",
          "form": "맛은 후자군용",
          "original_form": "맛은 후자군용",
          "speaker_id": "2",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021361.1.17",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021361.1.18",
          "form": "그렇게 카페가셨다가",
          "original_form": "그렇게 카페가셨다가",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021361.1.19",
          "form": "이바구길이 산책길이거든요",
          "original_form": "이바구길이 산책길이거든요",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021361.1.20",
          "form": "산책하시면서 동네구경하다보면",
          "original_form": "산책하시면서 동네구경하다보면",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100021361.1.21",
          "form": "모노레일도 타실수 있구요",
          "original_form": "모노레일도 타실수 있구요",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100021361.1.22",
          "form": "추억의 불량식품파는곳도 있어요",
          "original_form": "추억의 불량식품파는곳도 있어요",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100021361.1.23",
          "form": "오 저 모노레일 한번도 안타봤어요",
          "original_form": "오 저 모노레일 한번도 안타봤어요",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100021361.1.24",
          "form": "그렇게 가셨다가 버스타고 남포동 와서 이재모피자 드시면 하루코스로 좋아요",
          "original_form": "그렇게 가셨다가 버스타고 남포동 와서 이재모피자 드시면 하루코스로 좋아요",
          "speaker_id": "1",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100021361.1.25",
          "form": "이재모피자..",
          "original_form": "이재모피자..",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100021361.1.26",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100021361.1.27",
          "form": "첨 검색하자마자 나오는 피자가 너무 맛있게생겼어요 ㅋㅋ",
          "original_form": "첨 검색하자마자 나오는 피자가 너무 맛있게생겼어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100021361.1.28",
          "form": "앜ㅋㅋㅋ 피자의 기본을 잘하는집이예요!",
          "original_form": "앜ㅋㅋㅋ 피자의 기본을 잘하는집이예요!",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100021361.1.29",
          "form": "오 저 피자 좋아하거든요 ㅎㅎ",
          "original_form": "오 저 피자 좋아하거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:14"
        }
      ]
    }
  ]
