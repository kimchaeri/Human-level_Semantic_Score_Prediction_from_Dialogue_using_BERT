      "utterance": [
        {
          "id": "MDRW2100023760.1.1",
          "form": "종교가 있으신가요?",
          "original_form": "종교가 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 16:47"
        },
        {
          "id": "MDRW2100023760.1.2",
          "form": "저는 기독교예요",
          "original_form": "저는 기독교예요",
          "speaker_id": "2",
          "time": "20210830 16:48"
        },
        {
          "id": "MDRW2100023760.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210830 16:48"
        },
        {
          "id": "MDRW2100023760.1.4",
          "form": "저도 기독교예요, 모태신앙이신가여?",
          "original_form": "저도 기독교예요, 모태신앙이신가여?",
          "speaker_id": "1",
          "time": "20210830 16:59"
        },
        {
          "id": "MDRW2100023760.1.5",
          "form": "네네 부모님이 기독교셔서요",
          "original_form": "네네 부모님이 기독교셔서요",
          "speaker_id": "2",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100023760.1.6",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100023760.1.7",
          "form": "저도 모태신앙이에요ㅋㅋㅋ 저까지 하면 3대네요ㅠ",
          "original_form": "저도 모태신앙이에요ㅋㅋㅋ 저까지 하면 3대네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 18:16"
        },
        {
          "id": "MDRW2100023760.1.8",
          "form": "저도..ㅋㅋㅋ",
          "original_form": "저도..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100023760.1.9",
          "form": "요즘 기독교 평판도 바닥이잖아요 후.ㅈ",
          "original_form": "요즘 기독교 평판도 바닥이잖아요 후.ㅈ",
          "speaker_id": "2",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100023760.1.10",
          "form": "맞아요ㅠ 참...하지 말라면 좀 안 했으면 하는데ㅠ",
          "original_form": "맞아요ㅠ 참...하지 말라면 좀 안 했으면 하는데ㅠ",
          "speaker_id": "1",
          "time": "20210830 18:44"
        },
        {
          "id": "MDRW2100023760.1.11",
          "form": "그니까요 싸잡아서 욕을 먹으니 기분이 참 그래요..",
          "original_form": "그니까요 싸잡아서 욕을 먹으니 기분이 참 그래요..",
          "speaker_id": "2",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023760.1.12",
          "form": "몸집이 너무 큰 게 문제라면 문제겠죠....ㅎ",
          "original_form": "몸집이 너무 큰 게 문제라면 문제겠죠....ㅎ",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023760.1.13",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100023760.1.14",
          "form": "아 그리고 요즘 눈에 띄는 행보를 보이시는분이...",
          "original_form": "아 그리고 요즘 눈에 띄는 행보를 보이시는분이...",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100023760.1.15",
          "form": "ㅎㅎ....정말 흐린 눈을 해도 안 가려지네요ㅠ",
          "original_form": "ㅎㅎ....정말 흐린 눈을 해도 안 가려지네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100023760.1.16",
          "form": "그니까요ㅎㅎ 티비까지 나오시니 가려지지 않아요",
          "original_form": "그니까요ㅎㅎ 티비까지 나오시니 가려지지 않아요",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100023760.1.17",
          "form": "제발 그냥 안에만 계셨으면ㅠㅠ",
          "original_form": "제발 그냥 안에만 계셨으면ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100023760.1.18",
          "form": "그러니까요ㅠ",
          "original_form": "그러니까요ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100023760.1.19",
          "form": "예배는 어떻게 드리고 계세요?",
          "original_form": "예배는 어떻게 드리고 계세요?",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100023760.1.20",
          "form": "유튜브 실시간 중계로 드려요ㅎㅎ",
          "original_form": "유튜브 실시간 중계로 드려요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100023760.1.21",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100023760.1.22",
          "form": "다 같군요ㅋㅋ",
          "original_form": "다 같군요ㅋㅋ",
