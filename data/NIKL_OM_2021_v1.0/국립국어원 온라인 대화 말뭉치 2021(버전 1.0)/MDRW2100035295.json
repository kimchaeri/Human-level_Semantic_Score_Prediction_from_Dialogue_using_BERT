      "utterance": [
        {
          "id": "MDRW2100035295.1.1",
          "form": "후식은 드셨나요 ㅎㅎ",
          "original_form": "후식은 드셨나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035295.1.2",
          "form": "이제 빙수 먹으려구오!",
          "original_form": "이제 빙수 먹으려구오!",
          "speaker_id": "1",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100035295.1.3",
          "form": "드셨나용?",
          "original_form": "드셨나용?",
          "speaker_id": "1",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100035295.1.4",
          "form": "저는 커피 시켰어요 ㅎㅎㅎ!",
          "original_form": "저는 커피 시켰어요 ㅎㅎㅎ!",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035295.1.5",
          "form": "오오~ 식후 커피가 좋죠~",
          "original_form": "오오~ 식후 커피가 좋죠~",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035295.1.6",
          "form": "배달 시키신거에요?",
          "original_form": "배달 시키신거에요?",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035295.1.7",
          "form": "네네 ! 빙수는 설빙 드시나요 ㅎㅎ ?",
          "original_form": "네네 ! 빙수는 설빙 드시나요 ㅎㅎ ?",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100035295.1.8",
          "form": "네!! 망고랑 티라미스요~~",
          "original_form": "네!! 망고랑 티라미스요~~",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100035295.1.9",
          "form": "아 망고 맛있죠 ㅠㅠ",
          "original_form": "아 망고 맛있죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:47"
        },
        {
          "id": "MDRW2100035295.1.10",
          "form": "티라미수 맛도 맛있나요???!",
          "original_form": "티라미수 맛도 맛있나요???!",
          "speaker_id": "2",
          "time": "20210829 14:47"
        },
        {
          "id": "MDRW2100035295.1.11",
          "form": "네! 위에 티라미수 케이크가 뙇! 올라가있어요",
          "original_form": "네! 위에 티라미수 케이크가 뙇! 올라가있어요",
          "speaker_id": "1",
          "time": "20210829 14:57"
        },
        {
          "id": "MDRW2100035295.1.12",
          "form": "헐 맛있겠어요 !",
          "original_form": "헐 맛있겠어요 !",
          "speaker_id": "2",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100035295.1.13",
          "form": "역시 설빙 ㅋㅋㅋ",
          "original_form": "역시 설빙 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100035295.1.14",
          "form": "밑에는 우유얼음이죠???",
          "original_form": "밑에는 우유얼음이죠???",
          "speaker_id": "2",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100035295.1.15",
          "form": "넹!!",
          "original_form": "넹!!",
          "speaker_id": "1",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100035295.1.16",
          "form": "연유에 섞어 먹음 꿀맛이죵",
          "original_form": "연유에 섞어 먹음 꿀맛이죵",
          "speaker_id": "1",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100035295.1.17",
          "form": "ㅜㅜ 먹고싶네요 ㅎㅎ 설빙에 미니붕어빵도 맛있잖아요 !",
          "original_form": "ㅜㅜ 먹고싶네요 ㅎㅎ 설빙에 미니붕어빵도 맛있잖아요 !",
          "speaker_id": "2",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100035295.1.18",
          "form": "헉 저 오로지 빙수먀 먹어봤어요",
          "original_form": "헉 저 오로지 빙수먀 먹어봤어요",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100035295.1.19",
          "form": "다음엔 붕어빵도 먹어봐야겠네요",
          "original_form": "다음엔 붕어빵도 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100035295.1.20",
          "form": "맛있어요 ! 여러가지 맛 있어요 ㅎㅎ",
          "original_form": "맛있어요 ! 여러가지 맛 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100035295.1.21",
          "form": "약간 그 델리만쥬 느낌이에요 ㅋㅋㅋ",
          "original_form": "약간 그 델리만쥬 느낌이에요 ㅋㅋㅋ",
