      "utterance": [
        {
          "id": "MDRW2100024058.1.1",
          "form": "에구 가을이라그런가",
          "original_form": "에구 가을이라그런가",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024058.1.2",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024058.1.3",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024058.1.4",
          "form": "그래서운동해야할까봐요",
          "original_form": "그래서운동해야할까봐요",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024058.1.5",
          "form": "운동 좋죠",
          "original_form": "운동 좋죠",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024058.1.6",
          "form": "전 운동하다 연말에 사진찍으려구요",
          "original_form": "전 운동하다 연말에 사진찍으려구요",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024058.1.7",
          "form": "바프요?",
          "original_form": "바프요?",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024058.1.8",
          "form": "바프까진아니구요",
          "original_form": "바프까진아니구요",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024058.1.9",
          "form": "그럼요?",
          "original_form": "그럼요?",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024058.1.10",
          "form": "그냥사진찍으려구요",
          "original_form": "그냥사진찍으려구요",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024058.1.11",
          "form": "에어비앤비?",
          "original_form": "에어비앤비?",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024058.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024058.1.13",
          "form": "갑자기 저도 하고싶은데요",
          "original_form": "갑자기 저도 하고싶은데요",
          "speaker_id": "1",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024058.1.14",
          "form": "그쵸 뭔가 친구들이랑 예쁜데 놀러가서 사진 예쁘게 찍을 생각하니 의욕돋죠",
          "original_form": "그쵸 뭔가 친구들이랑 예쁜데 놀러가서 사진 예쁘게 찍을 생각하니 의욕돋죠",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024058.1.15",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024058.1.16",
          "form": "어디가지",
          "original_form": "어디가지",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024058.1.17",
          "form": "호캉스도 좋네요",
          "original_form": "호캉스도 좋네요",
          "speaker_id": "1",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024058.1.18",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024058.1.19",
          "form": "엄청 운동해야겠어요",
          "original_form": "엄청 운동해야겠어요",
          "speaker_id": "1",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024058.1.20",
          "form": "곧 구월도오니까요ㅎㅎ",
          "original_form": "곧 구월도오니까요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024058.1.21",
          "form": "1일부터 운동은 진리죠",
          "original_form": "1일부터 운동은 진리죠",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024058.1.22",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "2",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024058.1.23",
          "form": "갑시다!",
          "original_form": "갑시다!",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024058.1.24",
          "form": "합시다!",
          "original_form": "합시다!",
          "speaker_id": "2",
          "time": "20210830 21:46"
        }
      ]
