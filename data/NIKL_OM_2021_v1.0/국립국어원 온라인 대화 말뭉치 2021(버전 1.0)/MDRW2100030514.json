      "utterance": [
        {
          "id": "MDRW2100030514.1.1",
          "form": "무슨요구르트?",
          "original_form": "무슨요구르트?",
          "speaker_id": "2",
          "time": "20210904 02:38"
        },
        {
          "id": "MDRW2100030514.1.2",
          "form": "아나 파운드케익 왔어!",
          "original_form": "아나 파운드케익 왔어!",
          "speaker_id": "2",
          "time": "20210904 02:38"
        },
        {
          "id": "MDRW2100030514.1.3",
          "form": "ㅋㅋㅋ 먹어봄?",
          "original_form": "ㅋㅋㅋ 먹어봄?",
          "speaker_id": "1",
          "time": "20210904 02:38"
        },
        {
          "id": "MDRW2100030514.1.4",
          "form": "ㅇㅇ아몬드먹어봄ㅋㅋ",
          "original_form": "ㅇㅇ아몬드먹어봄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:39"
        },
        {
          "id": "MDRW2100030514.1.5",
          "form": "괜찮나? ㅋㅋㅋㅋ",
          "original_form": "괜찮나? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:39"
        },
        {
          "id": "MDRW2100030514.1.6",
          "form": "응응근데 음료필수ㅋㅋ 맛잇게먹음",
          "original_form": "응응근데 음료필수ㅋㅋ 맛잇게먹음",
          "speaker_id": "2",
          "time": "20210904 02:39"
        },
        {
          "id": "MDRW2100030514.1.7",
          "form": "난 블루베리가 제일 맛있던데 퍽퍽하니 ㅋㅋㅋ 블베 멜론 바나나 아몬드 소보루 순으로 좋더라",
          "original_form": "난 블루베리가 제일 맛있던데 퍽퍽하니 ㅋㅋㅋ 블베 멜론 바나나 아몬드 소보루 순으로 좋더라",
          "speaker_id": "1",
          "time": "20210904 02:39"
        },
        {
          "id": "MDRW2100030514.1.8",
          "form": "작아보이는데 하나 다 먹으려니까 많더라ㅠㅜ",
          "original_form": "작아보이는데 하나 다 먹으려니까 많더라ㅠㅜ",
          "speaker_id": "2",
          "time": "20210904 02:39"
        },
        {
          "id": "MDRW2100030514.1.9",
          "form": "오 블베괜찮아?",
          "original_form": "오 블베괜찮아?",
          "speaker_id": "2",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100030514.1.10",
          "form": "어 좀 은근 양 있디 ㅋㅋㅋ",
          "original_form": "어 좀 은근 양 있디 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100030514.1.11",
          "form": "ㅇㅇ 난 그게 좋던데 ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 난 그게 좋던데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100030514.1.12",
          "form": "엄빠픽은 멜론",
          "original_form": "엄빠픽은 멜론",
          "speaker_id": "1",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100030514.1.13",
          "form": "내일은 블베먹어봐야겠다ㅌㅋ",
          "original_form": "내일은 블베먹어봐야겠다ㅌㅋ",
          "speaker_id": "2",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100030514.1.14",
          "form": "바나나도 ㄱㅊ",
          "original_form": "바나나도 ㄱㅊ",
          "speaker_id": "1",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100030514.1.15",
          "form": "난 사실 한번에 두개도먹을스있어...",
          "original_form": "난 사실 한번에 두개도먹을스있어...",
          "speaker_id": "1",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100030514.1.16",
          "form": "ㅋㅋㅋㅋㅋ엄마가 왜 하나만 샀냐고ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ엄마가 왜 하나만 샀냐고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100030514.1.17",
          "form": "아진짜? ㅋㅋㅋㅋ괜찮으셨나보네 ㅋㅋㅋㅋ",
          "original_form": "아진짜? ㅋㅋㅋㅋ괜찮으셨나보네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100030514.1.18",
          "form": "그거 세일자주해 ㅋㅋ",
          "original_form": "그거 세일자주해 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100030514.1.19",
          "form": "한번씩 봐야겠다ㅋㅋ",
          "original_form": "한번씩 봐야겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:42"
        }
      ]
