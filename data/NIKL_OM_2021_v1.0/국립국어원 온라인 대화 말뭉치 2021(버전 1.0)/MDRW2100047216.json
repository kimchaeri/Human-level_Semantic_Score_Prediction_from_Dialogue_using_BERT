      "utterance": [
        {
          "id": "MDRW2100047216.1.1",
          "form": "아 맞다 보톡스나 필러 맞은 사람 있나여 주변에",
          "original_form": "아 맞다 보톡스나 필러 맞은 사람 있나여 주변에",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100047216.1.2",
          "form": "움.  잘없는듯 왜?",
          "original_form": "움.  잘없는듯 왜?",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100047216.1.3",
          "form": "엄마가 주변에서 권유를 당하심",
          "original_form": "엄마가 주변에서 권유를 당하심",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100047216.1.4",
          "form": "같이 맞으러가자고 ㅋㅋ",
          "original_form": "같이 맞으러가자고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100047216.1.5",
          "form": "오우 어디어디?",
          "original_form": "오우 어디어디?",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100047216.1.6",
          "form": "이마?",
          "original_form": "이마?",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100047216.1.7",
          "form": "미간이라고 해야하나요",
          "original_form": "미간이라고 해야하나요",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100047216.1.8",
          "form": "그 내천자 부분?",
          "original_form": "그 내천자 부분?",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100047216.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100047216.1.10",
          "form": "괜찮은데",
          "original_form": "괜찮은데",
          "speaker_id": "1",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100047216.1.11",
          "form": "나도 이마주름 없애고파ㅜㅜㅜ",
          "original_form": "나도 이마주름 없애고파ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100047216.1.12",
          "form": "5만원이래요",
          "original_form": "5만원이래요",
          "speaker_id": "2",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100047216.1.13",
          "form": "의외로 싼거같기도",
          "original_form": "의외로 싼거같기도",
          "speaker_id": "2",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100047216.1.14",
          "form": "안비싸네",
          "original_form": "안비싸네",
          "speaker_id": "1",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100047216.1.15",
          "form": "필러는 모르겠고 보톡스만 맞는건 가격이 많임내려간듯",
          "original_form": "필러는 모르겠고 보톡스만 맞는건 가격이 많임내려간듯",
          "speaker_id": "2",
          "time": "20210826 08:02"
        },
        {
          "id": "MDRW2100047216.1.16",
          "form": "보편화돼서 많이들 맞나보당",
          "original_form": "보편화돼서 많이들 맞나보당",
          "speaker_id": "1",
          "time": "20210826 09:17"
        },
        {
          "id": "MDRW2100047216.1.17",
          "form": "근데 백신 맞고는 못 맞는데요",
          "original_form": "근데 백신 맞고는 못 맞는데요",
          "speaker_id": "2",
          "time": "20210826 09:19"
        },
        {
          "id": "MDRW2100047216.1.18",
          "form": "아 백신은 쉬어야제 사우나도 안된대",
          "original_form": "아 백신은 쉬어야제 사우나도 안된대",
          "speaker_id": "1",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100047216.1.19",
          "form": "그래서 일단은 보류상태",
          "original_form": "그래서 일단은 보류상태",
          "speaker_id": "2",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100047216.1.20",
          "form": "비포애프터가 참 궁금하다는 ㅋㅋ",
          "original_form": "비포애프터가 참 궁금하다는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100047216.1.21",
          "form": "오 나는 친구한거 봤는데",
          "original_form": "오 나는 친구한거 봤는데",
          "speaker_id": "1",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100047216.1.22",
          "form": "단기적으로 있댕",
          "original_form": "단기적으로 있댕",
          "speaker_id": "1",
          "time": "20210826 09:29"
        },
        {
          "id": "MDRW2100047216.1.23",
          "form": "효과가",
          "original_form": "효과가",
          "speaker_id": "1",
          "time": "20210826 09:29"
        }
      ]
