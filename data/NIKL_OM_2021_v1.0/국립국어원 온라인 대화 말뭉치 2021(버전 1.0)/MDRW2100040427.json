      "utterance": [
        {
          "id": "MDRW2100040427.1.1",
          "form": "휴 집안일 하시나요",
          "original_form": "휴 집안일 하시나요",
          "speaker_id": "1",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100040427.1.2",
          "form": "당연하죠ㅋㅋ유교가 살아있어요ㅋㅋ",
          "original_form": "당연하죠ㅋㅋ유교가 살아있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:06"
        },
        {
          "id": "MDRW2100040427.1.3",
          "form": "ㅋㅋㅋ몇가족인가요?",
          "original_form": "ㅋㅋㅋ몇가족인가요?",
          "speaker_id": "1",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100040427.1.4",
          "form": "저흰 5명이요?",
          "original_form": "저흰 5명이요?",
          "speaker_id": "2",
          "time": "20210905 11:56"
        },
        {
          "id": "MDRW2100040427.1.5",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210905 11:56"
        },
        {
          "id": "MDRW2100040427.1.6",
          "form": "몇분이세요?",
          "original_form": "몇분이세요?",
          "speaker_id": "2",
          "time": "20210905 11:56"
        },
        {
          "id": "MDRW2100040427.1.7",
          "form": "우와 그중 역할이 뭔가요",
          "original_form": "우와 그중 역할이 뭔가요",
          "speaker_id": "1",
          "time": "20210905 12:35"
        },
        {
          "id": "MDRW2100040427.1.8",
          "form": "전 4명인데 나와살아요ㅋㅋ",
          "original_form": "전 4명인데 나와살아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:35"
        },
        {
          "id": "MDRW2100040427.1.9",
          "form": "저요? 분라너? ㅋㅋ저는 중재자라 생각하는데ㅋㅋ",
          "original_form": "저요? 분라너? ㅋㅋ저는 중재자라 생각하는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:36"
        },
        {
          "id": "MDRW2100040427.1.10",
          "form": "ㅋㅋㅋ 둘째?",
          "original_form": "ㅋㅋㅋ 둘째?",
          "speaker_id": "1",
          "time": "20210905 12:44"
        },
        {
          "id": "MDRW2100040427.1.11",
          "form": "네ㅋㅋ 중간에 꼈어요ㅋㅋ",
          "original_form": "네ㅋㅋ 중간에 꼈어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100040427.1.12",
          "form": "안싸우나요? ㅋㅋ",
          "original_form": "안싸우나요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100040427.1.13",
          "form": "ㅋㅋㅋ네 요즘은 ㅋㅋ",
          "original_form": "ㅋㅋㅋ네 요즘은 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100040427.1.14",
          "form": "둘째들이 성격이 좋드라구요",
          "original_form": "둘째들이 성격이 좋드라구요",
          "speaker_id": "1",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100040427.1.15",
          "form": "ㅋㅋ저희도 요즘엔ㅋㅋ 안싸워요ㅋㅋ 지쳐요ㅋㅋ",
          "original_form": "ㅋㅋ저희도 요즘엔ㅋㅋ 안싸워요ㅋㅋ 지쳐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:00"
        },
        {
          "id": "MDRW2100040427.1.16",
          "form": "ㅋㅋㅋㅋ 남녀섞였나요",
          "original_form": "ㅋㅋㅋㅋ 남녀섞였나요",
          "speaker_id": "1",
          "time": "20210905 13:00"
        },
        {
          "id": "MDRW2100040427.1.17",
          "form": "ㅋㅋ남동생이랑 언니요",
          "original_form": "ㅋㅋ남동생이랑 언니요",
          "speaker_id": "2",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040427.1.18",
          "form": "ㅋㅋㅋ 남동생 말 잘들어요?",
          "original_form": "ㅋㅋㅋ 남동생 말 잘들어요?",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040427.1.19",
          "form": "어후...ㅜㅠ 지가 제일 잘났다고 생각하기에 누나말은 네버.. ㅋㄲ",
