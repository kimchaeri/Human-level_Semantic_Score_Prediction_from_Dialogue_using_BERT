      "utterance": [
        {
          "id": "MDRW2100020607.1.1",
          "form": "요즘 날씨 어떤 것 같아요?",
          "original_form": "요즘 날씨 어떤 것 같아요?",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020607.1.2",
          "form": "덥다 춥다 하는 거 같아요",
          "original_form": "덥다 춥다 하는 거 같아요",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020607.1.3",
          "form": "밤에는 좀 많이 춥더라고요 ㅎㅎ !",
          "original_form": "밤에는 좀 많이 춥더라고요 ㅎㅎ !",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020607.1.4",
          "form": "ㅋㅋㅋ 그래도 한여름보단 낫죠?",
          "original_form": "ㅋㅋㅋ 그래도 한여름보단 낫죠?",
          "speaker_id": "1",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020607.1.5",
          "form": "요새 소나기 자주 내렸는데 그건 별로였어요 ㅜㅜ",
          "original_form": "요새 소나기 자주 내렸는데 그건 별로였어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100020607.1.6",
          "form": "그니까요!!!!!!!",
          "original_form": "그니까요!!!!!!!",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020607.1.7",
          "form": "집에 있을째 비 오면 좋은데",
          "original_form": "집에 있을째 비 오면 좋은데",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020607.1.8",
          "form": "밖에 잇으면 싫어요",
          "original_form": "밖에 잇으면 싫어요",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020607.1.9",
          "form": "ㅋㅋㅋ 공감!",
          "original_form": "ㅋㅋㅋ 공감!",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020607.1.10",
          "form": "집에서 비내리는 소리 들으면 뭔가 편안해져요",
          "original_form": "집에서 비내리는 소리 들으면 뭔가 편안해져요",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020607.1.11",
          "form": "맞아요! 그리고 파전도 먹으면",
          "original_form": "맞아요! 그리고 파전도 먹으면",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020607.1.12",
          "form": "완전 짱이잖아요 ㅎㅎ",
          "original_form": "완전 짱이잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020607.1.13",
          "form": "비올때 파전 드세요~?!",
          "original_form": "비올때 파전 드세요~?!",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020607.1.14",
          "form": "파전보다는... 김치전이나 부추전이요 ㅎㅎ",
          "original_form": "파전보다는... 김치전이나 부추전이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100020607.1.15",
          "form": "그게 더 맛있는 것 같아요",
          "original_form": "그게 더 맛있는 것 같아요",
          "speaker_id": "1",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100020607.1.16",
          "form": "헛 맞아요 김치전 맛있잖요 ㅎ.ㅎ",
          "original_form": "헛 맞아요 김치전 맛있잖요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020607.1.17",
          "form": "일기예보 자주 챙겨보세요?!",
          "original_form": "일기예보 자주 챙겨보세요?!",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020607.1.18",
          "form": "거의 매일 챙겨보는 편이에요",
          "original_form": "거의 매일 챙겨보는 편이에요",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020607.1.19",
          "form": "그런데 최근에는 보고도 비 쫄딱 맞았어요 ㅋㅋ",
          "original_form": "그런데 최근에는 보고도 비 쫄딱 맞았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020607.1.20",
          "form": "헛 전 안 챙겨봐서 항상 쫄딱 젖어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헛 전 안 챙겨봐서 항상 쫄딱 젖어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020607.1.21",
          "form": "우산 맨날 사서 4개 쌓여있고요 ㅎㅎㅎ",
          "original_form": "우산 맨날 사서 4개 쌓여있고요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020607.1.22",
          "form": "앗~~정말 ㅜㅜ",
          "original_form": "앗~~정말 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020607.1.23",
          "form": "쓰지도 않는 비닐 우산 너무 아까워요",
          "original_form": "쓰지도 않는 비닐 우산 너무 아까워요",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020607.1.24",
          "form": "요즘 하나에 얼마 해요?",
          "original_form": "요즘 하나에 얼마 해요?",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020607.1.25",
          "form": "가게마다 다른데 편의점에서는 비닐 우산도 5천원은 기본이더라고요..",
          "original_form": "가게마다 다른데 편의점에서는 비닐 우산도 5천원은 기본이더라고요..",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020607.1.26",
          "form": "많이 올랐네요...",
          "original_form": "많이 올랐네요...",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020607.1.27",
          "form": "예전엔 2천원.. 비싸야 3천원이었던 것 같은데",
          "original_form": "예전엔 2천원.. 비싸야 3천원이었던 것 같은데",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020607.1.28",
          "form": "5천원 주고 사기 아깝지 않아요?ㅜㅜ",
          "original_form": "5천원 주고 사기 아깝지 않아요?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020607.1.29",
          "form": "아깝지만....   ... 비가 너무 와서 어쩔 수 없더라고요..",
          "original_form": "아깝지만....   ... 비가 너무 와서 어쩔 수 없더라고요..",
          "speaker_id": "2",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020607.1.30",
          "form": "다음부터는 정신차리고 일기예보 보려고요 %.%",
          "original_form": "다음부터는 정신차리고 일기예보 보려고요 %.%",
