        },
        {
          "id": "MDRW2100005334.1.2",
          "form": "런닝화랑 슬리퍼",
          "original_form": "런닝화랑 슬리퍼",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005334.1.3",
          "form": "왜 산 거야?",
          "original_form": "왜 산 거야?",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005334.1.4",
          "form": "세일해서 샀어 ㅋㅋㅋ",
          "original_form": "세일해서 샀어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005334.1.5",
          "form": "세일했어?",
          "original_form": "세일했어?",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005334.1.6",
          "form": "응 대박 세일해서 안살수가 없더라",
          "original_form": "응 대박 세일해서 안살수가 없더라",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005334.1.7",
          "form": "오 많이 저렴했나보구나",
          "original_form": "오 많이 저렴했나보구나",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005334.1.8",
          "form": "응 마침 런닝화 필요하기도 했고",
          "original_form": "응 마침 런닝화 필요하기도 했고",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005334.1.9",
          "form": "오 운동하려고?",
          "original_form": "오 운동하려고?",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005334.1.10",
          "form": "응 ㅋㅋㅋ 걷기 운동 해보려고",
          "original_form": "응 ㅋㅋㅋ 걷기 운동 해보려고",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005334.1.11",
          "form": "운동 좋지",
          "original_form": "운동 좋지",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005334.1.12",
          "form": "신발 진짜 편하다",
          "original_form": "신발 진짜 편하다",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005334.1.13",
          "form": "요즘 런닝화 잘나오더라",
          "original_form": "요즘 런닝화 잘나오더라",
          "speaker_id": "2",
