      "utterance": [
        {
          "id": "MDRW2100003374.1.1",
          "form": "덥다 낼 비온다는데",
          "original_form": "덥다 낼 비온다는데",
          "speaker_id": "2",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100003374.1.2",
          "form": "오늘 하루종일 비 오나봐ㅠ",
          "original_form": "오늘 하루종일 비 오나봐ㅠ",
          "speaker_id": "1",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003374.1.3",
          "form": "가을장마라서 계속 비가 온다고",
          "original_form": "가을장마라서 계속 비가 온다고",
          "speaker_id": "2",
          "time": "20210821 11:10"
        },
        {
          "id": "MDRW2100003374.1.4",
          "form": "날씨가 너무 짜증나는게 비가 왔다 맑았다 왔다 해",
          "original_form": "날씨가 너무 짜증나는게 비가 왔다 맑았다 왔다 해",
          "speaker_id": "1",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003374.1.5",
          "form": "그러게 비가 오니 더위가 가신건 좋은데",
          "original_form": "그러게 비가 오니 더위가 가신건 좋은데",
          "speaker_id": "2",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003374.1.6",
          "form": "어떨땐 약간 춥기도 해",
          "original_form": "어떨땐 약간 춥기도 해",
          "speaker_id": "1",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003374.1.7",
          "form": "좀 있으면 난방해야할듯하다",
          "original_form": "좀 있으면 난방해야할듯하다",
          "speaker_id": "2",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003374.1.8",
          "form": "계속 더울줄 알았더만 가을이 오긴 하네",
          "original_form": "계속 더울줄 알았더만 가을이 오긴 하네",
          "speaker_id": "1",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003374.1.9",
          "form": "비가 끝나고 조금 더웠다가 다시 추워지지 않겠나",
          "original_form": "비가 끝나고 조금 더웠다가 다시 추워지지 않겠나",
          "speaker_id": "2",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003374.1.10",
          "form": "추석 지나면 추워지지 않을까?",
          "original_form": "추석 지나면 추워지지 않을까?",
          "speaker_id": "1",
          "time": "20210821 11:14"
        },
        {
          "id": "MDRW2100003374.1.11",
          "form": "추석때는 항상 추워지기 시작하더라 어찌 그리 잘 맞는지",
          "original_form": "추석때는 항상 추워지기 시작하더라 어찌 그리 잘 맞는지",
          "speaker_id": "2",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003374.1.12",
          "form": "그러게 계절은 거짓말을 안해",
          "original_form": "그러게 계절은 거짓말을 안해",
          "speaker_id": "1",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003374.1.13",
          "form": "절기에 맞추어서 그렇게 잘 맞을 수 가 없다",
          "original_form": "절기에 맞추어서 그렇게 잘 맞을 수 가 없다",
          "speaker_id": "2",
          "time": "20210821 11:18"
        }
