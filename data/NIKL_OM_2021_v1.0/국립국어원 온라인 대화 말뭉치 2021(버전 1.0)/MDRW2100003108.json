        {
          "id": "MDRW2100003108.1.1",
          "form": "오늘도 고생 많았어",
          "original_form": "오늘도 고생 많았어",
          "speaker_id": "1",
          "time": "20210820 23:06"
        },
        {
          "id": "MDRW2100003108.1.2",
          "form": "고생은 무슴",
          "original_form": "고생은 무슴",
          "speaker_id": "2",
          "time": "20210820 23:06"
        },
        {
          "id": "MDRW2100003108.1.3",
          "form": "집안일 많이 못도와줘서 미안",
          "original_form": "집안일 많이 못도와줘서 미안",
          "speaker_id": "1",
          "time": "20210820 23:07"
        },
        {
          "id": "MDRW2100003108.1.4",
          "form": "늦게 와서 그렇자나 괜찮아~",
          "original_form": "늦게 와서 그렇자나 괜찮아~",
          "speaker_id": "2",
          "time": "20210820 23:08"
        },
        {
          "id": "MDRW2100003108.1.5",
          "form": "이해해 줘서 고마워",
          "original_form": "이해해 줘서 고마워",
          "speaker_id": "1",
          "time": "20210820 23:08"
        },
        {
          "id": "MDRW2100003108.1.6",
          "form": "별개다 고맙다 ㅎㅎ",
          "original_form": "별개다 고맙다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003108.1.7",
          "form": "주말인데 낼도 출근해서  같이 못있네",
          "original_form": "주말인데 낼도 출근해서  같이 못있네",
          "speaker_id": "1",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003108.1.8",
          "form": "회사 바쁘니 이해하오 !!",
          "original_form": "회사 바쁘니 이해하오 !!",
          "speaker_id": "2",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003108.1.9",
          "form": "일요일엔  name3랑 다같이 드라이브 가자",
          "original_form": "일요일엔  &name3&랑 다같이 드라이브 가자",
          "speaker_id": "1",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003108.1.10",
          "form": "오케이 좋아요!",
          "original_form": "오케이 좋아요!",
          "speaker_id": "2",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003108.1.11",
          "form": "어디로 갈까?",
          "original_form": "어디로 갈까?",
          "speaker_id": "1",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003108.1.12",
          "form": "가까운데가 좋겠지~",
          "original_form": "가까운데가 좋겠지~",
          "speaker_id": "2",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003108.1.13",
          "form": "음 바다 보고 올까?",
          "original_form": "음 바다 보고 올까?",
          "speaker_id": "1",
          "time": "20210820 23:12"
        },
        {
          "id": "MDRW2100003108.1.14",
          "form": "오케이 콜 !!!",
          "original_form": "오케이 콜 !!!",
          "speaker_id": "2",
          "time": "20210820 23:12"
        },
        {
          "id": "MDRW2100003108.1.15",
          "form": "요번주는 오빠 많이 바쁘니 분리수거는 내가 할께",
