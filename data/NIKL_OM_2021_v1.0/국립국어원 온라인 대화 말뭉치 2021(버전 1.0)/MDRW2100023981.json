      "utterance": [
        {
          "id": "MDRW2100023981.1.1",
          "form": "자주 머거야 하는데",
          "original_form": "자주 머거야 하는데",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023981.1.2",
          "form": "몇달 안샀네요",
          "original_form": "몇달 안샀네요",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023981.1.3",
          "form": "ㅠㅠ 장봐야되는데 ㅠㅠ",
          "original_form": "ㅠㅠ 장봐야되는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023981.1.4",
          "form": "닭다리살이 더 좋지만 그건",
          "original_form": "닭다리살이 더 좋지만 그건",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023981.1.5",
          "form": "닭브레스트보다 비싸요",
          "original_form": "닭브레스트보다 비싸요",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023981.1.6",
          "form": "ㅇㅇ 비싸요ㅠㅠ",
          "original_form": "ㅇㅇ 비싸요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023981.1.7",
          "form": "전 냉동 닭브레스트살 사서 카레에 넣어먹어요",
          "original_form": "전 냉동 닭브레스트살 사서 카레에 넣어먹어요",
          "speaker_id": "1",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023981.1.8",
          "form": "브레스트살만 갖곤 맛 없어서 못 먹겠어요",
          "original_form": "브레스트살만 갖곤 맛 없어서 못 먹겠어요",
          "speaker_id": "1",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023981.1.9",
          "form": "맛이 없으니 싼거에요",
          "original_form": "맛이 없으니 싼거에요",
          "speaker_id": "2",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100023981.1.10",
          "form": "싼데 부위도 많으니",
          "original_form": "싼데 부위도 많으니",
          "speaker_id": "2",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100023981.1.11",
          "form": "지방이 너무 적어서 고무같아요",
          "original_form": "지방이 너무 적어서 고무같아요",
          "speaker_id": "2",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100023981.1.12",
          "form": "마저요ㅠㅠ 그래도 염지해서 치킨으로 나온 건 먹을만 한데ㅠㅠ",
          "original_form": "마저요ㅠㅠ 그래도 염지해서 치킨으로 나온 건 먹을만 한데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100023981.1.13",
          "form": "염지는 집에서도 할수있어요",
          "original_form": "염지는 집에서도 할수있어요",
          "speaker_id": "2",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100023981.1.14",
          "form": "귀찮을뿐,,",
          "original_form": "귀찮을뿐,,",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023981.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 집에서 하지 않는 방향으로 갑시다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 집에서 하지 않는 방향으로 갑시다",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023981.1.16",
          "form": "직접해야 저렴한걸요",
          "original_form": "직접해야 저렴한걸요",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023981.1.17",
          "form": "ㅠㅠㅠㅠ 저렴함을 포기하고 편리함을 사겠습니다",
          "original_form": "ㅠㅠㅠㅠ 저렴함을 포기하고 편리함을 사겠습니다",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023981.1.18",
          "form": "식품회사가 좋아하실 타입인데요!",
          "original_form": "식품회사가 좋아하실 타입인데요!",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023981.1.19",
          "form": "1인 가구는 어쩔 수 없어요~",
          "original_form": "1인 가구는 어쩔 수 없어요~",
          "speaker_id": "1",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023981.1.20",
          "form": "가공식품이 점점 느는걸 보면 이게 대세같긴 해요",
          "original_form": "가공식품이 점점 느는걸 보면 이게 대세같긴 해요",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023981.1.21",
          "form": "마저요. 지금까지의 식생활은 집안일만 하는 사람이 있다는 가정 하에서나 가능한 수준이라고 생각해요",
          "original_form": "마저요. 지금까지의 식생활은 집안일만 하는 사람이 있다는 가정 하에서나 가능한 수준이라고 생각해요",
          "speaker_id": "1",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100023981.1.22",
          "form": "음식의 기준을 낮추는 것도 방법이에요",
          "original_form": "음식의 기준을 낮추는 것도 방법이에요",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023981.1.23",
          "form": "오트밀 닭브레스트 토마토오이",
          "original_form": "오트밀 닭브레스트 토마토오이",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023981.1.24",
          "form": "이런 식단으로 7일을 드시면",
          "original_form": "이런 식단으로 7일을 드시면",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023981.1.25",
          "form": "식사준비 3분컷 가능",
          "original_form": "식사준비 3분컷 가능",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023981.1.26",
          "form": "아주 건강해지겠네요...",
          "original_form": "아주 건강해지겠네요...",
          "speaker_id": "1",
          "time": "20210830 20:58"
        }
      ]
    }
