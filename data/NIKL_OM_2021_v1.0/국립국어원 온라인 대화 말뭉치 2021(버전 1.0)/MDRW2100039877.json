      "utterance": [
        {
          "id": "MDRW2100039877.1.1",
          "form": "어떤색 사셨어요",
          "original_form": "어떤색 사셨어요",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039877.1.2",
          "form": "저는!",
          "original_form": "저는!",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039877.1.3",
          "form": "티 흰색이랑 검정 사긴했는데",
          "original_form": "티 흰색이랑 검정 사긴했는데",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039877.1.4",
          "form": "또 보다보니 모나미룩이네요ㅋㅋㅋㅋㅋ",
          "original_form": "또 보다보니 모나미룩이네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039877.1.5",
          "form": "모나미가 최고에여",
          "original_form": "모나미가 최고에여",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039877.1.6",
          "form": "전 이번에 이케아 조명 샀는데",
          "original_form": "전 이번에 이케아 조명 샀는데",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039877.1.7",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039877.1.8",
          "form": "안돼요ㅠㅠ 모나미룩..",
          "original_form": "안돼요ㅠㅠ 모나미룩..",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039877.1.9",
          "form": "헐 이케아",
          "original_form": "헐 이케아",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039877.1.10",
          "form": "조명 저도 사고싶은데...",
          "original_form": "조명 저도 사고싶은데...",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039877.1.11",
          "form": "오천원이었어요",
          "original_form": "오천원이었어요",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039877.1.12",
          "form": "만족도 오만프로입니다",
          "original_form": "만족도 오만프로입니다",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039877.1.13",
          "form": "헉 오천원이요???대박대박",
          "original_form": "헉 오천원이요???대박대박",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039877.1.14",
          "form": "사진보고싶은데",
          "original_form": "사진보고싶은데",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039877.1.15",
          "form": "사진기능이 없네요",
          "original_form": "사진기능이 없네요",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039877.1.16",
          "form": "그러네요ㅠㅠ",
          "original_form": "그러네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039877.1.17",
          "form": "이름이 뭐더라ㅠㅠ",
          "original_form": "이름이 뭐더라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039877.1.18",
          "form": "나중에 생각나면 말해주세요!!!!",
          "original_form": "나중에 생각나면 말해주세요!!!!",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039877.1.19",
          "form": "나중에 이건 사야겠다 싶은 물건있으신가요??",
          "original_form": "나중에 이건 사야겠다 싶은 물건있으신가요??",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039877.1.20",
          "form": "저는 침대랑 전신거울요ㅋㅋㅋ",
          "original_form": "저는 침대랑 전신거울요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039877.1.21",
          "form": "있었는데 박살나고 깨져서... 히히..",
          "original_form": "있었는데 박살나고 깨져서... 히히..",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039877.1.22",
          "form": "음 나중에 식기세척기 사고파요",
          "original_form": "음 나중에 식기세척기 사고파요",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039877.1.23",
          "form": "전 침대 버리고 토퍼살까봐요",
          "original_form": "전 침대 버리고 토퍼살까봐요",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039877.1.24",
          "form": "강아지랑 같이 자서",
          "original_form": "강아지랑 같이 자서",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039877.1.25",
          "form": "식기세척기 짱이죠 진짜 최고...",
          "original_form": "식기세척기 짱이죠 진짜 최고...",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039877.1.26",
          "form": "있으신가요",
          "original_form": "있으신가요",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039877.1.27",
          "form": "그것도 좋아요 스타일러",
          "original_form": "그것도 좋아요 스타일러",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039877.1.28",
          "form": "와 스타일러 ㅠㅠ",
          "original_form": "와 스타일러 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039877.1.29",
          "form": "있었는데 좀돼서 고장났어요ㅜㅜ",
          "original_form": "있었는데 좀돼서 고장났어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039877.1.30",
          "form": "스타일러ㅠㅠ 요즘같은때에는 필요해요...",
          "original_form": "스타일러ㅠㅠ 요즘같은때에는 필요해요...",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039877.1.31",
          "form": "미세먼지랑 바이러스 으..",
          "original_form": "미세먼지랑 바이러스 으..",
          "speaker_id": "2",
