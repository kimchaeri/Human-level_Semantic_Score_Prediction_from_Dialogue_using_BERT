      "utterance": [
        {
          "id": "MDRW2100006303.1.1",
          "form": "누가 주도했던 건가요",
          "original_form": "누가 주도했던 건가요",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100006303.1.2",
          "form": "혼자가면 한그릇 밖에 못먹잖아요ㅋㅋ",
          "original_form": "혼자가면 한그릇 밖에 못먹잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100006303.1.3",
          "form": "아아 ㅋㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006303.1.4",
          "form": "종류도 하나고...",
          "original_form": "종류도 하나고...",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006303.1.5",
          "form": "마자요 종류는 다양해야 좋죠",
          "original_form": "마자요 종류는 다양해야 좋죠",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006303.1.6",
          "form": "누가 주도하셨었나요",
          "original_form": "누가 주도하셨었나요",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006303.1.7",
          "form": "이번엔 남자친구가요ㅋㅋㅋ운전을 제가 안해서",
          "original_form": "이번엔 남자친구가요ㅋㅋㅋ운전을 제가 안해서",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006303.1.8",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006303.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006303.1.10",
          "form": "멀리갈땐 본인 컨디션에 따라 가능한 날만 갑니다",
          "original_form": "멀리갈땐 본인 컨디션에 따라 가능한 날만 갑니다",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006303.1.11",
          "form": "도로변에 있었나 봐요",
          "original_form": "도로변에 있었나 봐요",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006303.1.12",
          "form": "가게가",
          "original_form": "가게가",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006303.1.13",
          "form": "당일치기라 체력도 시간도 받쳐줘야될듯해요",
          "original_form": "당일치기라 체력도 시간도 받쳐줘야될듯해요",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006303.1.14",
          "form": "그나마 차가 있어서",
          "original_form": "그나마 차가 있어서",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006303.1.15",
          "form": "당일치기라고 해도",
          "original_form": "당일치기라고 해도",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006303.1.16",
          "form": "의외로 폭이 넓을 거 같긴 한데",
          "original_form": "의외로 폭이 넓을 거 같긴 한데",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006303.1.17",
          "form": "네네 어디 알려진 여행지 근처 도로변이에요",
          "original_form": "네네 어디 알려진 여행지 근처 도로변이에요",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006303.1.18",
          "form": "그죠ㅋㅋㅋ차가 있으면",
          "original_form": "그죠ㅋㅋㅋ차가 있으면",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006303.1.19",
          "form": "오오...",
          "original_form": "오오...",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006303.1.20",
          "form": "안타깝게도 저는 아직",
          "original_form": "안타깝게도 저는 아직",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006303.1.21",
          "form": "확실히 폭이 다양해지죠",
          "original_form": "확실히 폭이 다양해지죠",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006303.1.22",
          "form": "차가 없어서;;",
          "original_form": "차가 없어서;;",
