      "utterance": [
        {
          "id": "MDRW2100012505.1.1",
          "form": "연예인 누구좋아하세여",
          "original_form": "연예인 누구좋아하세여",
          "speaker_id": "1",
          "time": "20210824 22:37"
        },
        {
          "id": "MDRW2100012505.1.2",
          "form": "저 악동뮤지션 이수현이요",
          "original_form": "저 악동뮤지션 이수현이요",
          "speaker_id": "2",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100012505.1.3",
          "form": "노래 너무 잘하져",
          "original_form": "노래 너무 잘하져",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012505.1.4",
          "form": "매력있엉",
          "original_form": "매력있엉",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012505.1.5",
          "form": "얼굴이 제 스타일임",
          "original_form": "얼굴이 제 스타일임",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012505.1.6",
          "form": "노래는 잘 몰라용",
          "original_form": "노래는 잘 몰라용",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012505.1.7",
          "form": "아 진짜요? 아니 노래도 유명한데...!",
          "original_form": "아 진짜요? 아니 노래도 유명한데...!",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012505.1.8",
          "form": "최근껀 잘 몰라요ㅋㅋ 걍 엄청 유명한건 아는데",
          "original_form": "최근껀 잘 몰라요ㅋㅋ 걍 엄청 유명한건 아는데",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012505.1.9",
          "form": "사실 뭐 어떤사람인지는 잘 모르는데 얼굴 특징들이 제 취향인거죠 뭐",
          "original_form": "사실 뭐 어떤사람인지는 잘 모르는데 얼굴 특징들이 제 취향인거죠 뭐",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012505.1.10",
          "form": "ㅋㅋㅋㅋ저는 이동욱 좋아해요",
          "original_form": "ㅋㅋㅋㅋ저는 이동욱 좋아해요",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100012505.1.11",
          "form": "남친분이 이동욱 닮으셨나봐요",
          "original_form": "남친분이 이동욱 닮으셨나봐요",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100012505.1.12",
          "form": "...????????",
          "original_form": "...????????",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012505.1.13",
          "form": "아뇨????",
          "original_form": "아뇨????",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012505.1.14",
          "form": "그게 그렇게 정색할 일인가요",
          "original_form": "그게 그렇게 정색할 일인가요",
          "speaker_id": "2",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100012505.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100012505.1.16",
          "form": "아늬...이동욱님은...일반인이 아니잖아요...",
          "original_form": "아늬...이동욱님은...일반인이 아니잖아요...",
          "speaker_id": "1",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012505.1.17",
          "form": "남치니는 음... 제 눈에는 송재림 조금 닮았어요",
          "original_form": "남치니는 음... 제 눈에는 송재림 조금 닮았어요",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012505.1.18",
          "form": "첨들어보는 이름인데",
          "original_form": "첨들어보는 이름인데",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012505.1.19",
          "form": "찾아보니 어떤 스타일인지 알겠네요",
          "original_form": "찾아보니 어떤 스타일인지 알겠네요",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012505.1.20",
          "form": "길쭉길쭉하게 생긴거 좋아하시는구나",
          "original_form": "길쭉길쭉하게 생긴거 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012505.1.21",
          "form": "길쭉길쭉ㅋㅋㅋ",
          "original_form": "길쭉길쭉ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100012505.1.22",
          "form": "얼굴이요?",
          "original_form": "얼굴이요?",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100012505.1.23",
          "form": "전체적으로 길쭉길쭉",
          "original_form": "전체적으로 길쭉길쭉",
          "speaker_id": "2",
          "time": "20210824 23:46"
        }
      ]
    }
