      "utterance": [
        {
          "id": "MDRW2100033619.1.1",
          "form": "이제 해가 나기 시작했어요ㅡ",
          "original_form": "이제 해가 나기 시작했어요ㅡ",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100033619.1.2",
          "form": "여기도 비그치고 구름사이로 해살짝보여요",
          "original_form": "여기도 비그치고 구름사이로 해살짝보여요",
          "speaker_id": "2",
          "time": "20210824 17:30"
        },
        {
          "id": "MDRW2100033619.1.3",
          "form": "이번주 이제 비안오는걸까요?",
          "original_form": "이번주 이제 비안오는걸까요?",
          "speaker_id": "1",
          "time": "20210824 17:30"
        },
        {
          "id": "MDRW2100033619.1.4",
          "form": "예보를 못봐서",
          "original_form": "예보를 못봐서",
          "speaker_id": "1",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100033619.1.5",
          "form": "수요일까지로봤는데 태풍도지나갔으니 내일은 그치지않을까요",
          "original_form": "수요일까지로봤는데 태풍도지나갔으니 내일은 그치지않을까요",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100033619.1.6",
          "form": "가을옷도 사야되는데 ㅎ",
          "original_form": "가을옷도 사야되는데 ㅎ",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100033619.1.7",
          "form": "찾아보기도 귀찮고",
          "original_form": "찾아보기도 귀찮고",
          "speaker_id": "1",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100033619.1.8",
          "form": "저도요. 작년엔 벗고살았나봐요",
          "original_form": "저도요. 작년엔 벗고살았나봐요",
          "speaker_id": "2",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100033619.1.9",
          "form": "옷 어떤거 입었는지 입을께 없네요",
          "original_form": "옷 어떤거 입었는지 입을께 없네요",
          "speaker_id": "2",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100033619.1.10",
          "form": "모든이들의 공통점인듯요ㅎ",
          "original_form": "모든이들의 공통점인듯요ㅎ",
          "speaker_id": "1",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100033619.1.11",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100033619.1.12",
          "form": "간절기 옷사봐야  몇번입지도 못하니 사야될지 말아야될지고민",
          "original_form": "간절기 옷사봐야  몇번입지도 못하니 사야될지 말아야될지고민",
          "speaker_id": "1",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100033619.1.13",
          "form": "저거 나쁜말아닌가보네요ㅋ",
          "original_form": "저거 나쁜말아닌가보네요ㅋ",
          "speaker_id": "1",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100033619.1.14",
          "form": "온집안이 끈적거리네요",
          "original_form": "온집안이 끈적거리네요",
          "speaker_id": "2",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100033619.1.15",
          "form": "맨날 걸러지더만ㅋ",
          "original_form": "맨날 걸러지더만ㅋ",
          "speaker_id": "1",
          "time": "20210824 17:35"
        },
        {
          "id": "MDRW2100033619.1.16",
          "form": "에어컨 말고 제습기트셔요ㅎ",
          "original_form": "에어컨 말고 제습기트셔요ㅎ",
          "speaker_id": "1",
          "time": "20210824 17:35"
        },
        {
          "id": "MDRW2100033619.1.17",
          "form": "벗고? 요거요?",
          "original_form": "벗고? 요거요?",
          "speaker_id": "2",
          "time": "20210824 17:35"
        },
        {
          "id": "MDRW2100033619.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210824 17:35"
        },
        {
          "id": "MDRW2100033619.1.19",
          "form": "제습기는 방하나에 틀어놨어요.  빨래 말리느라",
          "original_form": "제습기는 방하나에 틀어놨어요.  빨래 말리느라",
          "speaker_id": "2",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100033619.1.20",
          "form": "요런날은 건조기가 필수인데",
          "original_form": "요런날은 건조기가 필수인데",
          "speaker_id": "1",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100033619.1.21",
          "form": "그러게요.  주식이 대박나야하는데 ㅋㅋ",
          "original_form": "그러게요.  주식이 대박나야하는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100033619.1.22",
          "form": "ㅋㅋ  공모주로 벌어서 하나 장만하시는걸로",
          "original_form": "ㅋㅋ  공모주로 벌어서 하나 장만하시는걸로",
          "speaker_id": "1",
          "time": "20210824 17:38"
        }
      ]
