      "utterance": [
        {
          "id": "MDRW2100030386.1.1",
          "form": "게임 판타지 중에서 나는 될 놈이다를 읽고 있어요",
          "original_form": "게임 판타지 중에서 나는 될 놈이다를 읽고 있어요",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030386.1.2",
          "form": "음성인식하니 밤 샐 수 있을 거 같아요",
          "original_form": "음성인식하니 밤 샐 수 있을 거 같아요",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030386.1.3",
          "form": "매일 매일 한 편씩 올리는데",
          "original_form": "매일 매일 한 편씩 올리는데",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030386.1.4",
          "form": "게임 판타지는 뭐예요",
          "original_form": "게임 판타지는 뭐예요",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030386.1.5",
          "form": "벌써 5년 째에요",
          "original_form": "벌써 5년 째에요",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030386.1.6",
          "form": "가상현실 게임속에서 캐릭터 키우는 내용이에요",
          "original_form": "가상현실 게임속에서 캐릭터 키우는 내용이에요",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030386.1.7",
          "form": "5년 연재했다고요",
          "original_form": "5년 연재했다고요",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030386.1.8",
          "form": "메타 버스 같은 거 줘",
          "original_form": "메타 버스 같은 거 줘",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.9",
          "form": "완결된 후에 보는게 낫겠네요",
          "original_form": "완결된 후에 보는게 낫겠네요",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.10",
          "form": "네 엄청 길죠",
          "original_form": "네 엄청 길죠",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.11",
          "form": "시작하지 말아야죠",
          "original_form": "시작하지 말아야죠",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.12",
          "form": "이제 후반부 들어가요",
          "original_form": "이제 후반부 들어가요",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.13",
          "form": "이렇게 길 줄 나도 몰랐죠",
          "original_form": "이렇게 길 줄 나도 몰랐죠",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.14",
          "form": "뒤가 궁금하니까",
          "original_form": "뒤가 궁금하니까",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.15",
          "form": "초반부터 봤거든요",
          "original_form": "초반부터 봤거든요",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.16",
          "form": "안타깝네요",
          "original_form": "안타깝네요",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.17",
          "form": "아직도 재밌어요",
          "original_form": "아직도 재밌어요",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.18",
          "form": "완결되면 이야기해 주세요",
          "original_form": "완결되면 이야기해 주세요",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.19",
          "form": "매일매일 기다려 줘서 괜찮아요",
          "original_form": "매일매일 기다려 줘서 괜찮아요",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.20",
          "form": "몰라서 보게",
          "original_form": "몰라서 보게",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030386.1.21",
          "form": "주인공이랑 같이 늙어 가겠네요",
          "original_form": "주인공이랑 같이 늙어 가겠네요",
          "speaker_id": "1",
          "time": "20210904 00:25"
        }
      ]
