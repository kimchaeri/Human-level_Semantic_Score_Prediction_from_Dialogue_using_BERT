      "utterance": [
        {
          "id": "MDRW2100030435.1.1",
          "form": "게임 하다 보면 잠이 확 달아나지",
          "original_form": "게임 하다 보면 잠이 확 달아나지",
          "speaker_id": "2",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100030435.1.2",
          "form": "그러다가 밤 새요",
          "original_form": "그러다가 밤 새요",
          "speaker_id": "1",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100030435.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.4",
          "form": "몇 시에 자나요",
          "original_form": "몇 시에 자나요",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.5",
          "form": "어제는 4시 넘어서 잔 듯",
          "original_form": "어제는 4시 넘어서 잔 듯",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.6",
          "form": "자는 동안도 게임은 돌아가죠",
          "original_form": "자는 동안도 게임은 돌아가죠",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.7",
          "form": "게임 하느라 늦게 잤나요",
          "original_form": "게임 하느라 늦게 잤나요",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.8",
          "form": "튀김 키우기는 자동으로 건전 오키 기능 해 놨어요",
          "original_form": "튀김 키우기는 자동으로 건전 오키 기능 해 놨어요",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.9",
          "form": "그래도 일찍 일어나네요",
          "original_form": "그래도 일찍 일어나네요",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.10",
          "form": "책 읽는다고 늦게 잤어요",
          "original_form": "책 읽는다고 늦게 잤어요",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.11",
          "form": "게임도 중간에 한 번씩 돌려줘야 해요",
          "original_form": "게임도 중간에 한 번씩 돌려줘야 해요",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.12",
          "form": "책 읽으면 잠 오지 않나요",
          "original_form": "책 읽으면 잠 오지 않나요",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.13",
          "form": "대단하네요 부지런해요",
          "original_form": "대단하네요 부지런해요",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.14",
          "form": "로맨스 소설이라 잠이 깨 더군요",
          "original_form": "로맨스 소설이라 잠이 깨 더군요",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.15",
          "form": "성공할 거예요",
          "original_form": "성공할 거예요",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.16",
          "form": "궁금해서 잠이 안 오게 써요",
          "original_form": "궁금해서 잠이 안 오게 써요",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030435.1.17",
          "form": "미션 걸린 거 빨리 깨서 몇 개 성공했어요",
          "original_form": "미션 걸린 거 빨리 깨서 몇 개 성공했어요",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030435.1.18",
          "form": "완결난게 아니라서 다 못 읽었어요",
          "original_form": "완결난게 아니라서 다 못 읽었어요",
          "speaker_id": "2",
          "time": "20210904 01:01"
        }
      ]
