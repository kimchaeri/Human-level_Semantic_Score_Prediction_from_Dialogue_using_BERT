      "utterance": [
        {
          "id": "MDRW2100000891.1.1",
          "form": "마미~ 내 트랜치코트 하나 샀당",
          "original_form": "마미~ 내 트랜치코트 하나 샀당",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000891.1.2",
          "form": "트렌치코트를 왜 샀어",
          "original_form": "트렌치코트를 왜 샀어",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000891.1.3",
          "form": "내 하나도 없으니까 그렇징~",
          "original_form": "내 하나도 없으니까 그렇징~",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000891.1.4",
          "form": "30만원 넘는거 3만얼마에 삼 ㅋㅋ",
          "original_form": "30만원 넘는거 3만얼마에 삼 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000891.1.5",
          "form": "싸게 잘 샀네",
          "original_form": "싸게 잘 샀네",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000891.1.6",
          "form": "어디서 샀는데",
          "original_form": "어디서 샀는데",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000891.1.7",
          "form": "인터넷에서 ㅋㅋ 11번가에서 샀당",
          "original_form": "인터넷에서 ㅋㅋ 11번가에서 샀당",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000891.1.8",
          "form": "안 좋은 옷 아니야",
          "original_form": "안 좋은 옷 아니야",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000891.1.9",
          "form": "ㄴㄴ 사람들 후기가 괜찮더라~",
          "original_form": "ㄴㄴ 사람들 후기가 괜찮더라~",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000891.1.10",
          "form": "잘했다",
          "original_form": "잘했다",
          "speaker_id": "2",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000891.1.11",
          "form": "엄마랑 같이 입으면 되겠네",
          "original_form": "엄마랑 같이 입으면 되겠네",
          "speaker_id": "2",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000891.1.12",
          "form": "구랭 ㅋㅋ 사이즈 넉넉한듯",
          "original_form": "구랭 ㅋㅋ 사이즈 넉넉한듯",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000891.1.13",
          "form": "좋으네",
          "original_form": "좋으네",
          "speaker_id": "2",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000891.1.14",
          "form": "ㅎㅎ 겨울 되기전에 뽕 뽑아야지",
          "original_form": "ㅎㅎ 겨울 되기전에 뽕 뽑아야지",
          "speaker_id": "1",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000891.1.15",
          "form": "그래 금방 추워져서 못 입는다",
          "original_form": "그래 금방 추워져서 못 입는다",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000891.1.16",
          "form": "그니깡 ㅋㅋ 빨리 배송 오면 좋겠다",
          "original_form": "그니깡 ㅋㅋ 빨리 배송 오면 좋겠다",
          "speaker_id": "1",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000891.1.17",
          "form": "엄마 내 겨울 옷좀 사야겠다",
          "original_form": "엄마 내 겨울 옷좀 사야겠다",
