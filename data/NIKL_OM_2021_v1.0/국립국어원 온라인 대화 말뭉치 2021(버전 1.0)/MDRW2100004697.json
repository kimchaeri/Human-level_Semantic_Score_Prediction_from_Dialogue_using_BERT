      "utterance": [
        {
          "id": "MDRW2100004697.1.1",
          "form": "name2님 앵무새 무슨 색이에요?",
          "original_form": "&name2&님 앵무새 무슨 색이에요?",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004697.1.2",
          "form": "색도 앵무마다 다 다르죠?",
          "original_form": "색도 앵무마다 다 다르죠?",
          "speaker_id": "1",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004697.1.3",
          "form": "노란색인데 얼굴은 좀 빨개요!!",
          "original_form": "노란색인데 얼굴은 좀 빨개요!!",
          "speaker_id": "2",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004697.1.4",
          "form": "맞아요 저희는 골든체리모란이에요~=",
          "original_form": "맞아요 저희는 골든체리모란이에요~=",
          "speaker_id": "2",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004697.1.5",
          "form": "근데 새들이나 동물들 다 색이 넘 예뻐요ㅠㅠ",
          "original_form": "근데 새들이나 동물들 다 색이 넘 예뻐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004697.1.6",
          "form": "우와 너무 귀여울 거 같아요",
          "original_form": "우와 너무 귀여울 거 같아요",
          "speaker_id": "1",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004697.1.7",
          "form": "name3라는 이름이랑 딱 맞는 색이네요",
          "original_form": "&name3&라는 이름이랑 딱 맞는 색이네요",
          "speaker_id": "1",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004697.1.8",
          "form": "ㅎㅎㅎㅎ맞아요",
          "original_form": "ㅎㅎㅎㅎ맞아요",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004697.1.9",
          "form": "name1님은 고양이들한테",
          "original_form": "&name1&님은 고양이들한테",
          "speaker_id": "2",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004697.1.10",
          "form": "간식 준 적 있으세요??",
          "original_form": "간식 준 적 있으세요??",
          "speaker_id": "2",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004697.1.11",
          "form": "네 저 가끔 츄르 줘요!",
          "original_form": "네 저 가끔 츄르 줘요!",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004697.1.12",
          "form": "물도 주고..",
          "original_form": "물도 주고..",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004697.1.13",
          "form": "오오 저희 아파트에서는 그런 게 금지예요ㅜㅜ",
          "original_form": "오오 저희 아파트에서는 그런 게 금지예요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004697.1.14",
          "form": "친구 고양이한테 선물해봐야겠어요",
          "original_form": "친구 고양이한테 선물해봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004697.1.15",
          "form": "요새 금지하는 곳도 많더라구요ㅠㅠ",
          "original_form": "요새 금지하는 곳도 많더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004697.1.16",
          "form": "맞아요 밤에 고양이들 소리가 시끄럽긴했어요ㅠ",
          "original_form": "맞아요 밤에 고양이들 소리가 시끄럽긴했어요ㅠ",
          "speaker_id": "2",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004697.1.17",
          "form": "근데 친구네 보니까 고양이들은 장난감 사줘도 그 장난감보다 박스를 좋아하더라구요",
          "original_form": "근데 친구네 보니까 고양이들은 장난감 사줘도 그 장난감보다 박스를 좋아하더라구요",
          "speaker_id": "1",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004697.1.18",
          "form": "ㅋㅋㅋㅋ그래서 집사들은 속상해하는..ㅠ",
          "original_form": "ㅋㅋㅋㅋ그래서 집사들은 속상해하는..ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004697.1.19",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ 고양이들 특징..",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ 고양이들 특징..",
          "speaker_id": "2",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004697.1.20",
          "form": "너무 웃겨요 장난감은 거들떠보지도 않고",
          "original_form": "너무 웃겨요 장난감은 거들떠보지도 않고",
          "speaker_id": "1",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004697.1.21",
          "form": "그리고 제가 본 대부분의 고양이들은 엄청 차분하더라구요..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그리고 제가 본 대부분의 고양이들은 엄청 차분하더라구요..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004697.1.22",
          "form": "주인이 더 신나하는 장난감",
          "original_form": "주인이 더 신나하는 장난감",
          "speaker_id": "2",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004697.1.23",
          "form": "맞아요ㅋㅋㅋ애들마다 성격이 다 달라서 그런지",
          "original_form": "맞아요ㅋㅋㅋ애들마다 성격이 다 달라서 그런지",
          "speaker_id": "1",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004697.1.24",
          "form": "같은 집 고양이들도 하나는 엄청 뛰어다니구 하나는 얌전했어요",
          "original_form": "같은 집 고양이들도 하나는 엄청 뛰어다니구 하나는 얌전했어요",
          "speaker_id": "1",
          "time": "20210821 18:10"
        }
      ]
