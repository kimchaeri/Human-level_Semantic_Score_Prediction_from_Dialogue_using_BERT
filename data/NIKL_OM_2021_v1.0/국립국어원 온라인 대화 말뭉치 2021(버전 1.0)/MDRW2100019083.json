      "utterance": [
        {
          "id": "MDRW2100019083.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019083.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019083.1.3",
          "form": "오늘 점심 뭐드셨나요",
          "original_form": "오늘 점심 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100019083.1.4",
          "form": "닭볶음탕에 밥이랑 같이 먹었어요",
          "original_form": "닭볶음탕에 밥이랑 같이 먹었어요",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019083.1.5",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019083.1.6",
          "form": "네네 저는 시리얼로 떼웠어요",
          "original_form": "네네 저는 시리얼로 떼웠어요",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019083.1.7",
          "form": "저녁메뉴 뭐 먹을지 추천좀 해주세요",
          "original_form": "저녁메뉴 뭐 먹을지 추천좀 해주세요",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019083.1.8",
          "form": "요즘 날씨가 쌀쌀해서 얼큰한 국물이 땡기더라고요",
          "original_form": "요즘 날씨가 쌀쌀해서 얼큰한 국물이 땡기더라고요",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019083.1.9",
          "form": "칼국수나 짬뽕 어떠세요?",
          "original_form": "칼국수나 짬뽕 어떠세요?",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019083.1.10",
          "form": "헉.. 대구는 아직 더워서",
          "original_form": "헉.. 대구는 아직 더워서",
          "speaker_id": "1",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019083.1.11",
          "form": "쌀쌀하진 않은거같아요",
          "original_form": "쌀쌀하진 않은거같아요",
          "speaker_id": "1",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019083.1.12",
          "form": "다른메뉴는 어떤게 있을까요",
          "original_form": "다른메뉴는 어떤게 있을까요",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019083.1.13",
          "form": "아직 덥군요! 저는 더울 때 메밀 소바나 막국수 먹고 싶었어요!",
          "original_form": "아직 덥군요! 저는 더울 때 메밀 소바나 막국수 먹고 싶었어요!",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019083.1.14",
          "form": "면류를 추천해주시는군요",
          "original_form": "면류를 추천해주시는군요",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019083.1.15",
          "form": "마침 집에 메밀면 있어서",
          "original_form": "마침 집에 메밀면 있어서",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019083.1.16",
          "form": "메밀소바 해먹어야겠어요",
          "original_form": "메밀소바 해먹어야겠어요",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019083.1.17",
          "form": "제가 요즘 면요리를 못 먹어서 면요리만 떠오르네요ㅋㅋㅋ",
          "original_form": "제가 요즘 면요리를 못 먹어서 면요리만 떠오르네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019083.1.18",
          "form": "식사는 대부분 요리해서 드시나요?",
          "original_form": "식사는 대부분 요리해서 드시나요?",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019083.1.19",
          "form": "음 집에 재료있으면 혼자 해먹고",
          "original_form": "음 집에 재료있으면 혼자 해먹고",
          "speaker_id": "1",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100019083.1.20",
          "form": "귀찮을땐 배달로 먹어요 ㅋㅋ",
          "original_form": "귀찮을땐 배달로 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100019083.1.21",
          "form": "배달이 편하죠ㅎㅎ",
          "original_form": "배달이 편하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019083.1.22",
          "form": "그렇져 ㅋㅋ",
          "original_form": "그렇져 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:48"
        }
