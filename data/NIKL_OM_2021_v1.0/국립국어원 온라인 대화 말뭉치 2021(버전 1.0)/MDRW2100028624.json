      "utterance": [
        {
          "id": "MDRW2100028624.1.1",
          "form": "버블티 좋아하세요?",
          "original_form": "버블티 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028624.1.2",
          "form": "아 많이는 못먹어요,,!",
          "original_form": "아 많이는 못먹어요,,!",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028624.1.3",
          "form": "한개시키면 꼭 남겨서 잘안시켜요ㅠ",
          "original_form": "한개시키면 꼭 남겨서 잘안시켜요ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028624.1.4",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028624.1.5",
          "form": "저는 한참 빠져서 열심히 먹다가",
          "original_form": "저는 한참 빠져서 열심히 먹다가",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028624.1.6",
          "form": "요즘은 안먹고 있어요",
          "original_form": "요즘은 안먹고 있어요",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028624.1.7",
          "form": "그럼 다른 좋아하는 음료 있으신가요?",
          "original_form": "그럼 다른 좋아하는 음료 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028624.1.8",
          "form": "저 탄산진짜좋아하는데",
          "original_form": "저 탄산진짜좋아하는데",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028624.1.9",
          "form": "줄이려고 노력중입니다ㅠ",
          "original_form": "줄이려고 노력중입니다ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028624.1.10",
          "form": "탄산 좋아하시는구나",
          "original_form": "탄산 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028624.1.11",
          "form": "그죠 그것도 건강에 안좋다고는 하더라고요",
          "original_form": "그죠 그것도 건강에 안좋다고는 하더라고요",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028624.1.12",
          "form": "근데 이제 당뇨올까봐",
          "original_form": "근데 이제 당뇨올까봐",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028624.1.13",
          "form": "왜 맛있는건 다 그럴까요",
          "original_form": "왜 맛있는건 다 그럴까요",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028624.1.14",
          "form": "커피도좋아해요,..",
          "original_form": "커피도좋아해요,..",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028624.1.15",
          "form": "제로콜라나 나랑드사이다 같은건 좀 괜찮지 않을까요?",
          "original_form": "제로콜라나 나랑드사이다 같은건 좀 괜찮지 않을까요?",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028624.1.16",
          "form": "커피좋아하시는구나",
          "original_form": "커피좋아하시는구나",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028624.1.17",
          "form": "저는 카페인 약해서",
          "original_form": "저는 카페인 약해서",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028624.1.18",
          "form": "커피는 좀 피하는 편입니다",
          "original_form": "커피는 좀 피하는 편입니다",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028624.1.19",
          "form": "아,,,!",
          "original_form": "아,,,!",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028624.1.20",
          "form": "저도 잠깨려고 마시던게",
          "original_form": "저도 잠깨려고 마시던게",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028624.1.21",
          "form": "입에맞아버려서",
          "original_form": "입에맞아버려서",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028624.1.22",
          "form": "많이마셔요ㅠ",
          "original_form": "많이마셔요ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028624.1.23",
          "form": "하루에 얼마나 드세요,,?",
          "original_form": "하루에 얼마나 드세요,,?",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028624.1.24",
          "form": "줄였어요!4잔,,",
          "original_form": "줄였어요!4잔,,",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028624.1.25",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028624.1.26",
          "form": "줄여서 4잔이시면 그전엔 정말 많이 드셨나보군요..",
          "original_form": "줄여서 4잔이시면 그전엔 정말 많이 드셨나보군요..",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028624.1.27",
          "form": "화장실 자주가게 되진 않나요?",
          "original_form": "화장실 자주가게 되진 않나요?",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028624.1.28",
          "form": "저엄청가요",
          "original_form": "저엄청가요",
          "speaker_id": "2",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028624.1.29",
          "form": "근육도없어서",
          "original_form": "근육도없어서",
          "speaker_id": "2",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028624.1.30",
          "form": "수분을 몸에 저장 못한대요",
          "original_form": "수분을 몸에 저장 못한대요",
          "speaker_id": "2",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028624.1.31",
          "form": "저 물도많이마셔요",
          "original_form": "저 물도많이마셔요",
          "speaker_id": "2",
          "time": "20210902 21:36"
        }
      ]
