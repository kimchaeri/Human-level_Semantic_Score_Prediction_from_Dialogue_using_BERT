        },
        {
          "id": "MDRW2100015013.1.2",
          "form": "소금? 그때 그 히말라야 핑크 소금?",
          "original_form": "소금? 그때 그 히말라야 핑크 소금?",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015013.1.3",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015013.1.4",
          "form": "주문 어디서 하는뎅",
          "original_form": "주문 어디서 하는뎅",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015013.1.5",
          "form": "톡딜에 떴다",
          "original_form": "톡딜에 떴다",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015013.1.6",
          "form": "오홋 글쿤 그거 링크 보내줭",
          "original_form": "오홋 글쿤 그거 링크 보내줭",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015013.1.7",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015013.1.8",
          "form": "ㅋㅋ 빠른 주문 해주겠어용",
          "original_form": "ㅋㅋ 빠른 주문 해주겠어용",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015013.1.9",
          "form": "그래 고마워",
          "original_form": "그래 고마워",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015013.1.10",
          "form": "ㅎㅎ 필요한거니 빨리 사야지",
          "original_form": "ㅎㅎ 필요한거니 빨리 사야지",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015013.1.11",
          "form": "그래 양도 많고 가격 싸다",
          "original_form": "그래 양도 많고 가격 싸다",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015013.1.12",
          "form": "좋넴 오늘 주문하면 모레 오겠다",
          "original_form": "좋넴 오늘 주문하면 모레 오겠다",
