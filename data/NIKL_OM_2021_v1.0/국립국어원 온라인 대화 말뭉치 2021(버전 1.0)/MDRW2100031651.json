        {
          "id": "MDRW2100031651.1.26",
          "form": "악 안돼요 ㅋㅋㅋㅋㅋ 전 포기 할래요!ㅋㅋㅋㅋㅋ",
          "original_form": "악 안돼요 ㅋㅋㅋㅋㅋ 전 포기 할래요!ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100031651.1.27",
          "form": "겁이 진짜 많거든요 ㅋㅋㅋ",
          "original_form": "겁이 진짜 많거든요 ㅋㅋㅋ",
