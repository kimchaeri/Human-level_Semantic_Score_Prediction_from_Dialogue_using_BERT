      "utterance": [
        {
          "id": "MDRW2100011991.1.1",
          "form": "메뉴 어떤거 좋아하세요?",
          "original_form": "메뉴 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100011991.1.2",
          "form": "떡볶이 좋아해요",
          "original_form": "떡볶이 좋아해요",
          "speaker_id": "2",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011991.1.3",
          "form": "오 저도 떡볶이 좋아해요 엽떡!",
          "original_form": "오 저도 떡볶이 좋아해요 엽떡!",
          "speaker_id": "1",
          "time": "20210824 18:28"
        },
        {
          "id": "MDRW2100011991.1.4",
          "form": "맛있죠ㅋㅋ 요샌 로제를 자주 먹어요.",
          "original_form": "맛있죠ㅋㅋ 요샌 로제를 자주 먹어요.",
          "speaker_id": "2",
          "time": "20210824 18:53"
        },
        {
          "id": "MDRW2100011991.1.5",
          "form": "오 엽떡 로제 젤 좋아해요 로제중에ㅎㅎ",
          "original_form": "오 엽떡 로제 젤 좋아해요 로제중에ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100011991.1.6",
          "form": "엽떡 로제 딴데보다 매콤해서 맛있어요",
          "original_form": "엽떡 로제 딴데보다 매콤해서 맛있어요",
          "speaker_id": "2",
          "time": "20210824 18:57"
        },
        {
          "id": "MDRW2100011991.1.7",
          "form": "오오맞아요 다른데는 좀 느끼한데",
          "original_form": "오오맞아요 다른데는 좀 느끼한데",
          "speaker_id": "1",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100011991.1.8",
          "form": "혼자 먹을 양을 못시키는게 단점이긴 해요ㅋㅋ",
          "original_form": "혼자 먹을 양을 못시키는게 단점이긴 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100011991.1.9",
          "form": "맞아요 혼자먹기는 넘많죠",
          "original_form": "맞아요 혼자먹기는 넘많죠",
          "speaker_id": "1",
          "time": "20210824 19:58"
        },
        {
          "id": "MDRW2100011991.1.10",
          "form": "저녁은 드셨나요",
          "original_form": "저녁은 드셨나요",
          "speaker_id": "2",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100011991.1.11",
          "form": "네 육개장 먹었습니다ㅎㅎ",
          "original_form": "네 육개장 먹었습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100011991.1.12",
          "form": "맛있겠네요ㅋㅋ",
          "original_form": "맛있겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100011991.1.13",
          "form": "밥 먹고 나니까 후식도 당겨요",
          "original_form": "밥 먹고 나니까 후식도 당겨요",
          "speaker_id": "2",
          "time": "20210824 20:21"
        },
        {
          "id": "MDRW2100011991.1.14",
          "form": "단거 땡기네요ㅎㅎ",
          "original_form": "단거 땡기네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:36"
