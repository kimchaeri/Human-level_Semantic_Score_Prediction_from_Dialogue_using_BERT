      "utterance": [
        {
          "id": "MDRW2100015115.1.1",
          "form": "1200~1400만달러 추정이라네 출연료가",
          "original_form": "1200~1400만달러 추정이라네 출연료가",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015115.1.2",
          "form": "키아누 리브스",
          "original_form": "키아누 리브스",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015115.1.3",
          "form": "키아누 리브스 출연료가 1200~1400",
          "original_form": "키아누 리브스 출연료가 1200~1400",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015115.1.4",
          "form": "대박이다 120 억이가?",
          "original_form": "대박이다 120 억이가?",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015115.1.5",
          "form": "오~14위 우리의 톰아저씨다",
          "original_form": "오~14위 우리의 톰아저씨다",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015115.1.6",
          "form": "톰 영화도 많이 찍었는데 엄청 돈 많겠다",
          "original_form": "톰 영화도 많이 찍었는데 엄청 돈 많겠다",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015115.1.7",
          "form": "한 130억정도 되겠다",
          "original_form": "한 130억정도 되겠다",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015115.1.8",
          "form": "한편 찍을때 마다 그러면 1조이상 있겠다",
          "original_form": "한편 찍을때 마다 그러면 1조이상 있겠다",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015115.1.9",
          "form": "톰아저씨 탑건 후속편 찍었나보네 1300만",
          "original_form": "톰아저씨 탑건 후속편 찍었나보네 1300만",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015115.1.10",
          "form": "찍지말지. 그때랑 미모가 트린데",
          "original_form": "찍지말지. 그때랑 미모가 트린데",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015115.1.11",
          "form": "13위 마이클 B 조던",
          "original_form": "13위 마이클 B 조던",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015115.1.12",
          "form": "농구선수 말하는거임? ㅋ 아니지?",
          "original_form": "농구선수 말하는거임? ㅋ 아니지?",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015115.1.13",
          "form": "이분은 1500만달러인데 모르겠다 패스",
          "original_form": "이분은 1500만달러인데 모르겠다 패스",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015115.1.14",
          "form": "14위는 여배우 아냐?",
          "original_form": "14위는 여배우 아냐?",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015115.1.15",
          "form": "9위가 브래드 피트 2000만달러",
          "original_form": "9위가 브래드 피트 2000만달러",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015115.1.16",
          "form": "아 전부 남자네ㅋ 빵 아저씨도 많이 받네",
          "original_form": "아 전부 남자네ㅋ 빵 아저씨도 많이 받네",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015115.1.17",
          "form": "외국에도 우리나라처럼 여배우 출연료가 남자배우보다 적게 받는다고 하더라",
          "original_form": "외국에도 우리나라처럼 여배우 출연료가 남자배우보다 적게 받는다고 하더라",
          "speaker_id": "1",
          "time": "20210825 21:58"
        }
