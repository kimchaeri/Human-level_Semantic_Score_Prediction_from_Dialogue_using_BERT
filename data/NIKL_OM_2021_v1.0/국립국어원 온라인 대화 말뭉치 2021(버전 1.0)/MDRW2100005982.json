      "utterance": [
        {
          "id": "MDRW2100005982.1.1",
          "form": "지금도 두 마린데 어떡해",
          "original_form": "지금도 두 마린데 어떡해",
          "speaker_id": "2",
          "time": "20210907 18:18"
        },
        {
          "id": "MDRW2100005982.1.2",
          "form": "좀 그런가 ㅋㅋ",
          "original_form": "좀 그런가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:18"
        },
        {
          "id": "MDRW2100005982.1.3",
          "form": "그래 힘들다",
          "original_form": "그래 힘들다",
          "speaker_id": "2",
          "time": "20210907 18:18"
        },
        {
          "id": "MDRW2100005982.1.4",
          "form": "웅.. ㅜㅜ요새 잘 싸우는것같아서 강아지들이..",
          "original_form": "웅.. ㅜㅜ요새 잘 싸우는것같아서 강아지들이..",
          "speaker_id": "1",
          "time": "20210907 18:18"
        },
        {
          "id": "MDRW2100005982.1.5",
          "form": "아니다 사이좋다",
          "original_form": "아니다 사이좋다",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005982.1.6",
          "form": "사이 좋아서 싸우는건가 ㅋㅋ",
          "original_form": "사이 좋아서 싸우는건가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005982.1.7",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005982.1.8",
          "form": "ㅋㅋ 사람이랑 같구만",
          "original_form": "ㅋㅋ 사람이랑 같구만",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005982.1.9",
          "form": "한 마리 더키우면 엄청 힘들다",
          "original_form": "한 마리 더키우면 엄청 힘들다",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005982.1.10",
          "form": "음.. 그럴듯 ㅋ",
          "original_form": "음.. 그럴듯 ㅋ",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005982.1.11",
          "form": "그래 생각하지 마라",
          "original_form": "그래 생각하지 마라",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005982.1.12",
          "form": "알았다 ㅋㅋ 우리 강아지들 잘키워야지",
          "original_form": "알았다 ㅋㅋ 우리 강아지들 잘키워야지",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005982.1.13",
          "form": "당연하지",
