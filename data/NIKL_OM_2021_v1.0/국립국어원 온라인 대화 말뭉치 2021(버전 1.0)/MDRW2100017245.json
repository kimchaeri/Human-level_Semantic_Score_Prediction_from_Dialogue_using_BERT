      "utterance": [
        {
          "id": "MDRW2100017245.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017245.1.2",
          "form": "애인 있으세요?",
          "original_form": "애인 있으세요?",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017245.1.3",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017245.1.4",
          "form": "네 있어요!",
          "original_form": "네 있어요!",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017245.1.5",
          "form": "name1님도 있으시나요?",
          "original_form": "&name1&님도 있으시나요?",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017245.1.6",
          "form": "저는 없어요..",
          "original_form": "저는 없어요..",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017245.1.7",
          "form": "작년에 헤어졌어요!",
          "original_form": "작년에 헤어졌어요!",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017245.1.8",
          "form": "아하 그러시군요ㅠ",
          "original_form": "아하 그러시군요ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017245.1.9",
          "form": "잘 안맞아서 헤어지게 되셨어요?ㅠ",
          "original_form": "잘 안맞아서 헤어지게 되셨어요?ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017245.1.10",
          "form": "ㅜㅜ네.. 헤어질 때마다 붙잡더라고요.",
          "original_form": "ㅜㅜ네.. 헤어질 때마다 붙잡더라고요.",
          "speaker_id": "1",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017245.1.11",
          "form": "두 번인가 다시 사겼는데 그래도 같은 이유로 헤어졌어요.",
          "original_form": "두 번인가 다시 사겼는데 그래도 같은 이유로 헤어졌어요.",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017245.1.12",
          "form": "그렇군요ㅠ 한번 헤어지면 같은이유로 헤어질수밖에 없나봐요ㅠㅠ",
          "original_form": "그렇군요ㅠ 한번 헤어지면 같은이유로 헤어질수밖에 없나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017245.1.13",
          "form": "저도 지금 여자친구랑 오래 만나긴했는데 비슷한거때문에 몇번 헤어지긴헸어요ㅠㅠ",
          "original_form": "저도 지금 여자친구랑 오래 만나긴했는데 비슷한거때문에 몇번 헤어지긴헸어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017245.1.14",
          "form": "그럼 갈수록 더 힘들지 않으세요?",
          "original_form": "그럼 갈수록 더 힘들지 않으세요?",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017245.1.15",
          "form": "근데 잘 맞는 부분이 더 많고 점점 고쳐나가는중이어서 괜찮은거같아요! 예전에는 많이 싸우기도 했는데 요즘은 거의 싸울일도 없구요!",
          "original_form": "근데 잘 맞는 부분이 더 많고 점점 고쳐나가는중이어서 괜찮은거같아요! 예전에는 많이 싸우기도 했는데 요즘은 거의 싸울일도 없구요!",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017245.1.16",
          "form": "다행이네요~",
          "original_form": "다행이네요~",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017245.1.17",
          "form": "예전엔 주로 무엇 때문에 싸우셨나요?",
          "original_form": "예전엔 주로 무엇 때문에 싸우셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017245.1.18",
          "form": "여자친구가 기분이 안좋거나 그럴때 제기 잘 위로해주고 공감해주는 방법을 잘 몰랐던거같아요ㅠ 그런걸로 싸웟던거같아요!",
          "original_form": "여자친구가 기분이 안좋거나 그럴때 제기 잘 위로해주고 공감해주는 방법을 잘 몰랐던거같아요ㅠ 그런걸로 싸웟던거같아요!",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017245.1.19",
          "form": "아! 남녀차이 때문에 다퉜군요",
          "original_form": "아! 남녀차이 때문에 다퉜군요",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017245.1.20",
          "form": "얼마나 오래 사귀셨어요?",
          "original_form": "얼마나 오래 사귀셨어요?",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017245.1.21",
          "form": "네 약간 그런게 컸던거같아요ㅠ",
          "original_form": "네 약간 그런게 컸던거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017245.1.22",
          "form": "중간에 헤어지긴했는데 중학교3학년때부터 사귀고있어요! 지금 쭈욱 사귄건 한 1600일정도? 됐네요!",
          "original_form": "중간에 헤어지긴했는데 중학교3학년때부터 사귀고있어요! 지금 쭈욱 사귄건 한 1600일정도? 됐네요!",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017245.1.23",
          "form": "와 진짜 대박이네요!",
          "original_form": "와 진짜 대박이네요!",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017245.1.24",
          "form": "결혼도 생각중이세요?",
          "original_form": "결혼도 생각중이세요?",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017245.1.25",
          "form": "저는 결혼도 생각중이에요! 정말 좋은사람인거같아서요!ㅎㅎ",
          "original_form": "저는 결혼도 생각중이에요! 정말 좋은사람인거같아서요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017245.1.26",
          "form": "다행이다ㅜ 꼭 행복했음 좋겠어요",
          "original_form": "다행이다ㅜ 꼭 행복했음 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017245.1.27",
          "form": "그럼 주로 데이트는 어떻게 하세요?",
          "original_form": "그럼 주로 데이트는 어떻게 하세요?",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017245.1.28",
          "form": "그러면 참 좋겠네요!ㅎㅎ",
          "original_form": "그러면 참 좋겠네요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017245.1.29",
          "form": "데이트는 둘다 이야기하는걸 좋아해서 카페가는걸 좋아하구 영화도 많이보고 맛있는거 먹으러 더니구 그래요!",
          "original_form": "데이트는 둘다 이야기하는걸 좋아해서 카페가는걸 좋아하구 영화도 많이보고 맛있는거 먹으러 더니구 그래요!",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017245.1.30",
          "form": "연애하는 건 다 비슷비슷 하네요 ㅎㅎ",
          "original_form": "연애하는 건 다 비슷비슷 하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017245.1.31",
          "form": "그럼 코시국에선 어떻게 하셨어요?",
          "original_form": "그럼 코시국에선 어떻게 하셨어요?",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017245.1.32",
          "form": "그러게요 다 비슷한거같아요ㅎㅎ",
          "original_form": "그러게요 다 비슷한거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017245.1.33",
          "form": "코시국에서도 큰 차이는 없었던거같아요! 학기중에는 대학교 지역이 달라서 많이 못만나긴했지만 방학때는 주변에서 사람많은데만 안가구 여기저기 다니고 그랬어요!",
          "original_form": "코시국에서도 큰 차이는 없었던거같아요! 학기중에는 대학교 지역이 달라서 많이 못만나긴했지만 방학때는 주변에서 사람많은데만 안가구 여기저기 다니고 그랬어요!",
          "speaker_id": "2",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100017245.1.34",
          "form": "아 대학생이세요?",
          "original_form": "아 대학생이세요?",
          "speaker_id": "1",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100017245.1.35",
          "form": "그리고 제가 딱 코로나 터졌을때 군대에 있었어서 그때는 거의 못봤어요ㅠ",
          "original_form": "그리고 제가 딱 코로나 터졌을때 군대에 있었어서 그때는 거의 못봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100017245.1.36",
          "form": "네 대학생이에요!",
          "original_form": "네 대학생이에요!",
          "speaker_id": "2",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100017245.1.37",
          "form": "헉 그랬구나 ㅜㅜ",
          "original_form": "헉 그랬구나 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100017245.1.38",
          "form": "이상형이랑 지금 여친이랑 잘 맞아떨어져요?",
          "original_form": "이상형이랑 지금 여친이랑 잘 맞아떨어져요?",
          "speaker_id": "1",
          "time": "20210826 18:51"
        },
        {
          "id": "MDRW2100017245.1.39",
          "form": "네 딱 맞아떨어지는거같아요ㅎㅎ",
          "original_form": "네 딱 맞아떨어지는거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:53"
        }
      ]
