      "utterance": [
        {
          "id": "MDRW2100000583.1.1",
          "form": "오늘 배달의 민족 시켜 먹을까?",
          "original_form": "오늘 배달의 민족 시켜 먹을까?",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000583.1.2",
          "form": "오늘 소나기 오니까 라면먹을까?",
          "original_form": "오늘 소나기 오니까 라면먹을까?",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000583.1.3",
          "form": "요기요 만원 쿠폰 있어서 요기요 3일이내 한 번 시켜 먹어야해",
          "original_form": "요기요 만원 쿠폰 있어서 요기요 3일이내 한 번 시켜 먹어야해",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000583.1.4",
          "form": "배달의 민족 너무 많이 먹었는데 ㅋ",
          "original_form": "배달의 민족 너무 많이 먹었는데 ㅋ",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000583.1.5",
          "form": "오늘 소나기 온대? 그럼 랍스타 라면이지",
          "original_form": "오늘 소나기 온대? 그럼 랍스타 라면이지",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000583.1.6",
          "form": "차돌박이 라면도 괜찮을듯",
          "original_form": "차돌박이 라면도 괜찮을듯",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000583.1.7",
          "form": "15만 8천원짜리 해산물 라면 있잖아",
          "original_form": "15만 8천원짜리 해산물 라면 있잖아",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000583.1.8",
          "form": "조리법 내가 찾아놨어 ㅋ",
          "original_form": "조리법 내가 찾아놨어 ㅋ",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000583.1.9",
          "form": "차돌박이 몇 점 안 넣어주니까 5만원인가?",
          "original_form": "차돌박이 몇 점 안 넣어주니까 5만원인가?",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000583.1.10",
          "form": "근데 차돌박이가 없음",
          "original_form": "근데 차돌박이가 없음",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000583.1.11",
          "form": "그거 재료 정리 하는데 너무 오래걸림",
          "original_form": "그거 재료 정리 하는데 너무 오래걸림",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000583.1.12",
          "form": "랍스타라면은 먹고싶네",
          "original_form": "랍스타라면은 먹고싶네",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000583.1.13",
          "form": "해산물만 22종",
          "original_form": "해산물만 22종",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000583.1.14",
          "form": "랍스타라면은 괜찮을듯",
          "original_form": "랍스타라면은 괜찮을듯",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000583.1.15",
          "form": "그거 왜 문세윤만 랍스타라면을 먹은거지",
          "original_form": "그거 왜 문세윤만 랍스타라면을 먹은거지",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000583.1.16",
          "form": "잠깐 가서 랍스타 잡아올께",
          "original_form": "잠깐 가서 랍스타 잡아올께",
