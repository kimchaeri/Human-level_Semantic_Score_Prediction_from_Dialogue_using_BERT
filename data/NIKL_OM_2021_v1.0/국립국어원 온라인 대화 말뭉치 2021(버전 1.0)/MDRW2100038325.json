      "utterance": [
        {
          "id": "MDRW2100038325.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100038325.1.2",
          "form": "아침드셨어요?",
          "original_form": "아침드셨어요?",
          "speaker_id": "2",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100038325.1.3",
          "form": "못 먹고 나왔어요ㅠ",
          "original_form": "못 먹고 나왔어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100038325.1.4",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100038325.1.5",
          "form": "이번주부터 먹으려고 노력해서 우유에시리얼먹었네요ㅎㅎ",
          "original_form": "이번주부터 먹으려고 노력해서 우유에시리얼먹었네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100038325.1.6",
          "form": "점심 메뉴 회사서드세요?",
          "original_form": "점심 메뉴 회사서드세요?",
          "speaker_id": "2",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100038325.1.7",
          "form": "네 ㅎㅎ 배가 고파서 이것저것 다 먹고싶네요",
          "original_form": "네 ㅎㅎ 배가 고파서 이것저것 다 먹고싶네요",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100038325.1.8",
          "form": "점심 뭐 드실건가요?",
          "original_form": "점심 뭐 드실건가요?",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100038325.1.9",
          "form": "저희회사는 메뉴가 오늘 짜장면이요ㅋㅋ\r\n아 줄 길겠다...",
          "original_form": "저희회사는 메뉴가 오늘 짜장면이요ㅋㅋ\r\n아 줄 길겠다...",
          "speaker_id": "2",
          "time": "20210902 10:10"
        },
        {
          "id": "MDRW2100038325.1.10",
          "form": "저 같아도 그런 날은 사내식당 갈래요 ㅎㅎ",
          "original_form": "저 같아도 그런 날은 사내식당 갈래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100038325.1.11",
          "form": "저희는 오늘 추어탕인 것 같아서요ㅠ",
          "original_form": "저희는 오늘 추어탕인 것 같아서요ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100038325.1.12",
          "form": "와 추어탕 대박\r\n라면드시나여?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 추어탕 대박\r\n라면드시나여?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100038325.1.13",
          "form": "추어탕에 라면사리 넣어 먹드시나요??",
          "original_form": "추어탕에 라면사리 넣어 먹드시나요??",
          "speaker_id": "1",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100038325.1.14",
          "form": "저는 추어탕 별로 안좋아해서 나가서 사먹을 것 같아요 ㅎㅎ",
          "original_form": "저는 추어탕 별로 안좋아해서 나가서 사먹을 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100038325.1.15",
          "form": "아녀아녀 저도 추어탕 시러서\r\n편의점 라면먹을듯ㅋㅋ",
          "original_form": "아녀아녀 저도 추어탕 시러서\r\n편의점 라면먹을듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:08"
        },
        {
          "id": "MDRW2100038325.1.16",
          "form": "추어탕 으아ㅋㅋㅋ 끔찍",
          "original_form": "추어탕 으아ㅋㅋㅋ 끔찍",
          "speaker_id": "2",
          "time": "20210902 11:08"
        },
        {
          "id": "MDRW2100038325.1.17",
          "form": "나가서 맛난거드세요!",
          "original_form": "나가서 맛난거드세요!",
          "speaker_id": "2",
          "time": "20210902 11:08"
        },
        {
          "id": "MDRW2100038325.1.18",
          "form": "아아 ㅋㅋㅋㅋ 맞아요 나가서 먹으려구요 ㅎㅎ",
          "original_form": "아아 ㅋㅋㅋㅋ 맞아요 나가서 먹으려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100038325.1.19",
          "form": "제육덮밥이랑 닭볶음탕 중에 고민중이에요!",
          "original_form": "제육덮밥이랑 닭볶음탕 중에 고민중이에요!",
          "speaker_id": "1",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100038325.1.20",
          "form": "와... 닭볶음탕 저대신 먹어주세요ㅋㅋㅋ",
          "original_form": "와... 닭볶음탕 저대신 먹어주세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100038325.1.21",
          "form": "닭볶음탕으로 할까요 ㅎㅎ",
          "original_form": "닭볶음탕으로 할까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038325.1.22",
          "form": "밥까지 야무지게 볶아 먹고 올게요 ㅋㅋㅋㅋㅋ",
          "original_form": "밥까지 야무지게 볶아 먹고 올게요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:15"
        }
      ]
    }
  ]
