        {
          "id": "MDRW2100033808.1.1",
          "form": "닭 어쩌다 키우시게 되셨나요",
          "original_form": "닭 어쩌다 키우시게 되셨나요",
          "speaker_id": "1",
          "time": "20210825 03:33"
        },
        {
          "id": "MDRW2100033808.1.2",
          "form": "단",
          "original_form": "단",
          "speaker_id": "1",
          "time": "20210825 03:33"
        },
        {
          "id": "MDRW2100033808.1.3",
          "form": "초딩때 오백원주고 데려왔어요",
          "original_form": "초딩때 오백원주고 데려왔어요",
          "speaker_id": "2",
          "time": "20210825 03:33"
        },
        {
          "id": "MDRW2100033808.1.4",
          "form": "닭 어쩌다 키우시게 되셨나요",
          "original_form": "닭 어쩌다 키우시게 되셨나요",
          "speaker_id": "1",
          "time": "20210825 03:33"
        },
        {
          "id": "MDRW2100033808.1.5",
          "form": "아  병아리 때부터 키우셨구나",
          "original_form": "아  병아리 때부터 키우셨구나",
          "speaker_id": "1",
          "time": "20210825 03:33"
        },
        {
          "id": "MDRW2100033808.1.6",
          "form": "고양이 자랑해주세요",
          "original_form": "고양이 자랑해주세요",
          "speaker_id": "2",
          "time": "20210825 03:34"
        },
        {
          "id": "MDRW2100033808.1.7",
          "form": "네 애지중지 키웠어요",
          "original_form": "네 애지중지 키웠어요",
          "speaker_id": "2",
          "time": "20210825 03:34"
        },
        {
          "id": "MDRW2100033808.1.8",
          "form": "일단 귀여워요",
          "original_form": "일단 귀여워요",
          "speaker_id": "1",
          "time": "20210825 03:34"
        },
        {
          "id": "MDRW2100033808.1.9",
          "form": "고양이는 다 귀엽더라고요",
          "original_form": "고양이는 다 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210825 03:34"
        },
        {
          "id": "MDRW2100033808.1.10",
          "form": "말랑거리고",
          "original_form": "말랑거리고",
          "speaker_id": "1",
          "time": "20210825 03:34"
        },
        {
          "id": "MDRW2100033808.1.11",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 03:35"
        },
        {
          "id": "MDRW2100033808.1.12",
          "form": "음.. 사실 자랑하라면 못하겠어요 약간 주접떠는걸 못해서",
          "original_form": "음.. 사실 자랑하라면 못하겠어요 약간 주접떠는걸 못해서",
          "speaker_id": "1",
          "time": "20210825 03:35"
        },
        {
          "id": "MDRW2100033808.1.13",
          "form": "털이 무슨색인가요?",
          "original_form": "털이 무슨색인가요?",
          "speaker_id": "2",
          "time": "20210825 03:35"
        },
        {
          "id": "MDRW2100033808.1.14",
          "form": "노랑색이랑 흰색 섞여있어요",
          "original_form": "노랑색이랑 흰색 섞여있어요",
          "speaker_id": "1",
          "time": "20210825 03:35"
        },
        {
          "id": "MDRW2100033808.1.15",
          "form": "밥은 잘먹나요?",
          "original_form": "밥은 잘먹나요?",
          "speaker_id": "2",
          "time": "20210825 03:35"
        },
        {
          "id": "MDRW2100033808.1.16",
          "form": "가리지 않는 편이죠",
          "original_form": "가리지 않는 편이죠",
