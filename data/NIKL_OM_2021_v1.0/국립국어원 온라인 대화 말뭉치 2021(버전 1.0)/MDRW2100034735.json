      "utterance": [
        {
          "id": "MDRW2100034735.1.1",
          "form": "나는 엄마가 갈켜줏는뎅",
          "original_form": "나는 엄마가 갈켜줏는뎅",
          "speaker_id": "2",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034735.1.2",
          "form": "name1님은모름?",
          "original_form": "&name1&님은모름?",
          "speaker_id": "2",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034735.1.3",
          "form": "나는 한참 20대 때는 몰랐고",
          "original_form": "나는 한참 20대 때는 몰랐고",
          "speaker_id": "1",
          "time": "20210828 18:27"
        },
        {
          "id": "MDRW2100034735.1.4",
          "form": "지금도 아리까리함",
          "original_form": "지금도 아리까리함",
          "speaker_id": "1",
          "time": "20210828 18:27"
        },
        {
          "id": "MDRW2100034735.1.5",
          "form": "저녁때? 정도로 알고있음",
          "original_form": "저녁때? 정도로 알고있음",
          "speaker_id": "1",
          "time": "20210828 18:27"
        },
        {
          "id": "MDRW2100034735.1.6",
          "form": "어라",
          "original_form": "어라",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034735.1.7",
          "form": "별로 안중해서 까묵었나부지",
          "original_form": "별로 안중해서 까묵었나부지",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034735.1.8",
          "form": "걍 숫자에 약한거일지도 그다지 평소에 쓸일없기도하고 ㅋㅋ",
          "original_form": "걍 숫자에 약한거일지도 그다지 평소에 쓸일없기도하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100034735.1.9",
          "form": "응 사주볼꺼아님 왜필요하겠어",
          "original_form": "응 사주볼꺼아님 왜필요하겠어",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100034735.1.10",
          "form": "여하간 말하는데 되게 느낌이 좋아보였음",
          "original_form": "여하간 말하는데 되게 느낌이 좋아보였음",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100034735.1.11",
          "form": "꿈과 희망으로 가득찬",
          "original_form": "꿈과 희망으로 가득찬",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100034735.1.12",
          "form": "연애 근황 ㅋㅋㅋ",
          "original_form": "연애 근황 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100034735.1.13",
          "form": "ㅋㅋ 응 좋은소식이야",
          "original_form": "ㅋㅋ 응 좋은소식이야",
          "speaker_id": "2",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100034735.1.14",
          "form": "행복하다면 그만입니다",
          "original_form": "행복하다면 그만입니다",
          "speaker_id": "2",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100034735.1.15",
          "form": "늘 빨리 결혼하고 싶어했으니까",
          "original_form": "늘 빨리 결혼하고 싶어했으니까",
          "speaker_id": "1",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100034735.1.16",
          "form": "좋은 결과로 이어지길? ㅋㅋ",
          "original_form": "좋은 결과로 이어지길? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100034735.1.17",
          "form": "ㅇㅇ 솔직히 결혼생각하니까 그런거보는거같아",
          "original_form": "ㅇㅇ 솔직히 결혼생각하니까 그런거보는거같아",
          "speaker_id": "2",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100034735.1.18",
          "form": "ㅇㅇ모처럼 직장이 좋은 사람이니",
          "original_form": "ㅇㅇ모처럼 직장이 좋은 사람이니",
          "speaker_id": "1",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100034735.1.19",
          "form": "궁금하니까 동생남친님 생일시 빨리 알려주시길 ㅋ",
          "original_form": "궁금하니까 동생남친님 생일시 빨리 알려주시길 ㅋ",
          "speaker_id": "2",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100034735.1.20",
          "form": "결혼준비도 돼있을거같고",
          "original_form": "결혼준비도 돼있을거같고",
          "speaker_id": "1",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100034735.1.21",
          "form": "아 근데 한가지 걱정하더라는",
          "original_form": "아 근데 한가지 걱정하더라는",
          "speaker_id": "1",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100034735.1.22",
          "form": "머",
          "original_form": "머",
          "speaker_id": "2",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100034735.1.23",
          "form": "어떤거",
          "original_form": "어떤거",
          "speaker_id": "2",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100034735.1.24",
          "form": "노적금 올주식",
          "original_form": "노적금 올주식",
          "speaker_id": "1",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100034735.1.25",
          "form": "음 한방이네",
          "original_form": "음 한방이네",
          "speaker_id": "2",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100034735.1.26",
          "form": "심지어 월급도 그 회사주식에 자동으로 넣는거로 해놨대여 ㄷㅡ",
          "original_form": "심지어 월급도 그 회사주식에 자동으로 넣는거로 해놨대여 ㄷㅡ",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100034735.1.27",
          "form": "ㄷㄷㄷ",
          "original_form": "ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100034735.1.28",
          "form": "ㅋㅋㄱ",
          "original_form": "ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100034735.1.29",
          "form": "와우 너무 본격적인데",
          "original_form": "와우 너무 본격적인데",
          "speaker_id": "2",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100034735.1.30",
          "form": "근데 적금보다 안전한가 그게",
          "original_form": "근데 적금보다 안전한가 그게",
          "speaker_id": "2",
