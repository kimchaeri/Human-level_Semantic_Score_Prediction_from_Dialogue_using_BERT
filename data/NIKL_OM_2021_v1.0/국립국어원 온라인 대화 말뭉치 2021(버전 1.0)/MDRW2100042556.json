      "utterance": [
        {
          "id": "MDRW2100042556.1.1",
          "form": "스파게티도 좋아해서 그런가",
          "original_form": "스파게티도 좋아해서 그런가",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042556.1.2",
          "form": "그렇다기보다는 비빔면을 좋아해서 스파게티를 좋아하게 되었다 이런 느낌으로",
          "original_form": "그렇다기보다는 비빔면을 좋아해서 스파게티를 좋아하게 되었다 이런 느낌으로",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042556.1.3",
          "form": "국물 없는 면 좋아하나?",
          "original_form": "국물 없는 면 좋아하나?",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042556.1.4",
          "form": "근데 국물 있는 면도 좋아하고 그냥 돼지인 것으로",
          "original_form": "근데 국물 있는 면도 좋아하고{emoji:…} 그냥 돼지인 것으로",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042556.1.5",
          "form": "잘먹으면 좋지",
          "original_form": "잘먹으면 좋지",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042556.1.6",
          "form": "살만 안찌면 더 좋은데 라고 엄마가 다음에 말할듯한",
          "original_form": "살만 안찌면 더 좋은데 라고 엄마가 다음에 말할듯한",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042556.1.7",
          "form": "어떻게 알았냐",
          "original_form": "어떻게 알았냐",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042556.1.8",
          "form": "엄마 하는건 다 척이면 척이죠 ㅎㅎ",
          "original_form": "엄마 하는건 다 척이면 척이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042556.1.9",
          "form": "그래도 엄만 너 먹는거 보면 행복해",
          "original_form": "그래도 엄만 너 먹는거 보면 행복해",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042556.1.10",
          "form": "저두 엄마 잘 드시면 기분 좋아요",
          "original_form": "저두 엄마 잘 드시면 기분 좋아요",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042556.1.11",
          "form": "우리 딸 효녀네",
          "original_form": "우리 딸 효녀네",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042556.1.12",
          "form": "엄마가 잘키워서 그래요",
          "original_form": "엄마가 잘키워서 그래요",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042556.1.13",
          "form": "근데 신라면 볶음면은 맛없대",
          "original_form": "근데 신라면 볶음면은 맛없대",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042556.1.14",
          "form": "갑자기요",
          "original_form": "갑자기요",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042556.1.15",
          "form": "맞아요 근데 그거 맛없다고 하긴 하더라구요",
          "original_form": "맞아요 근데 그거 맛없다고 하긴 하더라구요",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042556.1.16",
          "form": "되게 짜고 별로래 당뇨 걸릴지도",
          "original_form": "되게 짜고 별로래 당뇨 걸릴지도",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042556.1.17",
          "form": "엄마 안그래도 위험군인데 안된다",
          "original_form": "엄마 안그래도 위험군인데 안된다",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042556.1.18",
          "form": "절대못먹어",
          "original_form": "절대못먹어",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042556.1.19",
          "form": "신라면 스프 그대로 넣고 국물 없이 볶아먹는 느낌이라더라",
          "original_form": "신라면 스프 그대로 넣고 국물 없이 볶아먹는 느낌이라더라",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042556.1.20",
          "form": "으 완전 상상되는 맛",
          "original_form": "으 완전 상상되는 맛",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042556.1.21",
          "form": "보리차 끓이고 왔다",
          "original_form": "보리차 끓이고 왔다",
          "speaker_id": "2",
