      "utterance": [
        {
          "id": "MDRW2100022608.1.1",
          "form": "아 저는 막걸리 잘 못마셔요 ㅜㅠ",
          "original_form": "아 저는 막걸리 잘 못마셔요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100022608.1.2",
          "form": "저도 일반막걸리는 잘 못마셔요 ㅜㅜ",
          "original_form": "저도 일반막걸리는 잘 못마셔요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100022608.1.3",
          "form": "사실 소주도 못마셔요 ㅋㅋㅋㅋ",
          "original_form": "사실 소주도 못마셔요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100022608.1.4",
          "form": "근데 과일막걸리 먹어본 적 있어요?",
          "original_form": "근데 과일막걸리 먹어본 적 있어요?",
          "speaker_id": "1",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100022608.1.5",
          "form": "그 예전에 밤막걸리 이런건 먹어봤어요!",
          "original_form": "그 예전에 밤막걸리 이런건 먹어봤어요!",
          "speaker_id": "2",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100022608.1.6",
          "form": "밤막걸맄 ㅋ 혹시 제주도에서?",
          "original_form": "밤막걸맄 ㅋ 혹시 제주도에서?",
          "speaker_id": "1",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100022608.1.7",
          "form": "아니욬ㅋㅋㅋㅋ저희지역에도 있더라구요",
          "original_form": "아니욬ㅋㅋㅋㅋ저희지역에도 있더라구요",
          "speaker_id": "2",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100022608.1.8",
          "form": "아 그렇구나 땅콩막걸리는요?",
          "original_form": "아 그렇구나 땅콩막걸리는요?",
          "speaker_id": "1",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100022608.1.9",
          "form": "전집 이런데에요 ㅋㅋㅋㅋ",
          "original_form": "전집 이런데에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100022608.1.10",
          "form": "아 그거 궁금햇는데 못먹어봣어요",
          "original_form": "아 그거 궁금햇는데 못먹어봣어요",
          "speaker_id": "2",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022608.1.11",
          "form": "오 전집 ㅎㅎ 맛있겠네요~",
          "original_form": "오 전집 ㅎㅎ 맛있겠네요~",
          "speaker_id": "1",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022608.1.12",
          "form": "우도에 팔지않아요?",
          "original_form": "우도에 팔지않아요?",
          "speaker_id": "2",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022608.1.13",
          "form": "저는 밤이랑 땅콩 둘다 별로였는ㄷ ㅔㅜㅜ",
          "original_form": "저는 밤이랑 땅콩 둘다 별로였는ㄷ ㅔㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022608.1.14",
          "form": "제주도에 팔아용 ㅎㅎ 하나로마트 ㅋㅋㅋ",
          "original_form": "제주도에 팔아용 ㅎㅎ 하나로마트 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022608.1.15",
          "form": "ㅋㅋㅋㅋㅋ아 제쥬도 전부파는구낰ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아 제쥬도 전부파는구낰ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022608.1.16",
          "form": "키위막걸리나 바나나막걸리 이런건요?",
          "original_form": "키위막걸리나 바나나막걸리 이런건요?",
          "speaker_id": "1",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022608.1.17",
          "form": "앗 저는 그냥 막걸리자체를 잘 안먹엇어요!",
          "original_form": "앗 저는 그냥 막걸리자체를 잘 안먹엇어요!",
          "speaker_id": "2",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022608.1.18",
          "form": "아 ㅜ 저도 막걸리 완전 안좋아하거든요?",
          "original_form": "아 ㅜ 저도 막걸리 완전 안좋아하거든요?",
          "speaker_id": "1",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022608.1.19",
          "form": "근데 과일막걸리는 진짜 맛있어요 ㅜㅜ",
          "original_form": "근데 과일막걸리는 진짜 맛있어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100022608.1.20",
          "form": "아 청포도막걸리인가? 그거는 먹어본거같아요",
          "original_form": "아 청포도막걸리인가? 그거는 먹어본거같아요",
          "speaker_id": "2",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100022608.1.21",
          "form": "한창 유행할때 ㅋㅋㅋㅋ",
          "original_form": "한창 유행할때 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100022608.1.22",
          "form": "그런것도 유행했었어요?",
          "original_form": "그런것도 유행했었어요?",
          "speaker_id": "1",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100022608.1.23",
          "form": "맛이 어때요 청포도는 ?",
          "original_form": "맛이 어때요 청포도는 ?",
          "speaker_id": "1",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100022608.1.24",
          "form": "네네 적당히 상큼하고 달앗던거같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "네네 적당히 상큼하고 달앗던거같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100022608.1.25",
          "form": "와 나중에 한번 찾아 먹어봐야겠어요",
          "original_form": "와 나중에 한번 찾아 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100022608.1.26",
          "form": "정보 공유 감사해요!",
          "original_form": "정보 공유 감사해요!",
          "speaker_id": "1",
          "time": "20210829 16:19"
        }
