      "utterance": [
        {
          "id": "MDRW2100031619.1.1",
          "form": "저는 블로그 하느라",
          "original_form": "저는 블로그 하느라",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031619.1.2",
          "form": "블로그 하느라... 그마저도 잘 안들어가네요.... 학교 커뮤니티도....",
          "original_form": "블로그 하느라... 그마저도 잘 안들어가네요.... 학교 커뮤니티도....",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031619.1.3",
          "form": "요즘은 여기 들어오느라 블로그를 잘 안하고 (?)",
          "original_form": "요즘은 여기 들어오느라 블로그를 잘 안하고 (?)",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031619.1.4",
          "form": "와 주제 변경도 되는군요!!",
          "original_form": "와 주제 변경도 되는군요!!",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031619.1.5",
          "form": "ㅋㅋㄲㅋㅋ그렇군요!!",
          "original_form": "ㅋㅋㄲㅋㅋ그렇군요!!",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031619.1.6",
          "form": "최근에 구매하신게 있으신가요..?",
          "original_form": "최근에 구매하신게 있으신가요..?",
          "speaker_id": "2",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031619.1.7",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031619.1.8",
          "form": "저는 며칠 전에 친구 생일선물로 러그를 샀어요!!",
          "original_form": "저는 며칠 전에 친구 생일선물로 러그를 샀어요!!",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031619.1.9",
          "form": "저는 엄청 큰 파티션을 샀는데..가족들에게 엄청 구박을 받고있어요",
          "original_form": "저는 엄청 큰 파티션을 샀는데..가족들에게 엄청 구박을 받고있어요",
          "speaker_id": "2",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031619.1.10",
          "form": "와~~~!",
          "original_form": "와~~~!",
          "speaker_id": "2",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031619.1.11",
          "form": "무직타이거라는 캐릭터 아시나요?",
          "original_form": "무직타이거라는 캐릭터 아시나요?",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031619.1.12",
          "form": "아니 어쩌다가 구박을!",
          "original_form": "아니 어쩌다가 구박을!",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031619.1.13",
          "form": "name2님 자취하시는 집에 두려고 사신거 아닌가요?",
          "original_form": "&name2&님 자취하시는 집에 두려고 사신거 아닌가요?",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031619.1.14",
          "form": "오! 그거 엄청 유명한 캐릭터잖아요",
          "original_form": "오! 그거 엄청 유명한 캐릭터잖아요",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031619.1.15",
          "form": "그 모양 러그라면 엄청 귀여울 것 같아요",
          "original_form": "그 모양 러그라면 엄청 귀여울 것 같아요",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031619.1.16",
          "form": "맞아요 아시는군요!!!!",
          "original_form": "맞아요 아시는군요!!!!",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031619.1.17",
          "form": "그거랑 소주잔도 샀어요...",
          "original_form": "그거랑 소주잔도 샀어요...{emoji:♡}",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031619.1.18",
          "form": "맞아요!자취방에ㅠㅠ 근데 무겁고..쓸데가 없어서..",
          "original_form": "맞아요!자취방에ㅠㅠ 근데 무겁고..쓸데가 없어서..",
          "speaker_id": "2",
          "time": "20210904 23:15"
