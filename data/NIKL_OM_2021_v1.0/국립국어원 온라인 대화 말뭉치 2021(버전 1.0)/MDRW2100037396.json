      "utterance": [
        {
          "id": "MDRW2100037396.1.1",
          "form": "음 아무래도 파티음식은 엽떡아닐까?",
          "original_form": "음 아무래도 파티음식은 엽떡아닐까?",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100037396.1.2",
          "form": "혼자서 먹기엔 힘든 음식이잖아",
          "original_form": "혼자서 먹기엔 힘든 음식이잖아",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.3",
          "form": "엽떡은 정말 맛있어",
          "original_form": "엽떡은 정말 맛있어",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.4",
          "form": "역시 넌 뭘쫌 아는구나",
          "original_form": "역시 넌 뭘쫌 아는구나",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.5",
          "form": "그 위에 치즈조차 맛있어",
          "original_form": "그 위에 치즈조차 맛있어",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.6",
          "form": "너는 그럼 떡볶이중엔 엽떡을 제일 좋아하니?",
          "original_form": "너는 그럼 떡볶이중엔 엽떡을 제일 좋아하니?",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.7",
          "form": "나는 거의 다 좋아해",
          "original_form": "나는 거의 다 좋아해",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.8",
          "form": "다른 추천하는 떡볶이는 잇어?",
          "original_form": "다른 추천하는 떡볶이는 잇어?",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.9",
          "form": "나는 배떡을 오랜만에 먹었더니 맛있더라",
          "original_form": "나는 배떡을 오랜만에 먹었더니 맛있더라",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.10",
          "form": "박 애 주 의 야",
          "original_form": "박 애 주 의 야",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.11",
          "form": "다사랑하구나",
          "original_form": "다사랑하구나",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.12",
          "form": "나도 최근에 먹었어",
          "original_form": "나도 최근에 먹었어",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.13",
          "form": "배떡은 근데 조금 느끼하지않아?",
          "original_form": "배떡은 근데 조금 느끼하지않아?",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037396.1.14",
          "form": "맞아 그리고 떡군이네랑 비슷해",
          "original_form": "맞아 그리고 떡군이네랑 비슷해",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037396.1.15",
          "form": "떡군이네가 더 맛있다던데 근데 난 둘다 똑같았어",
          "original_form": "떡군이네가 더 맛있다던데 근데 난 둘다 똑같았어",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037396.1.16",
          "form": "맞아 떡군이네를 빼낀거라는 말도 있더라",
          "original_form": "맞아 떡군이네를 빼낀거라는 말도 있더라",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037396.1.17",
          "form": "둘이 재료를 아끼는 거 같아",
          "original_form": "둘이 재료를 아끼는 거 같아",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037396.1.18",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037396.1.19",
          "form": "나 최근에 비빔면이 먹고싶어졌어",
          "original_form": "나 최근에 비빔면이 먹고싶어졌어",
          "speaker_id": "1",
          "time": "20210831 23:14"
        }
      ]
