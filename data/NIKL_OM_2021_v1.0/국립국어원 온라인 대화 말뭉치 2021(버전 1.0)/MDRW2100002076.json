      "utterance": [
        {
          "id": "MDRW2100002076.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002076.1.2",
          "form": "오늘 저녁 뭐 드셨나요?",
          "original_form": "오늘 저녁 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002076.1.3",
          "form": "저는 오늘저녁 집밥이요 ~",
          "original_form": "저는 오늘저녁 집밥이요 ~",
          "speaker_id": "2",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002076.1.4",
          "form": "무국과 젓갈에 ㅎㅎ 한식이져",
          "original_form": "무국과 젓갈에 ㅎㅎ 한식이져",
          "speaker_id": "2",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002076.1.5",
          "form": "앗 역시.. 저도 한식이요",
          "original_form": "앗 역시.. 저도 한식이요",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002076.1.6",
          "form": "된장찌개에 어묵무침 먹었어요ㅎㅎ",
          "original_form": "된장찌개에 어묵무침 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002076.1.7",
          "form": "한식이 최고인 거 같아요!",
          "original_form": "한식이 최고인 거 같아요!",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002076.1.8",
          "form": "오 맛있었겠어요 ㅠ",
          "original_form": "오 맛있었겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002076.1.9",
          "form": "날도 덥고 코로나때문에",
          "original_form": "날도 덥고 코로나때문에",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002076.1.10",
          "form": "배달 많이시켜드시져?",
          "original_form": "배달 많이시켜드시져?",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002076.1.11",
          "form": "네! 더워서 밥해먹기 너무 귀찮아요ㅠ",
          "original_form": "네! 더워서 밥해먹기 너무 귀찮아요ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002076.1.12",
          "form": "배달 진짜 좋아해요ㅋㅋㅋ",
          "original_form": "배달 진짜 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002076.1.13",
          "form": "그르게요 ㅠ 점심엔 회사에서 시켜먹고",
          "original_form": "그르게요 ㅠ 점심엔 회사에서 시켜먹고",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002076.1.14",
          "form": "저녁에도 시켜서 술한잔하고 ㅋㅋ",
          "original_form": "저녁에도 시켜서 술한잔하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002076.1.15",
          "form": "배달은 최고예여..ㅋㅋ",
          "original_form": "배달은 최고예여..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002076.1.16",
          "form": "ㅋㅋㅋ 맞아요! 배달 시키면 주로 뭐 드세요?",
          "original_form": "ㅋㅋㅋ 맞아요! 배달 시키면 주로 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002076.1.17",
          "form": "음 음식가리진않는데 요즘엔",
          "original_form": "음 음식가리진않는데 요즘엔",
          "speaker_id": "2",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002076.1.18",
          "form": "소바 냉면 콩국수 이런 시원한거 많이먹어요 !",
          "original_form": "소바 냉면 콩국수 이런 시원한거 많이먹어요 !",
          "speaker_id": "2",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002076.1.19",
          "form": "헐 듣기만 해도 쉬원해지는 기분",
          "original_form": "헐 듣기만 해도 쉬원해지는 기분",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002076.1.20",
          "form": "시원ㅋㅋㅋ",
          "original_form": "시원ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002076.1.21",
          "form": "좋아하는 음식있으세요?",
          "original_form": "좋아하는 음식있으세요?",
          "speaker_id": "2",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002076.1.22",
          "form": "전 피자랑 치킨 같은",
          "original_form": "전 피자랑 치킨 같은",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002076.1.23",
          "form": "패스트푸드요",
          "original_form": "패스트푸드요",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002076.1.24",
          "form": "족발 막창도 좋아해요ㅠㅠ",
          "original_form": "족발 막창도 좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002076.1.25",
          "form": "족발 막창 짱맛이죠 ㅠ",
          "original_form": "족발 막창 짱맛이죠 ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002076.1.26",
          "form": "저번 주말도 막창먹었어요 !",
          "original_form": "저번 주말도 막창먹었어요 !",
          "speaker_id": "2",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002076.1.27",
          "form": "진짜 막창 싫어하는 사람",
          "original_form": "진짜 막창 싫어하는 사람",
          "speaker_id": "1",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002076.1.28",
          "form": "못 본 거 같아요!",
          "original_form": "못 본 거 같아요!",
          "speaker_id": "1",
          "time": "20210819 23:29"
        }
      ]
    }
  ]
