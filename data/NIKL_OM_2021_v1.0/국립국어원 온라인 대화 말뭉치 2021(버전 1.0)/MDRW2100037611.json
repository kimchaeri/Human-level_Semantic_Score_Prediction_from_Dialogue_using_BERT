      "utterance": [
        {
          "id": "MDRW2100037611.1.1",
          "form": "점심 뭐드세여",
          "original_form": "점심 뭐드세여",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100037611.1.2",
          "form": "오리고기요",
          "original_form": "오리고기요",
          "speaker_id": "1",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100037611.1.3",
          "form": "국수 같은건 안드시나여",
          "original_form": "국수 같은건 안드시나여",
          "speaker_id": "2",
          "time": "20210901 13:32"
        },
        {
          "id": "MDRW2100037611.1.4",
          "form": "님이 드시게요?",
          "original_form": "님이 드시게요?",
          "speaker_id": "1",
          "time": "20210901 13:33"
        },
        {
          "id": "MDRW2100037611.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210901 13:37"
        },
        {
          "id": "MDRW2100037611.1.6",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100037611.1.7",
          "form": "헉..... 본인이 만드셨어요?",
          "original_form": "헉..... 본인이 만드셨어요?",
          "speaker_id": "1",
          "time": "20210901 13:47"
        },
        {
          "id": "MDRW2100037611.1.8",
          "form": "아뇨 저 오늘 그냥 밥안먹으려고요",
          "original_form": "아뇨 저 오늘 그냥 밥안먹으려고요",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100037611.1.9",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100037611.1.10",
          "form": "누군가가 해주는 요리 먹고싶지도않고",
          "original_form": "누군가가 해주는 요리 먹고싶지도않고",
          "speaker_id": "2",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037611.1.11",
          "form": "뭘 새로해먹으면 싫은소리들을것같아서요",
          "original_form": "뭘 새로해먹으면 싫은소리들을것같아서요",
          "speaker_id": "2",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037611.1.12",
          "form": "그리고 얘기듣고싶은 사람도잇고",
          "original_form": "그리고 얘기듣고싶은 사람도잇고",
          "speaker_id": "2",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037611.1.13",
          "form": "예..? 제요리 먹기 싫나요..?",
          "original_form": "예..? 제요리 먹기 싫나요..?",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100037611.1.14",
          "form": "누가 요리를 햇길래",
          "original_form": "누가 요리를 햇길래",
          "speaker_id": "2",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100037611.1.15",
          "form": "안남미 요리할생각이용",
          "original_form": "안남미 요리할생각이용",
          "speaker_id": "1",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100037611.1.16",
          "form": "오..그러군요..이럴때쓰네요",
          "original_form": "오..그러군요..이럴때쓰네요",
