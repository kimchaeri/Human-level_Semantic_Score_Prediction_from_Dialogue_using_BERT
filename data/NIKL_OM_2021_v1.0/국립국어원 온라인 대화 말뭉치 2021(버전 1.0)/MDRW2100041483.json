      "utterance": [
        {
          "id": "MDRW2100041483.1.1",
          "form": "월요일메뉴!",
          "original_form": "월요일메뉴!",
          "speaker_id": "1",
          "time": "20210906 01:15"
        },
        {
          "id": "MDRW2100041483.1.2",
          "form": "내일은 스테이크 밀키트 먹을 차례입니다~!",
          "original_form": "내일은 스테이크 밀키트 먹을 차례입니다~!",
          "speaker_id": "2",
          "time": "20210906 02:43"
        },
        {
          "id": "MDRW2100041483.1.3",
          "form": "오 먹어여?",
          "original_form": "오 먹어여?",
          "speaker_id": "1",
          "time": "20210906 11:05"
        },
        {
          "id": "MDRW2100041483.1.4",
          "form": "저녁에 먹을겁니다!",
          "original_form": "저녁에 먹을겁니다!",
          "speaker_id": "2",
          "time": "20210906 12:58"
        },
        {
          "id": "MDRW2100041483.1.5",
          "form": "그리고 레이어드 케잌 추천합니다 맛나요",
          "original_form": "그리고 레이어드 케잌 추천합니다 맛나요",
          "speaker_id": "2",
          "time": "20210906 12:58"
        },
        {
          "id": "MDRW2100041483.1.6",
          "form": "오 머먹엇어요",
          "original_form": "오 머먹엇어요",
          "speaker_id": "1",
          "time": "20210906 13:42"
        },
        {
          "id": "MDRW2100041483.1.7",
          "form": "이름정확히 모르겠는데 라즈베리 치즈케잌이요!",
          "original_form": "이름정확히 모르겠는데 라즈베리 치즈케잌이요!",
          "speaker_id": "2",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100041483.1.8",
          "form": "우와.. 꾸덕하나요",
          "original_form": "우와.. 꾸덕하나요",
          "speaker_id": "1",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100041483.1.9",
          "form": "아 근데 그게 완전히 치즈케잌아니고 시트는 빵이거든요? 그리고 위에 당케 크림치즈같은게 올라가있고용",
          "original_form": "아 근데 그게 완전히 치즈케잌아니고 시트는 빵이거든요? 그리고 위에 당케 크림치즈같은게 올라가있고용",
          "speaker_id": "2",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100041483.1.10",
          "form": "옹 네 근데 맛잇나요",
          "original_form": "옹 네 근데 맛잇나요",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041483.1.11",
          "form": "네네 담에 레이어드케잌 일찍가봐여겠어요",
          "original_form": "네네 담에 레이어드케잌 일찍가봐여겠어요",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041483.1.12",
          "form": "오... 의외네요",
          "original_form": "오... 의외네요",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041483.1.13",
          "form": "5시넘어서 갔더니 거의 다 솔드아웃이었어요",
          "original_form": "5시넘어서 갔더니 거의 다 솔드아웃이었어요",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041483.1.14",
          "form": "생긴거만 이쁘게보이는데",
          "original_form": "생긴거만 이쁘게보이는데",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041483.1.15",
          "form": "레이어드알바어때요?",
          "original_form": "레이어드알바어때요?",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100041483.1.16",
          "form": "헐... 레이어드 가봤어요?? 알바 ㅋㅋㅋ 훈남 & 문신남 많더라구요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐... 레이어드 가봤어요?? 알바 ㅋㅋㅋ 훈남 & 문신남 많더라구요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100041483.1.17",
          "form": "거기 알바때문에 인기많다던데요",
          "original_form": "거기 알바때문에 인기많다던데요",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100041483.1.18",
          "form": "특히 연남",
          "original_form": "특히 연남",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100041483.1.19",
          "form": "이유는 알바",
          "original_form": "이유는 알바",
          "speaker_id": "1",
          "time": "20210906 14:13"
        }
      ]
