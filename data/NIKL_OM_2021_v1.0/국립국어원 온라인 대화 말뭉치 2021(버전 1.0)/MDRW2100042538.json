      "utterance": [
        {
          "id": "MDRW2100042538.1.1",
          "form": "같은회사의 타겜이 터져서",
          "original_form": "같은회사의 타겜이 터져서",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042538.1.2",
          "form": "여파가 몰려오다니.....",
          "original_form": "여파가 몰려오다니.....",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042538.1.3",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042538.1.4",
          "form": "난 심지어",
          "original_form": "난 심지어",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042538.1.5",
          "form": "샤닉이 남성향 느낌나서 싫어했지",
          "original_form": "샤닉이 남성향 느낌나서 싫어했지",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042538.1.6",
          "form": "동북공정으로 터지는 상황은",
          "original_form": "동북공정으로 터지는 상황은",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042538.1.7",
          "form": "ㄹㅇ 상상도 못함",
          "original_form": "ㄹㅇ 상상도 못함",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042538.1.8",
          "form": "ㅇㅇㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042538.1.9",
          "form": "걍.....손이나 대지 말지.....",
          "original_form": "걍.....손이나 대지 말지.....",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042538.1.10",
          "form": "왜 굳이 한복을 넣어서",
          "original_form": "왜 굳이 한복을 넣어서",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042538.1.11",
          "form": "긁어 부스럼을......",
          "original_form": "긁어 부스럼을......",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042538.1.12",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042538.1.13",
          "form": "아니 적어도",
          "original_form": "아니 적어도",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042538.1.14",
          "form": "그쪽 업계에서 일하면",
          "original_form": "그쪽 업계에서 일하면",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042538.1.15",
          "form": "웨이보 분위기는 파악해야하는거 아냐?",
          "original_form": "웨이보 분위기는 파악해야하는거 아냐?",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042538.1.16",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042538.1.17",
          "form": "눈치챙겨;;;",
          "original_form": "눈치챙겨;;;",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042538.1.18",
          "form": "원신도 완전",
          "original_form": "원신도 완전",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042538.1.19",
          "form": "바보같이 대처했던데ㅎ",
          "original_form": "바보같이 대처했던데ㅎ",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.20",
          "form": "여기도 날라갈뻔함ㅋㅋㅋㅋㅋㅋ",
          "original_form": "여기도 날라갈뻔함ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.21",
          "form": "이게 그거 아님? 한국 계정이",
          "original_form": "이게 그거 아님? 한국 계정이",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.22",
          "form": "??당연히 한국옷이죠 했다",
          "original_form": "??당연히 한국옷이죠 했다",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.23",
          "form": "지운거",
          "original_form": "지운거",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.24",
          "form": "ㅇㅇ 그거 맞음",
          "original_form": "ㅇㅇ 그거 맞음",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
