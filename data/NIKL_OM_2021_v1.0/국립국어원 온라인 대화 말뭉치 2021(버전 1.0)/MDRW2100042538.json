          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.26",
          "form": "근데 거기서 안지웟다간",
          "original_form": "근데 거기서 안지웟다간",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.27",
          "form": "또 샤닉처럼 공카 리셋되고",
          "original_form": "또 샤닉처럼 공카 리셋되고",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.28",
          "form": "그건 그렇긴 해",
          "original_form": "그건 그렇긴 해",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.29",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.30",
          "form": "공지로 우리는 어쩌구저쩌구 올라오는거지 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "공지로 우리는 어쩌구저쩌구 올라오는거지 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042538.1.31",
          "form": "걍 입이나",
          "original_form": "걍 입이나",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042538.1.32",
          "form": "다물고 있어라 좀",
          "original_form": "다물고 있어라 좀",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042538.1.33",
          "form": "진짜 다신 그때처럼",
          "original_form": "진짜 다신 그때처럼",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042538.1.34",
          "form": "덕질 못할듯",
          "original_form": "덕질 못할듯",
