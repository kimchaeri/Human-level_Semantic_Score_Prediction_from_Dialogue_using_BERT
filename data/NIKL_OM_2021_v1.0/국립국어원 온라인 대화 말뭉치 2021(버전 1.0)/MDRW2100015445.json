      "utterance": [
        {
          "id": "MDRW2100015445.1.1",
          "form": "오늘 뭐 드시나여?",
          "original_form": "오늘 뭐 드시나여?",
          "speaker_id": "1",
          "time": "20210826 07:29"
        },
        {
          "id": "MDRW2100015445.1.2",
          "form": "김치찌개요",
          "original_form": "김치찌개요",
          "speaker_id": "2",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100015445.1.3",
          "form": "name1님은 아침 드셨어요?",
          "original_form": "&name1&님은 아침 드셨어요?",
          "speaker_id": "2",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100015445.1.4",
          "form": "시리얼 먹었어요",
          "original_form": "시리얼 먹었어요",
          "speaker_id": "1",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100015445.1.5",
          "form": "무슨 시리얼 드섰나요?",
          "original_form": "무슨 시리얼 드섰나요?",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100015445.1.6",
          "form": "현미요",
          "original_form": "현미요",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100015445.1.7",
          "form": "이게 제일 맛있어요",
          "original_form": "이게 제일 맛있어요",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100015445.1.8",
          "form": "현미 씨리얼도 있어요?",
          "original_form": "현미 씨리얼도 있어요?",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100015445.1.9",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100015445.1.10",
          "form": "신기하다뇨",
          "original_form": "신기하다뇨",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100015445.1.11",
          "form": "저 자주 먹거든요",
          "original_form": "저 자주 먹거든요",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100015445.1.12",
          "form": "저는 콘푸로스트만 먹어요",
          "original_form": "저는 콘푸로스트만 먹어요",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100015445.1.13",
          "form": "그렇네요",
          "original_form": "그렇네요",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100015445.1.14",
          "form": "너무 달아요",
          "original_form": "너무 달아요",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100015445.1.15",
          "form": "콘푸로스트 미만 잡이에요",
          "original_form": "콘푸로스트 미만 잡이에요",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100015445.1.16",
          "form": "씨리얼은 콘푸로스트를 제외하고는 논할 수 없어요",
          "original_form": "씨리얼은 콘푸로스트를 제외하고는 논할 수 없어요",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100015445.1.17",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100015445.1.18",
          "form": "건강한 곡물 시리얼이 최고죠",
          "original_form": "건강한 곡물 시리얼이 최고죠",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100015445.1.19",
          "form": "그 호랑이도 단 콘푸로스트 많이 먹어서 맛간것처럼 생겼잖아요",
          "original_form": "그 호랑이도 단 콘푸로스트 많이 먹어서 맛간것처럼 생겼잖아요",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100015445.1.20",
          "form": "아닌데요",
          "original_form": "아닌데요",
          "speaker_id": "2",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100015445.1.21",
          "form": "전 호랑이 안먹어요",
          "original_form": "전 호랑이 안먹어요",
          "speaker_id": "2",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100015445.1.22",
          "form": "걔는 콘푸라이트에요",
          "original_form": "걔는 콘푸라이트에요",
          "speaker_id": "2",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100015445.1.23",
          "form": "전 사자 먹어요",
          "original_form": "전 사자 먹어요",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100015445.1.24",
          "form": "저지금 김치찌개 퍼왔어요",
          "original_form": "저지금 김치찌개 퍼왔어요",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100015445.1.25",
          "form": "마을쫑이랑 같이 먹을거예요",
          "original_form": "마을쫑이랑 같이 먹을거예요",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100015445.1.26",
          "form": "name1님은 제일 좋아하는 반찬이 무엇인가요",
          "original_form": "&name1&님은 제일 좋아하는 반찬이 무엇인가요",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100015445.1.27",
          "form": "그개 그거죠",
          "original_form": "그개 그거죠",
          "speaker_id": "1",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100015445.1.28",
          "form": "저는 가지볶음이요",
          "original_form": "저는 가지볶음이요",
          "speaker_id": "1",
          "time": "20210826 10:49"
        }
      ]
    }
