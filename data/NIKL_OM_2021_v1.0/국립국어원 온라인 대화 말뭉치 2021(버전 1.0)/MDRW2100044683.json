      "utterance": [
        {
          "id": "MDRW2100044683.1.1",
          "form": "이게 제가 산것중에서는 젤 비싸요",
          "original_form": "이게 제가 산것중에서는 젤 비싸요",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044683.1.2",
          "form": "우드에 캔버스 갓 씌여있는데",
          "original_form": "우드에 캔버스 갓 씌여있는데",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044683.1.3",
          "form": "13만원정도 했어여..",
          "original_form": "13만원정도 했어여..",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044683.1.4",
          "form": "근데 이쁘고 젤 맘에들어용",
          "original_form": "근데 이쁘고 젤 맘에들어용",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044683.1.5",
          "form": "헐 맞아요 이쁜건 비싸더라구요 ㅋㅋㅋㅋ그래도",
          "original_form": "헐 맞아요 이쁜건 비싸더라구요 ㅋㅋㅋㅋ그래도",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044683.1.6",
          "form": "한번사면 오래쓰니까 ㅋㅋㅋ",
          "original_form": "한번사면 오래쓰니까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044683.1.7",
          "form": "잘사셨어요 ㅋㅋㅋ",
          "original_form": "잘사셨어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044683.1.8",
          "form": "그리고 거울도 샀어요!!",
          "original_form": "그리고 거울도 샀어요!!",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044683.1.9",
          "form": "우와 거울 원하시던 반거울 사셨어요?",
          "original_form": "우와 거울 원하시던 반거울 사셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044683.1.10",
          "form": "네 웨이브 미러라고 비정형 거울 사봤어요!",
          "original_form": "네 웨이브 미러라고 비정형 거울 사봤어요!",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044683.1.11",
          "form": "헐 그게뭐에요??",
          "original_form": "헐 그게뭐에요??",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044683.1.12",
          "form": "웨이브?!?!?",
          "original_form": "웨이브?!?!?",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044683.1.13",
          "form": "모양이 곡선의 조합이랄까요?? ㅋㅋㅋㅋ",
          "original_form": "모양이 곡선의 조합이랄까요?? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044683.1.14",
          "form": "아아아 그래서 웨이브군요 ㅠㅠ 예쁠것같아요!!!",
          "original_form": "아아아 그래서 웨이브군요 ㅠㅠ 예쁠것같아요!!!",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044683.1.15",
          "form": "근데...전신거울도 샀어요",
          "original_form": "근데...전신거울도 샀어요",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044683.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044683.1.17",
          "form": "ㅋㅋㅋㅋㅋ악ㅋㅋㅋㅋㅋ전신거울은 어디에둘거에요?",
          "original_form": "ㅋㅋㅋㅋㅋ악ㅋㅋㅋㅋㅋ전신거울은 어디에둘거에요?",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044683.1.18",
          "form": "고민이에요...오면 고민해보려구요 우선샀어요 쿠폰이 오늘까지라서요",
          "original_form": "고민이에요...오면 고민해보려구요 우선샀어요 쿠폰이 오늘까지라서요",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044683.1.19",
          "form": "아항 전신 거울 좋아요 ㅋㅋㅋㅋ",
          "original_form": "아항 전신 거울 좋아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044683.1.20",
          "form": "저는 원래 부엌에둿었는데",
          "original_form": "저는 원래 부엌에둿었는데",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044683.1.21",
