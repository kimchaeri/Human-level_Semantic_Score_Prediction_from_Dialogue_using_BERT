      "utterance": [
        {
          "id": "MDRW2100035431.1.1",
          "form": "오늘 날씨는 어떤가요",
          "original_form": "오늘 날씨는 어떤가요",
          "speaker_id": "2",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100035431.1.2",
          "form": "여기는 아까까지는 비왔는데 지금은 안와요 습하긴해요ㅠㅠ",
          "original_form": "여기는 아까까지는 비왔는데 지금은 안와요 습하긴해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100035431.1.3",
          "form": "여기도 오락가락하네요",
          "original_form": "여기도 오락가락하네요",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100035431.1.4",
          "form": "천둥 번개도 쳤는데 언제그랬냐는듯",
          "original_form": "천둥 번개도 쳤는데 언제그랬냐는듯",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100035431.1.5",
          "form": "오 맞아요 아침에 번개도 쳤는데 지금은 갰네요",
          "original_form": "오 맞아요 아침에 번개도 쳤는데 지금은 갰네요",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100035431.1.6",
          "form": "오 설마 같은 지역이신가.ㅋ",
          "original_form": "오 설마 같은 지역이신가.ㅋ",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100035431.1.7",
          "form": "그런가요? 전국적으로 이상하긴 한듯요 .ㅎ 이번주도 내내 비예보예요",
          "original_form": "그런가요? 전국적으로 이상하긴 한듯요 .ㅎ 이번주도 내내 비예보예요",
          "speaker_id": "2",
          "time": "20210829 17:06"
        },
        {
          "id": "MDRW2100035431.1.8",
          "form": "아 ㅠㅠ비넘나싫은데ㅠ",
          "original_form": "아 ㅠㅠ비넘나싫은데ㅠ",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100035431.1.9",
          "form": "지난주 날씨 진짜 우울했죠",
          "original_form": "지난주 날씨 진짜 우울했죠",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035431.1.10",
          "form": "흐릿하니ㅠㅠ침침하고",
          "original_form": "흐릿하니ㅠㅠ침침하고",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100035431.1.11",
          "form": "아침 등원길마다 비가 어찌나 오든지요 ㅠ",
          "original_form": "아침 등원길마다 비가 어찌나 오든지요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035431.1.12",
          "form": "아 그럼 진짜 불편ㅠ",
          "original_form": "아 그럼 진짜 불편ㅠ",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035431.1.13",
          "form": "네네 유독 오전에 더 흐리더라구요",
          "original_form": "네네 유독 오전에 더 흐리더라구요",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035431.1.14",
          "form": "날씨가 이러니 사람도 쳐지고",
          "original_form": "날씨가 이러니 사람도 쳐지고",
          "speaker_id": "1",
          "time": "20210829 17:32"
        }
      ]
