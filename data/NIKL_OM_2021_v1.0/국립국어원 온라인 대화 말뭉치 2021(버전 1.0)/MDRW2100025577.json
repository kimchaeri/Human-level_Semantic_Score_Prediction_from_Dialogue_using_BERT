      "utterance": [
        {
          "id": "MDRW2100025577.1.1",
          "form": "저 혼자 기숙사에서 지낼 때는",
          "original_form": "저 혼자 기숙사에서 지낼 때는",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025577.1.2",
          "form": "성당 안가곤 했거든요",
          "original_form": "성당 안가곤 했거든요",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025577.1.3",
          "form": "근데,,, 집에서 지내니까 이제,,,",
          "original_form": "근데,,, 집에서 지내니까 이제,,,",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025577.1.4",
          "form": "안갈수가 없더라구요....",
          "original_form": "안갈수가 없더라구요....{emoji:☆}",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025577.1.5",
          "form": "ㅎㅎ가족끼리 있으면 그럴수밖에 없죠..",
          "original_form": "ㅎㅎ가족끼리 있으면 그럴수밖에 없죠..",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025577.1.6",
          "form": "그래도 종교가 확실히 마음의 위안이되나요?",
          "original_form": "그래도 종교가 확실히 마음의 위안이되나요?",
          "speaker_id": "2",
          "time": "20210901 13:47"
        },
        {
          "id": "MDRW2100025577.1.7",
          "form": "가끔은 되기도 하고.. 가끔은 귀찮기도 하고,,,",
          "original_form": "가끔은 되기도 하고.. 가끔은 귀찮기도 하고,,,",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025577.1.8",
          "form": "근데 기적을 바랄 땐 하느님 찾게 되긴 하더라구요ㅠ",
          "original_form": "근데 기적을 바랄 땐 하느님 찾게 되긴 하더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025577.1.9",
          "form": "name2님은요?!",
          "original_form": "&name2&님은요?!",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025577.1.10",
          "form": "저도 양심없지만 기적을 바랄때 하느님마리아님 하는거같긴해요 ㅋㅋ",
          "original_form": "저도 양심없지만 기적을 바랄때 하느님마리아님 하는거같긴해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025577.1.11",
          "form": "그치만 오랫동안 안가서 그런지 종교에 의지한다는 것 자체가 상상이 안간달까요? 그런느낌이에요",
          "original_form": "그치만 오랫동안 안가서 그런지 종교에 의지한다는 것 자체가 상상이 안간달까요? 그런느낌이에요",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025577.1.12",
          "form": "생각이 안나요 ㅋㅋ",
          "original_form": "생각이 안나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025577.1.13",
          "form": "에이 양심없지 않아요~!~!",
          "original_form": "에이 양심없지 않아요~!~!",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025577.1.14",
          "form": "아 그쵸그쵸",
          "original_form": "아 그쵸그쵸",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025577.1.15",
          "form": "저도 사실 나이롱 신자예요ㅠ",
          "original_form": "저도 사실 나이롱 신자예요ㅠ",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025577.1.16",
          "form": "그리고 주일마다 헌금으로 내는 돈이 아까워요 ㅠㅋㅋㅋㅋㅋ",
          "original_form": "그리고 주일마다 헌금으로 내는 돈이 아까워요 ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025577.1.17",
          "form": "그래서 저는 2천원만 냈는데",
          "original_form": "그래서 저는 2천원만 냈는데",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
