      "utterance": [
        {
          "id": "MDRW2100016664.1.1",
          "form": "전 내년1월부터 캐나다와 한국으로 넘어갑니다ㅠ",
          "original_form": "전 내년1월부터 캐나다와 한국으로 넘어갑니다ㅠ",
          "speaker_id": "1",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100016664.1.2",
          "form": "장거리가 어디어디였는데요?ㅋㅋㅋㅋ",
          "original_form": "장거리가 어디어디였는데요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016664.1.3",
          "form": "와 이민가시나요??",
          "original_form": "와 이민가시나요??",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016664.1.4",
          "form": "시흥하고 포항이요ㅋㅋㅋㅋㅋ",
          "original_form": "시흥하고 포항이요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016664.1.5",
          "form": "버스로 4시간반?",
          "original_form": "버스로 4시간반?",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016664.1.6",
          "form": "와 ㅋㅋㅋㅋ 엄청머네요",
          "original_form": "와 ㅋㅋㅋㅋ 엄청머네요",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016664.1.7",
          "form": "남친이 유학이요,,,ㅋㅋㅋㅋ ㅠ",
          "original_form": "남친이 유학이요,,,ㅋㅋㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016664.1.8",
          "form": "그러게요 어떻게 했는지",
          "original_form": "그러게요 어떻게 했는지",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016664.1.9",
          "form": "ㅎㄷㄷ 유학",
          "original_form": "ㅎㄷㄷ 유학",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016664.1.10",
          "form": "최소 3년이라 끝이보이네요~",
          "original_form": "최소 3년이라 끝이보이네요~",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016664.1.11",
          "form": "왕복 9시간인데 미쳤다,,중간에서 만났었겠는데요",
          "original_form": "왕복 9시간인데 미쳤다,,중간에서 만났었겠는데요",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016664.1.12",
          "form": "능력있으신가봐요 이런 시기에 유학이면",
          "original_form": "능력있으신가봐요 이런 시기에 유학이면",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016664.1.13",
          "form": "중간에서 만나면 더 힘들던데요? 드물게 중간에서 놀고ㅋㅋ",
          "original_form": "중간에서 만나면 더 힘들던데요? 드물게 중간에서 놀고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016664.1.14",
          "form": "생각보다 싸던데요? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "생각보다 싸던데요? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016664.1.15",
          "form": "그래서 대전도 가봣어요 두어번",
          "original_form": "그래서 대전도 가봣어요 두어번",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016664.1.16",
          "form": "대전에서 할거 진짜~ 없어요 ㅋㅋㅋㅋㅋ",
          "original_form": "대전에서 할거 진짜~ 없어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016664.1.17",
          "form": "그 동물공원??? 거기 갔었는듯요",
          "original_form": "그 동물공원??? 거기 갔었는듯요",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016664.1.18",
          "form": "맞나 거기가 대전이",
          "original_form": "맞나 거기가 대전이",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016664.1.19",
          "form": "동물공원이라뇨 ㅋㅋㅋㅋㅋㅋㅋㅋ동물원,,",
          "original_form": "동물공원이라뇨 ㅋㅋㅋㅋㅋㅋㅋㅋ동물원,,",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016664.1.20",
          "form": "오월드갔구나",
          "original_form": "오월드갔구나",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016664.1.21",
          "form": "동물원은 아니었는데...",
          "original_form": "동물원은 아니었는데...",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016664.1.22",
          "form": "그냥 공원인데 동물 많았어요",
          "original_form": "그냥 공원인데 동물 많았어요",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016664.1.23",
          "form": "거긴 대군가ㅋㅋㅋㅋㅋㅋ",
          "original_form": "거긴 대군가ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016664.1.24",
          "form": "뭐지?",
          "original_form": "뭐지?",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016664.1.25",
          "form": "이름바꼇어요 원래 대전동물원이였는데 ㅋㅋㅋ",
          "original_form": "이름바꼇어요 원래 대전동물원이였는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016664.1.26",
          "form": "모르겠닼ㅋㅋㅋ",
          "original_form": "모르겠닼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016664.1.27",
          "form": "캐나다에서 최소 3년 사시는거에요?",
          "original_form": "캐나다에서 최소 3년 사시는거에요?",
          "speaker_id": "2",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100016664.1.28",
          "form": "졸업하려면 2년 학교다니고,, 졸업하면 취업비자를 3년동안준다네여",
          "original_form": "졸업하려면 2년 학교다니고,, 졸업하면 취업비자를 3년동안준다네여",
          "speaker_id": "1",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100016664.1.29",
          "form": "근데 거기서 일하고싶대요",
          "original_form": "근데 거기서 일하고싶대요",
          "speaker_id": "1",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016664.1.30",
          "form": "최대 5년 ㅋㅋㅋㅋㅋ",
          "original_form": "최대 5년 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016664.1.31",
          "form": "죄송하지만 나이가 어케되세요??",
          "original_form": "죄송하지만 나이가 어케되세요??",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016664.1.32",
          "form": "저 26이용 ㅋㅋㅋㅋ",
          "original_form": "저 26이용 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016664.1.33",
          "form": "어렵네요.. 결혼 생각하고 같이 넘어가면 모를까",
          "original_form": "어렵네요.. 결혼 생각하고 같이 넘어가면 모를까",
          "speaker_id": "2",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016664.1.34",
          "form": "빠르면 20중반에도 하긴 하던데",
          "original_form": "빠르면 20중반에도 하긴 하던데",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016664.1.35",
          "form": "에이 같이넘어가다뇨 ㅋㅋㅋㅋㅋ",
          "original_form": "에이 같이넘어가다뇨 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016664.1.36",
          "form": "결혼은 아직 남친은생각없더라구요",
          "original_form": "결혼은 아직 남친은생각없더라구요",
          "speaker_id": "1",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016664.1.37",
          "form": "저는빨리하고싶은데 ㅋㅋㅋ",
          "original_form": "저는빨리하고싶은데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016664.1.38",
          "form": "남친분은 나이가 우찌되나요?",
          "original_form": "남친분은 나이가 우찌되나요?",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016664.1.39",
          "form": "동갑임다~ ㅋㅋㅋ",
          "original_form": "동갑임다~ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016664.1.40",
          "form": "ㅠ... 예전의 저를 보는 느낌이군요",
          "original_form": "ㅠ... 예전의 저를 보는 느낌이군요",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016664.1.41",
          "form": "저는 사회생활 일찍시작해서 빨리결혼하고싶은데",
          "original_form": "저는 사회생활 일찍시작해서 빨리결혼하고싶은데",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016664.1.42",
          "form": "동갑이었는데ㅎㅎ 남자쪽이 다이아수저가 아닌 이상 그 나이엔 결혼 쉽지 않아가지구..",
          "original_form": "동갑이었는데ㅎㅎ 남자쪽이 다이아수저가 아닌 이상 그 나이엔 결혼 쉽지 않아가지구..",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016664.1.43",
          "form": "지금남친은 공뷰하러가는입장이라서",
          "original_form": "지금남친은 공뷰하러가는입장이라서",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016664.1.44",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016664.1.45",
          "form": "상황은 저랑 반대였네요",
          "original_form": "상황은 저랑 반대였네요",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016664.1.46",
          "form": "네 그러네여",
          "original_form": "네 그러네여",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016664.1.47",
          "form": "나이가어캐되세여?",
          "original_form": "나이가어캐되세여?",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016664.1.48",
          "form": "서른둘이네요ㅋㅋㅋ 하 언제 일케됐지",
          "original_form": "서른둘이네요ㅋㅋㅋ 하 언제 일케됐지",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016664.1.49",
          "form": "90이시군요 ㅋㅋㅋㅋ",
          "original_form": "90이시군요 ㅋㅋㅋㅋ",
