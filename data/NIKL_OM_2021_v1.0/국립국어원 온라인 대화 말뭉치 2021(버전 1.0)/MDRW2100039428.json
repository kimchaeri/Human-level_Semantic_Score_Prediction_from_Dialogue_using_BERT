      "utterance": [
        {
          "id": "MDRW2100039428.1.1",
          "form": "아니 뭐가 문제일까요!!!!",
          "original_form": "아니 뭐가 문제일까요!!!!",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039428.1.2",
          "form": "네네 저 유튜브 보면서",
          "original_form": "네네 저 유튜브 보면서",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039428.1.3",
          "form": "실내자전거 타는 게 전부...",
          "original_form": "실내자전거 타는 게 전부...",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039428.1.4",
          "form": "저스트댄스 따라하는거 전부였어요,,,",
          "original_form": "저스트댄스 따라하는거 전부였어요,,,",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039428.1.5",
          "form": "만 보 걷기 이 정도....?",
          "original_form": "만 보 걷기 이 정도....?",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039428.1.6",
          "form": "name1님 혹시 약대 대학원 다니는 친구분",
          "original_form": "&name1&님 혹시 약대 대학원 다니는 친구분",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039428.1.7",
          "form": "이번에 생화학 수업 들으시나요...?!?!",
          "original_form": "이번에 생화학 수업 들으시나요...?!?!",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039428.1.8",
          "form": "오 물어볼게요!",
          "original_form": "오 물어볼게요!",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039428.1.9",
          "form": "네네 약대 대학원에서도 누가 저랑 같은 수업을 듣길래",
          "original_form": "네네 약대 대학원에서도 누가 저랑 같은 수업을 듣길래",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039428.1.10",
          "form": "너무 궁금하네요!!! 히히 감사해요!!!",
          "original_form": "너무 궁금하네요!!! 히히 감사해요!!!",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039428.1.11",
          "form": "듣는대욬ㅋㅋㅋㅋ",
          "original_form": "듣는대욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039428.1.12",
          "form": "맞는가본데요?대박",
          "original_form": "맞는가본데요?대박",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039428.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039428.1.14",
          "form": "생화학분석 어쩌구라던데",
          "original_form": "생화학분석 어쩌구라던데",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039428.1.15",
          "form": "악",
          "original_form": "악",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039428.1.16",
          "form": "아 분석인가요",
          "original_form": "아 분석인가요",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039428.1.17",
          "form": "아 다른쉅이에요?",
          "original_form": "아 다른쉅이에요?",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039428.1.18",
          "form": "그럼 저랑 같은 수업은 아닌가보다 으악 ㅠ",
          "original_form": "그럼 저랑 같은 수업은 아닌가보다 으악 ㅠ",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039428.1.19",
          "form": "자연대 수업 아닌거죠???",
          "original_form": "자연대 수업 아닌거죠???",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039428.1.20",
          "form": "혹시 그분이름 기억나시는지",
          "original_form": "혹시 그분이름 기억나시는지",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039428.1.21",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039428.1.22",
          "form": "잠시만요~~",
          "original_form": "잠시만요~~",
