      "utterance": [
        {
          "id": "MDRW2100020804.1.1",
          "form": "보통 불 도트뎀 얼음 이속감소 전기 경직효과 인데",
          "original_form": "보통 불 도트뎀 얼음 이속감소 전기 경직효과 인데",
          "speaker_id": "2",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020804.1.2",
          "form": "범위 내 적 전체 빙결 스킬 있던데",
          "original_form": "범위 내 적 전체 빙결 스킬 있던데",
          "speaker_id": "1",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020804.1.3",
          "form": "난 그거보단",
          "original_form": "난 그거보단",
          "speaker_id": "2",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020804.1.4",
          "form": "이게 물 묻어야만 발동이면 너무 제한적이긴 해",
          "original_form": "이게 물 묻어야만 발동이면 너무 제한적이긴 해",
          "speaker_id": "1",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020804.1.5",
          "form": "이나즈마에선 유용하게 쓰겠디만...",
          "original_form": "이나즈마에선 유용하게 쓰겠디만...",
          "speaker_id": "1",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020804.1.6",
          "form": "지속 원소 묻히는거 있으면",
          "original_form": "지속 원소 묻히는거 있으면",
          "speaker_id": "2",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020804.1.7",
          "form": "좋겠어",
          "original_form": "좋겠어",
          "speaker_id": "2",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020804.1.8",
          "form": "그거는 타탈이긴 한데...",
          "original_form": "그거는 타탈이긴 한데...",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020804.1.9",
          "form": "타탈 스킬이 적한테 지속적으로 물원소 묻히는거야",
          "original_form": "타탈 스킬이 적한테 지속적으로 물원소 묻히는거야",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020804.1.10",
          "form": "아 물 말고",
          "original_form": "아 물 말고",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020804.1.11",
          "form": "얼음",
          "original_form": "얼음",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020804.1.12",
          "form": "물 필요없어...",
          "original_form": "물 필요없어...",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020804.1.13",
          "form": "아 얼음",
          "original_form": "아 얼음",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020804.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020804.1.15",
          "form": "불 전기 얼음 세개 돌려쓰는게",
          "original_form": "불 전기 얼음 세개 돌려쓰는게",
          "speaker_id": "2",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020804.1.16",
          "form": "과부하 초전도 융해",
          "original_form": "과부하 초전도 융해",
          "speaker_id": "2",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020804.1.17",
          "form": "돌아가면서 터져서",
          "original_form": "돌아가면서 터져서",
          "speaker_id": "2",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020804.1.18",
          "form": "제일 나은것같아",
          "original_form": "제일 나은것같아",
          "speaker_id": "2",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020804.1.19",
          "form": "그래서 게임 밸런스 맞추려고 안내는 걸지도...",
          "original_form": "그래서 게임 밸런스 맞추려고 안내는 걸지도...",
          "speaker_id": "1",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020804.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100020804.1.21",
          "form": "여튼 걍 좀",
          "original_form": "여튼 걍 좀",
          "speaker_id": "2",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100020804.1.22",
          "form": "쓸만했으면....",
          "original_form": "쓸만했으면....",
          "speaker_id": "2",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100020804.1.23",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100020804.1.24",
          "form": "플레이 영상에서는",
          "original_form": "플레이 영상에서는",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100020804.1.25",
          "form": "딱히 물 없는데 유적가디언 빙결시키던데",
          "original_form": "딱히 물 없는데 유적가디언 빙결시키던데",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100020804.1.26",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020804.1.27",
          "form": "홍보영상이라 믿을 수가 없음...",
          "original_form": "홍보영상이라 믿을 수가 없음...",
          "speaker_id": "1",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020804.1.28",
          "form": "그러면 좀 사긴데",
          "original_form": "그러면 좀 사긴데",
          "speaker_id": "2",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020804.1.29",
          "form": "ㅇㅇㅋㅋㅋㅋㅋ당장 영상에선",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋ당장 영상에선",
          "speaker_id": "2",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020804.1.30",
          "form": "주인공도 바위랑 바람 같이 씀",
          "original_form": "주인공도 바위랑 바람 같이 씀",
          "speaker_id": "2",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020804.1.31",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020804.1.32",
          "form": "그게 제일 사기야",
          "original_form": "그게 제일 사기야",
          "speaker_id": "1",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020804.1.33",
          "form": "플레이어도 그거 쓰게 해달라",
          "original_form": "플레이어도 그거 쓰게 해달라",
          "speaker_id": "1",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020804.1.34",
          "form": "바람이랑 전기 같이 쓰고싶다고",
          "original_form": "바람이랑 전기 같이 쓰고싶다고",
          "speaker_id": "1",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020804.1.35",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋ그거만 되면",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋ그거만 되면",
          "speaker_id": "2",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020804.1.36",
          "form": "5성이 왜필요해",
          "original_form": "5성이 왜필요해",
          "speaker_id": "2",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100020804.1.37",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100020804.1.38",
          "form": "ㅇㅇ타탈도 하는데",
          "original_form": "ㅇㅇ타탈도 하는데",
          "speaker_id": "2",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100020804.1.39",
          "form": "주인공의 장점을 달라고",
          "original_form": "주인공의 장점을 달라고",
          "speaker_id": "1",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100020804.1.40",
          "form": "왜 주인공은 안시켜주냐",
          "original_form": "왜 주인공은 안시켜주냐",
          "speaker_id": "2",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100020804.1.41",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100020804.1.42",
          "form": "주인공도 세가지 속성 동시에 쓸 줄 안다",
          "original_form": "주인공도 세가지 속성 동시에 쓸 줄 안다",
          "speaker_id": "1",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100020804.1.43",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210828 12:55"
        }
      ]
    }
