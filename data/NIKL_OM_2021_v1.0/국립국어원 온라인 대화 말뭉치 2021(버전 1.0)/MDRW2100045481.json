      "utterance": [
        {
          "id": "MDRW2100045481.1.1",
          "form": "기분전환겸 뭐 보시나요?",
          "original_form": "기분전환겸 뭐 보시나요?",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045481.1.2",
          "form": "취미 궁금해요!",
          "original_form": "취미 궁금해요!",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045481.1.3",
          "form": "요즘은 인 트리트먼트 시즌3을 봅니다.",
          "original_form": "요즘은 인 트리트먼트 시즌3을 봅니다.",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045481.1.4",
          "form": "얼마 전엔 빌리언스를 봤어요",
          "original_form": "얼마 전엔 빌리언스를 봤어요",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045481.1.5",
          "form": "오 다 미드인가요?",
          "original_form": "오 다 미드인가요?",
          "speaker_id": "1",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045481.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045481.1.7",
          "form": "뭐 보시나요?",
          "original_form": "뭐 보시나요?",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045481.1.8",
          "form": "전 요즘 유투브 알고리즘더분에 클래식ㅋㅋ",
          "original_form": "전 요즘 유투브 알고리즘더분에 클래식ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045481.1.9",
          "form": "우아한유령 추천이요",
          "original_form": "우아한유령 추천이요",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045481.1.10",
          "form": "오 찾아봐야겠어요",
          "original_form": "오 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045481.1.11",
          "form": "저도 연휴에 미드 봐야겠어요!",
          "original_form": "저도 연휴에 미드 봐야겠어요!",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045481.1.12",
          "form": "아 이거 좋네요, 감사해요 ㅎㅎ",
          "original_form": "아 이거 좋네요, 감사해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045481.1.13",
          "form": "빌리언스는 헤지펀드 얘기라 엄청 재밌었어요",
          "original_form": "빌리언스는 헤지펀드 얘기라 엄청 재밌었어요",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045481.1.14",
          "form": "네 그거랑 피아졸라 탱고의역사?",
          "original_form": "네 그거랑 피아졸라 탱고의역사?",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045481.1.15",
          "form": "추천이요",
          "original_form": "추천이요",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045481.1.16",
          "form": "헤지펀드 공부하겠네요!",
          "original_form": "헤지펀드 공부하겠네요!",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045481.1.17",
          "form": "오 저 피아졸라 좋아해요",
          "original_form": "오 저 피아졸라 좋아해요",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045481.1.18",
          "form": "아!!!",
          "original_form": "아!!!",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045481.1.19",
          "form": "저 이번에 알아서 찾아보고있었어요!",
          "original_form": "저 이번에 알아서 찾아보고있었어요!",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045481.1.20",
          "form": "주인공의 모태가 포인트72의 스티브 코헨이라더라고요",
          "original_form": "주인공의 모태가 포인트72의 스티브 코헨이라더라고요",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045481.1.21",
          "form": "아 잘모르는건데 공부하면서ㅋㅋ",
          "original_form": "아 잘모르는건데 공부하면서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045481.1.22",
          "form": "봐야겠어요",
          "original_form": "봐야겠어요",
          "speaker_id": "1",
          "time": "20210908 02:54"
        }
      ]
