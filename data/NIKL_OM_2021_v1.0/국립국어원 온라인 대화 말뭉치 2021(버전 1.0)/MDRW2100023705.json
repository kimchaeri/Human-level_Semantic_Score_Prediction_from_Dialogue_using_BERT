        {
          "id": "MDRW2100023705.1.1",
          "form": "탕수육 좋아하세요?",
          "original_form": "탕수육 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023705.1.2",
          "form": "저도 완전 !",
          "original_form": "저도 완전 !",
          "speaker_id": "2",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023705.1.3",
          "form": "탕수육은 부먹인가요 찍먹인가요?",
          "original_form": "탕수육은 부먹인가요 찍먹인가요?",
          "speaker_id": "2",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023705.1.4",
          "form": "저는 진짜 둘다 상관없어요",
          "original_form": "저는 진짜 둘다 상관없어요",
          "speaker_id": "1",
          "time": "20210830 15:04"
        },
        {
          "id": "MDRW2100023705.1.5",
          "form": "같이먹는 상대에따라서!!",
          "original_form": "같이먹는 상대에따라서!!",
          "speaker_id": "1",
          "time": "20210830 15:04"
        },
        {
          "id": "MDRW2100023705.1.6",
          "form": "오 배려심 좋습니다",
          "original_form": "오 배려심 좋습니다",
          "speaker_id": "2",
          "time": "20210830 15:04"
        },
        {
          "id": "MDRW2100023705.1.7",
          "form": "저도 다 좋아요",
          "original_form": "저도 다 좋아요",
          "speaker_id": "2",
          "time": "20210830 15:04"
        },
        {
          "id": "MDRW2100023705.1.8",
          "form": "바삭한거 좋아요?아니면 찹살 좋아하세요?",
          "original_form": "바삭한거 좋아요?아니면 찹살 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100023705.1.9",
          "form": "그것도 둘다 좋아해요 ㅎㅎ",
          "original_form": "그것도 둘다 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100023705.1.10",
          "form": "전 진차 다 잘먹어욥",
          "original_form": "전 진차 다 잘먹어욥",
          "speaker_id": "1",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100023705.1.11",
          "form": "좋은 식습관입니다 ㅎㅎ",
          "original_form": "좋은 식습관입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:01"
        },
        {
          "id": "MDRW2100023705.1.12",
          "form": "성격도 좋으실듯 ㅎㅎ",
          "original_form": "성격도 좋으실듯 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:01"
        },
        {
          "id": "MDRW2100023705.1.13",
          "form": "좋아하는 음식잇으신가요",
          "original_form": "좋아하는 음식잇으신가요",
          "speaker_id": "1",
          "time": "20210830 16:02"
        },
        {
          "id": "MDRW2100023705.1.14",
          "form": "음 방금 피자 시켰습니다 ㅎㅎ",
          "original_form": "음 방금 피자 시켰습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100023705.1.15",
          "form": "회도 좋아하구요",
          "original_form": "회도 좋아하구요",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023705.1.16",
          "form": "다 좋아해요 ㅋㅋ안가립니다",
          "original_form": "다 좋아해요 ㅋㅋ안가립니다",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023705.1.17",
          "form": "잡식성입니당 ㅋㅋ",
          "original_form": "잡식성입니당 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023705.1.18",
          "form": "아 저도 피자랑 회 해산물 이런거 진짜진짜 좋아해요",
          "original_form": "아 저도 피자랑 회 해산물 이런거 진짜진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100023705.1.19",
          "form": "없어서 못머는거져 ㅎㅎ",
          "original_form": "없어서 못머는거져 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100023705.1.20",
          "form": "저희집근처가 어시장입니다",
          "original_form": "저희집근처가 어시장입니다",
          "speaker_id": "2",
          "time": "20210831 12:04"
        },
        {
          "id": "MDRW2100023705.1.21",
          "form": "마산어시장 아시져?",
          "original_form": "마산어시장 아시져?",
          "speaker_id": "2",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100023705.1.22",
          "form": "이사온지 한달됫는데 해산물 넘쳐나요",
          "original_form": "이사온지 한달됫는데 해산물 넘쳐나요",
