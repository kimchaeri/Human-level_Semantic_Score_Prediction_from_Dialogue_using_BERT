      "utterance": [
        {
          "id": "MDRW2100021829.1.1",
          "form": "마스크 쓰면 눈만 보이니 눈성형도 가끔 고민되요 ㅎㅎ",
          "original_form": "마스크 쓰면 눈만 보이니 눈성형도 가끔 고민되요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021829.1.2",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021829.1.3",
          "form": "쌍수말이져",
          "original_form": "쌍수말이져",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021829.1.4",
          "form": "전 속쌍꺼플이라",
          "original_form": "전 속쌍꺼플이라",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021829.1.5",
          "form": "걍 포기했어요",
          "original_form": "걍 포기했어요",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021829.1.6",
          "form": "어멋 똑같네요 ㄷㄷ",
          "original_form": "어멋 똑같네요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021829.1.7",
          "form": "노력하지만 과하면 안되는",
          "original_form": "노력하지만 과하면 안되는",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021829.1.8",
          "form": "섀도우",
          "original_form": "섀도우",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021829.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021829.1.10",
          "form": "그래서 얼마전에 에뛰드하우스에서 나온 마스카라 샀는데",
          "original_form": "그래서 얼마전에 에뛰드하우스에서 나온 마스카라 샀는데",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021829.1.11",
          "form": "괜찮았어요",
          "original_form": "괜찮았어요",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021829.1.12",
          "form": "오홍 정보 주섬주섬~ ㅎㅎㅎ",
          "original_form": "오홍 정보 주섬주섬~ ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100021829.1.13",
          "form": "수분크림 어떤거쓰세요",
          "original_form": "수분크림 어떤거쓰세요",
          "speaker_id": "2",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100021829.1.14",
          "form": "다떨어졌는데",
          "original_form": "다떨어졌는데",
          "speaker_id": "2",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100021829.1.15",
          "form": "계절이 바뀌기까지해서",
          "original_form": "계절이 바뀌기까지해서",
          "speaker_id": "2",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100021829.1.16",
          "form": "뭘로 쓸지고민되네여",
          "original_form": "뭘로 쓸지고민되네여",
          "speaker_id": "2",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100021829.1.17",
          "form": "저는  에스트라 아토베리어 수분크림 써요",
          "original_form": "저는  에스트라 아토베리어 수분크림 써요",
          "speaker_id": "1",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100021829.1.18",
          "form": "오 거기꺼알아요",
          "original_form": "오 거기꺼알아요",
          "speaker_id": "2",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021829.1.19",
          "form": "썬크림써봤는데",
          "original_form": "썬크림써봤는데",
          "speaker_id": "2",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021829.1.20",
          "form": "성분이좋대요",
          "original_form": "성분이좋대요",
          "speaker_id": "2",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021829.1.21",
          "form": "네네 병원 처방도 되더라구요 \n수분크림은 모르겠고\n로션이랑 크림 된다해서 나중에  처방받아 사려구요 ㅎㅎ",
          "original_form": "네네 병원 처방도 되더라구요 \n수분크림은 모르겠고\n로션이랑 크림 된다해서 나중에  처방받아 사려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021829.1.22",
          "form": "아 그래요? 병원처방받으면 화장품인데",
          "original_form": "아 그래요? 병원처방받으면 화장품인데",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021829.1.23",
          "form": "더 싸게 사나요??",
          "original_form": "더 싸게 사나요??",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021829.1.24",
          "form": "보험으로 ㅎㅎ",
          "original_form": "보험으로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021829.1.25",
          "form": "실비청구 되요",
          "original_form": "실비청구 되요",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021829.1.26",
          "form": "우와 좋은정보감사해여",
          "original_form": "우와 좋은정보감사해여",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021829.1.27",
          "form": "ㅎㅎ 저는 이거 에스트라 이벤트때 당첨되서 쓰기 시작했는데  괜찮은거 같아요",
          "original_form": "ㅎㅎ 저는 이거 에스트라 이벤트때 당첨되서 쓰기 시작했는데  괜찮은거 같아요",
          "speaker_id": "1",
          "time": "20210828 23:15"
        }
      ]
