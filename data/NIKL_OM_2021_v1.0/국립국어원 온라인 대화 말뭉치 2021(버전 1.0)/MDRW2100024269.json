      "utterance": [
        {
          "id": "MDRW2100024269.1.1",
          "form": "오 그럼저는 필사하문서 다음책 빌려오께용",
          "original_form": "오 그럼저는 필사하문서 다음책 빌려오께용",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024269.1.2",
          "form": "보시고 괜찮은 책은 추천 부탁이요ㅎ",
          "original_form": "보시고 괜찮은 책은 추천 부탁이요ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024269.1.3",
          "form": "나는 주로 필사할듯 ㅋㅋ",
          "original_form": "나는 주로 필사할듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024269.1.4",
          "form": "그 필사한 지식이 내거가 되는 느낌이 좀 들던가요?",
          "original_form": "그 필사한 지식이 내거가 되는 느낌이 좀 들던가요?",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024269.1.5",
          "form": "효과적이던가요?",
          "original_form": "효과적이던가요?",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024269.1.6",
          "form": "정독하는효과가 있지만",
          "original_form": "정독하는효과가 있지만",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024269.1.7",
          "form": "내것은 안됩미다",
          "original_form": "내것은 안됩미다",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024269.1.8",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024269.1.9",
          "form": "1도 내것이 안되던가요?",
          "original_form": "1도 내것이 안되던가요?",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024269.1.10",
          "form": "가물가물치",
          "original_form": "가물가물치",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100024269.1.11",
          "form": "아 뜬금없지만 전어철이래요",
          "original_form": "아 뜬금없지만 전어철이래요",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024269.1.12",
          "form": "오 전어 맛나겠다",
          "original_form": "오 전어 맛나겠다",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024269.1.13",
          "form": "그럼 저너저너치",
          "original_form": "그럼 저너저너치",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024269.1.14",
          "form": "그쵸 가물가물치보다는 저너저너치가 더 계절에 맞는거 같아요ㅋㅋ",
          "original_form": "그쵸 가물가물치보다는 저너저너치가 더 계절에 맞는거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100024269.1.15",
          "form": "책이야기를 하자면 한강의 소년이 온다 필력이 참 대단했어요",
          "original_form": "책이야기를 하자면 한강의 소년이 온다 필력이 참 대단했어요",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100024269.1.16",
          "form": "적절히 찰떡같은 비유들이 많았음 기억이 안나지만",
          "original_form": "적절히 찰떡같은 비유들이 많았음 기억이 안나지만",
          "speaker_id": "2",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100024269.1.17",
          "form": "무슨 내용이던가요?",
          "original_form": "무슨 내용이던가요?",
          "speaker_id": "1",
          "time": "20210831 09:51"
        },
        {
          "id": "MDRW2100024269.1.18",
          "form": "주제가?",
          "original_form": "주제가?",
          "speaker_id": "1",
          "time": "20210831 09:51"
        },
        {
          "id": "MDRW2100024269.1.19",
          "form": "518광주민주항쟁 이야기죠 방송국에 남아 싸우던 사람들이야기나와요",
          "original_form": "518광주민주항쟁 이야기죠 방송국에 남아 싸우던 사람들이야기나와요",
          "speaker_id": "2",
          "time": "20210831 09:53"
        },
        {
          "id": "MDRW2100024269.1.20",
          "form": "앗 재밌겠는데요",
          "original_form": "앗 재밌겠는데요",
          "speaker_id": "1",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024269.1.21",
          "form": "얼마전에 전두환아프다는 뉴스봤는데",
          "original_form": "얼마전에 전두환아프다는 뉴스봤는데",
          "speaker_id": "1",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024269.1.22",
          "form": "수천명이 제명에 못살고 비명횡사했는데",
          "original_form": "수천명이 제명에 못살고 비명횡사했는데",
          "speaker_id": "2",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100024269.1.23",
          "form": "전두환은 노환이라네요",
          "original_form": "전두환은 노환이라네요",
          "speaker_id": "2",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100024269.1.24",
          "form": "아직도 본인은 왜 나만가지고 그러는지 모르시겠대요",
          "original_form": "아직도 본인은 왜 나만가지고 그러는지 모르시겠대요",
          "speaker_id": "1",
          "time": "20210831 11:03"
        },
        {
          "id": "MDRW2100024269.1.25",
          "form": "ㅋㅋ대단하군요 다른의미로",
          "original_form": "ㅋㅋ대단하군요 다른의미로",
          "speaker_id": "2",
          "time": "20210831 11:03"
        }
      ]
