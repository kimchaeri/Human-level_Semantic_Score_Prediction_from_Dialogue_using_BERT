      "utterance": [
        {
          "id": "MDRW2100005289.1.1",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005289.1.2",
          "form": "서울에 인왕산 추천해",
          "original_form": "서울에 인왕산 추천해",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005289.1.3",
          "form": "등산이야?",
          "original_form": "등산이야?",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005289.1.4",
          "form": "등산이라기보다는 야경이 정말 멋져",
          "original_form": "등산이라기보다는 야경이 정말 멋져",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005289.1.5",
          "form": "지금 사진 찾아보는 중인데 멋있다",
          "original_form": "지금 사진 찾아보는 중인데 멋있다",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005289.1.6",
          "form": "한양 성곽 따라서 올라가서 내려다 보는 서울 야경이 진짜 멋지더라",
          "original_form": "한양 성곽 따라서 올라가서 내려다 보는 서울 야경이 진짜 멋지더라",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005289.1.7",
          "form": "나도 꼭 가보고 싶다",
          "original_form": "나도 꼭 가보고 싶다",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005289.1.8",
          "form": "드라이브로 북악 스카이웨이도 추천해",
          "original_form": "드라이브로 북악 스카이웨이도 추천해",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005289.1.9",
          "form": "어 거기 가봤어",
          "original_form": "어 거기 가봤어",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005289.1.10",
          "form": "가봤구나",
          "original_form": "가봤구나",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005289.1.11",
          "form": "거기도 야경이 멋지잖아",
          "original_form": "거기도 야경이 멋지잖아",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005289.1.12",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005289.1.13",
          "form": "근데 차 없이는 가기 불편하겠더라",
          "original_form": "근데 차 없이는 가기 불편하겠더라",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005289.1.14",
          "form": "대중교통으로는 가기 어렵나",
          "original_form": "대중교통으로는 가기 어렵나",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005289.1.15",
          "form": "갈 수는 있는데 좀 복잡하지",
          "original_form": "갈 수는 있는데 좀 복잡하지",
          "speaker_id": "2",
