      "utterance": [
        {
          "id": "MDRW2100006684.1.1",
          "form": "샤브샤브하니까 거기 생각난다",
          "original_form": "샤브샤브하니까 거기 생각난다",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006684.1.2",
          "form": "여의도 그 뭐냐",
          "original_form": "여의도 그 뭐냐",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006684.1.3",
          "form": "IFC 몰에있는 제일제면소?",
          "original_form": "IFC 몰에있는 제일제면소?",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006684.1.4",
          "form": "어어 거기 진짜 너무 좋아ㅋㅋ",
          "original_form": "어어 거기 진짜 너무 좋아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006684.1.5",
          "form": "새우랑 관자 무한이자나ㅋㅋ",
          "original_form": "새우랑 관자 무한이자나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100006684.1.6",
          "form": "넌 새우킬러 난 고기킬러",
          "original_form": "넌 새우킬러 난 고기킬러",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100006684.1.7",
          "form": "우리 초반에 막 일주일에 한번씩 가니까",
          "original_form": "우리 초반에 막 일주일에 한번씩 가니까",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100006684.1.8",
          "form": "조리하시는분이 눈인사하고 그랬자나ㅋ",
          "original_form": "조리하시는분이 눈인사하고 그랬자나ㅋ",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100006684.1.9",
          "form": "얼마나 인상깊었으면 그랬겠냐ㅋㅋ",
          "original_form": "얼마나 인상깊었으면 그랬겠냐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100006684.1.10",
          "form": "인상 깊지... 고기를 막 오십접시 육십접시 먹는데 ㅋㅋ뇌리에 박히지ㅋㅋ",
          "original_form": "인상 깊지... 고기를 막 오십접시 육십접시 먹는데 ㅋㅋ뇌리에 박히지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100006684.1.11",
          "form": "내가 좀 잘 먹기는 했어ㅋㅋㅋ",
          "original_form": "내가 좀 잘 먹기는 했어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100006684.1.12",
          "form": "ㅋㅋㅋㅋ나는 알차게 먹었지ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ나는 알차게 먹었지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100006684.1.13",
          "form": "새우랑 관자 음층 먹고",
          "original_form": "새우랑 관자 음층 먹고",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100006684.1.14",
          "form": "칼국수먹고 마지막 죽까지 ㅋㅋ",
          "original_form": "칼국수먹고 마지막 죽까지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100006684.1.15",
          "form": "그러고보면 너도 잘 먹긴했어 ㅋㅋ",
          "original_form": "그러고보면 너도 잘 먹긴했어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100006684.1.16",
          "form": "웃긴게 둘 다 한시간내로 먹는다는거ㅋㅋㅋ",
          "original_form": "웃긴게 둘 다 한시간내로 먹는다는거ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100006684.1.17",
          "form": "우리는 어딜가던 많이 먹는데 막상 시간보면 한시간? 이래서ㅋㅋㅋ진정한 돼지인걸 느껴ㅋㅋ",
          "original_form": "우리는 어딜가던 많이 먹는데 막상 시간보면 한시간? 이래서ㅋㅋㅋ진정한 돼지인걸 느껴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100006684.1.18",
          "form": "진정한돼지ㅋㅋㅋ그 끝은 쁘띠첼 3접시라고ㅋㅋ",
          "original_form": "진정한돼지ㅋㅋㅋ그 끝은 쁘띠첼 3접시라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100006684.1.19",
          "form": "ㅋㅋㅋ쁘띠첼 맛있지 마지막에 니가 맨날 시키자나 난 배부른데 꾸역꾸역 먹은거야",
          "original_form": "ㅋㅋㅋ쁘띠첼 맛있지 마지막에 니가 맨날 시키자나 난 배부른데 꾸역꾸역 먹은거야",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100006684.1.20",
          "form": "거짓말하고있어ㅋㅋㅋㅋ 내가 시켜주길 기다렸으면서ㅋㅋ",
          "original_form": "거짓말하고있어ㅋㅋㅋㅋ 내가 시켜주길 기다렸으면서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100006684.1.21",
          "form": "ㅋㅋㅋ 부정은 안할게ㅋ",
          "original_form": "ㅋㅋㅋ 부정은 안할게ㅋ",
          "speaker_id": "2",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100006684.1.22",
          "form": "우리 엄청 잘먹고 다녔구나ㅋㅋ",
          "original_form": "우리 엄청 잘먹고 다녔구나ㅋㅋ",
