      "utterance": [
        {
          "id": "MDRW2100022367.1.1",
          "form": "여친과 만났던 소개팅처음하고 비교해보면",
          "original_form": "여친과 만났던 소개팅처음하고 비교해보면",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022367.1.2",
          "form": "저는 사진 서로 주고받고",
          "original_form": "저는 사진 서로 주고받고",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022367.1.3",
          "form": "의사 타진해서 나간건데",
          "original_form": "의사 타진해서 나간건데",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022367.1.4",
          "form": "아 그렇져 ㅋㅋㅋ",
          "original_form": "아 그렇져 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022367.1.5",
          "form": "주선자가 사진없다고",
          "original_form": "주선자가 사진없다고",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022367.1.6",
          "form": "여행갈때 비행기에서 자다 깬 사진 줘가지고",
          "original_form": "여행갈때 비행기에서 자다 깬 사진 줘가지고",
          "speaker_id": "2",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022367.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022367.1.8",
          "form": "솔직히 저 안하려고 했거든여",
          "original_form": "솔직히 저 안하려고 했거든여",
          "speaker_id": "2",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022367.1.9",
          "form": "아니 그래도 소개팅이 성사된거면",
          "original_form": "아니 그래도 소개팅이 성사된거면",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022367.1.10",
          "form": "ㅋㅌㅋㅋㅋㅋㅋ인물이 출중하신",
          "original_form": "ㅋㅌㅋㅋㅋㅋㅋ인물이 출중하신",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022367.1.11",
          "form": "아니에요",
          "original_form": "아니에요",
          "speaker_id": "2",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022367.1.12",
          "form": "저 딱 공대생처럼 생겼어요",
          "original_form": "저 딱 공대생처럼 생겼어요",
          "speaker_id": "2",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022367.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022367.1.14",
          "form": "진짜 이게 마지막이다",
          "original_form": "진짜 이게 마지막이다",
          "speaker_id": "2",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022367.1.15",
          "form": "소개팅에 너무 현타와서",
          "original_form": "소개팅에 너무 현타와서",
          "speaker_id": "2",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022367.1.16",
          "form": "넼ㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022367.1.17",
          "form": "그러고 나갔는데",
          "original_form": "그러고 나갔는데",
          "speaker_id": "2",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022367.1.18",
          "form": "여친이 여신처럼 빼입고 와서",
          "original_form": "여친이 여신처럼 빼입고 와서",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022367.1.19",
          "form": "우선 거기서 호감",
          "original_form": "우선 거기서 호감",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022367.1.20",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022367.1.21",
          "form": "대화가 잘통했고",
          "original_form": "대화가 잘통했고",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022367.1.22",
          "form": "리액션이 좋았어요 ㅋㅋㅋ",
          "original_form": "리액션이 좋았어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022367.1.23",
          "form": "밥먹고 차마시고 헤어지면서",
          "original_form": "밥먹고 차마시고 헤어지면서",
          "speaker_id": "2",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022367.1.24",
          "form": "우리 담주 주말에도 만날래요?",
          "original_form": "우리 담주 주말에도 만날래요?",
          "speaker_id": "2",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022367.1.25",
          "form": "했죠",
          "original_form": "했죠",
          "speaker_id": "2",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022367.1.26",
          "form": "오와아아",
          "original_form": "오와아아",
          "speaker_id": "1",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022367.1.27",
          "form": "좋다고 하길래",
          "original_form": "좋다고 하길래",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022367.1.28",
          "form": "헤어지고",
          "original_form": "헤어지고",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022367.1.29",
          "form": "계속 카톡하다가",
          "original_form": "계속 카톡하다가",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022367.1.30",
          "form": "제가 전화를 했어요",
          "original_form": "제가 전화를 했어요",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022367.1.31",
          "form": "크 심쿵햇겟다",
          "original_form": "크 심쿵햇겟다",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022367.1.32",
          "form": "그때부터 전화 엄청했거든요",
          "original_form": "그때부터 전화 엄청했거든요",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022367.1.33",
          "form": "한시간이상씩",
          "original_form": "한시간이상씩",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022367.1.34",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022367.1.35",
          "form": "대화를 해도 해도 할얘기가 계속 있는거에요",
          "original_form": "대화를 해도 해도 할얘기가 계속 있는거에요",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022367.1.36",
          "form": "ㅋㅋㅋㅋㅋ진짜 딱 좋네요",
          "original_form": "ㅋㅋㅋㅋㅋ진짜 딱 좋네요",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022367.1.37",
          "form": "그렇게 전화 자주해서 3번만나고 나서",
          "original_form": "그렇게 전화 자주해서 3번만나고 나서",
          "speaker_id": "2",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022367.1.38",
          "form": "왜 사귀자 안하냐 했던거 같아요",
          "original_form": "왜 사귀자 안하냐 했던거 같아요",
          "speaker_id": "2",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022367.1.39",
          "form": "ㅋㅋㅋㅋㅋ그러네요 전화계속 한 사이면 그런 얘기 나올만 했네여 진짜",
          "original_form": "ㅋㅋㅋㅋㅋ그러네요 전화계속 한 사이면 그런 얘기 나올만 했네여 진짜",
          "speaker_id": "1",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022367.1.40",
          "form": "호감 표시 다하고 사귀자 안하니까",
          "original_form": "호감 표시 다하고 사귀자 안하니까",
          "speaker_id": "2",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022367.1.41",
          "form": "자기 전화 오래하는거 싫어하는데",
          "original_form": "자기 전화 오래하는거 싫어하는데",
          "speaker_id": "2",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022367.1.42",
          "form": "저랑하면 길게해도 너무 좋다고 하길래",
          "original_form": "저랑하면 길게해도 너무 좋다고 하길래",
          "speaker_id": "2",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022367.1.43",
          "form": "저도 호감을 느꼈는데",
          "original_form": "저도 호감을 느꼈는데",
          "speaker_id": "2",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022367.1.44",
          "form": "꺄아아",
          "original_form": "꺄아아",
          "speaker_id": "1",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022367.1.45",
          "form": "한주뒤에 하려다가 그만...ㅋㅋㅋ",
          "original_form": "한주뒤에 하려다가 그만...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022367.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022367.1.47",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022367.1.48",
          "form": "name1님",
          "original_form": "&name1&님",
          "speaker_id": "2",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022367.1.49",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022367.1.50",
          "form": "존댓말써요 반말써요?",
          "original_form": "존댓말써요 반말써요?",
          "speaker_id": "2",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022367.1.51",
          "form": "반말써요",
          "original_form": "반말써요",
          "speaker_id": "1",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022367.1.52",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022367.1.53",
          "form": "세번째 만날때 말편하게 할래요? 하시길래",
          "original_form": "세번째 만날때 말편하게 할래요? 하시길래",
          "speaker_id": "1",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022367.1.54",
          "form": "그럼 호칭은요?",
          "original_form": "그럼 호칭은요?",
          "speaker_id": "2",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022367.1.55",
          "form": "아 그럼 오빠라고 해도될까요? 사실 ~씨라고 하기 너무 어색했다고",
          "original_form": "아 그럼 오빠라고 해도될까요? 사실 ~씨라고 하기 너무 어색했다고",
          "speaker_id": "1",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022367.1.56",
          "form": "어?",
          "original_form": "어?",
          "speaker_id": "2",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022367.1.57",
          "form": "ㅋㅋㅋㅋ그래서 그냥 오빠해요",
          "original_form": "ㅋㅋㅋㅋ그래서 그냥 오빠해요",
          "speaker_id": "1",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022367.1.58",
          "form": "그거 여친도 그랬는데",
          "original_form": "그거 여친도 그랬는데",
          "speaker_id": "2",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022367.1.59",
          "form": "아 그래요? ㅋㅋㅋㅋㅋ",
          "original_form": "아 그래요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022367.1.60",
          "form": "OO씨 너무 오글거린다고",
          "original_form": "OO씨 너무 오글거린다고",
          "speaker_id": "2",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022367.1.61",
          "form": "네ㅋㅋㅋㅋㅋ오글거리는데 먼저 오빠라고 불러도 되요? 하기엔 좀 앙큼해보여서",
          "original_form": "네ㅋㅋㅋㅋㅋ오글거리는데 먼저 오빠라고 불러도 되요? 하기엔 좀 앙큼해보여서",
          "speaker_id": "1",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022367.1.62",
          "form": "ㅋㅋㅋㅋㅋㅋ호칭정리를 못하고 있었어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ호칭정리를 못하고 있었어요",
          "speaker_id": "1",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022367.1.63",
          "form": "그럼 지금  오빠? 라고 부르는거에요?",
          "original_form": "그럼 지금  오빠? 라고 부르는거에요?",
