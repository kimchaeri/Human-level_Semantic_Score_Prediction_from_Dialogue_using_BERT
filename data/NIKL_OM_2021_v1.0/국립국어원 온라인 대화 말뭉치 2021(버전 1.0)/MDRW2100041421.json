      },
      "utterance": [
        {
          "id": "MDRW2100041421.1.1",
          "form": "집에 고추장이랑 떡 있어?",
          "original_form": "집에 고추장이랑 떡 있어?",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100041421.1.2",
          "form": "아니 재료 1도 없어...ㅎ",
          "original_form": "아니 재료 1도 없어...ㅎ",
          "speaker_id": "1",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100041421.1.3",
          "form": "시장 가서 사먹을까",
          "original_form": "시장 가서 사먹을까",
          "speaker_id": "1",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100041421.1.4",
          "form": "매떡에서 배달시켜 먹어",
          "original_form": "매떡에서 배달시켜 먹어",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100041421.1.5",
          "form": "지금 매떡 배달 시간 끝났어ㅠㅠ",
          "original_form": "지금 매떡 배달 시간 끝났어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100041421.1.6",
          "form": "아님 내일 편의점에서 컵떡볶이 사서 먹어야겠다",
          "original_form": "아님 내일 편의점에서 컵떡볶이 사서 먹어야겠다",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100041421.1.7",
          "form": "식혜떡볶이 알아? 그거 해봐",
          "original_form": "식혜떡볶이 알아? 그거 해봐",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100041421.1.8",
          "form": "식혜떡볶이가 뭐야",
          "original_form": "식혜떡볶이가 뭐야",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100041421.1.9",
          "form": "또 이상한 레시피 어디서 알아왔구만",
          "original_form": "또 이상한 레시피 어디서 알아왔구만",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100041421.1.10",
          "form": "아니야 유명한 거야",
          "original_form": "아니야 유명한 거야",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100041421.1.11",
          "form": "식혜국물에다 고추장이랑 떡만 넣으면 떡볶이 맛이 난대",
          "original_form": "식혜국물에다 고추장이랑 떡만 넣으면 떡볶이 맛이 난대",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100041421.1.12",
          "form": "ㅋㅋㅋㅋㅋ웃기다 그게 진짜 되려나",
          "original_form": "ㅋㅋㅋㅋㅋ웃기다 그게 진짜 되려나",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100041421.1.13",
          "form": "마늘 같은 재료가 필요 없다는 거지??",
          "original_form": "마늘 같은 재료가 필요 없다는 거지??",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100041421.1.14",
          "form": "응 그렇대",
          "original_form": "응 그렇대",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100041421.1.15",
          "form": "맛있을 거 같더라",
          "original_form": "맛있을 거 같더라",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100041421.1.16",
          "form": "아직 라이스페이퍼 떡볶이도 못 해봤는데",
          "original_form": "아직 라이스페이퍼 떡볶이도 못 해봤는데",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041421.1.17",
          "form": "유행이 너무 빠르군..ㅎ",
          "original_form": "유행이 너무 빠르군..ㅎ",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041421.1.18",
          "form": "그것도 맛 궁금해",
          "original_form": "그것도 맛 궁금해",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041421.1.19",
          "form": "쫀득할 것 같기는 한데",
          "original_form": "쫀득할 것 같기는 한데",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041421.1.20",
          "form": "그치?ㅋㅋㅋㅋ",
          "original_form": "그치?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041421.1.21",
          "form": "근데 난 지금 보통 떡볶이를 먹고 싶어",
          "original_form": "근데 난 지금 보통 떡볶이를 먹고 싶어",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041421.1.22",
          "form": "떡볶이에 참치김밥 아니면 주먹밥",
          "original_form": "떡볶이에 참치김밥 아니면 주먹밥",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041421.1.23",
          "form": "계란찜까지 먹으면 딱인데",
          "original_form": "계란찜까지 먹으면 딱인데",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041421.1.24",
          "form": "역시 우린 입맛이 비슷해...",
          "original_form": "역시 우린 입맛이 비슷해...",
          "speaker_id": "1",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100041421.1.25",
          "form": "떡볶이 진짜 너무 땡긴다",
          "original_form": "떡볶이 진짜 너무 땡긴다",
          "speaker_id": "1",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100041421.1.26",
          "form": "궁중떡볶이 같은 것도 먹고 싶음..ㅎ",
          "original_form": "궁중떡볶이 같은 것도 먹고 싶음..ㅎ",
          "speaker_id": "1",
          "time": "20210905 23:28"
        }
      ]
    }
  ]
