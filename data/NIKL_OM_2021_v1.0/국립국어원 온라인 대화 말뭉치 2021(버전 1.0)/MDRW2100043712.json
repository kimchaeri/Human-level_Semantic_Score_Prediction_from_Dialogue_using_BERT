      "utterance": [
        {
          "id": "MDRW2100043712.1.1",
          "form": "이제 반팔이 추워요",
          "original_form": "이제 반팔이 추워요",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043712.1.2",
          "form": "여긴 비많이오네요",
          "original_form": "여긴 비많이오네요",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043712.1.3",
          "form": "밤에는 문닫고 자야하고",
          "original_form": "밤에는 문닫고 자야하고",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043712.1.4",
          "form": "글게요 쌀쌀해요",
          "original_form": "글게요 쌀쌀해요",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043712.1.5",
          "form": "여긴 조금씩 오긴해요",
          "original_form": "여긴 조금씩 오긴해요",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043712.1.6",
          "form": "집은 또 더워서 전 선풍기켜요ㅠ",
          "original_form": "집은 또 더워서 전 선풍기켜요ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043712.1.7",
          "form": "옷정리도 해야하는데",
          "original_form": "옷정리도 해야하는데",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043712.1.8",
          "form": "밖은 추운데ㅎ",
          "original_form": "밖은 추운데ㅎ",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043712.1.9",
          "form": "이러다 리빙박스에서 꺼내입는거 아닌지 ㅎ",
          "original_form": "이러다 리빙박스에서 꺼내입는거 아닌지 ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043712.1.10",
          "form": "긴팔도 입을만한게 없어서",
          "original_form": "긴팔도 입을만한게 없어서",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043712.1.11",
          "form": "저도 아까 선풍기 켰어요",
          "original_form": "저도 아까 선풍기 켰어요",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043712.1.12",
          "form": "비가 계속올건가봐요",
          "original_form": "비가 계속올건가봐요",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043712.1.13",
          "form": "애들이 하도 뛰어댕겨서 땀이 뽀질뽀질 ㅎ",
          "original_form": "애들이 하도 뛰어댕겨서 땀이 뽀질뽀질 ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043712.1.14",
          "form": "태풍올라온대요",
          "original_form": "태풍올라온대요",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043712.1.15",
          "form": "글쵸? 작년에는 머입었나 싶어요",
          "original_form": "글쵸? 작년에는 머입었나 싶어요",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043712.1.16",
          "form": "앗 그래요???",
          "original_form": "앗 그래요???",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043712.1.17",
          "form": "저도요ㅎ 매년 같은생각 ㅋ",
          "original_form": "저도요ㅎ 매년 같은생각 ㅋ",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043712.1.18",
          "form": "가을 태풍 인가요?",
          "original_form": "가을 태풍 인가요?",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043712.1.19",
          "form": "작년엔 뭐입었나",
          "original_form": "작년엔 뭐입었나",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043712.1.20",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210907 20:06"
        }
      ]
    }
