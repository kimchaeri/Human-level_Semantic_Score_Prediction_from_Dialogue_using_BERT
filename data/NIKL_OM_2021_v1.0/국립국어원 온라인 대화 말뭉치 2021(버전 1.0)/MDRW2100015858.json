      "utterance": [
        {
          "id": "MDRW2100015858.1.1",
          "form": "가족 구성이 어떻게 되시나요?",
          "original_form": "가족 구성이 어떻게 되시나요?",
          "speaker_id": "2",
          "time": "20210826 01:55"
        },
        {
          "id": "MDRW2100015858.1.2",
          "form": "저 4명이요! 동생 한명이구요",
          "original_form": "저 4명이요! 동생 한명이구요",
          "speaker_id": "1",
          "time": "20210826 01:55"
        },
        {
          "id": "MDRW2100015858.1.3",
          "form": "아 그러시군요!\r\n저는 5명이에요 동생이 2명이라서요ㅎㅎ",
          "original_form": "아 그러시군요!\r\n저는 5명이에요 동생이 2명이라서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:57"
        },
        {
          "id": "MDRW2100015858.1.4",
          "form": "오! 동생이랑 나이차가 있으신가요?",
          "original_form": "오! 동생이랑 나이차가 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 01:57"
        },
        {
          "id": "MDRW2100015858.1.5",
          "form": "4살 8살 요렇게 차이가 나요\r\n셋이 나란히 4살 터울이에요ㅎㅎ",
          "original_form": "4살 8살 요렇게 차이가 나요\r\n셋이 나란히 4살 터울이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:58"
        },
        {
          "id": "MDRW2100015858.1.6",
          "form": "와! 8살이면 진짜 아기같겠어요ㅋㅋ",
          "original_form": "와! 8살이면 진짜 아기같겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:59"
        },
        {
          "id": "MDRW2100015858.1.7",
          "form": "8살 차이이면",
          "original_form": "8살 차이이면",
          "speaker_id": "1",
          "time": "20210826 01:59"
        },
        {
          "id": "MDRW2100015858.1.8",
          "form": "어릴 때는 진짜 아기같고 귀여웠는데 지금은...",
          "original_form": "어릴 때는 진짜 아기같고 귀여웠는데 지금은...",
          "speaker_id": "2",
          "time": "20210826 01:59"
        },
        {
          "id": "MDRW2100015858.1.9",
          "form": "음..지금은 전혀 그렇지 않아요ㅋㅋㅋ",
          "original_form": "음..지금은 전혀 그렇지 않아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:59"
        },
        {
          "id": "MDRW2100015858.1.10",
          "form": "웬수같나요?ㅋㅋ 저두 동생보면 그런데",
          "original_form": "웬수같나요?ㅋㅋ 저두 동생보면 그런데",
          "speaker_id": "1",
          "time": "20210826 01:59"
        },
        {
          "id": "MDRW2100015858.1.11",
          "form": "저보다  신체적으로도 크고, 말도 참 안 듣거든요ㅎㅎ",
          "original_form": "저보다  신체적으로도 크고, 말도 참 안 듣거든요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 02:00"
        },
        {
          "id": "MDRW2100015858.1.12",
          "form": "그쵸 가끔 그렇게 느껴지기도 해요!",
          "original_form": "그쵸 가끔 그렇게 느껴지기도 해요!",
          "speaker_id": "2",
          "time": "20210826 02:00"
        },
        {
          "id": "MDRW2100015858.1.13",
          "form": "앗 그건 제동생이랑 같은ㅋㅋㅋㅋ",
          "original_form": "앗 그건 제동생이랑 같은ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:00"
        },
        {
          "id": "MDRW2100015858.1.14",
          "form": "ㅋㅋㅋ  남동생 있으세요? 아니면 여동생..?",
          "original_form": "ㅋㅋㅋ  남동생 있으세요? 아니면 여동생..?",
          "speaker_id": "2",
          "time": "20210826 02:02"
        },
        {
          "id": "MDRW2100015858.1.15",
          "form": "여동생이에요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "여동생이에요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:02"
        },
        {
          "id": "MDRW2100015858.1.16",
          "form": "전 남동생/여동생 한 명씩 있는데, 둘 다 저보다 한참 커요ㅋㅋㅋㅋ",
          "original_form": "전 남동생/여동생 한 명씩 있는데, 둘 다 저보다 한참 커요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:03"
        },
        {
          "id": "MDRW2100015858.1.17",
          "form": "키가요? 와..",
          "original_form": "키가요? 와..",
          "speaker_id": "1",
          "time": "20210826 02:03"
        },
        {
          "id": "MDRW2100015858.1.18",
          "form": "키도 그렇고 몸무게도 그렇고 둘 다 체격이 좋은 편이거든요\r\n저는 작지만요..",
          "original_form": "키도 그렇고 몸무게도 그렇고 둘 다 체격이 좋은 편이거든요\r\n저는 작지만요..",
          "speaker_id": "2",
          "time": "20210826 02:05"
        },
        {
          "id": "MDRW2100015858.1.19",
          "form": "으아 몸싸움은 못하겠네요ㅎㅎ",
          "original_form": "으아 몸싸움은 못하겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:06"
        }
      ]
