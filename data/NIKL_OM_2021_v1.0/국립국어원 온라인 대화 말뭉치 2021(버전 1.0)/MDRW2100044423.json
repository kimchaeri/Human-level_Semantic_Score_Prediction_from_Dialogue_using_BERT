        },
        {
          "id": "MDRW2100044423.1.3",
          "form": "오오 블랙페퍼로 뭄?",
          "original_form": "오오 블랙페퍼로 뭄?",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044423.1.4",
          "form": "예쓰",
          "original_form": "예쓰",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044423.1.5",
          "form": "ㅋㅋㅋname3가 한개 주고가서",
          "original_form": "ㅋㅋㅋ&name3&가 한개 주고가서",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044423.1.6",
          "form": "오 그거만 사봣나",
          "original_form": "오 그거만 사봣나",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044423.1.7",
          "form": "아아 하나줘서 ㅋㅋㅋㅋ",
          "original_form": "아아 하나줘서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044423.1.8",
          "form": "냉동실 대여료로 받음ㅋㅋ",
          "original_form": "냉동실 대여료로 받음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044423.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ귀엽네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ귀엽네",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044423.1.10",
          "form": "간이 딱 잘됫다아이가",
          "original_form": "간이 딱 잘됫다아이가",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044423.1.11",
          "form": "어엉ㅋㅋㅋ근데 한...3분의 2만 됐으면...",
          "original_form": "어엉ㅋㅋㅋ근데 한...3분의 2만 됐으면...",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044423.1.12",
          "form": "왜 ㅋㅋㅋㅋㅋ양이 많음?",
          "original_form": "왜 ㅋㅋㅋㅋㅋ양이 많음?",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044423.1.13",
          "form": "아니 그럼 덜 질릴거 같아서 ㅋㅋㅋ",
          "original_form": "아니 그럼 덜 질릴거 같아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044423.1.14",
          "form": "ㅋㅋㅋㅋㅋ아아 하긴",
          "original_form": "ㅋㅋㅋㅋㅋ아아 하긴",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044423.1.15",
          "form": "은근 후추향이 세서?",
          "original_form": "은근 후추향이 세서?",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044423.1.16",
          "form": "흠...ㅇㅇㅇ그런 거 같음 ㅋㅋㅋ",
          "original_form": "흠...ㅇㅇㅇ그런 거 같음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044423.1.17",
          "form": "넌 저녁 뭐먹엇어",
          "original_form": "넌 저녁 뭐먹엇어",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044423.1.18",
          "form": "나는 호랑이치킨ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나는 호랑이치킨ㅋㅋㅋㅋㅋㅋㅋ",
