        },
        {
          "id": "MDRW2100029505.1.2",
          "form": "커피드세요",
          "original_form": "커피드세요",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029505.1.3",
          "form": "고디바커피.다 마셔가는데",
          "original_form": "고디바커피.다 마셔가는데",
          "speaker_id": "2",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029505.1.4",
          "form": "아이스 아메리카노인가요",
          "original_form": "아이스 아메리카노인가요",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029505.1.5",
          "form": "또시켜야할까요",
          "original_form": "또시켜야할까요",
          "speaker_id": "2",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029505.1.6",
          "form": "드립커피요",
          "original_form": "드립커피요",
          "speaker_id": "2",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029505.1.7",
          "form": "추석오기전에 사야죠",
          "original_form": "추석오기전에 사야죠",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029505.1.8",
          "form": "추석 연휴오기전에",
          "original_form": "추석 연휴오기전에",
          "speaker_id": "2",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029505.1.9",
          "form": "지마켓 20%할인합니다",
          "original_form": "지마켓 20%할인합니다",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029505.1.10",
          "form": "사야겠죠",
          "original_form": "사야겠죠",
          "speaker_id": "2",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029505.1.11",
          "form": "쟁여야합니다",
          "original_form": "쟁여야합니다",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029505.1.12",
          "form": "미국서 사오는건데 할인되나요",
          "original_form": "미국서 사오는건데 할인되나요",
