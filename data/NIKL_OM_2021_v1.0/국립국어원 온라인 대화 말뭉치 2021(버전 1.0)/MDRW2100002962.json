      "utterance": [
        {
          "id": "MDRW2100002962.1.1",
          "form": "넌 요즘 보는 유튜브 채널 있어?",
          "original_form": "넌 요즘 보는 유튜브 채널 있어?",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002962.1.2",
          "form": "유튜브로 예능을 좀 많이 봐",
          "original_form": "유튜브로 예능을 좀 많이 봐",
          "speaker_id": "2",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002962.1.3",
          "form": "어떤 예능 봐?",
          "original_form": "어떤 예능 봐?",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002962.1.4",
          "form": "난 요즘 피식대학이라는 채널 조금씩 봐",
          "original_form": "난 요즘 피식대학이라는 채널 조금씩 봐",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002962.1.5",
          "form": "무한도전 봐",
          "original_form": "무한도전 봐",
          "speaker_id": "2",
          "time": "20210820 17:06"
        },
        {
          "id": "MDRW2100002962.1.6",
          "form": "이름은 들어본것 같아",
          "original_form": "이름은 들어본것 같아",
          "speaker_id": "2",
          "time": "20210820 17:06"
        },
        {
          "id": "MDRW2100002962.1.7",
          "form": "무한도전 한번도 안봤어",
          "original_form": "무한도전 한번도 안봤어",
          "speaker_id": "1",
          "time": "20210820 17:07"
        },
        {
          "id": "MDRW2100002962.1.8",
          "form": "유튜브에서 무료로 볼 수 있어?",
          "original_form": "유튜브에서 무료로 볼 수 있어?",
          "speaker_id": "1",
          "time": "20210820 17:07"
        },
        {
          "id": "MDRW2100002962.1.9",
          "form": "mbc에서 무한도전 조금씩 올려주더라고",
          "original_form": "mbc에서 무한도전 조금씩 올려주더라고",
          "speaker_id": "2",
          "time": "20210820 17:07"
        },
        {
          "id": "MDRW2100002962.1.10",
          "form": "그거 좋네",
          "original_form": "그거 좋네",
          "speaker_id": "1",
          "time": "20210820 17:07"
        },
        {
          "id": "MDRW2100002962.1.11",
          "form": "풀버전은 아닌데 재밌아",
          "original_form": "풀버전은 아닌데 재밌아",
          "speaker_id": "2",
          "time": "20210820 17:07"
        },
        {
          "id": "MDRW2100002962.1.12",
          "form": "돈 내고 보긴 좀 그래서",
          "original_form": "돈 내고 보긴 좀 그래서",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002962.1.13",
          "form": "재미있는 부분만 보낸거겠지",
          "original_form": "재미있는 부분만 보낸거겠지",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002962.1.14",
          "form": "피식대학은 어떤 채널이야?",
          "original_form": "피식대학은 어떤 채널이야?",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002962.1.15",
          "form": "전직 개그맨들이 만든 채널이야",
          "original_form": "전직 개그맨들이 만든 채널이야",
          "speaker_id": "1",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002962.1.16",
          "form": "와 재밌겠네",
          "original_form": "와 재밌겠네",
          "speaker_id": "2",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002962.1.17",
          "form": "개그프로보다 더 재미있어",
          "original_form": "개그프로보다 더 재미있어",
          "speaker_id": "1",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002962.1.18",
          "form": "90년대생이 60년대생처럼 입고 말하는 컨셉 있거든?",
          "original_form": "90년대생이 60년대생처럼 입고 말하는 컨셉 있거든?",
          "speaker_id": "1",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002962.1.19",
          "form": "한사랑산악회라고",
          "original_form": "한사랑산악회라고",
          "speaker_id": "1",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002962.1.20",
          "form": "진짜... 아빠같아ㅋㅋ",
          "original_form": "진짜... 아빠같아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002962.1.21",
          "form": "와ㅋㅋㅋㅋ 신기하다",
          "original_form": "와ㅋㅋㅋㅋ 신기하다",
          "speaker_id": "2",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002962.1.22",
          "form": "얘기만 들어도 재밌을 것 같네",
          "original_form": "얘기만 들어도 재밌을 것 같네",
          "speaker_id": "2",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002962.1.23",
          "form": "진짜 이 사람들이 우리랑 나이가 비슷하다고? 싶을정도야",
          "original_form": "진짜 이 사람들이 우리랑 나이가 비슷하다고? 싶을정도야",
