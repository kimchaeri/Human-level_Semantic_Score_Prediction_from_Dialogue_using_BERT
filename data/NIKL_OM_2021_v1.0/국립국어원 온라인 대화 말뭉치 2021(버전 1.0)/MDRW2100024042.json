      "utterance": [
        {
          "id": "MDRW2100024042.1.1",
          "form": "님도 만들어보세요",
          "original_form": "님도 만들어보세요",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024042.1.2",
          "form": "제료는\r\n크림치즈\r\n생크림 (요거트로 대체가능)\r\n계란\r\n전분(생략가능 밀가루로 대체가능)",
          "original_form": "제료는\r\n크림치즈\r\n생크림 (요거트로 대체가능)\r\n계란\r\n전분(생략가능 밀가루로 대체가능)",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024042.1.3",
          "form": "설탕",
          "original_form": "설탕",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024042.1.4",
          "form": "크림치즈 생크림 전분 설탕이 없네요",
          "original_form": "크림치즈 생크림 전분 설탕이 없네요",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024042.1.5",
          "form": "설탕...은 슈가스틱 있긴 해요 커피에 넣는 거",
          "original_form": "설탕...은 슈가스틱 있긴 해요 커피에 넣는 거",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024042.1.6",
          "form": "다 사면 되죠",
          "original_form": "다 사면 되죠",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024042.1.7",
          "form": "홈플에서 한번에 ㅋㅋㅋ",
          "original_form": "홈플에서 한번에 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024042.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ 걍 치즈케이크를 사먹는 게 낫지 않을까요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 걍 치즈케이크를 사먹는 게 낫지 않을까요...",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024042.1.9",
          "form": "유ㅗㄴ거걔선울 해보자구요",
          "original_form": "유ㅗㄴ거걔선울 해보자구요",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100024042.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 저의 시급도 포함해주세요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저의 시급도 포함해주세요",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024042.1.11",
          "form": "이마트몰 기준으로",
          "original_form": "이마트몰 기준으로",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024042.1.12",
          "form": "크림치즈 226그램 오천원",
          "original_form": "크림치즈 226그램 오천원",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024042.1.13",
          "form": "계란 두알 천원",
          "original_form": "계란 두알 천원",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024042.1.14",
          "form": "설탕 집에있다치구",
          "original_form": "설탕 집에있다치구",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024042.1.15",
          "form": "플레인 요거트 작은거 하나 700원",
          "original_form": "플레인 요거트 작은거 하나 700원",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024042.1.16",
          "form": "30분걸리면 시급 오천워너쳐서",
          "original_form": "30분걸리면 시급 오천워너쳐서",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024042.1.17",
          "form": "한판에",
          "original_form": "한판에",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024042.1.18",
          "form": "일단 저는 30분 안에 못 만들거고여",
          "original_form": "일단 저는 30분 안에 못 만들거고여",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024042.1.19",
          "form": "만든 뒤 설거지랑 뒷정리하는 시간까지 포함하면",
          "original_form": "만든 뒤 설거지랑 뒷정리하는 시간까지 포함하면",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024042.1.20",
          "form": "2시간 정도 들 걸여...",
          "original_form": "2시간 정도 들 걸여...",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024042.1.21",
          "form": "흠흠,, 밖에 안나가고 치즈케이크를 먹는다는 장점",
          "original_form": "흠흠,, 밖에 안나가고 치즈케이크를 먹는다는 장점",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024042.1.22",
          "form": "치케는 냉동해도 좋다는 장점",
          "original_form": "치케는 냉동해도 좋다는 장점",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024042.1.23",
          "form": "어때요 뽐뿌가 오시는지?",
          "original_form": "어때요 뽐뿌가 오시는지?",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024042.1.24",
          "form": "아뇨...",
          "original_form": "아뇨...",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024042.1.25",
          "form": "걍 카페에서 먹는 게 편한 거 같아여 하하하핳",
          "original_form": "걍 카페에서 먹는 게 편한 거 같아여 하하하핳",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024042.1.26",
          "form": "췟,,,,창작욕이 생기시지 않냐구요",
          "original_form": "췟,,,,창작욕이 생기시지 않냐구요",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024042.1.27",
          "form": "요리엔 별로 의욕을 못 느끼네옄ㅋㅋㅋㅋ",
          "original_form": "요리엔 별로 의욕을 못 느끼네옄ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024042.1.28",
          "form": "그럼 다른 종류의 창작욕은 있으신건가요",
          "original_form": "그럼 다른 종류의 창작욕은 있으신건가요",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024042.1.29",
          "form": "지금은 밥얘길 해야하니 다른때 그걸 또 말하죠 ㅋㅋ",
          "original_form": "지금은 밥얘길 해야하니 다른때 그걸 또 말하죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024042.1.30",
          "form": "ㅋㅋㅋㅋㅋ 음식에 대한 창작욕은",
          "original_form": "ㅋㅋㅋㅋㅋ 음식에 대한 창작욕은",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024042.1.31",
          "form": "냉장고가 꽉 차서 더 미루다간 썩겠다 싶을 때",
          "original_form": "냉장고가 꽉 차서 더 미루다간 썩겠다 싶을 때",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024042.1.32",
          "form": "최고로 발휘되는 것 같아요",
          "original_form": "최고로 발휘되는 것 같아요",
          "speaker_id": "2",
          "time": "20210830 21:40"
        }
      ]
