      "utterance": [
        {
          "id": "MDRW2100029224.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029224.1.2",
          "form": "저 대체로 잘 먹는 편인데 고르자면 떡볶이인 것 같아요 ㅎㅎ",
          "original_form": "저 대체로 잘 먹는 편인데 고르자면 떡볶이인 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029224.1.3",
          "form": "떡볶이 맛있죠ㅜㅜㅜ",
          "original_form": "떡볶이 맛있죠ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029224.1.4",
          "form": "요즘 로제 너무 맛있어요",
          "original_form": "요즘 로제 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029224.1.5",
          "form": "로제 유행인지 연달아 나오더라고요~",
          "original_form": "로제 유행인지 연달아 나오더라고요~",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029224.1.6",
          "form": "전 신전이랑 엽떡 로제 먹어봤어요",
          "original_form": "전 신전이랑 엽떡 로제 먹어봤어요",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029224.1.7",
          "form": "맞아요 한번 먹어보고 반했어요",
          "original_form": "맞아요 한번 먹어보고 반했어요",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029224.1.8",
          "form": "저는 감탄이랑 엽떡요",
          "original_form": "저는 감탄이랑 엽떡요",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029224.1.9",
          "form": "신전은 어떠셨어요?",
          "original_form": "신전은 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029224.1.10",
          "form": "엽떡 기본맛이 되게 매운데",
          "original_form": "엽떡 기본맛이 되게 매운데",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029224.1.11",
          "form": "로제는 덜해서 좋아요 ㅎㅎ",
          "original_form": "로제는 덜해서 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029224.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029224.1.13",
          "form": "신전도 맛있었어요 그런데 전 엽떡이 더 좋은것 같네요 ㅎㅎ",
          "original_form": "신전도 맛있었어요 그런데 전 엽떡이 더 좋은것 같네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029224.1.14",
          "form": "감탄도 괜찮아요",
          "original_form": "감탄도 괜찮아요",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029224.1.15",
          "form": "집근처라 그냥 포장했는데",
          "original_form": "집근처라 그냥 포장했는데",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029224.1.16",
          "form": "후추맛이 살짝 나면서 맛있어요",
          "original_form": "후추맛이 살짝 나면서 맛있어요",
          "speaker_id": "1",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029224.1.17",
          "form": "집근처에 없어서 검색해봤는데 맛있겠어요",
          "original_form": "집근처에 없어서 검색해봤는데 맛있겠어요",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029224.1.18",
          "form": "그냥 떡볶이도 맛있어 보이네요",
          "original_form": "그냥 떡볶이도 맛있어 보이네요",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029224.1.19",
          "form": "그냥은 정말 보통이여요 ㅎ",
          "original_form": "그냥은 정말 보통이여요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029224.1.20",
          "form": "ㅋㅋㅋ솔직한 감상 감사합니다",
          "original_form": "ㅋㅋㅋ솔직한 감상 감사합니다",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029224.1.21",
          "form": "네 엽떡이 더 맛있어요 ㅋㅋ",
          "original_form": "네 엽떡이 더 맛있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029224.1.22",
          "form": "다른 음식은 뭐 좋아하세요?",
          "original_form": "다른 음식은 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 00:33"
        }
      ]
