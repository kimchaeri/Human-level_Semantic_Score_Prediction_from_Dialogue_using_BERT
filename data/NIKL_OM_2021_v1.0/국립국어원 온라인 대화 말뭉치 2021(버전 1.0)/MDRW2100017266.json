      "utterance": [
        {
          "id": "MDRW2100017266.1.1",
          "form": "봉사를 해보신 적이 있나요?",
          "original_form": "봉사를 해보신 적이 있나요?",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017266.1.2",
          "form": "봉사 즐기시나요?",
          "original_form": "봉사 즐기시나요?",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017266.1.3",
          "form": "네. 해본적있고 즐기진않네요 ㅠ",
          "original_form": "네. 해본적있고 즐기진않네요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017266.1.4",
          "form": "저는 봉사를 즐긴답니다",
          "original_form": "저는 봉사를 즐긴답니다",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017266.1.5",
          "form": "어떤타입의 봉사를 말씀하시는거세요?",
          "original_form": "어떤타입의 봉사를 말씀하시는거세요?",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017266.1.6",
          "form": "가족을 위해서 이런저런 봉사를 한답니다",
          "original_form": "가족을 위해서 이런저런 봉사를 한답니다",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017266.1.7",
          "form": "그렇군여. 최근에 가장 힘들었던 봉사가 뭐인가요",
          "original_form": "그렇군여. 최근에 가장 힘들었던 봉사가 뭐인가요",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017266.1.8",
          "form": "갠적으로 말동무하고 기분 맞춰주고 하는 봉사가 꽤 힘들더라고요",
          "original_form": "갠적으로 말동무하고 기분 맞춰주고 하는 봉사가 꽤 힘들더라고요",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017266.1.9",
          "form": "아니 그 대상이 누구신가요",
          "original_form": "아니 그 대상이 누구신가요",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017266.1.10",
          "form": "제가 언니라고 부르는 분이세요",
          "original_form": "제가 언니라고 부르는 분이세요",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017266.1.11",
          "form": "친언니 인가요?",
          "original_form": "친언니 인가요?",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017266.1.12",
          "form": "ㅇ0ㅇ",
          "original_form": "ㅇ0ㅇ",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017266.1.13",
          "form": "네 그렇습니다^^",
          "original_form": "네 그렇습니다^^",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017266.1.14",
          "form": "언니분에게 바라는점이 뭔가요??",
          "original_form": "언니분에게 바라는점이 뭔가요??",
          "speaker_id": "2",
