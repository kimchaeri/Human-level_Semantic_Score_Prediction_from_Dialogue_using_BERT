      "utterance": [
        {
          "id": "MDRW2100044939.1.1",
          "form": "해방타운 보다가 잠들엇네요 ㅠㅜ",
          "original_form": "해방타운 보다가 잠들엇네요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100044939.1.2",
          "form": "잼날것같았는데",
          "original_form": "잼날것같았는데",
          "speaker_id": "1",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100044939.1.3",
          "form": "해방타운 힐링되고 좋아요",
          "original_form": "해방타운 힐링되고 좋아요",
          "speaker_id": "2",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100044939.1.4",
          "form": "요즘 뭉쳐야찬다도 잼나고",
          "original_form": "요즘 뭉쳐야찬다도 잼나고",
          "speaker_id": "2",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100044939.1.5",
          "form": "골때리는...",
          "original_form": "골때리는...",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100044939.1.6",
          "form": "그것도 공차는거아니예요?",
          "original_form": "그것도 공차는거아니예요?",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100044939.1.7",
          "form": "축구요ㅎ",
          "original_form": "축구요ㅎ",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100044939.1.8",
          "form": "안정환이 감독",
          "original_form": "안정환이 감독",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100044939.1.9",
          "form": "축구가 인기네요 ㅎㅎ",
          "original_form": "축구가 인기네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100044939.1.10",
          "form": "농구하더니 다시 축구로 바꼈어요",
          "original_form": "농구하더니 다시 축구로 바꼈어요",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100044939.1.11",
          "form": "시즌2",
          "original_form": "시즌2",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100044939.1.12",
          "form": "농구도 했어오?",
          "original_form": "농구도 했어오?",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100044939.1.13",
          "form": "네 상암불낙스?ㅋ 허재가 감독하고",
          "original_form": "네 상암불낙스?ㅋ 허재가 감독하고",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100044939.1.14",
          "form": "요즘 스포츠들이 대세네요 ㅎ",
          "original_form": "요즘 스포츠들이 대세네요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100044939.1.15",
          "form": "이동국이 못하는 운동이  없는듯",
          "original_form": "이동국이 못하는 운동이  없는듯",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100044939.1.16",
          "form": "이동국도나와요?",
          "original_form": "이동국도나와요?",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100044939.1.17",
          "form": "네 농구로 선수하고 지금 축구코치로ㅎ",
          "original_form": "네 농구로 선수하고 지금 축구코치로ㅎ",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100044939.1.18",
          "form": "멋진 이동국 ㅋ",
          "original_form": "멋진 이동국 ㅋ",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100044939.1.19",
