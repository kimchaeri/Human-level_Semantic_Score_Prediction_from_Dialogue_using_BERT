      "utterance": [
        {
          "id": "MDRW2100045653.1.1",
          "form": "저도 집 근처엔 뚜레쥬르 멸종했는데",
          "original_form": "저도 집 근처엔 뚜레쥬르 멸종했는데",
          "speaker_id": "2",
          "time": "20210908 04:46"
        },
        {
          "id": "MDRW2100045653.1.2",
          "form": "이상하게 사무실 있는 동네에만 많더라구요",
          "original_form": "이상하게 사무실 있는 동네에만 많더라구요",
          "speaker_id": "2",
          "time": "20210908 04:46"
        },
        {
          "id": "MDRW2100045653.1.3",
          "form": "그래서 샌드위치 사면 아메리카노 주는줄도 거의 처음알았어요",
          "original_form": "그래서 샌드위치 사면 아메리카노 주는줄도 거의 처음알았어요",
          "speaker_id": "2",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045653.1.4",
          "form": "행운을 거머쥐셨네요ㅋㅋㅋㅋ",
          "original_form": "행운을 거머쥐셨네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045653.1.5",
          "form": "닉네임을 부르기가 어려우니까",
          "original_form": "닉네임을 부르기가 어려우니까",
          "speaker_id": "1",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045653.1.6",
          "form": "name2님이라고 해도 되나요?",
          "original_form": "&name2&님이라고 해도 되나요?",
          "speaker_id": "1",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045653.1.7",
          "form": "넵넵ㅋㅋㅋㅋㅋㅋ",
          "original_form": "넵넵ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045653.1.8",
          "form": "name1님은 어제 뭐 드셨나요!",
          "original_form": "&name1&님은 어제 뭐 드셨나요!",
          "speaker_id": "2",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045653.1.9",
          "form": "저 어제 맛없는 삼겹살 김밥먹엌ㅅ어요",
          "original_form": "저 어제 맛없는 삼겹살 김밥먹엌ㅅ어요",
          "speaker_id": "1",
          "time": "20210908 04:48"
        },
        {
          "id": "MDRW2100045653.1.10",
          "form": "앗 맛없다니ㅠㅠㅠㅠ",
          "original_form": "앗 맛없다니ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:48"
        },
        {
          "id": "MDRW2100045653.1.11",
          "form": "혹시 편의점꺼였나요?",
          "original_form": "혹시 편의점꺼였나요?",
          "speaker_id": "2",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045653.1.12",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045653.1.13",
          "form": "편의점에 그런 메뉴가 많던데",
          "original_form": "편의점에 그런 메뉴가 많던데",
          "speaker_id": "2",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045653.1.14",
          "form": "귀신이세요?",
          "original_form": "귀신이세요?",
          "speaker_id": "1",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045653.1.15",
          "form": "저 울뻔했어요",
          "original_form": "저 울뻔했어요",
          "speaker_id": "1",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045653.1.16",
          "form": "아이고 ㅠㅠㅠㅠㅠ",
          "original_form": "아이고 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045653.1.17",
          "form": "김밥이라우삼겹살 둘다 좋아하는 메뉴라 기대하고 샀는데 비계만 씹혔어요",
          "original_form": "김밥이라우삼겹살 둘다 좋아하는 메뉴라 기대하고 샀는데 비계만 씹혔어요",
          "speaker_id": "1",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045653.1.18",
          "form": "편의점 메뉴 사기전에는 그럴듯한데",
          "original_form": "편의점 메뉴 사기전에는 그럴듯한데",
          "speaker_id": "2",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045653.1.19",
          "form": "포장뜯어보면 내용물은 적고",
          "original_form": "포장뜯어보면 내용물은 적고",
          "speaker_id": "2",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100045653.1.20",
          "form": "차디차게 식어있고",
          "original_form": "차디차게 식어있고",
          "speaker_id": "2",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100045653.1.21",
          "form": "서럽고",
          "original_form": "서럽고",
          "speaker_id": "2",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100045653.1.22",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100045653.1.23",
          "form": "바보같이 또속았어요",
          "original_form": "바보같이 또속았어요",
          "speaker_id": "1",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100045653.1.24",
          "form": "이건 아니겠지 했는데",
          "original_form": "이건 아니겠지 했는데",
          "speaker_id": "1",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100045653.1.25",
          "form": "진짜 아니엌ㅅ던것이에요",
          "original_form": "진짜 아니엌ㅅ던것이에요",
          "speaker_id": "1",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100045653.1.26",
          "form": "아이고ㅠㅠㅠㅠㅠ",
          "original_form": "아이고ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100045653.1.27",
          "form": "name1님 잘못은 아니고 편의점이 나빴어요",
          "original_form": "&name1&님 잘못은 아니고 편의점이 나빴어요",
          "speaker_id": "2",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100045653.1.28",
          "form": "그런 어중간한걸 만들고 그럴듯한 문구와 포장으로 소비자를 현혹하다니",
          "original_form": "그런 어중간한걸 만들고 그럴듯한 문구와 포장으로 소비자를 현혹하다니",
          "speaker_id": "2",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100045653.1.29",
          "form": "맛없는걸 그대로 파는건 괜찮지만 맛없는걸  마치 맛있는 것 처럼 파는건 용서할 수 없어요",
          "original_form": "맛없는걸 그대로 파는건 괜찮지만 맛없는걸  마치 맛있는 것 처럼 파는건 용서할 수 없어요",
          "speaker_id": "2",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045653.1.30",
          "form": "앜ㅋㅋㅋ아낰ㅋㅋㅋㅋㅋ 너무 웃겨요 감사합니다",
          "original_form": "앜ㅋㅋㅋ아낰ㅋㅋㅋㅋㅋ 너무 웃겨요 감사합니다",
          "speaker_id": "1",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045653.1.31",
          "form": "ㅋㅋㅋ 대신 화냐주셔서 감사해요",
