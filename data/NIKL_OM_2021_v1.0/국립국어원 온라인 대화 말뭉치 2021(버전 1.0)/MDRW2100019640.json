      "utterance": [
        {
          "id": "MDRW2100019640.1.1",
          "form": "제일 재미있게 읽었던 책 있어?",
          "original_form": "제일 재미있게 읽었던 책 있어?",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019640.1.2",
          "form": "요즘 딱히 없는데;;",
          "original_form": "요즘 딱히 없는데;;",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019640.1.3",
          "form": "전에 읽었던 거라도",
          "original_form": "전에 읽었던 거라도",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019640.1.4",
          "form": "없어",
          "original_form": "없어",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019640.1.5",
          "form": "읽고싶은 작품도 없어?",
          "original_form": "읽고싶은 작품도 없어?",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019640.1.6",
          "form": "없어  예전에 있었는데 잘 기억이 안나",
          "original_form": "없어  예전에 있었는데 잘 기억이 안나",
          "speaker_id": "2",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019640.1.7",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019640.1.8",
          "form": "최근에 읽었던 책 있어?",
          "original_form": "최근에 읽었던 책 있어?",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019640.1.9",
          "form": "요즘 토지 읽고있어",
          "original_form": "요즘 토지 읽고있어",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019640.1.10",
          "form": "토지가 뭐야?",
          "original_form": "토지가 뭐야?",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019640.1.11",
          "form": "박경리 작가 대하소설",
          "original_form": "박경리 작가 대하소설",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019640.1.12",
          "form": "대하소설이 머야?",
          "original_form": "대하소설이 머야?",
