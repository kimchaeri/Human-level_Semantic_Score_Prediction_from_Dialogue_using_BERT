      "utterance": [
        {
          "id": "MDRW2100021782.1.1",
          "form": "다이소에 발길을 끊은지 좀 되어서",
          "original_form": "다이소에 발길을 끊은지 좀 되어서",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021782.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021782.1.3",
          "form": "동네에 다이소 없어용?",
          "original_form": "동네에 다이소 없어용?",
          "speaker_id": "1",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021782.1.4",
          "form": "그런 신상품이 있는지 몰랐네요",
          "original_form": "그런 신상품이 있는지 몰랐네요",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021782.1.5",
          "form": "아니여 큰게 있는데",
          "original_form": "아니여 큰게 있는데",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021782.1.6",
          "form": "미니멀리스트가 되기로 했으니까",
          "original_form": "미니멀리스트가 되기로 했으니까",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021782.1.7",
          "form": "다이소부터 끊어야하거든요",
          "original_form": "다이소부터 끊어야하거든요",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021782.1.8",
          "form": "미니멀리스트... 쉽지 않아요",
          "original_form": "미니멀리스트... 쉽지 않아요",
          "speaker_id": "1",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021782.1.9",
          "form": "저는 언젠가 쓰겠지 하고 많이 쟁여놔요 ㅠㅠ",
          "original_form": "저는 언젠가 쓰겠지 하고 많이 쟁여놔요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021782.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ제가 그랬다가",
          "original_form": "ㅋㅋㅋㅋㅋㅋ제가 그랬다가",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021782.1.11",
          "form": "엄마한테 집을 창고로 만들셈이냐",
          "original_form": "엄마한테 집을 창고로 만들셈이냐",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021782.1.12",
          "form": "잔소리 듣고",
          "original_form": "잔소리 듣고",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021782.1.13",
          "form": "코시국 1년 6개월째인데 손님 언제 올지 알고 얼마 전에 코스터도 샀어요^^",
          "original_form": "코시국 1년 6개월째인데 손님 언제 올지 알고 얼마 전에 코스터도 샀어요^^",
          "speaker_id": "1",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021782.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021782.1.15",
          "form": "그 나무로 된 코스터 진짜 너무 예뻐가지고",
          "original_form": "그 나무로 된 코스터 진짜 너무 예뻐가지고",
          "speaker_id": "1",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021782.1.16",
          "form": "홈파티가 대세인데요 !",
          "original_form": "홈파티가 대세인데요 !",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021782.1.17",
          "form": "홈파티 너무 좋아요 ㅠㅠㅠ",
          "original_form": "홈파티 너무 좋아요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021782.1.18",
          "form": "저 예전에 그것도 샀어요",
          "original_form": "저 예전에 그것도 샀어요",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021782.1.19",
          "form": "저는 라탄 코스터 만들려고 라탄 심줄을 샀단 말이죠",
          "original_form": "저는 라탄 코스터 만들려고 라탄 심줄을 샀단 말이죠",
          "speaker_id": "2",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021782.1.20",
          "form": "원통인데 거기에 술넣으면 정수기처럼 졸졸 나오는거요",
          "original_form": "원통인데 거기에 술넣으면 정수기처럼 졸졸 나오는거요",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021782.1.21",
          "form": "그냥 라탄 코스터만 샀으면 됐을것을",
          "original_form": "그냥 라탄 코스터만 샀으면 됐을것을",
          "speaker_id": "2",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021782.1.22",
          "form": "라탄 심줄 그대로 있겠네요 ㅋㅋㅋㅋㅋ",
          "original_form": "라탄 심줄 그대로 있겠네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021782.1.23",
          "form": "라탄 화분 바구니 예쁘던데요 ㅜ",
          "original_form": "라탄 화분 바구니 예쁘던데요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021782.1.24",
          "form": "오오 그 뭔줄 알아요 대형 캔",
          "original_form": "오오 그 뭔줄 알아요 대형 캔",
          "speaker_id": "2",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021782.1.25",
          "form": "그거 예뻐서 저 라탄 화분 하나 샀어요",
          "original_form": "그거 예뻐서 저 라탄 화분 하나 샀어요",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021782.1.26",
          "form": "라탄이 진짜 분위기 있어요 그래서 다 만들려고 공예도 배우고",
          "original_form": "라탄이 진짜 분위기 있어요 그래서 다 만들려고 공예도 배우고",
          "speaker_id": "2",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021782.1.27",
          "form": "자료사고 안할뿐",
          "original_form": "자료사고 안할뿐",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021782.1.28",
          "form": "재료..",
          "original_form": "재료..",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021782.1.29",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021782.1.30",
          "form": "뭔가 감성적이고 예쁘더라구요",
          "original_form": "뭔가 감성적이고 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021782.1.31",
          "form": "우리집이 한강뷰였으면 더 잘 어울렸을텐데..",
          "original_form": "우리집이 한강뷰였으면 더 잘 어울렸을텐데..",
          "speaker_id": "1",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021782.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021782.1.33",
          "form": "시티뷰도 갬성이죠",
          "original_form": "시티뷰도 갬성이죠",
          "speaker_id": "2",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021782.1.34",
          "form": "저는...",
          "original_form": "저는...",
          "speaker_id": "1",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021782.1.35",
          "form": "옆집뷰에요...",
          "original_form": "옆집뷰에요...",
          "speaker_id": "1",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021782.1.36",
          "form": "저두 옆집뷰..",
          "original_form": "저두 옆집뷰..",
          "speaker_id": "2",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021782.1.37",
          "form": "자꾸 눈마주챠요..",
          "original_form": "자꾸 눈마주챠요..",
          "speaker_id": "2",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021782.1.38",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "1",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021782.1.39",
          "form": "가끔 창문 열거나 닫을때",
          "original_form": "가끔 창문 열거나 닫을때",
          "speaker_id": "1",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021782.1.40",
          "form": "고소 당할까봐 좀 무서워요 관음증 환자 아닌데 오해 받을까바 ㅜㅜ",
