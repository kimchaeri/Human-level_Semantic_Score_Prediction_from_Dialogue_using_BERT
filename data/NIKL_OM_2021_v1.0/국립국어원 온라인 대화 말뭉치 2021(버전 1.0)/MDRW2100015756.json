        },
        {
          "id": "MDRW2100015756.1.4",
          "form": "네 뮤지컬 몇 번 봤어요 조금",
          "original_form": "네 뮤지컬 몇 번 봤어요 조금",
          "speaker_id": "1",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100015756.1.5",
          "form": "작품 제목이 뭔가요?",
          "original_form": "작품 제목이 뭔가요?",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100015756.1.6",
          "form": "웃는남자 봤는데 재밌더라구요",
          "original_form": "웃는남자 봤는데 재밌더라구요",
          "speaker_id": "1",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100015756.1.7",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100015756.1.8",
          "form": "콘서트 가보셨나요?",
          "original_form": "콘서트 가보셨나요?",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100015756.1.9",
          "form": "예전에 드림콘서트 가봤어요",
          "original_form": "예전에 드림콘서트 가봤어요",
          "speaker_id": "1",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100015756.1.10",
          "form": "오 아이돌 나오는 콘서트맞나요?",
          "original_form": "오 아이돌 나오는 콘서트맞나요?",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100015756.1.11",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100015756.1.12",
          "form": "공연 보신 적 있으세요?",
          "original_form": "공연 보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100015756.1.13",
