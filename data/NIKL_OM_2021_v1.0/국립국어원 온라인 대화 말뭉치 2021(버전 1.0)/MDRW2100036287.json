      "utterance": [
        {
          "id": "MDRW2100036287.1.1",
          "form": "여행계획 세우시는 편이신가용?",
          "original_form": "여행계획 세우시는 편이신가용?",
          "speaker_id": "1",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100036287.1.2",
          "form": "아니욬ㅋㅋㅋㅋ",
          "original_form": "아니욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036287.1.3",
          "form": "예전엔 꼼꼼히 세웠는데 이상하게 시간이가면갈수록",
          "original_form": "예전엔 꼼꼼히 세웠는데 이상하게 시간이가면갈수록",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036287.1.4",
          "form": "즉흥여행을 하게되더라구요",
          "original_form": "즉흥여행을 하게되더라구요",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036287.1.5",
          "form": "뭐 근데 그마저도 코로나 이후로는 제대로 못하고있지만요^^;",
          "original_form": "뭐 근데 그마저도 코로나 이후로는 제대로 못하고있지만요^^;",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036287.1.6",
          "form": "으윽 그쵸ㅠㅜ 코로나ㅜㅠ",
          "original_form": "으윽 그쵸ㅠㅜ 코로나ㅜㅠ",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036287.1.7",
          "form": "여행가서  물속에 여러사람들이 같이 놀고있는 사진들보면",
          "original_form": "여행가서  물속에 여러사람들이 같이 놀고있는 사진들보면",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036287.1.8",
          "form": "너무오싹해요;ㅠ  대단하다싶고",
          "original_form": "너무오싹해요;ㅠ  대단하다싶고",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036287.1.9",
          "form": "저도 처음 가보기전까진 나노단위로 세웠는데 가보고 의미 없음을 깨달았어요ㅋㅋㅠ",
          "original_form": "저도 처음 가보기전까진 나노단위로 세웠는데 가보고 의미 없음을 깨달았어요ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036287.1.10",
          "form": "아.. 맞아요",
          "original_form": "아.. 맞아요",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036287.1.11",
          "form": "나노단위...ㅎㅎㅎ",
          "original_form": "나노단위...ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036287.1.12",
          "form": "이번 여름 해변 피크라는거 듣고 놀랐어요",
          "original_form": "이번 여름 해변 피크라는거 듣고 놀랐어요",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036287.1.13",
          "form": "그쵸 ㅎㅎㅎ 저도 집밖에 나가면",
          "original_form": "그쵸 ㅎㅎㅎ 저도 집밖에 나가면",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036287.1.14",
          "form": "조금만 나가도 해수욕장이 널린곳에사는데",
          "original_form": "조금만 나가도 해수욕장이 널린곳에사는데",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036287.1.15",
          "form": "사람이 너무많아서 무섭더라구요ㅠㅠ",
          "original_form": "사람이 너무많아서 무섭더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036287.1.16",
          "form": "여행지라서 타지사람도 엄청 많이오는곳이거든요",
          "original_form": "여행지라서 타지사람도 엄청 많이오는곳이거든요",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036287.1.17",
          "form": "우와 이번에 바글바글 했었나요??",
          "original_form": "우와 이번에 바글바글 했었나요??",
          "speaker_id": "1",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100036287.1.18",
          "form": "집앞이시면 더 그러셨겠어요ㅜㅠ",
          "original_form": "집앞이시면 더 그러셨겠어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100036287.1.19",
          "form": "네 여기가",
          "original_form": "네 여기가",
          "speaker_id": "2",
