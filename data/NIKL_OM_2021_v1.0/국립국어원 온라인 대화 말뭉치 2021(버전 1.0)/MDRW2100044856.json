      "utterance": [
        {
          "id": "MDRW2100044856.1.1",
          "form": "저는 꿀 찍어먹는 거 좋아해요",
          "original_form": "저는 꿀 찍어먹는 거 좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044856.1.2",
          "form": "가래떡은 약간 짭짤하잖아요",
          "original_form": "가래떡은 약간 짭짤하잖아요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044856.1.3",
          "form": "그래서 단짠으로 ㅋㅋㅋㅋ",
          "original_form": "그래서 단짠으로 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044856.1.4",
          "form": "맞아요ㅋㅋㅋ 조청도 찍어 먹어보셨어요?",
          "original_form": "맞아요ㅋㅋㅋ 조청도 찍어 먹어보셨어요?",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044856.1.5",
          "form": "조청은 할머니집가면 내어주셔서 먹어봤는데 저는 꿀이 더 좋아요 ㅋㅋ",
          "original_form": "조청은 할머니집가면 내어주셔서 먹어봤는데 저는 꿀이 더 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044856.1.6",
          "form": "ㅎㅎ 저도 조청보단 꿀이 좋아요",
          "original_form": "ㅎㅎ 저도 조청보단 꿀이 좋아요",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044856.1.7",
          "form": "name2님 가래떡에 김 싸서 드셔보셨엉ㅅ?",
          "original_form": "&name2&님 가래떡에 김 싸서 드셔보셨엉ㅅ?",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044856.1.8",
          "form": "어요?",
          "original_form": "어요?",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044856.1.9",
          "form": "아뇨 김도 싸먹어요??",
          "original_form": "아뇨 김도 싸먹어요??",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044856.1.10",
          "form": "조미김 있짆아요 소금 뿌려진거",
          "original_form": "조미김 있짆아요 소금 뿌려진거",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044856.1.11",
          "form": "그거 나중에 싸서 드셔보세요",
          "original_form": "그거 나중에 싸서 드셔보세요",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044856.1.12",
          "form": "짱맛입니다",
          "original_form": "짱맛입니다",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044856.1.13",
          "form": "오 조만간 가래떡도 사야겠는데요 ㅋㅋㅋ",
          "original_form": "오 조만간 가래떡도 사야겠는데요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044856.1.14",
          "form": "지금 집에 김은 있거든요",
          "original_form": "지금 집에 김은 있거든요",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044856.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044856.1.16",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044856.1.17",
          "form": "추천합니다",
          "original_form": "추천합니다",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044856.1.18",
          "form": "방앗간을 찾아봐야겠어요 ㅋㅋㅋ",
          "original_form": "방앗간을 찾아봐야겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044856.1.19",
          "form": "아까 말씀하신 갓 나온 가래떡",
          "original_form": "아까 말씀하신 갓 나온 가래떡",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044856.1.20",
          "form": "그게 먹고 싶네요 ㅋㅋ",
          "original_form": "그게 먹고 싶네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044856.1.21",
          "form": "헿",
          "original_form": "헿",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044856.1.22",
          "form": "떡얘기하니까 저도 먹고 싶네요",
          "original_form": "떡얘기하니까 저도 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044856.1.23",
          "form": "그쵸 ㅋㅋㄱㄱ 먹을거 얘기하면 항상 이렇게 되네요 ㅋㅋㄱ",
          "original_form": "그쵸 ㅋㅋㄱㄱ 먹을거 얘기하면 항상 이렇게 되네요 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044856.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ 내일 뭐드실거에요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 내일 뭐드실거에요?",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044856.1.25",
          "form": "가래떡? ㅋㄱㄱㅋㄱㅋㄱ",
          "original_form": "가래떡? ㅋㄱㄱㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044856.1.26",
          "form": "원래 계란말이였는데 말이죠 ㅋㅋㅋ",
          "original_form": "원래 계란말이였는데 말이죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044856.1.27",
          "form": "앜ㅋㅋㅋㅋㅋ 계란말이는 반찬이고 가래떡은 간식이자나요",
          "original_form": "앜ㅋㅋㅋㅋㅋ 계란말이는 반찬이고 가래떡은 간식이자나요",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044856.1.28",
          "form": "계란멀이는 반찬이고",
          "original_form": "계란멀이는 반찬이고",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044856.1.29",
          "form": "가래떡은 간식이자나욬ㅋㅋㅋㅋ",
