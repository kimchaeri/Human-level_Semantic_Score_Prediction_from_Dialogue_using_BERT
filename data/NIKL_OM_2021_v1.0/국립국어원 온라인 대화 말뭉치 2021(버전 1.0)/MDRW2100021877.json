      "utterance": [
        {
          "id": "MDRW2100021877.1.1",
          "form": "어쩌다 보니 눈여겨보는 아이돌이 많아지더라고요",
          "original_form": "어쩌다 보니 눈여겨보는 아이돌이 많아지더라고요",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021877.1.2",
          "form": "전 투피엠 안좋아했는데",
          "original_form": "전 투피엠 안좋아했는데",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021877.1.3",
          "form": "작년에 우리집 유행하면서 ㅋㅋㅋ",
          "original_form": "작년에 우리집 유행하면서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021877.1.4",
          "form": "왜 어른들이 좋아했는지 알겠다..(?)",
          "original_form": "왜 어른들이 좋아했는지 알겠다..(?)",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021877.1.5",
          "form": "역시 어른들이 현명해요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "역시 어른들이 현명해요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021877.1.6",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021877.1.7",
          "form": "좋아하는 아이돌 있으세요?",
          "original_form": "좋아하는 아이돌 있으세요?",
          "speaker_id": "1",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021877.1.8",
          "form": "음 저는 최근에 있지 좋더라구요!!",
          "original_form": "음 저는 최근에 있지 좋더라구요!!",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021877.1.9",
          "form": "오 제오ㅏ피~",
          "original_form": "오 제오ㅏ피~",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021877.1.10",
          "form": "투피엠부터 제이와이피 픽이시군요",
          "original_form": "투피엠부터 제이와이피 픽이시군요",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021877.1.11",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021877.1.12",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021877.1.13",
          "form": "그러게요?",
          "original_form": "그러게요?",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021877.1.14",
          "form": "쩝",
          "original_form": "쩝",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021877.1.15",
          "form": "와이지랑은 멀긴해요..",
          "original_form": "와이지랑은 멀긴해요..",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021877.1.16",
          "form": "앗 저도... 와이지 중에선 악뮤만 좋아해요",
          "original_form": "앗 저도... 와이지 중에선 악뮤만 좋아해요",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021877.1.17",
          "form": "저듀요   ... 아니면 투애니원..?",
          "original_form": "저듀요   ... 아니면 투애니원..?",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021877.1.18",
          "form": "악뮤 신곡은 들어보셨어요?",
          "original_form": "악뮤 신곡은 들어보셨어요?",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021877.1.19",
          "form": "아뇨ㅠㅠㅠ 제가 가요를 잘 안들어서",
          "original_form": "아뇨ㅠㅠㅠ 제가 가요를 잘 안들어서",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021877.1.20",
          "form": "악뮤도 신곡이 나왔군요  ..",
          "original_form": "악뮤도 신곡이 나왔군요  ..",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021877.1.21",
          "form": "업데이트가 느린 편입니다..",
          "original_form": "업데이트가 느린 편입니다..",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021877.1.22",
          "form": "저도 다 듣진 않았는데 타이틀이 이선희 씨가 피쳐링했대요",
          "original_form": "저도 다 듣진 않았는데 타이틀이 이선희 씨가 피쳐링했대요",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021877.1.23",
          "form": "마블 영화 좋아하세요?",
          "original_form": "마블 영화 좋아하세요?",
          "speaker_id": "1",
