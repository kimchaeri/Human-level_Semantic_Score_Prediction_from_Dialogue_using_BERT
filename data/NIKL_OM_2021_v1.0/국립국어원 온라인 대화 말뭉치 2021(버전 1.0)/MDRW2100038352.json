      "utterance": [
        {
          "id": "MDRW2100038352.1.1",
          "form": "요새 펭수도 다 빠졌지",
          "original_form": "요새 펭수도 다 빠졌지",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100038352.1.2",
          "form": "펭수 인기 거품이에요?ㅠㅠ",
          "original_form": "펭수 인기 거품이에요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100038352.1.3",
          "form": "버블버블이에요",
          "original_form": "버블버블이에요",
          "speaker_id": "1",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100038352.1.4",
          "form": "어쩔수없지... 애기들은 아직 좋아할 거 같은뎅",
          "original_form": "어쩔수없지... 애기들은 아직 좋아할 거 같은뎅",
          "speaker_id": "1",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100038352.1.5",
          "form": "배라에 아직 콜라보 상품 많던데ㅠㅠ",
          "original_form": "배라에 아직 콜라보 상품 많던데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100038352.1.6",
          "form": "그럼 아직 인기 있나바ㅋㅋㅋ 조회수는 그냥 그렇던데",
          "original_form": "그럼 아직 인기 있나바ㅋㅋㅋ 조회수는 그냥 그렇던데",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100038352.1.7",
          "form": "성인들은 동심이 없어서 식었나...",
          "original_form": "성인들은 동심이 없어서 식었나...",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100038352.1.8",
          "form": "죽어버린 동심...ㅋㅋㅋㅋㅋ",
          "original_form": "죽어버린 동심...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100038352.1.9",
          "form": "어른들은 자극적인 무언가를 찾아 떠났어ㅠㅠ",
          "original_form": "어른들은 자극적인 무언가를 찾아 떠났어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100038352.1.10",
          "form": "요새 엄청 인기있고 그런 콘텐츠는 없는듯",
          "original_form": "요새 엄청 인기있고 그런 콘텐츠는 없는듯",
          "speaker_id": "1",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100038352.1.11",
          "form": "다들 각자 자신만의 방에 자신만의 알고리즘과 노나바",
          "original_form": "다들 각자 자신만의 방에 자신만의 알고리즘과 노나바",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100038352.1.12",
          "form": "맨날 똑같은거 또보긴함",
          "original_form": "맨날 똑같은거 또보긴함",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100038352.1.13",
          "form": "소화기관이 죽어버린 오타쿠라 되새김질만 한다는 틧 생각나네",
          "original_form": "소화기관이 죽어버린 오타쿠라 되새김질만 한다는 틧 생각나네",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038352.1.14",
          "form": "원래 보던게 존맛인거임...",
          "original_form": "원래 보던게 존맛인거임...",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038352.1.15",
          "form": "맛긴한데 진짜 늙어서 기력 없는 사람 같애",
