      "utterance": [
        {
          "id": "MDRW2100007434.1.1",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007434.1.2",
          "form": "무슨과예요",
          "original_form": "무슨과예요",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007434.1.3",
          "form": "ㅋㅋㅋㅋ전 통상과에요",
          "original_form": "ㅋㅋㅋㅋ전 통상과에요",
          "speaker_id": "2",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007434.1.4",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007434.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007434.1.6",
          "form": "통상과가 뭐예요?",
          "original_form": "통상과가 뭐예요?",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007434.1.7",
          "form": "석사끝내고 직장인3년차예요",
          "original_form": "석사끝내고 직장인3년차예요",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007434.1.8",
          "form": "무역, 통상, 경제 이런거에요",
          "original_form": "무역, 통상, 경제 이런거에요",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007434.1.9",
          "form": "와 석사까지..! 무슨과 나오셨는지 여쭤봐도 될까영",
          "original_form": "와 석사까지..! 무슨과 나오셨는지 여쭤봐도 될까영",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007434.1.10",
          "form": "막내는 대학생이예요",
          "original_form": "막내는 대학생이예요",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007434.1.11",
          "form": "심리예요 ㅎㅎ",
          "original_form": "심리예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007434.1.12",
          "form": "심리는 석박사가 필수라서",
          "original_form": "심리는 석박사가 필수라서",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007434.1.13",
          "form": "우와 심리학과 다니는 친구들이 뭔가 멋져보였는데",
          "original_form": "우와 심리학과 다니는 친구들이 뭔가 멋져보였는데",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007434.1.14",
          "form": "석박사가 필수더라도 석박사까지 하신게 멋져요",
          "original_form": "석박사가 필수더라도 석박사까지 하신게 멋져요",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007434.1.15",
          "form": "엄마도 심리상담사세요",
          "original_form": "엄마도 심리상담사세요",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007434.1.16",
          "form": "심리 집안이시네요ㅎㅎ",
          "original_form": "심리 집안이시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007434.1.17",
          "form": "어릴때부터 영향이 컸던거같아요",
          "original_form": "어릴때부터 영향이 컸던거같아요",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007434.1.18",
          "form": "무역 통상 경제 코로나때문에 영향있는 직업맞죠?",
          "original_form": "무역 통상 경제 코로나때문에 영향있는 직업맞죠?",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007434.1.19",
          "form": "언니가 통역사인데 요즘은 일이 많이 줄었다고 하더라그요",
          "original_form": "언니가 통역사인데 요즘은 일이 많이 줄었다고 하더라그요",
          "speaker_id": "1",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007434.1.20",
          "form": "예전에 대학다닐때 무역에 관심많았는데",
          "original_form": "예전에 대학다닐때 무역에 관심많았는데",
          "speaker_id": "1",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007434.1.21",
          "form": "전공이랑 너무 다르기도했고",
          "original_form": "전공이랑 너무 다르기도했고",
          "speaker_id": "1",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007434.1.22",
          "form": "생각보다 더 어렵더라고요",
          "original_form": "생각보다 더 어렵더라고요",
          "speaker_id": "1",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007434.1.23",
          "form": "저도 대학오고 약간 후회...ㅋㅋㅋ했는데",
          "original_form": "저도 대학오고 약간 후회...ㅋㅋㅋ했는데",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007434.1.24",
          "form": "제가 원해서 온거다보니까 재밌긴 하더라구요",
          "original_form": "제가 원해서 온거다보니까 재밌긴 하더라구요",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007434.1.25",
          "form": "외국어공부도 필수죠?",
          "original_form": "외국어공부도 필수죠?",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007434.1.26",
          "form": "넵 영어가 필수에요!",
          "original_form": "넵 영어가 필수에요!",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007434.1.27",
          "form": "무역에 한참 관심있을때 스페인어배웠어요 학원다니고",
          "original_form": "무역에 한참 관심있을때 스페인어배웠어요 학원다니고",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007434.1.28",
          "form": "무역학과도 아닌데 교수님찾아가곸ㅋㅋ",
          "original_form": "무역학과도 아닌데 교수님찾아가곸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007434.1.29",
          "form": "대학생때는 열정이넘쳤어요",
          "original_form": "대학생때는 열정이넘쳤어요",
          "speaker_id": "1",
          "time": "20210822 02:15"
        }
      ]
