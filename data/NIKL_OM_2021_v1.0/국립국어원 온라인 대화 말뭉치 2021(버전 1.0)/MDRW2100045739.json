      "utterance": [
        {
          "id": "MDRW2100045739.1.1",
          "form": "사과는 최고죠ㅠㅠ",
          "original_form": "사과는 최고죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:30"
        },
        {
          "id": "MDRW2100045739.1.2",
          "form": "근데 과일하니까 동남아 과일이 그렇게 맛있담서요?",
          "original_form": "근데 과일하니까 동남아 과일이 그렇게 맛있담서요?",
          "speaker_id": "1",
          "time": "20210908 06:30"
        },
        {
          "id": "MDRW2100045739.1.3",
          "form": "제철사과로 담근 애플시나몬도 먹고 싶네요ㅠㅠ",
          "original_form": "제철사과로 담근 애플시나몬도 먹고 싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:30"
        },
        {
          "id": "MDRW2100045739.1.4",
          "form": "동남아 과일이 뭐가 있을까요??",
          "original_form": "동남아 과일이 뭐가 있을까요??",
          "speaker_id": "2",
          "time": "20210908 06:30"
        },
        {
          "id": "MDRW2100045739.1.5",
          "form": "람부탄??",
          "original_form": "람부탄??",
          "speaker_id": "2",
          "time": "20210908 06:30"
        },
        {
          "id": "MDRW2100045739.1.6",
          "form": "리치같은 거요!",
          "original_form": "리치같은 거요!",
          "speaker_id": "1",
          "time": "20210908 06:31"
        },
        {
          "id": "MDRW2100045739.1.7",
          "form": "엄청엄청 달대요",
          "original_form": "엄청엄청 달대요",
          "speaker_id": "1",
          "time": "20210908 06:31"
        },
        {
          "id": "MDRW2100045739.1.8",
          "form": "사진으로만 보고 가공된걸로만 먹어본 리치!",
          "original_form": "사진으로만 보고 가공된걸로만 먹어본 리치!",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045739.1.9",
          "form": "생과일로 먹으면 얼마나 맛있을까요ㅠㅠ",
          "original_form": "생과일로 먹으면 얼마나 맛있을까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045739.1.10",
          "form": "크으ㅠㅠㅠㅠ",
          "original_form": "크으ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045739.1.11",
          "form": "망고도 엄청 달대요!",
          "original_form": "망고도 엄청 달대요!",
          "speaker_id": "1",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045739.1.12",
          "form": "망고도 그렇게 싸다면서요ㅠㅠ",
          "original_form": "망고도 그렇게 싸다면서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045739.1.13",
          "form": "싸고 맛있고...천국같을 거 같애요ㅠㅠ",
          "original_form": "싸고 맛있고...천국같을 거 같애요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045739.1.14",
          "form": "드러니까요ㅠㅠㅠㅠ",
          "original_form": "드러니까요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045739.1.15",
          "form": "언젠가 꼭 가서 과일배만 채우고 싶네요ㅋㅋ",
          "original_form": "언젠가 꼭 가서 과일배만 채우고 싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045739.1.16",
          "form": "저두요ㅠㅠ",
          "original_form": "저두요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045739.1.17",
          "form": "망고하나만 먹자고 가지는 않겠지만 간다면",
          "original_form": "망고하나만 먹자고 가지는 않겠지만 간다면",
          "speaker_id": "2",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045739.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045739.1.19",
          "form": "과일 엄청 많이 먹고 오고 싶어요ㅠㅠ",
          "original_form": "과일 엄청 많이 먹고 오고 싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045739.1.20",
          "form": "관광지 보고 먹고 보고 먹고ㅎㅎ",
          "original_form": "관광지 보고 먹고 보고 먹고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045739.1.21",
          "form": "맞아요ㅠㅠㅠ 돌아나니면서 하나씩 먹고 길거리음식도 뭐가 있는지는 모르지만 잔뜩 먹고오고 싶어요ㅠ",
          "original_form": "맞아요ㅠㅠㅠ 돌아나니면서 하나씩 먹고 길거리음식도 뭐가 있는지는 모르지만 잔뜩 먹고오고 싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045739.1.22",
          "form": "크 길거리음식ㅠㅠㅠ 너무 맛있을 거 같아요",
          "original_form": "크 길거리음식ㅠㅠㅠ 너무 맛있을 거 같아요",
          "speaker_id": "1",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045739.1.23",
          "form": "본 식사보다 간식으로 배 찰 것 같은ㅋㅋㅋ",
          "original_form": "본 식사보다 간식으로 배 찰 것 같은ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045739.1.24",
          "form": "저두욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저두욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045739.1.25",
          "form": "가면 돌아다니느라 소화될테니",
          "original_form": "가면 돌아다니느라 소화될테니",
          "speaker_id": "2",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045739.1.26",
          "form": "하루종일 끊임없이 먹을 것 같아요ㅠㅠ",
          "original_form": "하루종일 끊임없이 먹을 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:38"
        }
      ]
    }
