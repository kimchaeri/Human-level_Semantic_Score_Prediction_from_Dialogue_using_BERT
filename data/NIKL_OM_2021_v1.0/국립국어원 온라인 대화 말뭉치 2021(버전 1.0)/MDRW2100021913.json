        {
          "id": "MDRW2100021913.1.1",
          "form": "나 그 시뇨라 짝사랑ㅈ남 성유물 찾음",
          "original_form": "나 그 시뇨라 짝사랑ㅈ남 성유물 찾음",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021913.1.2",
          "form": "수호자의 마음?",
          "original_form": "수호자의 마음?",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021913.1.3",
          "form": "헐 그거",
          "original_form": "헐 그거",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021913.1.4",
          "form": "반도 안읽었는데",
          "original_form": "반도 안읽었는데",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021913.1.5",
          "form": "ㅇㅇ 그거 맞아",
          "original_form": "ㅇㅇ 그거 맞아",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100021913.1.6",
          "form": "중간중간에 뜨는 설명 읽어보면",
          "original_form": "중간중간에 뜨는 설명 읽어보면",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100021913.1.7",
          "form": "선령이 과거 인류를 이끌었던 위대한 종족이라고 되어있음",
          "original_form": "선령이 과거 인류를 이끌었던 위대한 종족이라고 되어있음",
          "speaker_id": "1",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021913.1.8",
          "form": "아 설마 혹시",
          "original_form": "아 설마 혹시",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021913.1.9",
          "form": "심연메이지들이 들고다니는",
          "original_form": "심연메이지들이 들고다니는",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021913.1.10",
          "form": "하얀 나뭇가지",
          "original_form": "하얀 나뭇가지",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021913.1.11",
          "form": "그거 비경 깨면 보상주는",
          "original_form": "그거 비경 깨면 보상주는",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021913.1.12",
          "form": "그 나무 가지인가?",
          "original_form": "그 나무 가지인가?",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021913.1.13",
          "form": "아 그럴지도",
          "original_form": "아 그럴지도",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021913.1.14",
          "form": "나선비경이나",
          "original_form": "나선비경이나",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021913.1.15",
          "form": "인동의 고목이",
          "original_form": "인동의 고목이",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021913.1.16",
          "form": "다 나무로 표현되는거 보면",
          "original_form": "다 나무로 표현되는거 보면",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021913.1.17",
          "form": "여기도 세계수 개념 있는듯",
          "original_form": "여기도 세계수 개념 있는듯",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021913.1.18",
          "form": "아니 인동의 고목은",
          "original_form": "아니 인동의 고목은",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021913.1.19",
          "form": "왜 그 용의 피로",
          "original_form": "왜 그 용의 피로",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021913.1.20",
          "form": "자라는거야",
          "original_form": "자라는거야",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021913.1.21",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.22",
          "form": "너무 불길하잖아",
          "original_form": "너무 불길하잖아",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.23",
          "form": "몰라 무서워...",
          "original_form": "몰라 무서워...",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.24",
          "form": "이 겜 장르 가끔보면 바꿔야 할거같음",
          "original_form": "이 겜 장르 가끔보면 바꿔야 할거같음",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.25",
          "form": "다 주면 거기서 알이라도 나오는거 아님?",
          "original_form": "다 주면 거기서 알이라도 나오는거 아님?",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.26",
          "form": "심지어 심장",
          "original_form": "심지어 심장",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.27",
          "form": "살아있잖아",
          "original_form": "살아있잖아",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ예쁜 날개 코스튬을 줍니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ예쁜 날개 코스튬을 줍니다",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.29",
          "form": "ㅇㅇ뭔가 좀",
          "original_form": "ㅇㅇ뭔가 좀",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.30",
          "form": "플래그라고 할만한걸",
          "original_form": "플래그라고 할만한걸",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.31",
          "form": "자꾸 세우게 해",
          "original_form": "자꾸 세우게 해",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.32",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.33",
          "form": "주인공은",
          "original_form": "주인공은",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.34",
          "form": "세계멸망 플래그.....",
          "original_form": "세계멸망 플래그.....",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021913.1.35",
          "form": "선령과 같은 종족으로 추정됨",
          "original_form": "선령과 같은 종족으로 추정됨",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021913.1.36",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021913.1.37",
          "form": "이건 어디까지나 추정임",
          "original_form": "이건 어디까지나 추정임",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021913.1.38",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021913.1.39",
          "form": "볼수있어서?",
          "original_form": "볼수있어서?",
          "speaker_id": "2",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021913.1.40",
          "form": "일단 캐릭터 화면에서",
          "original_form": "일단 캐릭터 화면에서",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021913.1.41",
          "form": "음성 들을 수 있잖아",
          "original_form": "음성 들을 수 있잖아",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021913.1.42",
          "form": "거기에 간단한 대화가 실려있는데",
          "original_form": "거기에 간단한 대화가 실려있는데",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021913.1.43",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021913.1.44",
          "form": "주인공이랑 페이몬이 선령에 대해 대화하는 거에서",
          "original_form": "주인공이랑 페이몬이 선령에 대해 대화하는 거에서",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021913.1.45",
          "form": "주인공이",
          "original_form": "주인공이",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021913.1.46",
          "form": "왜 선령을 보면 슬픈 감정이 드는걸까",
          "original_form": "왜 선령을 보면 슬픈 감정이 드는걸까",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.47",
          "form": "선령이랑 대화도 해??",
          "original_form": "선령이랑 대화도 해??",
          "speaker_id": "2",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.48",
          "form": "라고 말함",
          "original_form": "라고 말함",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.49",
          "form": "ㅇㅎ.....",
          "original_form": "ㅇㅎ.....",
          "speaker_id": "2",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.50",
          "form": "아니 근데 그러면",
          "original_form": "아니 근데 그러면",
          "speaker_id": "2",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.51",
          "form": "너무 찝찝하잖아",
          "original_form": "너무 찝찝하잖아",
          "speaker_id": "2",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.52",
          "form": "워프포인트도 현재 대륙 사람들은 아무도 사용법을 모르는대",
          "original_form": "워프포인트도 현재 대륙 사람들은 아무도 사용법을 모르는대",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.53",
          "form": "나 선령 염색도 해서",
          "original_form": "나 선령 염색도 해서",
          "speaker_id": "2",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.54",
          "form": "장식용이라고",
          "original_form": "장식용이라고",
          "speaker_id": "2",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.55",
          "form": "주인공은 알 고 있음",
          "original_form": "주인공은 알 고 있음",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.56",
          "form": "달고다니는데....",
          "original_form": "달고다니는데....",
          "speaker_id": "2",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.57",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.58",
          "form": "그리고 마지막으로",
          "original_form": "그리고 마지막으로",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021913.1.59",
          "form": "근데 그럼",
          "original_form": "근데 그럼",
          "speaker_id": "2",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021913.1.60",
          "form": "캐서린은?",
          "original_form": "캐서린은?",
          "speaker_id": "2",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021913.1.61",
          "form": "공식 게임 홈페이지에서 주인공 소개문구 안봤지?",
          "original_form": "공식 게임 홈페이지에서 주인공 소개문구 안봤지?",
          "speaker_id": "1",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021913.1.62",
          "form": "ㅇㅇ안봤어",
          "original_form": "ㅇㅇ안봤어",
          "speaker_id": "2",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021913.1.63",
          "form": "네 고국은 멸망하고 큰 왕국은 이미 모래로 화했다. 허나 망국의 후예여 지나간 일로 슬퍼하지 말라",
          "original_form": "네 고국은 멸망하고 큰 왕국은 이미 모래로 화했다. 허나 망국의 후예여 지나간 일로 슬퍼하지 말라",
          "speaker_id": "1",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021913.1.64",
          "form": "주인공 소개문구임",
          "original_form": "주인공 소개문구임",
          "speaker_id": "1",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021913.1.65",
          "form": "ㅇㅎ.....",
          "original_form": "ㅇㅎ.....",
          "speaker_id": "2",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021913.1.66",
          "form": "그리고 나선비경 소개글 보면",
          "original_form": "그리고 나선비경 소개글 보면",
          "speaker_id": "1",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021913.1.67",
          "form": "아주 오래전 멸망한 왕국의 산물이고",
          "original_form": "아주 오래전 멸망한 왕국의 산물이고",
          "speaker_id": "1",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021913.1.68",
