      "utterance": [
        {
          "id": "MDRW2100032484.1.1",
          "form": "그렇지 맛집 유명한 데가 많지",
          "original_form": "그렇지 맛집 유명한 데가 많지",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100032484.1.2",
          "form": "밀면도 유명하고",
          "original_form": "밀면도 유명하고",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032484.1.3",
          "form": "국제시장에 냉족발 맛있더라",
          "original_form": "국제시장에 냉족발 맛있더라",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032484.1.4",
          "form": "또 뭐가 유명하지",
          "original_form": "또 뭐가 유명하지",
          "speaker_id": "2",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032484.1.5",
          "form": "그 시장에 떡볶이랑 씨앗호떡도 유명하던데",
          "original_form": "그 시장에 떡볶이랑 씨앗호떡도 유명하던데",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032484.1.6",
          "form": "나는 기억이 잘 안나네",
          "original_form": "나는 기억이 잘 안나네",
          "speaker_id": "2",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032484.1.7",
          "form": "낙곱새 완전 유명하고",
          "original_form": "낙곱새 완전 유명하고",
          "speaker_id": "1",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032484.1.8",
          "form": "낙곱새 서울에도 생겼더라",
          "original_form": "낙곱새 서울에도 생겼더라",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032484.1.9",
          "form": "홍대인가 여기저기 생겼다더라",
          "original_form": "홍대인가 여기저기 생겼다더라",
          "speaker_id": "1",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032484.1.10",
          "form": "부산 하면 해산물인가",
          "original_form": "부산 하면 해산물인가",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032484.1.11",
          "form": "수변공원 회센터도 유명하지",
          "original_form": "수변공원 회센터도 유명하지",
          "speaker_id": "1",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100032484.1.12",
          "form": "진짜  맛집 많네",
          "original_form": "진짜  맛집 많네",
