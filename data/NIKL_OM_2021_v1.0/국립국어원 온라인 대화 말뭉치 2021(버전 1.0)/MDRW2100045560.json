      "utterance": [
        {
          "id": "MDRW2100045560.1.1",
          "form": "구입하고 싶은 물건 있나요?",
          "original_form": "구입하고 싶은 물건 있나요?",
          "speaker_id": "2",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100045560.1.2",
          "form": "네. 있습니다 집이요",
          "original_form": "네. 있습니다 집이요",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100045560.1.3",
          "form": "그거말고 좀더 작은 물건은 뭐 있나요?",
          "original_form": "그거말고 좀더 작은 물건은 뭐 있나요?",
          "speaker_id": "2",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100045560.1.4",
          "form": "오븐이요.",
          "original_form": "오븐이요.",
          "speaker_id": "1",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100045560.1.5",
          "form": "큰 오븐이요.ㅠㅜ이태리산",
          "original_form": "큰 오븐이요.ㅠㅜ이태리산",
          "speaker_id": "1",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100045560.1.6",
          "form": "아 특별히 갖고싶은 오븐 브랜드가 있나요?",
          "original_form": "아 특별히 갖고싶은 오븐 브랜드가 있나요?",
          "speaker_id": "2",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100045560.1.7",
          "form": "지에라나 스메그나 우녹스요",
          "original_form": "지에라나 스메그나 우녹스요",
          "speaker_id": "1",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100045560.1.8",
          "form": "많이 비싼편인가요???",
          "original_form": "많이 비싼편인가요???",
          "speaker_id": "2",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100045560.1.9",
          "form": "네 백만원부터 삼백만원까지 합니닷",
          "original_form": "네 백만원부터 삼백만원까지 합니닷",
          "speaker_id": "1",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100045560.1.10",
          "form": "와 삼백짜린 많이 큰거겠죠???",
          "original_form": "와 삼백짜린 많이 큰거겠죠???",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100045560.1.11",
          "form": "네 집이 좀 커야해요 흑흑",
          "original_form": "네 집이 좀 커야해요 흑흑",
          "speaker_id": "1",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100045560.1.12",
          "form": "가스오븐이나 전기오븐 둘중에 어떤걸 선호하세요?",
          "original_form": "가스오븐이나 전기오븐 둘중에 어떤걸 선호하세요?",
          "speaker_id": "2",
          "time": "20210908 03:37"
        },
        {
          "id": "MDRW2100045560.1.13",
          "form": "저는 전기오븐이욥",
          "original_form": "저는 전기오븐이욥",
          "speaker_id": "1",
          "time": "20210908 03:38"
        },
        {
          "id": "MDRW2100045560.1.14",
          "form": "아 저는 가스오븐이 더 좋은건줄 알았어요!",
          "original_form": "아 저는 가스오븐이 더 좋은건줄 알았어요!",
          "speaker_id": "2",
          "time": "20210908 03:38"
        },
        {
          "id": "MDRW2100045560.1.15",
          "form": "하나 설치해보고싶어서 집에 오븐 넣을 자리도 연구해보고 그랬엇어요.",
          "original_form": "하나 설치해보고싶어서 집에 오븐 넣을 자리도 연구해보고 그랬엇어요.",
          "speaker_id": "1",
          "time": "20210908 03:39"
        }
      ]
