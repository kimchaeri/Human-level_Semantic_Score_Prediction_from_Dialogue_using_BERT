      "utterance": [
        {
          "id": "MDRW2100015941.1.1",
          "form": "올림픽 보셨나요?",
          "original_form": "올림픽 보셨나요?",
          "speaker_id": "2",
          "time": "20210826 03:23"
        },
        {
          "id": "MDRW2100015941.1.2",
          "form": "네네 봤어요",
          "original_form": "네네 봤어요",
          "speaker_id": "1",
          "time": "20210826 03:24"
        },
        {
          "id": "MDRW2100015941.1.3",
          "form": "양궁이랑 여자배구요ㅎㅎㅎ",
          "original_form": "양궁이랑 여자배구요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 03:24"
        },
        {
          "id": "MDRW2100015941.1.4",
          "form": "나머진 안봣어요",
          "original_form": "나머진 안봣어요",
          "speaker_id": "1",
          "time": "20210826 03:24"
        },
        {
          "id": "MDRW2100015941.1.5",
          "form": "배구 재밌더라고요 팬되었어요",
          "original_form": "배구 재밌더라고요 팬되었어요",
          "speaker_id": "2",
          "time": "20210826 03:24"
        },
        {
          "id": "MDRW2100015941.1.6",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 03:24"
        },
        {
          "id": "MDRW2100015941.1.7",
          "form": "김희진 선수랑",
          "original_form": "김희진 선수랑",
          "speaker_id": "1",
          "time": "20210826 03:24"
        },
        {
          "id": "MDRW2100015941.1.8",
          "form": "김연경 선수",
          "original_form": "김연경 선수",
          "speaker_id": "1",
          "time": "20210826 03:24"
        },
        {
          "id": "MDRW2100015941.1.9",
          "form": "멋잇어요",
          "original_form": "멋잇어요",
          "speaker_id": "1",
          "time": "20210826 03:24"
        },
        {
          "id": "MDRW2100015941.1.10",
          "form": "저는 양효진 선수 멋있더라고요",
          "original_form": "저는 양효진 선수 멋있더라고요",
          "speaker_id": "2",
          "time": "20210826 03:26"
        },
        {
          "id": "MDRW2100015941.1.11",
          "form": "너무 잘막아요ㅋㅋ",
          "original_form": "너무 잘막아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:26"
        },
        {
          "id": "MDRW2100015941.1.12",
          "form": "아 양효진선수도 멋잇구 ㅎㅎㅎ",
          "original_form": "아 양효진선수도 멋잇구 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 03:28"
        },
        {
          "id": "MDRW2100015941.1.13",
          "form": "진짜 다들 넘 잘하셨어요",
          "original_form": "진짜 다들 넘 잘하셨어요",
          "speaker_id": "1",
          "time": "20210826 03:29"
        },
        {
          "id": "MDRW2100015941.1.14",
          "form": "배구 직관 가보신적있으세요?",
          "original_form": "배구 직관 가보신적있으세요?",
          "speaker_id": "2",
          "time": "20210826 03:29"
        },
        {
          "id": "MDRW2100015941.1.15",
          "form": "아뇨! 저는 한번도 안가봤어요",
          "original_form": "아뇨! 저는 한번도 안가봤어요",
          "speaker_id": "1",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100015941.1.16",
          "form": "사실 이번 올림픽 전에는 배구에 그다지 관심이 없었어요ㅠㅠ",
