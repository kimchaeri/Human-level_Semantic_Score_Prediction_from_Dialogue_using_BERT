      "utterance": [
        {
          "id": "MDRW2100003809.1.1",
          "form": "남친은 자주 만나세요?",
          "original_form": "남친은 자주 만나세요?",
          "speaker_id": "2",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003809.1.2",
          "form": "아뇨 남친 애있는 돌싱이거든요?",
          "original_form": "아뇨 남친 애있는 돌싱이거든요?",
          "speaker_id": "1",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003809.1.3",
          "form": "요즘 애 유치원 방학이라고 매주 애 만나더라구요",
          "original_form": "요즘 애 유치원 방학이라고 매주 애 만나더라구요",
          "speaker_id": "1",
          "time": "20210821 12:23"
        },
        {
          "id": "MDRW2100003809.1.4",
          "form": "그래서 요즘은 자주 안봐요",
          "original_form": "그래서 요즘은 자주 안봐요",
          "speaker_id": "1",
          "time": "20210821 12:23"
        },
        {
          "id": "MDRW2100003809.1.5",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:23"
        },
        {
          "id": "MDRW2100003809.1.6",
          "form": "애는 안 키우나봐요",
          "original_form": "애는 안 키우나봐요",
          "speaker_id": "2",
          "time": "20210821 12:23"
        },
        {
          "id": "MDRW2100003809.1.7",
          "form": "name1님한테 잘 해주시나요?",
          "original_form": "&name1&님한테 잘 해주시나요?",
          "speaker_id": "2",
          "time": "20210821 12:23"
        },
        {
          "id": "MDRW2100003809.1.8",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003809.1.9",
          "form": "객관적으로보면 아닌것같은데 잘해주려는 편이에요",
          "original_form": "객관적으로보면 아닌것같은데 잘해주려는 편이에요",
          "speaker_id": "1",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003809.1.10",
          "form": "애는 전부인이 키워요",
          "original_form": "애는 전부인이 키워요",
          "speaker_id": "1",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003809.1.11",
          "form": "애초에 양육비 받아가려고 전부인네 가족이 애 납치해갔어요ㅋㅋㅋ",
          "original_form": "애초에 양육비 받아가려고 전부인네 가족이 애 납치해갔어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003809.1.12",
          "form": "초면인데 이런 얘기해도 되겠죠?",
          "original_form": "초면인데 이런 얘기해도 되겠죠?",
          "speaker_id": "1",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003809.1.13",
          "form": "헉 납치라니",
          "original_form": "헉 납치라니",
          "speaker_id": "2",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003809.1.14",
          "form": "네~그럼요 ㅎㅎ",
          "original_form": "네~그럼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003809.1.15",
          "form": "처음에 애있는 돌싱인거 알고 만난거거든요?",
          "original_form": "처음에 애있는 돌싱인거 알고 만난거거든요?",
          "speaker_id": "1",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003809.1.16",
          "form": "근데 이혼과정얘기 듣는데 무슨 막장드라마인거예요",
          "original_form": "근데 이혼과정얘기 듣는데 무슨 막장드라마인거예요",
          "speaker_id": "1",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003809.1.17",
          "form": "그래서 이혼소장까지 직접 읽어봤어요",
          "original_form": "그래서 이혼소장까지 직접 읽어봤어요",
          "speaker_id": "1",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003809.1.18",
          "form": "아 진짜요ㅠ",
          "original_form": "아 진짜요ㅠ",
          "speaker_id": "2",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003809.1.19",
          "form": "누가 연애하면서 이혼소장을 읽어볼까요",
          "original_form": "누가 연애하면서 이혼소장을 읽어볼까요",
          "speaker_id": "1",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003809.1.20",
          "form": "다 보여주셨구나",
          "original_form": "다 보여주셨구나",
          "speaker_id": "2",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003809.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003809.1.22",
          "form": "어이없죠",
          "original_form": "어이없죠",
          "speaker_id": "1",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003809.1.23",
          "form": "사유가 뭐래요?",
          "original_form": "사유가 뭐래요?",
          "speaker_id": "2",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003809.1.24",
          "form": "부인네 집안이 좀 문제가 있었어요",
          "original_form": "부인네 집안이 좀 문제가 있었어요",
          "speaker_id": "1",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003809.1.25",
          "form": "아 어떤요?",
          "original_form": "아 어떤요?",
          "speaker_id": "2",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003809.1.26",
          "form": "저는 그 전부인한테 아무 생각 없고",
          "original_form": "저는 그 전부인한테 아무 생각 없고",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003809.1.27",
          "form": "네~~~그렇겠죠",
          "original_form": "네~~~그렇겠죠",
          "speaker_id": "2",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003809.1.28",
          "form": "애초에 남자친구도 문제가 있으니 이혼한거라고 보거든요?",
          "original_form": "애초에 남자친구도 문제가 있으니 이혼한거라고 보거든요?",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003809.1.29",
          "form": "연애때부터 정말 안맞았대요",
          "original_form": "연애때부터 정말 안맞았대요",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003809.1.30",
          "form": "그러다 결혼한건데",
          "original_form": "그러다 결혼한건데",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003809.1.31",
          "form": "애초에 안맞았다니",
          "original_form": "애초에 안맞았다니",
          "speaker_id": "2",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003809.1.32",
          "form": "결혼하고나서 서로 미친듯이 싸우고 남처럼 지내다가",
          "original_form": "결혼하고나서 서로 미친듯이 싸우고 남처럼 지내다가",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003809.1.33",
          "form": "라면국물 하나 튀었다고",
          "original_form": "라면국물 하나 튀었다고",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003809.1.34",
          "form": "그걸로 이혼결심해서 시작했대요",
          "original_form": "그걸로 이혼결심해서 시작했대요",
          "speaker_id": "1",
          "time": "20210821 12:28"
        },
        {
          "id": "MDRW2100003809.1.35",
          "form": "헉!!!!!맙소사",
          "original_form": "헉!!!!!맙소사",
          "speaker_id": "2",
          "time": "20210821 12:28"
        },
        {
          "id": "MDRW2100003809.1.36",
          "form": "합의이혼 하기로했는데",
          "original_form": "합의이혼 하기로했는데",
          "speaker_id": "1",
          "time": "20210821 12:28"
        },
        {
          "id": "MDRW2100003809.1.37",
          "form": "부인쪽집안에서",
          "original_form": "부인쪽집안에서",
          "speaker_id": "1",
          "time": "20210821 12:28"
        },
        {
          "id": "MDRW2100003809.1.38",
          "form": "뭐",
          "original_form": "뭐",
          "speaker_id": "1",
          "time": "20210821 12:28"
        },
        {
          "id": "MDRW2100003809.1.39",
          "form": "문짝 뜯어서",
          "original_form": "문짝 뜯어서",
          "speaker_id": "1",
          "time": "20210821 12:28"
        },
        {
          "id": "MDRW2100003809.1.40",
          "form": "뭐 그런것도 있겄고",
          "original_form": "뭐 그런것도 있겄고",
          "speaker_id": "1",
          "time": "20210821 12:29"
        },
        {
          "id": "MDRW2100003809.1.41",
          "form": "애도 납치하고",
          "original_form": "애도 납치하고",
          "speaker_id": "1",
          "time": "20210821 12:29"
        },
        {
          "id": "MDRW2100003809.1.42",
          "form": "난리도 아니였더라구요",
          "original_form": "난리도 아니였더라구요",
          "speaker_id": "1",
          "time": "20210821 12:29"
        },
        {
          "id": "MDRW2100003809.1.43",
          "form": "소장에 다 써있는게 신기했어요",
          "original_form": "소장에 다 써있는게 신기했어요",
          "speaker_id": "1",
          "time": "20210821 12:29"
        },
        {
          "id": "MDRW2100003809.1.44",
          "form": "알고 만난건데 그정도일줄은...",
          "original_form": "알고 만난건데 그정도일줄은...",
          "speaker_id": "1",
