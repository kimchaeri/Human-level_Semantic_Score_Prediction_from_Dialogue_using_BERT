      "utterance": [
        {
          "id": "MDRW2100005304.1.1",
          "form": "응 어릴 때",
          "original_form": "응 어릴 때",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005304.1.2",
          "form": "뭐 키웠는데?",
          "original_form": "뭐 키웠는데?",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005304.1.3",
          "form": "병아리 ㅋㅋㅋ",
          "original_form": "병아리 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005304.1.4",
          "form": "병아리? 헉 ㅋㅋㅋ 초등학교 문방구 앞에서",
          "original_form": "병아리? 헉 ㅋㅋㅋ 초등학교 문방구 앞에서",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005304.1.5",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005304.1.6",
          "form": "가족이 거기서 사와서",
          "original_form": "가족이 거기서 사와서",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005304.1.7",
          "form": "병아리 근데 너무 약하지 않아",
          "original_form": "병아리 근데 너무 약하지 않아",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005304.1.8",
          "form": "그런데 우리는 닭까지 키웠어 ㅋㅋㅋ",
          "original_form": "그런데 우리는 닭까지 키웠어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005304.1.9",
          "form": "우와 대박이다",
          "original_form": "우와 대박이다",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005304.1.10",
          "form": "심지어 아파트였는데",
          "original_form": "심지어 아파트였는데",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005304.1.11",
          "form": "헉 아파트에서 닭을 ㅋㅋㅋ",
          "original_form": "헉 아파트에서 닭을 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005304.1.12",
          "form": "엄마가 닭장도 만들어주고",
          "original_form": "엄마가 닭장도 만들어주고",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005304.1.13",
          "form": "아 키 잘못 눌렀다 미안",
          "original_form": "아 키 잘못 눌렀다 미안",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005304.1.14",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005304.1.15",
          "form": "무튼 엄마가 지극정성으로 보살펴서",
          "original_form": "무튼 엄마가 지극정성으로 보살펴서",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005304.1.16",
          "form": "닭이 됐어 ㅋㅋ",
          "original_form": "닭이 됐어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005304.1.17",
          "form": "우와 보통 병든 병아리라 금방 죽는다 그러잖아",
          "original_form": "우와 보통 병든 병아리라 금방 죽는다 그러잖아",
