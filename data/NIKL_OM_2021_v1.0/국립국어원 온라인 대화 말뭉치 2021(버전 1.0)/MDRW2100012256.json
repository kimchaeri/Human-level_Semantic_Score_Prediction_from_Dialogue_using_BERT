      "utterance": [
        {
          "id": "MDRW2100012256.1.1",
          "form": "요즘 재택근무 많아졌더라",
          "original_form": "요즘 재택근무 많아졌더라",
          "speaker_id": "1",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100012256.1.2",
          "form": "아 그니까 코로나 영향때문인 듯?",
          "original_form": "아 그니까 코로나 영향때문인 듯?",
          "speaker_id": "2",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100012256.1.3",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "1",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100012256.1.4",
          "form": "나는 요즘 녹음알바 해",
          "original_form": "나는 요즘 녹음알바 해",
          "speaker_id": "1",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100012256.1.5",
          "form": "우와 그건 모야?",
          "original_form": "우와 그건 모야?",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012256.1.6",
          "form": "이것도 재택근무인데 대사를 녹음해서 보내면 돼",
          "original_form": "이것도 재택근무인데 대사를 녹음해서 보내면 돼",
          "speaker_id": "1",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012256.1.7",
          "form": "자세하게 알려줄 수 있어?",
          "original_form": "자세하게 알려줄 수 있어?",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012256.1.8",
          "form": "어떤게 궁금한데?",
          "original_form": "어떤게 궁금한데?",
          "speaker_id": "1",
          "time": "20210824 20:46"
        },
        {
          "id": "MDRW2100012256.1.9",
          "form": "무슨 대사?",
          "original_form": "무슨 대사?",
          "speaker_id": "2",
          "time": "20210824 20:46"
        },
        {
          "id": "MDRW2100012256.1.10",
          "form": "주제를 정해주면 그에 맞는 대사를 지어서 녹음하면 돼",
          "original_form": "주제를 정해주면 그에 맞는 대사를 지어서 녹음하면 돼",
          "speaker_id": "1",
          "time": "20210824 20:47"
        },
        {
          "id": "MDRW2100012256.1.11",
          "form": "급여 얼마야?",
          "original_form": "급여 얼마야?",
          "speaker_id": "2",
          "time": "20210824 20:47"
        },
        {
          "id": "MDRW2100012256.1.12",
          "form": "5분에 1500원이야!",
          "original_form": "5분에 1500원이야!",
          "speaker_id": "1",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012256.1.13",
          "form": "우와 얼마나 벌었어?",
          "original_form": "우와 얼마나 벌었어?",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012256.1.14",
          "form": "이번주에는 5만원정도 벌었어",
          "original_form": "이번주에는 5만원정도 벌었어",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012256.1.15",
          "form": "헐 짱 많이 벌었다!",
          "original_form": "헐 짱 많이 벌었다!",
          "speaker_id": "2",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012256.1.16",
          "form": "너는 재택알바 안해?",
          "original_form": "너는 재택알바 안해?",
          "speaker_id": "1",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100012256.1.17",
          "form": "재택알바?나는 안하는뎅",
          "original_form": "재택알바?나는 안하는뎅",
          "speaker_id": "2",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100012256.1.18",
          "form": "요즘 타이핑 알바도 많더라",
          "original_form": "요즘 타이핑 알바도 많더라",
          "speaker_id": "1",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100012256.1.19",
          "form": "타이핑 알바가 모야",
          "original_form": "타이핑 알바가 모야",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012256.1.20",
          "form": "그것도 재택알바인데 문서에 있는 문장을 그대로 따라서 타이핑하면 돼",
          "original_form": "그것도 재택알바인데 문서에 있는 문장을 그대로 따라서 타이핑하면 돼",
          "speaker_id": "1",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012256.1.21",
          "form": "넌 타이핑 알바 해봤어?",
          "original_form": "넌 타이핑 알바 해봤어?",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012256.1.22",
          "form": "나는 지원은 했는데 연락은 안오더라ㅠㅠ",
          "original_form": "나는 지원은 했는데 연락은 안오더라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100012256.1.23",
          "form": "헐 요새 알바자리 빡세긴 하지",
          "original_form": "헐 요새 알바자리 빡세긴 하지",
          "speaker_id": "2",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100012256.1.24",
          "form": "맞아 경쟁률이 어마어마해",
          "original_form": "맞아 경쟁률이 어마어마해",
          "speaker_id": "1",
          "time": "20210824 20:54"
        }
      ]
