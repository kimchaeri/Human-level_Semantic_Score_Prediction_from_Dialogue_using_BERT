        {
          "id": "MDRW2100017765.1.1",
          "form": "마라탕좋아하세요",
          "original_form": "마라탕좋아하세요",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017765.1.2",
          "form": "네 마라탕 엄청좋아해서 마라탕식재료 오늘도 사왔어요",
          "original_form": "네 마라탕 엄청좋아해서 마라탕식재료 오늘도 사왔어요",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017765.1.3",
          "form": "아 집에서 만들어드세요",
          "original_form": "아 집에서 만들어드세요",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017765.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017765.1.5",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017765.1.6",
          "form": "요즘 홈카페도 유행이라 일리커피머신기도 샀어요",
          "original_form": "요즘 홈카페도 유행이라 일리커피머신기도 샀어요",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017765.1.7",
          "form": "일리머신 좋나요?",
          "original_form": "일리머신 좋나요?",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017765.1.8",
          "form": "네 커피도 맛있어요",
          "original_form": "네 커피도 맛있어요",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017765.1.9",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017765.1.10",
          "form": "혹시 커피머신기 사시려면 일리 추천드려요",
          "original_form": "혹시 커피머신기 사시려면 일리 추천드려요",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017765.1.11",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017765.1.12",
          "form": "가을옷 사셨어요?",
          "original_form": "가을옷 사셨어요?",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017765.1.13",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017765.1.14",
          "form": "전 사모으는 중이에요",
          "original_form": "전 사모으는 중이에요",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017765.1.15",
          "form": "ㅋㅋ 어떤거사셨어요",
          "original_form": "ㅋㅋ 어떤거사셨어요",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017765.1.16",
          "form": "가디건위주요",
          "original_form": "가디건위주요",
