      "utterance": [
        {
          "id": "MDRW2100047403.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100047403.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047403.1.3",
          "form": "좋아하는 게임 있으신가요",
          "original_form": "좋아하는 게임 있으신가요",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047403.1.4",
          "form": "사실 저는 게임 안좋아해서 잘 몰라요 ㅜ",
          "original_form": "사실 저는 게임 안좋아해서 잘 몰라요 ㅜ",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100047403.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100047403.1.6",
          "form": "전 가끔요 ㅎ",
          "original_form": "전 가끔요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100047403.1.7",
          "form": "해 본 게임 없나요?",
          "original_form": "해 본 게임 없나요?",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100047403.1.8",
          "form": "저는 테일즈런너 쿠키런 요런거 해봤어요!",
          "original_form": "저는 테일즈런너 쿠키런 요런거 해봤어요!",
          "speaker_id": "1",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100047403.1.9",
          "form": "근데 게임하고 적성에 안맞더라구요 ㅋㅋ",
          "original_form": "근데 게임하고 적성에 안맞더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100047403.1.10",
          "form": "ㅋㅋㅋㅋㅋ 아 게임 취향이 어니신가요??!",
          "original_form": "ㅋㅋㅋㅋㅋ 아 게임 취향이 어니신가요??!",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100047403.1.11",
          "form": "네ㅜㅜ계속 져서 그런거같아요",
          "original_form": "네ㅜㅜ계속 져서 그런거같아요",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100047403.1.12",
          "form": "아 인정요 ㅠㅠ",
          "original_form": "아 인정요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100047403.1.13",
          "form": "계속지면 하기 싫어요",
          "original_form": "계속지면 하기 싫어요",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100047403.1.14",
          "form": "맞아요 그래서 포기한게임이 참 많아요",
          "original_form": "맞아요 그래서 포기한게임이 참 많아요",
          "speaker_id": "1",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100047403.1.15",
          "form": "ㅋㅋㅋㅋㅋ저도요 ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ저도요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100047403.1.16",
          "form": "맨날 져서 슬퍼요",
          "original_form": "맨날 져서 슬퍼요",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100047403.1.17",
          "form": "맞아요 ㅋㅋ 그래서 더더욱 새로운거 시도도 안해본다는 ㅜ",
          "original_form": "맞아요 ㅋㅋ 그래서 더더욱 새로운거 시도도 안해본다는 ㅜ",
          "speaker_id": "1",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100047403.1.18",
          "form": "전 하고 흥미를",
          "original_form": "전 하고 흥미를",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100047403.1.19",
          "form": "잂어버리쥬...",
          "original_form": "잂어버리쥬...",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100047403.1.20",
          "form": "ㅋㅋㅋ 저두요",
          "original_form": "ㅋㅋㅋ 저두요",
          "speaker_id": "1",
          "time": "20210826 19:42"
        }
      ]
