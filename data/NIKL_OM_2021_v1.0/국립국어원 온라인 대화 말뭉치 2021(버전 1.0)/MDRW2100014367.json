      "utterance": [
        {
          "id": "MDRW2100014367.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014367.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014367.1.3",
          "form": "혹시 요새 다이어트 하시나요?",
          "original_form": "혹시 요새 다이어트 하시나요?",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014367.1.4",
          "form": "아니요. 하지만 해야겠다고 생각하고 있어요.",
          "original_form": "아니요. 하지만 해야겠다고 생각하고 있어요.",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014367.1.5",
          "form": "다이어트라기보다는 건강을 위해서",
          "original_form": "다이어트라기보다는 건강을 위해서",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014367.1.6",
          "form": "운동을 해야할 필요성을 느끼고 있어요",
          "original_form": "운동을 해야할 필요성을 느끼고 있어요",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014367.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014367.1.8",
          "form": "아하! 전 다이어트 말로는 한다고하는데 그 다짐이 일주일도 안가더라구요ㅠ",
          "original_form": "아하! 전 다이어트 말로는 한다고하는데 그 다짐이 일주일도 안가더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014367.1.9",
          "form": "맞아요. 저도 운동해야겠다는 다짐이 작심삼일이에요ㅎㅎ",
          "original_form": "맞아요. 저도 운동해야겠다는 다짐이 작심삼일이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014367.1.10",
          "form": "그죠 ㅠ",
          "original_form": "그죠 ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014367.1.11",
          "form": "혹시 백신 맞으셨어요??",
          "original_form": "혹시 백신 맞으셨어요??",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014367.1.12",
          "form": "아니요. 9월에 맞을 예정이예요.",
          "original_form": "아니요. 9월에 맞을 예정이예요.",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014367.1.13",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014367.1.14",
          "form": "저는 잔여 백신을 신청하지않고 10부제에 맞춰서 신청했어요.",
          "original_form": "저는 잔여 백신을 신청하지않고 10부제에 맞춰서 신청했어요.",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014367.1.15",
          "form": "아 그렇군여!",
          "original_form": "아 그렇군여!",
          "speaker_id": "1",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014367.1.16",
          "form": "전 2차까지 맞았어요 ㅎㅎ",
          "original_form": "전 2차까지 맞았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014367.1.17",
          "form": "우와 어떻게 이렇게 일찍 다 맞으셨어요?",
          "original_form": "우와 어떻게 이렇게 일찍 다 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014367.1.18",
          "form": "부작용은 없으셨나요?",
          "original_form": "부작용은 없으셨나요?",
          "speaker_id": "2",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014367.1.19",
          "form": "저는 운좋게 잔여백신 잡아서 화이자맞고왔어요 ㅎㅎ",
          "original_form": "저는 운좋게 잔여백신 잡아서 화이자맞고왔어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014367.1.20",
          "form": "다행이 아무부작용도없이 수월하게지나갔어요~",
          "original_form": "다행이 아무부작용도없이 수월하게지나갔어요~",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014367.1.21",
          "form": "우와 대단하시네요",
          "original_form": "우와 대단하시네요",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014367.1.22",
          "form": "부작용이 없었다니 다행이에요!!!",
          "original_form": "부작용이 없었다니 다행이에요!!!",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014367.1.23",
          "form": "정말 사바사라 그냥 운이 참 좋았죠 ㅎㅎ",
          "original_form": "정말 사바사라 그냥 운이 참 좋았죠 ㅎㅎ",
