      "utterance": [
        {
          "id": "MDRW2100008651.1.1",
          "form": "고양이 어때?",
          "original_form": "고양이 어때?",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008651.1.2",
          "form": "너나중에동물키울거임?",
          "original_form": "너나중에동물키울거임?",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008651.1.3",
          "form": "아고양이좋음",
          "original_form": "아고양이좋음",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008651.1.4",
          "form": "새벽에깨우는것만아니면좋음",
          "original_form": "새벽에깨우는것만아니면좋음",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008651.1.5",
          "form": "앜ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008651.1.6",
          "form": "새벽에 깨워?",
          "original_form": "새벽에 깨워?",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008651.1.7",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008651.1.8",
          "form": "뭐야 그 한숨ㅋㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭐야 그 한숨ㅋㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008651.1.9",
          "form": "귀엽긴해",
          "original_form": "귀엽긴해",
          "speaker_id": "2",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008651.1.10",
          "form": "귀엽기만해",
          "original_form": "귀엽기만해",
          "speaker_id": "2",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008651.1.11",
          "form": "지필요한거있을때만와",
          "original_form": "지필요한거있을때만와",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008651.1.12",
          "form": "평소에는 이름불러도 들은척도 안함",
          "original_form": "평소에는 이름불러도 들은척도 안함",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008651.1.13",
          "form": "아닠ㅌㅋㅋㅋㅋㅋ 귀엽네",
          "original_form": "아닠ㅌㅋㅋㅋㅋㅋ 귀엽네",
          "speaker_id": "1",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008651.1.14",
          "form": "너도한번당해봐야됨",
          "original_form": "너도한번당해봐야됨",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008651.1.15",
          "form": "원래고양이는그루밍이라고",
          "original_form": "원래고양이는그루밍이라고",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008651.1.16",
          "form": "자기털정리하는걸하거든",
          "original_form": "자기털정리하는걸하거든",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100008651.1.17",
          "form": "아 그거 알아",
          "original_form": "아 그거 알아",
          "speaker_id": "1",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100008651.1.18",
          "form": "근데우리집애는그거귀찮아해서",
          "original_form": "근데우리집애는그거귀찮아해서",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100008651.1.19",
          "form": "우리가해줌",
          "original_form": "우리가해줌",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100008651.1.20",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100008651.1.21",
          "form": "오 색다른걸",
          "original_form": "오 색다른걸",
          "speaker_id": "1",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100008651.1.22",
          "form": "그러면엄청좋아해",
          "original_form": "그러면엄청좋아해",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100008651.1.23",
          "form": "꼭 자기 잘 때 해달라고 하는데",
          "original_form": "꼭 자기 잘 때 해달라고 하는데",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100008651.1.24",
          "form": "해주다가 팔 아파서 그만하면 눈 떠서",
          "original_form": "해주다가 팔 아파서 그만하면 눈 떠서",
          "speaker_id": "2",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100008651.1.25",
          "form": "계속하라고함",
          "original_form": "계속하라고함",
          "speaker_id": "2",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100008651.1.26",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "2",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100008651.1.27",
          "form": "ㅋㅋㅋㅋㅋㅌㅋㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅋㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100008651.1.28",
          "form": "털고르기 전문가되겠넼ㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "털고르기 전문가되겠넼ㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100008651.1.29",
          "form": "아 귀엽겠다",
          "original_form": "아 귀엽겠다",
          "speaker_id": "1",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100008651.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ그니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그니까",
          "speaker_id": "2",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100008651.1.31",
          "form": "너 도마뱀같은거 좋아하냐",
          "original_form": "너 도마뱀같은거 좋아하냐",
          "speaker_id": "2",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008651.1.32",
          "form": "얍얍 좋아함",
          "original_form": "얍얍 좋아함",
          "speaker_id": "1",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008651.1.33",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008651.1.34",
          "form": "오ㅑ",
          "original_form": "오ㅑ",
          "speaker_id": "1",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008651.1.35",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "1",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008651.1.36",
          "form": "그러면뱀같은것도좋아함?",
          "original_form": "그러면뱀같은것도좋아함?",
          "speaker_id": "2",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008651.1.37",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008651.1.38",
          "form": "나 뱀띠라서 뱀 좋아함",
          "original_form": "나 뱀띠라서 뱀 좋아함",
          "speaker_id": "1",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008651.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ논리적이네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ논리적이네",
          "speaker_id": "2",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008651.1.40",
          "form": "나중에독립하면키울거임?",
          "original_form": "나중에독립하면키울거임?",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008651.1.41",
          "form": "오 ㄴㄴ",
          "original_form": "오 ㄴㄴ",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008651.1.42",
          "form": "연이 닿으면 키우고 아님 말려고",
          "original_form": "연이 닿으면 키우고 아님 말려고",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008651.1.43",
          "form": "뱀이런거 별로 손 안갈줄 알았는데",
          "original_form": "뱀이런거 별로 손 안갈줄 알았는데",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008651.1.44",
          "form": "은근신경쓸게많긴하더라",
          "original_form": "은근신경쓸게많긴하더라",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008651.1.45",
          "form": "뱀 키워봤어???",
          "original_form": "뱀 키워봤어???",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008651.1.46",
          "form": "ㄴㄴㄴㄴ그건아니고",
          "original_form": "ㄴㄴㄴㄴ그건아니고",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008651.1.47",
          "form": "그냥이런건어떻게키우나",
          "original_form": "그냥이런건어떻게키우나",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008651.1.48",
          "form": "찾아보다가",
          "original_form": "찾아보다가",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008651.1.49",
          "form": "알게됨",
          "original_form": "알게됨",
          "speaker_id": "2",
          "time": "20210822 17:20"
        }
      ]
