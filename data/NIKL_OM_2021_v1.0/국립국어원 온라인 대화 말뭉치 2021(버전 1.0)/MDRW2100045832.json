      "utterance": [
        {
          "id": "MDRW2100045832.1.1",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045832.1.2",
          "form": "네...좋아합니다 ㅋㅋ돈이없어서그럿지",
          "original_form": "네...좋아합니다 ㅋㅋ돈이없어서그럿지",
          "speaker_id": "1",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045832.1.3",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045832.1.4",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045832.1.5",
          "form": "저도 좋아해요ㅋㅋ 돈이 문제지만요",
          "original_form": "저도 좋아해요ㅋㅋ 돈이 문제지만요",
          "speaker_id": "2",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045832.1.6",
          "form": "그래도 가끔 생필품 싸게 사면 기분 좋더라고요",
          "original_form": "그래도 가끔 생필품 싸게 사면 기분 좋더라고요",
          "speaker_id": "2",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045832.1.7",
          "form": "네 ㅋㅋ 저는 쟁임병이 있어서",
          "original_form": "네 ㅋㅋ 저는 쟁임병이 있어서",
          "speaker_id": "1",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100045832.1.8",
          "form": "대량구매로 싸게사기 좋아해요",
          "original_form": "대량구매로 싸게사기 좋아해요",
          "speaker_id": "1",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100045832.1.9",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100045832.1.10",
          "form": "시리얼도 쌀 때 쟁여놔요",
          "original_form": "시리얼도 쌀 때 쟁여놔요",
          "speaker_id": "2",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100045832.1.11",
          "form": "시리얼 어떤거좋아하세요?",
          "original_form": "시리얼 어떤거좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100045832.1.12",
          "form": "포스트 그래놀라요!",
          "original_form": "포스트 그래놀라요!",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045832.1.13",
          "form": "저도 그거좋아해요 ㅎㅎ",
          "original_form": "저도 그거좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045832.1.14",
          "form": "캘로그 그래놀라엔 건포도 들어있어서",
          "original_form": "캘로그 그래놀라엔 건포도 들어있어서",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045832.1.15",
          "form": "짜증나요",
          "original_form": "짜증나요",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045832.1.16",
          "form": "아 헐 저도 건포도 너무 싫어해요",
          "original_form": "아 헐 저도 건포도 너무 싫어해요",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045832.1.17",
          "form": "기억해둬야겠어요ㅠㅠ",
          "original_form": "기억해둬야겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045832.1.18",
          "form": "씹으면 소름끼쳐요 ㅋㅋ",
          "original_form": "씹으면 소름끼쳐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045832.1.19",
          "form": "포도는 맛있는데 건포도는 왜그러는지 ㅎㅎ",
          "original_form": "포도는 맛있는데 건포도는 왜그러는지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045832.1.20",
          "form": "맞아요ㅠㅠㅜㅠ",
          "original_form": "맞아요ㅠㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045832.1.21",
          "form": "누가 그걸 말려서ㅜㅜㅜ",
          "original_form": "누가 그걸 말려서ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045832.1.22",
          "form": "그러니까요ㅎㅎ 옷같은건 주로 어디서사세요?",
          "original_form": "그러니까요ㅎㅎ 옷같은건 주로 어디서사세요?",
          "speaker_id": "1",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100045832.1.23",
          "form": "오프라인으로 봤다가 온라인으로 사는 편이에요!",
          "original_form": "오프라인으로 봤다가 온라인으로 사는 편이에요!",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045832.1.24",
          "form": "케네스레이디 계열사도 일년에 두 번씩 패밀리세일을 하더라고요",
          "original_form": "케네스레이디 계열사도 일년에 두 번씩 패밀리세일을 하더라고요",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045832.1.25",
          "form": "그럴 때 건지면 개꿀이에요",
          "original_form": "그럴 때 건지면 개꿀이에요",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045832.1.26",
          "form": "맞아요 무조건 파격세일할때....ㅋㅋ",
          "original_form": "맞아요 무조건 파격세일할때....ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:58"
        }
      ]
    }
