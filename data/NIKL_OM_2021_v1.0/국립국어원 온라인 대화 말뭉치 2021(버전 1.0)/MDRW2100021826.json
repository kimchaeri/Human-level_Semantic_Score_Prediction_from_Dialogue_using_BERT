          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100021826.1.56",
          "form": "한두개가 아니잖아요",
          "original_form": "한두개가 아니잖아요",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
