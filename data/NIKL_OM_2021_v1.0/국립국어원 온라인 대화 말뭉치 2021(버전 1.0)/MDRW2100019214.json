      "utterance": [
        {
          "id": "MDRW2100019214.1.1",
          "form": "학생인가요?",
          "original_form": "학생인가요?",
          "speaker_id": "1",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100019214.1.2",
          "form": "저는 직장인이에요",
          "original_form": "저는 직장인이에요",
          "speaker_id": "2",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019214.1.3",
          "form": "어떤 직무에서 일하세요?",
          "original_form": "어떤 직무에서 일하세요?",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019214.1.4",
          "form": "전 서비스요",
          "original_form": "전 서비스요",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019214.1.5",
          "form": "고객 응대 힘들겠네요",
          "original_form": "고객 응대 힘들겠네요",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019214.1.6",
          "form": "좋은 사람이 훨씬 많아용",
          "original_form": "좋은 사람이 훨씬 많아용",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019214.1.7",
          "form": "아하 직무에 만족하시나봐요",
          "original_form": "아하 직무에 만족하시나봐요",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019214.1.8",
          "form": "서비스업은 사람 상대가 힘들잖아요",
          "original_form": "서비스업은 사람 상대가 힘들잖아요",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019214.1.9",
          "form": "힘들 때도 있는데 즐거워요ㅎㅎ",
          "original_form": "힘들 때도 있는데 즐거워요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100019214.1.10",
          "form": "안좋은 일있거나 별로인 손님은 같이 씹고 털어버려요",
          "original_form": "안좋은 일있거나 별로인 손님은 같이 씹고 털어버려요",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100019214.1.11",
          "form": "저는 서비스직 아르바이트로 했는데 힘들었거든요 대단하시네요",
          "original_form": "저는 서비스직 아르바이트로 했는데 힘들었거든요 대단하시네요",
          "speaker_id": "1",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100019214.1.12",
          "form": "직원들이랑 상사가 너무 좋은 일터라 ㅜㅜ",
          "original_form": "직원들이랑 상사가 너무 좋은 일터라 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100019214.1.13",
          "form": "오 좋은 곳인가봐요 부러워요!",
          "original_form": "오 좋은 곳인가봐요 부러워요!",
          "speaker_id": "1",
          "time": "20210830 07:09"
        },
        {
          "id": "MDRW2100019214.1.14",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "2",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100019214.1.15",
          "form": "직장은 괜찮으신가요",
          "original_form": "직장은 괜찮으신가요",
          "speaker_id": "2",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100019214.1.16",
          "form": "아 저는 이직 준비중ㅈ이에요",
          "original_form": "아 저는 이직 준비중ㅈ이에요",
          "speaker_id": "1",
          "time": "20210830 13:46"
        },
        {
          "id": "MDRW2100019214.1.17",
          "form": "요즘 갠찮나요 취업자리ㅜㅜ",
          "original_form": "요즘 갠찮나요 취업자리ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100019214.1.18",
          "form": "뭐 좋은 곳 찾기가 힘들네요",
          "original_form": "뭐 좋은 곳 찾기가 힘들네요",
          "speaker_id": "1",
          "time": "20210830 23:56"
        }
      ]
