      "utterance": [
        {
          "id": "MDRW2100005476.1.1",
          "form": "name2님은 학번이 어떻게 되세요?",
          "original_form": "&name2&님은 학번이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005476.1.2",
          "form": "저는 14학번요",
          "original_form": "저는 14학번요",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005476.1.3",
          "form": "헉 저도요ㅋㅋ신기하네요",
          "original_form": "헉 저도요ㅋㅋ신기하네요",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005476.1.4",
          "form": "14라면 오티같은 행사에서 오캬..같은 노래..하셧나요?",
          "original_form": "14라면 오티같은 행사에서 오캬..같은 노래..하셧나요?",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005476.1.5",
          "form": "헉 진짜요???ㅋㅋㅋㅋㅋ 저희 되게 신기하게 비슷한게 많네요!",
          "original_form": "헉 진짜요???ㅋㅋㅋㅋㅋ 저희 되게 신기하게 비슷한게 많네요!",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005476.1.6",
          "form": "저는 걸스데이 썸씽 했어서 아직 그노래 못듣겠어요ㅋㅋㅋ창피해서",
          "original_form": "저는 걸스데이 썸씽 했어서 아직 그노래 못듣겠어요ㅋㅋㅋ창피해서",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005476.1.7",
          "form": "저는 오캬말고",
          "original_form": "저는 오캬말고",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005476.1.8",
          "form": "레이디스 코드 했었던거같아요...ㅎㅎㅎ",
          "original_form": "레이디스 코드 했었던거같아요...ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005476.1.9",
          "form": "예뻐예뻐도 했었어유..",
          "original_form": "예뻐예뻐도 했었어유..",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005476.1.10",
          "form": "진짜 그땐 그런거 왜그렇게 강압적으로 시켰나 의문ㅠㅠ",
          "original_form": "진짜 그땐 그런거 왜그렇게 강압적으로 시켰나 의문ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005476.1.11",
          "form": "헉 맞아요 저도 예뻐예뻐...",
          "original_form": "헉 맞아요 저도 예뻐예뻐...",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005476.1.12",
          "form": "그니까요ㅠㅠ 1학년때는 그런거 있으면",
          "original_form": "그니까요ㅠㅠ 1학년때는 그런거 있으면",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005476.1.13",
          "form": "다 해야하는 줄 알고 다 하고",
          "original_form": "다 해야하는 줄 알고 다 하고",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005476.1.14",
          "form": "학과에서 과비 내라는것도 다 내고ㅠㅠ",
          "original_form": "학과에서 과비 내라는것도 다 내고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005476.1.15",
          "form": "저흰 막 강의실에 모아놓고 과잠 안살사람 지금 손들어~~이러고",
          "original_form": "저흰 막 강의실에 모아놓고 과잠 안살사람 지금 손들어~~이러고",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005476.1.16",
          "form": "회비 안내면 부모님한테 전화하고ㅡㅡ",
          "original_form": "회비 안내면 부모님한테 전화하고ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005476.1.17",
          "form": "결국 그 돈 빼먹어서 자기 카메라 샀더라고요",
          "original_form": "결국 그 돈 빼먹어서 자기 카메라 샀더라고요",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005476.1.18",
          "form": "가만안둬",
          "original_form": "가만안둬",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005476.1.19",
          "form": "헐ㅋㅋㅋㅋㅋㅋㅋ 부모님한테 전화까지요??",
          "original_form": "헐ㅋㅋㅋㅋㅋㅋㅋ 부모님한테 전화까지요??",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005476.1.20",
          "form": "저희는 안내면 일일이 갠톡하면서 눈치줬어요",
          "original_form": "저희는 안내면 일일이 갠톡하면서 눈치줬어요",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005476.1.21",
          "form": "그래놓고 나중에 과 학생회 간부들끼리 펜션 백만원짜리를 ㅡㅡ",
          "original_form": "그래놓고 나중에 과 학생회 간부들끼리 펜션 백만원짜리를 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005476.1.22",
          "form": "name2님 과는 과대 혼자 카메라 해먹은거에요?",
          "original_form": "&name2&님 과는 과대 혼자 카메라 해먹은거에요?",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005476.1.23",
          "form": "네 돈 안냈다고 완전 어이없죠",
          "original_form": "네 돈 안냈다고 완전 어이없죠",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005476.1.24",
          "form": "돌았네요 아주 지들만",
          "original_form": "돌았네요 아주 지들만",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005476.1.25",
          "form": "아뇨 뭐 자기들끼리 자주 모여서 뭐 먹고 이런 건 흔하고",
          "original_form": "아뇨 뭐 자기들끼리 자주 모여서 뭐 먹고 이런 건 흔하고",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005476.1.26",
          "form": "부모님한테 전화가는건 진짜로",
          "original_form": "부모님한테 전화가는건 진짜로",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005476.1.27",
          "form": "너무 선넘은 느낌이에요",
          "original_form": "너무 선넘은 느낌이에요",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005476.1.28",
          "form": "다들 서로 성인이면서ㅡ.ㅡ",
          "original_form": "다들 서로 성인이면서ㅡ.ㅡ",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005476.1.29",
          "form": "여자친구 뭐도 해줬다고 했는데 기억이 안 나네요",
          "original_form": "여자친구 뭐도 해줬다고 했는데 기억이 안 나네요",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005476.1.30",
          "form": "헐.... 그거 밝혀지고나서는 어떻게 물어냈나요?",
          "original_form": "헐.... 그거 밝혀지고나서는 어떻게 물어냈나요?",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005476.1.31",
          "form": "카메라에 여자친구 선물에",
          "original_form": "카메라에 여자친구 선물에",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005476.1.32",
          "form": "아주 선을 제대로 넘었제요",
          "original_form": "아주 선을 제대로 넘었제요",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005476.1.33",
          "form": "네요",
          "original_form": "네요",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005476.1.34",
          "form": "그니까요ㅡㅡ 교수님들도 묵인하시니 어쩔수없었죠",
