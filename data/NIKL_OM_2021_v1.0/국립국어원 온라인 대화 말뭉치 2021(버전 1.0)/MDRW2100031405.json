      "utterance": [
        {
          "id": "MDRW2100031405.1.1",
          "form": "아 진짜 열심",
          "original_form": "아 진짜 열심",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031405.1.2",
          "form": "진짜 착한 사람들이에요!",
          "original_form": "진짜 착한 사람들이에요!",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031405.1.3",
          "form": "공유하는거 쉬운 일이 아닌데요!!!",
          "original_form": "공유하는거 쉬운 일이 아닌데요!!!",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031405.1.4",
          "form": "그러니까요 ㅋㅋㅋ 진짜 대단한 분들",
          "original_form": "그러니까요 ㅋㅋㅋ 진짜 대단한 분들",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031405.1.5",
          "form": "저도 고마워서 치즈 한번 올린적 있어요",
          "original_form": "저도 고마워서 치즈 한번 올린적 있어요",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031405.1.6",
          "form": "싸길래",
          "original_form": "싸길래",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031405.1.7",
          "form": "ㅋㅋㅋㅋ 글 한번도 써본적 없었거든요",
          "original_form": "ㅋㅋㅋㅋ 글 한번도 써본적 없었거든요",
          "speaker_id": "1",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031405.1.8",
          "form": "ㅋㅋ근데 진짜 받기만하다보면 생기면 뭐라도 나누고싶고",
          "original_form": "ㅋㅋ근데 진짜 받기만하다보면 생기면 뭐라도 나누고싶고",
          "speaker_id": "2",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031405.1.9",
          "form": "뷰방 붙박이신가여?",
          "original_form": "뷰방 붙박이신가여?",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031405.1.10",
          "form": "붙박이라고 하기엔 오늘도 안들어갔는데",
          "original_form": "붙박이라고 하기엔 오늘도 안들어갔는데",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031405.1.11",
          "form": "평일에 더 지르고 싶어서",
          "original_form": "평일에 더 지르고 싶어서",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031405.1.12",
          "form": "평일에는 붙박이 같네요 ㅋㅋㅋㅋ",
          "original_form": "평일에는 붙박이 같네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031405.1.13",
          "form": "ㅋㅋ지르고싶을때 저도 꼭 가요",
          "original_form": "ㅋㅋ지르고싶을때 저도 꼭 가요",
          "speaker_id": "2",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031405.1.14",
          "form": "저 이번에 신세계도 홈플도 다... 놓쳐서 속상...",
          "original_form": "저 이번에 신세계도 홈플도 다... 놓쳐서 속상...",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031405.1.15",
          "form": "뭐 있었어요??",
          "original_form": "뭐 있었어요??",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031405.1.16",
          "form": "저 오늘 결혼식 다녀오고 정신없어서 안갔더니만",
          "original_form": "저 오늘 결혼식 다녀오고 정신없어서 안갔더니만",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031405.1.17",
          "form": "ㅋㅋㅋㅋ 역시 놓치면 안돼",
          "original_form": "ㅋㅋㅋㅋ 역시 놓치면 안돼",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031405.1.18",
          "form": "아뇨ㅋㅋ 이건 이번주에 있던 거예요 다 끝... 눈앞에서 다 놓쳤어요....",
          "original_form": "아뇨ㅋㅋ 이건 이번주에 있던 거예요 다 끝... 눈앞에서 다 놓쳤어요....",
          "speaker_id": "2",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100031405.1.19",
          "form": "요즘 열심히 안봤나봐요",
          "original_form": "요즘 열심히 안봤나봐요",
          "speaker_id": "1",
          "time": "20210904 21:26"
        },
        {
          "id": "MDRW2100031405.1.20",
          "form": "어쩌다가 놓쳤을까 ㅋㅋㅋ",
          "original_form": "어쩌다가 놓쳤을까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:26"
        },
        {
          "id": "MDRW2100031405.1.21",
          "form": "name2님과 제가 꼭 핫딜에 다 참여할수 있기를 기원",
          "original_form": "&name2&님과 제가 꼭 핫딜에 다 참여할수 있기를 기원",
          "speaker_id": "1",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100031405.1.22",
          "form": "진짜요ㅜㅜ 전 진짜 놓처서 넘나 속상하고 그\r\n 글은 아예 안보고 잇어욕ㅋㅋ",
          "original_form": "진짜요ㅜㅜ 전 진짜 놓처서 넘나 속상하고 그\r\n 글은 아예 안보고 잇어욕ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:29"
        },
        {
          "id": "MDRW2100031405.1.23",
          "form": "그정도라시니까 궁금하네요 ㅋㅋㅋ 저 찾아보고 올게오",
          "original_form": "그정도라시니까 궁금하네요 ㅋㅋㅋ 저 찾아보고 올게오",
          "speaker_id": "1",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100031405.1.24",
          "form": "홈플 50프로 신세계는 만원 공짜ㅋㅋ",
          "original_form": "홈플 50프로 신세계는 만원 공짜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:31"
        },
        {
          "id": "MDRW2100031405.1.25",
          "form": "앙",
          "original_form": "앙",
          "speaker_id": "1",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100031405.1.26",
          "form": "악",
          "original_form": "악",
          "speaker_id": "1",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100031405.1.27",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100031405.1.28",
          "form": "지금 봤어욬ㅋㅋ",
          "original_form": "지금 봤어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100031405.1.29",
          "form": "왜 저런건",
          "original_form": "왜 저런건",
          "speaker_id": "1",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100031405.1.30",
          "form": "못보는걸까요! 븉방 헛다녔다!",
          "original_form": "못보는걸까요! 븉방 헛다녔다!",
          "speaker_id": "1",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100031405.1.31",
          "form": "알기전에 행복했다고요!",
          "original_form": "알기전에 행복했다고요!",
          "speaker_id": "2",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100031405.1.32",
          "form": "아니 홈플은 한달동안 한다더니 갑자기 말바꿨어요!!!!",
          "original_form": "아니 홈플은 한달동안 한다더니 갑자기 말바꿨어요!!!!",
          "speaker_id": "2",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100031405.1.33",
          "form": "눈팅하는 분들이 많아서",
          "original_form": "눈팅하는 분들이 많아서",
          "speaker_id": "1",
          "time": "20210904 21:34"
        },
        {
          "id": "MDRW2100031405.1.34",
          "form": "참여자 엄청 많았을거 같아요",
          "original_form": "참여자 엄청 많았을거 같아요",
          "speaker_id": "1",
          "time": "20210904 21:34"
        },
        {
          "id": "MDRW2100031405.1.35",
          "form": "그 신세계 저는 사실 보고도 안했어요",
          "original_form": "그 신세계 저는 사실 보고도 안했어요",
          "speaker_id": "1",
          "time": "20210904 21:34"
        },
        {
          "id": "MDRW2100031405.1.36",
          "form": "귀찮기도 했고 너무 실수같아서 남일같지 않아서욬ㅋㅋㅋㅋ",
          "original_form": "귀찮기도 했고 너무 실수같아서 남일같지 않아서욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100031405.1.37",
          "form": "저는 아예 아침에 늦게 보고ㅋㅋ 홈플이나 해야지했는데",
          "original_form": "저는 아예 아침에 늦게 보고ㅋㅋ 홈플이나 해야지했는데",
          "speaker_id": "2",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100031405.1.38",
          "form": "홈플이 배신을!!!",
          "original_form": "홈플이 배신을!!!",
          "speaker_id": "2",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100031405.1.39",
          "form": "name2님과 저는 그분을 조금이나마 도왔다 생각하졐ㅋㅋㅋㅋ",
          "original_form": "&name2&님과 저는 그분을 조금이나마 도왔다 생각하졐ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100031405.1.40",
          "form": "근데 홈플은",
          "original_form": "근데 홈플은",
          "speaker_id": "1",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100031405.1.41",
          "form": "그죠ㅋㅋ",
          "original_form": "그죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100031405.1.42",
          "form": "ㅋㅋㅋㅋㅋ그러게여",
          "original_form": "ㅋㅋㅋㅋㅋ그러게여",
          "speaker_id": "1",
          "time": "20210904 21:36"
        },
        {
          "id": "MDRW2100031405.1.43",
          "form": "홈플 앱 다운받지않겠어요!",
          "original_form": "홈플 앱 다운받지않겠어요!",
          "speaker_id": "2",
