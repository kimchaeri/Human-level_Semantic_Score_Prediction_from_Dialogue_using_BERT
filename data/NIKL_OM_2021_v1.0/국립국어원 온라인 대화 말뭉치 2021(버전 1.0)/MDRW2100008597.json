      "utterance": [
        {
          "id": "MDRW2100008597.1.1",
          "form": "하죠!",
          "original_form": "하죠!",
          "speaker_id": "2",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008597.1.2",
          "form": "집안청소 해요!",
          "original_form": "집안청소 해요!",
          "speaker_id": "2",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008597.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008597.1.4",
          "form": "아하 청소담당이시구나",
          "original_form": "아하 청소담당이시구나",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008597.1.5",
          "form": "네 화장실 청소요",
          "original_form": "네 화장실 청소요",
          "speaker_id": "1",
          "time": "20210822 17:43"
        },
        {
          "id": "MDRW2100008597.1.6",
          "form": "아하 전 안방청소에요!",
          "original_form": "아하 전 안방청소에요!",
          "speaker_id": "2",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100008597.1.7",
          "form": "주기는오?",
          "original_form": "주기는오?",
          "speaker_id": "1",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008597.1.8",
          "form": "하루에 한번이요!",
          "original_form": "하루에 한번이요!",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008597.1.9",
          "form": "전 화장실 일주일에 1번",
          "original_form": "전 화장실 일주일에 1번",
          "speaker_id": "1",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100008597.1.10",
          "form": "일주일 1번이면 꽤 자주 하시네요!",
          "original_form": "일주일 1번이면 꽤 자주 하시네요!",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008597.1.11",
          "form": "저희집은 2주에 1번인지러",
          "original_form": "저희집은 2주에 1번인지러",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008597.1.12",
          "form": "곰팡이걱정은요",
          "original_form": "곰팡이걱정은요",
          "speaker_id": "1",
          "time": "20210822 19:46"
        },
        {
          "id": "MDRW2100008597.1.13",
          "form": "곰팡이 걱정 많죠 ㅠ",
          "original_form": "곰팡이 걱정 많죠 ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100008597.1.14",
          "form": "아직까진 없는데 그래도 불안해요 ㅠㅠ",
          "original_form": "아직까진 없는데 그래도 불안해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100008597.1.15",
          "form": "생기면 잘 안 ㅇ없어짐다고",
          "original_form": "생기면 잘 안 ㅇ없어짐다고",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100008597.1.16",
          "form": "맞아요 저번 집에 곰팡이 있었는데",
          "original_form": "맞아요 저번 집에 곰팡이 있었는데",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100008597.1.17",
          "form": "제거자체가 엄청힘들어요..",
          "original_form": "제거자체가 엄청힘들어요..",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100008597.1.18",
          "form": "또 생겨요",
          "original_form": "또 생겨요",
          "speaker_id": "1",
          "time": "20210822 22:59"
        }
      ]
