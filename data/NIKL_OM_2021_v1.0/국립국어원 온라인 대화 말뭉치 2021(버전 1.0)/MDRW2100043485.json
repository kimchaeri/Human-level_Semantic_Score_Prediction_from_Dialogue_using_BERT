      "utterance": [
        {
          "id": "MDRW2100043485.1.1",
          "form": "저녁 뭐드셨어용?",
          "original_form": "저녁 뭐드셨어용?",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043485.1.2",
          "form": "아직이시려나",
          "original_form": "아직이시려나",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043485.1.3",
          "form": "저 아직이요! ㅋㅋㅋㅋ name1님은요?",
          "original_form": "저 아직이요! ㅋㅋㅋㅋ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043485.1.4",
          "form": "저 이제 먹고 왔어요",
          "original_form": "저 이제 먹고 왔어요",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043485.1.5",
          "form": "name2님 뭐 드실예정??",
          "original_form": "&name2&님 뭐 드실예정??",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043485.1.6",
          "form": "고민중이에용 뭐드셨어요~?",
          "original_form": "고민중이에용 뭐드셨어요~?",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043485.1.7",
          "form": "저 치즈돈까스 먹고 왔어요",
          "original_form": "저 치즈돈까스 먹고 왔어요",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043485.1.8",
          "form": "생선까스도 함께 ㅎㅎ",
          "original_form": "생선까스도 함께 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043485.1.9",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043485.1.10",
          "form": "생선까스 좋아하세요?",
          "original_form": "생선까스 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043485.1.11",
          "form": "저 타르타르 먹는 맛에 먹는거 같아요 ㅋㅋㅋㅋ",
          "original_form": "저 타르타르 먹는 맛에 먹는거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043485.1.12",
          "form": "타르타르 좋아요!!",
          "original_form": "타르타르 좋아요!!",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043485.1.13",
          "form": "오옹 저는 급식 생선까스의 피해자예요.....",
          "original_form": "오옹 저는 급식 생선까스의 피해자예요.....",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043485.1.14",
          "form": "비린내나던 그게 안잊혀져서",
          "original_form": "비린내나던 그게 안잊혀져서",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043485.1.15",
          "form": "막상 입에 넣으면 맛있는거 아는데도",
          "original_form": "막상 입에 넣으면 맛있는거 아는데도",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043485.1.16",
          "form": "보기만하면 못먹겠어요ㅠ",
          "original_form": "보기만하면 못먹겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043485.1.17",
          "form": "ㅋㅋㅋ 생선까스 생각보다 맛있어요",
          "original_form": "ㅋㅋㅋ 생선까스 생각보다 맛있어요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043485.1.18",
          "form": "전 급식도 괜찮았었거든요",
          "original_form": "전 급식도 괜찮았었거든요",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043485.1.19",
          "form": "근데 비린거 안좋아하시면 안될듯",
          "original_form": "근데 비린거 안좋아하시면 안될듯",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043485.1.20",
          "form": "그니가요 먹으면 맛있는데!!!",
          "original_form": "그니가요 먹으면 맛있는데!!!",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043485.1.21",
          "form": "아니에요 맛이 그랬던건 아니라서 ㅋㅋㅋㅋ",
          "original_form": "아니에요 맛이 그랬던건 아니라서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043485.1.22",
          "form": "지금도 먹긴 먹어요",
          "original_form": "지금도 먹긴 먹어요",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043485.1.23",
          "form": "특히 뜨끈할때 먹는 생선까스는",
          "original_form": "특히 뜨끈할때 먹는 생선까스는",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043485.1.24",
          "form": "돈까스보다 더 맛있죠",
          "original_form": "돈까스보다 더 맛있죠",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043485.1.25",
          "form": "맞아요 뜨끈해야돼요!!",
          "original_form": "맞아요 뜨끈해야돼요!!",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043485.1.26",
          "form": "그러면 맛있어요!!",
          "original_form": "그러면 맛있어요!!",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043485.1.27",
          "form": "냉동된 치돈 생선까스였는데여",
          "original_form": "냉동된 치돈 생선까스였는데여",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043485.1.28",
          "form": "치즈돈까스 풀무원꺼 맛있어요",
          "original_form": "치즈돈까스 풀무원꺼 맛있어요",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043485.1.29",
          "form": "그냥 참고하시라고...ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 참고하시라고...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043485.1.30",
          "form": "오 꿀팁 감사합니다 ㅋㅋㅋ",
          "original_form": "오 꿀팁 감사합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043485.1.31",
          "form": "근데 냉동까스는 어떻게 조리해요?",
          "original_form": "근데 냉동까스는 어떻게 조리해요?",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043485.1.32",
          "form": "에어프라이어에 앞뒤로 한 십분 십오분씩 돌려주면 돼요",
          "original_form": "에어프라이어에 앞뒤로 한 십분 십오분씩 돌려주면 돼요",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043485.1.33",
          "form": "오븐있으시면 오븐도 좋구여!",
          "original_form": "오븐있으시면 오븐도 좋구여!",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043485.1.34",
          "form": "아! ㅠ 저희집엔 그런 굉장한 요리 도구 없어요...",
          "original_form": "아! ㅠ 저희집엔 그런 굉장한 요리 도구 없어요...",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043485.1.35",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043485.1.36",
