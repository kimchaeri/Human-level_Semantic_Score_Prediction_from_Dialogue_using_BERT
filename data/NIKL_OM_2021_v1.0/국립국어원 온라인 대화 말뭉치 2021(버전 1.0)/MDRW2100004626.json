      "utterance": [
        {
          "id": "MDRW2100004626.1.1",
          "form": "비오나요?",
          "original_form": "비오나요?",
          "speaker_id": "1",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004626.1.2",
          "form": "이젠 그치는 것 같아요",
          "original_form": "이젠 그치는 것 같아요",
          "speaker_id": "2",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100004626.1.3",
          "form": "여기도 슬슬 그치는거같아요",
          "original_form": "여기도 슬슬 그치는거같아요",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004626.1.4",
          "form": "많이 안내려 다행이에요",
          "original_form": "많이 안내려 다행이에요",
          "speaker_id": "2",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004626.1.5",
          "form": "담주내내 비던데요",
          "original_form": "담주내내 비던데요",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004626.1.6",
          "form": "늦은 장마시작같아요",
          "original_form": "늦은 장마시작같아요",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004626.1.7",
          "form": "일본은 천미리씩내렿다는데",
          "original_form": "일본은 천미리씩내렿다는데",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004626.1.8",
          "form": "그렇게까지 안오길 바래봐요",
          "original_form": "그렇게까지 안오길 바래봐요",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004626.1.9",
          "form": "너무 많이 내리네요.",
          "original_form": "너무 많이 내리네요.",
          "speaker_id": "2",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100004626.1.10",
          "form": "여기도 지금은 많이 오네요",
          "original_form": "여기도 지금은 많이 오네요",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100004626.1.11",
          "form": "비오면 시원할줄알았는데 습해서 자꾸 에어컨을 켜게되네요",
          "original_form": "비오면 시원할줄알았는데 습해서 자꾸 에어컨을 켜게되네요",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100004626.1.12",
          "form": "그쵸. 제습을 하거나 보일러 돌려야해요.",
          "original_form": "그쵸. 제습을 하거나 보일러 돌려야해요.",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004626.1.13",
          "form": "에어컨을 더켜게되는거같아요",
          "original_form": "에어컨을 더켜게되는거같아요",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100004626.1.14",
          "form": "그쵸. 아직까지는요.^^",
          "original_form": "그쵸. 아직까지는요.^^",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100004626.1.15",
          "form": "태풍도 같이 오려나봐요",
          "original_form": "태풍도 같이 오려나봐요",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100004626.1.16",
          "form": "태풍이 온다그러던데요",
          "original_form": "태풍이 온다그러던데요",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100004626.1.17",
          "form": "별일없이 지나가야할뎬데요",
          "original_form": "별일없이 지나가야할뎬데요",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100004626.1.18",
          "form": "일본 난리났다고해서 걱정되긴해요",
          "original_form": "일본 난리났다고해서 걱정되긴해요",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100004626.1.19",
          "form": "우리도 다음주 영향을 미칠 것 같다니 걱정이네요",
          "original_form": "우리도 다음주 영향을 미칠 것 같다니 걱정이네요",
          "speaker_id": "2",
          "time": "20210823 00:11"
        }
