        {
          "id": "MDRW2100045414.1.1",
          "form": "지금 저는 풍월량 봐요",
          "original_form": "지금 저는 풍월량 봐요",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045414.1.2",
          "form": "저도 한 번 보려구요",
          "original_form": "저도 한 번 보려구요",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
