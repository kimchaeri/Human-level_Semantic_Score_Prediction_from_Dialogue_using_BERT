      "utterance": [
        {
          "id": "MDRW2100032212.1.1",
          "form": "해외여행 가본 적 있어??",
          "original_form": "해외여행 가본 적 있어??",
          "speaker_id": "2",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100032212.1.2",
          "form": "몇 번 있어",
          "original_form": "몇 번 있어",
          "speaker_id": "1",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100032212.1.3",
          "form": "아일랜드가 젤 좋았어",
          "original_form": "아일랜드가 젤 좋았어",
          "speaker_id": "1",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100032212.1.4",
          "form": "헐!!! 나 그렇게 멀리 가본 적은 없어",
          "original_form": "헐!!! 나 그렇게 멀리 가본 적은 없어",
          "speaker_id": "2",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100032212.1.5",
          "form": "아일랜드 뭐가 좋았어??",
          "original_form": "아일랜드 뭐가 좋았어??",
          "speaker_id": "2",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100032212.1.6",
          "form": "스켈리그 마이클이라고",
          "original_form": "스켈리그 마이클이라고",
          "speaker_id": "1",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032212.1.7",
          "form": "그 스타워즈 7 8에 나온 섬 갔는데",
          "original_form": "그 스타워즈 7 8에 나온 섬 갔는데",
          "speaker_id": "1",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032212.1.8",
          "form": "주위 풍경도 넘 좋고",
          "original_form": "주위 풍경도 넘 좋고",
          "speaker_id": "1",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032212.1.9",
          "form": "유명 관광지가 아니라서 물가도 싸고 사람들도 친절하고 좋아",
          "original_form": "유명 관광지가 아니라서 물가도 싸고 사람들도 친절하고 좋아",
          "speaker_id": "1",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032212.1.10",
          "form": "헐... 꼭 가보고 싶다",
          "original_form": "헐... 꼭 가보고 싶다",
          "speaker_id": "2",
          "time": "20210905 14:10"
        },
        {
          "id": "MDRW2100032212.1.11",
          "form": "나중에 유럽여행 가면 꼭 가보기",
          "original_form": "나중에 유럽여행 가면 꼭 가보기",
          "speaker_id": "1",
          "time": "20210905 14:10"
        },
        {
          "id": "MDRW2100032212.1.12",
          "form": "스켈리그 마이클 내 네이버 검색 기록에 박제 완. ㅋㅋ",
          "original_form": "스켈리그 마이클 내 네이버 검색 기록에 박제 완. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032212.1.13",
          "form": "보니까 영국사람들이. 주말여행 정도로 가는 곳 같더라고",
          "original_form": "보니까 영국사람들이. 주말여행 정도로 가는 곳 같더라고",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032212.1.14",
          "form": "현지 사람들이 여행 가는 곳이 진짜 좋은 곳이라는 말이 있지... 방금 내가 지어냄",
          "original_form": "현지 사람들이 여행 가는 곳이 진짜 좋은 곳이라는 말이 있지... 방금 내가 지어냄",
          "speaker_id": "2",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032212.1.15",
          "form": "음 나도 들어본 말 같군 ㅋㅋㅋ",
          "original_form": "음 나도 들어본 말 같군 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032212.1.16",
          "form": "아 스켈리그 마이클",
          "original_form": "아 스켈리그 마이클",
          "speaker_id": "1",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032212.1.17",
          "form": "투어 예약은 빨리 해야 해",
          "original_form": "투어 예약은 빨리 해야 해",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032212.1.18",
          "form": "안그럼 나처럼 고생한다..",
          "original_form": "안그럼 나처럼 고생한다..",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032212.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 접수완.",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 접수완.",
          "speaker_id": "2",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032212.1.20",
          "form": "인두는 어디 가봤어??",
          "original_form": "인두는 어디 가봤어??",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032212.1.21",
          "form": "나는 괌이랑 사이판밖에 못 가봤어! 근데 거의 다 한국 사람이더라 ㅋㅋㅋ",
          "original_form": "나는 괌이랑 사이판밖에 못 가봤어! 근데 거의 다 한국 사람이더라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032212.1.22",
          "form": "그렇지 ㅋㅋㅋ",
          "original_form": "그렇지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032212.1.23",
          "form": "외국에 한국인들 진짜 많아",
          "original_form": "외국에 한국인들 진짜 많아",
          "speaker_id": "1",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032212.1.24",
          "form": "맞아... 한국인이 진짜 많음 그리고 괌이랑 사이판 둘 다 섬이라서 난 좋았음!! 어딜 가도 바다였어 ㅋㅋ",
          "original_form": "맞아... 한국인이 진짜 많음 그리고 괌이랑 사이판 둘 다 섬이라서 난 좋았음!! 어딜 가도 바다였어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:17"
        }
      ]
