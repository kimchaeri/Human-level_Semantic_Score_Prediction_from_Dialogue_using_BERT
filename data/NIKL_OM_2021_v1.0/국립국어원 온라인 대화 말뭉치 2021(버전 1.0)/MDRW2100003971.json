      "utterance": [
        {
          "id": "MDRW2100003971.1.1",
          "form": "여기도 날씨 갑자기 맑아졌네",
          "original_form": "여기도 날씨 갑자기 맑아졌네",
          "speaker_id": "2",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003971.1.2",
          "form": "비 그치고 햇빛 났는데 무지개 떳나",
          "original_form": "비 그치고 햇빛 났는데 무지개 떳나",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003971.1.3",
          "form": "다른 지역은 폭우 온다네",
          "original_form": "다른 지역은 폭우 온다네",
          "speaker_id": "2",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003971.1.4",
          "form": "청계천 수위 높아졌더라",
          "original_form": "청계천 수위 높아졌더라",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003971.1.5",
          "form": "피해 없어야할텐데",
          "original_form": "피해 없어야할텐데",
          "speaker_id": "2",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003971.1.6",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003971.1.7",
          "form": "근데 비 이제 더 안오려나",
          "original_form": "근데 비 이제 더 안오려나",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003971.1.8",
          "form": "장마라고 했으니까 또 오지 않을까",
          "original_form": "장마라고 했으니까 또 오지 않을까",
          "speaker_id": "1",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003971.1.9",
          "form": "가을 장마인가",
          "original_form": "가을 장마인가",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003971.1.10",
          "form": "가을비 촉촉",
          "original_form": "가을비 촉촉",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003971.1.11",
          "form": "햇빛이 쨍쨍 쪄야  곡식이 잘 영근다던데",
          "original_form": "햇빛이 쨍쨍 쪄야  곡식이 잘 영근다던데",
          "speaker_id": "2",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003971.1.12",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003971.1.13",
          "form": "가끔 비도 와야하지만",
          "original_form": "가끔 비도 와야하지만",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003971.1.14",
          "form": "장마는 위험해",
          "original_form": "장마는 위험해",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003971.1.15",
          "form": "곡식 가격 오르는 건 아니겠지",
          "original_form": "곡식 가격 오르는 건 아니겠지",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003971.1.16",
          "form": "가을인가봐",
          "original_form": "가을인가봐",
