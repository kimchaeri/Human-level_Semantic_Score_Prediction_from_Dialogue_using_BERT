        {
          "id": "MDRW2100002598.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002598.1.2",
          "form": "아니요 운동 안좋아해요 운동좋아하세요?",
          "original_form": "아니요 운동 안좋아해요 운동좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002598.1.3",
          "form": "홈트해여",
          "original_form": "홈트해여",
          "speaker_id": "1",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002598.1.4",
          "form": "홈트  좋다더라구요",
          "original_form": "홈트  좋다더라구요",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002598.1.5",
          "form": "요즘 코어운동 필수라던데 어떤거 위주로 하시나요",
          "original_form": "요즘 코어운동 필수라던데 어떤거 위주로 하시나요",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002598.1.6",
          "form": "플랭크요",
          "original_form": "플랭크요",
          "speaker_id": "1",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002598.1.7",
          "form": "그거 힘들잖아요",
          "original_form": "그거 힘들잖아요",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002598.1.8",
          "form": "저 그거 한개도 못해요ㅋㅋ 너무 힘들어서",
          "original_form": "저 그거 한개도 못해요ㅋㅋ 너무 힘들어서",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002598.1.9",
          "form": "익숙해지면 나아요",
          "original_form": "익숙해지면 나아요",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002598.1.10",
          "form": "홈트할 때 참고하는 곳이 있나요?",
          "original_form": "홈트할 때 참고하는 곳이 있나요?",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002598.1.11",
          "form": "혼자서는 아무래도 힘들더라구요",
          "original_form": "혼자서는 아무래도 힘들더라구요",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002598.1.12",
          "form": "그롷죠",
          "original_form": "그롷죠",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002598.1.13",
          "form": "유투브 보세요",
          "original_form": "유투브 보세요",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002598.1.14",
          "form": "플랭크 말고는 또 어떤거하세요?",
          "original_form": "플랭크 말고는 또 어떤거하세요?",
