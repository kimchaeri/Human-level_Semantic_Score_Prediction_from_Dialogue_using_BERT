      "utterance": [
        {
          "id": "MDRW2100040038.1.1",
          "form": "좀......공략좀 보고와야겠어",
          "original_form": "좀......공략좀 보고와야겠어",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100040038.1.2",
          "form": "그거 맨 마지막에 뭔 조형물 설치되면",
          "original_form": "그거 맨 마지막에 뭔 조형물 설치되면",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100040038.1.3",
          "form": "마지막 기술은 뭐 어쩌란건지",
          "original_form": "마지막 기술은 뭐 어쩌란건지",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.4",
          "form": "감이 안잡혀",
          "original_form": "감이 안잡혀",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.5",
          "form": "가장 가까운 하나 골라잡아서",
          "original_form": "가장 가까운 하나 골라잡아서",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.6",
          "form": "그거 부숴야대?",
          "original_form": "그거 부숴야대?",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.7",
          "form": "시간이 10초가 되기 전까지 부숴야함",
          "original_form": "시간이 10초가 되기 전까지 부숴야함",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.8",
          "form": "ㅇㅋ.........",
          "original_form": "ㅇㅋ.........",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.9",
          "form": "근데 나 다이루크로 냅다 때리는데도 시간이 아슬아슬해서",
          "original_form": "근데 나 다이루크로 냅다 때리는데도 시간이 아슬아슬해서",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.10",
          "form": "일단 무조건",
          "original_form": "일단 무조건",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.11",
          "form": "공략 보고도 몇 번 실패함",
          "original_form": "공략 보고도 몇 번 실패함",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.12",
          "form": "스테미너 감소 음식은",
          "original_form": "스테미너 감소 음식은",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.13",
          "form": "먹어야겠네",
          "original_form": "먹어야겠네",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.14",
          "form": "ㅇㅇ 그거랑 공격업 꼭 먹어",
          "original_form": "ㅇㅇ 그거랑 공격업 꼭 먹어",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.15",
          "form": "겁나 뛰댕겨야대.....",
          "original_form": "겁나 뛰댕겨야대.....",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.16",
          "form": "ㅇㅇ 맞아 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 맞아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100040038.1.17",
          "form": "아 그럼 얼음 원소 공격력 상승 물약을",
