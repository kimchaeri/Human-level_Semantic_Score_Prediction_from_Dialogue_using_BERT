      "utterance": [
        {
          "id": "MDRW2100024733.1.1",
          "form": "ㅋㅋㅋㅋㅋ좋다 그럼 캐나다 여행 갈 예정인 사람들이나",
          "original_form": "ㅋㅋㅋㅋㅋ좋다 그럼 캐나다 여행 갈 예정인 사람들이나",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024733.1.2",
          "form": "그럼 비행기값은 회수되지않을까",
          "original_form": "그럼 비행기값은 회수되지않을까",
          "speaker_id": "1",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024733.1.3",
          "form": "여행 이미 다녀온 사람들이 찾아볼 것 같아",
          "original_form": "여행 이미 다녀온 사람들이 찾아볼 것 같아",
          "speaker_id": "2",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024733.1.4",
          "form": "재밌겠다 계획세워봐야지",
          "original_form": "재밌겠다 계획세워봐야지",
          "speaker_id": "1",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024733.1.5",
          "form": "난 일본여행 짧게 다녀온 기억도 안 적어놔서 가물가물하잖어",
          "original_form": "난 일본여행 짧게 다녀온 기억도 안 적어놔서 가물가물하잖어",
          "speaker_id": "2",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024733.1.6",
          "form": "캐나다 코스짜고 경비랑 준비물이랑",
          "original_form": "캐나다 코스짜고 경비랑 준비물이랑",
          "speaker_id": "1",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024733.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ기록의 중요성이네 나도 일기읽으면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ기록의 중요성이네 나도 일기읽으면",
          "speaker_id": "1",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100024733.1.8",
          "form": "그날기억이 잘나더라",
          "original_form": "그날기억이 잘나더라",
          "speaker_id": "1",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100024733.1.9",
          "form": "여행코스 준비 벌써부터 설레겠다ㅋㅋㅋㅋㅋ",
          "original_form": "여행코스 준비 벌써부터 설레겠다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100024733.1.10",
          "form": "맞아 기록이 짱이야",
          "original_form": "맞아 기록이 짱이야",
          "speaker_id": "2",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100024733.1.11",
          "form": "에어비앤비랑 비행기값도 아낄방법 찾아봐야지",
          "original_form": "에어비앤비랑 비행기값도 아낄방법 찾아봐야지",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100024733.1.12",
          "form": "캐나나 물가도 찾아봐야겠어",
          "original_form": "캐나나 물가도 찾아봐야겠어",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100024733.1.13",
          "form": "비행기값 아끼는 방법이 뭐 있었던 것 같은데",
          "original_form": "비행기값 아끼는 방법이 뭐 있었던 것 같은데",
          "speaker_id": "2",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100024733.1.14",
          "form": "그나저나 캐나다 달러쓰나",
          "original_form": "그나저나 캐나다 달러쓰나",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100024733.1.15",
          "form": "자꾸 조회하면 가격 오른다고 시크릿모드로 들어가랬아 그랬던 듯",
          "original_form": "자꾸 조회하면 가격 오른다고 시크릿모드로 들어가랬아 그랬던 듯",
          "speaker_id": "2",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100024733.1.16",
          "form": "달러쓰지않나?",
          "original_form": "달러쓰지않나?",
          "speaker_id": "2",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100024733.1.17",
          "form": "시크릿모드로 들어가야된다니 어이없네",
          "original_form": "시크릿모드로 들어가야된다니 어이없네",
          "speaker_id": "1",
          "time": "20210831 17:50"
        },
        {
          "id": "MDRW2100024733.1.18",
          "form": "달러 쓰는 나라면 달러 가격 내렸을 때 쟁여놔야겠는걸",
          "original_form": "달러 쓰는 나라면 달러 가격 내렸을 때 쟁여놔야겠는걸",
          "speaker_id": "2",
          "time": "20210831 17:50"
        },
        {
          "id": "MDRW2100024733.1.19",
          "form": "헐  맟네 그생각돚 해야겠다",
          "original_form": "헐  맟네 그생각돚 해야겠다",
          "speaker_id": "1",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024733.1.20",
          "form": "여행가서 서러운 것 = 외지에서 굶기",
          "original_form": "여행가서 서러운 것 = 외지에서 굶기",
          "speaker_id": "2",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024733.1.21",
          "form": "사진도 많이 남기고 캐나다음식 리뷰도 써 주라",
          "original_form": "사진도 많이 남기고 캐나다음식 리뷰도 써 주라",
          "speaker_id": "2",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024733.1.22",
          "form": "그래? 굶었었니!!!??",
          "original_form": "그래? 굶었었니!!!??",
          "speaker_id": "1",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024733.1.23",
          "form": "외국 먹거리 궁금해",
          "original_form": "외국 먹거리 궁금해",
          "speaker_id": "2",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024733.1.24",
          "form": "가서 오히려 많이 먹는게 걱정인데",
          "original_form": "가서 오히려 많이 먹는게 걱정인데",
          "speaker_id": "1",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100024733.1.25",
          "form": "아 소매치기안당하게 조심해야겠다",
          "original_form": "아 소매치기안당하게 조심해야겠다",
          "speaker_id": "1",
          "time": "20210831 17:52"
        }
      ]
