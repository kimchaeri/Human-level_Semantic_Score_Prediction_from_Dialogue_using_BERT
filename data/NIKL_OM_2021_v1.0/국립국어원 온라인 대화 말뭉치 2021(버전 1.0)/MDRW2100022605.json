      "utterance": [
        {
          "id": "MDRW2100022605.1.1",
          "form": "그쵸 잘 드세요?",
          "original_form": "그쵸 잘 드세요?",
          "speaker_id": "2",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100022605.1.2",
          "form": "잘 먹진 않은데 술자리는 좋아하네요.",
          "original_form": "잘 먹진 않은데 술자리는 좋아하네요.",
          "speaker_id": "1",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100022605.1.3",
          "form": "대신 도수 쎈건 잘먹어요",
          "original_form": "대신 도수 쎈건 잘먹어요",
          "speaker_id": "1",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100022605.1.4",
          "form": "오 잘 드시는데 잘 안마시는구나",
          "original_form": "오 잘 드시는데 잘 안마시는구나",
          "speaker_id": "2",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100022605.1.5",
          "form": "네네 아무래도 요즘 코로나 때문에 모이기 어려우니까요",
          "original_form": "네네 아무래도 요즘 코로나 때문에 모이기 어려우니까요",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100022605.1.6",
          "form": "좋아하는 주류있어요?",
          "original_form": "좋아하는 주류있어요?",
          "speaker_id": "2",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100022605.1.7",
          "form": "보드카류 완전 좋아해요 ㅎㅎ",
          "original_form": "보드카류 완전 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100022605.1.8",
          "form": "그리고 과일소주요!",
          "original_form": "그리고 과일소주요!",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100022605.1.9",
          "form": "과일소주 먹어봤어요?",
          "original_form": "과일소주 먹어봤어요?",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100022605.1.10",
          "form": "오 그엏군요 옛날에 한창 그 유자 순하리 기억나세요?",
          "original_form": "오 그엏군요 옛날에 한창 그 유자 순하리 기억나세요?",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022605.1.11",
          "form": "ㅋㅋㅋㅋ그때만 자주먹은거같아요",
          "original_form": "ㅋㅋㅋㅋ그때만 자주먹은거같아요",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022605.1.12",
          "form": "어 맞아요~ 그게 시초 아니에요?ㅋㅋㅋ",
          "original_form": "어 맞아요~ 그게 시초 아니에요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022605.1.13",
          "form": "유자맛이랑 씨원자몽맛 최고였어요 완전",
          "original_form": "유자맛이랑 씨원자몽맛 최고였어요 완전",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022605.1.14",
          "form": "네맞아요 ㅋㅋㅋㅋ정말 유행이엇는데",
          "original_form": "네맞아요 ㅋㅋㅋㅋ정말 유행이엇는데",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022605.1.15",
          "form": "근데 지금 유행 끝났나봐요..?",
          "original_form": "근데 지금 유행 끝났나봐요..?",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022605.1.16",
          "form": "요즘 잘 안보이더라구용",
          "original_form": "요즘 잘 안보이더라구용",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022605.1.17",
          "form": "가게에 잘 안보이던데 ㅠㅠ 너무 슬퍼요",
          "original_form": "가게에 잘 안보이던데 ㅠㅠ 너무 슬퍼요",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022605.1.18",
          "form": "혹시 좋은데이 블루베리맛 청포도맛 알아요?ㅋㅋ",
          "original_form": "혹시 좋은데이 블루베리맛 청포도맛 알아요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022605.1.19",
          "form": "그 깔라만씨 소주 드셔보셧어요?",
          "original_form": "그 깔라만씨 소주 드셔보셧어요?",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022605.1.20",
          "form": "깔라만앀ㅋㅋㅋ저하고 좀 안맞았어요 넘 써요",
          "original_form": "깔라만앀ㅋㅋㅋ저하고 좀 안맞았어요 넘 써요",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022605.1.21",
          "form": "아 그것도 예전에 맛본거같아욬ㅋㅋㅋ",
          "original_form": "아 그것도 예전에 맛본거같아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022605.1.22",
          "form": "어우 사탕 녹인 맛 아니에요? ㅠ",
          "original_form": "어우 사탕 녹인 맛 아니에요? ㅠ",
          "speaker_id": "1",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022605.1.23",
          "form": "ㅋㅋㅋㅋㅋ약간쌉싸름한데 소주치고 괜찮앗어욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ약간쌉싸름한데 소주치고 괜찮앗어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022605.1.24",
          "form": "과일소주는 딱 유자 자몽까지가 괜찮았던 것 같아요",
          "original_form": "과일소주는 딱 유자 자몽까지가 괜찮았던 것 같아요",
          "speaker_id": "1",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022605.1.25",
          "form": "네 저는 그이후로 잘안먹엇어욬ㅋㅋㅋ",
          "original_form": "네 저는 그이후로 잘안먹엇어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022605.1.26",
          "form": "그건 어때요?",
          "original_form": "그건 어때요?",
          "speaker_id": "1",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022605.1.27",
          "form": "과일막걸리요 ㅎㅎ",
          "original_form": "과일막걸리요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:16"
        }
      ]
