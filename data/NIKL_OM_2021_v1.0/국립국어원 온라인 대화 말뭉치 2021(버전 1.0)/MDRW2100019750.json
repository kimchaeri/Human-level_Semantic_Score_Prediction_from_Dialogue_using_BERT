      "utterance": [
        {
          "id": "MDRW2100019750.1.1",
          "form": "무슨음식 좋아하세요??",
          "original_form": "무슨음식 좋아하세요??",
          "speaker_id": "1",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019750.1.2",
          "form": "저 치킨이랑 떡볶이 좋아해요!!",
          "original_form": "저 치킨이랑 떡볶이 좋아해요!!",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019750.1.3",
          "form": "그럼 걸작 좋아하시나요? ㅋㅋㅋㅋ",
          "original_form": "그럼 걸작 좋아하시나요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019750.1.4",
          "form": "헐 네ㅠㅠㅋㅋㅋ",
          "original_form": "헐 네ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019750.1.5",
          "form": "걸작 완전 좋아해요",
          "original_form": "걸작 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019750.1.6",
          "form": "제가 치킨먹을 때 닭가슴살부위를 좋아해서ㅋㅋ",
          "original_form": "제가 치킨먹을 때 닭가슴살부위를 좋아해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019750.1.7",
          "form": "전 안먹어봤는데",
          "original_form": "전 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019750.1.8",
          "form": "치킨 떡볶이하니까 딱 생각났어요",
          "original_form": "치킨 떡볶이하니까 딱 생각났어요",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019750.1.9",
          "form": "아거기 순살이 가슴살인가요??",
          "original_form": "아거기 순살이 가슴살인가요??",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019750.1.10",
          "form": "걸작 치킨이 그렇더라고요",
          "original_form": "걸작 치킨이 그렇더라고요",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019750.1.11",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019750.1.12",
          "form": "그랬던거같아여",
          "original_form": "그랬던거같아여",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019750.1.13",
          "form": "아 저는 다리살파라 ㅜㅜ 저랑 반대시네요 ㅋㅋㅋ",
          "original_form": "아 저는 다리살파라 ㅜㅜ 저랑 반대시네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019750.1.14",
          "form": "ㅋㅋㅋ보통 다리살 좋아하시죠 다들",
          "original_form": "ㅋㅋㅋ보통 다리살 좋아하시죠 다들",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019750.1.15",
          "form": "그래서 친구들이랑 치킨먹을 때 너무 좋아요ㅋㅋ",
          "original_form": "그래서 친구들이랑 치킨먹을 때 너무 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019750.1.16",
          "form": "오 그러겠네요 그런 장점이 ㅋㅋㅋㅋ",
          "original_form": "오 그러겠네요 그런 장점이 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019750.1.17",
          "form": "저는 닭다리 말고 닭가슴살을 먹어서!!",
          "original_form": "저는 닭다리 말고 닭가슴살을 먹어서!!",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019750.1.18",
          "form": "나눠먹기 딱이겠어요!!",
          "original_form": "나눠먹기 딱이겠어요!!",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019750.1.19",
          "form": "네네도 퍽살로 주더라구요",
          "original_form": "네네도 퍽살로 주더라구요",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019750.1.20",
          "form": "맞아여",
