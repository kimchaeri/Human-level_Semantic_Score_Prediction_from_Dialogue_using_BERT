      "utterance": [
        {
          "id": "MDRW2100026743.1.1",
          "form": "맞아요 칠성은 옛날부터 기억이 나는데",
          "original_form": "맞아요 칠성은 옛날부터 기억이 나는데",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026743.1.2",
          "form": "스프라이트는 뭔가 새로 등장한 느낌이에요",
          "original_form": "스프라이트는 뭔가 새로 등장한 느낌이에요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026743.1.3",
          "form": "이온음료는 뭐 제일 많이드시나요",
          "original_form": "이온음료는 뭐 제일 많이드시나요",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026743.1.4",
          "form": "역시 포카리?",
          "original_form": "역시 포카리?",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026743.1.5",
          "form": "포카리죠",
          "original_form": "포카리죠",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026743.1.6",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026743.1.7",
          "form": "저도 ㅋㅋㅋ",
          "original_form": "저도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026743.1.8",
          "form": "그거 진짜 맛없어요 토레타",
          "original_form": "그거 진짜 맛없어요 토레타",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026743.1.9",
          "form": "박보영이 광고를 너무 귀엽게해서.. 속았지만 진짜 맛없어요",
          "original_form": "박보영이 광고를 너무 귀엽게해서.. 속았지만 진짜 맛없어요",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026743.1.10",
          "form": "아 토레타...",
          "original_form": "아 토레타...",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026743.1.11",
          "form": "저는 일단 입에도 안댔어요",
          "original_form": "저는 일단 입에도 안댔어요",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026743.1.12",
          "form": "이프로 같은 맛인줄 알았는데",
          "original_form": "이프로 같은 맛인줄 알았는데",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026743.1.13",
          "form": "아닌가봐요?",
          "original_form": "아닌가봐요?",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026743.1.14",
          "form": "네 진짜..뭐라 설명할 수 없는 이상한 맛입니다",
          "original_form": "네 진짜..뭐라 설명할 수 없는 이상한 맛입니다",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026743.1.15",
          "form": "소주vs맥주vs와인",
          "original_form": "소주vs맥주vs와인",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026743.1.16",
          "form": "아니면 그외?",
          "original_form": "아니면 그외?",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026743.1.17",
          "form": "ㅋㅋㅋ저는 와인이요",
          "original_form": "ㅋㅋㅋ저는 와인이요",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026743.1.18",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026743.1.19",
          "form": "소맥 섞어마셔야죻ㅎㅎ",
          "original_form": "소맥 섞어마셔야죻ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026743.1.20",
          "form": "와인은 뭔가 진입장벽이임ㅅ네요 가격때문인가",
          "original_form": "와인은 뭔가 진입장벽이임ㅅ네요 가격때문인가",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026743.1.21",
          "form": "아 소맥이 있었네요ㅋㅋㅋ",
          "original_form": "아 소맥이 있었네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026743.1.22",
          "form": "저는 소주따로 맥주따로 먹으면 알코올향이 너무 나더라고요",
          "original_form": "저는 소주따로 맥주따로 먹으면 알코올향이 너무 나더라고요",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026743.1.23",
          "form": "오 공감합니다",
          "original_form": "오 공감합니다",
          "speaker_id": "1",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026743.1.24",
          "form": "근데 이상하게 섞어마시면 괜찮죠",
          "original_form": "근데 이상하게 섞어마시면 괜찮죠",
          "speaker_id": "1",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026743.1.25",
          "form": "맞아요 사실 와인도 그나마 알코올향이 덜나서...",
          "original_form": "맞아요 사실 와인도 그나마 알코올향이 덜나서...",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026743.1.26",
          "form": "오호 와인은 가격대에 따라 맛이 많이 다른가요?",
          "original_form": "오호 와인은 가격대에 따라 맛이 많이 다른가요?",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026743.1.27",
          "form": "가격대보단 종류가 많으니까 맛의 선택의 폭이 넓어지더라고요",
          "original_form": "가격대보단 종류가 많으니까 맛의 선택의 폭이 넓어지더라고요",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026743.1.28",
          "form": "오 좋아하시는 종류가 뭐에요??",
          "original_form": "오 좋아하시는 종류가 뭐에요??",
          "speaker_id": "1",
          "time": "20210902 08:26"
        }
      ]
