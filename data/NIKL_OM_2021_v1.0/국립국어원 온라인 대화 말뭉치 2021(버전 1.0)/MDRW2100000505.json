      "utterance": [
        {
          "id": "MDRW2100000505.1.1",
          "form": "여름휴가계획 세운거있어?",
          "original_form": "여름휴가계획 세운거있어?",
          "speaker_id": "1",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000505.1.2",
          "form": "휴가 끝!\n코로나때문에 집콕했어 ㅠ",
          "original_form": "휴가 끝!\n코로나때문에 집콕했어 ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000505.1.3",
          "form": "넌 다녀왔어?",
          "original_form": "넌 다녀왔어?",
          "speaker_id": "2",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000505.1.4",
          "form": "집콕이라니 ㅠㅠ넘슬프다",
          "original_form": "집콕이라니 ㅠㅠ넘슬프다",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000505.1.5",
          "form": "나는 저번주에 오이도 다녀왔어!",
          "original_form": "나는 저번주에 오이도 다녀왔어!",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000505.1.6",
          "form": "오이도? 어디쯤 있어?",
          "original_form": "오이도? 어디쯤 있어?",
          "speaker_id": "2",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000505.1.7",
          "form": "좋았겠다",
          "original_form": "좋았겠다",
          "speaker_id": "2",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000505.1.8",
          "form": "코로나 끝나면 나도 실컷 다니고싶어",
          "original_form": "코로나 끝나면 나도 실컷 다니고싶어",
          "speaker_id": "2",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000505.1.9",
          "form": "서울이랑 가까워 1시간거리더라",
          "original_form": "서울이랑 가까워 1시간거리더라",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000505.1.10",
          "form": "코로나가 없는것처럼 느껴졋어 사람 진짜많았어",
          "original_form": "코로나가 없는것처럼 느껴졋어 사람 진짜많았어",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000505.1.11",
          "form": "서울 나는 큰맘 먹고 가야하는 휴가지야 ㅎ",
          "original_form": "서울 나는 큰맘 먹고 가야하는 휴가지야 ㅎ",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000505.1.12",
          "form": "맞아 뉴스 보면 왜 확진자가 많은지 알겠더라",
          "original_form": "맞아 뉴스 보면 왜 확진자가 많은지 알겠더라",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000505.1.13",
          "form": "나는 못간 휴간데 남들은 매주 다니더만 ㅠ",
          "original_form": "나는 못간 휴간데 남들은 매주 다니더만 ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000505.1.14",
          "form": "어디라도 가지 그랬어ㅠ 나는 약가 지금 반포기야",
          "original_form": "어디라도 가지 그랬어ㅠ 나는 약가 지금 반포기야",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000505.1.15",
          "form": "이정도면 코로나 안걸린게 신기하더라구",
          "original_form": "이정도면 코로나 안걸린게 신기하더라구",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000505.1.16",
          "form": "나는 스쳐간거 아닐까 싶더라고",
          "original_form": "나는 스쳐간거 아닐까 싶더라고",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000505.1.17",
          "form": "실은 더위를 싫어하기도하고 사람 많은걸 싫어해",
          "original_form": "실은 더위를 싫어하기도하고 사람 많은걸 싫어해",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000505.1.18",
          "form": "날 선선해지면 산에 다니고 싶은데 ㅎ 몸이 무거워져버렸네 ㅠ",
          "original_form": "날 선선해지면 산에 다니고 싶은데 ㅎ 몸이 무거워져버렸네 ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000505.1.19",
          "form": "오이도는 어때? 나중에 서울 가게 되면 코스에 끼워넣을만할까?",
          "original_form": "오이도는 어때? 나중에 서울 가게 되면 코스에 끼워넣을만할까?",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000505.1.20",
          "form": "산 좋아하는구나 나는 벌레때매 산 너무시러해",
          "original_form": "산 좋아하는구나 나는 벌레때매 산 너무시러해",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000505.1.21",
          "form": "음,,,오이도를 간게 계획하고간게 아니라서 무작정 가긴했는데",
          "original_form": "음,,,오이도를 간게 계획하고간게 아니라서 무작정 가긴했는데",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000505.1.22",
          "form": "아니살빼려고 ㅋㅋㅋㅋㅋ",
          "original_form": "아니살빼려고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000505.1.23",
          "form": "두번갈곳은 아닌거같아 ㅎㅎ 다른곳으로가!",
          "original_form": "두번갈곳은 아닌거같아 ㅎㅎ 다른곳으로가!",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000505.1.24",
          "form": "아 ㅋㅋㅋㅋ나도 살빼야하는데 하하하",
          "original_form": "아 ㅋㅋㅋㅋ나도 살빼야하는데 하하하",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000505.1.25",
          "form": "서울대공원 또 가고 싶어 ㅋㅋㅋ",
          "original_form": "서울대공원 또 가고 싶어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000505.1.26",
          "form": "나도 애들도 너무 신나게 놀고 왔었거든",
          "original_form": "나도 애들도 너무 신나게 놀고 왔었거든",
          "speaker_id": "2",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000505.1.27",
          "form": "서울대공원?나는 어린이대공원만 가봤옹",
          "original_form": "서울대공원?나는 어린이대공원만 가봤옹",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000505.1.28",
          "form": "거기어때 좋아?",
          "original_form": "거기어때 좋아?",
          "speaker_id": "1",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000505.1.29",
          "form": "헐 다른거야?",
          "original_form": "헐 다른거야?",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000505.1.30",
          "form": "벚꽃길 코끼리주전차열찬가",
          "original_form": "벚꽃길 코끼리주전차열찬가",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000505.1.31",
          "form": "아? 내가 서울사람이 아니라 잘 모르겠어 ㅋㅋㅋㅋㅋ",
          "original_form": "아? 내가 서울사람이 아니라 잘 모르겠어 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000505.1.32",
          "form": "리프트타고",
          "original_form": "리프트타고",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000505.1.33",
          "form": "아 서울사람인줄 알았어 미안 ㅎ",
          "original_form": "아 서울사람인줄 알았어 미안 ㅎ",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000505.1.34",
          "form": "서울대공원은 ㄱ과천에있는거네",
          "original_form": "서울대공원은 ㄱ과천에있는거네",
          "speaker_id": "1",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000505.1.35",
          "form": "아 나 어린이대공원이다",
          "original_form": "아 나 어린이대공원이다",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000505.1.36",
          "form": "ㅋㅋㅋ아 괜히 아는척했어",
          "original_form": "ㅋㅋㅋ아 괜히 아는척했어",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000505.1.37",
          "form": "아 ㅋㅋㅋㅋ어린이대공원 좋지",
          "original_form": "아 ㅋㅋㅋㅋ어린이대공원 좋지",
          "speaker_id": "1",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000505.1.38",
          "form": "근데 동물들이 좀 불쌍하더라구 ㅠㅠ",
          "original_form": "근데 동물들이 좀 불쌍하더라구 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000505.1.39",
          "form": "서울사람한테 자랑하고싶었어 ㅋㅋㅋㅋ",
          "original_form": "서울사람한테 자랑하고싶었어 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000505.1.40",
          "form": "이해해줘 ㅋㅋㅋㅋ",
          "original_form": "이해해줘 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000505.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋ이해해",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이해해",
          "speaker_id": "1",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000505.1.42",
          "form": "넌 착한 아이구나 ㅋㅋㅋㅋㅋ",
          "original_form": "넌 착한 아이구나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000505.1.43",
          "form": "코로나 끝나면 또 가고 싶은 여행지가 많은데 갈수 있을까 ㅠ",
          "original_form": "코로나 끝나면 또 가고 싶은 여행지가 많은데 갈수 있을까 ㅠ",
