      "utterance": [
        {
          "id": "MDRW2100018509.1.1",
          "form": "봉사에 대해 어떻게 생각하세요?",
          "original_form": "봉사에 대해 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210827 07:12"
        },
        {
          "id": "MDRW2100018509.1.2",
          "form": "글쎄요",
          "original_form": "글쎄요",
          "speaker_id": "2",
          "time": "20210827 07:13"
        },
        {
          "id": "MDRW2100018509.1.3",
          "form": "전 예전에",
          "original_form": "전 예전에",
          "speaker_id": "2",
          "time": "20210827 07:13"
        },
        {
          "id": "MDRW2100018509.1.4",
          "form": "학교도서 봉사",
          "original_form": "학교도서 봉사",
          "speaker_id": "2",
          "time": "20210827 07:13"
        },
        {
          "id": "MDRW2100018509.1.5",
          "form": "구연동화 봉사 했어요",
          "original_form": "구연동화 봉사 했어요",
          "speaker_id": "2",
          "time": "20210827 07:13"
        },
        {
          "id": "MDRW2100018509.1.6",
          "form": "좋죠 할수 있으면 ㅎㅎ",
          "original_form": "좋죠 할수 있으면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:13"
        },
        {
          "id": "MDRW2100018509.1.7",
          "form": "학교도서 봉사는 어떤건가요?",
          "original_form": "학교도서 봉사는 어떤건가요?",
          "speaker_id": "1",
          "time": "20210827 07:14"
        },
        {
          "id": "MDRW2100018509.1.8",
          "form": "그쵸ㅎㅎ 할 수 있음 좋은거같아요",
          "original_form": "그쵸ㅎㅎ 할 수 있음 좋은거같아요",
          "speaker_id": "1",
          "time": "20210827 07:14"
        },
        {
          "id": "MDRW2100018509.1.9",
          "form": "학교에서 도서관이 있어요",
          "original_form": "학교에서 도서관이 있어요",
          "speaker_id": "2",
          "time": "20210827 07:14"
        },
        {
          "id": "MDRW2100018509.1.10",
          "form": "거기서 도서관리해요",
          "original_form": "거기서 도서관리해요",
          "speaker_id": "2",
          "time": "20210827 07:14"
        },
        {
          "id": "MDRW2100018509.1.11",
          "form": "행사도 하고요",
          "original_form": "행사도 하고요",
          "speaker_id": "2",
          "time": "20210827 07:14"
        },
        {
          "id": "MDRW2100018509.1.12",
          "form": "근데 요새 구연동화 신청했는데 거의 2년째 소식도 없어요 중지요",
          "original_form": "근데 요새 구연동화 신청했는데 거의 2년째 소식도 없어요 중지요",
          "speaker_id": "2",
          "time": "20210827 07:15"
        },
        {
          "id": "MDRW2100018509.1.13",
          "form": "신기하넹ㆍ",
          "original_form": "신기하넹ㆍ",
          "speaker_id": "1",
          "time": "20210827 07:15"
        },
        {
          "id": "MDRW2100018509.1.14",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210827 07:15"
        },
        {
          "id": "MDRW2100018509.1.15",
          "form": "코시국이라ㅜㅜ",
          "original_form": "코시국이라ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 07:16"
        },
        {
          "id": "MDRW2100018509.1.16",
          "form": "봉사 시간 다 인정되요 ㅎㅎ",
          "original_form": "봉사 시간 다 인정되요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:16"
        },
        {
          "id": "MDRW2100018509.1.17",
          "form": "네 중단 아이들이 많은걸 못누리죠",
          "original_form": "네 중단 아이들이 많은걸 못누리죠",
          "speaker_id": "2",
          "time": "20210827 07:16"
        },
        {
          "id": "MDRW2100018509.1.18",
          "form": "오 좋네요!ㅎㅎ",
          "original_form": "오 좋네요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:16"
        },
        {
          "id": "MDRW2100018509.1.19",
          "form": "사라져서",
          "original_form": "사라져서",
          "speaker_id": "2",
          "time": "20210827 07:16"
        },
        {
          "id": "MDRW2100018509.1.20",
          "form": "그쵸ㅠ 안타까워요",
          "original_form": "그쵸ㅠ 안타까워요",
          "speaker_id": "1",
          "time": "20210827 07:16"
        },
        {
          "id": "MDRW2100018509.1.21",
          "form": "네 무슨 봉사 하셨어요?",
          "original_form": "네 무슨 봉사 하셨어요?",
          "speaker_id": "2",
          "time": "20210827 07:17"
        },
        {
          "id": "MDRW2100018509.1.22",
          "form": "저는 어린이집에서 보조 해봤어요",
          "original_form": "저는 어린이집에서 보조 해봤어요",
          "speaker_id": "1",
          "time": "20210827 07:18"
        },
        {
          "id": "MDRW2100018509.1.23",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 07:19"
        },
        {
          "id": "MDRW2100018509.1.24",
          "form": "아이들 보기 힘드시죠",
          "original_form": "아이들 보기 힘드시죠",
          "speaker_id": "2",
          "time": "20210827 07:19"
        },
        {
          "id": "MDRW2100018509.1.25",
          "form": "아이들 좋아 하시나봐요 ㅎㅎ",
          "original_form": "아이들 좋아 하시나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:19"
        },
        {
          "id": "MDRW2100018509.1.26",
          "form": "전 노인복지시설 청소도 해보긴 했어요",
          "original_form": "전 노인복지시설 청소도 해보긴 했어요",
          "speaker_id": "2",
          "time": "20210827 07:20"
        },
        {
          "id": "MDRW2100018509.1.27",
          "form": "네ㅎㅎ 그래도 귀엽더라구요",
          "original_form": "네ㅎㅎ 그래도 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210827 07:20"
        },
        {
          "id": "MDRW2100018509.1.28",
          "form": "어머 다행이네요",
          "original_form": "어머 다행이네요",
          "speaker_id": "2",
          "time": "20210827 07:20"
        },
        {
          "id": "MDRW2100018509.1.29",
          "form": "복지시설은 힘들다던데 어떠셨어요?",
          "original_form": "복지시설은 힘들다던데 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210827 07:20"
        },
        {
          "id": "MDRW2100018509.1.30",
          "form": "어린이집 아가들 귀여워요 어려서",
          "original_form": "어린이집 아가들 귀여워요 어려서",
          "speaker_id": "2",
          "time": "20210827 07:20"
        },
        {
          "id": "MDRW2100018509.1.31",
          "form": "음 저는 초보라 청소만 해서",
          "original_form": "음 저는 초보라 청소만 해서",
          "speaker_id": "2",
          "time": "20210827 07:20"
        },
        {
          "id": "MDRW2100018509.1.32",
          "form": "어렵지 않았어요",
          "original_form": "어렵지 않았어요",
          "speaker_id": "2",
          "time": "20210827 07:21"
        },
        {
          "id": "MDRW2100018509.1.33",
          "form": "쉬운거 시켜 주셔서요",
          "original_form": "쉬운거 시켜 주셔서요",
          "speaker_id": "2",
          "time": "20210827 07:21"
        },
        {
          "id": "MDRW2100018509.1.34",
          "form": "밀대로 물청소",
          "original_form": "밀대로 물청소",
          "speaker_id": "2",
          "time": "20210827 07:21"
        },
        {
          "id": "MDRW2100018509.1.35",
          "form": "바닥 닦기요",
          "original_form": "바닥 닦기요",
          "speaker_id": "2",
          "time": "20210827 07:21"
        }
      ]
    }
