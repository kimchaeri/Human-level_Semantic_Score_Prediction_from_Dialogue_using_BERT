      "utterance": [
        {
          "id": "MDRW2100030849.1.1",
          "form": "술자리 게임 고수신가요?",
          "original_form": "술자리 게임 고수신가요?",
          "speaker_id": "2",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030849.1.2",
          "form": "술마시기도 바쁜데 왜 게임을 하쥬ㅎㅎ",
          "original_form": "술마시기도 바쁜데 왜 게임을 하쥬ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030849.1.3",
          "form": "와 진짜가 나타났다!",
          "original_form": "와 진짜가 나타났다!",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030849.1.4",
          "form": "술자리서 게임 자주 하세요?",
          "original_form": "술자리서 게임 자주 하세요?",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030849.1.5",
          "form": "음 술마시고 게임합니다 ㅋㅋ",
          "original_form": "음 술마시고 게임합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030849.1.6",
          "form": "제기차기",
          "original_form": "제기차기",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030849.1.7",
          "form": "ㅋㅋ 음주게임",
          "original_form": "ㅋㅋ 음주게임",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030849.1.8",
          "form": "아니ㅋ 제기는 어디서 나서요",
          "original_form": "아니ㅋ 제기는 어디서 나서요",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030849.1.9",
          "form": "음주게임해서 친구들하고 딱밤때리기",
          "original_form": "음주게임해서 친구들하고 딱밤때리기",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030849.1.10",
          "form": "차에 실려있어요 ㅋㅋㅋ",
          "original_form": "차에 실려있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030849.1.11",
          "form": "미쵸ㅋㅋ",
          "original_form": "미쵸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030849.1.12",
          "form": "연날리기용 연도 있구요",
          "original_form": "연날리기용 연도 있구요",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030849.1.13",
          "form": "전  모바일 게임 중 제기차기 있는 줄ㅋㅋ",
          "original_form": "전  모바일 게임 중 제기차기 있는 줄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030849.1.14",
          "form": "찐자 제기차기입니다 ㅋㅋ",
          "original_form": "찐자 제기차기입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030849.1.15",
          "form": "아니ㅋㅋ 음주 후 너무 건실한 놀이를",
          "original_form": "아니ㅋㅋ 음주 후 너무 건실한 놀이를",
          "speaker_id": "1",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030849.1.16",
          "form": "음주하고 노래방도 감",
          "original_form": "음주하고 노래방도 감",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030849.1.17",
          "form": "전 음주후 피방 가서 스타한다는 줄 알았어요ㅎㅎ",
          "original_form": "전 음주후 피방 가서 스타한다는 줄 알았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030849.1.18",
          "form": "스타도 했섯는데 ㅋㅋ",
          "original_form": "스타도 했섯는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030849.1.19",
          "form": "이제는 안가구요",
          "original_form": "이제는 안가구요",
          "speaker_id": "2",
