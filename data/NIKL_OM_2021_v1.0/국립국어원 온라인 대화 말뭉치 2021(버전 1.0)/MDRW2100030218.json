        },
        {
          "id": "MDRW2100030218.1.2",
          "form": "광명사는 사람 입니다",
          "original_form": "광명사는 사람 입니다",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030218.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030218.1.4",
          "form": "우와~~~~~~",
          "original_form": "우와~~~~~~",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030218.1.5",
          "form": "저는 서울사는사람입니다",
          "original_form": "저는 서울사는사람입니다",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030218.1.6",
          "form": "고양이는 잘보고오셨나요???",
          "original_form": "고양이는 잘보고오셨나요???",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030218.1.7",
          "form": "이번기회에 고양이가족이 생기는건가요??",
          "original_form": "이번기회에 고양이가족이 생기는건가요??",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030218.1.8",
          "form": "아뇨 저 알러지 있어서 고양이 가족은 무리일거 같슴니다ㅠㅠㅋㅋ",
          "original_form": "아뇨 저 알러지 있어서 고양이 가족은 무리일거 같슴니다ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030218.1.9",
          "form": "서울 사시는 분이군요~~~",
          "original_form": "서울 사시는 분이군요~~~",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030218.1.10",
          "form": "요즘 비가와서 집안일이 많겠어요",
          "original_form": "요즘 비가와서 집안일이 많겠어요",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030218.1.11",
          "form": "특히 빨래가 안마르지않나요???",
          "original_form": "특히 빨래가 안마르지않나요???",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030218.1.12",
          "form": "그렇슴니댜",
          "original_form": "그렇슴니댜",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030218.1.13",
          "form": "습해서 말이 아니에오 제습기를 얼마나 틀었는지",
          "original_form": "습해서 말이 아니에오 제습기를 얼마나 틀었는지",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030218.1.14",
          "form": "그래도 요즘엔 바람이 많이 불어서 괜찮지 않나오?",
          "original_form": "그래도 요즘엔 바람이 많이 불어서 괜찮지 않나오?",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030218.1.15",
          "form": "저는 베란다에 오래두었어요ㅠㅠ",
          "original_form": "저는 베란다에 오래두었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030218.1.16",
          "form": "오늘은 그래두 바람이 불어서 마른것같아요",
          "original_form": "오늘은 그래두 바람이 불어서 마른것같아요",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030218.1.17",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030218.1.18",
          "form": "곰팡이가 괴롭히지는 않았나오?",
          "original_form": "곰팡이가 괴롭히지는 않았나오?",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030218.1.19",
          "form": "지난번에 벽에 생긴 벽때문에 고생하고",
          "original_form": "지난번에 벽에 생긴 벽때문에 고생하고",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030218.1.20",
          "form": "자체 공사를 했는데",
          "original_form": "자체 공사를 했는데",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030218.1.21",
          "form": "아직은 비올때 곰팡이 냄새가 스믈스믈 바닥에서 냄새나요",
          "original_form": "아직은 비올때 곰팡이 냄새가 스믈스믈 바닥에서 냄새나요",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030218.1.22",
          "form": "헐 그럼 어째요....",
          "original_form": "헐 그럼 어째요....",
          "speaker_id": "1",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030218.1.23",
          "form": "겨울에 결로생기면 또 클나는거 아닙니까",
          "original_form": "겨울에 결로생기면 또 클나는거 아닙니까",
          "speaker_id": "1",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030218.1.24",
          "form": "저도 방수 페인트 베란다 발라야 한다는...고통....",
          "original_form": "저도 방수 페인트 베란다 발라야 한다는...고통....",
          "speaker_id": "1",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030218.1.25",
          "form": "저희는 방수페인트랑 실리콘 다시 했는데",
          "original_form": "저희는 방수페인트랑 실리콘 다시 했는데",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030218.1.26",
          "form": "올 겨울에 한번봐야죠",
          "original_form": "올 겨울에 한번봐야죠",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030218.1.27",
          "form": "오 두개 작업 하셨군영",
          "original_form": "오 두개 작업 하셨군영",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030218.1.28",
          "form": "실리콘 작업 힘들지 않았나오?",
          "original_form": "실리콘 작업 힘들지 않았나오?",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030218.1.29",
          "form": "요즘 가루로된 페이트 집안에 습기먹는페인트 있는데",
          "original_form": "요즘 가루로된 페이트 집안에 습기먹는페인트 있는데",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030218.1.30",
          "form": "그거 좋데요",
          "original_form": "그거 좋데요",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030218.1.31",
          "form": "조금 비싸긴하지만",
          "original_form": "조금 비싸긴하지만",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030218.1.32",
          "form": "오오 습기먹는 페인트 좋군여",
          "original_form": "오오 습기먹는 페인트 좋군여",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030218.1.33",
          "form": "꿀팁 감사합니댜",
          "original_form": "꿀팁 감사합니댜",
          "speaker_id": "1",
          "time": "20210903 23:11"
        }
      ]
