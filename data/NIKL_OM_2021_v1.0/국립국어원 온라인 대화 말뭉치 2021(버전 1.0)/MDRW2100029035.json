      "utterance": [
        {
          "id": "MDRW2100029035.1.1",
          "form": "내일은 돈까스 시켜먹자",
          "original_form": "내일은 돈까스 시켜먹자",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100029035.1.2",
          "form": "어때",
          "original_form": "어때",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100029035.1.3",
          "form": "내일 뭐 시켜먹자!!",
          "original_form": "내일 뭐 시켜먹자!!",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100029035.1.4",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100029035.1.5",
          "form": "돈까스는 어디가 맛있어?",
          "original_form": "돈까스는 어디가 맛있어?",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100029035.1.6",
          "form": "웬일이야?",
          "original_form": "웬일이야?",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100029035.1.7",
          "form": "돈까스 맛집 알려줘",
          "original_form": "돈까스 맛집 알려줘",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100029035.1.8",
          "form": "돈까스 별로 안좋아하잖아",
          "original_form": "돈까스 별로 안좋아하잖아",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100029035.1.9",
          "form": "아무튼 알겟어",
          "original_form": "아무튼 알겟어",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100029035.1.10",
          "form": "돈까쓰는 대부분 좋아하지",
          "original_form": "돈까쓰는 대부분 좋아하지",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100029035.1.11",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100029035.1.12",
          "form": "돈까스는 치즈돈까스가 맛잇으면",
          "original_form": "돈까스는 치즈돈까스가 맛잇으면",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.13",
          "form": "맛집이야.",
          "original_form": "맛집이야.",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.14",
          "form": "치즈돈까스야말로 근본음식이기 때문이지",
          "original_form": "치즈돈까스야말로 근본음식이기 때문이지",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.15",
          "form": "그렇구나!! 난 돈까스의 비결은",
          "original_form": "그렇구나!! 난 돈까스의 비결은",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.16",
          "form": "고기라고 생각해",
          "original_form": "고기라고 생각해",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.17",
          "form": "고기가 기름기많고 맛없으면",
          "original_form": "고기가 기름기많고 맛없으면",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.18",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.19",
          "form": "아무리 튀김이 맛있어도 맛없지만",
          "original_form": "아무리 튀김이 맛있어도 맛없지만",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.20",
          "form": "고기가 맛있으면",
          "original_form": "고기가 맛있으면",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.21",
          "form": "아무리 튀김이 맛없어도",
          "original_form": "아무리 튀김이 맛없어도",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.22",
          "form": "먹을만하거든",
          "original_form": "먹을만하거든",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.23",
          "form": "네 말이 맞다.",
          "original_form": "네 말이 맞다.",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.24",
          "form": "인정?",
          "original_form": "인정?",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.25",
          "form": "일단 돈까스는 고기가 두꺼워야 함",
          "original_form": "일단 돈까스는 고기가 두꺼워야 함",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.26",
          "form": "나도 그렇게생각해",
          "original_form": "나도 그렇게생각해",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.27",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100029035.1.28",
          "form": "빨리 먹고싶당 ㅎㅎ",
          "original_form": "빨리 먹고싶당 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100029035.1.29",
          "form": "짬뽕이 좋아 짜장면이 좋아?",
          "original_form": "짬뽕이 좋아 짜장면이 좋아?",
          "speaker_id": "1",
          "time": "20210903 22:46"
        }
      ]
    }
  ]
