      "utterance": [
        {
          "id": "MDRW2100015935.1.1",
          "form": "실시간 강의가 뭐야",
          "original_form": "실시간 강의가 뭐야",
          "speaker_id": "2",
          "time": "20210826 03:09"
        },
        {
          "id": "MDRW2100015935.1.2",
          "form": "어플로 교수님이랑 수업하는거야",
          "original_form": "어플로 교수님이랑 수업하는거야",
          "speaker_id": "1",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100015935.1.3",
          "form": "그래서?",
          "original_form": "그래서?",
          "speaker_id": "2",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100015935.1.4",
          "form": "나 캠이 없어서 엄마 노트북 써야해",
          "original_form": "나 캠이 없어서 엄마 노트북 써야해",
          "speaker_id": "1",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100015935.1.5",
          "form": "노트북 빌려달라고?",
          "original_form": "노트북 빌려달라고?",
          "speaker_id": "2",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100015935.1.6",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100015935.1.7",
          "form": "너 컴퓨터도 있잖아",
          "original_form": "너 컴퓨터도 있잖아",
          "speaker_id": "2",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100015935.1.8",
          "form": "노트북에만 캠이 달려있아",
          "original_form": "노트북에만 캠이 달려있아",
          "speaker_id": "1",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100015935.1.9",
          "form": "컴퓨터에는 없어?",
          "original_form": "컴퓨터에는 없어?",
          "speaker_id": "2",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100015935.1.10",
          "form": "컴퓨터로 하려면 캠을 따로 사야해",
          "original_form": "컴퓨터로 하려면 캠을 따로 사야해",
          "speaker_id": "1",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100015935.1.11",
          "form": "하나 사",
          "original_form": "하나 사",
          "speaker_id": "2",
          "time": "20210826 03:11"
        },
        {
          "id": "MDRW2100015935.1.12",
          "form": "돈낭비 같은데 그냥 노트북 쓸래",
          "original_form": "돈낭비 같은데 그냥 노트북 쓸래",
          "speaker_id": "1",
          "time": "20210826 03:11"
        },
        {
          "id": "MDRW2100015935.1.13",
          "form": "그래 그럼",
          "original_form": "그래 그럼",
          "speaker_id": "2",
          "time": "20210826 03:11"
        },
        {
          "id": "MDRW2100015935.1.14",
          "form": "얼마나 쓰는데?",
          "original_form": "얼마나 쓰는데?",
          "speaker_id": "2",
          "time": "20210826 03:11"
        },
        {
          "id": "MDRW2100015935.1.15",
          "form": "거의 매일 써야해",
          "original_form": "거의 매일 써야해",
          "speaker_id": "1",
          "time": "20210826 03:11"
        },
        {
          "id": "MDRW2100015935.1.16",
          "form": "모든 과목이 다 실시간 강의야?",
          "original_form": "모든 과목이 다 실시간 강의야?",
          "speaker_id": "2",
          "time": "20210826 03:11"
        },
        {
          "id": "MDRW2100015935.1.17",
          "form": "아니 두과목 빼고 다",
          "original_form": "아니 두과목 빼고 다",
          "speaker_id": "1",
          "time": "20210826 03:11"
        },
        {
          "id": "MDRW2100015935.1.18",
          "form": "몇시부터 몇시까지 하는데?",
          "original_form": "몇시부터 몇시까지 하는데?",
          "speaker_id": "2",
          "time": "20210826 03:12"
        },
        {
          "id": "MDRW2100015935.1.19",
          "form": "매일 달라",
          "original_form": "매일 달라",
          "speaker_id": "1",
          "time": "20210826 03:12"
        },
        {
          "id": "MDRW2100015935.1.20",
          "form": "평일에만 하는거지?",
          "original_form": "평일에만 하는거지?",
          "speaker_id": "2",
          "time": "20210826 03:12"
        },
        {
          "id": "MDRW2100015935.1.21",
          "form": "응 평일에는 수업듣고 주말에는 과제해야해",
          "original_form": "응 평일에는 수업듣고 주말에는 과제해야해",
          "speaker_id": "1",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015935.1.22",
          "form": "과제는 노트북 없어도 되지?",
          "original_form": "과제는 노트북 없어도 되지?",
          "speaker_id": "2",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015935.1.23",
          "form": "응응 과제는 컴퓨터로 해도 돼",
          "original_form": "응응 과제는 컴퓨터로 해도 돼",
          "speaker_id": "1",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015935.1.24",
          "form": "저녁에는 노트북 줘야한다",
          "original_form": "저녁에는 노트북 줘야한다",
          "speaker_id": "2",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015935.1.25",
          "form": "알겠야",
          "original_form": "알겠야",
          "speaker_id": "1",
          "time": "20210826 03:13"
        }
      ]
    }
