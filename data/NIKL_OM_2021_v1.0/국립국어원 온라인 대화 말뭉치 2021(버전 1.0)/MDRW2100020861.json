          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020861.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:33"
        },
        {
          "id": "MDRW2100020861.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그치만 엠버는 성능이 구린걸...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그치만 엠버는 성능이 구린걸...",
          "speaker_id": "1",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020861.1.5",
          "form": "부정할수없지.....",
          "original_form": "부정할수없지.....",
          "speaker_id": "2",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020861.1.6",
          "form": "아무리 기본지급캐라고 해도",
          "original_form": "아무리 기본지급캐라고 해도",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020861.1.7",
          "form": "리사보다도 효율이 별로인거같아",
          "original_form": "리사보다도 효율이 별로인거같아",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020861.1.8",
          "form": "노엘은 6돌파하면 은근 쓸만하대",
          "original_form": "노엘은 6돌파하면 은근 쓸만하대",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020861.1.9",
          "form": "ㅇㅇ아무리 기본지급캐여도",
          "original_form": "ㅇㅇ아무리 기본지급캐여도",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020861.1.10",
          "form": "주인공보다 구리면",
          "original_form": "주인공보다 구리면",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020861.1.11",
          "form": "6돌파???",
          "original_form": "6돌파???",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020861.1.12",
          "form": "하면",
          "original_form": "하면",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020861.1.13",
          "form": "안쓸만한 애",
          "original_form": "안쓸만한 애",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020861.1.14",
          "form": "엠버말고잇을가",
          "original_form": "엠버말고잇을가",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020861.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020861.1.16",
          "form": "아냐 메인 딜러로 쓸만하대",
          "original_form": "아냐 메인 딜러로 쓸만하대",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020861.1.17",
          "form": "노엘?",
          "original_form": "노엘?",
          "speaker_id": "2",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020861.1.18",
          "form": "딜러??",
          "original_form": "딜러??",
          "speaker_id": "2",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020861.1.19",
          "form": "4성이 아니라 5성들이랑 비교해서도 나름 괜찮은가봐",
          "original_form": "4성이 아니라 5성들이랑 비교해서도 나름 괜찮은가봐",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020861.1.20",
          "form": "ㅇㅇ 대검캐라서",
          "original_form": "ㅇㅇ 대검캐라서",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020861.1.21",
          "form": "와 누가봐도 탱커같은 스킬셋인데",
          "original_form": "와 누가봐도 탱커같은 스킬셋인데",
          "speaker_id": "2",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020861.1.22",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020861.1.23",
          "form": "방어막 두르고 평타로 미친듯이 후드리는건가",
          "original_form": "방어막 두르고 평타로 미친듯이 후드리는건가",
          "speaker_id": "2",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020861.1.24",
          "form": "그 비슷해보임",
          "original_form": "그 비슷해보임",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020861.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그치만",
