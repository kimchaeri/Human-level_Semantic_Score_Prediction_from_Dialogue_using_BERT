      "utterance": [
        {
          "id": "MDRW2100044463.1.1",
          "form": "그건 상대가 좋아서지",
          "original_form": "그건 상대가 좋아서지",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044463.1.2",
          "form": "애를 낳고 싶어서는 아닌데...",
          "original_form": "애를 낳고 싶어서는 아닌데...",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044463.1.3",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044463.1.4",
          "form": "상대가 좋아서 상대릉 닮은 애를 낳고 싶다는 건 엄청난 희생인데",
          "original_form": "상대가 좋아서 상대릉 닮은 애를 낳고 싶다는 건 엄청난 희생인데",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044463.1.5",
          "form": "ㅇㅇ 그건 아무리 포장해도 희생임",
          "original_form": "ㅇㅇ 그건 아무리 포장해도 희생임",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044463.1.6",
          "form": "ㅇㅇ 희생이지",
          "original_form": "ㅇㅇ 희생이지",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044463.1.7",
          "form": "내몸이 아픈건 어쩔것이야",
          "original_form": "내몸이 아픈건 어쩔것이야",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044463.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044463.1.9",
          "form": "ㅇㅇ 그래서 난 사실",
          "original_form": "ㅇㅇ 그래서 난 사실",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044463.1.10",
          "form": "모성과 출산은 위대하다 어쩌구",
          "original_form": "모성과 출산은 위대하다 어쩌구",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044463.1.11",
          "form": "하는 글 보면 ㅇ알레르기가 돋아..",
          "original_form": "하는 글 보면 ㅇ알레르기가 돋아..",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044463.1.12",
          "form": "위대하면",
          "original_form": "위대하면",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044463.1.13",
          "form": "존중부터 해주시지",
          "original_form": "존중부터 해주시지",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044463.1.14",
          "form": "텍사스 낙태법 개정된거 들을 때마다 소름인데",
          "original_form": "텍사스 낙태법 개정된거 들을 때마다 소름인데",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044463.1.15",
          "form": "ㅇㅇ 우리나라도 그럴까봐 무서움",
          "original_form": "ㅇㅇ 우리나라도 그럴까봐 무서움",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044463.1.16",
          "form": "그런 법 생기면 더 연애 못할듯",
          "original_form": "그런 법 생기면 더 연애 못할듯",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044463.1.17",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044463.1.18",
          "form": "낙태 기준이 어떤거라도 안된다는건데(원인이",
          "original_form": "낙태 기준이 어떤거라도 안된다는건데(원인이",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044463.1.19",
          "form": "(나쁜말 됨 짜증",
          "original_form": "(나쁜말 됨 짜증",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044463.1.20",
          "form": "그럼 우리나라 지옥이야",
          "original_form": "그럼 우리나라 지옥이야",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044463.1.21",
          "form": "그치 ㅋㅋㅋㅋㅋ 가뜩이나 사회 분위기가 이런데...",
          "original_form": "그치 ㅋㅋㅋㅋㅋ 가뜩이나 사회 분위기가 이런데...",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044463.1.22",
          "form": "연애하기도 무서운 나라 될듯",
          "original_form": "연애하기도 무서운 나라 될듯",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044463.1.23",
          "form": "ㅋㅋ큐ㅠㅠㅠㅠ 진짜..",
          "original_form": "ㅋㅋ큐ㅠㅠㅠㅠ 진짜..",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044463.1.24",
          "form": "무서워서 못나가는 대한민국 될듯",
          "original_form": "무서워서 못나가는 대한민국 될듯",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044463.1.25",
          "form": "치안 좋다는 건 다 거짓말이야 이게 결혼이랑 상관이 있나 암튼...",
          "original_form": "치안 좋다는 건 다 거짓말이야 이게 결혼이랑 상관이 있나 암튼...",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044463.1.26",
          "form": "그리고 당장에",
          "original_form": "그리고 당장에",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044463.1.27",
          "form": "난 애랑 상관없이",
          "original_form": "난 애랑 상관없이",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044463.1.28",
          "form": "출산도 결혼이랑 관련있다는 나라니까 낙태도 관련이야",
          "original_form": "출산도 결혼이랑 관련있다는 나라니까 낙태도 관련이야",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044463.1.29",
          "form": "그 사람이 좋아서 연애결혼 하고싶지",
          "original_form": "그 사람이 좋아서 연애결혼 하고싶지",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044463.1.30",
          "form": "(급기야다",
          "original_form": "(급기야다",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044463.1.31",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044463.1.32",
          "form": "애가 생겨서 못이긴척 하는 결혼은 싫음...",
          "original_form": "애가 생겨서 못이긴척 하는 결혼은 싫음...",
          "speaker_id": "1",
          "time": "20210907 23:09"
        }
      ]
