      "utterance": [
        {
          "id": "MDRW2100029239.1.1",
          "form": "최근뉴스 알려면",
          "original_form": "최근뉴스 알려면",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029239.1.2",
          "form": "옛날에는 12시에 민토 앞에서 봐요",
          "original_form": "옛날에는 12시에 민토 앞에서 봐요",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029239.1.3",
          "form": "커뮤니티가 좋은듯",
          "original_form": "커뮤니티가 좋은듯",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029239.1.4",
          "form": "근데 남 혐여 혐 은 중국인들이 많이 댓글 조작한데요",
          "original_form": "근데 남 혐여 혐 은 중국인들이 많이 댓글 조작한데요",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029239.1.5",
          "form": "정말 그런가요",
          "original_form": "정말 그런가요",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029239.1.6",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029239.1.7",
          "form": "잠만",
          "original_form": "잠만",
          "speaker_id": "2",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029239.1.8",
          "form": "조선인들인가요",
          "original_form": "조선인들인가요",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029239.1.9",
          "form": "내가알기론",
          "original_form": "내가알기론",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029239.1.10",
          "form": "남혐은",
          "original_form": "남혐은",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029239.1.11",
          "form": "남자혐오라기보다는",
          "original_form": "남자혐오라기보다는",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029239.1.12",
          "form": "평소 여성들이 받는 차별을 극복하자는",
          "original_form": "평소 여성들이 받는 차별을 극복하자는",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029239.1.13",
          "form": "엄 코라는 말도 웃기다",
          "original_form": "엄 코라는 말도 웃기다",
          "speaker_id": "1",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029239.1.14",
          "form": "근데 요새는 워낙 워낙 많이 써서",
          "original_form": "근데 요새는 워낙 워낙 많이 써서",
          "speaker_id": "1",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029239.1.15",
          "form": "실제로 조작도 엄청 한데요",
          "original_form": "실제로 조작도 엄청 한데요",
          "speaker_id": "1",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029239.1.16",
          "form": "페미니즘 이 여성위주 커뮤에서많이 퍼져서",
          "original_form": "페미니즘 이 여성위주 커뮤에서많이 퍼져서",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029239.1.17",
          "form": "남자들하는행동 미러링해서",
          "original_form": "남자들하는행동 미러링해서",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029239.1.18",
          "form": "그걸로 정치 이슈화시키는 거죠",
          "original_form": "그걸로 정치 이슈화시키는 거죠",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029239.1.19",
          "form": "남혐처럼보인거고",
          "original_form": "남혐처럼보인거고",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029239.1.20",
          "form": "ㅇㅇ그건 커뮤통해서많이 이뤄지고 있고",
          "original_form": "ㅇㅇ그건 커뮤통해서많이 이뤄지고 있고",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029239.1.21",
          "form": "조선족들도 활동많이하는데",
          "original_form": "조선족들도 활동많이하는데",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029239.1.22",
          "form": "나쁜 사람들이 많아요",
          "original_form": "나쁜 사람들이 많아요",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029239.1.23",
          "form": "조선족 싫어요",
          "original_form": "조선족 싫어요",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029239.1.24",
          "form": "중국문화 관련",
          "original_form": "중국문화 관련",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029239.1.25",
          "form": "중국인더 실행",
          "original_form": "중국인더 실행",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029239.1.26",
          "form": "마라탕 인기많다는글",
          "original_form": "마라탕 인기많다는글",
          "speaker_id": "2",
