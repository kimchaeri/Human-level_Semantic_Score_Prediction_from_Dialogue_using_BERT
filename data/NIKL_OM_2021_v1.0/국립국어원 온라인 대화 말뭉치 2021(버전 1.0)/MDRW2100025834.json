      "utterance": [
        {
          "id": "MDRW2100025834.1.1",
          "form": "나 월요일에 우산샀어",
          "original_form": "나 월요일에 우산샀어",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025834.1.2",
          "form": "우산?? 어떤 우산?",
          "original_form": "우산?? 어떤 우산?",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025834.1.3",
          "form": "장우산 마린?스타일 내 양산이랑 비슷해",
          "original_form": "장우산 마린?스타일 내 양산이랑 비슷해",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025834.1.4",
          "form": "어떤 디자인으로 샀어?? 마린 스타일이 뭐지..?",
          "original_form": "어떤 디자인으로 샀어?? 마린 스타일이 뭐지..?",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025834.1.5",
          "form": "너도 보면알텐데 민무늬에 가장자리에 두 줄이 그려져있어",
          "original_form": "너도 보면알텐데 민무늬에 가장자리에 두 줄이 그려져있어",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025834.1.6",
          "form": "마린룩처럼",
          "original_form": "마린룩처럼",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025834.1.7",
          "form": "아아아 알겠다알겠다",
          "original_form": "아아아 알겠다알겠다",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025834.1.8",
          "form": "ㅋㅋㅋ 사실 내 예쁜 우산이 있었는데 엄마가 갖고다녀서..새로 샀지",
          "original_form": "ㅋㅋㅋ 사실 내 예쁜 우산이 있었는데 엄마가 갖고다녀서..새로 샀지",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025834.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 우산이 탐나셨나봐",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 우산이 탐나셨나봐",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025834.1.10",
          "form": "마장 진짜 귀여웠거든 근데 그 사이트에서 우산이 단종돼서 더이상 못사..",
          "original_form": "마장 진짜 귀여웠거든 근데 그 사이트에서 우산이 단종돼서 더이상 못사..",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025834.1.11",
          "form": "나는 디자인 우산.. 남들이 들고 다니는거 보면 귀엽다 예쁘다 싶은데 나는 걍 민무늬 단색 우산 쓰게 되더라구",
          "original_form": "나는 디자인 우산.. 남들이 들고 다니는거 보면 귀엽다 예쁘다 싶은데 나는 걍 민무늬 단색 우산 쓰게 되더라구",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025834.1.12",
          "form": "단종된 우산은 아쉽겠다",
          "original_form": "단종된 우산은 아쉽겠다",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025834.1.13",
          "form": "맞아 전에 너한테 보여준적 있을듯",
          "original_form": "맞아 전에 너한테 보여준적 있을듯",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025834.1.14",
          "form": "어쩐지 봤던 거 같아",
          "original_form": "어쩐지 봤던 거 같아",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025834.1.15",
          "form": "음 다음에 새우산 오면 보여줄게",
          "original_form": "음 다음에 새우산 오면 보여줄게",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025834.1.16",
          "form": "너 앞으로 사고싶은거 뭐있어?",
          "original_form": "너 앞으로 사고싶은거 뭐있어?",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100025834.1.17",
          "form": "당장 사고싶은거",
          "original_form": "당장 사고싶은거",
          "speaker_id": "2",
          "time": "20210901 19:39"
        }
