      "utterance": [
        {
          "id": "MDRW2100005644.1.1",
          "form": "혼자서 자취하시나요?",
          "original_form": "혼자서 자취하시나요?",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005644.1.2",
          "form": "자취는 아닌데 조부모님이랑 살고 있어요",
          "original_form": "자취는 아닌데 조부모님이랑 살고 있어요",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005644.1.3",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005644.1.4",
          "form": "저는 학기중에는 자취하고",
          "original_form": "저는 학기중에는 자취하고",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005644.1.5",
          "form": "방학때는 잠깐 본가 내려와있어요",
          "original_form": "방학때는 잠깐 본가 내려와있어요",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005644.1.6",
          "form": "자취하면 어떤가요?",
          "original_form": "자취하면 어떤가요?",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005644.1.7",
          "form": "자취하고 싶은데 제가 저를 못 챙길 거 같아서 망설여져요",
          "original_form": "자취하고 싶은데 제가 저를 못 챙길 거 같아서 망설여져요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005644.1.8",
          "form": "음 그런면도 있죠",
          "original_form": "음 그런면도 있죠",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005644.1.9",
          "form": "집안일이 워낙 시간이 드는 일이라",
          "original_form": "집안일이 워낙 시간이 드는 일이라",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005644.1.10",
          "form": "시간이 부족하고 쫓기면 그만큼 신경쓰는게 힘든거같아요ㅜ",
          "original_form": "시간이 부족하고 쫓기면 그만큼 신경쓰는게 힘든거같아요ㅜ",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005644.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005644.1.12",
          "form": "지금도 청소랑 설거지 정도는 하는데",
          "original_form": "지금도 청소랑 설거지 정도는 하는데",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005644.1.13",
          "form": "시험기간이나 바쁘면 점점 안 해서",
          "original_form": "시험기간이나 바쁘면 점점 안 해서",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005644.1.14",
          "form": "엉망진창으로 살까봐...ㅋㅋㅋ",
          "original_form": "엉망진창으로 살까봐...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005644.1.15",
          "form": "그쵸ㅜㅜ",
          "original_form": "그쵸ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005644.1.16",
          "form": "전그래서 mbti 말고 방bti라고",
          "original_form": "전그래서 mbti 말고 방bti라고",
          "speaker_id": "2",
