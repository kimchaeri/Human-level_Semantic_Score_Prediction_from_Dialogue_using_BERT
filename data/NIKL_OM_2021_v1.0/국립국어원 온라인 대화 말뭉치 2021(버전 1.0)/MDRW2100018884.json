      "utterance": [
        {
          "id": "MDRW2100018884.1.1",
          "form": "안녕하세요 식사하셨나요?",
          "original_form": "안녕하세요 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018884.1.2",
          "form": "지금요!",
          "original_form": "지금요!",
          "speaker_id": "2",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018884.1.3",
          "form": "김밥이욤",
          "original_form": "김밥이욤",
          "speaker_id": "2",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018884.1.4",
          "form": "어떤 김밥 드시나요!",
          "original_form": "어떤 김밥 드시나요!",
          "speaker_id": "1",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018884.1.5",
          "form": "그냥 편의점 주먹밥입니딩",
          "original_form": "그냥 편의점 주먹밥입니딩",
          "speaker_id": "2",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100018884.1.6",
          "form": "뭐드셔욤",
          "original_form": "뭐드셔욤",
          "speaker_id": "2",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100018884.1.7",
          "form": "저는 닭볶음탕이랑 먹었어요!",
          "original_form": "저는 닭볶음탕이랑 먹었어요!",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018884.1.8",
          "form": "닭고기 좋아하시나요?",
          "original_form": "닭고기 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018884.1.9",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018884.1.10",
          "form": "닭요리는 다 좋아요",
          "original_form": "닭요리는 다 좋아요",
          "speaker_id": "2",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018884.1.11",
          "form": "치킨도 좋아하시나요?",
          "original_form": "치킨도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018884.1.12",
          "form": "아 당연하죠",
          "original_form": "아 당연하죠",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018884.1.13",
          "form": "누가 싫어하죠",
          "original_form": "누가 싫어하죠",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018884.1.14",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018884.1.15",
          "form": "어디서 자주 시켜드세요?",
          "original_form": "어디서 자주 시켜드세요?",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018884.1.16",
          "form": "전 근데 뼈잇는게 더 맛있는거 같아요",
          "original_form": "전 근데 뼈잇는게 더 맛있는거 같아요",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018884.1.17",
          "form": "그냥 사먹기도하고",
          "original_form": "그냥 사먹기도하고",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018884.1.18",
          "form": "뼈있는 게 더 맛있죠!",
          "original_form": "뼈있는 게 더 맛있죠!",
          "speaker_id": "1",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018884.1.19",
          "form": "저는 bhc에서 자주 시켜먹는 거 같아요",
          "original_form": "저는 bhc에서 자주 시켜먹는 거 같아요",
          "speaker_id": "1",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018884.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018884.1.21",
          "form": "저는 맛있는건 교촌인거 같아요",
          "original_form": "저는 맛있는건 교촌인거 같아요",
          "speaker_id": "2",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018884.1.22",
          "form": "최근에는 귭네 많이 먹었고요",
          "original_form": "최근에는 귭네 많이 먹었고요",
          "speaker_id": "2",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018884.1.23",
          "form": "저도 굽네 고 추바사삭 맛있게 먹었어요!",
          "original_form": "저도 굽네 고 추바사삭 맛있게 먹었어요!",
          "speaker_id": "1",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018884.1.24",
          "form": "교촌에서는 어떤 메뉴 시켜드시나요?",
          "original_form": "교촌에서는 어떤 메뉴 시켜드시나요?",
          "speaker_id": "1",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018884.1.25",
          "form": "레드요",
          "original_form": "레드요",
          "speaker_id": "2",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018884.1.26",
          "form": "존.맛.탱",
          "original_form": "존.맛.탱",
          "speaker_id": "2",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018884.1.27",
          "form": "저는 매운거 좋아하거든요",
          "original_form": "저는 매운거 좋아하거든요",
          "speaker_id": "2",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018884.1.28",
          "form": "매운거 잘 드세요?",
          "original_form": "매운거 잘 드세요?",
          "speaker_id": "2",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018884.1.29",
          "form": "네 좋아해요! 굽네 레드 맛있죠",
          "original_form": "네 좋아해요! 굽네 레드 맛있죠",
          "speaker_id": "1",
          "time": "20210827 13:27"
        }
      ]
    }
