      "utterance": [
        {
          "id": "MDRW2100033041.1.1",
          "form": "대상포진은 주의해야할 음식 있나요?",
          "original_form": "대상포진은 주의해야할 음식 있나요?",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033041.1.2",
          "form": "먹는 것과는 상관이 없고",
          "original_form": "먹는 것과는 상관이 없고",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033041.1.3",
          "form": "면역력만 관리 잘하면 됩니다",
          "original_form": "면역력만 관리 잘하면 됩니다",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033041.1.4",
          "form": "통풍은 고기를 조심해야 한다고 들었어요",
          "original_form": "통풍은 고기를 조심해야 한다고 들었어요",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100033041.1.5",
          "form": "맞습니까?",
          "original_form": "맞습니까?",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100033041.1.6",
          "form": "네 소고기 해산물 이런 거 안좋아요",
          "original_form": "네 소고기 해산물 이런 거 안좋아요",
          "speaker_id": "1",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100033041.1.7",
          "form": "다 제가 좋아하는 것들이네요",
          "original_form": "다 제가 좋아하는 것들이네요",
          "speaker_id": "2",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100033041.1.8",
          "form": "통풍 안걸리게 조심해야겠어요",
          "original_form": "통풍 안걸리게 조심해야겠어요",
          "speaker_id": "2",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100033041.1.9",
          "form": "삼비마님도 대상포진 조심하세요",
          "original_form": "삼비마님도 대상포진 조심하세요",
          "speaker_id": "2",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100033041.1.10",
          "form": "먹는거와는 상관없지만  삶의 질이 떨어지네요",
          "original_form": "먹는거와는 상관없지만  삶의 질이 떨어지네요",
          "speaker_id": "2",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100033041.1.11",
          "form": "네 통풍도 질 엄청 떨어져요 맛있는걸 못 먹어요ㅜㅜ",
          "original_form": "네 통풍도 질 엄청 떨어져요 맛있는걸 못 먹어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100033041.1.12",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100033041.1.13",
          "form": "특히 좋아했던 음식이면 더 그렇겠습니다",
          "original_form": "특히 좋아했던 음식이면 더 그렇겠습니다",
          "speaker_id": "2",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100033041.1.14",
          "form": "네 그런것들을 엄청 먹어서 걸린 거 같기도해요ㅜㅜ",
          "original_form": "네 그런것들을 엄청 먹어서 걸린 거 같기도해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100033041.1.15",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "2",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100033041.1.16",
          "form": "저도 조심해야겠네요",
          "original_form": "저도 조심해야겠네요",
          "speaker_id": "2",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100033041.1.17",
          "form": "방금 저녁도 생선요리 먹었는데",
          "original_form": "방금 저녁도 생선요리 먹었는데",
          "speaker_id": "2",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100033041.1.18",
          "form": "생선은 괜찮죠ㅋ 일찍퇴근하셨네요ㅋ",
          "original_form": "생선은 괜찮죠ㅋ 일찍퇴근하셨네요ㅋ",
          "speaker_id": "1",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100033041.1.19",
          "form": "저는 은퇴자라 집에 있답니다",
          "original_form": "저는 은퇴자라 집에 있답니다",
          "speaker_id": "2",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100033041.1.20",
          "form": "헛... 그러시군요! 죄송합니다ㅜ",
          "original_form": "헛... 그러시군요! 죄송합니다ㅜ",
          "speaker_id": "1",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100033041.1.21",
          "form": "죄송할께 무엇이 있을까요",
          "original_form": "죄송할께 무엇이 있을까요",
          "speaker_id": "2",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100033041.1.22",
          "form": "괜찮습니다",
          "original_form": "괜찮습니다",
          "speaker_id": "2",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100033041.1.23",
          "form": "은퇴할 나이이지요",
          "original_form": "은퇴할 나이이지요",
          "speaker_id": "2",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100033041.1.24",
          "form": "아 그러시군요...",
          "original_form": "아 그러시군요...",
          "speaker_id": "1",
          "time": "20210906 22:15"
        }
      ]
    }
