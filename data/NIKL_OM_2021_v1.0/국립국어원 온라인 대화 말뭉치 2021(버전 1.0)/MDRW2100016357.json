      "utterance": [
        {
          "id": "MDRW2100016357.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016357.1.2",
          "form": "이번 올림픽 재밌게 보셨나요",
          "original_form": "이번 올림픽 재밌게 보셨나요",
          "speaker_id": "1",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016357.1.3",
          "form": "안녕하세요! 네네 너무 재밌었어요 ㅠㅠ",
          "original_form": "안녕하세요! 네네 너무 재밌었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016357.1.4",
          "form": "배구 가장 기억에 남아요 ㅠㅠ",
          "original_form": "배구 가장 기억에 남아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016357.1.5",
          "form": "저도 배구 너무 재밌게 봤어요 ㅜㅜ",
          "original_form": "저도 배구 너무 재밌게 봤어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016357.1.6",
          "form": "그쳐 ㅠㅠ진짜 원래 배구 관심없었거든요",
          "original_form": "그쳐 ㅠㅠ진짜 원래 배구 관심없었거든요",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016357.1.7",
          "form": "보다가 자세히 찾아보고 그러니까 감동적이엇어요",
          "original_form": "보다가 자세히 찾아보고 그러니까 감동적이엇어요",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016357.1.8",
          "form": "맞아요 저도 배구 잘 몰랐는데 이번 기회에 관심 많이 갖게 되었어요",
          "original_form": "맞아요 저도 배구 잘 몰랐는데 이번 기회에 관심 많이 갖게 되었어요",
          "speaker_id": "1",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016357.1.9",
          "form": "맞아요...김연경선수 진짜 대단하더라구요",
          "original_form": "맞아요...김연경선수 진짜 대단하더라구요",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016357.1.10",
          "form": "나혼산 때문에 처음 알게됫는데",
          "original_form": "나혼산 때문에 처음 알게됫는데",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016357.1.11",
          "form": "맞아요 김연경 선수 너무 멋있고 ㅜㅜ",
          "original_form": "맞아요 김연경 선수 너무 멋있고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100016357.1.12",
          "form": "맞아요 ㅠㅠ 다른선수들도 관심가지게되더라구요",
          "original_form": "맞아요 ㅠㅠ 다른선수들도 관심가지게되더라구요",
          "speaker_id": "2",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100016357.1.13",
          "form": "양궁은 진짜 희열감최고엿어요",
          "original_form": "양궁은 진짜 희열감최고엿어요",
          "speaker_id": "2",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100016357.1.14",
          "form": "양궁은 정말 경기가 속도감 있고 긴장돼서 더 재힜는거 같아요",
          "original_form": "양궁은 정말 경기가 속도감 있고 긴장돼서 더 재힜는거 같아요",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016357.1.15",
          "form": "맞아요 ㅠㅠ꿀잼이엇어요",
          "original_form": "맞아요 ㅠㅠ꿀잼이엇어요",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016357.1.16",
          "form": "쿵야라고 부르는것도 넘 귀엽구 ㅋㅋㅋㅋ",
          "original_form": "쿵야라고 부르는것도 넘 귀엽구 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016357.1.17",
          "form": "ㅋㅋㅋㅋ 맞아요 선수들 쿵야라고 부르는거 너무 재밌어요 ㄴㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 맞아요 선수들 쿵야라고 부르는거 너무 재밌어요 ㄴㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016357.1.18",
          "form": "그죠 저는 수면쿵야 너무 웃겻어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그죠 저는 수면쿵야 너무 웃겻어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016357.1.19",
          "form": "악 ㅋㅋ ㅋ 어떻게 올림픽 경기에서 안정적인 심박수를 유지하는건지 ㅋㅋㅋ",
          "original_form": "악 ㅋㅋ ㅋ 어떻게 올림픽 경기에서 안정적인 심박수를 유지하는건지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016357.1.20",
          "form": "너무 신기해요",
          "original_form": "너무 신기해요",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016357.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ그러니까요 얼마전에 라스 봤는데 여자친구얘기에는 엄청 올라가더라구욬ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러니까요 얼마전에 라스 봤는데 여자친구얘기에는 엄청 올라가더라구욬ㅋㅋㅋㅋㅋ",
