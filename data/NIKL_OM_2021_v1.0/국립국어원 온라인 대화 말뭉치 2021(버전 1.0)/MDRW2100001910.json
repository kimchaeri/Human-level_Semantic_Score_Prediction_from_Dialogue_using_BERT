      "utterance": [
        {
          "id": "MDRW2100001910.1.1",
          "form": "날씨 선선하니 운동하기 좋을거같은데, name2님 하시는 운동 있으세요?",
          "original_form": "날씨 선선하니 운동하기 좋을거같은데, &name2&님 하시는 운동 있으세요?",
          "speaker_id": "1",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001910.1.2",
          "form": "저는 필라테스 하고있어요",
          "original_form": "저는 필라테스 하고있어요",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001910.1.3",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001910.1.4",
          "form": "오 저도 코로나직전까지 필라테스 했어요 ㅋㅋㅋ",
          "original_form": "오 저도 코로나직전까지 필라테스 했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001910.1.5",
          "form": "필라테스 재밌죠!!",
          "original_form": "필라테스 재밌죠!!",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001910.1.6",
          "form": "너무요! 운동안좋아하는데 첨으로 저랑 맞는 운동인거같았어요ㅋㅋ",
          "original_form": "너무요! 운동안좋아하는데 첨으로 저랑 맞는 운동인거같았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001910.1.7",
          "form": "저도요!! 자세교정도 되고",
          "original_form": "저도요!! 자세교정도 되고",
          "speaker_id": "2",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001910.1.8",
          "form": "저도 잘 맞는것같아요",
          "original_form": "저도 잘 맞는것같아요",
          "speaker_id": "2",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001910.1.9",
          "form": "허리아팠던것도 필라테스하니 좋아지더라구요ㅎㅎ",
          "original_form": "허리아팠던것도 필라테스하니 좋아지더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001910.1.10",
          "form": "저는 어깨가 아파서 시작했는데",
          "original_form": "저는 어깨가 아파서 시작했는데",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001910.1.11",
          "form": "많이 좋아진것같아요",
          "original_form": "많이 좋아진것같아요",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001910.1.12",
          "form": "역시 자기한테 맞는 운동을 꾸준히 하는게 좋은거같아요ㅎㅎ",
          "original_form": "역시 자기한테 맞는 운동을 꾸준히 하는게 좋은거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001910.1.13",
          "form": "등산도 시작해보려고 하는데",
          "original_form": "등산도 시작해보려고 하는데",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001910.1.14",
          "form": "주말만되면 의지가 사라져요ㅎㅎ",
          "original_form": "주말만되면 의지가 사라져요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001910.1.15",
          "form": "운동 좋아하시는거예요? 등산이라니.... 대단하세요ㅎㅎ",
          "original_form": "운동 좋아하시는거예요? 등산이라니.... 대단하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001910.1.16",
          "form": "운동 좋아하려고 노력중이에요ㅎㅎㅎ",
          "original_form": "운동 좋아하려고 노력중이에요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001910.1.17",
          "form": "그렇구나 ㅎㅎ 저도 필라테스 다시 시작하려구요ㅎㅎ",
          "original_form": "그렇구나 ㅎㅎ 저도 필라테스 다시 시작하려구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001910.1.18",
          "form": "같이 화이팅해요!!!",
          "original_form": "같이 화이팅해요!!!",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001910.1.19",
