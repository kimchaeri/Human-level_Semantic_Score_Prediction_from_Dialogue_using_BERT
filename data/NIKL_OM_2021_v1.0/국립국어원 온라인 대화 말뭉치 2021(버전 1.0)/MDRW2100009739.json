      "utterance": [
        {
          "id": "MDRW2100009739.1.1",
          "form": "결혼생활은 햄볶나요?",
          "original_form": "결혼생활은 햄볶나요?",
          "speaker_id": "2",
          "time": "20210823 10:34"
        },
        {
          "id": "MDRW2100009739.1.2",
          "form": "나름 햄볶는중",
          "original_form": "나름 햄볶는중",
          "speaker_id": "1",
          "time": "20210823 11:29"
        },
        {
          "id": "MDRW2100009739.1.3",
          "form": "일단 시댁과 마찰이 없는것으로 만족중",
          "original_form": "일단 시댁과 마찰이 없는것으로 만족중",
          "speaker_id": "1",
          "time": "20210823 11:29"
        },
        {
          "id": "MDRW2100009739.1.4",
          "form": "ㅋㅋ좋네여",
          "original_form": "ㅋㅋ좋네여",
          "speaker_id": "2",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009739.1.5",
          "form": "ㅋㅋ 시댁이랑 마찰이 있다면 진짜 헬",
          "original_form": "ㅋㅋ 시댁이랑 마찰이 있다면 진짜 헬",
          "speaker_id": "1",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009739.1.6",
          "form": "ㅋㅋ맞아 그럼 사랑과전쟁",
          "original_form": "ㅋㅋ맞아 그럼 사랑과전쟁",
          "speaker_id": "2",
          "time": "20210823 11:54"
        },
        {
          "id": "MDRW2100009739.1.7",
          "form": "날마다 우울할듯",
          "original_form": "날마다 우울할듯",
          "speaker_id": "1",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009739.1.8",
          "form": "근데 그런집 많더라",
          "original_form": "근데 그런집 많더라",
          "speaker_id": "1",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009739.1.9",
          "form": "ㅋㅋ생각보다요",
          "original_form": "ㅋㅋ생각보다요",
          "speaker_id": "2",
          "time": "20210823 12:08"
        },
        {
          "id": "MDRW2100009739.1.10",
          "form": "완전 자기들 멋대로 종살이 시키는 시댁",
          "original_form": "완전 자기들 멋대로 종살이 시키는 시댁",
          "speaker_id": "1",
          "time": "20210823 13:02"
        },
        {
          "id": "MDRW2100009739.1.11",
          "form": "ㅋㅋ시댁만 잘만나도 천국",
          "original_form": "ㅋㅋ시댁만 잘만나도 천국",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009739.1.12",
          "form": "맞아 안그럼 지침",
          "original_form": "맞아 안그럼 지침",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100009739.1.13",
          "form": "ㅋㅋ저도 중간은 가니까..",
          "original_form": "ㅋㅋ저도 중간은 가니까..",
          "speaker_id": "2",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100009739.1.14",
          "form": "친구네는 시어머니가 일주일에 2회는 오시니대",
          "original_form": "친구네는 시어머니가 일주일에 2회는 오시니대",
          "speaker_id": "1",
          "time": "20210823 13:58"
        },
        {
          "id": "MDRW2100009739.1.15",
          "form": "오신대",
          "original_form": "오신대",
          "speaker_id": "1",
          "time": "20210823 13:58"
        },
        {
          "id": "MDRW2100009739.1.16",
          "form": "시누 애데리고 ㄷㄷ",
          "original_form": "시누 애데리고 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210823 13:58"
        },
        {
          "id": "MDRW2100009739.1.17",
          "form": "엄청나다",
          "original_form": "엄청나다",
          "speaker_id": "2",
          "time": "20210823 14:00"
        },
        {
          "id": "MDRW2100009739.1.18",
          "form": "그래서 시간 때우다간대",
          "original_form": "그래서 시간 때우다간대",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100009739.1.19",
          "form": "밤까지",
          "original_form": "밤까지",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100009739.1.20",
          "form": "ㅋㅋ밤까지는.. 좋으시다",
          "original_form": "ㅋㅋ밤까지는.. 좋으시다",
          "speaker_id": "2",
          "time": "20210823 14:22"
        },
        {
          "id": "MDRW2100009739.1.21",
          "form": "아니 애를 안보고 ㅋㅋ",
          "original_form": "아니 애를 안보고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100009739.1.22",
          "form": "원래 친구 애들 2에 시누애 1까지 봐야한대",
          "original_form": "원래 친구 애들 2에 시누애 1까지 봐야한대",
          "speaker_id": "1",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100009739.1.23",
          "form": "밤까지 ㅋㅋㅋ",
          "original_form": "밤까지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100009739.1.24",
          "form": "ㅋㅋㅋㅋㅋ그니까ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:40"
        },
        {
          "id": "MDRW2100009739.1.25",
          "form": "너무 감사하네ㅋㅋ사랑과전쟁나가야해",
          "original_form": "너무 감사하네ㅋㅋ사랑과전쟁나가야해",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100009739.1.26",
          "form": "ㅋㅋ 애들이 소리내서 없는척도 못함",
          "original_form": "ㅋㅋ 애들이 소리내서 없는척도 못함",
          "speaker_id": "1",
          "time": "20210823 16:51"
        },
        {
          "id": "MDRW2100009739.1.27",
          "form": "ㅋㅋ그런 어머님은 없어서 다행",
          "original_form": "ㅋㅋ그런 어머님은 없어서 다행",
          "speaker_id": "2",
          "time": "20210823 17:26"
        },
        {
          "id": "MDRW2100009739.1.28",
          "form": "진짜로 ㅋㅋ 안그랬음 기절할뻔",
          "original_form": "진짜로 ㅋㅋ 안그랬음 기절할뻔",
          "speaker_id": "1",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100009739.1.29",
          "form": "ㅋㅋㅋ우리도 감사하며 살아야함",
          "original_form": "ㅋㅋㅋ우리도 감사하며 살아야함",
          "speaker_id": "2",
          "time": "20210823 18:00"
        },
        {
          "id": "MDRW2100009739.1.30",
          "form": "축복받은 시댁이라며 ㅎㅎ",
          "original_form": "축복받은 시댁이라며 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100009739.1.31",
          "form": "ㅋㅋ축복까지는..,",
          "original_form": "ㅋㅋ축복까지는..,",
          "speaker_id": "2",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100009739.1.32",
          "form": "아 그건 또 아니라고 ㅋㅋㅋ",
