      "utterance": [
        {
          "id": "MDRW2100004294.1.1",
          "form": "요즘 뭐 사고싶으세요?",
          "original_form": "요즘 뭐 사고싶으세요?",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004294.1.2",
          "form": "저 갤럭시워치 사고 싶은데 고민중이에요 고민하고 계신거 있으세요?",
          "original_form": "저 갤럭시워치 사고 싶은데 고민중이에요 고민하고 계신거 있으세요?",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004294.1.3",
          "form": "저 버즈2 오늘 사려고요!",
          "original_form": "저 버즈2 오늘 사려고요!",
          "speaker_id": "2",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004294.1.4",
          "form": "그 버즈 지플립케이스가 너무 귀여워요",
          "original_form": "그 버즈 지플립케이스가 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004294.1.5",
          "form": "우와 무슨색으로요? 저는 에어팟쓰고있어요ㅎㅎ",
          "original_form": "우와 무슨색으로요? 저는 에어팟쓰고있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004294.1.6",
          "form": "맞아요 케이스 모으는 재미ㅎㅎ",
          "original_form": "맞아요 케이스 모으는 재미ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004294.1.7",
          "form": "올리브랑 라벤더중에 고민중이에요ㅜㅜ",
          "original_form": "올리브랑 라벤더중에 고민중이에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004294.1.8",
          "form": "추천해주세요 색상!!",
          "original_form": "추천해주세요 색상!!",
          "speaker_id": "2",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004294.1.9",
          "form": "저는 올리브가 너무 이쁘던데ㅠㅠ그런 색깔이 나온거 너무 이뻐요ㅎㅎ",
          "original_form": "저는 올리브가 너무 이쁘던데ㅠㅠ그런 색깔이 나온거 너무 이뻐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004294.1.10",
          "form": "근데 맘에 드는걸루 하셔야 실증도 안나궁ㅎㅎ",
          "original_form": "근데 맘에 드는걸루 하셔야 실증도 안나궁ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004294.1.11",
          "form": "어제부터 고민중이었어요",
          "original_form": "어제부터 고민중이었어요",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004294.1.12",
          "form": "올리브 딱 그 완두콩",
          "original_form": "올리브 딱 그 완두콩",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004294.1.13",
          "form": "같아요",
          "original_form": "같아요",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004294.1.14",
          "form": "맞아요 귀엽더라구요ㅎㅎ",
          "original_form": "맞아요 귀엽더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004294.1.15",
          "form": "둘다 넘 기여워서..",
          "original_form": "둘다 넘 기여워서..",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004294.1.16",
          "form": "둘다사면 참좋은데ㅋㅋㅋㅋ",
          "original_form": "둘다사면 참좋은데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004294.1.17",
          "form": "통장 눈감아..쇼핑은 너무 좋지만..",
          "original_form": "통장 눈감아..쇼핑은 너무 좋지만..",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004294.1.18",
          "form": "저도 막 사고 싶네요ㅎㅎ",
          "original_form": "저도 막 사고 싶네요ㅎㅎ",
