      "utterance": [
        {
          "id": "MDRW2100041083.1.1",
          "form": "지방흡입 하고 싶어요 ㅋㅋㅋ",
          "original_form": "지방흡입 하고 싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100041083.1.2",
          "form": "ㅋㅋ다이어트하시는거예요?? ㅋㅋ",
          "original_form": "ㅋㅋ다이어트하시는거예요?? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100041083.1.3",
          "form": "음..아니요 다이어트 딱히 안해서",
          "original_form": "음..아니요 다이어트 딱히 안해서",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100041083.1.4",
          "form": "시술로 한방에!",
          "original_form": "시술로 한방에!",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100041083.1.5",
          "form": "지방 확",
          "original_form": "지방 확",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100041083.1.6",
          "form": "ㅋㅋ한탕ㅋㅋ",
          "original_form": "ㅋㅋ한탕ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100041083.1.7",
          "form": "어디하고싶으세요?",
          "original_form": "어디하고싶으세요?",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100041083.1.8",
          "form": "다리요",
          "original_form": "다리요",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100041083.1.9",
          "form": "코끼리임",
          "original_form": "코끼리임",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100041083.1.10",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100041083.1.11",
          "form": "아ㅜㅜ전 등살...",
          "original_form": "아ㅜㅜ전 등살...",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100041083.1.12",
          "form": "폼롤러하세요??",
          "original_form": "폼롤러하세요??",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100041083.1.13",
          "form": "ㅋ네네 해요 시원해요",
          "original_form": "ㅋ네네 해요 시원해요",
          "speaker_id": "1",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041083.1.14",
          "form": "ㅋㅋ그걸로 허벅지 조진 사람들 많더라고요",
          "original_form": "ㅋㅋ그걸로 허벅지 조진 사람들 많더라고요",
          "speaker_id": "2",
          "time": "20210905 17:50"
        },
        {
          "id": "MDRW2100041083.1.15",
          "form": "오잉 살도빠져요? ㅋㅋㅋ",
          "original_form": "오잉 살도빠져요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:59"
        },
        {
          "id": "MDRW2100041083.1.16",
          "form": "네 그렇다던데요ㅋㅋ",
          "original_form": "네 그렇다던데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:59"
        },
        {
          "id": "MDRW2100041083.1.17",
          "form": "사실 이소라가 최고래요ㅋㅋ",
          "original_form": "사실 이소라가 최고래요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:59"
        },
        {
          "id": "MDRW2100041083.1.18",
          "form": "컥...",
          "original_form": "컥...",
          "speaker_id": "1",
          "time": "20210905 18:00"
        },
        {
          "id": "MDRW2100041083.1.19",
          "form": "ㅋㅋ 근데 노래가 넘 구식이요",
          "original_form": "ㅋㅋ 근데 노래가 넘 구식이요",
          "speaker_id": "2",
          "time": "20210905 18:00"
        },
        {
          "id": "MDRW2100041083.1.20",
          "form": "이소라하세용?",
          "original_form": "이소라하세용?",
          "speaker_id": "1",
          "time": "20210905 18:01"
        },
        {
          "id": "MDRW2100041083.1.21",
          "form": "옛날에ㅋㅋ",
          "original_form": "옛날에ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:01"
        },
        {
          "id": "MDRW2100041083.1.22",
          "form": "ㅋㅋㅋ 효과잇엇어요?",
          "original_form": "ㅋㅋㅋ 효과잇엇어요?",
          "speaker_id": "1",
          "time": "20210905 18:02"
        },
        {
          "id": "MDRW2100041083.1.23",
          "form": "요즘은 유투브에 최신곡으로 된 영상이 많아서요ㅎ",
          "original_form": "요즘은 유투브에 최신곡으로 된 영상이 많아서요ㅎ",
          "speaker_id": "2",
          "time": "20210905 18:02"
        },
        {
          "id": "MDRW2100041083.1.24",
          "form": "있긴해요",
          "original_form": "있긴해요",
          "speaker_id": "2",
          "time": "20210905 18:02"
        },
        {
          "id": "MDRW2100041083.1.25",
          "form": "다음날 통증이ㅋㅋ",
          "original_form": "다음날 통증이ㅋㅋ",
