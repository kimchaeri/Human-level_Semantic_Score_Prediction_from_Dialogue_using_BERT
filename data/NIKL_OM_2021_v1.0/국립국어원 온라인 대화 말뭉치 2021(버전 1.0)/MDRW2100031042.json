      "utterance": [
        {
          "id": "MDRW2100031042.1.1",
          "form": "커피는 드시나요?",
          "original_form": "커피는 드시나요?",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100031042.1.2",
          "form": "아 저는 주로 고구마랑 포테이토요",
          "original_form": "아 저는 주로 고구마랑 포테이토요",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100031042.1.3",
          "form": "아 커피 잘안먹어요ㅜ",
          "original_form": "아 커피 잘안먹어요ㅜ",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100031042.1.4",
          "form": "포테이토 맛있죠 저도 자주시켜먹어요",
          "original_form": "포테이토 맛있죠 저도 자주시켜먹어요",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100031042.1.5",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100031042.1.6",
          "form": "아니용ㅎ",
          "original_form": "아니용ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100031042.1.7",
          "form": "포테이토는 어디서 주로 시켜드세요?",
          "original_form": "포테이토는 어디서 주로 시켜드세요?",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100031042.1.8",
          "form": "도미노요 할인할때만 시켜요",
          "original_form": "도미노요 할인할때만 시켜요",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031042.1.9",
          "form": "피자 넘 비싸서ㅜ",
          "original_form": "피자 넘 비싸서ㅜ",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031042.1.10",
          "form": "그쵸 도미노는 할인안하면 뭔가 손해보는 기분이에요",
          "original_form": "그쵸 도미노는 할인안하면 뭔가 손해보는 기분이에요",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031042.1.11",
          "form": "맞아요 요즘 피자집들이 다 그런거 같아요",
          "original_form": "맞아요 요즘 피자집들이 다 그런거 같아요",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031042.1.12",
          "form": "저렴한곳을 찾고싶은데 잘없네요",
          "original_form": "저렴한곳을 찾고싶은데 잘없네요",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031042.1.13",
          "form": "네 피자한판에 4만 넘는것도 있더라구요",
          "original_form": "네 피자한판에 4만 넘는것도 있더라구요",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031042.1.14",
          "form": "그럴바엔 뷔페를가지",
          "original_form": "그럴바엔 뷔페를가지",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031042.1.15",
          "form": "물가가 점점 올라요ㅜㅜ",
          "original_form": "물가가 점점 올라요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031042.1.16",
          "form": "내월급은 그대로인데 ㅜㅜㅜ",
          "original_form": "내월급은 그대로인데 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031042.1.17",
          "form": "우리나라가 유독 식재료가 비싼거같아요",
          "original_form": "우리나라가 유독 식재료가 비싼거같아요",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031042.1.18",
          "form": "우유도 비싸고",
          "original_form": "우유도 비싸고",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031042.1.19",
          "form": "맞아요 진짜",
          "original_form": "맞아요 진짜",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031042.1.20",
          "form": "장보면 금방 십만원 넘어요",
          "original_form": "장보면 금방 십만원 넘어요",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031042.1.21",
          "form": "근데 뭐 산게없고ㅠㅠ",
          "original_form": "근데 뭐 산게없고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031042.1.22",
          "form": "과자도 비싸고",
          "original_form": "과자도 비싸고",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031042.1.23",
          "form": "그니까요 ㅜㅜ",
          "original_form": "그니까요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031042.1.24",
          "form": "저렇게사도 일주일이면",
          "original_form": "저렇게사도 일주일이면",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031042.1.25",
          "form": "요즘애들 과자 비싸서 어떻게 먹나요ㅋㅋ",
          "original_form": "요즘애들 과자 비싸서 어떻게 먹나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031042.1.26",
          "form": "순삭이예요",
          "original_form": "순삭이예요",
          "speaker_id": "1",
          "time": "20210904 15:53"
        }
      ]
    }
