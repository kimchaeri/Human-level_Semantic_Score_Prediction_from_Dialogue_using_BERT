      "utterance": [
        {
          "id": "MDRW2100006016.1.1",
          "form": "다이어트 해보셨나요",
          "original_form": "다이어트 해보셨나요",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100006016.1.2",
          "form": "이제 해볼려구요ㅋㅋㅋ",
          "original_form": "이제 해볼려구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100006016.1.3",
          "form": "피티궁금해요",
          "original_form": "피티궁금해요",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100006016.1.4",
          "form": "근데 제 주변에 피티 경험자들 보면 딱 그때만 반짝하더라구요",
          "original_form": "근데 제 주변에 피티 경험자들 보면 딱 그때만 반짝하더라구요",
          "speaker_id": "2",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100006016.1.5",
          "form": "운동법이랑 자세 배워놓고싶은데!",
          "original_form": "운동법이랑 자세 배워놓고싶은데!",
          "speaker_id": "1",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100006016.1.6",
          "form": "다이어트해보셧어여?",
          "original_form": "다이어트해보셧어여?",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100006016.1.7",
          "form": "저도 학창시절 살이 쪄서 성인되서 빼고 유지하려고 노력해요",
          "original_form": "저도 학창시절 살이 쪄서 성인되서 빼고 유지하려고 노력해요",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100006016.1.8",
          "form": "가르쳐주세요ㅜㅜㅜㅜㅜㅜ",
          "original_form": "가르쳐주세요ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100006016.1.9",
          "form": "대박!!!!!",
          "original_form": "대박!!!!!",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100006016.1.10",
          "form": "성공하셧군요",
          "original_form": "성공하셧군요",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100006016.1.11",
          "form": "그냥 닭가슴살 먹고 운동하고 무식?하게 뺐죠...",
          "original_form": "그냥 닭가슴살 먹고 운동하고 무식?하게 뺐죠...",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100006016.1.12",
          "form": "제일 정석으로!",
          "original_form": "제일 정석으로!",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100006016.1.13",
          "form": "식단 운동 하셧군요",
          "original_form": "식단 운동 하셧군요",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100006016.1.14",
          "form": "네 그리고 지금은 그냥 먹지만 야식.간식은 안먹어요",
          "original_form": "네 그리고 지금은 그냥 먹지만 야식.간식은 안먹어요",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100006016.1.15",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100006016.1.16",
          "form": "의지력 짱짱",
          "original_form": "의지력 짱짱",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100006016.1.17",
          "form": "간식.야식은 습관인것 같긴해요...저는 어릴때부터 간식은 안먹어서..",
          "original_form": "간식.야식은 습관인것 같긴해요...저는 어릴때부터 간식은 안먹어서..",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100006016.1.18",
          "form": "몇기로정도 빼고 유지중이신가욤",
          "original_form": "몇기로정도 빼고 유지중이신가욤",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100006016.1.19",
          "form": "고3 야식먹던 최고 몸무게에서 10키로 이상 차이 날거예요...",
          "original_form": "고3 야식먹던 최고 몸무게에서 10키로 이상 차이 날거예요...",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006016.1.20",
          "form": "ㅋㅋㅋㅋ유지하시는게 진짜 대단하셔유",
