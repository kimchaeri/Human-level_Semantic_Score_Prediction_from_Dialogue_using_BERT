      "utterance": [
        {
          "id": "MDRW2100036673.1.1",
          "form": "아빠가 이번에 구피 잘 돌봐달라고 부탁했지?",
          "original_form": "아빠가 이번에 구피 잘 돌봐달라고 부탁했지?",
          "speaker_id": "2",
          "time": "20210831 07:32"
        },
        {
          "id": "MDRW2100036673.1.2",
          "form": "네 잠시 집을 비우신다고 챙겨달라고 하섰어요",
          "original_form": "네 잠시 집을 비우신다고 챙겨달라고 하섰어요",
          "speaker_id": "1",
          "time": "20210831 07:32"
        },
        {
          "id": "MDRW2100036673.1.3",
          "form": "그래.. 구피밥은 줬니?",
          "original_form": "그래.. 구피밥은 줬니?",
          "speaker_id": "2",
          "time": "20210831 08:03"
        },
        {
          "id": "MDRW2100036673.1.4",
          "form": "지금 막 줬어요",
          "original_form": "지금 막 줬어요",
          "speaker_id": "1",
          "time": "20210831 08:07"
        },
        {
          "id": "MDRW2100036673.1.5",
          "form": "엄마가 말 안햇으면 깜박할 뻔 했구나??",
          "original_form": "엄마가 말 안햇으면 깜박할 뻔 했구나??",
          "speaker_id": "2",
          "time": "20210831 08:07"
        },
        {
          "id": "MDRW2100036673.1.6",
          "form": "하마터면요ㅎㅎ",
          "original_form": "하마터면요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 08:07"
        },
        {
          "id": "MDRW2100036673.1.7",
          "form": "잊지말고 며칠 신경써라..",
          "original_form": "잊지말고 며칠 신경써라..",
          "speaker_id": "2",
          "time": "20210831 08:10"
        },
        {
          "id": "MDRW2100036673.1.8",
          "form": "구피도 생명이잖니",
          "original_form": "구피도 생명이잖니",
          "speaker_id": "2",
          "time": "20210831 08:10"
        },
        {
          "id": "MDRW2100036673.1.9",
          "form": "그렇조 ㅎㅎ",
          "original_form": "그렇조 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 08:10"
        },
        {
          "id": "MDRW2100036673.1.10",
          "form": "어렸을때 강아지 키우고 싶다고 그래서",
          "original_form": "어렸을때 강아지 키우고 싶다고 그래서",
          "speaker_id": "2",
          "time": "20210831 08:10"
        },
        {
          "id": "MDRW2100036673.1.11",
          "form": "데려오고는 거의 엄마가 뒤처리 다하고",
          "original_form": "데려오고는 거의 엄마가 뒤처리 다하고",
          "speaker_id": "2",
          "time": "20210831 08:11"
        },
        {
          "id": "MDRW2100036673.1.12",
          "form": "생각해보면 전 예뻐만 했네요",
          "original_form": "생각해보면 전 예뻐만 했네요",
          "speaker_id": "1",
          "time": "20210831 08:11"
        },
        {
          "id": "MDRW2100036673.1.13",
          "form": "그래.. 키우는 건 내가 다했지.. 너네는 놀아주기만 하고",
          "original_form": "그래.. 키우는 건 내가 다했지.. 너네는 놀아주기만 하고",
          "speaker_id": "2",
          "time": "20210831 08:11"
        },
        {
          "id": "MDRW2100036673.1.14",
          "form": "엄마가 고생 많으셨네요",
          "original_form": "엄마가 고생 많으셨네요",
          "speaker_id": "1",
          "time": "20210831 08:11"
        },
        {
          "id": "MDRW2100036673.1.15",
          "form": "아니 다행이다",
          "original_form": "아니 다행이다",
          "speaker_id": "2",
          "time": "20210831 08:13"
        },
        {
          "id": "MDRW2100036673.1.16",
          "form": "그럼요 잘 알죠 ㅎㅎ",
          "original_form": "그럼요 잘 알죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 08:16"
