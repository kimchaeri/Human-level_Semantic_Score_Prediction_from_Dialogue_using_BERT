      "utterance": [
        {
          "id": "MDRW2100044716.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 전 살색이었어여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 전 살색이었어여",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044716.1.2",
          "form": "어디보니 뽀로로스티커 타요스티커 해줬다는데",
          "original_form": "어디보니 뽀로로스티커 타요스티커 해줬다는데",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044716.1.3",
          "form": "저도...",
          "original_form": "저도...",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044716.1.4",
          "form": "아맞다 그러고보니 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아맞다 그러고보니 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044716.1.5",
          "form": "가세연 다잡혀갔다면서요",
          "original_form": "가세연 다잡혀갔다면서요",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044716.1.6",
          "form": "????",
          "original_form": "????",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044716.1.7",
          "form": "김용호도 체포당했대",
          "original_form": "김용호도 체포당했대",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.8",
          "form": "김용호가???",
          "original_form": "김용호가???",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.9",
          "form": "드디어????",
          "original_form": "드디어????",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.10",
          "form": "뭘로..???",
          "original_form": "뭘로..???",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.11",
          "form": "그 조국딸이랑",
          "original_form": "그 조국딸이랑",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.12",
          "form": "누구아들",
          "original_form": "누구아들",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.13",
          "form": "이런거 씹어대서",
          "original_form": "이런거 씹어대서",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.14",
          "form": "잡혀갔대 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잡혀갔대 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.17",
          "form": "아 유툽볼때마다 짜증났는데",
          "original_form": "아 유툽볼때마다 짜증났는데",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.18",
          "form": "강용석인가 걔도",
          "original_form": "강용석인가 걔도",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.19",
          "form": "잘됐다",
          "original_form": "잘됐다",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.20",
          "form": "강용석도 잡아감",
          "original_form": "강용석도 잡아감",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.21",
          "form": "또 법정 가겠네",
          "original_form": "또 법정 가겠네",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.23",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044716.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜 꼬시다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜 꼬시다",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044716.1.25",
          "form": "렉카충의 우두머리들",
          "original_form": "렉카충의 우두머리들",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044716.1.26",
          "form": "인생 한번 명예롭게 산다..",
          "original_form": "인생 한번 명예롭게 산다..",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044716.1.27",
          "form": "유툽요새 렉카충들이",
          "original_form": "유툽요새 렉카충들이",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044716.1.28",
          "form": "제일 극도로 거지같은데",
          "original_form": "제일 극도로 거지같은데",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044716.1.29",
          "form": "그중의 우두머리를 잘 잡아갔군요",
          "original_form": "그중의 우두머리를 잘 잡아갔군요",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044716.1.30",
          "form": "전 정권이고 그런얘기 다 떠나서",
          "original_form": "전 정권이고 그런얘기 다 떠나서",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044716.1.31",
          "form": "가세연 신도들이 또 얼마나 날뛸지",
          "original_form": "가세연 신도들이 또 얼마나 날뛸지",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044716.1.32",
          "form": "그냥 가세연과 김용호가 망했으면 좋게써\n이미 ㅇㅇ...",
          "original_form": "그냥 가세연과 김용호가 망했으면 좋게써\n이미 ㅇㅇ...",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044716.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이미 망했던데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이미 망했던데",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044716.1.34",
          "form": "날뛰는중",
          "original_form": "날뛰는중",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044716.1.35",
          "form": "유투브 물 흐리는 선봉대 아닙니까",
          "original_form": "유투브 물 흐리는 선봉대 아닙니까",
