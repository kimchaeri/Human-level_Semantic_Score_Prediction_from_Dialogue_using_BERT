        },
        {
          "id": "MDRW2100026083.1.2",
          "form": "아뇨ㅋㅋ 아직 학생입니다",
          "original_form": "아뇨ㅋㅋ 아직 학생입니다",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026083.1.3",
          "form": "언제쯤?",
          "original_form": "언제쯤?",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026083.1.4",
          "form": "결혼 언제 할 거냐고요?",
          "original_form": "결혼 언제 할 거냐고요?",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100026083.1.5",
          "form": "전 비혼주의자라 결혼 생각은 없어요",
          "original_form": "전 비혼주의자라 결혼 생각은 없어요",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100026083.1.6",
          "form": "결혼 하셨나요?",
          "original_form": "결혼 하셨나요?",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100026083.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026083.1.8",
          "form": "결혼하면 어때요?",
          "original_form": "결혼하면 어때요?",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026083.1.9",
          "form": "전 아직 결혼을 꼭 해야 하나 싶은데 주변에서는 결혼 꼭 하라고 하더라고요ᅮᅲ",
          "original_form": "전 아직 결혼을 꼭 해야 하나 싶은데 주변에서는 결혼 꼭 하라고 하더라고요ᅮᅲ",
          "speaker_id": "2",
          "time": "20210901 19:47"
        },
        {
          "id": "MDRW2100026083.1.10",
          "form": "제멋에 사는거죠",
          "original_form": "제멋에 사는거죠",
          "speaker_id": "1",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026083.1.11",
          "form": "저희 어머니도 저에게 그런 소리 많이 하셔서 알 것 같네요ㅋㅋㅋㅋ",
          "original_form": "저희 어머니도 저에게 그런 소리 많이 하셔서 알 것 같네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026083.1.12",
          "form": "편하게 살명 좋죠",
          "original_form": "편하게 살명 좋죠",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026083.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026083.1.14",
          "form": "내 삶이까오",
          "original_form": "내 삶이까오",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026083.1.15",
          "form": "그쵸 내 삶 내가 사는 게 낫죠ㅎㅎ",
          "original_form": "그쵸 내 삶 내가 사는 게 낫죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026083.1.16",
          "form": "결혼 잘 생각해 봐야겠어요ㅎㅎ",
          "original_form": "결혼 잘 생각해 봐야겠어요ㅎㅎ",
