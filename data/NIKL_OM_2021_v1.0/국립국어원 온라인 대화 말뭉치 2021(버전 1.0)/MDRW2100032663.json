      "utterance": [
        {
          "id": "MDRW2100032663.1.1",
          "form": "좋아하는 티비프로그램있으세요?",
          "original_form": "좋아하는 티비프로그램있으세요?",
          "speaker_id": "1",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032663.1.2",
          "form": "드라마 봐요",
          "original_form": "드라마 봐요",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032663.1.3",
          "form": "사극 봐요",
          "original_form": "사극 봐요",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032663.1.4",
          "form": "어떤거보세요?",
          "original_form": "어떤거보세요?",
          "speaker_id": "1",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032663.1.5",
          "form": "아 사극",
          "original_form": "아 사극",
          "speaker_id": "1",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032663.1.6",
          "form": "네 사극 좋아해요 ㅎㅎ",
          "original_form": "네 사극 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032663.1.7",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032663.1.8",
          "form": "저도 사극좋아요!",
          "original_form": "저도 사극좋아요!",
          "speaker_id": "1",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032663.1.9",
          "form": "선덕여왕좋아했어요",
          "original_form": "선덕여왕좋아했어요",
          "speaker_id": "1",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032663.1.10",
          "form": "선덕여왕 재미있었죠 ㅎ",
          "original_form": "선덕여왕 재미있었죠 ㅎ",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032663.1.11",
          "form": "네네ㅋㅋ",
          "original_form": "네네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032663.1.12",
          "form": "예능도 보세요?!",
          "original_form": "예능도 보세요?!",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032663.1.13",
          "form": "예능은 별로 안보는것같네요",
          "original_form": "예능은 별로 안보는것같네요",
          "speaker_id": "2",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032663.1.14",
          "form": "별로안보시는군요ㅠ",
          "original_form": "별로안보시는군요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032663.1.15",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "2",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032663.1.16",
          "form": "저는 런닝맨 좋아해요!",
          "original_form": "저는 런닝맨 좋아해요!",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032663.1.17",
          "form": "유재석 나오는?",
          "original_form": "유재석 나오는?",
          "speaker_id": "2",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032663.1.18",
          "form": "네네 맞아요 유재석!",
          "original_form": "네네 맞아요 유재석!",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032663.1.19",
          "form": "재미있겠네요 ㅎㅎ",
          "original_form": "재미있겠네요 ㅎㅎ",
