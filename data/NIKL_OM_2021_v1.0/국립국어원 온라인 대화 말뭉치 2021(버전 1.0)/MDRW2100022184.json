      "utterance": [
        {
          "id": "MDRW2100022184.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100022184.1.2",
          "form": "아니용ㅋㅋ 자신없어서",
          "original_form": "아니용ㅋㅋ 자신없어서",
          "speaker_id": "1",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100022184.1.3",
          "form": "그냥 친구네 강아지만 귀여워해줘요",
          "original_form": "그냥 친구네 강아지만 귀여워해줘요",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100022184.1.4",
          "form": "우와 그래도 근처에 귀여운 동물친구가 있군요",
          "original_form": "우와 그래도 근처에 귀여운 동물친구가 있군요",
          "speaker_id": "2",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022184.1.5",
          "form": "저는 최근에 길냥이 데려왔어요",
          "original_form": "저는 최근에 길냥이 데려왔어요",
          "speaker_id": "2",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022184.1.6",
          "form": "우왕! 너무 좋으시겠어요",
          "original_form": "우왕! 너무 좋으시겠어요",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022184.1.7",
          "form": "이름이 뭔가요",
          "original_form": "이름이 뭔가요",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022184.1.8",
          "form": "name3이에요ㅋㅋㅋㅋㅋ 색깔이 식물 name3 같아서 ㅋㅋㅋㅋㅋ",
          "original_form": "&name3&이에요ㅋㅋㅋㅋㅋ 색깔이 식물 &name3& 같아서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022184.1.9",
          "form": "name3ㅋㅋㅋ",
          "original_form": "&name3&ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022184.1.10",
          "form": "잘 지으셨네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잘 지으셨네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022184.1.11",
          "form": "느낌있어요",
          "original_form": "느낌있어요",
          "speaker_id": "1",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100022184.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 잘 알아들으려나 싶었는데 자기 이름은 기가막히게 잘 알더라고요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 잘 알아들으려나 싶었는데 자기 이름은 기가막히게 잘 알더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022184.1.13",
          "form": "요즘은 냥이 간식사는 재미에 빠져있네요 ㅋㅋ",
          "original_form": "요즘은 냥이 간식사는 재미에 빠져있네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022184.1.14",
          "form": "앗 이름도 잘 알고 똑똑한 냥이네여!!",
          "original_form": "앗 이름도 잘 알고 똑똑한 냥이네여!!",
          "speaker_id": "1",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022184.1.15",
          "form": "뭘 좋아하던가요",
          "original_form": "뭘 좋아하던가요",
          "speaker_id": "1",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022184.1.16",
          "form": "템테이션 제일 좋아하더라고요ㅋㅋ",
          "original_form": "템테이션 제일 좋아하더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022184.1.17",
          "form": "ㅋㅋㅋㅋㅋ간식 잘 주는 집사님 만나서 행복해하겠아요",
          "original_form": "ㅋㅋㅋㅋㅋ간식 잘 주는 집사님 만나서 행복해하겠아요",
          "speaker_id": "1",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022184.1.18",
          "form": "ㅋㅋㅋㅋ그랬으면 소원이 없겠네요 부디 행복하길 ㅠ",
          "original_form": "ㅋㅋㅋㅋ그랬으면 소원이 없겠네요 부디 행복하길 ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022184.1.19",
          "form": "간택당하셨나요",
          "original_form": "간택당하셨나요",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100022184.1.20",
          "form": "아무래도 그런 것 같아요 ㅋㅋㅋ 계속 따라다니고 제 차도 알더라고요",
          "original_form": "아무래도 그런 것 같아요 ㅋㅋㅋ 계속 따라다니고 제 차도 알더라고요",
