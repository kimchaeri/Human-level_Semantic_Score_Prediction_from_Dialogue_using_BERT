      "utterance": [
        {
          "id": "MDRW2100041852.1.1",
          "form": "좋아하는",
          "original_form": "좋아하는",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041852.1.2",
          "form": "아이돌 있으세요?",
          "original_form": "아이돌 있으세요?",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041852.1.3",
          "form": "네! 저 스테이씨 좋아해요",
          "original_form": "네! 저 스테이씨 좋아해요",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041852.1.4",
          "form": "오 숙대입시",
          "original_form": "오 숙대입시",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041852.1.5",
          "form": "오늘 색안경이라는 신곡 나오는 .. ㅎㅎ",
          "original_form": "오늘 색안경이라는 신곡 나오는 .. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041852.1.6",
          "form": "노래 중독성 있어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "노래 중독성 있어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041852.1.7",
          "form": "그 별명 넘 귀엽죠",
          "original_form": "그 별명 넘 귀엽죠",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041852.1.8",
          "form": "맞아요ㅠㅠ 전 곡도 좋아서..",
          "original_form": "맞아요ㅠㅠ 전 곡도 좋아서..",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041852.1.9",
          "form": "네ㅋㅋㅋㅋㅋㅋ 요즘은 어떤 아이돌이",
          "original_form": "네ㅋㅋㅋㅋㅋㅋ 요즘은 어떤 아이돌이",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041852.1.10",
          "form": "인기 많나요?",
          "original_form": "인기 많나요?",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041852.1.11",
          "form": "음 ~~ 친구들마다 다르긴 한데",
          "original_form": "음 ~~ 친구들마다 다르긴 한데",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041852.1.12",
          "form": "에스파가 인기 많더라고요",
          "original_form": "에스파가 인기 많더라고요",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041852.1.13",
          "form": "에스파 노래도 좋아요",
          "original_form": "에스파 노래도 좋아요",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041852.1.14",
          "form": "암언더넥스트",
          "original_form": "암언더넥스트",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041852.1.15",
          "form": "오ㅋㅋㅋ 맞아요",
          "original_form": "오ㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041852.1.16",
          "form": "원곡이랑 또 느낌이 다른..",
          "original_form": "원곡이랑 또 느낌이 다른..",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041852.1.17",
          "form": "ㅋㅋㅋㅋ 맞아요 ㅋㅋㅋ ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 맞아요 ㅋㅋㅋ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100041852.1.18",
          "form": "요즘은 여돌이 핫하군요",
          "original_form": "요즘은 여돌이 핫하군요",
          "speaker_id": "1",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100041852.1.19",
          "form": "네ㅋㅋㅋㅋ 최근에 비슷한 시기에 컴백해서 그런가",
          "original_form": "네ㅋㅋㅋㅋ 최근에 비슷한 시기에 컴백해서 그런가",
          "speaker_id": "2",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100041852.1.20",
          "form": "넥스트레벨-던던댄스-마피아 이렇게 세 개 자주 듣더라고요",
          "original_form": "넥스트레벨-던던댄스-마피아 이렇게 세 개 자주 듣더라고요",
          "speaker_id": "2",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100041852.1.21",
