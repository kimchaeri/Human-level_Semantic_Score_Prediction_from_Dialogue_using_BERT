      "utterance": [
        {
          "id": "MDRW2100040051.1.1",
          "form": "죄다",
          "original_form": "죄다",
          "speaker_id": "2",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040051.1.2",
          "form": "종려 들고 하네",
          "original_form": "종려 들고 하네",
          "speaker_id": "2",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040051.1.3",
          "form": "공략이랍시고 벤티랑 종려 포함된 파티 구성해놓고",
          "original_form": "공략이랍시고 벤티랑 종려 포함된 파티 구성해놓고",
          "speaker_id": "1",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040051.1.4",
          "form": "사기꾼들같으니",
          "original_form": "사기꾼들같으니",
          "speaker_id": "2",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040051.1.5",
          "form": "ㅇㅇㄹㅇ",
          "original_form": "ㅇㅇㄹㅇ",
          "speaker_id": "2",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040051.1.6",
          "form": "사기꾼이야",
          "original_form": "사기꾼이야",
          "speaker_id": "2",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040051.1.7",
          "form": "참 쉽죠? 하고있으면 열받는다 그랫는데",
          "original_form": "참 쉽죠? 하고있으면 열받는다 그랫는데",
          "speaker_id": "1",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040051.1.8",
          "form": "그런 사람들이 진짜 많네 ㅋㅋㅋㅋ",
          "original_form": "그런 사람들이 진짜 많네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040051.1.9",
          "form": "4성으로 깨기",
          "original_form": "4성으로 깨기",
          "speaker_id": "2",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040051.1.10",
          "form": "케이아버젼 있네",
          "original_form": "케이아버젼 있네",
          "speaker_id": "2",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040051.1.11",
          "form": "이걸 봐바야겠어",
          "original_form": "이걸 봐바야겠어",
          "speaker_id": "2",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100040051.1.12",
          "form": "오 궁금하다",
          "original_form": "오 궁금하다",
          "speaker_id": "1",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100040051.1.13",
          "form": "설마 케이아 종려 호두 피슬 조합은 아니겟지",
          "original_form": "설마 케이아 종려 호두 피슬 조합은 아니겟지",
          "speaker_id": "1",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100040051.1.14",
          "form": "케이아 설탕 중운 엠버네",
          "original_form": "케이아 설탕 중운 엠버네",
          "speaker_id": "2",
          "time": "20210904 01:43"
        },
        {
          "id": "MDRW2100040051.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210904 01:43"
        },
        {
          "id": "MDRW2100040051.1.16",
          "form": "설탕 궁이",
          "original_form": "설탕 궁이",
          "speaker_id": "2",
          "time": "20210904 01:43"
        },
        {
          "id": "MDRW2100040051.1.17",
          "form": "진짜 기본캐네",
          "original_form": "진짜 기본캐네",
          "speaker_id": "1",
          "time": "20210904 01:43"
        },
        {
          "id": "MDRW2100040051.1.18",
          "form": "포인트인가봐",
          "original_form": "포인트인가봐",
          "speaker_id": "2",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100040051.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100040051.1.20",
          "form": "그치만 난 중운이 없긴 함......",
          "original_form": "그치만 난 중운이 없긴 함......",
          "speaker_id": "2",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100040051.1.21",
          "form": "확산있어서?",
          "original_form": "확산있어서?",
          "speaker_id": "1",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100040051.1.22",
          "form": "그런가봐",
          "original_form": "그런가봐",
          "speaker_id": "2",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100040051.1.23",
          "form": "근데 나머지 영상은",
          "original_form": "근데 나머지 영상은",
          "speaker_id": "2",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100040051.1.24",
          "form": "죄 종려들고있는걸 봐선",
          "original_form": "죄 종려들고있는걸 봐선",
          "speaker_id": "2",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100040051.1.25",
          "form": "ㄹㅇ원소스킬로",
          "original_form": "ㄹㅇ원소스킬로",
          "speaker_id": "2",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100040051.1.26",
          "form": "5초안에 때려야되나봐",
          "original_form": "5초안에 때려야되나봐",
          "speaker_id": "2",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100040051.1.27",
          "form": "그래서 4성으로",
          "original_form": "그래서 4성으로",
          "speaker_id": "2",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100040051.1.28",
          "form": "깨려면",
          "original_form": "깨려면",
          "speaker_id": "2",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100040051.1.29",
          "form": "확산 필요한거고...........",
          "original_form": "확산 필요한거고...........",
          "speaker_id": "2",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100040051.1.30",
          "form": "일단 ㅎㅏ",
          "original_form": "일단 ㅎㅏ",
          "speaker_id": "2",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.31",
          "form": "ㅇㅎ....",
          "original_form": "ㅇㅎ....",
          "speaker_id": "1",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.32",
          "form": "시도나 해보겠음",
          "original_form": "시도나 해보겠음",
          "speaker_id": "2",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.33",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.34",
          "form": "엠버는 왜 소환한거지",
          "original_form": "엠버는 왜 소환한거지",
          "speaker_id": "2",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.35",
          "form": "잘 깨고와...",
          "original_form": "잘 깨고와...",
          "speaker_id": "1",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.36",
          "form": "그 멍청해보이는 토끼를",
          "original_form": "그 멍청해보이는 토끼를",
          "speaker_id": "2",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.37",
          "form": "걍... 이렇게 기본캐만 있단걸 보여주려고...?",
          "original_form": "걍... 이렇게 기본캐만 있단걸 보여주려고...?",
          "speaker_id": "1",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.38",
          "form": "걍 토템 위에",
          "original_form": "걍 토템 위에",
          "speaker_id": "2",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.39",
          "form": "올려놓고 사라졌어",
          "original_form": "올려놓고 사라졌어",
          "speaker_id": "2",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.40",
          "form": "왜지.....?",
          "original_form": "왜지.....?",
          "speaker_id": "2",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.41",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100040051.1.42",
          "form": "그용도면 차라리 누룽지가 낫지 않나 ㅋㅋㅋㅋ",
          "original_form": "그용도면 차라리 누룽지가 낫지 않나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:47"
        },
        {
          "id": "MDRW2100040051.1.43",
          "form": "아니 불도 안뿜었어",
          "original_form": "아니 불도 안뿜었어",
          "speaker_id": "2",
          "time": "20210904 01:47"
        }
      ]
