      "utterance": [
        {
          "id": "MDRW2100040577.1.1",
          "form": "학교 다니니?",
          "original_form": "학교 다니니?",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040577.1.2",
          "form": "학교 다뇨?",
          "original_form": "학교 다뇨?",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040577.1.3",
          "form": "아니 졸업했으",
          "original_form": "아니 졸업했으",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040577.1.4",
          "form": "글쿠나 학교생활 어땠어",
          "original_form": "글쿠나 학교생활 어땠어",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040577.1.5",
          "form": "그냥 무난했던 듯??",
          "original_form": "그냥 무난했던 듯??",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040577.1.6",
          "form": "나도나도",
          "original_form": "나도나도",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040577.1.7",
          "form": "근데 조금 후회되",
          "original_form": "근데 조금 후회되",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040577.1.8",
          "form": "동아리 같은거 잘 안했어",
          "original_form": "동아리 같은거 잘 안했어",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040577.1.9",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040577.1.10",
          "form": "나도ㅠ 유럽 좀 다녀올걸 용기내서",
          "original_form": "나도ㅠ 유럽 좀 다녀올걸 용기내서",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040577.1.11",
          "form": "별거아닌데ㅜㅜ",
          "original_form": "별거아닌데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040577.1.12",
          "form": "맞아 동아리활동 잘 안한거랑",
          "original_form": "맞아 동아리활동 잘 안한거랑",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040577.1.13",
          "form": "해외여행 안가본거 후회되더라ㅜㅜ",
          "original_form": "해외여행 안가본거 후회되더라ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040577.1.14",
          "form": "맞지 나도 그래",
          "original_form": "맞지 나도 그래",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040577.1.15",
          "form": "긍데 여행은 몰라도 돌아가면 동아리는 안할거같아 ㅋㅋㅋㅋ",
          "original_form": "긍데 여행은 몰라도 돌아가면 동아리는 안할거같아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040577.1.16",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100040577.1.17",
          "form": "난 안해봐서",
          "original_form": "난 안해봐서",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100040577.1.18",
          "form": "궁금하다",
          "original_form": "궁금하다",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100040577.1.19",
          "form": "그냥 사람에 치이는거 싫어서 ㅋㅋ",
          "original_form": "그냥 사람에 치이는거 싫어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040577.1.20",
          "form": "취미는 혼자해도되니까 ㅋㅋ",
          "original_form": "취미는 혼자해도되니까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040577.1.21",
          "form": "앗 그건 인정",
          "original_form": "앗 그건 인정",
          "speaker_id": "2",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100040577.1.22",
          "form": "어딜가도 사람이 힘들지",
          "original_form": "어딜가도 사람이 힘들지",
          "speaker_id": "2",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100040577.1.23",
          "form": "맞아 그런거같아",
          "original_form": "맞아 그런거같아",
          "speaker_id": "1",
          "time": "20210904 23:11"
        }
      ]
