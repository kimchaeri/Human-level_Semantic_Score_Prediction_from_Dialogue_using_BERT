      "utterance": [
        {
          "id": "MDRW2100023967.1.1",
          "form": "김치 얻어먹는 것도",
          "original_form": "김치 얻어먹는 것도",
          "speaker_id": "2",
          "time": "20210830 20:41"
        },
        {
          "id": "MDRW2100023967.1.2",
          "form": "자차가 있음 편한데...",
          "original_form": "자차가 있음 편한데...",
          "speaker_id": "2",
          "time": "20210830 20:41"
        },
        {
          "id": "MDRW2100023967.1.3",
          "form": "에이 기름써서 왔다갔다 하는 돈이 김치값일거에요",
          "original_form": "에이 기름써서 왔다갔다 하는 돈이 김치값일거에요",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100023967.1.4",
          "form": "집에서 양념을 얻어와서",
          "original_form": "집에서 양념을 얻어와서",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100023967.1.5",
          "form": "배추 한두포기 절여드시는 것두 조은데",
          "original_form": "배추 한두포기 절여드시는 것두 조은데",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100023967.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 난이도 갑자기 급상승",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 난이도 갑자기 급상승",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023967.1.7",
          "form": "글서 전 걍 비비고 썰은 배추김치를 사먹습니다...",
          "original_form": "글서 전 걍 비비고 썰은 배추김치를 사먹습니다...",
          "speaker_id": "2",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100023967.1.8",
          "form": "아 그럼 김볶밥 할거 없이 다 드시나요?",
          "original_form": "아 그럼 김볶밥 할거 없이 다 드시나요?",
          "speaker_id": "1",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100023967.1.9",
          "form": "넹ㅋㅋㅋ 볶음밥 김치는 좀 쉬어야 맛있는데",
          "original_form": "넹ㅋㅋㅋ 볶음밥 김치는 좀 쉬어야 맛있는데",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100023967.1.10",
          "form": "쉬기 전에 밥반찬으로 뚝딱 다 먹어요",
          "original_form": "쉬기 전에 밥반찬으로 뚝딱 다 먹어요",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100023967.1.11",
          "form": "백종워너 가라사대 그럼 식초를 타라",
          "original_form": "백종워너 가라사대 그럼 식초를 타라",
          "speaker_id": "1",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100023967.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 멀 글케까지 해서...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 멀 글케까지 해서...",
          "speaker_id": "2",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100023967.1.13",
          "form": "집에서 만든 김치볶음밥을 먹을 필요가 있나요",
          "original_form": "집에서 만든 김치볶음밥을 먹을 필요가 있나요",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023967.1.14",
          "form": "걍 냉동 볶음밥 먹어여... 편함...",
          "original_form": "걍 냉동 볶음밥 먹어여... 편함...",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023967.1.15",
          "form": "집에 김치가 많은데 왜 냉볶밥 먹습니까 떼잉",
          "original_form": "집에 김치가 많은데 왜 냉볶밥 먹습니까 떼잉",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023967.1.16",
          "form": "냉동실에 밥따위가 들어갈 자리가 없습니다",
          "original_form": "냉동실에 밥따위가 들어갈 자리가 없습니다",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023967.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 라구소스를 채워하 야하니까여?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 라구소스를 채워하 야하니까여?",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023967.1.18",
          "form": "다진 양파와 파와...",
          "original_form": "다진 양파와 파와...",
          "speaker_id": "2",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100023967.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100023967.1.20",
          "form": "자리가 없ㅇ요",
          "original_form": "자리가 없ㅇ요",
          "speaker_id": "1",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100023967.1.21",
          "form": "밥은,, 크잖아요?",
          "original_form": "밥은,, 크잖아요?",
          "speaker_id": "1",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100023967.1.22",
          "form": "네 그래서 전 닭브레스트라고 말해요",
          "original_form": "네 그래서 전 닭브레스트라고 말해요",
          "speaker_id": "1",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023967.1.23",
          "form": "따라해보세요 닭 브레스트 ㅋㅋ",
          "original_form": "따라해보세요 닭 브레스트 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023967.1.24",
          "form": "여하간 저 둘 넣고 나면 저의 좁아터진 자취집 냉동고가 꽉 차버리는 것이예요",
          "original_form": "여하간 저 둘 넣고 나면 저의 좁아터진 자취집 냉동고가 꽉 차버리는 것이예요",
          "speaker_id": "2",
          "time": "20210830 20:50"
        }
      ]
    }
