      "utterance": [
        {
          "id": "MDRW2100040818.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100040818.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100040818.1.3",
          "form": "주말 내내 날씨가 좋네요",
          "original_form": "주말 내내 날씨가 좋네요",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100040818.1.4",
          "form": "네 다행인거같아요",
          "original_form": "네 다행인거같아요",
          "speaker_id": "2",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100040818.1.5",
          "form": "네 한동안 비가 많이 와서",
          "original_form": "네 한동안 비가 많이 와서",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100040818.1.6",
          "form": "월요일부터는 또 비가 오긴 하더라구요..",
          "original_form": "월요일부터는 또 비가 오긴 하더라구요..",
          "speaker_id": "2",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100040818.1.7",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100040818.1.8",
          "form": "며칠동안 하늘도 파랗고 맑아서 좋았는데..",
          "original_form": "며칠동안 하늘도 파랗고 맑아서 좋았는데..",
          "speaker_id": "1",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100040818.1.9",
          "form": "네 이쪽은 월화 비내려요",
          "original_form": "네 이쪽은 월화 비내려요",
          "speaker_id": "2",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100040818.1.10",
          "form": "저도 일기예보 확인해봐야겠네요",
          "original_form": "저도 일기예보 확인해봐야겠네요",
          "speaker_id": "1",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100040818.1.11",
          "form": "저희도 월화 비소식이 있네요",
          "original_form": "저희도 월화 비소식이 있네요",
          "speaker_id": "1",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100040818.1.12",
          "form": "그쵸 그리고 요즘 예보는",
          "original_form": "그쵸 그리고 요즘 예보는",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040818.1.13",
          "form": "당장 2~3일 남은 게 아니면",
          "original_form": "당장 2~3일 남은 게 아니면",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040818.1.14",
          "form": "수시로 변경되는거같아서",
          "original_form": "수시로 변경되는거같아서",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040818.1.15",
          "form": "언제부터 우리나라에 가을장마가 생긴건지",
          "original_form": "언제부터 우리나라에 가을장마가 생긴건지",
          "speaker_id": "1",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040818.1.16",
          "form": "수요일부터 맑다는 얘기도 잘 못믿겠어요",
          "original_form": "수요일부터 맑다는 얘기도 잘 못믿겠어요",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040818.1.17",
          "form": "요즘은 갑자기 비오고 그래서",
          "original_form": "요즘은 갑자기 비오고 그래서",
          "speaker_id": "1",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040818.1.18",
          "form": "우산을 늘 챙겨야하는 듯요",
          "original_form": "우산을 늘 챙겨야하는 듯요",
          "speaker_id": "1",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040818.1.19",
          "form": "네네 작은거 하나 들고다녀야 하는거같아요",
          "original_form": "네네 작은거 하나 들고다녀야 하는거같아요",
          "speaker_id": "2",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040818.1.20",
          "form": "비가 많이 오네요",
          "original_form": "비가 많이 오네요",
          "speaker_id": "1",
