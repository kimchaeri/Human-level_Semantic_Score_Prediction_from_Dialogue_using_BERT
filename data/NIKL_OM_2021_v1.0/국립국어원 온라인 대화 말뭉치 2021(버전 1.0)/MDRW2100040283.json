      "utterance": [
        {
          "id": "MDRW2100040283.1.1",
          "form": "점심 뭐 드셨나요?",
          "original_form": "점심 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040283.1.2",
          "form": "집에 있는밥이요.",
          "original_form": "집에 있는밥이요.",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040283.1.3",
          "form": "반찬은요?",
          "original_form": "반찬은요?",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040283.1.4",
          "form": "집에반찬이별로없어서ㅜㅜ",
          "original_form": "집에반찬이별로없어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040283.1.5",
          "form": "저도 그게 늘 고민이예요 ㅠ",
          "original_form": "저도 그게 늘 고민이예요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100040283.1.6",
          "form": "애들위주로 하시면되겠네요",
          "original_form": "애들위주로 하시면되겠네요",
          "speaker_id": "2",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100040283.1.7",
          "form": "그것만해도 완전편할듯",
          "original_form": "그것만해도 완전편할듯",
          "speaker_id": "2",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100040283.1.8",
          "form": "오늘은 떡갈비 데워서 줬고요 저녁은 뭐할지 고민이네요",
          "original_form": "오늘은 떡갈비 데워서 줬고요 저녁은 뭐할지 고민이네요",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100040283.1.9",
          "form": "저도 아침에떡갈비줬어요",
          "original_form": "저도 아침에떡갈비줬어요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040283.1.10",
          "form": "어찌 같은거 줬네요",
          "original_form": "어찌 같은거 줬네요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040283.1.11",
          "form": "혹시 비비고 떡갈비인가요?? ㅋ",
          "original_form": "혹시 비비고 떡갈비인가요?? ㅋ",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040283.1.12",
          "form": "전 마켓컬리에서샀어요",
          "original_form": "전 마켓컬리에서샀어요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040283.1.13",
          "form": "비비고 떡갈비맛나죠",
          "original_form": "비비고 떡갈비맛나죠",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040283.1.14",
          "form": "저희딸이 제일좋아하는 떡갈비가 비비고예요",
          "original_form": "저희딸이 제일좋아하는 떡갈비가 비비고예요",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040283.1.15",
          "form": "아~ 마켓컬리 맛난거 많죠~",
          "original_form": "아~ 마켓컬리 맛난거 많죠~",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040283.1.16",
          "form": "비비고는 만두도 맛있습니다^-^",
          "original_form": "비비고는 만두도 맛있습니다^-^",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040283.1.17",
          "form": "비비고는 진짜 다맛나요",
          "original_form": "비비고는 진짜 다맛나요",
          "speaker_id": "2",
          "time": "20210904 15:33"
        },
        {
          "id": "MDRW2100040283.1.18",
          "form": "대기업의 맛 ㅎㅎㅎ",
          "original_form": "대기업의 맛 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:33"
        },
        {
          "id": "MDRW2100040283.1.19",
          "form": "비비고 육개장은 좀 묽긴 했어요",
          "original_form": "비비고 육개장은 좀 묽긴 했어요",
