        {
          "id": "MDRW2100041260.1.4",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041260.1.5",
          "form": "특히 여행 갔다가 올때",
          "original_form": "특히 여행 갔다가 올때",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041260.1.6",
          "form": "타면 좋을것 같아",
          "original_form": "타면 좋을것 같아",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041260.1.7",
          "form": "아 ㅇㅈ",
          "original_form": "아 ㅇㅈ",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041260.1.8",
          "form": "개피곤한데",
          "original_form": "개피곤한데",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041260.1.9",
          "form": "양옆으로 아저씨 앉아있으면",
          "original_form": "양옆으로 아저씨 앉아있으면",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041260.1.10",
          "form": "성격 ㄱ ㅐ꼬일듯",
          "original_form": "성격 ㄱ ㅐ꼬일듯",
