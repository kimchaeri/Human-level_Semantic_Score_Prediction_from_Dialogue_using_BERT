      "utterance": [
        {
          "id": "MDRW2100010716.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210823 21:18"
        },
        {
          "id": "MDRW2100010716.1.2",
          "form": "맛없는거 먹었어요 ㅎㅎ",
          "original_form": "맛없는거 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 21:43"
        },
        {
          "id": "MDRW2100010716.1.3",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210823 21:43"
        },
        {
          "id": "MDRW2100010716.1.4",
          "form": "헐 뭐드셨길래 ㅋㅋㅋㅋㅋ",
          "original_form": "헐 뭐드셨길래 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100010716.1.5",
          "form": "전 저녁 패스했어요! 점심이 너무 거나했어서 ㅋㅋㅋ",
          "original_form": "전 저녁 패스했어요! 점심이 너무 거나했어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100010716.1.6",
          "form": "뭘드셨길래ㅋㅋ!!",
          "original_form": "뭘드셨길래ㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100010716.1.7",
          "form": "맛난거 드셨나봐요",
          "original_form": "맛난거 드셨나봐요",
          "speaker_id": "1",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100010716.1.8",
          "form": "콩나물국이긴 했는데 이제 밥 두그릇을 곁들인 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "콩나물국이긴 했는데 이제 밥 두그릇을 곁들인 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 07:48"
        },
        {
          "id": "MDRW2100010716.1.9",
          "form": "아아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100010716.1.10",
          "form": "진짜 맛있었나봐요",
          "original_form": "진짜 맛있었나봐요",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100010716.1.11",
          "form": "오늘도 맛난거 드셨나요!",
          "original_form": "오늘도 맛난거 드셨나요!",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100010716.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 오늘은.... 죽먹었슴다...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 오늘은.... 죽먹었슴다...",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100010716.1.13",
          "form": "어제밤부터 생리가 터졌는데",
          "original_form": "어제밤부터 생리가 터졌는데",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100010716.1.14",
          "form": "이번 생리통은 너무 심해여...",
          "original_form": "이번 생리통은 너무 심해여...",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100010716.1.15",
          "form": "밥을 먹을수가 없슴다....",
          "original_form": "밥을 먹을수가 없슴다....",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100010716.1.16",
          "form": "어우 진짜 심하시네요ㅠㅠ",
          "original_form": "어우 진짜 심하시네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100010716.1.17",
          "form": "고생이 많으시겠어요...",
          "original_form": "고생이 많으시겠어요...",
          "speaker_id": "1",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100010716.1.18",
          "form": "지금은 좀 괜찮으세요?",
          "original_form": "지금은 좀 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100010716.1.19",
          "form": "훨씬요...! 약먹고 누웠더니 훨씬 낫네요 히히",
          "original_form": "훨씬요...! 약먹고 누웠더니 훨씬 낫네요 히히",
          "speaker_id": "2",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100010716.1.20",
          "form": "name1 저녁 드셨나요?",
          "original_form": "&name1& 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100010716.1.21",
          "form": "아니요! 이제 먹어야해요 ㅎㅎ",
          "original_form": "아니요! 이제 먹어야해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:46"
        },
        {
          "id": "MDRW2100010716.1.22",
          "form": "아깐 엄청 배고팠는데",
          "original_form": "아깐 엄청 배고팠는데",
          "speaker_id": "1",
          "time": "20210824 19:46"
        },
        {
          "id": "MDRW2100010716.1.23",
          "form": "고통의 시간을 지나니 괜찮아서 ..나태하게 누어ㅓ있어요",
          "original_form": "고통의 시간을 지나니 괜찮아서 ..나태하게 누어ㅓ있어요",
          "speaker_id": "1",
          "time": "20210824 19:46"
        },
        {
          "id": "MDRW2100010716.1.24",
          "form": "저녁도 죽드시나요? ㅠㅠ",
          "original_form": "저녁도 죽드시나요? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 19:46"
        },
        {
          "id": "MDRW2100010716.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 고통의시간",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 고통의시간",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100010716.1.26",
          "form": "고민중이에요 배가 고프긴 한데...",
          "original_form": "고민중이에요 배가 고프긴 한데...",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100010716.1.27",
          "form": "닭발먹고싶어요...",
          "original_form": "닭발먹고싶어요...",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100010716.1.28",
          "form": "근데 먹으면 난리나겟죠...,",
          "original_form": "근데 먹으면 난리나겟죠...,",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100010716.1.29",
          "form": "닭발은...확실히 그렇죠",
          "original_form": "닭발은...확실히 그렇죠",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100010716.1.30",
          "form": "마음을 정하셨나요 ㅋㅎㅋㅎㅎㅎ",
          "original_form": "마음을 정하셨나요 ㅋㅎㅋㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100010716.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그냥 점심에 먹고 남은 죽을 먹기로 했어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그냥 점심에 먹고 남은 죽을 먹기로 했어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100010716.1.32",
          "form": "참아본다...!+!",
          "original_form": "참아본다...!+!",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100010716.1.33",
