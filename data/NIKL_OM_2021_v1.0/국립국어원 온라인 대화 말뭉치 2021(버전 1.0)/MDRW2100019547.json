      "utterance": [
        {
          "id": "MDRW2100019547.1.1",
          "form": "음 크림치즈 베이스도 있고 생크림도 있는데",
          "original_form": "음 크림치즈 베이스도 있고 생크림도 있는데",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019547.1.2",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019547.1.3",
          "form": "주로 화려한데는 크림치즈더라구여!",
          "original_form": "주로 화려한데는 크림치즈더라구여!",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019547.1.4",
          "form": "오 그래요??",
          "original_form": "오 그래요??",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019547.1.5",
          "form": "그 도시락 케이크라고 좀 작은거 파는데 그거 한번 드셔보세요!",
          "original_form": "그 도시락 케이크라고 좀 작은거 파는데 그거 한번 드셔보세요!",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019547.1.6",
          "form": "와 그런거도있나요",
          "original_form": "와 그런거도있나요",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019547.1.7",
          "form": "궁금하네요 어떨지 ㅋㅋㅋㅋ",
          "original_form": "궁금하네요 어떨지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019547.1.8",
          "form": "네! 그건 혼자 드실 수 있을거에요!",
          "original_form": "네! 그건 혼자 드실 수 있을거에요!",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019547.1.9",
          "form": "그 일회용 은색 도시락접시 같은데에 담겨있어욬ㅋ",
          "original_form": "그 일회용 은색 도시락접시 같은데에 담겨있어욬ㅋ",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019547.1.10",
          "form": "혼자 사먹으면 부담되는데 좋네요 ㅋㅋㅋ",
          "original_form": "혼자 사먹으면 부담되는데 좋네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019547.1.11",
          "form": "네 ㅋㅋㅋ 그거는 가격도 아주 비싸진 않더라규여!",
          "original_form": "네 ㅋㅋㅋ 그거는 가격도 아주 비싸진 않더라규여!",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019547.1.12",
          "form": "약간 뭔지 알거같기도 해요ㅋㅋㅋ",
          "original_form": "약간 뭔지 알거같기도 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019547.1.13",
          "form": "요샌 그거 말고도 또 다른 형태도 있던데",
          "original_form": "요샌 그거 말고도 또 다른 형태도 있던데",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019547.1.14",
          "form": "오 딱 일인용으로 적당하겠어요",
          "original_form": "오 딱 일인용으로 적당하겠어요",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019547.1.15",
          "form": "이름이 따로 있는거 같은데 까먹었네욬ㅋㅋ",
          "original_form": "이름이 따로 있는거 같은데 까먹었네욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019547.1.16",
          "form": "진짜 먹방 이런거 유행하면서",
          "original_form": "진짜 먹방 이런거 유행하면서",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019547.1.17",
          "form": "신기한 음식들이 많아진거같아요",
          "original_form": "신기한 음식들이 많아진거같아요",
          "speaker_id": "1",
          "time": "20210827 20:14"
        }
      ]
