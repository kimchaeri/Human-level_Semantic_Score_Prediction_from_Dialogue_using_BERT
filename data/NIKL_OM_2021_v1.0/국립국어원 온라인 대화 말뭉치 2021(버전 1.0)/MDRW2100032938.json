      "utterance": [
        {
          "id": "MDRW2100032938.1.1",
          "form": "점심 뭐드시나요",
          "original_form": "점심 뭐드시나요",
          "speaker_id": "2",
          "time": "20210906 10:53"
        },
        {
          "id": "MDRW2100032938.1.2",
          "form": "칼국수 먹으려고요",
          "original_form": "칼국수 먹으려고요",
          "speaker_id": "1",
          "time": "20210906 11:04"
        },
        {
          "id": "MDRW2100032938.1.3",
          "form": "별로 좋아하는 메뉴가 아니라 안 기다려지네요ㅎ",
          "original_form": "별로 좋아하는 메뉴가 아니라 안 기다려지네요ㅎ",
          "speaker_id": "1",
          "time": "20210906 11:05"
        },
        {
          "id": "MDRW2100032938.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210906 11:05"
        },
        {
          "id": "MDRW2100032938.1.5",
          "form": "순대국이죠",
          "original_form": "순대국이죠",
          "speaker_id": "2",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100032938.1.6",
          "form": "제 1픽입니다",
          "original_form": "제 1픽입니다",
          "speaker_id": "2",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100032938.1.7",
          "form": "순대내장반반인가요?",
          "original_form": "순대내장반반인가요?",
          "speaker_id": "1",
          "time": "20210906 11:45"
        },
        {
          "id": "MDRW2100032938.1.8",
          "form": "순대는 빼고 내장100이요",
          "original_form": "순대는 빼고 내장100이요",
          "speaker_id": "2",
          "time": "20210906 11:45"
        },
        {
          "id": "MDRW2100032938.1.9",
          "form": "맛잘알ㅎㅎ",
          "original_form": "맛잘알ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 11:46"
        },
        {
          "id": "MDRW2100032938.1.10",
          "form": "얼큰한가요?!!",
          "original_form": "얼큰한가요?!!",
          "speaker_id": "1",
          "time": "20210906 11:46"
        },
        {
          "id": "MDRW2100032938.1.11",
          "form": "네 얼큰한거 먹어야 해장이 ㅎㅎ",
          "original_form": "네 얼큰한거 먹어야 해장이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100032938.1.12",
          "form": "ㅎㅎ 부럽네요",
          "original_form": "ㅎㅎ 부럽네요",
          "speaker_id": "1",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100032938.1.13",
          "form": "메뉴 선택권이 없으시군요",
          "original_form": "메뉴 선택권이 없으시군요",
          "speaker_id": "2",
          "time": "20210906 11:49"
        },
        {
          "id": "MDRW2100032938.1.14",
          "form": "저도 회사가 이전하기전엔 완전 얼큰한 순대국 파는곳이 근처였는데",
          "original_form": "저도 회사가 이전하기전엔 완전 얼큰한 순대국 파는곳이 근처였는데",
          "speaker_id": "1",
          "time": "20210906 11:49"
        },
        {
          "id": "MDRW2100032938.1.15",
          "form": "지금은 주변 순대국이 다 너무 담백해요ㅜ",
          "original_form": "지금은 주변 순대국이 다 너무 담백해요ㅜ",
          "speaker_id": "1",
          "time": "20210906 11:49"
        },
        {
          "id": "MDRW2100032938.1.16",
          "form": "다데기로 커버가 안됨",
          "original_form": "다데기로 커버가 안됨",
          "speaker_id": "1",
          "time": "20210906 11:49"
        },
        {
          "id": "MDRW2100032938.1.17",
          "form": "ㅋㅋ 빨개야죠 순대국은",
          "original_form": "ㅋㅋ 빨개야죠 순대국은",
          "speaker_id": "2",
          "time": "20210906 11:50"
        },
        {
          "id": "MDRW2100032938.1.18",
          "form": "그렇죠ㅜ!!!",
          "original_form": "그렇죠ㅜ!!!",
          "speaker_id": "1",
          "time": "20210906 11:50"
        },
        {
          "id": "MDRW2100032938.1.19",
          "form": "진짜 여백 순대국이 그립네요",
          "original_form": "진짜 여백 순대국이 그립네요",
          "speaker_id": "1",
          "time": "20210906 11:50"
        },
        {
          "id": "MDRW2100032938.1.20",
          "form": "거긴 순한맛조차 빨갰어요",
          "original_form": "거긴 순한맛조차 빨갰어요",
          "speaker_id": "1",
          "time": "20210906 11:50"
        },
        {
          "id": "MDRW2100032938.1.21",
          "form": "여백은 여의도에 있군요",
          "original_form": "여백은 여의도에 있군요",
          "speaker_id": "2",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100032938.1.22",
          "form": "넵 여기만큼 얼큰한 순대국집 못봤어요ㅎ",
          "original_form": "넵 여기만큼 얼큰한 순대국집 못봤어요ㅎ",
          "speaker_id": "1",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100032938.1.23",
          "form": "근본이네요",
          "original_form": "근본이네요",
          "speaker_id": "2",
          "time": "20210906 12:46"
        },
        {
          "id": "MDRW2100032938.1.24",
          "form": "맛있겠당",
          "original_form": "맛있겠당",
          "speaker_id": "2",
          "time": "20210906 12:46"
        }
      ]
