      "utterance": [
        {
          "id": "MDRW2100025602.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025602.1.2",
          "form": "저는 음 훌륭한 랜선 집사네요 ㅎㅎ",
          "original_form": "저는 음 훌륭한 랜선 집사네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:14"
        },
        {
          "id": "MDRW2100025602.1.3",
          "form": "오 고양이파신가요?",
          "original_form": "오 고양이파신가요?",
          "speaker_id": "1",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025602.1.4",
          "form": "저는 고양이 두마리 키워요",
          "original_form": "저는 고양이 두마리 키워요",
          "speaker_id": "1",
          "time": "20210901 14:59"
        },
        {
          "id": "MDRW2100025602.1.5",
          "form": "앗 저 고양이 좋아해요 이름이 뭔가요?ㅎ",
          "original_form": "앗 저 고양이 좋아해요 이름이 뭔가요?ㅎ",
          "speaker_id": "2",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100025602.1.6",
          "form": "초코랑 하임이에요 마침 옆에 과자가 있었어요 ㅋㅋ",
          "original_form": "초코랑 하임이에요 마침 옆에 과자가 있었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100025602.1.7",
          "form": "혹시 초코색과 크림색입니까 ㅋㅋ",
          "original_form": "혹시 초코색과 크림색입니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100025602.1.8",
          "form": "길냥이가 집안 옷장에 몰래 애기 두마리 낳고 갔어요",
          "original_form": "길냥이가 집안 옷장에 몰래 애기 두마리 낳고 갔어요",
          "speaker_id": "1",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100025602.1.9",
          "form": "맞아요 ㅋㅋㅋㅋ 엄마닮아 장모터앙믹스? 흰애랑 고등어랑 이렇게요",
          "original_form": "맞아요 ㅋㅋㅋㅋ 엄마닮아 장모터앙믹스? 흰애랑 고등어랑 이렇게요",
          "speaker_id": "1",
          "time": "20210901 18:55"
        },
        {
          "id": "MDRW2100025602.1.10",
          "form": "아빠가 고등어였나봐요 ㅋㅋ",
          "original_form": "아빠가 고등어였나봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:55"
        },
        {
          "id": "MDRW2100025602.1.11",
          "form": "집안옷장이요?ㄷㄷ임보하신건가요? 아님 어캐들어온거지 ㅋㅋ",
          "original_form": "집안옷장이요?ㄷㄷ임보하신건가요? 아님 어캐들어온거지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100025602.1.12",
          "form": "빌라 1층이라 창문 타고 올라오더라구요 ㄷㄷ 똑똑한 애였어요",
          "original_form": "빌라 1층이라 창문 타고 올라오더라구요 ㄷㄷ 똑똑한 애였어요",
          "speaker_id": "1",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100025602.1.13",
          "form": "근데 사람손은 안타던앤데...",
          "original_form": "근데 사람손은 안타던앤데...",
          "speaker_id": "1",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100025602.1.14",
          "form": "고양이의 고짜도 모르던 시절이라 너무 놀랬어요",
          "original_form": "고양이의 고짜도 모르던 시절이라 너무 놀랬어요",
          "speaker_id": "1",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100025602.1.15",
          "form": "세상에 똑똑해!",
          "original_form": "세상에 똑똑해!",
          "speaker_id": "2",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100025602.1.16",
          "form": "그럼 완전 신생아때부터 키우신거네요?",
          "original_form": "그럼 완전 신생아때부터 키우신거네요?",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100025602.1.17",
          "form": "네 고양이가 집안에 있는지도 몰랐는데 옷장에서 튀어나와서 ㄷㄷ",
          "original_form": "네 고양이가 집안에 있는지도 몰랐는데 옷장에서 튀어나와서 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100025602.1.18",
          "form": "가방 위에 두마리를 낳아놨더라구요",
          "original_form": "가방 위에 두마리를 낳아놨더라구요",
          "speaker_id": "1",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100025602.1.19",
          "form": "집안에 있는지도 몰랐을 정도면 ㄷㄷ",
          "original_form": "집안에 있는지도 몰랐을 정도면 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100025602.1.20",
          "form": "스파이수준 ㄷㄷㄷ",
          "original_form": "스파이수준 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100025602.1.21",
          "form": "그러고 그냥 빠이한거에요?",
          "original_form": "그러고 그냥 빠이한거에요?",
          "speaker_id": "2",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100025602.1.22",
          "form": "하긴 고양이들은 칼같이 독립할시기되면 정땐다고듣긴했어요",
          "original_form": "하긴 고양이들은 칼같이 독립할시기되면 정땐다고듣긴했어요",
          "speaker_id": "2",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100025602.1.23",
          "form": "ㅋㅋㅋㅋ 아뇨 2개월 넘게 젖은 물리더라구여 근데 창문으로 들락날락 거렸어요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 아뇨 2개월 넘게 젖은 물리더라구여 근데 창문으로 들락날락 거렸어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100025602.1.24",
          "form": "아고 애기들 때매 창문도 못닫고사셨겠어요ㄷㄷ",
          "original_form": "아고 애기들 때매 창문도 못닫고사셨겠어요ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100025602.1.25",
          "form": "언제올지 모르니",
          "original_form": "언제올지 모르니",
          "speaker_id": "2",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100025602.1.26",
          "form": "한 3개월쯤부터 엄마가 잘안오더니",
          "original_form": "한 3개월쯤부터 엄마가 잘안오더니",
          "speaker_id": "1",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100025602.1.27",
          "form": "근데 그래놓고 그냥 그집에 입양시켜주고 갔어 ㄷㄷㄷ",
          "original_form": "근데 그래놓고 그냥 그집에 입양시켜주고 갔어 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100025602.1.28",
          "form": "안보이네요",
          "original_form": "안보이네요",
          "speaker_id": "1",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100025602.1.29",
          "form": "4년전 ㄷㄷ",
          "original_form": "4년전 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100025602.1.30",
          "form": "여름이라 살았어요 ㅋㅋ",
          "original_form": "여름이라 살았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100025602.1.31",
          "form": "악 그러고 4년이 지난건가요? ㅎㅎ",
          "original_form": "악 그러고 4년이 지난건가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100025602.1.32",
          "form": "이건 진짜 묘연이다 ㅋㅋㅋ",
          "original_form": "이건 진짜 묘연이다 ㅋㅋㅋ",
