      "utterance": [
        {
          "id": "MDRW2100023607.1.1",
          "form": "그러게 맛있어 보이더라\n할인율도 크고",
          "original_form": "그러게 맛있어 보이더라\n할인율도 크고",
          "speaker_id": "2",
          "time": "20210830 12:49"
        },
        {
          "id": "MDRW2100023607.1.2",
          "form": "이거근데 단점이 여러개사면 천원할인이 하나밖에안되더라",
          "original_form": "이거근데 단점이 여러개사면 천원할인이 하나밖에안되더라",
          "speaker_id": "1",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100023607.1.3",
          "form": "배송비때메 어쩔수없이 천원할인포기함 ㅠ",
          "original_form": "배송비때메 어쩔수없이 천원할인포기함 ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100023607.1.4",
          "form": "왜저런지몰게써",
          "original_form": "왜저런지몰게써",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023607.1.5",
          "form": "아쉽다 그래도 저렴히 산거아이가?\n일부러 막은걸수도있지만 잘 산거같다",
          "original_form": "아쉽다 그래도 저렴히 산거아이가?\n일부러 막은걸수도있지만 잘 산거같다",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023607.1.6",
          "form": "ㅇㅇ맛있었으면 좋겠다",
          "original_form": "ㅇㅇ맛있었으면 좋겠다",
          "speaker_id": "1",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100023607.1.7",
          "form": "나 굳지않는 떡도 샀고",
          "original_form": "나 굳지않는 떡도 샀고",
          "speaker_id": "1",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100023607.1.8",
          "form": "등갈비도 샀고",
          "original_form": "등갈비도 샀고",
          "speaker_id": "1",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100023607.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100023607.1.10",
          "form": "오 맛집 차리나ㅋㅋ",
          "original_form": "오 맛집 차리나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100023607.1.11",
          "form": "좋겠다~",
          "original_form": "좋겠다~",
          "speaker_id": "2",
          "time": "20210830 18:13"
        },
        {
          "id": "MDRW2100023607.1.12",
          "form": "시간이 벌써 저녁시간이다",
          "original_form": "시간이 벌써 저녁시간이다",
          "speaker_id": "2",
          "time": "20210830 18:17"
        },
        {
          "id": "MDRW2100023607.1.13",
          "form": "어캐 저녁 먹으까 마까 생각중ㅎ",
          "original_form": "어캐 저녁 먹으까 마까 생각중ㅎ",
          "speaker_id": "2",
          "time": "20210830 18:17"
        },
        {
          "id": "MDRW2100023607.1.14",
          "form": "난 아몬드후레이크 먹음",
          "original_form": "난 아몬드후레이크 먹음",
          "speaker_id": "1",
          "time": "20210830 18:17"
        },
        {
          "id": "MDRW2100023607.1.15",
          "form": "씨리얼",
          "original_form": "씨리얼",
          "speaker_id": "1",
          "time": "20210830 18:18"
        },
        {
          "id": "MDRW2100023607.1.16",
          "form": "나 아침에억음ㅎ",
          "original_form": "나 아침에억음ㅎ",
          "speaker_id": "2",
          "time": "20210830 18:18"
        },
        {
          "id": "MDRW2100023607.1.17",
          "form": "씨리얼은 역시 아몬드가 젤 좋더라",
          "original_form": "씨리얼은 역시 아몬드가 젤 좋더라",
          "speaker_id": "2",
          "time": "20210830 18:18"
        },
        {
          "id": "MDRW2100023607.1.18",
          "form": "ㅇㅇ아몬드시리얼이 젤 나아",
          "original_form": "ㅇㅇ아몬드시리얼이 젤 나아",
          "speaker_id": "1",
          "time": "20210830 18:19"
        },
        {
          "id": "MDRW2100023607.1.19",
          "form": "그치 이것저것 먹어봐도 잠깐이고\n아몬드는 질리지않아\n아몬드씨리얼 회사 큰데 2곳이자나\n나 이름 잊어서ㅋ 원래 먹던데 아이고 다른데 먹었더니 맛 차이나더라",
          "original_form": "그치 이것저것 먹어봐도 잠깐이고\n아몬드는 질리지않아\n아몬드씨리얼 회사 큰데 2곳이자나\n나 이름 잊어서ㅋ 원래 먹던데 아이고 다른데 먹었더니 맛 차이나더라",
          "speaker_id": "2",
          "time": "20210830 18:20"
        },
        {
          "id": "MDRW2100023607.1.20",
          "form": "아진짜? 난 켈로그나 포스트 둘다 걍 먹는듯",
          "original_form": "아진짜? 난 켈로그나 포스트 둘다 걍 먹는듯",
          "speaker_id": "1",
          "time": "20210830 18:25"
        },
        {
          "id": "MDRW2100023607.1.21",
          "form": "지금 먹는건 포스트였나?",
          "original_form": "지금 먹는건 포스트였나?",
          "speaker_id": "1",
          "time": "20210830 18:26"
        },
        {
          "id": "MDRW2100023607.1.22",
          "form": "맛이 달라 안사야지 했었는데\n내가 좋아하는 맛 회사가 어딘지 매번 까묵",
          "original_form": "맛이 달라 안사야지 했었는데\n내가 좋아하는 맛 회사가 어딘지 매번 까묵",
          "speaker_id": "2",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100023607.1.23",
          "form": "맞나 나 이번에 산건 덜 달더라 켈로그는 더 달았던거같아",
          "original_form": "맞나 나 이번에 산건 덜 달더라 켈로그는 더 달았던거같아",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023607.1.24",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023607.1.25",
          "form": "내 좋아하는건 겔로그 인가보다ㅋ",
          "original_form": "내 좋아하는건 겔로그 인가보다ㅋ",
          "speaker_id": "2",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023607.1.26",
          "form": "맞나 ㅋㅋ 난 이번에 먹어보니 덜 단게 낫더라",
          "original_form": "맞나 ㅋㅋ 난 이번에 먹어보니 덜 단게 낫더라",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023607.1.27",
          "form": "난 단거ㅋㅋ",
          "original_form": "난 단거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100023607.1.28",
          "form": "겔로그가 동원? 동서,",
          "original_form": "겔로그가 동원? 동서,",
          "speaker_id": "2",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100023607.1.29",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100023607.1.30",
          "form": "포스트가 동서",
          "original_form": "포스트가 동서",
          "speaker_id": "1",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100023607.1.31",
          "form": "동서로 기억했는데 또 혼란ㅎ\n안단거가 동서?",
          "original_form": "동서로 기억했는데 또 혼란ㅎ\n안단거가 동서?",
          "speaker_id": "2",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100023607.1.32",
          "form": "ㅇㅇ동서 포스트",
          "original_form": "ㅇㅇ동서 포스트",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023607.1.33",
          "form": "기억해야겠다ㅋㅋ",
          "original_form": "기억해야겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023607.1.34",
          "form": "저녁밥 잔치국수 먹었어",
          "original_form": "저녁밥 잔치국수 먹었어",
          "speaker_id": "2",
          "time": "20210830 19:53"
        }
      ]
    }
