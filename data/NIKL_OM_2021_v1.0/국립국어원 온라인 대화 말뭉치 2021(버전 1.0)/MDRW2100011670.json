      "utterance": [
        {
          "id": "MDRW2100011670.1.1",
          "form": "여기는 이제 비가 많이 와요ㅠㅠ",
          "original_form": "여기는 이제 비가 많이 와요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011670.1.2",
          "form": "저는 안에 있어서 비가 오는지 잘 안보이네요ㅠ",
          "original_form": "저는 안에 있어서 비가 오는지 잘 안보이네요ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011670.1.3",
          "form": "비 이번주 내내 올거같던데요",
          "original_form": "비 이번주 내내 올거같던데요",
          "speaker_id": "2",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011670.1.4",
          "form": "네 태풍도 왔다하구...날이 벌써 시원해지네요",
          "original_form": "네 태풍도 왔다하구...날이 벌써 시원해지네요",
          "speaker_id": "1",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011670.1.5",
          "form": "맞아요 이러다가 금방 겨울될지도...",
          "original_form": "맞아요 이러다가 금방 겨울될지도...",
          "speaker_id": "2",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011670.1.6",
          "form": "코트 금방 꺼낼거같아요",
          "original_form": "코트 금방 꺼낼거같아요",
          "speaker_id": "2",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011670.1.7",
          "form": "그러게요ㅎㅎ 이번 가을엔 트렌치 꼭 입어보구 싶은데 금방 패딩입을거같아요",
          "original_form": "그러게요ㅎㅎ 이번 가을엔 트렌치 꼭 입어보구 싶은데 금방 패딩입을거같아요",
          "speaker_id": "1",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011670.1.8",
          "form": "비오니까 진짜 띵까띵까 집에서 쉬고싶은데",
          "original_form": "비오니까 진짜 띵까띵까 집에서 쉬고싶은데",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011670.1.9",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011670.1.10",
          "form": "ㅠㅠ저는 카페가서 비오는거 구경하고 싶어요ㅠㅠ",
          "original_form": "ㅠㅠ저는 카페가서 비오는거 구경하고 싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011670.1.11",
          "form": "ㅋㅋㅋㅋ실내에서 보는 비가 최고",
          "original_form": "ㅋㅋㅋㅋ실내에서 보는 비가 최고",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011670.1.12",
          "form": "옷 젖는거는 싫은데",
          "original_form": "옷 젖는거는 싫은데",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011670.1.13",
          "form": "맞아요 축축한건 싫어서ㅋㅋ",
          "original_form": "맞아요 축축한건 싫어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011670.1.14",
          "form": "비 소리는 좋은거같아요",
          "original_form": "비 소리는 좋은거같아요",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011670.1.15",
          "form": "오 저도요 천둥은 싫구ㅋㅋ",
          "original_form": "오 저도요 천둥은 싫구ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011670.1.16",
          "form": "이번 여름은 막판에 비가 장마처럼 와서",
          "original_form": "이번 여름은 막판에 비가 장마처럼 와서",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011670.1.17",
          "form": "진짜 지구가 아프긴아픈가싶기도하고...",
          "original_form": "진짜 지구가 아프긴아픈가싶기도하고...",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011670.1.18",
          "form": "6월 이때가 주로 장마였던거같은데...",
          "original_form": "6월 이때가 주로 장마였던거같은데...",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011670.1.19",
          "form": "맞아요 가을에 장마라니...이상해요",
          "original_form": "맞아요 가을에 장마라니...이상해요",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011670.1.20",
          "form": "저 그래서 그때 일부러 장화도 샀는데",
          "original_form": "저 그래서 그때 일부러 장화도 샀는데",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011670.1.21",
          "form": "두달묵혀두다가",
          "original_form": "두달묵혀두다가",
          "speaker_id": "2",
