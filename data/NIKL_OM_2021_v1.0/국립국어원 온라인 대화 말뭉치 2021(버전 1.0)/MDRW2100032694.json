      "utterance": [
        {
          "id": "MDRW2100032694.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032694.1.2",
          "form": "요즘 ott 보는거 있어요?",
          "original_form": "요즘 ott 보는거 있어요?",
          "speaker_id": "1",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032694.1.3",
          "form": "아니요! 저는 유투브만 봐요!",
          "original_form": "아니요! 저는 유투브만 봐요!",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032694.1.4",
          "form": "뮤ㅓ 재밌는거 보세요?",
          "original_form": "뮤ㅓ 재밌는거 보세요?",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032694.1.5",
          "form": "저는 넷플로 드라마 자주 봐요!",
          "original_form": "저는 넷플로 드라마 자주 봐요!",
          "speaker_id": "1",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100032694.1.6",
          "form": "어떤 드라마 보세요?",
          "original_form": "어떤 드라마 보세요?",
          "speaker_id": "2",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032694.1.7",
          "form": "슬의랑 비밀의 숲 재미있게 봤어요!",
          "original_form": "슬의랑 비밀의 숲 재미있게 봤어요!",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032694.1.8",
          "form": "슬기로운 의사생활이요?",
          "original_form": "슬기로운 의사생활이요?",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032694.1.9",
          "form": "저는 시즌1보다가 하차했더니 다시 보기가 어려워요",
          "original_form": "저는 시즌1보다가 하차했더니 다시 보기가 어려워요",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032694.1.10",
          "form": "아 맞아요 한회당 길이가 조금 긴편이긴 하죠ㅠ",
          "original_form": "아 맞아요 한회당 길이가 조금 긴편이긴 하죠ㅠ",
          "speaker_id": "1",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100032694.1.11",
          "form": "맞아요 그리고 러브라인이 너무 뜬금없는거 같아서요",
          "original_form": "맞아요 그리고 러브라인이 너무 뜬금없는거 같아서요",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100032694.1.12",
          "form": "굳이 없어도 될거 같은데?? 하는 느김..!",
          "original_form": "굳이 없어도 될거 같은데?? 하는 느김..!",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100032694.1.13",
          "form": "심심님은 어떠신가요",
          "original_form": "심심님은 어떠신가요",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100032694.1.14",
          "form": "요즘 자극적인 드라마가 많아서 그런지",
          "original_form": "요즘 자극적인 드라마가 많아서 그런지",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032694.1.15",
          "form": "편하게 틀어놓고 보기에 좋은거 같더라고요",
          "original_form": "편하게 틀어놓고 보기에 좋은거 같더라고요",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032694.1.16",
          "form": "앗 그건 또 그렇네요!",
          "original_form": "앗 그건 또 그렇네요!",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100032694.1.17",
          "form": "제일 좋아하는 드라마는 어떤거세요?",
          "original_form": "제일 좋아하는 드라마는 어떤거세요?",
          "speaker_id": "2",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032694.1.18",
          "form": "라이프 온 마스 아세요?",
          "original_form": "라이프 온 마스 아세요?",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032694.1.19",
          "form": "저는 이 드라마가 재미있더라고요!",
          "original_form": "저는 이 드라마가 재미있더라고요!",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032694.1.20",
          "form": "오 네!! 전 안봤지만 저희 가족들이 엄청 재밌게 봤었아요!!",
          "original_form": "오 네!! 전 안봤지만 저희 가족들이 엄청 재밌게 봤었아요!!",
          "speaker_id": "2",
          "time": "20210906 08:00"
        },
        {
          "id": "MDRW2100032694.1.21",
          "form": "정경호 나오는 드라마죠!",
          "original_form": "정경호 나오는 드라마죠!",
          "speaker_id": "2",
          "time": "20210906 08:00"
        },
        {
          "id": "MDRW2100032694.1.22",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "1",
          "time": "20210906 10:52"
        },
        {
          "id": "MDRW2100032694.1.23",
          "form": "거의 정경호 브이로그일 정도로 많이나오더라고요ㅋㅋㅋ",
          "original_form": "거의 정경호 브이로그일 정도로 많이나오더라고요ㅋㅋㅋ",
