      "utterance": [
        {
          "id": "MDRW2100030273.1.1",
          "form": "음 이마트에서 장봤어요 ㅎㅎ",
          "original_form": "음 이마트에서 장봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030273.1.2",
          "form": "주말 먹거리 사신건가요 ?",
          "original_form": "주말 먹거리 사신건가요 ?",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030273.1.3",
          "form": "목적은 그랬는뎅",
          "original_form": "목적은 그랬는뎅",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030273.1.4",
          "form": "어제 반먹었네요 ㅎㅎ",
          "original_form": "어제 반먹었네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030273.1.5",
          "form": "요즘 물가가 비싸서 많이 구매가 되지 않죠",
          "original_form": "요즘 물가가 비싸서 많이 구매가 되지 않죠",
          "speaker_id": "1",
          "time": "20210904 07:56"
        },
        {
          "id": "MDRW2100030273.1.6",
          "form": "계란이 너무 비싸요ㅠ",
          "original_form": "계란이 너무 비싸요ㅠ",
          "speaker_id": "2",
          "time": "20210904 08:01"
        },
        {
          "id": "MDRW2100030273.1.7",
          "form": "채소값도 장난 아니죠",
          "original_form": "채소값도 장난 아니죠",
          "speaker_id": "1",
          "time": "20210904 08:21"
        },
        {
          "id": "MDRW2100030273.1.8",
          "form": "그래서 본가 가서 털어옵니다 ㅋㅋ",
          "original_form": "그래서 본가 가서 털어옵니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 08:27"
        },
        {
          "id": "MDRW2100030273.1.9",
          "form": "점점 장보기가 힘들어지네요",
          "original_form": "점점 장보기가 힘들어지네요",
          "speaker_id": "1",
          "time": "20210904 08:29"
        },
        {
          "id": "MDRW2100030273.1.10",
          "form": "전 주말에 일주일 먹을 장보러 갑니다",
          "original_form": "전 주말에 일주일 먹을 장보러 갑니다",
          "speaker_id": "1",
          "time": "20210904 08:30"
        },
        {
          "id": "MDRW2100030273.1.11",
          "form": "어디로 가세요 ㅎㅎ",
          "original_form": "어디로 가세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 08:32"
        },
        {
          "id": "MDRW2100030273.1.12",
          "form": "주로 롯데마트로 가고 가끔은 이마트도 가요",
          "original_form": "주로 롯데마트로 가고 가끔은 이마트도 가요",
          "speaker_id": "1",
          "time": "20210904 08:33"
        },
        {
          "id": "MDRW2100030273.1.13",
          "form": "롯데가 더 좋나보네요",
          "original_form": "롯데가 더 좋나보네요",
          "speaker_id": "2",
          "time": "20210904 08:39"
        },
        {
          "id": "MDRW2100030273.1.14",
          "form": "두곳 다 장단점이 있죠",
          "original_form": "두곳 다 장단점이 있죠",
          "speaker_id": "1",
          "time": "20210904 08:55"
        },
        {
          "id": "MDRW2100030273.1.15",
          "form": "롯데의 장점은 뭔가요",
          "original_form": "롯데의 장점은 뭔가요",
          "speaker_id": "2",
          "time": "20210904 09:00"
        },
        {
          "id": "MDRW2100030273.1.16",
          "form": "생선류 (탕) 초밥 덮밥 쪽 구매가 좋아요",
          "original_form": "생선류 (탕) 초밥 덮밥 쪽 구매가 좋아요",
          "speaker_id": "1",
          "time": "20210904 09:03"
        },
        {
          "id": "MDRW2100030273.1.17",
          "form": "오 초밥 좋네요",
          "original_form": "오 초밥 좋네요",
          "speaker_id": "2",
          "time": "20210904 09:05"
        }
