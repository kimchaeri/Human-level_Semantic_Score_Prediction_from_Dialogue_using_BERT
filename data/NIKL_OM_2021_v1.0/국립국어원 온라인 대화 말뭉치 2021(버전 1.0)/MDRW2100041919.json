      "utterance": [
        {
          "id": "MDRW2100041919.1.1",
          "form": "ㅜㅜ진짜 휴덕중인 저도 공연이그리운데",
          "original_form": "ㅜㅜ진짜 휴덕중인 저도 공연이그리운데",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041919.1.2",
          "form": "얼마나 가고싶으실까",
          "original_form": "얼마나 가고싶으실까",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041919.1.3",
          "form": "아맞다 요새 저",
          "original_form": "아맞다 요새 저",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041919.1.4",
          "form": "ㅠㅁㅠ \n코로나 좀 잠잠해질때 콘서트공지났었거든요  티켓팅도다했는데 못가게돼서 얼마나슬펐던지...",
          "original_form": "ㅠㅁㅠ \n코로나 좀 잠잠해질때 콘서트공지났었거든요  티켓팅도다했는데 못가게돼서 얼마나슬펐던지...",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041919.1.5",
          "form": "환승연애",
          "original_form": "환승연애",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041919.1.6",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041919.1.7",
          "form": "달리고잇어요",
          "original_form": "달리고잇어요",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041919.1.8",
          "form": "오 그거 재밌나요ㅇㅁㅇ?",
          "original_form": "오 그거 재밌나요ㅇㅁㅇ?",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041919.1.9",
          "form": "저는 하트시그널도 다봣어서",
          "original_form": "저는 하트시그널도 다봣어서",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041919.1.10",
          "form": "환승연애 재밋게 가볍게 잘보구잇어여",
          "original_form": "환승연애 재밋게 가볍게 잘보구잇어여",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041919.1.11",
          "form": "콘서트 갈때마다 예전엔 서울가는게 힘들엇던 기억이...",
          "original_form": "콘서트 갈때마다 예전엔 서울가는게 힘들엇던 기억이...",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041919.1.12",
          "form": "제 빅뱅덕질 흑역사에유....ㅋㅋㅋㅋ",
          "original_form": "제 빅뱅덕질 흑역사에유....ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041919.1.13",
          "form": "하트시그널도 저는 안봤지요ㅋㅋㅋ일반인 나오는거 잘안보게되더라구요ㅋㅋㄱㅋ",
          "original_form": "하트시그널도 저는 안봤지요ㅋㅋㅋ일반인 나오는거 잘안보게되더라구요ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041919.1.14",
          "form": "앗 빅뱅ㅋㅋㅋㅋ",
          "original_form": "앗 빅뱅ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041919.1.15",
          "form": "인기쩔었는데..",
          "original_form": "인기쩔었는데..",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041919.1.16",
          "form": "사라졌어...",
          "original_form": "사라졌어...",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041919.1.17",
          "form": "제친구들도 일반인연애 안물안궁이라고",
          "original_form": "제친구들도 일반인연애 안물안궁이라고",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041919.1.18",
          "form": "안보더라구요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "안보더라구요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041919.1.19",
          "form": "ㅋㅋㅋㅋㅋㄲㅋ그리구 핫게에서 환승연애출연자 욕하는거? 뭐본거있는데",
          "original_form": "ㅋㅋㅋㅋㅋㄲㅋ그리구 핫게에서 환승연애출연자 욕하는거? 뭐본거있는데",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041919.1.20",
          "form": "제가",
          "original_form": "제가",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041919.1.21",
          "form": "그거보다가",
          "original_form": "그거보다가",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041919.1.22",
          "form": "아 방송에서 이런다구? 하면서ㅋㅋㄱ..",
          "original_form": "아 방송에서 이런다구? 하면서ㅋㅋㄱ..",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041919.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041919.1.24",
          "form": "도대체머지 싶어서",
          "original_form": "도대체머지 싶어서",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041919.1.25",
          "form": "보다가",
          "original_form": "보다가",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041919.1.26",
          "form": "지금 정주행하고잇어유",
          "original_form": "지금 정주행하고잇어유",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041919.1.27",
          "form": "악ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041919.1.28",
          "form": "저는 댓글로대강파악하구말았지여ㅋㅋㄱㅋㅋ",
          "original_form": "저는 댓글로대강파악하구말았지여ㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:43"
        }
      ]
