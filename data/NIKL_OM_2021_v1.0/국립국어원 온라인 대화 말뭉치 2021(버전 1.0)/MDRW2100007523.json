      "utterance": [
        {
          "id": "MDRW2100007523.1.1",
          "form": "여행 좋아하시나용??",
          "original_form": "여행 좋아하시나용??",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007523.1.2",
          "form": "네 좋아하는데 자주 가지는 못해요",
          "original_form": "네 좋아하는데 자주 가지는 못해요",
          "speaker_id": "2",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007523.1.3",
          "form": "최근에 어디 가셨어요?",
          "original_form": "최근에 어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007523.1.4",
          "form": "저도 요즘에는 자주가지 못하지만",
          "original_form": "저도 요즘에는 자주가지 못하지만",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007523.1.5",
          "form": "얼마전에 울릉도에 다녀왔어요",
          "original_form": "얼마전에 울릉도에 다녀왔어요",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007523.1.6",
          "form": "어디 다녀오신곳 있으시요??",
          "original_form": "어디 다녀오신곳 있으시요??",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007523.1.7",
          "form": "와 얼마나 걸렸어요?",
          "original_form": "와 얼마나 걸렸어요?",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007523.1.8",
          "form": "저는 여행간지 오래돼서 기억도 안나요ㅎㅎ",
          "original_form": "저는 여행간지 오래돼서 기억도 안나요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007523.1.9",
          "form": "가는데만 6시간 넘게 걸린것 같아요ㅠㅠ",
          "original_form": "가는데만 6시간 넘게 걸린것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007523.1.10",
          "form": "버스타고.. 배타고...",
          "original_form": "버스타고.. 배타고...",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007523.1.11",
          "form": "여행 좋지만 요즘 코로나라 쉽지 않죠ㅠㅠ",
          "original_form": "여행 좋지만 요즘 코로나라 쉽지 않죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:55"
        },
        {
          "id": "MDRW2100007523.1.12",
          "form": "생각보다는 오래 안걸리네요",
          "original_form": "생각보다는 오래 안걸리네요",
          "speaker_id": "2",
          "time": "20210822 02:55"
        },
        {
          "id": "MDRW2100007523.1.13",
          "form": "여행은 이동시간에도 즐거운거같아요",
          "original_form": "여행은 이동시간에도 즐거운거같아요",
          "speaker_id": "2",
          "time": "20210822 02:55"
        },
        {
          "id": "MDRW2100007523.1.14",
          "form": "설레고",
          "original_form": "설레고",
          "speaker_id": "2",
          "time": "20210822 02:55"
        },
        {
          "id": "MDRW2100007523.1.15",
          "form": "맞아요 여행떠날때의 기분 넘 좋아요",
          "original_form": "맞아요 여행떠날때의 기분 넘 좋아요",
          "speaker_id": "1",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007523.1.16",
          "form": "올해는 그럼 여행 계획 딱히 없으신가요?",
          "original_form": "올해는 그럼 여행 계획 딱히 없으신가요?",
          "speaker_id": "1",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007523.1.17",
          "form": "기분좋죠 피곤하지만",
          "original_form": "기분좋죠 피곤하지만",
          "speaker_id": "2",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007523.1.18",
          "form": "계획은 딱히 없네요",
          "original_form": "계획은 딱히 없네요",
          "speaker_id": "2",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007523.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007523.1.20",
          "form": "유튭으로 브이로그 보고 대리만족하는것도 재밌어요",
          "original_form": "유튭으로 브이로그 보고 대리만족하는것도 재밌어요",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007523.1.21",
          "form": "피곤하지만ㅠㅠ",
          "original_form": "피곤하지만ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007523.1.22",
          "form": "맞아요 요즘엔 온라인 여행이 대세라고 하더라구요",
          "original_form": "맞아요 요즘엔 온라인 여행이 대세라고 하더라구요",
          "speaker_id": "1",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007523.1.23",
          "form": "몸이 편해서 좋아요",
          "original_form": "몸이 편해서 좋아요",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007523.1.24",
          "form": "돈도 안들고",
          "original_form": "돈도 안들고",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007523.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 02:59"
        },
        {
          "id": "MDRW2100007523.1.26",
          "form": "인터넷만 있다면 어디든 가능하니가요ㅎㅎ",
          "original_form": "인터넷만 있다면 어디든 가능하니가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:59"
        }
      ]
