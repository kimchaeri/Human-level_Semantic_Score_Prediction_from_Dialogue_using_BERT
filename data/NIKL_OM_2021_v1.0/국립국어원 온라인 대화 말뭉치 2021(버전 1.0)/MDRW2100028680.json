      "utterance": [
        {
          "id": "MDRW2100028680.1.1",
          "form": "근데 제친구도",
          "original_form": "근데 제친구도",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028680.1.2",
          "form": "그냥 싫다는건 말이 안되는거 같아요",
          "original_form": "그냥 싫다는건 말이 안되는거 같아요",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028680.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028680.1.4",
          "form": "정말 남친 엄청 좋아하고",
          "original_form": "정말 남친 엄청 좋아하고",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028680.1.5",
          "form": "첫 연애인데 6년 만나는 중이거든요",
          "original_form": "첫 연애인데 6년 만나는 중이거든요",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028680.1.6",
          "form": "근데 부모님이 엄청 반대해요",
          "original_form": "근데 부모님이 엄청 반대해요",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028680.1.7",
          "form": "특히 남자 부모님이",
          "original_form": "특히 남자 부모님이",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028680.1.8",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028680.1.9",
          "form": "물어봐도 되나요?",
          "original_form": "물어봐도 되나요?",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028680.1.10",
          "form": "지금 현재 친구 남친이 딱 30대 되고서는",
          "original_form": "지금 현재 친구 남친이 딱 30대 되고서는",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028680.1.11",
          "form": "회사 관두고 대학원 가더니",
          "original_form": "회사 관두고 대학원 가더니",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028680.1.12",
          "form": "외국으로 유학가서",
          "original_form": "외국으로 유학가서",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028680.1.13",
          "form": "3년뒤에 돌아올 예정인데",
          "original_form": "3년뒤에 돌아올 예정인데",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028680.1.14",
          "form": "3년이요?",
          "original_form": "3년이요?",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028680.1.15",
          "form": "보통 이러면 결혼을 하고 가거나",
          "original_form": "보통 이러면 결혼을 하고 가거나",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028680.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028680.1.17",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028680.1.18",
          "form": "뭔가 부모님께 소개는 시키고 가야하자나요?",
          "original_form": "뭔가 부모님께 소개는 시키고 가야하자나요?",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028680.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028680.1.20",
          "form": "근데 둘다 안하고",
          "original_form": "근데 둘다 안하고",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028680.1.21",
          "form": "그냥 미안해하고 있는 중이라",
          "original_form": "그냥 미안해하고 있는 중이라",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028680.1.22",
          "form": "너무 이기적이네요",
          "original_form": "너무 이기적이네요",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028680.1.23",
          "form": "제가 개인적으로 왜 그 남자 붙들고 잇는지 이해안되는 중인데",
          "original_form": "제가 개인적으로 왜 그 남자 붙들고 잇는지 이해안되는 중인데",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028680.1.24",
          "form": "뭐 그만큼 좋고 얘보다 좋은 남잘 못만날거같다는 거에요",
          "original_form": "뭐 그만큼 좋고 얘보다 좋은 남잘 못만날거같다는 거에요",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028680.1.25",
          "form": "심지어 남자쪽에서 반대?",
          "original_form": "심지어 남자쪽에서 반대?",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028680.1.26",
          "form": "남친 어머님과",
          "original_form": "남친 어머님과",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028680.1.27",
          "form": "여친어머님이",
          "original_form": "여친어머님이",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028680.1.28",
          "form": "서로 동창? 계모임? 같은",
          "original_form": "서로 동창? 계모임? 같은",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028680.1.29",
          "form": "암튼 서로 아는 사인데",
          "original_form": "암튼 서로 아는 사인데",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028680.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028680.1.31",
          "form": "너무 서로 다른 스타일",
          "original_form": "너무 서로 다른 스타일",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028680.1.32",
          "form": "남친부모님 전업주부라",
          "original_form": "남친부모님 전업주부라",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028680.1.33",
          "form": "엄청 남편 모시고 사는 타입",
          "original_form": "엄청 남편 모시고 사는 타입",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028680.1.34",
          "form": "여친 부모님 워킹맘이고 뭐 흔히 모임에서",
          "original_form": "여친 부모님 워킹맘이고 뭐 흔히 모임에서",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028680.1.35",
          "form": "남편욕할때도 잇고 그런 타입",
          "original_form": "남편욕할때도 잇고 그런 타입",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028680.1.36",
          "form": "그래서 남친 어머님이 어케 아내가 남편 욕을하냐 저런집 장가가면 안된다",
          "original_form": "그래서 남친 어머님이 어케 아내가 남편 욕을하냐 저런집 장가가면 안된다",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028680.1.37",
          "form": "이런 상태신가봐요....ㅍ",
          "original_form": "이런 상태신가봐요....ㅍ",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028680.1.38",
          "form": "뭔....",
          "original_form": "뭔....",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028680.1.39",
          "form": "이유도 가지가지네요",
          "original_form": "이유도 가지가지네요",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028680.1.40",
          "form": "그초",
          "original_form": "그초",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028680.1.41",
          "form": "그래서 그쪽 어머님이 계속 남자애한테",
          "original_form": "그래서 그쪽 어머님이 계속 남자애한테",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028680.1.42",
          "form": "헤어져라 자주 압박하는데",
          "original_form": "헤어져라 자주 압박하는데",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028680.1.43",
          "form": "전혀 들어먹질 않으니",
          "original_form": "전혀 들어먹질 않으니",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028680.1.44",
          "form": "주룩님 친구가 더 아까운거 같은데",
          "original_form": "주룩님 친구가 더 아까운거 같은데",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028680.1.45",
          "form": "제 친구나 제친구 어머니 불러내서 지금 나이를 생각해라고 헤어지라고",
          "original_form": "제 친구나 제친구 어머니 불러내서 지금 나이를 생각해라고 헤어지라고",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028680.1.46",
          "form": "그니까어.....제친구 예쁘고 능력있고 집안도 나쁘지않고 성격도 매력있고 그런데",
          "original_form": "그니까어.....제친구 예쁘고 능력있고 집안도 나쁘지않고 성격도 매력있고 그런데",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028680.1.47",
          "form": "그땐그렇죠",
          "original_form": "그땐그렇죠",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028680.1.48",
          "form": "이사람같은 사람 없을거같고",
          "original_form": "이사람같은 사람 없을거같고",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028680.1.49",
          "form": "다신못만날거 같고",
          "original_form": "다신못만날거 같고",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028680.1.50",
          "form": "그러게요 한번 헤어지면 사실 별거아닌데",
          "original_form": "그러게요 한번 헤어지면 사실 별거아닌데",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028680.1.51",
          "form": "이별의 경험이 없다보니 견뎌낼 자신도 없고",
          "original_form": "이별의 경험이 없다보니 견뎌낼 자신도 없고",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028680.1.52",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028680.1.53",
          "form": "그래서 어머니께서 소개팅 한번 주선햇는데",
          "original_form": "그래서 어머니께서 소개팅 한번 주선햇는데",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028680.1.54",
          "form": "친구가 싫다 안나간다 버티고 있어가지고",
          "original_form": "친구가 싫다 안나간다 버티고 있어가지고",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028680.1.55",
          "form": "제가 효도하는 셈 치고 한번 다녀와드려...^^...하고 등 떠밀었어요",
          "original_form": "제가 효도하는 셈 치고 한번 다녀와드려...^^...하고 등 떠밀었어요",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028680.1.56",
          "form": "참....",
          "original_form": "참....",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028680.1.57",
          "form": "회사그만두고 유학이라니",
          "original_form": "회사그만두고 유학이라니",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028680.1.58",
          "form": "대학원까지는 이해되는데",
          "original_form": "대학원까지는 이해되는데",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028680.1.59",
          "form": "그니까요 게다가 가깝지도 않아요...유럽...",
          "original_form": "그니까요 게다가 가깝지도 않아요...유럽...",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028680.1.60",
          "form": "코로나땜에 유학 간뒤로 한번도 만나지도 못했거든요",
          "original_form": "코로나땜에 유학 간뒤로 한번도 만나지도 못했거든요",
          "speaker_id": "1",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100028680.1.61",
          "form": "좋은남자 만나면 그냥 환승... 하면 안되나",
          "original_form": "좋은남자 만나면 그냥 환승... 하면 안되나",
          "speaker_id": "2",
          "time": "20210902 21:27"
        }
      ]
