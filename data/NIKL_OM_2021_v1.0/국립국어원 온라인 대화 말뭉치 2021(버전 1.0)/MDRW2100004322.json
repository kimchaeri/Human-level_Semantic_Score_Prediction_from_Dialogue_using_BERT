      "utterance": [
        {
          "id": "MDRW2100004322.1.1",
          "form": "운동하시는것 있으세요?",
          "original_form": "운동하시는것 있으세요?",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004322.1.2",
          "form": "없어요 내일부터 홈트하려고요!",
          "original_form": "없어요 내일부터 홈트하려고요!",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004322.1.3",
          "form": "저는 헬스 다니는데 쉬고있어서 이제 다시 해야해요ㅎㅎ",
          "original_form": "저는 헬스 다니는데 쉬고있어서 이제 다시 해야해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004322.1.4",
          "form": "오 헬스 어때요?",
          "original_form": "오 헬스 어때요?",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004322.1.5",
          "form": "처음엔 재밌는데 갈수록 너무 귀찮아지는ㅎㅎㅎ",
          "original_form": "처음엔 재밌는데 갈수록 너무 귀찮아지는ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004322.1.6",
          "form": "앜ㅋㅋㅋㅋㅠㅠ",
          "original_form": "앜ㅋㅋㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004322.1.7",
          "form": "그건 국룰이져",
          "original_form": "그건 국룰이져",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004322.1.8",
          "form": "나중에 돈아깝고 눈물나요ㅎㅎ 살은 그대로구ㅎㅎ",
          "original_form": "나중에 돈아깝고 눈물나요ㅎㅎ 살은 그대로구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004322.1.9",
          "form": "피티도 받으셨어요?",
          "original_form": "피티도 받으셨어요?",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004322.1.10",
          "form": "네 작년초에 받았는데 코로나로 문닫고 그래서 ㅠㅠ 거의 못받고 끝났네요",
          "original_form": "네 작년초에 받았는데 코로나로 문닫고 그래서 ㅠㅠ 거의 못받고 끝났네요",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004322.1.11",
          "form": "홈트 뭐하시려구요?",
          "original_form": "홈트 뭐하시려구요?",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004322.1.12",
          "form": "역시나 땅끄부부요!",
          "original_form": "역시나 땅끄부부요!",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004322.1.13",
          "form": "오 저도 그 채널 좋아해요 허벅지살 많이 도움됐어요ㅎㅎ",
          "original_form": "오 저도 그 채널 좋아해요 허벅지살 많이 도움됐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004322.1.14",
          "form": "오 그래요? 제가 또 하비라 허벅지살이..",
          "original_form": "오 그래요? 제가 또 하비라 허벅지살이..",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004322.1.15",
          "form": "저도 하비라ㅠㅠㅋㅋ다른건 안해도 허벅지는 꼭 해야했어요ㅋㅋ",
          "original_form": "저도 하비라ㅠㅠㅋㅋ다른건 안해도 허벅지는 꼭 해야했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004322.1.16",
          "form": "마자요 하비는 슬퍼요",
          "original_form": "마자요 하비는 슬퍼요",
          "speaker_id": "2",
          "time": "20210821 16:14"
        }
      ]
