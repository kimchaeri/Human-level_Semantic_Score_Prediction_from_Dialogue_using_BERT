      "utterance": [
        {
          "id": "MDRW2100040870.1.1",
          "form": "여행은 어디로 가셨어요?",
          "original_form": "여행은 어디로 가셨어요?",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100040870.1.2",
          "form": "저 오늘은 인사동이랑 북촌, 시간되면 경복궁까지요",
          "original_form": "저 오늘은 인사동이랑 북촌, 시간되면 경복궁까지요",
          "speaker_id": "2",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100040870.1.3",
          "form": "와 너무좋네요. ㅋㅋㅋ",
          "original_form": "와 너무좋네요. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:02"
        },
        {
          "id": "MDRW2100040870.1.4",
          "form": "북촌가고싶었는데ㅜ못갔네요ㅠ",
          "original_form": "북촌가고싶었는데ㅜ못갔네요ㅠ",
          "speaker_id": "1",
          "time": "20210905 12:02"
        },
        {
          "id": "MDRW2100040870.1.5",
          "form": "북촌 예쁘죠?ㅋ",
          "original_form": "북촌 예쁘죠?ㅋ",
          "speaker_id": "1",
          "time": "20210905 12:03"
        },
        {
          "id": "MDRW2100040870.1.6",
          "form": "네넹, 한옥들 늘어서 있어서 사진도 잘 나와요",
          "original_form": "네넹, 한옥들 늘어서 있어서 사진도 잘 나와요",
          "speaker_id": "2",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040870.1.7",
          "form": "name1님은 올해 여행 가신 곳 있나요?",
          "original_form": "&name1&님은 올해 여행 가신 곳 있나요?",
          "speaker_id": "2",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040870.1.8",
          "form": "사진찍기에는 정말 최고일듯요ㅋㅋ",
          "original_form": "사진찍기에는 정말 최고일듯요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100040870.1.9",
          "form": "저는 여수갔다왔었어요ㅋㅋ",
          "original_form": "저는 여수갔다왔었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100040870.1.10",
          "form": "우와, 저 여수 가려다가 서울 왔는데",
          "original_form": "우와, 저 여수 가려다가 서울 왔는데",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100040870.1.11",
          "form": "아 그러셨구나ㅋㅋ",
          "original_form": "아 그러셨구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100040870.1.12",
          "form": "여수 좋던가요",
          "original_form": "여수 좋던가요",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100040870.1.13",
          "form": "여수도 진짜 좋더라구요.ㅋㅋ",
          "original_form": "여수도 진짜 좋더라구요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100040870.1.14",
          "form": "네 저도 오랫만에 갔는데 갔던 호텔도 너무 괜찮았구요.ㅋㅋ",
          "original_form": "네 저도 오랫만에 갔는데 갔던 호텔도 너무 괜찮았구요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100040870.1.15",
          "form": "아이들이랑 가기에는 좋았어요ㅋ",
          "original_form": "아이들이랑 가기에는 좋았어요ㅋ",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100040870.1.16",
          "form": "오, 호텔은 어디 묵으셨어요? 비싼 곳이었나요?",
          "original_form": "오, 호텔은 어디 묵으셨어요? 비싼 곳이었나요?",
          "speaker_id": "2",
          "time": "20210905 13:59"
        },
        {
          "id": "MDRW2100040870.1.17",
          "form": "가격대비 괜찮았어요ㅋ",
          "original_form": "가격대비 괜찮았어요ㅋ",
          "speaker_id": "1",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100040870.1.18",
          "form": "오, 다행이다",
          "original_form": "오, 다행이다",
          "speaker_id": "2",
          "time": "20210905 14:05"
        },
        {
          "id": "MDRW2100040870.1.19",
          "form": "저는 어제 숙소때문에 고생했어요",
          "original_form": "저는 어제 숙소때문에 고생했어요",
          "speaker_id": "2",
          "time": "20210905 14:05"
        },
        {
          "id": "MDRW2100040870.1.20",
          "form": "아 그러시구나ㅠ숙소가 문제있었나요?",
          "original_form": "아 그러시구나ㅠ숙소가 문제있었나요?",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100040870.1.21",
          "form": "최대3인 가능한 방을 예약했는데 가보니 입실거부당했어요ㅜㅜ",
          "original_form": "최대3인 가능한 방을 예약했는데 가보니 입실거부당했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100040870.1.22",
          "form": "유의사항에 언급이 있었대요",
          "original_form": "유의사항에 언급이 있었대요",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100040870.1.23",
          "form": "아하ㅜㅜ그렇구나",
          "original_form": "아하ㅜㅜ그렇구나",
          "speaker_id": "1",
          "time": "20210905 18:58"
        },
        {
          "id": "MDRW2100040870.1.24",
          "form": "정말 난감하셨겠어요",
          "original_form": "정말 난감하셨겠어요",
          "speaker_id": "1",
          "time": "20210905 18:58"
        }
      ]
    }
  ]
