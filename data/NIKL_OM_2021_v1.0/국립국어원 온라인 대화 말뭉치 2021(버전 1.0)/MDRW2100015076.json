      "utterance": [
        {
          "id": "MDRW2100015076.1.1",
          "form": "술은 전혀 안마셔요 ㅋ",
          "original_form": "술은 전혀 안마셔요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015076.1.2",
          "form": "지금 바른치킨 양념 먹었어요 ㅋ",
          "original_form": "지금 바른치킨 양념 먹었어요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015076.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015076.1.4",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015076.1.5",
          "form": "완전 맛있게 먹었어요",
          "original_form": "완전 맛있게 먹었어요",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015076.1.6",
          "form": "좋네요ㅋㅋㅋ",
          "original_form": "좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015076.1.7",
          "form": "말랑복 vs 딱복은 어떠세요?",
          "original_form": "말랑복 vs 딱복은 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015076.1.8",
          "form": "저는 말랑한복숭아 사랑해요 ㅋ 지금 냉장고에도 황도 복숭아 있어요",
          "original_form": "저는 말랑한복숭아 사랑해요 ㅋ 지금 냉장고에도 황도 복숭아 있어요",
          "speaker_id": "2",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100015076.1.9",
          "form": "ㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋ저도요",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100015076.1.10",
          "form": "복숭아 진짜 존맛이예요",
          "original_form": "복숭아 진짜 존맛이예요",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100015076.1.11",
          "form": "복숭아 지금 아니면 못먹죠 ㅜ",
          "original_form": "복숭아 지금 아니면 못먹죠 ㅜ",
          "speaker_id": "2",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100015076.1.12",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100015076.1.13",
          "form": "아쉬워요그래서",
          "original_form": "아쉬워요그래서",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100015076.1.14",
          "form": "황도와 백도 중에 어떤 복숭아를 더 선호하세용?",
          "original_form": "황도와 백도 중에 어떤 복숭아를 더 선호하세용?",
          "speaker_id": "2",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100015076.1.15",
          "form": "백도아니겠어요?!!!!!",
          "original_form": "백도아니겠어요?!!!!!",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100015076.1.16",
          "form": "그치용 ㅎㅎ 저녁은 뭐드실건가용?",
          "original_form": "그치용 ㅎㅎ 저녁은 뭐드실건가용?",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100015076.1.17",
          "form": "아직이요ㅋㅋㅋ",
          "original_form": "아직이요ㅋㅋㅋ",
