      "utterance": [
        {
          "id": "MDRW2100015301.1.1",
          "form": "음악들을 때 일반 유튜브 쓰시나요?",
          "original_form": "음악들을 때 일반 유튜브 쓰시나요?",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015301.1.2",
          "form": "네~ 일반 유튜브 말고 다른 게 있어요?",
          "original_form": "네~ 일반 유튜브 말고 다른 게 있어요?",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015301.1.3",
          "form": "아 유튜브 뮤직이요!",
          "original_form": "아 유튜브 뮤직이요!",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015301.1.4",
          "form": "유튭 프리미엄 쓰면 쓸수있는데",
          "original_form": "유튭 프리미엄 쓰면 쓸수있는데",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015301.1.5",
          "form": "무료체험중이라",
          "original_form": "무료체험중이라",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015301.1.6",
          "form": "오오 저 처음 들었어요",
          "original_form": "오오 저 처음 들었어요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015301.1.7",
          "form": "유튭 프리미엄 써본적 있는데 그때도 뮤직 따로 있는지 몰랐네요ㅋㅋㅋ",
          "original_form": "유튭 프리미엄 써본적 있는데 그때도 뮤직 따로 있는지 몰랐네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015301.1.8",
          "form": "아 아쉽네요ㅠ",
          "original_form": "아 아쉽네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015301.1.9",
          "form": "원래 무료고 광고나오는 정도였는데",
          "original_form": "원래 무료고 광고나오는 정도였는데",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015301.1.10",
          "form": "유튜브에서 갑자기 프리미엄만 되게끔 만들었더라고요",
          "original_form": "유튜브에서 갑자기 프리미엄만 되게끔 만들었더라고요",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015301.1.11",
          "form": "이런ㅠㅠ 사용자 모으고 인기있어져서 유료전환 했나보네요",
          "original_form": "이런ㅠㅠ 사용자 모으고 인기있어져서 유료전환 했나보네요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015301.1.12",
          "form": "일찍 알았으면 좋았을 뻔했어요",
          "original_form": "일찍 알았으면 좋았을 뻔했어요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015301.1.13",
          "form": "아 그런가봐요",
          "original_form": "아 그런가봐요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015301.1.14",
          "form": "유튜브로 노래 들은 게 올 봄 이후라 소식이 너무 늦었네요ㅋㅋ",
          "original_form": "유튜브로 노래 들은 게 올 봄 이후라 소식이 너무 늦었네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015301.1.15",
          "form": "별생각없이 왜 없앴지 라고만 생각했는데ㅋㅋㅋ",
          "original_form": "별생각없이 왜 없앴지 라고만 생각했는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015301.1.16",
          "form": "그런가보내요",
          "original_form": "그런가보내요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015301.1.17",
          "form": "ㅋㅋㅋㅋ 저는 아마 무료체험 끝나면",
          "original_form": "ㅋㅋㅋㅋ 저는 아마 무료체험 끝나면",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015301.1.18",
          "form": "유튜브 화면 내리고 듣지 않을까 생각합니다..",
          "original_form": "유튜브 화면 내리고 듣지 않을까 생각합니다..",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015301.1.19",
          "form": "ㅋㅋ 저도 광고제거 확장이랑 작은창 이용해서 들어요ㅋㅋ",
          "original_form": "ㅋㅋ 저도 광고제거 확장이랑 작은창 이용해서 들어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015301.1.20",
          "form": "앜ㅋㅋ 그런게있군요",
          "original_form": "앜ㅋㅋ 그런게있군요",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015301.1.21",
          "form": "예전에 그 앱까는건 해봤는데 늘 업데이트 해야되니까",
          "original_form": "예전에 그 앱까는건 해봤는데 늘 업데이트 해야되니까",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015301.1.22",
          "form": "또 귀찮더라고요",
          "original_form": "또 귀찮더라고요",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015301.1.23",
          "form": "아 번거로우면 안 돼죠 정신건강에 해로워요ㅋㅋ",
          "original_form": "아 번거로우면 안 돼죠 정신건강에 해로워요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015301.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015301.1.25",
          "form": "다른 컨텐츠플랫폼 이용하는 거 있으세요?",
          "original_form": "다른 컨텐츠플랫폼 이용하는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015301.1.26",
          "form": "저는 넷플써요!",
          "original_form": "저는 넷플써요!",
          "speaker_id": "2",
          "time": "20210825 22:44"
        }
      ]
    }
