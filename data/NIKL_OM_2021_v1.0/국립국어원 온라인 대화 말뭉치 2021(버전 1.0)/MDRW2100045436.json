        },
        {
          "id": "MDRW2100045436.1.2",
          "form": "엉 시청률도 잘 나옹",
          "original_form": "엉 시청률도 잘 나옹",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045436.1.3",
          "form": "그러네 16%",
          "original_form": "그러네 16%",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045436.1.4",
          "form": "이제훈이 연기 잘 하잖아",
          "original_form": "이제훈이 연기 잘 하잖아",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045436.1.5",
          "form": "그래 김의성 이분은 카르스마가",
          "original_form": "그래 김의성 이분은 카르스마가",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045436.1.6",
          "form": "삼신들이 정말 좋음",
          "original_form": "삼신들이 정말 좋음",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045436.1.7",
          "form": "시즌2도 나올려나",
          "original_form": "시즌2도 나올려나",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045436.1.8",
          "form": "나올듯한데",
          "original_form": "나올듯한데",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045436.1.9",
          "form": "기사로 시즌2 확정 되었네",
          "original_form": "기사로 시즌2 확정 되었네",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045436.1.10",
          "form": "내생각에 림여사가 왕따오지에게 영혼이 나간듯",
          "original_form": "내생각에 림여사가 왕따오지에게 영혼이 나간듯",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045436.1.11",
          "form": "그래 두고 보면 알겠지",
          "original_form": "그래 두고 보면 알겠지",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045436.1.12",
          "form": "이제훈 짜장면 먹방",
          "original_form": "이제훈 짜장면 먹방",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045436.1.13",
          "form": "이제훈은 너무 말랐더라",
          "original_form": "이제훈은 너무 말랐더라",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045436.1.14",
          "form": "조금 외소한편이지",
          "original_form": "조금 외소한편이지",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045436.1.15",
          "form": "그래 근육을 키워야할듯",
          "original_form": "그래 근육을 키워야할듯",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
