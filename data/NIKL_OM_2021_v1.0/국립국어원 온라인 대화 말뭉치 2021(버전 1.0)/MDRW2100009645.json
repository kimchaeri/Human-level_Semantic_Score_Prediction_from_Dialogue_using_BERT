      "utterance": [
        {
          "id": "MDRW2100009645.1.1",
          "form": "스포츠에 관심 있으세요?",
          "original_form": "스포츠에 관심 있으세요?",
          "speaker_id": "2",
          "time": "20210823 02:15"
        },
        {
          "id": "MDRW2100009645.1.2",
          "form": "네 전 배구랑 야구에 관심있어요!",
          "original_form": "네 전 배구랑 야구에 관심있어요!",
          "speaker_id": "1",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009645.1.3",
          "form": "혹시 관심있으신 종목 있으셔요?",
          "original_form": "혹시 관심있으신 종목 있으셔요?",
          "speaker_id": "1",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009645.1.4",
          "form": "저는 배구에 관심이 생겼어요!",
          "original_form": "저는 배구에 관심이 생겼어요!",
          "speaker_id": "2",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009645.1.5",
          "form": "고등학교 때 하이큐라는 배구만화가 유행했었거든요",
          "original_form": "고등학교 때 하이큐라는 배구만화가 유행했었거든요",
          "speaker_id": "2",
          "time": "20210823 02:17"
        },
        {
          "id": "MDRW2100009645.1.6",
          "form": "진짜 재밌게 봤었어요",
          "original_form": "진짜 재밌게 봤었어요",
          "speaker_id": "2",
          "time": "20210823 02:17"
        },
        {
          "id": "MDRW2100009645.1.7",
          "form": "와!! 하이큐 들어봤는데 다들 재밌다하더라고요",
          "original_form": "와!! 하이큐 들어봤는데 다들 재밌다하더라고요",
          "speaker_id": "1",
          "time": "20210823 02:17"
        },
        {
          "id": "MDRW2100009645.1.8",
          "form": "저도 나중에 봐야겠군요!",
          "original_form": "저도 나중에 봐야겠군요!",
          "speaker_id": "1",
          "time": "20210823 02:17"
        },
        {
          "id": "MDRW2100009645.1.9",
          "form": "ㅎㅎ꼭보세요",
          "original_form": "ㅎㅎ꼭보세요",
          "speaker_id": "2",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009645.1.10",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009645.1.11",
          "form": "배구경기 자주 보시나요?",
          "original_form": "배구경기 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009645.1.12",
          "form": "야구는 음 name1님은 응원하는 팀이 있으세요??",
          "original_form": "야구는 음 &name1&님은 응원하는 팀이 있으세요??",
          "speaker_id": "2",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009645.1.13",
          "form": "배구요..!ㅠ아뇨 이번 올림픽 때 처음으로 제대로 챙겨봤어요",
          "original_form": "배구요..!ㅠ아뇨 이번 올림픽 때 처음으로 제대로 챙겨봤어요",
          "speaker_id": "2",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009645.1.14",
          "form": "아하!! 이번 올림픽 여배 진짜 멋있었죠ㅜㅜ!!",
          "original_form": "아하!! 이번 올림픽 여배 진짜 멋있었죠ㅜㅜ!!",
          "speaker_id": "1",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009645.1.15",
          "form": "진짜 김연경 선수한테 완전 반해버렸어요",
          "original_form": "진짜 김연경 선수한테 완전 반해버렸어요",
          "speaker_id": "2",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009645.1.16",
          "form": "맞아요 진짜 멋있으시더라고요",
          "original_form": "맞아요 진짜 멋있으시더라고요",
          "speaker_id": "1",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009645.1.17",
          "form": "리더십도 있으시고",
          "original_form": "리더십도 있으시고",
          "speaker_id": "1",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009645.1.18",
          "form": "ㅎㅎㅎㅎ야구 좋아하세요??",
          "original_form": "ㅎㅎㅎㅎ야구 좋아하세요??",
          "speaker_id": "2",
          "time": "20210823 02:20"
        },
        {
          "id": "MDRW2100009645.1.19",
          "form": "네!! 야구좋아해요!!",
          "original_form": "네!! 야구좋아해요!!",
          "speaker_id": "1",
          "time": "20210823 02:21"
        },
        {
          "id": "MDRW2100009645.1.20",
          "form": "키움 응원해요ㅎㅎ",
          "original_form": "키움 응원해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:21"
        },
        {
          "id": "MDRW2100009645.1.21",
          "form": "어 왜 처음들어보는거같지?? 생긴 지 얼마 안 된 팀인가요?",
          "original_form": "어 왜 처음들어보는거같지?? 생긴 지 얼마 안 된 팀인가요?",
          "speaker_id": "2",
          "time": "20210823 02:21"
        },
        {
          "id": "MDRW2100009645.1.22",
          "form": "아ㅎㅎ네! 아주 예전부터 있었던 팀은 아니에요",
          "original_form": "아ㅎㅎ네! 아주 예전부터 있었던 팀은 아니에요",
          "speaker_id": "1",
          "time": "20210823 02:22"
        },
        {
          "id": "MDRW2100009645.1.23",
          "form": "넥센히어로즈였는데",
          "original_form": "넥센히어로즈였는데",
          "speaker_id": "1",
          "time": "20210823 02:22"
        },
        {
          "id": "MDRW2100009645.1.24",
          "form": "키움히어로즈로 바뀌었어요~!",
          "original_form": "키움히어로즈로 바뀌었어요~!",
          "speaker_id": "1",
          "time": "20210823 02:22"
        },
        {
          "id": "MDRW2100009645.1.25",
          "form": "아 넥센!!",
          "original_form": "아 넥센!!",
          "speaker_id": "2",
          "time": "20210823 02:24"
        },
        {
          "id": "MDRW2100009645.1.26",
          "form": "오오 아시는군요!!",
          "original_form": "오오 아시는군요!!",
          "speaker_id": "1",
          "time": "20210823 02:24"
        },
        {
          "id": "MDRW2100009645.1.27",
          "form": "네 근데 들어만 봤구 선구들은 잘 몰라요..",
          "original_form": "네 근데 들어만 봤구 선구들은 잘 몰라요..",
          "speaker_id": "2",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009645.1.28",
          "form": "그럼 이번에 올림픽 야구도 보셨겠네요??",
          "original_form": "그럼 이번에 올림픽 야구도 보셨겠네요??",
          "speaker_id": "2",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009645.1.29",
          "form": "그럴 수 있죠!ㅎㅎ",
          "original_form": "그럴 수 있죠!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009645.1.30",
          "form": "아 봤어요!!",
          "original_form": "아 봤어요!!",
          "speaker_id": "1",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009645.1.31",
          "form": "과몰입하면 안되는데 지니까 제가 다 아깝더라고요",
          "original_form": "과몰입하면 안되는데 지니까 제가 다 아깝더라고요",
          "speaker_id": "1",
          "time": "20210823 02:26"
        }
      ]
