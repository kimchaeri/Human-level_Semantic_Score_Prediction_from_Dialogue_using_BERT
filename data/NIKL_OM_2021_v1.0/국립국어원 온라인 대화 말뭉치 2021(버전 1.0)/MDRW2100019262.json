        {
          "id": "MDRW2100019262.1.2",
          "form": "난 향수",
          "original_form": "난 향수",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019262.1.3",
          "form": "겨울옷?",
          "original_form": "겨울옷?",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019262.1.4",
          "form": "무슨향수 사게?",
          "original_form": "무슨향수 사게?",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019262.1.5",
          "form": "글고보니 겨울옷도없네..예쁜거..",
          "original_form": "글고보니 겨울옷도없네..예쁜거..",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019262.1.6",
          "form": "향수가 1도 없어 니가 그때 준거 그거",
          "original_form": "향수가 1도 없어 니가 그때 준거 그거",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019262.1.7",
          "form": "말고는",
          "original_form": "말고는",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019262.1.8",
          "form": "내가 준게있엇나..",
          "original_form": "내가 준게있엇나..",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019262.1.9",
          "form": "향신료 향수같은거",
          "original_form": "향신료 향수같은거",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019262.1.10",
          "form": "향신료.??",
          "original_form": "향신료.??",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019262.1.11",
          "form": "인도향기날꺼같네..",
          "original_form": "인도향기날꺼같네..",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019262.1.12",
          "form": "누가 사왓다면서 향수 줫자나",
          "original_form": "누가 사왓다면서 향수 줫자나",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
