      "utterance": [
        {
          "id": "MDRW2100012230.1.1",
          "form": "오 안그래도 저 카페가는중이에요",
          "original_form": "오 안그래도 저 카페가는중이에요",
          "speaker_id": "2",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100012230.1.2",
          "form": "가서 어떤 메뉴 드실 예정이신가요?",
          "original_form": "가서 어떤 메뉴 드실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012230.1.3",
          "form": "수박스무디를 시켰습니다 히히",
          "original_form": "수박스무디를 시켰습니다 히히",
          "speaker_id": "2",
          "time": "20210824 20:47"
        },
        {
          "id": "MDRW2100012230.1.4",
          "form": "진짜 맛있더라구요",
          "original_form": "진짜 맛있더라구요",
          "speaker_id": "2",
          "time": "20210824 20:47"
        },
        {
          "id": "MDRW2100012230.1.5",
          "form": "달고 시원했을 것 같아요",
          "original_form": "달고 시원했을 것 같아요",
          "speaker_id": "1",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100012230.1.6",
          "form": "이번 여름에 수박 많이 드셨나요!",
          "original_form": "이번 여름에 수박 많이 드셨나요!",
          "speaker_id": "1",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100012230.1.7",
          "form": "그럼요! 근데 이번 수박은 뭔가 작았던 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "그럼요! 근데 이번 수박은 뭔가 작았던 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012230.1.8",
          "form": "그랬던 것 같기도하네요!!",
          "original_form": "그랬던 것 같기도하네요!!",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012230.1.9",
          "form": "요즘 수박 종류가 다양해졌더라구요 신기해요",
          "original_form": "요즘 수박 종류가 다양해졌더라구요 신기해요",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012230.1.10",
          "form": "맞아요 뭔가 다양하게 나온 것 같더라구요!",
          "original_form": "맞아요 뭔가 다양하게 나온 것 같더라구요!",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012230.1.11",
          "form": "저는 도전해보지못했지만...ㅎ",
          "original_form": "저는 도전해보지못했지만...ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012230.1.12",
          "form": "저도요ㅋㅋ.. 항상 먹던 일반 수박만 먹었네요ㅎㅎ",
          "original_form": "저도요ㅋㅋ.. 항상 먹던 일반 수박만 먹었네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012230.1.13",
          "form": "뭔가 이번 여름엔 제철과일을 많이 못먹은 기분이라 쬐금 아쉬워요 ㅎㅎㅎㅎ",
          "original_form": "뭔가 이번 여름엔 제철과일을 많이 못먹은 기분이라 쬐금 아쉬워요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:39"
        },
        {
          "id": "MDRW2100012230.1.14",
          "form": "그쵸 ㅠㅠ 제철과일은 그때 딱 먹어야 맛있잖아요..",
          "original_form": "그쵸 ㅠㅠ 제철과일은 그때 딱 먹어야 맛있잖아요..",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100012230.1.15",
          "form": "복숭아는 좀 드셨나요!",
          "original_form": "복숭아는 좀 드셨나요!",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100012230.1.16",
          "form": "후후 복숭아는 열심히 먹었습니다",
          "original_form": "후후 복숭아는 열심히 먹었습니다",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100012230.1.17",
          "form": "물복 딱복!",
          "original_form": "물복 딱복!",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100012230.1.18",
          "form": "저는 완전 물복파입니다 .. 근데 요즘 딱복의 맛도 조금 알아버렸어요..",
          "original_form": "저는 완전 물복파입니다 .. 근데 요즘 딱복의 맛도 조금 알아버렸어요..",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100012230.1.19",
          "form": "크으 저도 물복파입니다",
          "original_form": "크으 저도 물복파입니다",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100012230.1.20",
          "form": "말랑말랑 물복이 최고였어요ㅠ",
          "original_form": "말랑말랑 물복이 최고였어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100012230.1.21",
          "form": "딱복의 맛은 어떤가유",
          "original_form": "딱복의 맛은 어떤가유",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100012230.1.22",
          "form": "약간 딱딱한데 씹으면 달달하구 맛나요.. 잘못고르면 아무맛안나지만요 ...ㅎㅎ",
          "original_form": "약간 딱딱한데 씹으면 달달하구 맛나요.. 잘못고르면 아무맛안나지만요 ...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:59"
        }
      ]
    }
