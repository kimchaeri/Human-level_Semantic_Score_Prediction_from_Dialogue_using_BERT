      "utterance": [
        {
          "id": "MDRW2100012622.1.1",
          "form": "요새 보톡스를 생각중입니다 ㅋㅋ",
          "original_form": "요새 보톡스를 생각중입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012622.1.2",
          "form": "미용을 위한 선택이겠죠",
          "original_form": "미용을 위한 선택이겠죠",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012622.1.3",
          "form": "네 미간 주름때문에요",
          "original_form": "네 미간 주름때문에요",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012622.1.4",
          "form": "갑자기 훅  파였어요",
          "original_form": "갑자기 훅  파였어요",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012622.1.5",
          "form": "그리고 검진하는건 치아요",
          "original_form": "그리고 검진하는건 치아요",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012622.1.6",
          "form": "잇몸이 너무 안좋아서요",
          "original_form": "잇몸이 너무 안좋아서요",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012622.1.7",
          "form": "치과 가는건 좀 무섭죠",
          "original_form": "치과 가는건 좀 무섭죠",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100012622.1.8",
          "form": "네 항상 무서워요",
          "original_form": "네 항상 무서워요",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100012622.1.9",
          "form": "얼마전 정기 검진가서",
          "original_form": "얼마전 정기 검진가서",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100012622.1.10",
          "form": "너무 아파서 ㅜㅜ",
          "original_form": "너무 아파서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100012622.1.11",
          "form": "눈물 찔끔 났어요",
          "original_form": "눈물 찔끔 났어요",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100012622.1.12",
          "form": "치아는 선강하세요?",
          "original_form": "치아는 선강하세요?",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100012622.1.13",
          "form": "건강",
          "original_form": "건강",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100012622.1.14",
          "form": "네 현재 까지는 충치없이 잘유지하고 있어요",
          "original_form": "네 현재 까지는 충치없이 잘유지하고 있어요",
          "speaker_id": "1",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100012622.1.15",
          "form": "전 충치는 없는데",
          "original_form": "전 충치는 없는데",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100012622.1.16",
          "form": "원래 잇몸이 약해요",
          "original_form": "원래 잇몸이 약해요",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100012622.1.17",
          "form": "그래서 6개월마다 검진하다 3개월로 바꼈어요",
          "original_form": "그래서 6개월마다 검진하다 3개월로 바꼈어요",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100012622.1.18",
          "form": "잇몸이 아프면 신경 엄청 쓰이는데 ....",
          "original_form": "잇몸이 아프면 신경 엄청 쓰이는데 ....",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100012622.1.19",
          "form": "네 자꾸 붓고 피나서요",
          "original_form": "네 자꾸 붓고 피나서요",
          "speaker_id": "2",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100012622.1.20",
          "form": "잘닦지도 못해요 겁나서",
          "original_form": "잘닦지도 못해요 겁나서",
          "speaker_id": "2",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100012622.1.21",
          "form": "건강 신경쓰는건 어떤건가요?",
          "original_form": "건강 신경쓰는건 어떤건가요?",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100012622.1.22",
          "form": "잇몸에 인사돌 먹는건 어떨까요 ??",
          "original_form": "잇몸에 인사돌 먹는건 어떨까요 ??",
          "speaker_id": "1",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100012622.1.23",
          "form": "아 그거 효과없데요 ㅜㅜ",
          "original_form": "아 그거 효과없데요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100012622.1.24",
          "form": "나중에 그거 드시지 마세요",
          "original_form": "나중에 그거 드시지 마세요",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012622.1.25",
          "form": "영양제 뭐드세요?",
          "original_form": "영양제 뭐드세요?",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012622.1.26",
          "form": "전에 디스크 초기로 시술후 운동을 열심히 \n한답니다",
          "original_form": "전에 디스크 초기로 시술후 운동을 열심히 \n한답니다",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012622.1.27",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012622.1.28",
          "form": "디스크는 운동이 최고의 시술이래요",
          "original_form": "디스크는 운동이 최고의 시술이래요",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012622.1.29",
          "form": "정말 부지런 하시네요",
          "original_form": "정말 부지런 하시네요",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012622.1.30",
          "form": "꼭 운동 잘해서 건강 유지하세요",
          "original_form": "꼭 운동 잘해서 건강 유지하세요",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012622.1.31",
          "form": "칼슘제 눈영양제 종핲비타민",
          "original_form": "칼슘제 눈영양제 종핲비타민",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012622.1.32",
          "form": "저희남편도 그래서 매일걷고 그래요",
          "original_form": "저희남편도 그래서 매일걷고 그래요",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012622.1.33",
          "form": "전 눈 비타민 오메가 유산군요",
          "original_form": "전 눈 비타민 오메가 유산군요",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012622.1.34",
          "form": "다들 비슷한거 같아요",
          "original_form": "다들 비슷한거 같아요",
          "speaker_id": "2",
          "time": "20210824 23:29"
        }
      ]
    }
