      "utterance": [
        {
          "id": "MDRW2100035639.1.1",
          "form": "대만이 그렇게 먹방하기 좋다던데요",
          "original_form": "대만이 그렇게 먹방하기 좋다던데요",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035639.1.2",
          "form": "딱 한국인들 도는 먹코스가 있던데요ㅋㅋ",
          "original_form": "딱 한국인들 도는 먹코스가 있던데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035639.1.3",
          "form": "아 진짜요?ㅋㅋㅋㅋ",
          "original_form": "아 진짜요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035639.1.4",
          "form": "대만은 한국인들이 자주가는",
          "original_form": "대만은 한국인들이 자주가는",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035639.1.5",
          "form": "관광지 코스가 있긴한것같아요",
          "original_form": "관광지 코스가 있긴한것같아요",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035639.1.6",
          "form": "네 가면 이거 먹어줘야하고",
          "original_form": "네 가면 이거 먹어줘야하고",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035639.1.7",
          "form": "줄여서 단어로 만들던데",
          "original_form": "줄여서 단어로 만들던데",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035639.1.8",
          "form": "그담에 이거 먹어줘야하고~",
          "original_form": "그담에 이거 먹어줘야하고~",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035639.1.9",
          "form": "무슨 폭포가고 센과치히로에 나온 장소가고",
          "original_form": "무슨 폭포가고 센과치히로에 나온 장소가고",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035639.1.10",
          "form": "네넼ㅋㅋㅋ",
          "original_form": "네넼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035639.1.11",
          "form": "ㅋㅋㅋ 맞아요 센과치히로나온 그 시장!",
          "original_form": "ㅋㅋㅋ 맞아요 센과치히로나온 그 시장!",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035639.1.12",
          "form": "여행 가실때 패키지로가세용 자유로 가세용??",
          "original_form": "여행 가실때 패키지로가세용 자유로 가세용??",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035639.1.13",
          "form": "전 자유요!",
          "original_form": "전 자유요!",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035639.1.14",
          "form": "전 반반인데 둘다 장단점이 있더라구요",
          "original_form": "전 반반인데 둘다 장단점이 있더라구요",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035639.1.15",
          "form": "느릿느릿하게 몇개만 충분히 보고오는걸 좋아합니다ㅋㅋ",
          "original_form": "느릿느릿하게 몇개만 충분히 보고오는걸 좋아합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035639.1.16",
          "form": "아하 넘 좋죠",
          "original_form": "아하 넘 좋죠",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035639.1.17",
          "form": "패키지는 빡빡해요ㅋㅋㅋ",
          "original_form": "패키지는 빡빡해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035639.1.18",
          "form": "그래도 딱 중요한거 다 볼수있으니",
          "original_form": "그래도 딱 중요한거 다 볼수있으니",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035639.1.19",
          "form": "그것도 장점같아요ㅋㅋ",
          "original_form": "그것도 장점같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035639.1.20",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035639.1.21",
          "form": "패키지로",
          "original_form": "패키지로",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100035639.1.22",
          "form": "여행갔었는대",
          "original_form": "여행갔었는대",
