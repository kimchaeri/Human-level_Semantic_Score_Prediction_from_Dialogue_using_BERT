      "utterance": [
        {
          "id": "MDRW2100011584.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011584.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011584.1.3",
          "form": "간단하게 챙겨먹었어요 ㅎㅎ",
          "original_form": "간단하게 챙겨먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011584.1.4",
          "form": "저도 간단하게 먹었어요ㅎㅎ",
          "original_form": "저도 간단하게 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011584.1.5",
          "form": "맛집가는거 좋아하시나요?",
          "original_form": "맛집가는거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011584.1.6",
          "form": "아 좋아하죠ㅠㅠ",
          "original_form": "아 좋아하죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011584.1.7",
          "form": "맛있는건 알아도알아도 부족해요",
          "original_form": "맛있는건 알아도알아도 부족해요",
          "speaker_id": "1",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011584.1.8",
          "form": "그쵸 ㅠㅠ 외식할때 주로 어떤 메뉴 드시나요?",
          "original_form": "그쵸 ㅠㅠ 외식할때 주로 어떤 메뉴 드시나요?",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011584.1.9",
          "form": "한때 마라탕집 탐방다녔어요 ㅋㅋㅋㅋ",
          "original_form": "한때 마라탕집 탐방다녔어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100011584.1.10",
          "form": "오 저도 마라탕 좋아해요",
          "original_form": "오 저도 마라탕 좋아해요",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100011584.1.11",
          "form": "마라탕 드실때 어떤 재료 넣으시나요?",
          "original_form": "마라탕 드실때 어떤 재료 넣으시나요?",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100011584.1.12",
          "form": "저는 목이버섯 엄청 좋아해요 ㄹㅎ",
          "original_form": "저는 목이버섯 엄청 좋아해요 ㄹㅎ",
          "speaker_id": "1",
          "time": "20210824 20:40"
        },
        {
          "id": "MDRW2100011584.1.13",
          "form": "그리고 두부피도 사랑해요 ㅎㅎㅎ",
          "original_form": "그리고 두부피도 사랑해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100011584.1.14",
          "form": "맞아요 둘다 맛있죠ㅎㅎ",
          "original_form": "맞아요 둘다 맛있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100011584.1.15",
          "form": "전 옥수수당면이랑 팽이버섯 꼭 넣어서 먹어요",
          "original_form": "전 옥수수당면이랑 팽이버섯 꼭 넣어서 먹어요",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100011584.1.16",
          "form": "아 둘다 빠지면 너무 아쉽죠 ㅠㅠ",
          "original_form": "아 둘다 빠지면 너무 아쉽죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100011584.1.17",
          "form": "버섯 중에 팽이버섯이랑 목이버섯 전 투탑이라 생각해요",
          "original_form": "버섯 중에 팽이버섯이랑 목이버섯 전 투탑이라 생각해요",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100011584.1.18",
          "form": "면은 옥수수면이 진짜 안물리는것같아요",
          "original_form": "면은 옥수수면이 진짜 안물리는것같아요",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100011584.1.19",
          "form": "그쵸그쵸 그 버섯 2개는 꼭 넣어여해요",
          "original_form": "그쵸그쵸 그 버섯 2개는 꼭 넣어여해요",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100011584.1.20",
          "form": "옥수수면이 얇아서 계속 들어가더라구요!",
          "original_form": "옥수수면이 얇아서 계속 들어가더라구요!",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100011584.1.21",
          "form": "흑흑 말하다보니 너무 땡겨요 ㅠㅠㅠ",
          "original_form": "흑흑 말하다보니 너무 땡겨요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100011584.1.22",
          "form": "마라샹궈는 좋아하세요?",
          "original_form": "마라샹궈는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100011584.1.23",
          "form": "네네 좋아해요!! 근데 샹궈는 저만 좋아해서 가끔밖에 못먹어요ㅠㅠ",
          "original_form": "네네 좋아해요!! 근데 샹궈는 저만 좋아해서 가끔밖에 못먹어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100011584.1.24",
          "form": "퐁퐁님도 샹궈좋아하시나요!",
          "original_form": "퐁퐁님도 샹궈좋아하시나요!",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100011584.1.25",
          "form": "저 진짜 사랑해요...",
          "original_form": "저 진짜 사랑해요...",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100011584.1.26",
          "form": "숙주 가득 넣어서...",
          "original_form": "숙주 가득 넣어서...",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100011584.1.27",
          "form": "후루룹 ㅠㅠ",
          "original_form": "후루룹 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100011584.1.28",
          "form": "근데 진ㅁ자 먹기 어려운 음식이죠..",
          "original_form": "근데 진ㅁ자 먹기 어려운 음식이죠..",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100011584.1.29",
          "form": "진짜루요 .. 샹궈는 무조건 숙주많이 넣어야돼요",
          "original_form": "진짜루요 .. 샹궈는 무조건 숙주많이 넣어야돼요",
          "speaker_id": "2",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100011584.1.30",
          "form": "꿔바로우랑 같이 먹으면 그냥 짱이에요!",
          "original_form": "꿔바로우랑 같이 먹으면 그냥 짱이에요!",
          "speaker_id": "2",
          "time": "20210824 23:12"
        }
      ]
