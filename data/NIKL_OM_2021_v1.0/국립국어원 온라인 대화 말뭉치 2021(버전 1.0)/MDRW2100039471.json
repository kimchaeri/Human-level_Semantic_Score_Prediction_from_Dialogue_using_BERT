      "utterance": [
        {
          "id": "MDRW2100039471.1.1",
          "form": "요즘 비대면이죠?",
          "original_form": "요즘 비대면이죠?",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039471.1.2",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039471.1.3",
          "form": "영상녹화 틀어두고",
          "original_form": "영상녹화 틀어두고",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039471.1.4",
          "form": "누워서 웹소설을 읽으니 정말",
          "original_form": "누워서 웹소설을 읽으니 정말",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039471.1.5",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039471.1.6",
          "form": "ㅋ ㅋㅋ ㅋ ㅋ ㅋ ㅋ",
          "original_form": "ㅋ ㅋㅋ ㅋ ㅋ ㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039471.1.7",
          "form": "수강신청은 잘 하셨어요?",
          "original_form": "수강신청은 잘 하셨어요?",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039471.1.8",
          "form": "아 저 아주끝내주게",
          "original_form": "아 저 아주끝내주게",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039471.1.9",
          "form": "올클해줬죠",
          "original_form": "올클해줬죠",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039471.1.10",
          "form": "ㅋㅋ전..망햇는데",
          "original_form": "ㅋㅋ전..망햇는데",
          "speaker_id": "1",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039471.1.11",
          "form": "행복하세요.. .?",
          "original_form": "행복하세요.. .?",
          "speaker_id": "1",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039471.1.12",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039471.1.13",
          "form": "다음학기엔..잘..하실수잇을거에요",
          "original_form": "다음학기엔..잘..하실수잇을거에요",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039471.1.14",
          "form": "제가",
          "original_form": "제가",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039471.1.15",
          "form": "도와드림.아ㅋㅋ보여드림ㅋㅋ",
          "original_form": "도와드림.아ㅋㅋ보여드림ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039471.1.16",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039471.1.17",
          "form": "올클신화 보여드립니다ㅋㅋ",
          "original_form": "올클신화 보여드립니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039471.1.18",
          "form": "아 ㅋㅋ 가보자고요",
          "original_form": "아 ㅋㅋ 가보자고요",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039471.1.19",
          "form": "그러면",
          "original_form": "그러면",
          "speaker_id": "1",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039471.1.20",
          "form": "줌으로 수업하세요?",
          "original_form": "줌으로 수업하세요?",
          "speaker_id": "1",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039471.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039471.1.22",
          "form": "교수님들 고소할라고요",
          "original_form": "교수님들 고소할라고요",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039471.1.23",
          "form": "아하~ 고소 좋죠",
          "original_form": "아하~ 고소 좋죠",
          "speaker_id": "1",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039471.1.24",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039471.1.25",
          "form": "저도 고소하려고 이것저것 찾아보는중이에요!",
          "original_form": "저도 고소하려고 이것저것 찾아보는중이에요!",
          "speaker_id": "1",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039471.1.26",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039471.1.27",
          "form": "고소 좀 해보셨나봐요",
          "original_form": "고소 좀 해보셨나봐요",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039471.1.28",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039471.1.29",
