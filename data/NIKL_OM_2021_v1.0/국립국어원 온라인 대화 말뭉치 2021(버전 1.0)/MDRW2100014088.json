      "utterance": [
        {
          "id": "MDRW2100014088.1.1",
          "form": "어디가보셨어요?",
          "original_form": "어디가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014088.1.2",
          "form": "미국, 일본, 동남아시아 나라들 정도 갔는데",
          "original_form": "미국, 일본, 동남아시아 나라들 정도 갔는데",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014088.1.3",
          "form": "아직 유럽을 많이 못가봤어요ㅋㅋ",
          "original_form": "아직 유럽을 많이 못가봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014088.1.4",
          "form": "와 많이다니셨네요 대단하세요",
          "original_form": "와 많이다니셨네요 대단하세요",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014088.1.5",
          "form": "네 회사 출장 끼다보니 많이 나갔네요ㅋㅋ",
          "original_form": "네 회사 출장 끼다보니 많이 나갔네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014088.1.6",
          "form": "name2은 어디가보셨어요?",
          "original_form": "&name2&은 어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014088.1.7",
          "form": "우와 멋지네요 해외출장이라니",
          "original_form": "우와 멋지네요 해외출장이라니",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014088.1.8",
          "form": "전 일본이랑 코타키나발루요",
          "original_form": "전 일본이랑 코타키나발루요",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014088.1.9",
          "form": "홍콩이랑요",
          "original_form": "홍콩이랑요",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014088.1.10",
          "form": "오 코타키나발루 가보고 싶었는데ㅜ",
          "original_form": "오 코타키나발루 가보고 싶었는데ㅜ",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014088.1.11",
          "form": "해변이 예쁘다면서요ㅎㅎ",
          "original_form": "해변이 예쁘다면서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014088.1.12",
          "form": "원숭이 반딧불이 보러갔다가 모기뜯기 기억밖에 없어요",
          "original_form": "원숭이 반딧불이 보러갔다가 모기뜯기 기억밖에 없어요",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014088.1.13",
          "form": "비가많이오더라고요",
          "original_form": "비가많이오더라고요",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014088.1.14",
          "form": "아 그래요? 여행장소로 추천하시나요?ㅋㅋ",
          "original_form": "아 그래요? 여행장소로 추천하시나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014088.1.15",
          "form": "음 비추천이네요ㅋㅋㅋ",
          "original_form": "음 비추천이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014088.1.16",
          "form": "전 휴양지 가보고 싶어요",
          "original_form": "전 휴양지 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014088.1.17",
          "form": "전 다낭 갔었는데 휴양지로 괜찮은 거 같아요ㅎㅎ",
          "original_form": "전 다낭 갔었는데 휴양지로 괜찮은 거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014088.1.18",
          "form": "거기 좋다더라고요\r\n흑 가고싶네요",
          "original_form": "거기 좋다더라고요\r\n흑 가고싶네요",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014088.1.19",
          "form": "네 진짜 해변에 쉬거나 마사지 엄청 받았어요ㅋㅋ",
          "original_form": "네 진짜 해변에 쉬거나 마사지 엄청 받았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014088.1.20",
          "form": "와 해변에서 석양보면서 그건가요?!!!",
          "original_form": "와 해변에서 석양보면서 그건가요?!!!",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014088.1.21",
          "form": "네 맞아요ㅎㅎ",
          "original_form": "네 맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014088.1.22",
          "form": "사람 없는데들 많아서",
          "original_form": "사람 없는데들 많아서",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014088.1.23",
          "form": "휴양하기엔 진짜 좋아요ㅋㅋ",
          "original_form": "휴양하기엔 진짜 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014088.1.24",
          "form": "아 너무너무 가고싶네요",
          "original_form": "아 너무너무 가고싶네요",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014088.1.25",
          "form": "ㅎㅎ백신 맞으셨으면 갈수는 있다고 하네요ㅎㅎ",
          "original_form": "ㅎㅎ백신 맞으셨으면 갈수는 있다고 하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:42"
        }
      ]
