      "utterance": [
        {
          "id": "MDRW2100035003.1.1",
          "form": "전 요즘 포도랑 무화과를 먹었어요",
          "original_form": "전 요즘 포도랑 무화과를 먹었어요",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100035003.1.2",
          "form": "요즘 어떧ㄴ 과일을 드셨나요?",
          "original_form": "요즘 어떧ㄴ 과일을 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100035003.1.3",
          "form": "오 저도 거봉이랑",
          "original_form": "오 저도 거봉이랑",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100035003.1.4",
          "form": "메론 먹었어요!",
          "original_form": "메론 먹었어요!",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100035003.1.5",
          "form": "무화과 와 맛있겠다",
          "original_form": "무화과 와 맛있겠다",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100035003.1.6",
          "form": "우와 거봉도 맛있겠어요!",
          "original_form": "우와 거봉도 맛있겠어요!",
          "speaker_id": "1",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100035003.1.7",
          "form": "메론도요",
          "original_form": "메론도요",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100035003.1.8",
          "form": "요즘",
          "original_form": "요즘",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100035003.1.9",
          "form": "밀키드 잘 나오던데",
          "original_form": "밀키드 잘 나오던데",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100035003.1.10",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100035003.1.11",
          "form": "밀키트로 나온건 안사봤구 마트서 파는 냉장 떡볶이 이런건 가끔 해먹어요",
          "original_form": "밀키트로 나온건 안사봤구 마트서 파는 냉장 떡볶이 이런건 가끔 해먹어요",
          "speaker_id": "1",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100035003.1.12",
          "form": "오호 냉장떡볶이 풀무원꺼 맛있더라구요",
          "original_form": "오호 냉장떡볶이 풀무원꺼 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100035003.1.13",
          "form": "밀키트 감바스 드셔보세요!",
          "original_form": "밀키트 감바스 드셔보세요!",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100035003.1.14",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100035003.1.15",
          "form": "아하 전 집에 올리브오일이 좀 많아서 그냥 새우사서 기름에 삶음 될것 같아요",
          "original_form": "아하 전 집에 올리브오일이 좀 많아서 그냥 새우사서 기름에 삶음 될것 같아요",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100035003.1.16",
          "form": "게으름병이 사라지는 날 시도해볼게요 ㅋㅋㅋ",
          "original_form": "게으름병이 사라지는 날 시도해볼게요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100035003.1.17",
          "form": "ㅋㅋㅋ 새우 큰걸로 사서 드세요!",
          "original_form": "ㅋㅋㅋ 새우 큰걸로 사서 드세요!",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100035003.1.18",
          "form": "넘 맛있답니다ㅜㅜ",
          "original_form": "넘 맛있답니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100035003.1.19",
          "form": "전 새우 넘 좋아해서",
          "original_form": "전 새우 넘 좋아해서",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100035003.1.20",
          "form": "파스타해먹을때도 꼭 새우넣어요",
          "original_form": "파스타해먹을때도 꼭 새우넣어요",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100035003.1.21",
          "form": "킹타이거 새우를 드셔보셨나요? 요즘유행이에요",
          "original_form": "킹타이거 새우를 드셔보셨나요? 요즘유행이에요",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035003.1.22",
          "form": "어머 이런게 있어요??",
          "original_form": "어머 이런게 있어요??",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035003.1.23",
          "form": "메모해둬야지...",
          "original_form": "메모해둬야지...",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035003.1.24",
          "form": "안먹어봤어요ㅜㅜ",
          "original_form": "안먹어봤어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035003.1.25",
          "form": "엄청 큰 새우인데오 너무 커서 마리당 만원? 이렇게도 팔더라구요",
          "original_form": "엄청 큰 새우인데오 너무 커서 마리당 만원? 이렇게도 팔더라구요",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035003.1.26",
          "form": "저도 안ㅁ거어 봐서 여쭤봤어요 ㅋㅋ",
          "original_form": "저도 안ㅁ거어 봐서 여쭤봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035003.1.27",
          "form": "헤엑 대박",
          "original_form": "헤엑 대박",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035003.1.28",
          "form": "저는 그냥 칵테일 새우나 일키로 사서 먹어야겠어요ㅠㅠㅠ",
          "original_form": "저는 그냥 칵테일 새우나 일키로 사서 먹어야겠어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100035003.1.29",
          "form": "저도 그런걸 더 좋아하는데 ㅋㅋ요즘 또 유행이라서",
          "original_form": "저도 그런걸 더 좋아하는데 ㅋㅋ요즘 또 유행이라서",
          "speaker_id": "1",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100035003.1.30",
          "form": "궁금은 하네요",
          "original_form": "궁금은 하네요",
          "speaker_id": "1",
          "time": "20210828 23:51"
        }
      ]
    }
  ]
