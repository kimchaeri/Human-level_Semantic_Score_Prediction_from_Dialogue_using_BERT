      "utterance": [
        {
          "id": "MDRW2100041410.1.1",
          "form": "네 하리보인가 여튼 곰돌이 젤리",
          "original_form": "네 하리보인가 여튼 곰돌이 젤리",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041410.1.2",
          "form": "하 리본은 너무 즐겨요",
          "original_form": "하 리본은 너무 즐겨요",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041410.1.3",
          "form": "있는 걸로 들고 가는데 하리보 같은 것이 한 번 뜯으면 일곱 개 해서 여덟 개 정도 들어 있기 때문에 수업 할 때 편해요",
          "original_form": "있는 걸로 들고 가는데 하리보 같은 것이 한 번 뜯으면 일곱 개 해서 여덟 개 정도 들어 있기 때문에 수업 할 때 편해요",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041410.1.4",
          "form": "질겨요",
          "original_form": "질겨요",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041410.1.5",
          "form": "하리보 보다는 베베토가",
          "original_form": "하리보 보다는 베베토가",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041410.1.6",
          "form": "마이 구미도 맛이 있던데 마이 구미는 제 돈 주고 사기 좀 그래서요",
          "original_form": "마이 구미도 맛이 있던데 마이 구미는 제 돈 주고 사기 좀 그래서요",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041410.1.7",
          "form": "베베토가 훨씬 말랑하고 맛있는 거 같아요",
          "original_form": "베베토가 훨씬 말랑하고 맛있는 거 같아요",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041410.1.8",
          "form": "마이구미는 그냥 네이브로",
          "original_form": "마이구미는 그냥 네이브로",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041410.1.9",
          "form": "하리보 아니고 배베토인 거 같아요",
          "original_form": "하리보 아니고 배베토인 거 같아요",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041410.1.10",
          "form": "마이 잇",
          "original_form": "마이 잇",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041410.1.11",
          "form": "베베토 들고 다닙니다",
          "original_form": "베베토 들고 다닙니다",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041410.1.12",
          "form": "많이 주는 제가 좋아합니다 당 충전이 잘 돼요",
          "original_form": "많이 주는 제가 좋아합니다 당 충전이 잘 돼요",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041410.1.13",
          "form": "젤리 중에 포도 모양 젤리가 있는데 네 너무 맛있어서 자주 먹었었던 기억이나 해요",
          "original_form": "젤리 중에 포도 모양 젤리가 있는데 네 너무 맛있어서 자주 먹었었던 기억이나 해요",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041410.1.14",
          "form": "마이쮸",
          "original_form": "마이쮸",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041410.1.15",
          "form": "마이구미 아닌가요",
          "original_form": "마이구미 아닌가요",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041410.1.16",
          "form": "오사카 갔을 때 젤리 만 젤리 5만원 지 사온 거 같아요",
          "original_form": "오사카 갔을 때 젤리 만 젤리 5만원 지 사온 거 같아요",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041410.1.17",
          "form": "여튼 젤리는 굉장히 다양하고 맛이 있고이가 잘 썩지요",
          "original_form": "여튼 젤리는 굉장히 다양하고 맛이 있고이가 잘 썩지요",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041410.1.18",
          "form": "사가 맛보도 맛 요 로케",
          "original_form": "사가 맛보도 맛 요 로케",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041410.1.19",
          "form": "맞아요 사탕 젤리",
          "original_form": "맞아요 사탕 젤리",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041410.1.20",
          "form": "저도 먹어보고 싶네요 젤리 좋아합니다",
          "original_form": "저도 먹어보고 싶네요 젤리 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 23:08"
        }
