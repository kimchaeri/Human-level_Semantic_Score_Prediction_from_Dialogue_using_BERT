      "utterance": [
        {
          "id": "MDRW2100036019.1.1",
          "form": "요즘 즐겨보는  프로그램 잇으세요?",
          "original_form": "요즘 즐겨보는  프로그램 잇으세요?",
          "speaker_id": "2",
          "time": "20210830 14:19"
        },
        {
          "id": "MDRW2100036019.1.2",
          "form": "슬의생 보고 있어요ㅎㅎ",
          "original_form": "슬의생 보고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:22"
        },
        {
          "id": "MDRW2100036019.1.3",
          "form": "앗저는 다끝나면 정주행 하려고 안보고잇어여 ㅋ",
          "original_form": "앗저는 다끝나면 정주행 하려고 안보고잇어여 ㅋ",
          "speaker_id": "2",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100036019.1.4",
          "form": "시즌 3까지 갈 수도 있다는데 어떻게 기다리죠ㅠ",
          "original_form": "시즌 3까지 갈 수도 있다는데 어떻게 기다리죠ㅠ",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100036019.1.5",
          "form": "ㅎㅎ 기다리는것도 묘미~",
          "original_form": "ㅎㅎ 기다리는것도 묘미~",
          "speaker_id": "2",
          "time": "20210830 14:30"
        },
        {
          "id": "MDRW2100036019.1.6",
          "form": "한 1년 반 정도 기다리면 될까요ㅋㅋㅋ",
          "original_form": "한 1년 반 정도 기다리면 될까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:09"
        },
        {
          "id": "MDRW2100036019.1.7",
          "form": "ㅎㅎㅎㅎ 그사이에 또 띵작이 나올거에욮ㅋ",
          "original_form": "ㅎㅎㅎㅎ 그사이에 또 띵작이 나올거에욮ㅋ",
          "speaker_id": "2",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100036019.1.8",
          "form": "ㅋㅋㅋㅋ 펜하시즌4인가요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 펜하시즌4인가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100036019.1.9",
          "form": "아 전 펜하는 안봤어요  드라마는 즐겨보진 않거든요 ㅎㅎ",
          "original_form": "아 전 펜하는 안봤어요  드라마는 즐겨보진 않거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100036019.1.10",
          "form": "펜하...정말 부부의세계, 아내의 유혹, 스카이캐슬의 합작ㅠ",
          "original_form": "펜하...정말 부부의세계, 아내의 유혹, 스카이캐슬의 합작ㅠ",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100036019.1.11",
          "form": "ㅎㅎㅎ 전 다 안본거 같어요 ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ 전 다 안본거 같어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100036019.1.12",
          "form": "안 보신 게 좋은 것 같아요ㅎㅎ",
          "original_form": "안 보신 게 좋은 것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100036019.1.13",
          "form": "ㅎㅎ 넘 빠지는 군요 ㅎㅎ",
          "original_form": "ㅎㅎ 넘 빠지는 군요 ㅎㅎ",
