        {
          "id": "MDRW2100018845.1.1",
          "form": "아니에요 !!!!!!!!!",
          "original_form": "아니에요 !!!!!!!!!",
          "speaker_id": "1",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018845.1.2",
          "form": "거기 날씨 어때요??",
          "original_form": "거기 날씨 어때요??",
          "speaker_id": "1",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018845.1.3",
          "form": "오늘 날씨 어떤가요",
          "original_form": "오늘 날씨 어떤가요",
          "speaker_id": "2",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018845.1.4",
          "form": "앗 여긴 해떴어요",
          "original_form": "앗 여긴 해떴어요",
          "speaker_id": "2",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018845.1.5",
          "form": "근데 비오려나 아직 좀 습한게 있네요",
          "original_form": "근데 비오려나 아직 좀 습한게 있네요",
          "speaker_id": "2",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018845.1.6",
          "form": "ㅎ 여기는 아직 우중충한데",
          "original_form": "ㅎ 여기는 아직 우중충한데",
          "speaker_id": "1",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018845.1.7",
          "form": "바람이 엄청 시원해요!!!!!!",
          "original_form": "바람이 엄청 시원해요!!!!!!",
          "speaker_id": "1",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018845.1.8",
          "form": "아 습한 거 별로인데 ㅠㅠ",
          "original_form": "아 습한 거 별로인데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018845.1.9",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018845.1.10",
          "form": "아까 나갔더니 습해서 에어컨을 못 끄고 있거든요ㅎ",
          "original_form": "아까 나갔더니 습해서 에어컨을 못 끄고 있거든요ㅎ",
          "speaker_id": "2",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018845.1.11",
          "form": "창문만열어놔도 시원하겠어요",
          "original_form": "창문만열어놔도 시원하겠어요",
          "speaker_id": "2",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018845.1.12",
          "form": "네ㅋㅋㅋㅋㅋ!",
          "original_form": "네ㅋㅋㅋㅋㅋ!",
          "speaker_id": "1",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018845.1.13",
          "form": "추워거 닫아버렸어요 ㅎ",
          "original_form": "추워거 닫아버렸어요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018845.1.14",
          "form": "닫았다구오?ㅋㅋㅋㅋ",
          "original_form": "닫았다구오?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018845.1.15",
          "form": "바람이 이제 찬기운이 섞이나봐요",
          "original_form": "바람이 이제 찬기운이 섞이나봐요",
          "speaker_id": "2",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018845.1.16",
          "form": "ㅋㅋㅋㅋㅋ네.. 추워서요...",
          "original_form": "ㅋㅋㅋㅋㅋ네.. 추워서요...",
