      "utterance": [
        {
          "id": "MDRW2100005363.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005363.1.2",
          "form": "아뇨 그쳤어요",
          "original_form": "아뇨 그쳤어요",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005363.1.3",
          "form": "여기도요",
          "original_form": "여기도요",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005363.1.4",
          "form": "오늘은 그만 오려나봐요",
          "original_form": "오늘은 그만 오려나봐요",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005363.1.5",
          "form": "새벽에 오려나요",
          "original_form": "새벽에 오려나요",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005363.1.6",
          "form": "오락가락해요",
          "original_form": "오락가락해요",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005363.1.7",
          "form": "장마라 그런가봐요",
          "original_form": "장마라 그런가봐요",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005363.1.8",
          "form": "늦장마네요",
          "original_form": "늦장마네요",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005363.1.9",
          "form": "네 여름 내내 비도 얼마 안오더니 이제 오네요",
          "original_form": "네 여름 내내 비도 얼마 안오더니 이제 오네요",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005363.1.10",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005363.1.11",
          "form": "비가 요새 안와서 계곡이 다말랐던데",
          "original_form": "비가 요새 안와서 계곡이 다말랐던데",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005363.1.12",
          "form": "이제 비와서 채워졌겠어요",
          "original_form": "이제 비와서 채워졌겠어요",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005363.1.13",
          "form": "가뭄 심하다 뉴스에서 나오더니 해결됐을거 같아요",
          "original_form": "가뭄 심하다 뉴스에서 나오더니 해결됐을거 같아요",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005363.1.14",
          "form": "ㅎㅎ 적당히가 좋은데",
          "original_form": "ㅎㅎ 적당히가 좋은데",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005363.1.15",
          "form": "그렇죠 전 불편한데 또 안올수도 앖고",
          "original_form": "그렇죠 전 불편한데 또 안올수도 앖고",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005363.1.16",
          "form": "없고",
          "original_form": "없고",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005363.1.17",
          "form": "비가 싫긴해요",
          "original_form": "비가 싫긴해요",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005363.1.18",
          "form": "눈은 좋아 하세요?",
          "original_form": "눈은 좋아 하세요?",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005363.1.19",
          "form": "눈은 별로요",
          "original_form": "눈은 별로요",
          "speaker_id": "1",
          "time": "20210821 22:26"
        }
