      "utterance": [
        {
          "id": "MDRW2100020621.1.1",
          "form": "게임 어떤 장르 좋아하시나요?",
          "original_form": "게임 어떤 장르 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020621.1.2",
          "form": "혹시... 로스트아크라고 아시나요?",
          "original_form": "혹시... 로스트아크라고 아시나요?",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020621.1.3",
          "form": "mmorpg 좋아합니다",
          "original_form": "mmorpg 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020621.1.4",
          "form": "로스트아크는 처음 들어봐요",
          "original_form": "로스트아크는 처음 들어봐요",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020621.1.5",
          "form": "그렇군요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇군요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020621.1.6",
          "form": "지금 유행하는(?) rpg 컴퓨터게임이라고 생각하시면 됩니다!",
          "original_form": "지금 유행하는(?) rpg 컴퓨터게임이라고 생각하시면 됩니다!",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020621.1.7",
          "form": "혹시 좋아하는 게임장르 있으신가요?",
          "original_form": "혹시 좋아하는 게임장르 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020621.1.8",
          "form": "저는 시뮬레이션 좋아해요",
          "original_form": "저는 시뮬레이션 좋아해요",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020621.1.9",
          "form": "시뮬레이션 게임이 뭐가있나요??",
          "original_form": "시뮬레이션 게임이 뭐가있나요??",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020621.1.10",
          "form": "심즈요 최근에는 안 하고 있어요",
          "original_form": "심즈요 최근에는 안 하고 있어요",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020621.1.11",
          "form": "심즈가 시뮬레이션이였네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "심즈가 시뮬레이션이였네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020621.1.12",
          "form": "전에 할인할때 심즈4 샀었는데",
          "original_form": "전에 할인할때 심즈4 샀었는데",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020621.1.13",
          "form": "어떻게 하는지 몰라서 멍때리다가 껐던기억이 나네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어떻게 하는지 몰라서 멍때리다가 껐던기억이 나네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020621.1.14",
          "form": "아직도 어떻게 하는지 모르는데",
          "original_form": "아직도 어떻게 하는지 모르는데",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020621.1.15",
          "form": "유튜브 보다보면 이해가 갈까요?",
          "original_form": "유튜브 보다보면 이해가 갈까요?",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020621.1.16",
          "form": "심즈는 쉬운 편이니까 금방 잘 하실걸요",
          "original_form": "심즈는 쉬운 편이니까 금방 잘 하실걸요",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020621.1.17",
          "form": "다음에 도전해봐야겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "다음에 도전해봐야겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100020621.1.18",
          "form": "심즈 얘기하니까 급 심즈하고 싶어지네요 ㅎㅎ",
          "original_form": "심즈 얘기하니까 급 심즈하고 싶어지네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100020621.1.19",
          "form": "다음에 심즈 해봐야겠어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다음에 심즈 해봐야겠어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100020621.1.20",
          "form": "같이 샀던 마크는 머리비우고 하기 좋던데 해보셨어요??",
          "original_form": "같이 샀던 마크는 머리비우고 하기 좋던데 해보셨어요??",
          "speaker_id": "2",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100020621.1.21",
          "form": "마크가 마인크래프트인가요??",
          "original_form": "마크가 마인크래프트인가요??",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100020621.1.22",
          "form": "제가 게임을 잘 몰라서요",
          "original_form": "제가 게임을 잘 몰라서요",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100020621.1.23",
          "form": "네네 마인크래프트요",
          "original_form": "네네 마인크래프트요",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100020621.1.24",
          "form": "친구가 아무생각없이 돌만 캐면 된다고 해서",
          "original_form": "친구가 아무생각없이 돌만 캐면 된다고 해서",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100020621.1.25",
          "form": "해봤는데",
          "original_form": "해봤는데",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100020621.1.26",
          "form": "생각보다 재밌더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "생각보다 재밌더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100020621.1.27",
          "form": "나중에 기회가 되면 해 봐야겠어요",
