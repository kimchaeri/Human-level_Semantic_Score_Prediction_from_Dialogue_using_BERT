        },
        {
          "id": "MDRW2100040957.1.2",
          "form": "추어탕이영",
          "original_form": "추어탕이영",
          "speaker_id": "1",
          "time": "20210905 16:46"
        },
        {
          "id": "MDRW2100040957.1.3",
          "form": "꽈배기 먹었어요",
          "original_form": "꽈배기 먹었어요",
          "speaker_id": "1",
          "time": "20210905 16:46"
        },
        {
          "id": "MDRW2100040957.1.4",
          "form": "우왕 추어탕 맛있나요?",
          "original_form": "우왕 추어탕 맛있나요?",
          "speaker_id": "2",
          "time": "20210905 18:14"
        },
        {
          "id": "MDRW2100040957.1.5",
          "form": "한번도 안 먹어봤어요!",
          "original_form": "한번도 안 먹어봤어요!",
          "speaker_id": "2",
          "time": "20210905 18:14"
        },
        {
          "id": "MDRW2100040957.1.6",
          "form": "헉 정말여",
          "original_form": "헉 정말여",
          "speaker_id": "1",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100040957.1.7",
          "form": "저는조아해용",
          "original_form": "저는조아해용",
          "speaker_id": "1",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100040957.1.8",
          "form": "어떤 맛에 좋아하시나요?",
          "original_form": "어떤 맛에 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 18:38"
        },
        {
          "id": "MDRW2100040957.1.9",
          "form": "ㅋㅋㅋㅋ 아이게 안먹어보면 비교할게 없네요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 아이게 안먹어보면 비교할게 없네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 18:38"
        },
        {
          "id": "MDRW2100040957.1.10",
          "form": "식당마다 맛도좀씩 다다르고...",
          "original_form": "식당마다 맛도좀씩 다다르고...",
          "speaker_id": "1",
          "time": "20210905 18:38"
        },
        {
          "id": "MDRW2100040957.1.11",
          "form": "시도해보세요",
          "original_form": "시도해보세요",
          "speaker_id": "1",
          "time": "20210905 18:39"
        },
        {
          "id": "MDRW2100040957.1.12",
          "form": "근처에 즐겨먹는 사람이 없어서",
          "original_form": "근처에 즐겨먹는 사람이 없어서",
          "speaker_id": "2",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100040957.1.13",
          "form": "먹을 기회가 따로 없었던 것 같아요",
          "original_form": "먹을 기회가 따로 없었던 것 같아요",
          "speaker_id": "2",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100040957.1.14",
          "form": "맛집이 있으시다면 추천해주세요!",
          "original_form": "맛집이 있으시다면 추천해주세요!",
          "speaker_id": "2",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100040957.1.15",
          "form": "그냥 집근처잇는데 엄마아빠 따라다녀요ㅠㅠㅋㅋㅋ",
          "original_form": "그냥 집근처잇는데 엄마아빠 따라다녀요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100040957.1.16",
          "form": "남원추어탕? 이런덴 체인점같던데 ㅋㅋㅋ",
          "original_form": "남원추어탕? 이런덴 체인점같던데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100040957.1.17",
          "form": "흠.. 참고 할게요 !",
          "original_form": "흠.. 참고 할게요 !",
          "speaker_id": "2",
          "time": "20210905 18:45"
        },
        {
          "id": "MDRW2100040957.1.18",
