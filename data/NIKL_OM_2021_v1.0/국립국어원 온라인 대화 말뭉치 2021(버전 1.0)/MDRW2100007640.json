      "utterance": [
        {
          "id": "MDRW2100007640.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 08:58"
        },
        {
          "id": "MDRW2100007640.1.2",
          "form": "저는 거의 설거지랑 강아지 케어 정도만요",
          "original_form": "저는 거의 설거지랑 강아지 케어 정도만요",
          "speaker_id": "2",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007640.1.3",
          "form": "아...케어도 힘들죠",
          "original_form": "아...케어도 힘들죠",
          "speaker_id": "1",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007640.1.4",
          "form": "가족들이랑 같이 사세요?",
          "original_form": "가족들이랑 같이 사세요?",
          "speaker_id": "2",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007640.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007640.1.6",
          "form": "가사는 주로 누가 담당하세요?",
          "original_form": "가사는 주로 누가 담당하세요?",
          "speaker_id": "2",
          "time": "20210822 09:01"
        },
        {
          "id": "MDRW2100007640.1.7",
          "form": "청소담당입니다",
          "original_form": "청소담당입니다",
          "speaker_id": "1",
          "time": "20210822 09:02"
        },
        {
          "id": "MDRW2100007640.1.8",
          "form": "전 청소가 제일 귀찮던데ㅜㅜ 대단하세요!",
          "original_form": "전 청소가 제일 귀찮던데ㅜㅜ 대단하세요!",
          "speaker_id": "2",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007640.1.9",
          "form": "저도 그렇지맘 어쩔 수 없조",
          "original_form": "저도 그렇지맘 어쩔 수 없조",
          "speaker_id": "1",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007640.1.10",
          "form": "가족들과 함께 식사하는 편이세요? 아니면 각자 솔플하는 편이세요?",
          "original_form": "가족들과 함께 식사하는 편이세요? 아니면 각자 솔플하는 편이세요?",
          "speaker_id": "2",
          "time": "20210822 09:04"
        },
        {
          "id": "MDRW2100007640.1.11",
          "form": "함께요 더 맛나요",
          "original_form": "함께요 더 맛나요",
          "speaker_id": "1",
          "time": "20210822 09:04"
        },
        {
          "id": "MDRW2100007640.1.12",
          "form": "가족과 함께 시간 보내시는거 좋아하시는군요!",
          "original_form": "가족과 함께 시간 보내시는거 좋아하시는군요!",
