      "utterance": [
        {
          "id": "MDRW2100016765.1.1",
          "form": "아 맞아요 저 그거 좋아해요ㅋㅋㅋ",
          "original_form": "아 맞아요 저 그거 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016765.1.2",
          "form": "맞죠 ㅋㅋ 그거 나오는 맛마다 맛있더라구요",
          "original_form": "맞죠 ㅋㅋ 그거 나오는 맛마다 맛있더라구요",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016765.1.3",
          "form": "민초송이도 있던데 !",
          "original_form": "민초송이도 있던데 !",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016765.1.4",
          "form": "민초송이 맛있어요 ?? ㅋㅋ",
          "original_form": "민초송이 맛있어요 ?? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016765.1.5",
          "form": "뭔가 맛있을거같긴한데",
          "original_form": "뭔가 맛있을거같긴한데",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016765.1.6",
          "form": "네ㅋㅋㅋ한번 드셔보세요ㅋㅋ민초 좋아하시면!",
          "original_form": "네ㅋㅋㅋ한번 드셔보세요ㅋㅋ민초 좋아하시면!",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016765.1.7",
          "form": "오오 좋아해서 한번 먹어봐야겠어요 ㅎㅎ",
          "original_form": "오오 좋아해서 한번 먹어봐야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016765.1.8",
          "form": "민초 소주 드셔보셨어요? 추천은 안해요ㅠㅠㅠㅋㅋㅋ",
          "original_form": "민초 소주 드셔보셨어요? 추천은 안해요ㅠㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016765.1.9",
          "form": "스벅에 민초프라페도 맛있던데 ! 드셔보셨어요?",
          "original_form": "스벅에 민초프라페도 맛있던데 ! 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016765.1.10",
          "form": "아 저 그거 먹고",
          "original_form": "아 저 그거 먹고",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016765.1.11",
          "form": "버렸어요 ㅎㅎ",
          "original_form": "버렸어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016765.1.12",
          "form": "ㅋㅋㄱㅋㄲ 민초 소주 ㅠㅠ",
          "original_form": "ㅋㅋㄱㅋㄲ 민초 소주 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016765.1.13",
          "form": "아 네 저 좋아해요ㅋㅋㅋㅋ",
          "original_form": "아 네 저 좋아해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016765.1.14",
          "form": "저도 깜짝 놀랐네요...ㅠㅠㅋㅋㅋㄴ",
          "original_form": "저도 깜짝 놀랐네요...ㅠㅠㅋㅋㅋㄴ",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016765.1.15",
          "form": "제가 유일하게 싫어하는 민초음식이 아닐까 ㅋㅋ 싶어요",
          "original_form": "제가 유일하게 싫어하는 민초음식이 아닐까 ㅋㅋ 싶어요",
          "speaker_id": "1",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016765.1.16",
          "form": "그 초코 맛이 ㅋㅋㄱ 아 너무 충격이였어요",
          "original_form": "그 초코 맛이 ㅋㅋㄱ 아 너무 충격이였어요",
          "speaker_id": "1",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016765.1.17",
          "form": "진짜 품을후가 없는 맛이더라구요ㅋㅋㅋ",
          "original_form": "진짜 품을후가 없는 맛이더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016765.1.18",
          "form": "진짜욬ㅋㅋㅋ 아 맛있다는 사람이 이해가 안되더라구요",
          "original_form": "진짜욬ㅋㅋㅋ 아 맛있다는 사람이 이해가 안되더라구요",
          "speaker_id": "1",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100016765.1.19",
          "form": "전 진짜 모르겠어요ㅋㅋ",
          "original_form": "전 진짜 모르겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016765.1.20",
          "form": "색깔은 되게 이쁘지않나요 ㅋㅋㅋ",
          "original_form": "색깔은 되게 이쁘지않나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016765.1.21",
          "form": "네 그래서 산건데ㅠㅠㅋㅋㅋㄴ",
          "original_form": "네 그래서 산건데ㅠㅠㅋㅋㅋㄴ",
