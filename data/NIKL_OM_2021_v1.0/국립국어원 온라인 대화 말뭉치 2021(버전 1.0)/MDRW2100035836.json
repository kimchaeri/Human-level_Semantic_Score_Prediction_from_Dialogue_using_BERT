      "utterance": [
        {
          "id": "MDRW2100035836.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210830 14:44"
        },
        {
          "id": "MDRW2100035836.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100035836.1.3",
          "form": "식사하셨어요~?",
          "original_form": "식사하셨어요~?",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100035836.1.4",
          "form": "네~ 먹었어요ㅎㅎ",
          "original_form": "네~ 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:13"
        },
        {
          "id": "MDRW2100035836.1.5",
          "form": "저녁은 뭐드실꺼에요~~시간빠르네요ㅠㅠ",
          "original_form": "저녁은 뭐드실꺼에요~~시간빠르네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 17:25"
        },
        {
          "id": "MDRW2100035836.1.6",
          "form": "네~ 시간 정말 빨라요ㅜㅜ\n아직 저녁 생각을 못 했어요",
          "original_form": "네~ 시간 정말 빨라요ㅜㅜ\n아직 저녁 생각을 못 했어요",
          "speaker_id": "2",
          "time": "20210830 17:28"
        },
        {
          "id": "MDRW2100035836.1.7",
          "form": "그쵸ㅠㅠ 전 저녁메뉴정해서 뭔가 편해요 ㅎ",
          "original_form": "그쵸ㅠㅠ 전 저녁메뉴정해서 뭔가 편해요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100035836.1.8",
          "form": "저녁메뉴 무엇인가요?",
          "original_form": "저녁메뉴 무엇인가요?",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100035836.1.9",
          "form": "저녁에 집에들어가면서 닭탕 포장해가려구요!",
          "original_form": "저녁에 집에들어가면서 닭탕 포장해가려구요!",
          "speaker_id": "1",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100035836.1.10",
          "form": "닭볶음탕에 국물이 많은거래요 ㅎ 저도 첨먹어봅니당!",
          "original_form": "닭볶음탕에 국물이 많은거래요 ㅎ 저도 첨먹어봅니당!",
          "speaker_id": "1",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100035836.1.11",
          "form": "오 닭볶음탕 맛있겠어요! \n국물이 많다니 사리면 추가하면 좋겠네요",
          "original_form": "오 닭볶음탕 맛있겠어요! \n국물이 많다니 사리면 추가하면 좋겠네요",
          "speaker_id": "2",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100035836.1.12",
          "form": "그쵸 집에서 당면을 넣어볼까싶어요!",
          "original_form": "그쵸 집에서 당면을 넣어볼까싶어요!",
          "speaker_id": "1",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100035836.1.13",
          "form": "당면! 양념 베어들면 맛있죠",
          "original_form": "당면! 양념 베어들면 맛있죠",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100035836.1.14",
          "form": "날도 살짝흐리고 몸도 으슬으슬해서",
          "original_form": "날도 살짝흐리고 몸도 으슬으슬해서",
          "speaker_id": "1",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100035836.1.15",
          "form": "얼큰하게 골라봣어요 ㅎㅎ",
          "original_form": "얼큰하게 골라봣어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100035836.1.16",
          "form": "그럴때는 국물있고 따뜻한 음식이 생각나죠",
          "original_form": "그럴때는 국물있고 따뜻한 음식이 생각나죠",
          "speaker_id": "2",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100035836.1.17",
          "form": "네ㅠㅠ 맞아요~ 밥만하면되겠어요!",
          "original_form": "네ㅠㅠ 맞아요~ 밥만하면되겠어요!",
          "speaker_id": "1",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100035836.1.18",
          "form": "저녁먹을 시간이 기다려지겠어요",
          "original_form": "저녁먹을 시간이 기다려지겠어요",
          "speaker_id": "2",
          "time": "20210830 18:01"
        }
      ]
