        },
        {
          "id": "MDRW2100033731.1.3",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033731.1.4",
          "form": "전 초밥이요~",
          "original_form": "전 초밥이요~",
          "speaker_id": "1",
          "time": "20210825 00:00"
        },
        {
          "id": "MDRW2100033731.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 00:00"
        },
        {
          "id": "MDRW2100033731.1.6",
          "form": "전 떡볶이 좋아해요",
          "original_form": "전 떡볶이 좋아해요",
          "speaker_id": "2",
          "time": "20210825 09:20"
        },
        {
          "id": "MDRW2100033731.1.7",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 09:20"
        },
        {
          "id": "MDRW2100033731.1.8",
          "form": "아침은패스요ㅎㅎ",
          "original_form": "아침은패스요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 11:31"
        },
        {
          "id": "MDRW2100033731.1.9",
          "form": "엽떡vs죠스떡?",
          "original_form": "엽떡vs죠스떡?",
          "speaker_id": "1",
          "time": "20210825 11:31"
        },
        {
          "id": "MDRW2100033731.1.10",
          "form": "엽떡이요",
          "original_form": "엽떡이요",
          "speaker_id": "2",
          "time": "20210825 11:47"
        },
        {
          "id": "MDRW2100033731.1.11",
          "form": "죠스는 안 먹어본지 오래됐는데 엽떡보단 별로였어요",
          "original_form": "죠스는 안 먹어본지 오래됐는데 엽떡보단 별로였어요",
          "speaker_id": "2",
          "time": "20210825 11:47"
        },
        {
          "id": "MDRW2100033731.1.12",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
