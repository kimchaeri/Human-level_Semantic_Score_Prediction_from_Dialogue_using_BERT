        },
        {
          "id": "MDRW2100037516.1.2",
          "form": "저희 언니는 잔소리꾼이에요.......ㅠ",
          "original_form": "저희 언니는 잔소리꾼이에요.......ㅠ",
          "speaker_id": "1",
          "time": "20210901 02:36"
        },
        {
          "id": "MDRW2100037516.1.3",
          "form": "평소에 잔소리를 많이 듣나요?",
          "original_form": "평소에 잔소리를 많이 듣나요?",
          "speaker_id": "2",
          "time": "20210901 02:36"
        },
        {
          "id": "MDRW2100037516.1.4",
          "form": "1일 10잔소리요",
          "original_form": "1일 10잔소리요",
          "speaker_id": "1",
          "time": "20210901 02:37"
        },
        {
          "id": "MDRW2100037516.1.5",
          "form": "주로 어떤 상황에서 들으시나요?",
          "original_form": "주로 어떤 상황에서 들으시나요?",
          "speaker_id": "2",
          "time": "20210901 02:38"
        },
        {
          "id": "MDRW2100037516.1.6",
          "form": "방금 다리떠니까 뭐라하네여",
          "original_form": "방금 다리떠니까 뭐라하네여",
          "speaker_id": "1",
          "time": "20210901 02:38"
        },
        {
          "id": "MDRW2100037516.1.7",
          "form": "11잔소리 됐어요",
          "original_form": "11잔소리 됐어요",
          "speaker_id": "1",
          "time": "20210901 02:38"
        },
        {
          "id": "MDRW2100037516.1.8",
          "form": "ㅋㅋㅋㅋㅋ 방을 같이 쓰나요?",
          "original_form": "ㅋㅋㅋㅋㅋ 방을 같이 쓰나요?",
          "speaker_id": "2",
          "time": "20210901 02:39"
        },
        {
          "id": "MDRW2100037516.1.9",
          "form": "아뇨 거실에 잇거든요",
          "original_form": "아뇨 거실에 잇거든요",
          "speaker_id": "1",
          "time": "20210901 02:39"
        },
        {
          "id": "MDRW2100037516.1.10",
          "form": "아 거실에 같이 있군요",
          "original_form": "아 거실에 같이 있군요",
          "speaker_id": "2",
          "time": "20210901 02:39"
        },
        {
          "id": "MDRW2100037516.1.11",
          "form": "근데 다리떠는 습관은 안 좋긴 하죠",
          "original_form": "근데 다리떠는 습관은 안 좋긴 하죠",
          "speaker_id": "2",
          "time": "20210901 02:40"
        },
        {
          "id": "MDRW2100037516.1.12",
          "form": "고치고 싶지만...",
          "original_form": "고치고 싶지만...",
          "speaker_id": "1",
          "time": "20210901 02:40"
        },
        {
          "id": "MDRW2100037516.1.13",
          "form": "힘들죠?",
          "original_form": "힘들죠?",
          "speaker_id": "2",
          "time": "20210901 02:41"
        },
        {
          "id": "MDRW2100037516.1.14",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210901 02:41"
        },
        {
          "id": "MDRW2100037516.1.15",
          "form": "다리 안떠세요?",
          "original_form": "다리 안떠세요?",
          "speaker_id": "1",
          "time": "20210901 02:41"
        },
        {
          "id": "MDRW2100037516.1.16",
          "form": "습관들은 고치기 힘들죠ㅜㅜ",
          "original_form": "습관들은 고치기 힘들죠ㅜㅜ",
