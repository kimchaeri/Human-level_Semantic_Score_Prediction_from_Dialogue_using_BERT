      "utterance": [
        {
          "id": "MDRW2100043679.1.1",
          "form": "지금 국가 끝났당",
          "original_form": "지금 국가 끝났당",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043679.1.2",
          "form": "레바논 큰 점수로 이겨야대..아니면 가시밭길",
          "original_form": "레바논 큰 점수로 이겨야대..아니면 가시밭길",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043679.1.3",
          "form": "이겼다며?",
          "original_form": "이겼다며?",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100043679.1.4",
          "form": "ㅇㅇ겨우ㅋㅋㅋ",
          "original_form": "ㅇㅇ겨우ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100043679.1.5",
          "form": "티비엔 중계안했으면 ㅋㅋ...",
          "original_form": "티비엔 중계안했으면 ㅋㅋ...",
          "speaker_id": "2",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100043679.1.6",
          "form": "부정타는거같아 ㅎㄹ",
          "original_form": "부정타는거같아 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100043679.1.7",
          "form": "근데 궁금한게 황의찬이 황의조 동생이야?",
          "original_form": "근데 궁금한게 황의찬이 황의조 동생이야?",
          "speaker_id": "1",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100043679.1.8",
          "form": "엥아니욬ㅋㅋ",
          "original_form": "엥아니욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100043679.1.9",
          "form": "이름비슷한데",
          "original_form": "이름비슷한데",
          "speaker_id": "1",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100043679.1.10",
          "form": "니가이름을 잘못알고있을거란생각은1도안하는구나",
          "original_form": "니가이름을 잘못알고있을거란생각은1도안하는구나",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100043679.1.11",
          "form": "의찬이가아니고 희찬이란다",
          "original_form": "의찬이가아니고 희찬이란다",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100043679.1.12",
          "form": "아그래? 오늘 뉴스에서 봤어",
          "original_form": "아그래? 오늘 뉴스에서 봤어",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100043679.1.13",
          "form": "뉴스에 모래?",
          "original_form": "뉴스에 모래?",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100043679.1.14",
          "form": "잘햇때? 졸전이래?",
          "original_form": "잘햇때? 졸전이래?",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100043679.1.15",
          "form": "요즘 뉴스도 안봐 스트레스..근데 어제 뉴스에 현기차가 수소차로만 만든대서 버스같은거 ㅋㅋㅋ 잠깐 솔깃했어",
          "original_form": "요즘 뉴스도 안봐 스트레스..근데 어제 뉴스에 현기차가 수소차로만 만든대서 버스같은거 ㅋㅋㅋ 잠깐 솔깃했어",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100043679.1.16",
          "form": "후반 교체한애가 1골넣었대",
          "original_form": "후반 교체한애가 1골넣었대",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100043679.1.17",
          "form": "수소주식사야하나 뉴스 믿어?",
          "original_form": "수소주식사야하나 뉴스 믿어?",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100043679.1.18",
          "form": "ㅋㅋㅋㅋ아니근데 그런걸 거짓말하진않겠짘ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아니근데 그런걸 거짓말하진않겠짘ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100043679.1.19",
          "form": "아니근데 어제 축구중계해주는데",
          "original_form": "아니근데 어제 축구중계해주는데",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100043679.1.20",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100043679.1.21",
          "form": "광고가 엄청많은거야",
          "original_form": "광고가 엄청많은거야",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100043679.1.22",
          "form": "오래함?",
          "original_form": "오래함?",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100043679.1.23",
          "form": "그래서 앞부분 50초날려먹음..",
          "original_form": "그래서 앞부분 50초날려먹음..",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100043679.1.24",
          "form": "말이되냐...방송국..",
          "original_form": "말이되냐...방송국..",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100043679.1.25",
          "form": "야그거아냐",
          "original_form": "야그거아냐",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100043679.1.26",
          "form": "주식방송 개미는 뚠뚠",
          "original_form": "주식방송 개미는 뚠뚠",
