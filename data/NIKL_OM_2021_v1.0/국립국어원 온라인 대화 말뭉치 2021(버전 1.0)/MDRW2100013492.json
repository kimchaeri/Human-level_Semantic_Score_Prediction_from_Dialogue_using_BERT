      "utterance": [
        {
          "id": "MDRW2100013492.1.1",
          "form": "name1님 지역의 날씨는 어떤가요?",
          "original_form": "&name1&님 지역의 날씨는 어떤가요?",
          "speaker_id": "2",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100013492.1.2",
          "form": "지금 해가 쨍쨍해요!",
          "original_form": "지금 해가 쨍쨍해요!",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100013492.1.3",
          "form": "오 바람은 많이 안부나요?",
          "original_form": "오 바람은 많이 안부나요?",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100013492.1.4",
          "form": "네! 몇시간 전엔 흐렸는데",
          "original_form": "네! 몇시간 전엔 흐렸는데",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100013492.1.5",
          "form": "지금 바람도 안불고 뜨거워요ㅋㅋ",
          "original_form": "지금 바람도 안불고 뜨거워요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100013492.1.6",
          "form": "아 정말요??? 저희는 완전 어둡고 비까지 내리고 있어요...",
          "original_form": "아 정말요??? 저희는 완전 어둡고 비까지 내리고 있어요...",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013492.1.7",
          "form": "헉 지금 어두워요?!",
          "original_form": "헉 지금 어두워요?!",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013492.1.8",
          "form": "헐 신기하군요....",
          "original_form": "헐 신기하군요....",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013492.1.9",
          "form": "네 아침부터 지금까지 계속 비가 내리더라구요ㅋㅋㅋㅋ",
          "original_form": "네 아침부터 지금까지 계속 비가 내리더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013492.1.10",
          "form": "분명 어제는 해가 떠서 산책도 갔던 거 같은데...ㅠㅠ",
          "original_form": "분명 어제는 해가 떠서 산책도 갔던 거 같은데...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013492.1.11",
          "form": "헉ㅋㅋㄱㄱ저희 지역이랑 반대네요",
          "original_form": "헉ㅋㅋㄱㄱ저희 지역이랑 반대네요",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013492.1.12",
          "form": "저흰 어제 우중충하고 비 내렸어요...ㅋㅋ",
          "original_form": "저흰 어제 우중충하고 비 내렸어요...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013492.1.13",
          "form": "오... 완전 반대네요ㅋㅋㅋ 신기해요ㅋㅋㅋ",
          "original_form": "오... 완전 반대네요ㅋㅋㅋ 신기해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013492.1.14",
          "form": "이제 쌀쌀해서 겉옷도 챙겨야 할 것 같아요",
          "original_form": "이제 쌀쌀해서 겉옷도 챙겨야 할 것 같아요",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013492.1.15",
          "form": "맞아요! 이제 여름 거의 지나간 것 같아요",
          "original_form": "맞아요! 이제 여름 거의 지나간 것 같아요",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013492.1.16",
          "form": "밤에 잘 때도 좀 춥더라고요ㅎㅎ",
          "original_form": "밤에 잘 때도 좀 춥더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013492.1.17",
          "form": "맞아요ㅋㅋㅋ 곧 가을이라서 가을 옷 입을 생각에 설레네요!",
          "original_form": "맞아요ㅋㅋㅋ 곧 가을이라서 가을 옷 입을 생각에 설레네요!",
          "speaker_id": "2",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013492.1.18",
          "form": "저도 봄가을옷이 좋아요!",
          "original_form": "저도 봄가을옷이 좋아요!",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013492.1.19",
          "form": "트렌치코트랑 가디건 이런거 넘 입고싶네요",
          "original_form": "트렌치코트랑 가디건 이런거 넘 입고싶네요",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013492.1.20",
          "form": "어우 트렌치코트 좋죠ㅋㅋㅋ 전 빨리 후드티 입고 싶네요",
          "original_form": "어우 트렌치코트 좋죠ㅋㅋㅋ 전 빨리 후드티 입고 싶네요",
          "speaker_id": "2",
