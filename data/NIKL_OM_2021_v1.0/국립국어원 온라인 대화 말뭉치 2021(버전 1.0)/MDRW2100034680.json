      "utterance": [
        {
          "id": "MDRW2100034680.1.1",
          "form": "그게 너무 멋져보였어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그게 너무 멋져보였어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100034680.1.2",
          "form": "진짜 멋지긴하네요ㅋㅋ",
          "original_form": "진짜 멋지긴하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100034680.1.3",
          "form": "점심시간에요?",
          "original_form": "점심시간에요?",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100034680.1.4",
          "form": "그쳐 ㅋㅋㅋ",
          "original_form": "그쳐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100034680.1.5",
          "form": "그래서 더 로망이었던거같아요",
          "original_form": "그래서 더 로망이었던거같아요",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100034680.1.6",
          "form": "와 진짜 멋있네요ㅋㅋㅋ",
          "original_form": "와 진짜 멋있네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100034680.1.7",
          "form": "지금 생각하면 그때가 좋았죠 야자하든 안하든 ㅋㅋㅋ",
          "original_form": "지금 생각하면 그때가 좋았죠 야자하든 안하든 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100034680.1.8",
          "form": "마자요 별걱정없이 공부만 하면 되니까요",
          "original_form": "마자요 별걱정없이 공부만 하면 되니까요",
          "speaker_id": "2",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100034680.1.9",
          "form": "맞아요 돈걱정도 없고",
          "original_form": "맞아요 돈걱정도 없고",
          "speaker_id": "1",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100034680.1.10",
          "form": "일할 필요가 없었어서요 ㅋㅋㅋㅋ",
          "original_form": "일할 필요가 없었어서요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100034680.1.11",
          "form": "그쵸 그거하나는 참 좋죠!",
          "original_form": "그쵸 그거하나는 참 좋죠!",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100034680.1.12",
          "form": "다 같은 생각이네요 ㅋㅋㅋ",
          "original_form": "다 같은 생각이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100034680.1.13",
          "form": "어른들도 항상 그얘기하시더라구요",
          "original_form": "어른들도 항상 그얘기하시더라구요",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100034680.1.14",
          "form": "맞아요 ㅋㅋㅋ커야 알게되나봐요",
          "original_form": "맞아요 ㅋㅋㅋ커야 알게되나봐요",
          "speaker_id": "1",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100034680.1.15",
          "form": "돈걱정없이 공부만 하는게 얼마나 좋냐구요",
          "original_form": "돈걱정없이 공부만 하는게 얼마나 좋냐구요",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100034680.1.16",
          "form": "그니까요ㅋㅋ",
          "original_form": "그니까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100034680.1.17",
          "form": "맞아요 꿀이죠..",
