          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039632.1.6",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039632.1.7",
          "form": "오늘 먹어야죠",
          "original_form": "오늘 먹어야죠",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039632.1.8",
          "form": "저녁 뭐 먹었어요",
          "original_form": "저녁 뭐 먹었어요",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039632.1.9",
          "form": "오늘 감자탕먹었어여ㅋㅋㅋ",
          "original_form": "오늘 감자탕먹었어여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039632.1.10",
          "form": "소스가없어서아쉬웠지만 나름대로 많이머굼요",
          "original_form": "소스가없어서아쉬웠지만 나름대로 많이머굼요",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039632.1.11",
          "form": "홀",
          "original_form": "홀",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039632.1.12",
          "form": "왜 혼자 맛있는거 먹어요",
          "original_form": "왜 혼자 맛있는거 먹어요",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039632.1.13",
          "form": "ㅋㅋㅋ왜여 옛날도시락드셨짜나여",
          "original_form": "ㅋㅋㅋ왜여 옛날도시락드셨짜나여",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039632.1.14",
          "form": "ㅋㅋㅋㅋ그것도 맛있었지만",
          "original_form": "ㅋㅋㅋㅋ그것도 맛있었지만",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039632.1.15",
          "form": "감자탕은 고기잖아여",
          "original_form": "감자탕은 고기잖아여",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039632.1.16",
          "form": "그리고 국물도 맛있고",
          "original_form": "그리고 국물도 맛있고",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039632.1.17",
          "form": "라면사리 먹었어요",
          "original_form": "라면사리 먹었어요",
