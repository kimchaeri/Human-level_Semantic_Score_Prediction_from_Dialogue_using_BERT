        },
        {
          "id": "MDRW2100045639.1.2",
          "form": "나 어제 쇼핑한거 택배옴",
          "original_form": "나 어제 쇼핑한거 택배옴",
          "speaker_id": "1",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045639.1.3",
          "form": "뭐샀는데?",
          "original_form": "뭐샀는데?",
          "speaker_id": "2",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045639.1.4",
          "form": "양념감자 샀어",
          "original_form": "양념감자 샀어",
          "speaker_id": "1",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045639.1.5",
          "form": "냉장식품이야?",
          "original_form": "냉장식품이야?",
          "speaker_id": "2",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045639.1.6",
          "form": "아니 냉동식품이야",
          "original_form": "아니 냉동식품이야",
          "speaker_id": "1",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045639.1.7",
          "form": "오 그럼 기름에 튀겨먹어?",
          "original_form": "오 그럼 기름에 튀겨먹어?",
          "speaker_id": "2",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045639.1.8",
          "form": "아니 에어프라이기만 돌려",
          "original_form": "아니 에어프라이기만 돌려",
          "speaker_id": "1",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045639.1.9",
          "form": "아 튀김이구나",
          "original_form": "아 튀김이구나",
          "speaker_id": "2",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045639.1.10",
          "form": "응 에어프라이기만 돌리면 되니까 엄첨 편해",
          "original_form": "응 에어프라이기만 돌리면 되니까 엄첨 편해",
          "speaker_id": "1",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045639.1.11",
          "form": "맛은 어떤데",
          "original_form": "맛은 어떤데",
          "speaker_id": "2",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045639.1.12",
          "form": "동원식품껀데 맛있더라고",
          "original_form": "동원식품껀데 맛있더라고",
          "speaker_id": "1",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045639.1.13",
          "form": "그거에 맥주먹니?",
          "original_form": "그거에 맥주먹니?",
