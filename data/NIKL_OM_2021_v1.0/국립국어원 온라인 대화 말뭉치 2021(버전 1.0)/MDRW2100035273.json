        {
          "id": "MDRW2100035273.1.1",
          "form": "점심은요?",
          "original_form": "점심은요?",
          "speaker_id": "2",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100035273.1.2",
          "form": "저 두부면파스타 먹었어요!",
          "original_form": "저 두부면파스타 먹었어요!",
          "speaker_id": "1",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100035273.1.3",
          "form": "점심 드셨나요",
          "original_form": "점심 드셨나요",
          "speaker_id": "1",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100035273.1.4",
          "form": "네~^^ 미역국으로요",
          "original_form": "네~^^ 미역국으로요",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100035273.1.5",
          "form": "오 맛있으셨겠어요 ㅎㅎ",
          "original_form": "오 맛있으셨겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100035273.1.6",
          "form": "네ㅎㅎ 잘 끓여졌어용",
          "original_form": "네ㅎㅎ 잘 끓여졌어용",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100035273.1.7",
          "form": "소고기 넣구 하신건가요*",
          "original_form": "소고기 넣구 하신건가요*",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100035273.1.8",
          "form": "네 양지 넣고 끓였어요 ㅎㅎ",
          "original_form": "네 양지 넣고 끓였어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100035273.1.9",
          "form": "오 고소했을것 같아요ㅋㅋ",
          "original_form": "오 고소했을것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100035273.1.10",
          "form": "미역국 끓이실때 참기름으로 볶으시나요",
          "original_form": "미역국 끓이실때 참기름으로 볶으시나요",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100035273.1.11",
          "form": "네 고기 먼저 참기름으로 볶고 미역 볶고 물 넣고 계속 끓이다가 간 맞추고 ㅎ",
          "original_form": "네 고기 먼저 참기름으로 볶고 미역 볶고 물 넣고 계속 끓이다가 간 맞추고 ㅎ",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100035273.1.12",
          "form": "그쵸 참기름으로 볶아야 더 맛있죠..!",
          "original_form": "그쵸 참기름으로 볶아야 더 맛있죠..!",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100035273.1.13",
          "form": "맞아요 달달달 볶아야",
          "original_form": "맞아요 달달달 볶아야",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100035273.1.14",
          "form": "저녁은 뭐드실 예정이신가요?",
          "original_form": "저녁은 뭐드실 예정이신가요?",
          "speaker_id": "1",
