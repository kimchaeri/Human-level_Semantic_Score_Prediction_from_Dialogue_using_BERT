      "utterance": [
        {
          "id": "MDRW2100041569.1.1",
          "form": "제가 아플때 식구들이 많이 보살펴줘서 지금도 고마워요",
          "original_form": "제가 아플때 식구들이 많이 보살펴줘서 지금도 고마워요",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041569.1.2",
          "form": "name1님의 수고를 어머니도 잘 아실거에요",
          "original_form": "&name1&님의 수고를 어머니도 잘 아실거에요",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041569.1.3",
          "form": "네. 뭐. 그러시겠죠.",
          "original_form": "네. 뭐. 그러시겠죠.",
          "speaker_id": "1",
          "time": "20210906 14:58"
        },
        {
          "id": "MDRW2100041569.1.4",
          "form": "빨리 건강해지시면 좋을텐데요..",
          "original_form": "빨리 건강해지시면 좋을텐데요..",
          "speaker_id": "1",
          "time": "20210906 14:58"
        },
        {
          "id": "MDRW2100041569.1.5",
          "form": "맞아요 그리고 킴님도 건강 잘 챙겨야해요",
          "original_form": "맞아요 그리고 킴님도 건강 잘 챙겨야해요",
          "speaker_id": "2",
          "time": "20210906 15:00"
        },
        {
          "id": "MDRW2100041569.1.6",
          "form": "간병이 절대 쉬운 일이 아니거든요",
          "original_form": "간병이 절대 쉬운 일이 아니거든요",
          "speaker_id": "2",
          "time": "20210906 15:00"
        },
        {
          "id": "MDRW2100041569.1.7",
          "form": "아, 정말로요.",
          "original_form": "아, 정말로요.",
          "speaker_id": "1",
          "time": "20210906 15:05"
        },
        {
          "id": "MDRW2100041569.1.8",
          "form": "병원 보호자침대에서 이틀 있다보니까 일단 허리가 너무 아파서....",
          "original_form": "병원 보호자침대에서 이틀 있다보니까 일단 허리가 너무 아파서....",
          "speaker_id": "1",
          "time": "20210906 15:05"
        },
        {
          "id": "MDRW2100041569.1.9",
          "form": "저 아팠을때 주변 분들이 제 딸도 챙겨주더라구요",
          "original_form": "저 아팠을때 주변 분들이 제 딸도 챙겨주더라구요",
          "speaker_id": "2",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100041569.1.10",
          "form": "간병하다가 병걸린다고 잘 먹으라고",
          "original_form": "간병하다가 병걸린다고 잘 먹으라고",
          "speaker_id": "2",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100041569.1.11",
          "form": "주변 분들이 온정의 손길을 내밀고 계세요.",
          "original_form": "주변 분들이 온정의 손길을 내밀고 계세요.",
          "speaker_id": "1",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041569.1.12",
          "form": "얻어먹은 반찬과 죽이 벌써 몇그릇인지",
          "original_form": "얻어먹은 반찬과 죽이 벌써 몇그릇인지",
          "speaker_id": "1",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041569.1.13",
          "form": "감사한 일이죠.",
          "original_form": "감사한 일이죠.",
          "speaker_id": "1",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041569.1.14",
          "form": "저땜에 딸이 참 한참동안 죽 많이 먹었어요",
          "original_form": "저땜에 딸이 참 한참동안 죽 많이 먹었어요",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100041569.1.15",
          "form": "죽 만들어보더니 본죽이 비싼 이유를 알겠다고 하더라구요",
          "original_form": "죽 만들어보더니 본죽이 비싼 이유를 알겠다고 하더라구요",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100041569.1.16",
          "form": "저희집은 걍 비비고...",
          "original_form": "저희집은 걍 비비고...",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041569.1.17",
          "form": "의외로 본죽 맛없다고 하시더라구요",
          "original_form": "의외로 본죽 맛없다고 하시더라구요",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041569.1.18",
          "form": "싱겁대요.",
          "original_form": "싱겁대요.",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041569.1.19",
          "form": "그래서 환자용인가보군요",
          "original_form": "그래서 환자용인가보군요",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041569.1.20",
          "form": "환자들 본죽 많이 사주잖아요",
          "original_form": "환자들 본죽 많이 사주잖아요",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041569.1.21",
          "form": "전 비비고를 안먹어봐서 비교가 어렵네요",
          "original_form": "전 비비고를 안먹어봐서 비교가 어렵네요",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041569.1.22",
          "form": "나쁘지 않습니다. 파우치로 된거 하나면 딱 2끼 나와요",
          "original_form": "나쁘지 않습니다. 파우치로 된거 하나면 딱 2끼 나와요",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041569.1.23",
          "form": "일반인한텐 1끼겠지만요",
          "original_form": "일반인한텐 1끼겠지만요",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041569.1.24",
          "form": "맞아요.. 죽양이 많아서 나눠먹어야 하더라구요",
          "original_form": "맞아요.. 죽양이 많아서 나눠먹어야 하더라구요",
          "speaker_id": "2",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041569.1.25",
          "form": "종류도 그럭저럭 다양해서 나쁘지 않아요.",
          "original_form": "종류도 그럭저럭 다양해서 나쁘지 않아요.",
          "speaker_id": "1",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041569.1.26",
          "form": "가끔은 진밥에 맑은 국해서 드시기도 하고요",
          "original_form": "가끔은 진밥에 맑은 국해서 드시기도 하고요",
          "speaker_id": "1",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041569.1.27",
          "form": "지금 진밥까진 가능하신거군요",
          "original_form": "지금 진밥까진 가능하신거군요",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041569.1.28",
          "form": "네, 국에 말아서",
          "original_form": "네, 국에 말아서",
          "speaker_id": "1",
          "time": "20210906 15:50"
        }
      ]
