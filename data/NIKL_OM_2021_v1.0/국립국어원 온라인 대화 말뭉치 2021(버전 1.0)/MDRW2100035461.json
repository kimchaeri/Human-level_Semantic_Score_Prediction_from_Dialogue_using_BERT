      "utterance": [
        {
          "id": "MDRW2100035461.1.1",
          "form": "더벤티 한번도 안가봤어요",
          "original_form": "더벤티 한번도 안가봤어요",
          "speaker_id": "2",
          "time": "20210829 19:30"
        },
        {
          "id": "MDRW2100035461.1.2",
          "form": "당충전 땡기네요.. 아메리카노 방금 한잔 때렸는데",
          "original_form": "당충전 땡기네요.. 아메리카노 방금 한잔 때렸는데",
          "speaker_id": "2",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100035461.1.3",
          "form": "밤에 잠 안오시면머째요ㅜㅜ",
          "original_form": "밤에 잠 안오시면머째요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100035461.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ아 낮에먹어서 괜찮아요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 낮에먹어서 괜찮아요!",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100035461.1.5",
          "form": "초코라떼 땡기네유 저도",
          "original_form": "초코라떼 땡기네유 저도",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100035461.1.6",
          "form": "여름과일은 많이 드셧어요?",
          "original_form": "여름과일은 많이 드셧어요?",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100035461.1.7",
          "form": "음 저는 과일을 좋아하는편은아니라서ㅜ",
          "original_form": "음 저는 과일을 좋아하는편은아니라서ㅜ",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035461.1.8",
          "form": "그래도 먹으려고는해요ㅜㅜ",
          "original_form": "그래도 먹으려고는해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035461.1.9",
          "form": "아 과일을 안좋아하시는구나",
          "original_form": "아 과일을 안좋아하시는구나",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035461.1.10",
          "form": "저는 단게 좋아서 좋아하는데",
          "original_form": "저는 단게 좋아서 좋아하는데",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035461.1.11",
          "form": "싫어하게된 이유나 계기가 있으신가요?",
          "original_form": "싫어하게된 이유나 계기가 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035461.1.12",
          "form": "그냥 저는 원래 그랬어요 ㅋㅋ",
          "original_form": "그냥 저는 원래 그랬어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035461.1.13",
          "form": "언제부터가 아니라",
          "original_form": "언제부터가 아니라",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035461.1.14",
          "form": "근데 저도 단건 좋아요 ㅋㅋ",
          "original_form": "근데 저도 단건 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100035461.1.15",
          "form": "아핰ㅋㅋㅋ 하긴 저도 원래 토마토를 싫어해요",
          "original_form": "아핰ㅋㅋㅋ 하긴 저도 원래 토마토를 싫어해요",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035461.1.16",
          "form": "좀 역한 맛이 나서",
          "original_form": "좀 역한 맛이 나서",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035461.1.17",
          "form": "그러고보면 저는 오이 별로 안좋아해욥 ㅋㅋ",
          "original_form": "그러고보면 저는 오이 별로 안좋아해욥 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035461.1.18",
          "form": "오이 호불호 많이 갈리더라구요!",
          "original_form": "오이 호불호 많이 갈리더라구요!",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035461.1.19",
          "form": "저녁 맛난거 드셨어요?ㅎ",
          "original_form": "저녁 맛난거 드셨어요?ㅎ",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035461.1.20",
          "form": "저녁에 라면먹었어요..ㅋㅋㅋㅋ",
          "original_form": "저녁에 라면먹었어요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:29"
        },
        {
          "id": "MDRW2100035461.1.21",
          "form": "라면 좋아하시나요",
          "original_form": "라면 좋아하시나요",
          "speaker_id": "2",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100035461.1.22",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035461.1.23",
          "form": "라면 뭐 젤 좋아하세요?",
          "original_form": "라면 뭐 젤 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035461.1.24",
          "form": "진라면 순한맛 ㅋㅋ제일 오래 꾸준히 먹게되요 ㅋㅋㅋ",
          "original_form": "진라면 순한맛 ㅋㅋ제일 오래 꾸준히 먹게되요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100035461.1.25",
          "form": "진순 매니아가 잇다닠ㅋㅋㅋ",
          "original_form": "진순 매니아가 잇다닠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035461.1.26",
          "form": "저희집에 동생이 어려서 매운걸 못먹어가지구",
          "original_form": "저희집에 동생이 어려서 매운걸 못먹어가지구",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035461.1.27",
          "form": "저희집도 진라면 순한맛만 먹어요",
          "original_form": "저희집도 진라면 순한맛만 먹어요",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035461.1.28",
          "form": "저도 길들여졋어요",
          "original_form": "저도 길들여졋어요",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035461.1.29",
          "form": "진라면은 부셔먹어도 맛있어요 ㅋㅋ",
          "original_form": "진라면은 부셔먹어도 맛있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:06"
        }
      ]
    }
