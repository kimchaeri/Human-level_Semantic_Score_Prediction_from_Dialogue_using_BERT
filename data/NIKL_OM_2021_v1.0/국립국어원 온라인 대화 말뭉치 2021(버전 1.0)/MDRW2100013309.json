      "utterance": [
        {
          "id": "MDRW2100013309.1.1",
          "form": "티비 많이 보세요?",
          "original_form": "티비 많이 보세요?",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100013309.1.2",
          "form": "테레지는 특정 프로만 찍어서봐요..",
          "original_form": "테레지는 특정 프로만 찍어서봐요..",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100013309.1.3",
          "form": "어떤 프로 좋아하세요ㅡ",
          "original_form": "어떤 프로 좋아하세요ㅡ",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100013309.1.4",
          "form": "전 금쪽이여",
          "original_form": "전 금쪽이여",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100013309.1.5",
          "form": "일일극이요?",
          "original_form": "일일극이요?",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100013309.1.6",
          "form": "아니여 육아프로예여",
          "original_form": "아니여 육아프로예여",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100013309.1.7",
          "form": "아.. 제가 테레비로 보는게 아니라서.. 몰랐네요..",
          "original_form": "아.. 제가 테레비로 보는게 아니라서.. 몰랐네요..",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100013309.1.8",
          "form": "예능은 잘 안보세요?",
          "original_form": "예능은 잘 안보세요?",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100013309.1.9",
          "form": "예능은 모가 재밌나요? 잘몰라서ㅠ",
          "original_form": "예능은 모가 재밌나요? 잘몰라서ㅠ",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100013309.1.10",
          "form": "유재석 나오는거 자주봐요..",
          "original_form": "유재석 나오는거 자주봐요..",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100013309.1.11",
          "form": "식스센스나 놀면뭐하니..",
          "original_form": "식스센스나 놀면뭐하니..",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100013309.1.12",
          "form": "전 육아로 늘 만화만ㅠ",
          "original_form": "전 육아로 늘 만화만ㅠ",
          "speaker_id": "1",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100013309.1.13",
          "form": "애기 보는거요? 또봇? 콩순이?",
          "original_form": "애기 보는거요? 또봇? 콩순이?",
          "speaker_id": "2",
          "time": "20210827 00:14"
        },
        {
          "id": "MDRW2100013309.1.14",
          "form": "자두요ㅠ",
          "original_form": "자두요ㅠ",
          "speaker_id": "1",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100013309.1.15",
          "form": "ㅋㅋㅋ 요즘은 자두가 대세에요?",
          "original_form": "ㅋㅋㅋ 요즘은 자두가 대세에요?",
          "speaker_id": "2",
          "time": "20210827 10:22"
        },
        {
          "id": "MDRW2100013309.1.16",
          "form": "자두엄마가 저같대요",
          "original_form": "자두엄마가 저같대요",
          "speaker_id": "1",
          "time": "20210827 11:13"
        }
