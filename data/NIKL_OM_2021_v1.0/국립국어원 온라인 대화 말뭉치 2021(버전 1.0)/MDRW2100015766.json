      "utterance": [
        {
          "id": "MDRW2100015766.1.1",
          "form": "네 저는 하나도 안 매웠어요",
          "original_form": "네 저는 하나도 안 매웠어요",
          "speaker_id": "2",
          "time": "20210826 00:53"
        },
        {
          "id": "MDRW2100015766.1.2",
          "form": "배민에서 가끔 대폭 할인하니까 드셔보세요 ㅋㅋㅋ",
          "original_form": "배민에서 가끔 대폭 할인하니까 드셔보세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015766.1.3",
          "form": "오호 ㅋㅋㅋ좋아요!!!도전해볼래용",
          "original_form": "오호 ㅋㅋㅋ좋아요!!!도전해볼래용",
          "speaker_id": "1",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015766.1.4",
          "form": "오늘 요기요 슈퍼클럽 가입할까 고민했어요",
          "original_form": "오늘 요기요 슈퍼클럽 가입할까 고민했어요",
          "speaker_id": "1",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015766.1.5",
          "form": "떡볶이 먹을까 싶어서ㅜㅜ",
          "original_form": "떡볶이 먹을까 싶어서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015766.1.6",
          "form": "돼지클럽으로 유명하더라구여...",
          "original_form": "돼지클럽으로 유명하더라구여...",
          "speaker_id": "2",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015766.1.7",
          "form": "ㅠㅠ 어디거 드시랴고 하셨어요?",
          "original_form": "ㅠㅠ 어디거 드시랴고 하셨어요?",
          "speaker_id": "2",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015766.1.8",
          "form": "동네에 로제로제인가",
          "original_form": "동네에 로제로제인가",
          "speaker_id": "1",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015766.1.9",
          "form": "로제떡볶이 전문으로 하는곳 생겼더라구요 후후",
          "original_form": "로제떡볶이 전문으로 하는곳 생겼더라구요 후후",
          "speaker_id": "1",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015766.1.10",
          "form": "와 전문이면 맛있을 것 같아요",
          "original_form": "와 전문이면 맛있을 것 같아요",
          "speaker_id": "2",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015766.1.11",
          "form": "헉 돼지클럽이라는 말 들으니까 참아야겠어요...",
          "original_form": "헉 돼지클럽이라는 말 들으니까 참아야겠어요...",
          "speaker_id": "1",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015766.1.12",
          "form": "진짜 돼지될거같아요ㅜㅠ",
          "original_form": "진짜 돼지될거같아요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015766.1.13",
          "form": "헉 ",
          "original_form": "헉 ",
          "speaker_id": "2",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015766.1.14",
          "form": "행복한 돼지가 꿈이에요",
          "original_form": "행복한 돼지가 꿈이에요",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015766.1.15",
          "form": "저두 글킨한데... 슬픈 돼지가 되어버릴까봐....",
          "original_form": "저두 글킨한데... 슬픈 돼지가 되어버릴까봐....",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015766.1.16",
          "form": "파바 샐러드나 먹을까해서 고민한건데",
          "original_form": "파바 샐러드나 먹을까해서 고민한건데",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015766.1.17",
          "form": "아악 그게 바로 저네요..ㅠㅠ",
          "original_form": "아악 그게 바로 저네요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015766.1.18",
          "form": "헉 오타입이다ㅜㅜ",
          "original_form": "헉 오타입이다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015766.1.19",
          "form": "파바 샐러드 맛나나요??",
          "original_form": "파바 샐러드 맛나나요??",
          "speaker_id": "2",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100015766.1.20",
          "form": "네네 비싼데 양많고 맛있어요!ㅋㅋ요기요에서 자주 할인해서",
          "original_form": "네네 비싼데 양많고 맛있어요!ㅋㅋ요기요에서 자주 할인해서",
          "speaker_id": "1",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100015766.1.21",
          "form": "만원이상사면 오천원 할인해주거든요",
          "original_form": "만원이상사면 오천원 할인해주거든요",
          "speaker_id": "1",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100015766.1.22",
          "form": "슈퍼클럽까지하면 2천원이라.... 고민중입니다..",
          "original_form": "슈퍼클럽까지하면 2천원이라.... 고민중입니다..",
          "speaker_id": "1",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100015766.1.23",
          "form": "아 요즘 하더라구요 ㅋㅋㅋ",
          "original_form": "아 요즘 하더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:57"
        }
      ]
    }
