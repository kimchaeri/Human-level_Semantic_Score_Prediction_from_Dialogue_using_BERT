      "utterance": [
        {
          "id": "MDRW2100028602.1.1",
          "form": "다이어트 잘 돼가?",
          "original_form": "다이어트 잘 돼가?",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028602.1.2",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028602.1.3",
          "form": "너무 힘들어",
          "original_form": "너무 힘들어",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028602.1.4",
          "form": "내가 추천한 팔뚝 영상은 하고 있겠지?",
          "original_form": "내가 추천한 팔뚝 영상은 하고 있겠지?",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028602.1.5",
          "form": "당연",
          "original_form": "당연",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028602.1.6",
          "form": "하지만 더 먹어",
          "original_form": "하지만 더 먹어",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028602.1.7",
          "form": "그거라도 하는게 어디야 저녁을 그럼 굶지말고 반만 먹으렴",
          "original_form": "그거라도 하는게 어디야 저녁을 그럼 굶지말고 반만 먹으렴",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028602.1.8",
          "form": "그래 너는 다이어트 어때?",
          "original_form": "그래 너는 다이어트 어때?",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028602.1.9",
          "form": "난 운동은 꾸준히 하는데 빠지진 않아",
          "original_form": "난 운동은 꾸준히 하는데 빠지진 않아",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028602.1.10",
          "form": "눈바디로는 좀 빠진거 같아",
          "original_form": "눈바디로는 좀 빠진거 같아",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028602.1.11",
          "form": "헬스다녀",
          "original_form": "헬스다녀",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028602.1.12",
          "form": "나도 돈 모아서 다녀야겠어",
          "original_form": "나도 돈 모아서 다녀야겠어",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028602.1.13",
          "form": "할부로 긁고 갚으면 안되니?",
          "original_form": "할부로 긁고 갚으면 안되니?",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028602.1.14",
          "form": "Sm휘트니스 없어졌어?",
          "original_form": "Sm휘트니스 없어졌어?",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028602.1.15",
          "form": "요즘 문자가 안와",
          "original_form": "요즘 문자가 안와",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028602.1.16",
          "form": "아직 운영할걸?",
          "original_form": "아직 운영할걸?",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028602.1.17",
          "form": "그래?  근데 거긴 너무 고인물파티야",
          "original_form": "그래?  근데 거긴 너무 고인물파티야",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028602.1.18",
          "form": "근데 더 멀리 가긴 싫어",
          "original_form": "근데 더 멀리 가긴 싫어",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028602.1.19",
          "form": "태전동 헬스장",
          "original_form": "태전동 헬스장",
          "speaker_id": "1",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028602.1.20",
          "form": "광주에서 제일 크다",
          "original_form": "광주에서 제일 크다",
