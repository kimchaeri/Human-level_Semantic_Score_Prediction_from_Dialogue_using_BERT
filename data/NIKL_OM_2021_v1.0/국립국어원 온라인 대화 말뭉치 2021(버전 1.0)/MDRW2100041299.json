      "utterance": [
        {
          "id": "MDRW2100041299.1.1",
          "form": "그럼 지금은 동생이랑 같이 안사는 건가요?",
          "original_form": "그럼 지금은 동생이랑 같이 안사는 건가요?",
          "speaker_id": "1",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100041299.1.2",
          "form": "네 걔가 지방에서 일해서",
          "original_form": "네 걔가 지방에서 일해서",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041299.1.3",
          "form": "사실 어머니 아프시니까 그나마 오는거지 아니었으면 명절때나 보지않았을까 싶어요",
          "original_form": "사실 어머니 아프시니까 그나마 오는거지 아니었으면 명절때나 보지않았을까 싶어요",
          "speaker_id": "2",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100041299.1.4",
          "form": "차로 한두시간 걸리거든요",
          "original_form": "차로 한두시간 걸리거든요",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100041299.1.5",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041299.1.6",
          "form": "name2님이 어머니랑 같이 살고 계시는 거군요",
          "original_form": "&name2&님이 어머니랑 같이 살고 계시는 거군요",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041299.1.7",
          "form": "어머니 건강이 안좋으시나봐요",
          "original_form": "어머니 건강이 안좋으시나봐요",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041299.1.8",
          "form": "name2님이 수고가 많겠네요",
          "original_form": "&name2&님이 수고가 많겠네요",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100041299.1.9",
          "form": "음... 암투병중이세요",
          "original_form": "음... 암투병중이세요",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100041299.1.10",
          "form": "저런.. 제가 몇년전에 암수술 했거든요",
          "original_form": "저런.. 제가 몇년전에 암수술 했거든요",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100041299.1.11",
          "form": "항암치료는 하고 계신 건가요?",
          "original_form": "항암치료는 하고 계신 건가요?",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100041299.1.12",
          "form": "네, 좀 겨워하시긴한데",
          "original_form": "네, 좀 겨워하시긴한데",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041299.1.13",
          "form": "계속 치료를 받고 있습니다",
          "original_form": "계속 치료를 받고 있습니다",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041299.1.14",
          "form": "저도 8번 항암치료 받으러 다녔는데 정말 힘들었어요",
          "original_form": "저도 8번 항암치료 받으러 다녔는데 정말 힘들었어요",
          "speaker_id": "1",
          "time": "20210906 06:00"
        },
        {
          "id": "MDRW2100041299.1.15",
          "form": "아이고... 고생많으셨어요",
          "original_form": "아이고... 고생많으셨어요",
          "speaker_id": "2",
          "time": "20210906 08:13"
        },
        {
          "id": "MDRW2100041299.1.16",
          "form": "지금은 괜찮으세요?",
          "original_form": "지금은 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210906 08:13"
        },
        {
          "id": "MDRW2100041299.1.17",
          "form": "17년도에 수술 받고 18년도에 항암하고",
          "original_form": "17년도에 수술 받고 18년도에 항암하고",
          "speaker_id": "1",
          "time": "20210906 08:36"
        },
        {
          "id": "MDRW2100041299.1.18",
          "form": "그 뒤로 추적검사 하고 있는데 아직까진 예후가 좋다하더라구요",
          "original_form": "그 뒤로 추적검사 하고 있는데 아직까진 예후가 좋다하더라구요",
          "speaker_id": "1",
          "time": "20210906 08:36"
        },
        {
          "id": "MDRW2100041299.1.19",
          "form": "다음달에 또 병원 가서 검사 받아야 해요",
          "original_form": "다음달에 또 병원 가서 검사 받아야 해요",
          "speaker_id": "1",
          "time": "20210906 08:36"
        },
        {
          "id": "MDRW2100041299.1.20",
          "form": "글쿤요. 5년동안 괜찮으면 완치판정이죠",
          "original_form": "글쿤요. 5년동안 괜찮으면 완치판정이죠",
          "speaker_id": "2",
          "time": "20210906 08:44"
        },
        {
          "id": "MDRW2100041299.1.21",
          "form": "그 완치판정 받아도 검사는 꾸준히 받으세요...",
          "original_form": "그 완치판정 받아도 검사는 꾸준히 받으세요...",
          "speaker_id": "2",
          "time": "20210906 08:44"
        },
        {
          "id": "MDRW2100041299.1.22",
          "form": "네 그럴려구요",
          "original_form": "네 그럴려구요",
          "speaker_id": "1",
          "time": "20210906 08:45"
        },
        {
          "id": "MDRW2100041299.1.23",
          "form": "어머니는 치료 시작한지 얼마나 되신건가요?",
          "original_form": "어머니는 치료 시작한지 얼마나 되신건가요?",
          "speaker_id": "1",
          "time": "20210906 08:45"
        },
        {
          "id": "MDRW2100041299.1.24",
          "form": "저희어머니 유방암 완치 받았는데 지금 다시 위암...",
          "original_form": "저희어머니 유방암 완치 받았는데 지금 다시 위암...",
          "speaker_id": "2",
          "time": "20210906 08:45"
        },
        {
          "id": "MDRW2100041299.1.25",
          "form": "아이고 재발이군요",
          "original_form": "아이고 재발이군요",
          "speaker_id": "1",
          "time": "20210906 08:45"
        },
        {
          "id": "MDRW2100041299.1.26",
          "form": "내시경 꼭 받으셔요...",
          "original_form": "내시경 꼭 받으셔요...",
          "speaker_id": "2",
          "time": "20210906 08:45"
        },
        {
          "id": "MDRW2100041299.1.27",
          "form": "재발이라고 해야하나...",
          "original_form": "재발이라고 해야하나...",
          "speaker_id": "2",
          "time": "20210906 08:45"
        },
        {
          "id": "MDRW2100041299.1.28",
          "form": "네 저도 위암이어서 내시경 받고 있어요",
          "original_form": "네 저도 위암이어서 내시경 받고 있어요",
          "speaker_id": "1",
          "time": "20210906 08:45"
        },
        {
          "id": "MDRW2100041299.1.29",
          "form": "전이암은 아니고",
          "original_form": "전이암은 아니고",
          "speaker_id": "2",
          "time": "20210906 08:45"
        }
      ]
    }
