      "utterance": [
        {
          "id": "MDRW2100010676.1.1",
          "form": "곰표 맥주 보신 적 있나요?",
          "original_form": "곰표 맥주 보신 적 있나요?",
          "speaker_id": "1",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010676.1.2",
          "form": "아니요 주류코너에 잘 안가요. 그냥 식당에 있는 술을 먹어요.",
          "original_form": "아니요 주류코너에 잘 안가요. 그냥 식당에 있는 술을 먹어요.",
          "speaker_id": "2",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010676.1.3",
          "form": "전 사실 술을 안 먹습니다!!",
          "original_form": "전 사실 술을 안 먹습니다!!",
          "speaker_id": "1",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010676.1.4",
          "form": "곰표 맥주도 포장지만 봤어요ㅋㅋㅋ",
          "original_form": "곰표 맥주도 포장지만 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010676.1.5",
          "form": "그럼 맥주 말고 곰표 팝콘이나 나쵸는 드셔보셨나요?",
          "original_form": "그럼 맥주 말고 곰표 팝콘이나 나쵸는 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010676.1.6",
          "form": "식당맥주는 피쳐잔에 나오잖아요",
          "original_form": "식당맥주는 피쳐잔에 나오잖아요",
          "speaker_id": "2",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100010676.1.7",
          "form": "로고를 볼일이 없어요",
          "original_form": "로고를 볼일이 없어요",
          "speaker_id": "2",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100010676.1.8",
          "form": "아뇨~",
          "original_form": "아뇨~",
          "speaker_id": "2",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100010676.1.9",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100010676.1.10",
          "form": "팝콘만 먹어봤어요.",
          "original_form": "팝콘만 먹어봤어요.",
          "speaker_id": "1",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100010676.1.11",
          "form": "생각보다 극장 팝콘 맛에 가까워 신기했습니다ㅎㅎ",
          "original_form": "생각보다 극장 팝콘 맛에 가까워 신기했습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:34"
        },
        {
          "id": "MDRW2100010676.1.12",
          "form": "카라멜 팝콘인가요?",
          "original_form": "카라멜 팝콘인가요?",
          "speaker_id": "2",
          "time": "20210823 20:34"
        },
        {
          "id": "MDRW2100010676.1.13",
          "form": "첨가물 없는 옥수수 팝콘 맛이예요.",
          "original_form": "첨가물 없는 옥수수 팝콘 맛이예요.",
          "speaker_id": "1",
          "time": "20210823 20:35"
        },
        {
          "id": "MDRW2100010676.1.14",
          "form": "적당히 짭짤해서 좋았습니다ㅎ",
          "original_form": "적당히 짭짤해서 좋았습니다ㅎ",
          "speaker_id": "1",
          "time": "20210823 20:35"
        },
        {
          "id": "MDRW2100010676.1.15",
          "form": "아 그렇구나..코로나때문에 먹어본지도 한참 됐어요..",
          "original_form": "아 그렇구나..코로나때문에 먹어본지도 한참 됐어요..",
          "speaker_id": "2",
          "time": "20210823 20:35"
        },
        {
          "id": "MDRW2100010676.1.16",
          "form": "진로 두꺼비 모델이 아마 샤이니였을 거에요",
          "original_form": "진로 두꺼비 모델이 아마 샤이니였을 거에요",
          "speaker_id": "2",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010676.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ TV가 없으니 광고를 볼 일이 없어서...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ TV가 없으니 광고를 볼 일이 없어서...",
          "speaker_id": "1",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010676.1.18",
          "form": "샤이니가 광고를 했군요.",
          "original_form": "샤이니가 광고를 했군요.",
          "speaker_id": "1",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010676.1.19",
          "form": "종현이 죽은 뒤니 최근이었던 것 같은데",
          "original_form": "종현이 죽은 뒤니 최근이었던 것 같은데",
          "speaker_id": "2",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010676.1.20",
          "form": "개인 멤버가 아니라",
          "original_form": "개인 멤버가 아니라",
          "speaker_id": "1",
          "time": "20210823 20:38"
        },
        {
          "id": "MDRW2100010676.1.21",
          "form": "네 명 다 부른 건가요?",
          "original_form": "네 명 다 부른 건가요?",
          "speaker_id": "1",
          "time": "20210823 20:38"
        },
        {
          "id": "MDRW2100010676.1.22",
          "form": "네 다 모델일걸요? 저도 잘 몰라요.",
          "original_form": "네 다 모델일걸요? 저도 잘 몰라요.",
          "speaker_id": "2",
          "time": "20210823 20:38"
        },
        {
          "id": "MDRW2100010676.1.23",
          "form": "곰표 콜라보는 맥주 팝콘을 봤고",
          "original_form": "곰표 콜라보는 맥주 팝콘을 봤고",
          "speaker_id": "1",
          "time": "20210823 20:39"
        },
        {
          "id": "MDRW2100010676.1.24",
          "form": "진로는... 컵라면 상품만 본 거 같아요.",
          "original_form": "진로는... 컵라면 상품만 본 거 같아요.",
          "speaker_id": "1",
          "time": "20210823 20:39"
        },
        {
          "id": "MDRW2100010676.1.25",
          "form": "티셔츠 같은 상품도 팔았어요.",
          "original_form": "티셔츠 같은 상품도 팔았어요.",
          "speaker_id": "2",
          "time": "20210823 20:40"
        }
      ]
