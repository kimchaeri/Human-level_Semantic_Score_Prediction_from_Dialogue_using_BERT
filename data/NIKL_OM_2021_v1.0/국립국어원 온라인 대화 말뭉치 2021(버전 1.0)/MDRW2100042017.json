      "utterance": [
        {
          "id": "MDRW2100042017.1.1",
          "form": "ㅠㅠ아침을 조금이라도 드세요!! ㅠㅠㅠ",
          "original_form": "ㅠㅠ아침을 조금이라도 드세요!! ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100042017.1.2",
          "form": "빵같은거라도!!",
          "original_form": "빵같은거라도!!",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100042017.1.3",
          "form": "그렇게할려구요ㅜㅜ",
          "original_form": "그렇게할려구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042017.1.4",
          "form": "아침에아무것도안먹으니깐",
          "original_form": "아침에아무것도안먹으니깐",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042017.1.5",
          "form": "힘이없더라구요",
          "original_form": "힘이없더라구요",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042017.1.6",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042017.1.7",
          "form": "맞아요 ㅠㅠㅠ 진짜 힘들어요 ㅠㅠㅠ 우유라도 꼭 한잔하세요!!!",
          "original_form": "맞아요 ㅠㅠㅠ 진짜 힘들어요 ㅠㅠㅠ 우유라도 꼭 한잔하세요!!!",
          "speaker_id": "2",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100042017.1.8",
          "form": "계속 참다가",
          "original_form": "계속 참다가",
          "speaker_id": "1",
          "time": "20210907 06:52"
        },
        {
          "id": "MDRW2100042017.1.9",
          "form": "샌드위치를샀어요",
          "original_form": "샌드위치를샀어요",
          "speaker_id": "1",
          "time": "20210907 06:52"
        },
        {
          "id": "MDRW2100042017.1.10",
          "form": "맛있어영",
          "original_form": "맛있어영",
          "speaker_id": "1",
          "time": "20210907 06:52"
        },
        {
          "id": "MDRW2100042017.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 06:52"
        },
        {
          "id": "MDRW2100042017.1.12",
          "form": "굿굿! 오늘부터 아침드시는군요! ㅋㅋ",
          "original_form": "굿굿! 오늘부터 아침드시는군요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100042017.1.13",
          "form": "실천하는 모습이 아주 좋습니다! ㅋㅋ",
          "original_form": "실천하는 모습이 아주 좋습니다! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100042017.1.14",
          "form": "낼은 아침머먹지ㅋㅋㅋ",
          "original_form": "낼은 아침머먹지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:02"
        },
        {
          "id": "MDRW2100042017.1.15",
          "form": "오늘 저녁도 머먹지ㄲㅋㅋ",
          "original_form": "오늘 저녁도 머먹지ㄲㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:02"
        },
        {
          "id": "MDRW2100042017.1.16",
          "form": "먹는게 남는거죠 ㅋㅋㅋㅋ",
          "original_form": "먹는게 남는거죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100042017.1.17",
          "form": "맞아요!!!제가 그때얘기했자나요ㅋㅋㅋㅇ",
          "original_form": "맞아요!!!제가 그때얘기했자나요ㅋㅋㅋㅇ",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100042017.1.18",
          "form": "먹는게 남는거라고",
          "original_form": "먹는게 남는거라고",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100042017.1.19",
          "form": "ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100042017.1.20",
          "form": "먹기위해서 살기 때문메",
          "original_form": "먹기위해서 살기 때문메",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100042017.1.21",
          "form": "ㅋㅋㅋ오늘 저녁은 뭘 드실건가요",
          "original_form": "ㅋㅋㅋ오늘 저녁은 뭘 드실건가요",
          "speaker_id": "2",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100042017.1.22",
          "form": "어제 먹다 남우 홈플치킨 먹을 가능성이있네요",
          "original_form": "어제 먹다 남우 홈플치킨 먹을 가능성이있네요",
          "speaker_id": "1",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100042017.1.23",
          "form": "집에가는길에 던킨들러서 음료먹고있어요",
          "original_form": "집에가는길에 던킨들러서 음료먹고있어요",
          "speaker_id": "1",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100042017.1.24",
          "form": "ㅋㅋㅋ저도 어제 먹다 남은 곱전...",
          "original_form": "ㅋㅋㅋ저도 어제 먹다 남은 곱전...",
          "speaker_id": "2",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100042017.1.25",
          "form": "모두의 마블 이벤트 하셨나요?ㅋㅋ",
          "original_form": "모두의 마블 이벤트 하셨나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100042017.1.26",
          "form": "앜ㅋㅋㄱ맞아요ㅋㅋㅋ 아는구나",
          "original_form": "앜ㅋㅋㄱ맞아요ㅋㅋㅋ 아는구나",
          "speaker_id": "1",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100042017.1.27",
          "form": "처음으로당첨됐어요ㅋㄱㅋ",
          "original_form": "처음으로당첨됐어요ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100042017.1.28",
          "form": "무료로주더라구용",
          "original_form": "무료로주더라구용",
          "speaker_id": "1",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100042017.1.29",
          "form": "이벤트좋아해욕ㅋㅋㄱㄱ",
          "original_form": "이벤트좋아해욕ㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100042017.1.30",
          "form": "ㅋㅋㅋ맞아요 전 주위에 던킨이 없어서 친구 줬어욬ㅋㅋ",
          "original_form": "ㅋㅋㅋ맞아요 전 주위에 던킨이 없어서 친구 줬어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100042017.1.31",
          "form": "그랬더니 친구는 배스킨 모찌 쿠폰 받았다고 주더라구요 ㅋㅋ",
          "original_form": "그랬더니 친구는 배스킨 모찌 쿠폰 받았다고 주더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100042017.1.32",
          "form": "오홍ㅋㅋㅋ교환했구낭",
          "original_form": "오홍ㅋㅋㅋ교환했구낭",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100042017.1.33",
          "form": "저 처음으로 당첨 된거라 바꾸니깐 좋더라구용ㅅ",
          "original_form": "저 처음으로 당첨 된거라 바꾸니깐 좋더라구용ㅅ",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100042017.1.34",
          "form": "혹시나했는데 무료라서 좋았어요",
          "original_form": "혹시나했는데 무료라서 좋았어요",
          "speaker_id": "1",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100042017.1.35",
          "form": "굽네치킨도 먹어봐야져!!",
          "original_form": "굽네치킨도 먹어봐야져!!",
          "speaker_id": "1",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100042017.1.36",
          "form": "운이좋았으면좋겠어요",
          "original_form": "운이좋았으면좋겠어요",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100042017.1.37",
          "form": "그래야 당첨이 잘되니깐요",
          "original_form": "그래야 당첨이 잘되니깐요",
