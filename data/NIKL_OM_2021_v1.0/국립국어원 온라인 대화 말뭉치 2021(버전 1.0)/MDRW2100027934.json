      "utterance": [
        {
          "id": "MDRW2100027934.1.1",
          "form": "이거 어때",
          "original_form": "이거 어때",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027934.1.2",
          "form": "너무좋다",
          "original_form": "너무좋다",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027934.1.3",
          "form": "코카콜라 제로에 얼음넣어서 원샷하고싶어",
          "original_form": "코카콜라 제로에 얼음넣어서 원샷하고싶어",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027934.1.4",
          "form": "나한테 맛집 추천해줄래?",
          "original_form": "나한테 맛집 추천해줄래?",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027934.1.5",
          "form": "왜 제로야 콜라는 오리지날로 먹어야지",
          "original_form": "왜 제로야 콜라는 오리지날로 먹어야지",
          "speaker_id": "1",
          "time": "20210902 16:14"
        },
        {
          "id": "MDRW2100027934.1.6",
          "form": "야탑에 수타우동겐 정말 맛있어",
          "original_form": "야탑에 수타우동겐 정말 맛있어",
          "speaker_id": "2",
          "time": "20210902 16:14"
        },
        {
          "id": "MDRW2100027934.1.7",
          "form": "이제 당 생각할 나이잖아",
          "original_form": "이제 당 생각할 나이잖아",
          "speaker_id": "2",
          "time": "20210902 16:14"
        },
        {
          "id": "MDRW2100027934.1.8",
          "form": "수타우동겐 메뉴가 뭐야?",
          "original_form": "수타우동겐 메뉴가 뭐야?",
          "speaker_id": "1",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100027934.1.9",
          "form": "크림우동이 정말 맛있어",
          "original_form": "크림우동이 정말 맛있어",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027934.1.10",
          "form": "크림우동 면빨이 끝내준단다",
          "original_form": "크림우동 면빨이 끝내준단다",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027934.1.11",
          "form": "크림우동 맛있겠다",
          "original_form": "크림우동 맛있겠다",
          "speaker_id": "1",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027934.1.12",
          "form": "나도 식당 추천 !남양주 피맥컴퍼니",
          "original_form": "나도 식당 추천 !남양주 피맥컴퍼니",
          "speaker_id": "1",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100027934.1.13",
          "form": "강가보면서 피맥먹으면 아주 맛있단다",
          "original_form": "강가보면서 피맥먹으면 아주 맛있단다",
          "speaker_id": "1",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100027934.1.14",
          "form": "오 피맥컴퍼니 서현에 있는건 가봤는데 체인점이야?",
          "original_form": "오 피맥컴퍼니 서현에 있는건 가봤는데 체인점이야?",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027934.1.15",
          "form": "피자엔 맥주지",
          "original_form": "피자엔 맥주지",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027934.1.16",
          "form": "체인점인지는 모르겠어",
          "original_form": "체인점인지는 모르겠어",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027934.1.17",
          "form": "주말에 사람 많아서 웨이팅 있음",
          "original_form": "주말에 사람 많아서 웨이팅 있음",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027934.1.18",
          "form": "무슨 피자 종류 먹었어?",
          "original_form": "무슨 피자 종류 먹었어?",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027934.1.19",
          "form": "페퍼로니피자!",
          "original_form": "페퍼로니피자!",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027934.1.20",
          "form": "페퍼로니 피자에 갈릭소스 찍어먹으면 정말 천국이겠다",
          "original_form": "페퍼로니 피자에 갈릭소스 찍어먹으면 정말 천국이겠다",
          "speaker_id": "2",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027934.1.21",
          "form": "맞아맞아 최근에 먹었던 거중에 제일 맛있었던 음식은 뭐야?",
          "original_form": "맞아맞아 최근에 먹었던 거중에 제일 맛있었던 음식은 뭐야?",
          "speaker_id": "1",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100027934.1.22",
          "form": "야탑 이탈리안식당에서 먹은 크림리조또",
          "original_form": "야탑 이탈리안식당에서 먹은 크림리조또",
          "speaker_id": "2",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027934.1.23",
          "form": "크림이 찐덕하고 맛있었어",
          "original_form": "크림이 찐덕하고 맛있었어",
          "speaker_id": "2",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027934.1.24",
          "form": "크림종류 좋아하는구나",
          "original_form": "크림종류 좋아하는구나",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027934.1.25",
          "form": "오늘 점심 메뉴는?",
          "original_form": "오늘 점심 메뉴는?",
          "speaker_id": "2",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027934.1.26",
          "form": "오늘 돈까스먹었어",
          "original_form": "오늘 돈까스먹었어",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027934.1.27",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027934.1.28",
          "form": "헐 나도 돈가스 먹었어 통했네 ",
          "original_form": "헐 나도 돈가스 먹었어 통했네 ",
          "speaker_id": "2",
          "time": "20210902 16:31"
        }
      ]
    }
