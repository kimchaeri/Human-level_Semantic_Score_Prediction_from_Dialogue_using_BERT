      "utterance": [
        {
          "id": "MDRW2100015373.1.1",
          "form": "요즘 어떤 부분 신경쓰세요?",
          "original_form": "요즘 어떤 부분 신경쓰세요?",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015373.1.2",
          "form": "몸의 부분이요?",
          "original_form": "몸의 부분이요?",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015373.1.3",
          "form": "전  마스크때문에  기초 화장네 더 공들이네요",
          "original_form": "전  마스크때문에  기초 화장네 더 공들이네요",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015373.1.4",
          "form": "미용 건강쪽으로요 ㅎㅎ",
          "original_form": "미용 건강쪽으로요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015373.1.5",
          "form": "오히려 안하게 되지 안나요?",
          "original_form": "오히려 안하게 되지 안나요?",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015373.1.6",
          "form": "전 파데 바르면 마스크랑 트러블 나서..",
          "original_form": "전 파데 바르면 마스크랑 트러블 나서..",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015373.1.7",
          "form": "저도 기초만요",
          "original_form": "저도 기초만요",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015373.1.8",
          "form": "기초이상 바르면  트러블이 ㅜㅜㅋ",
          "original_form": "기초이상 바르면  트러블이 ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015373.1.9",
          "form": "저도요.. 마스크에 습기 때문인거 가기도하고..",
          "original_form": "저도요.. 마스크에 습기 때문인거 가기도하고..",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015373.1.10",
          "form": "마스크 닿는 분위로 여드름이 막 나요",
          "original_form": "마스크 닿는 분위로 여드름이 막 나요",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015373.1.11",
          "form": "저오 오돌도돌 슬퍼요",
          "original_form": "저오 오돌도돌 슬퍼요",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015373.1.12",
          "form": "얼렁 코로나 끊나야... \r\n시원하게 런닝해보고 싶네요..",
          "original_form": "얼렁 코로나 끊나야... \r\n시원하게 런닝해보고 싶네요..",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015373.1.13",
          "form": "저는 나의시랑 사우나 가고싶어요 ㅎㅎㅎ",
          "original_form": "저는 나의시랑 사우나 가고싶어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015373.1.14",
          "form": "나의신랑?",
          "original_form": "나의신랑?",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015373.1.15",
          "form": "사랑여 오타가 ㅋㅋ",
          "original_form": "사랑여 오타가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015373.1.16",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015373.1.17",
          "form": "사우나 좋아하세요? 닺답하지 않아요?",
          "original_form": "사우나 좋아하세요? 닺답하지 않아요?",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015373.1.18",
          "form": "저는 넘넘 좋아해요 ㅜㅁ ㅜ",
          "original_form": "저는 넘넘 좋아해요 ㅜㅁ ㅜ",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015373.1.19",
          "form": "하고나면 피부 안땡기나요?",
          "original_form": "하고나면 피부 안땡기나요?",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015373.1.20",
          "form": "네전 괜찮아요 ㅎㅎ",
          "original_form": "네전 괜찮아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:15"
        }
