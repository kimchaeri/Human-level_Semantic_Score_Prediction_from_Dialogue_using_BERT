      "utterance": [
        {
          "id": "MDRW2100034645.1.1",
          "form": "자취/독립 해보셨나요",
          "original_form": "자취/독립 해보셨나요",
          "speaker_id": "1",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100034645.1.2",
          "form": "결혼 전에요",
          "original_form": "결혼 전에요",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100034645.1.3",
          "form": "와 저는 그런 경험이 없어요",
          "original_form": "와 저는 그런 경험이 없어요",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100034645.1.4",
          "form": "ㅎㅎ 혼자 살아보고 싶어서 결혼도 늦어지고",
          "original_form": "ㅎㅎ 혼자 살아보고 싶어서 결혼도 늦어지고",
          "speaker_id": "2",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100034645.1.5",
          "form": "혼자 사는건 어땠나요?",
          "original_form": "혼자 사는건 어땠나요?",
          "speaker_id": "1",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100034645.1.6",
          "form": "잠만 잤어요 ㅋㅋㅋㅋ",
          "original_form": "잠만 잤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100034645.1.7",
          "form": "그러셨군요 자취/결혼 어떤 생활이..나으신가요?",
          "original_form": "그러셨군요 자취/결혼 어떤 생활이..나으신가요?",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100034645.1.8",
          "form": "ㅋㅋㅋㅋ 둘 다 흠 ㅋㅋ 부모님과 사는게 젤 좋았어요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 둘 다 흠 ㅋㅋ 부모님과 사는게 젤 좋았어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100034645.1.9",
          "form": "아 그런가요... 엄마밥 그립네요",
          "original_form": "아 그런가요... 엄마밥 그립네요",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100034645.1.10",
          "form": "ㅋㅋㅋ 멀리 사세요?",
          "original_form": "ㅋㅋㅋ 멀리 사세요?",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100034645.1.11",
          "form": "매우 가깝습니다..ㅋ 그래도 자주 식사를 하진 않아서..",
          "original_form": "매우 가깝습니다..ㅋ 그래도 자주 식사를 하진 않아서..",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100034645.1.12",
          "form": "아하 그럴수있죠 ㅋㅋ",
          "original_form": "아하 그럴수있죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100034645.1.13",
          "form": "생각나는 엄마반찬? 있나요?",
          "original_form": "생각나는 엄마반찬? 있나요?",
          "speaker_id": "1",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100034645.1.14",
          "form": "김치찌개 예술 ㅎ 전 매일 오셔서 ㅎㅎㅎ",
