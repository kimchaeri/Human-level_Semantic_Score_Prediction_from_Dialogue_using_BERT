      "utterance": [
        {
          "id": "MDRW2100046556.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211029 11:09"
        },
        {
          "id": "MDRW2100046556.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211029 11:13"
        },
        {
          "id": "MDRW2100046556.1.3",
          "form": "이번 주말에 일정 있으신가요?",
          "original_form": "이번 주말에 일정 있으신가요?",
          "speaker_id": "2",
          "time": "20211029 11:13"
        },
        {
          "id": "MDRW2100046556.1.4",
          "form": "술약속 이써뇨",
          "original_form": "술약속 이써뇨",
          "speaker_id": "1",
          "time": "20211029 11:13"
        },
        {
          "id": "MDRW2100046556.1.5",
          "form": "술 좋아하시나봐요 ㅎㅎ",
          "original_form": "술 좋아하시나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211029 11:14"
        },
        {
          "id": "MDRW2100046556.1.6",
          "form": "주말에 뭐하시나요?",
          "original_form": "주말에 뭐하시나요?",
          "speaker_id": "1",
          "time": "20211029 11:14"
        },
        {
          "id": "MDRW2100046556.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20211029 11:14"
        },
        {
          "id": "MDRW2100046556.1.8",
          "form": "술 너무 조아함",
          "original_form": "술 너무 조아함",
          "speaker_id": "1",
          "time": "20211029 11:14"
        },
        {
          "id": "MDRW2100046556.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20211029 11:14"
        },
        {
          "id": "MDRW2100046556.1.10",
          "form": "가장 즐기는 술은 어떤 술이셔요?",
          "original_form": "가장 즐기는 술은 어떤 술이셔요?",
          "speaker_id": "2",
          "time": "20211029 11:14"
        },
        {
          "id": "MDRW2100046556.1.11",
          "form": "전 쏘주요",
          "original_form": "전 쏘주요",
          "speaker_id": "1",
          "time": "20211029 11:14"
        },
        {
          "id": "MDRW2100046556.1.12",
          "form": "술 좋아하세요?",
          "original_form": "술 좋아하세요?",
          "speaker_id": "1",
          "time": "20211029 11:15"
        },
        {
          "id": "MDRW2100046556.1.13",
          "form": "저는 그렇게 즐기는 편은 아닌데",
          "original_form": "저는 그렇게 즐기는 편은 아닌데",
          "speaker_id": "2",
          "time": "20211029 11:15"
        },
        {
          "id": "MDRW2100046556.1.14",
          "form": "술자리는 좋아해요 ㅋㅋㅋ",
          "original_form": "술자리는 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211029 11:15"
        },
        {
          "id": "MDRW2100046556.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20211029 11:16"
        },
        {
          "id": "MDRW2100046556.1.16",
          "form": "그게 좋은거죠 뭐",
          "original_form": "그게 좋은거죠 뭐",
          "speaker_id": "1",
          "time": "20211029 11:16"
        },
        {
          "id": "MDRW2100046556.1.17",
          "form": "그런가요 ㅋㅋㅋㅋㅋ",
          "original_form": "그런가요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211029 11:16"
        },
        {
          "id": "MDRW2100046556.1.18",
          "form": "술 많이 마셔봤자",
          "original_form": "술 많이 마셔봤자",
          "speaker_id": "1",
          "time": "20211029 11:16"
        },
        {
          "id": "MDRW2100046556.1.19",
          "form": "몸만 안 좋아지니",
          "original_form": "몸만 안 좋아지니",
          "speaker_id": "1",
          "time": "20211029 11:16"
        },
        {
          "id": "MDRW2100046556.1.20",
          "form": "뭐든 적당히가 좋은 듯",
          "original_form": "뭐든 적당히가 좋은 듯",
          "speaker_id": "1",
          "time": "20211029 11:16"
        },
        {
          "id": "MDRW2100046556.1.21",
          "form": "저는 맛있는 술 좋아해요",
          "original_form": "저는 맛있는 술 좋아해요",
          "speaker_id": "2",
          "time": "20211029 11:16"
        },
        {
          "id": "MDRW2100046556.1.22",
          "form": "칵테일?",
          "original_form": "칵테일?",
          "speaker_id": "1",
          "time": "20211029 11:16"
        },
        {
          "id": "MDRW2100046556.1.23",
          "form": "달달한 와인이나",
          "original_form": "달달한 와인이나",
          "speaker_id": "2",
          "time": "20211029 11:16"
        },
        {
          "id": "MDRW2100046556.1.24",
          "form": "네 그런거요!",
          "original_form": "네 그런거요!",
          "speaker_id": "2",
          "time": "20211029 11:16"
        },
        {
          "id": "MDRW2100046556.1.25",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20211029 11:17"
        },
        {
          "id": "MDRW2100046556.1.26",
          "form": "맛있죠",
          "original_form": "맛있죠",
