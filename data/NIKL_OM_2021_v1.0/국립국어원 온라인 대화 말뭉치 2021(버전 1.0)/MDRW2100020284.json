      "utterance": [
        {
          "id": "MDRW2100020284.1.1",
          "form": "헛 ㅋㅋㅋ 초기화 됐네요 ㅎ",
          "original_form": "헛 ㅋㅋㅋ 초기화 됐네요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020284.1.2",
          "form": "암튼 목튀김이 순살이라니",
          "original_form": "암튼 목튀김이 순살이라니",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020284.1.3",
          "form": "네 뼈를 발라내고 목살만 튀긴거래요",
          "original_form": "네 뼈를 발라내고 목살만 튀긴거래요",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020284.1.4",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020284.1.5",
          "form": "네 먹기도 편하고 맛잇어서 몇번 시켜먹엇어요",
          "original_form": "네 먹기도 편하고 맛잇어서 몇번 시켜먹엇어요",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020284.1.6",
          "form": "담에 꼭 시켜 먹어봐야것네요 ㅎ",
          "original_form": "담에 꼭 시켜 먹어봐야것네요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020284.1.7",
          "form": "패리카나 사이드 메뉴라고 하셨죠?",
          "original_form": "패리카나 사이드 메뉴라고 하셨죠?",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020284.1.8",
          "form": "네 추천해요 ㅋㅋ",
          "original_form": "네 추천해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020284.1.9",
          "form": "넵 감사합니다 ㅎㅎ",
          "original_form": "넵 감사합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020284.1.10",
          "form": "물론 점바점이겟지만",
          "original_form": "물론 점바점이겟지만",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020284.1.11",
          "form": "웬만하면 맛잇지 않을까요",
          "original_form": "웬만하면 맛잇지 않을까요",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020284.1.12",
          "form": "사이드는 안시키고 기본 후라이드만 시켜서",
          "original_form": "사이드는 안시키고 기본 후라이드만 시켜서",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020284.1.13",
          "form": "요즘 치킨 맛집 찾느라요 ㅜㅡ",
          "original_form": "요즘 치킨 맛집 찾느라요 ㅜㅡ",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020284.1.14",
          "form": "name1도 후라이드파시군요",
          "original_form": "&name1&도 후라이드파시군요",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020284.1.15",
          "form": "넵 ㅎ 기본 후라이드 시켜서 맛집이다 싶으면 거기서 이것저것 옆가지 치기를 ㅎㅎㅎㅎ",
          "original_form": "넵 ㅎ 기본 후라이드 시켜서 맛집이다 싶으면 거기서 이것저것 옆가지 치기를 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020284.1.16",
          "form": "치킨을 워낙 좋아해서요",
          "original_form": "치킨을 워낙 좋아해서요",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020284.1.17",
          "form": "아 중요하죠 ㅋㅋㅋㅋㅋ",
          "original_form": "아 중요하죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020284.1.18",
          "form": "기본을 잘해야 거기에 양념을 묻혀도 맛잇는거죠",
          "original_form": "기본을 잘해야 거기에 양념을 묻혀도 맛잇는거죠",
          "speaker_id": "2",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020284.1.19",
          "form": "글죠? ㅋㅋㅋㅋ",
          "original_form": "글죠? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020284.1.20",
          "form": "군데 제 주변은 그걸 이해 못해요 ㅜㅡ",
          "original_form": "군데 제 주변은 그걸 이해 못해요 ㅜㅡ",
          "speaker_id": "1",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020284.1.21",
          "form": "프렌차이즈 치킨점도 점바점인거 아시죠?",
          "original_form": "프렌차이즈 치킨점도 점바점인거 아시죠?",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020284.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ전 강경후라이드파예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전 강경후라이드파예요",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020284.1.23",
          "form": "가지치기도 잘 안하는 후라이드외길인생",
          "original_form": "가지치기도 잘 안하는 후라이드외길인생",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020284.1.24",
          "form": "맞아요 점바점 심하죠 ㅠㅠ",
          "original_form": "맞아요 점바점 심하죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020284.1.25",
          "form": "헛 ㅋㅋㅋㅋ 진짜 후라이드 강경파시군요",
          "original_form": "헛 ㅋㅋㅋㅋ 진짜 후라이드 강경파시군요",
