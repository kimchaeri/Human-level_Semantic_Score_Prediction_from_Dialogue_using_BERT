      "utterance": [
        {
          "id": "MDRW2100014868.1.1",
          "form": "최근에 여행은 어디가셨나요??",
          "original_form": "최근에 여행은 어디가셨나요??",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014868.1.2",
          "form": "코로나 때문에 어딜간적이 없어요 ㅠ",
          "original_form": "코로나 때문에 어딜간적이 없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014868.1.3",
          "form": "근데 진짜 여행떠나고싶네요",
          "original_form": "근데 진짜 여행떠나고싶네요",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014868.1.4",
          "form": "어디가신곳 있으세요?",
          "original_form": "어디가신곳 있으세요?",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014868.1.5",
          "form": "저도 코로나 전에만 여행갓지",
          "original_form": "저도 코로나 전에만 여행갓지",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014868.1.6",
          "form": "그이후엔.간곳이없어요",
          "original_form": "그이후엔.간곳이없어요",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014868.1.7",
          "form": "좀 위험하긴하지만",
          "original_form": "좀 위험하긴하지만",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014868.1.8",
          "form": "지금 강릉 여행준비중이에요!",
          "original_form": "지금 강릉 여행준비중이에요!",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014868.1.9",
          "form": "언제까지 참고살아요 조심하며 떠나야죠",
          "original_form": "언제까지 참고살아요 조심하며 떠나야죠",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014868.1.10",
          "form": "강릉 부럽네요",
          "original_form": "강릉 부럽네요",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014868.1.11",
          "form": "바다가는건가요?",
          "original_form": "바다가는건가요?",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014868.1.12",
          "form": "넵 맞아요 사실은 별보러 가려고했는데",
          "original_form": "넵 맞아요 사실은 별보러 가려고했는데",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014868.1.13",
          "form": "뚜벅이들 뿐이라",
          "original_form": "뚜벅이들 뿐이라",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014868.1.14",
          "form": "바다만 보기로했어요 ㅎㅎ",
          "original_form": "바다만 보기로했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014868.1.15",
          "form": "오 낭만이 있네요",
          "original_form": "오 낭만이 있네요",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014868.1.16",
          "form": "친구들끼리 여럿이 가는건가요?",
          "original_form": "친구들끼리 여럿이 가는건가요?",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014868.1.17",
          "form": "단둘이 가는거에요!",
          "original_form": "단둘이 가는거에요!",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014868.1.18",
          "form": "동네친구랑 단둘이 갑니당",
          "original_form": "동네친구랑 단둘이 갑니당",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014868.1.19",
          "form": "와 친한친구면 진짜 재밌겠네요",
          "original_form": "와 친한친구면 진짜 재밌겠네요",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014868.1.20",
          "form": "강릉은 어디가 유명하죠?",
          "original_form": "강릉은 어디가 유명하죠?",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014868.1.21",
          "form": "바다? 음식?",
          "original_form": "바다? 음식?",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014868.1.22",
          "form": "사실 바다는 핑계고",
          "original_form": "사실 바다는 핑계고",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014868.1.23",
          "form": "음식을 먹기위해 떠납니다 ㅋㅋ",
          "original_form": "음식을 먹기위해 떠납니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014868.1.24",
          "form": "초당순두부가 유명하고",
          "original_form": "초당순두부가 유명하고",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014868.1.25",
          "form": "꼬막이나 물회도 유명해요!!",
          "original_form": "꼬막이나 물회도 유명해요!!",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014868.1.26",
          "form": "와 순두부 진짜 좋아해요",
          "original_form": "와 순두부 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014868.1.27",
          "form": "꼬막 물회도 맛있죠",
          "original_form": "꼬막 물회도 맛있죠",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014868.1.28",
          "form": "당일치기인가요??",
          "original_form": "당일치기인가요??",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014868.1.29",
          "form": "1박2일이에요",
          "original_form": "1박2일이에요",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014868.1.30",
          "form": "에어비앤비에서 숙소 잡았어요 ㅎㅎ",
          "original_form": "에어비앤비에서 숙소 잡았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014868.1.31",
          "form": "주말에 갔다오세요??",
          "original_form": "주말에 갔다오세요??",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014868.1.32",
          "form": "오 에어비앤비 좋죠",
          "original_form": "오 에어비앤비 좋죠",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014868.1.33",
          "form": "금요일에 월차내고 금토 가기로했어요!!",
          "original_form": "금요일에 월차내고 금토 가기로했어요!!",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014868.1.34",
          "form": "숙소값이 금토랑 토일이랑 많이 차이나더라구요",
          "original_form": "숙소값이 금토랑 토일이랑 많이 차이나더라구요",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014868.1.35",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014868.1.36",
          "form": "와 그렇군요 월차내고 그만큼 재미있을거에요 ㅋㅋㅋ",
          "original_form": "와 그렇군요 월차내고 그만큼 재미있을거에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014868.1.37",
          "form": "그래서 기대중이에요!!그전에 코로나 확진자가 많이 줄었으면 좋겠어요ㅠㅠ",
          "original_form": "그래서 기대중이에요!!그전에 코로나 확진자가 많이 줄었으면 좋겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:12"
        }
      ]
