      "utterance": [
        {
          "id": "MDRW2100044352.1.1",
          "form": "옛날에는",
          "original_form": "옛날에는",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044352.1.2",
          "form": "여적여 너무 흔했죠",
          "original_form": "여적여 너무 흔했죠",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044352.1.3",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044352.1.4",
          "form": "드라마 아내의유혹만해도",
          "original_form": "드라마 아내의유혹만해도",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044352.1.5",
          "form": "악녀 너무 싫다",
          "original_form": "악녀 너무 싫다",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044352.1.6",
          "form": "이랬는데",
          "original_form": "이랬는데",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044352.1.7",
          "form": "요즘 악녀",
          "original_form": "요즘 악녀",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.8",
          "form": "최고다",
          "original_form": "최고다",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.9",
          "form": "나랑결혼해요언니",
          "original_form": "나랑결혼해요언니",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.10",
          "form": "그러지않나요",
          "original_form": "그러지않나요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.11",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.12",
          "form": "후...",
          "original_form": "후...",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.13",
          "form": "잠깐 엄마가",
          "original_form": "잠깐 엄마가",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.14",
          "form": "불렀었어요",
          "original_form": "불렀었어요",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.15",
          "form": "이런데도 엄마는",
          "original_form": "이런데도 엄마는",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.16",
          "form": "네넹ㅋ",
          "original_form": "네넹ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.17",
          "form": "쿠큐큐ㅠ",
          "original_form": "쿠큐큐ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.18",
          "form": "결혼하라고 ㅎ",
          "original_form": "결혼하라고 ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.19",
          "form": "ㅇ앗",
          "original_form": "ㅇ앗",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.20",
          "form": "아니근데",
          "original_form": "아니근데",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.21",
          "form": "큐ㅠㅠㅠㅠㅠㅠ",
          "original_form": "큐ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.22",
          "form": "머리에 총 맞지 않은 이상",
          "original_form": "머리에 총 맞지 않은 이상",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044352.1.23",
          "form": "저는 남자 만날 일이ㅜ없어요",
          "original_form": "저는 남자 만날 일이ㅜ없어요",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.24",
          "form": "ㅋㅋㅋ큐큐ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋ큐큐ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.25",
          "form": "너무좋내요",
          "original_form": "너무좋내요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.26",
          "form": "네,,, 하튼 진짜",
          "original_form": "네,,, 하튼 진짜",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.27",
          "form": "좋은 여자만나서",
          "original_form": "좋은 여자만나서",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.28",
          "form": "같이살아도되죵",
          "original_form": "같이살아도되죵",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.29",
          "form": "근제 저는 남자가 빻은 소리하면",
          "original_form": "근제 저는 남자가 빻은 소리하면",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.30",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.31",
          "form": "진짜개무시하거나",
          "original_form": "진짜개무시하거나",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.32",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.33",
          "form": "ㅋㅋ큐ㅠㅠㅠ큐큐큐ㅠ",
          "original_form": "ㅋㅋ큐ㅠㅠㅠ큐큐큐ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.34",
          "form": "제가 진짜 돌아버리면",
          "original_form": "제가 진짜 돌아버리면",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.35",
          "form": "ㅋㅋ...ㅋㅋㅋ...",
          "original_form": "ㅋㅋ...ㅋㅋㅋ...",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.36",
          "form": "패기라도 하겠는데",
          "original_form": "패기라도 하겠는데",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.37",
          "form": "같은 여자가",
          "original_form": "같은 여자가",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.38",
          "form": "저를 내려다보거나 그러면",
          "original_form": "저를 내려다보거나 그러면",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.39",
          "form": "진짜 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.40",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044352.1.41",
          "form": "외상 - 내상 같아요",
          "original_form": "외상 - 내상 같아요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044352.1.42",
          "form": "이야...",
          "original_form": "이야...",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044352.1.43",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044352.1.44",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210907 22:26"
        }
      ]
