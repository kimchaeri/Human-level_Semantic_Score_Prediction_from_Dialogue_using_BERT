      "utterance": [
        {
          "id": "MDRW2100038489.1.1",
          "form": "이제뭐하노?",
          "original_form": "이제뭐하노?",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038489.1.2",
          "form": "저는 이제 열무비빔밥 먹고 ㅋㅋ쉬고있어요",
          "original_form": "저는 이제 열무비빔밥 먹고 ㅋㅋ쉬고있어요",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038489.1.3",
          "form": "내일은 또 못쉬는날이라서 ㅋㅋㅋ",
          "original_form": "내일은 또 못쉬는날이라서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038489.1.4",
          "form": "그래 내일은 일하네ㅇㅅㅎ",
          "original_form": "그래 내일은 일하네ㅇㅅㅎ",
          "speaker_id": "2",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100038489.1.5",
          "form": "일진은 다팔았다ㅋ",
          "original_form": "일진은 다팔았다ㅋ",
          "speaker_id": "2",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100038489.1.6",
          "form": "얍얍 내일은 일하죠ㅋ",
          "original_form": "얍얍 내일은 일하죠ㅋ",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100038489.1.7",
          "form": "일진가격아름답게 팔았어요?ㅋ",
          "original_form": "일진가격아름답게 팔았어요?ㅋ",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100038489.1.8",
          "form": "저도 다팔았슈ㅋ",
          "original_form": "저도 다팔았슈ㅋ",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100038489.1.9",
          "form": "언니도 고기파티예요?ㅋ",
          "original_form": "언니도 고기파티예요?ㅋ",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100038489.1.10",
          "form": "아름답지못했다ㅋㅋ",
          "original_form": "아름답지못했다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100038489.1.11",
          "form": "그래도 이정도면 만족ㅋ",
          "original_form": "그래도 이정도면 만족ㅋ",
          "speaker_id": "2",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100038489.1.12",
          "form": "오늘삼겹살 꿉꿉",
          "original_form": "오늘삼겹살 꿉꿉",
          "speaker_id": "2",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100038489.1.13",
          "form": "삼겹살먹었군요",
          "original_form": "삼겹살먹었군요",
          "speaker_id": "1",
          "time": "20210903 08:28"
        },
        {
          "id": "MDRW2100038489.1.14",
          "form": "저희는 소불고기랑 목살ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저희는 소불고기랑 목살ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:28"
        },
        {
          "id": "MDRW2100038489.1.15",
          "form": "고기파티했군",
          "original_form": "고기파티했군",
          "speaker_id": "2",
          "time": "20210903 08:43"
        },
        {
          "id": "MDRW2100038489.1.16",
          "form": "출근하겠네오늘",
          "original_form": "출근하겠네오늘",
          "speaker_id": "2",
          "time": "20210903 08:43"
        },
        {
          "id": "MDRW2100038489.1.17",
          "form": "비온다  지금ㅜ",
          "original_form": "비온다  지금ㅜ",
          "speaker_id": "2",
          "time": "20210903 08:43"
        },
        {
          "id": "MDRW2100038489.1.18",
          "form": "아하ㅜㅜ비오는구만요.ㅜㅜ",
          "original_form": "아하ㅜㅜ비오는구만요.ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100038489.1.19",
          "form": "지안이 우산안들고갔는데ㅜ",
          "original_form": "지안이 우산안들고갔는데ㅜ",
          "speaker_id": "1",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100038489.1.20",
          "form": "오후는좋데",
          "original_form": "오후는좋데",
          "speaker_id": "2",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100038489.1.21",
          "form": "다른지역은 쨍한가을이다",
          "original_form": "다른지역은 쨍한가을이다",
          "speaker_id": "2",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100038489.1.22",
          "form": "여기만비오네",
          "original_form": "여기만비오네",
          "speaker_id": "2",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100038489.1.23",
          "form": "여기도 비가 오네요",
          "original_form": "여기도 비가 오네요",
          "speaker_id": "1",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100038489.1.24",
          "form": "이제 일 마치고 집에 가는 길이에요",
          "original_form": "이제 일 마치고 집에 가는 길이에요",
          "speaker_id": "1",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100038489.1.25",
          "form": "배고프쟁?",
          "original_form": "배고프쟁?",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100038489.1.26",
          "form": "난커피에 군것질 했더만 ㅋㅋ",
          "original_form": "난커피에 군것질 했더만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100038489.1.27",
          "form": "네 완전ㅋㅋㅋ",
          "original_form": "네 완전ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100038489.1.28",
          "form": "ㅋㅍ",
          "original_form": "ㅋㅍ",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100038489.1.29",
          "form": "저도 커피만 마셨네유ㅜ",
          "original_form": "저도 커피만 마셨네유ㅜ",
          "speaker_id": "1",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100038489.1.30",
          "form": "배고픔ㅜ",
          "original_form": "배고픔ㅜ",
          "speaker_id": "1",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100038489.1.31",
          "form": "얼른집에가서 밥먹어라ㅜㅜ",
          "original_form": "얼른집에가서 밥먹어라ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 11:48"
        }
      ]
    }
