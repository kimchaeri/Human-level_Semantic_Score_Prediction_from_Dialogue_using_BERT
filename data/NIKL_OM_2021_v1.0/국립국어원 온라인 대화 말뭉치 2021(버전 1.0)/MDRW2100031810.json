      "utterance": [
        {
          "id": "MDRW2100031810.1.1",
          "form": "이시간에 야식 먹는다고? 살 쪄",
          "original_form": "이시간에 야식 먹는다고? 살 쪄",
          "speaker_id": "2",
          "time": "20210905 01:11"
        },
        {
          "id": "MDRW2100031810.1.2",
          "form": "괜찮음ㅋㅋ미국시간으로는 아침임ㅋㅋ",
          "original_form": "괜찮음ㅋㅋ미국시간으로는 아침임ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:12"
        },
        {
          "id": "MDRW2100031810.1.3",
          "form": "이건 또 무슨 신박한 멍멍이소리야ㅋㅋㅋ",
          "original_form": "이건 또 무슨 신박한 멍멍이소리야ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:12"
        },
        {
          "id": "MDRW2100031810.1.4",
          "form": "먹을생각에 너무 신나고있음ㅋㅋ",
          "original_form": "먹을생각에 너무 신나고있음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:13"
        },
        {
          "id": "MDRW2100031810.1.5",
          "form": "뭐 먹으려고?  설마 아까 사온 투썸케익?",
          "original_form": "뭐 먹으려고?  설마 아까 사온 투썸케익?",
          "speaker_id": "2",
          "time": "20210905 01:13"
        },
        {
          "id": "MDRW2100031810.1.6",
          "form": "응 당근케익하고 레몬무스 사봤음",
          "original_form": "응 당근케익하고 레몬무스 사봤음",
          "speaker_id": "1",
          "time": "20210905 01:13"
        },
        {
          "id": "MDRW2100031810.1.7",
          "form": "당근케익은 예전에 너랑 먹어봤고",
          "original_form": "당근케익은 예전에 너랑 먹어봤고",
          "speaker_id": "2",
          "time": "20210905 01:13"
        },
        {
          "id": "MDRW2100031810.1.8",
          "form": "레몬무스는 어때? 그것도 맛있어보이던데",
          "original_form": "레몬무스는 어때? 그것도 맛있어보이던데",
          "speaker_id": "2",
          "time": "20210905 01:14"
        },
        {
          "id": "MDRW2100031810.1.9",
          "form": "잠깐만 무스 꺼내보실게용",
          "original_form": "잠깐만 무스 꺼내보실게용",
          "speaker_id": "1",
          "time": "20210905 01:14"
        },
        {
          "id": "MDRW2100031810.1.10",
          "form": "기본은 하겠지ㅋ 당근케익도 그냥 저냥 먹을만했음",
          "original_form": "기본은 하겠지ㅋ 당근케익도 그냥 저냥 먹을만했음",
          "speaker_id": "2",
          "time": "20210905 01:15"
        },
        {
          "id": "MDRW2100031810.1.11",
          "form": "으 레몬무스 괜히 사왔어",
          "original_form": "으 레몬무스 괜히 사왔어",
          "speaker_id": "1",
          "time": "20210905 01:15"
        },
        {
          "id": "MDRW2100031810.1.12",
          "form": "내스타일 아니야...",
          "original_form": "내스타일 아니야...",
          "speaker_id": "1",
          "time": "20210905 01:15"
        },
        {
          "id": "MDRW2100031810.1.13",
          "form": "왜? 기본도 아니야?",
          "original_form": "왜? 기본도 아니야?",
          "speaker_id": "2",
          "time": "20210905 01:15"
        },
        {
          "id": "MDRW2100031810.1.14",
          "form": "응 그 위에 뭐라고하지 아이싱? 은 아니고 이름 기억 안나는데 느끼해",
          "original_form": "응 그 위에 뭐라고하지 아이싱? 은 아니고 이름 기억 안나는데 느끼해",
          "speaker_id": "1",
          "time": "20210905 01:16"
        },
        {
          "id": "MDRW2100031810.1.15",
          "form": "느끼해? 니가 무스 별로 안좋아해서 그런거아니고?",
          "original_form": "느끼해? 니가 무스 별로 안좋아해서 그런거아니고?",
          "speaker_id": "2",
          "time": "20210905 01:17"
        },
        {
          "id": "MDRW2100031810.1.16",
          "form": "그럴수도있는데 레몬맛도 별로 안강하고",
          "original_form": "그럴수도있는데 레몬맛도 별로 안강하고",
          "speaker_id": "1",
          "time": "20210905 01:17"
        },
        {
          "id": "MDRW2100031810.1.17",
          "form": "투썸에서 무스는 패스~~",
          "original_form": "투썸에서 무스는 패스~~",
          "speaker_id": "1",
          "time": "20210905 01:17"
        },
        {
          "id": "MDRW2100031810.1.18",
          "form": "그정도야? 그럼 나도 패스다",
          "original_form": "그정도야? 그럼 나도 패스다",
          "speaker_id": "2",
          "time": "20210905 01:18"
        },
        {
          "id": "MDRW2100031810.1.19",
          "form": "ㅋ응 당근케익이 제일 괜찮다!!  무난무난",
          "original_form": "ㅋ응 당근케익이 제일 괜찮다!!  무난무난",
          "speaker_id": "1",
          "time": "20210905 01:18"
        },
        {
          "id": "MDRW2100031810.1.20",
          "form": "저녁은 뭐 먹지?",
          "original_form": "저녁은 뭐 먹지?",
