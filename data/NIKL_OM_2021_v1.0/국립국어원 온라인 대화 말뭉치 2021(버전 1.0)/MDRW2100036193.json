      "utterance": [
        {
          "id": "MDRW2100036193.1.1",
          "form": "배달인가유?",
          "original_form": "배달인가유?",
          "speaker_id": "2",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100036193.1.2",
          "form": "아니유 엄마랑 드라이브스루해왔슈 ㅋㅋㅋ",
          "original_form": "아니유 엄마랑 드라이브스루해왔슈 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100036193.1.3",
          "form": "오 !!",
          "original_form": "오 !!",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036193.1.4",
          "form": "잘햇구만유",
          "original_form": "잘햇구만유",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036193.1.5",
          "form": "버거킹 ?",
          "original_form": "버거킹 ?",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036193.1.6",
          "form": "매장행사잇나유,",
          "original_form": "매장행사잇나유,",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036193.1.7",
          "form": "배부르당ㅋㅋㅋ응응!!",
          "original_form": "배부르당ㅋㅋㅋ응응!!",
          "speaker_id": "1",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036193.1.8",
          "form": "불고기와퍼 먹었앙ㅋㅋㅋ엄마가 맛있다해서",
          "original_form": "불고기와퍼 먹었앙ㅋㅋㅋ엄마가 맛있다해서",
          "speaker_id": "1",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036193.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036193.1.10",
          "form": "좋지유",
          "original_form": "좋지유",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036193.1.11",
          "form": "와 근데 넘 배불러 ㅋㅋㅋㅋ진짜 배터지겠슈",
          "original_form": "와 근데 넘 배불러 ㅋㅋㅋㅋ진짜 배터지겠슈",
          "speaker_id": "1",
          "time": "20210830 18:54"
        },
        {
          "id": "MDRW2100036193.1.12",
          "form": "하나가!?",
          "original_form": "하나가!?",
          "speaker_id": "2",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100036193.1.13",
          "form": "역시와퍼군 ㅋㅋㅋ!!!",
          "original_form": "역시와퍼군 ㅋㅋㅋ!!!",
          "speaker_id": "2",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100036193.1.14",
          "form": "응ㅋㅋㅋ진짜 주니어는 또 너무 작고 중간 크기 만들어주라!!!",
          "original_form": "응ㅋㅋㅋ진짜 주니어는 또 너무 작고 중간 크기 만들어주라!!!",
          "speaker_id": "1",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036193.1.15",
          "form": "ㅋㅋㅋㅋ급하게 먹은거아냐!?!",
          "original_form": "ㅋㅋㅋㅋ급하게 먹은거아냐!?!",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036193.1.16",
          "form": "근데또 금방꺼져ㅠㅠ",
          "original_form": "근데또 금방꺼져ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036193.1.17",
          "form": "배가 꺼지기 전 잠들어야해 ㅋㅋㅋ",
          "original_form": "배가 꺼지기 전 잠들어야해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036193.1.18",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036193.1.19",
          "form": "오늘 할일은 하셧는가유?",
          "original_form": "오늘 할일은 하셧는가유?",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036193.1.20",
          "form": "난 닭탕먹을꺼에요 알죠?",
          "original_form": "난 닭탕먹을꺼에요 알죠?",
          "speaker_id": "2",
          "time": "20210830 18:59"
