      "utterance": [
        {
          "id": "MDRW2100037698.1.1",
          "form": "전 최근에 유튜브보고 셀프미용을 했어요",
          "original_form": "전 최근에 유튜브보고 셀프미용을 했어요",
          "speaker_id": "1",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100037698.1.2",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100037698.1.3",
          "form": "뭘 주로보시나요",
          "original_form": "뭘 주로보시나요",
          "speaker_id": "2",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100037698.1.4",
          "form": "주로 안보는데",
          "original_form": "주로 안보는데",
          "speaker_id": "1",
          "time": "20210901 16:30"
        },
        {
          "id": "MDRW2100037698.1.5",
          "form": "환절기라 경각심이 드네요 요새 알게된 꿀팁알려주쇼",
          "original_form": "환절기라 경각심이 드네요 요새 알게된 꿀팁알려주쇼",
          "speaker_id": "2",
          "time": "20210901 16:43"
        },
        {
          "id": "MDRW2100037698.1.6",
          "form": "꿀?",
          "original_form": "꿀?",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100037698.1.7",
          "form": "물?",
          "original_form": "물?",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100037698.1.8",
          "form": "미지근한 물?",
          "original_form": "미지근한 물?",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100037698.1.9",
          "form": "유툽서 머 본거 없소",
          "original_form": "유툽서 머 본거 없소",
          "speaker_id": "2",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100037698.1.10",
          "form": "없어요..",
          "original_form": "없어요..",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100037698.1.11",
          "form": "위에 유툽보고 셀프미용 했담시요",
          "original_form": "위에 유툽보고 셀프미용 했담시요",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100037698.1.12",
          "form": "환절기는 안챙겨봤엉ᆢ",
          "original_form": "환절기는 안챙겨봤엉ᆢ",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100037698.1.13",
          "form": "해봤다는 셀프미용 어떤건가요?",
          "original_form": "해봤다는 셀프미용 어떤건가요?",
          "speaker_id": "2",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100037698.1.14",
          "form": "머리길이를 짧게하려고 했는데",
          "original_form": "머리길이를 짧게하려고 했는데",
          "speaker_id": "1",
          "time": "20210901 18:11"
        },
        {
          "id": "MDRW2100037698.1.15",
          "form": "샤기컷이에요",
          "original_form": "샤기컷이에요",
          "speaker_id": "1",
          "time": "20210901 18:11"
        },
        {
          "id": "MDRW2100037698.1.16",
          "form": "잘잘라졌는데",
          "original_form": "잘잘라졌는데",
          "speaker_id": "1",
          "time": "20210901 18:11"
        },
        {
          "id": "MDRW2100037698.1.17",
          "form": "아니 셀프로머리자름? 대단쓰",
          "original_form": "아니 셀프로머리자름? 대단쓰",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037698.1.18",
          "form": "긴머리라ㅋ",
          "original_form": "긴머리라ㅋ",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100037698.1.19",
          "form": "근데 백신2차맞고 바로  미용실가려구요ㅋㅋ",
