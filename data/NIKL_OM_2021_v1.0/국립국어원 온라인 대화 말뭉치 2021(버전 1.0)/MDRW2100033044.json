      "utterance": [
        {
          "id": "MDRW2100033044.1.1",
          "form": "아 넵넵",
          "original_form": "아 넵넵",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033044.1.2",
          "form": "장관이던가?하튼 누구요",
          "original_form": "장관이던가?하튼 누구요",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033044.1.3",
          "form": "아니 신기한게",
          "original_form": "아니 신기한게",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033044.1.4",
          "form": "지금 우리나라 윗사람들 다",
          "original_form": "지금 우리나라 윗사람들 다",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033044.1.5",
          "form": "공부 엄청 잘하고",
          "original_form": "공부 엄청 잘하고",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033044.1.6",
          "form": "잘 배운 사람들일텐데 어쩜 그리 일을 못하죠",
          "original_form": "잘 배운 사람들일텐데 어쩜 그리 일을 못하죠",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033044.1.7",
          "form": "그러면서 민생 챙긴다 어쩌구 그러고,,",
          "original_form": "그러면서 민생 챙긴다 어쩌구 그러고,,",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033044.1.8",
          "form": "개인정보위 데이터보호가 뭐죠",
          "original_form": "개인정보위 데이터보호가 뭐죠",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033044.1.9",
          "form": "데이터 보호 때문에 세금을 더 걷겠다는걸까요?!",
          "original_form": "데이터 보호 때문에 세금을 더 걷겠다는걸까요?!",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033044.1.10",
          "form": "검색해도 안나오네요",
          "original_form": "검색해도 안나오네요",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033044.1.11",
          "form": "아니면 예산만 더 편성하겠다는걸까요?",
          "original_form": "아니면 예산만 더 편성하겠다는걸까요?",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033044.1.12",
          "form": "예산편성인데 결국은",
          "original_form": "예산편성인데 결국은",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033044.1.13",
          "form": "세금과 연결되지않을까요",
          "original_form": "세금과 연결되지않을까요",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033044.1.14",
          "form": "세금도 더 걷으려나요?",
          "original_form": "세금도 더 걷으려나요?",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033044.1.15",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033044.1.16",
          "form": "아니 뭔 이 나라는",
          "original_form": "아니 뭔 이 나라는",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033044.1.17",
          "form": "세금은 오지게 걷네요",
          "original_form": "세금은 오지게 걷네요",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033044.1.18",
          "form": "당분간은 안걷으려나ㅋㅋㅋ",
          "original_form": "당분간은 안걷으려나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100033044.1.19",
          "form": "포퓰리즘이 너무 심해서 탈이에요",
          "original_form": "포퓰리즘이 너무 심해서 탈이에요",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100033044.1.20",
          "form": "데이터 보호 라는 것이",
          "original_form": "데이터 보호 라는 것이",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100033044.1.21",
          "form": "개인정보라는건지",
          "original_form": "개인정보라는건지",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100033044.1.22",
          "form": "당장 데이터라는 글자만 보면... 휴대폰 데이터 요금밖에 생각이 안나네요 ㅠㅠㅠㅋㅋㅋㅋ",
          "original_form": "당장 데이터라는 글자만 보면... 휴대폰 데이터 요금밖에 생각이 안나네요 ㅠㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100033044.1.23",
          "form": "ㅋㅋ저도요",
          "original_form": "ㅋㅋ저도요",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033044.1.24",
          "form": "무슨 데이터를 보호한다는거지",
          "original_form": "무슨 데이터를 보호한다는거지",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033044.1.25",
          "form": "개인정보 위원회에서",
          "original_form": "개인정보 위원회에서",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033044.1.26",
          "form": "뭔진 모르겠지만",
          "original_form": "뭔진 모르겠지만",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033044.1.27",
          "form": "뭔가 보호하려고 예산 더 받겠다는건데....",
          "original_form": "뭔가 보호하려고 예산 더 받겠다는건데....",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033044.1.28",
          "form": "지금 180석 때문에",
          "original_form": "지금 180석 때문에",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033044.1.29",
          "form": "지들 마음대로 뚱땅뚱땅 하고 패스하면...;",
          "original_form": "지들 마음대로 뚱땅뚱땅 하고 패스하면...;",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033044.1.30",
          "form": "안전하게 개인정보를 보관할수있도록 하는거래요",
          "original_form": "안전하게 개인정보를 보관할수있도록 하는거래요",
          "speaker_id": "1",
          "time": "20210906 14:35"
        }
      ]
