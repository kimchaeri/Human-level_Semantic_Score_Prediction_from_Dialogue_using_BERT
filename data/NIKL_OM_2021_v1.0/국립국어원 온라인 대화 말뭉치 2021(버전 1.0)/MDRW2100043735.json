      "utterance": [
        {
          "id": "MDRW2100043735.1.1",
          "form": "최명길은 단역은 없엇는듯",
          "original_form": "최명길은 단역은 없엇는듯",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043735.1.2",
          "form": "이영범나오고",
          "original_form": "이영범나오고",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043735.1.3",
          "form": "그러네요ㅋ 예뻐서 그렇겠죠",
          "original_form": "그러네요ㅋ 예뻐서 그렇겠죠",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043735.1.4",
          "form": "길용우, 전혜진도 나왔었네",
          "original_form": "길용우, 전혜진도 나왔었네",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043735.1.5",
          "form": "이영범 오래만에 들어보네",
          "original_form": "이영범 오래만에 들어보네",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043735.1.6",
          "form": "이쁘고 연기를 잘하니 여기저기 주연으로",
          "original_form": "이쁘고 연기를 잘하니 여기저기 주연으로",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043735.1.7",
          "form": "개그우먼 남편이었던ㅋ",
          "original_form": "개그우먼 남편이었던ㅋ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043735.1.8",
          "form": "같은해에 재회드라마주연",
          "original_form": "같은해에 재회드라마주연",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043735.1.9",
          "form": "와 진짜 끊임없이 나오네",
          "original_form": "와 진짜 끊임없이 나오네",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043735.1.10",
          "form": "ㅋㅋ출연진 한인수 김용건",
          "original_form": "ㅋㅋ출연진 한인수 김용건",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043735.1.11",
          "form": "한인수는 모르는 사람ㅋ",
          "original_form": "한인수는 모르는 사람ㅋ",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043735.1.12",
          "form": "그다음해에 결혼드라마",
          "original_form": "그다음해에 결혼드라마",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043735.1.13",
          "form": "결혼드라마는 머죠?",
          "original_form": "결혼드라마는 머죠?",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043735.1.14",
          "form": "이마인가 사마귀있으신 배우",
          "original_form": "이마인가 사마귀있으신 배우",
          "speaker_id": "2",
          "time": "20210907 20:11"
        }
