      "utterance": [
        {
          "id": "MDRW2100033504.1.1",
          "form": "어느부분이 재밌던가요",
          "original_form": "어느부분이 재밌던가요",
          "speaker_id": "1",
          "time": "20210824 08:18"
        },
        {
          "id": "MDRW2100033504.1.2",
          "form": "체격도 있으시지만?",
          "original_form": "체격도 있으시지만?",
          "speaker_id": "1",
          "time": "20210824 08:18"
        },
        {
          "id": "MDRW2100033504.1.3",
          "form": "키도 배도 있으시거든요  ㅋㅋ 브라운신부 역할 하시는 배우분이요",
          "original_form": "키도 배도 있으시거든요  ㅋㅋ 브라운신부 역할 하시는 배우분이요",
          "speaker_id": "2",
          "time": "20210824 08:18"
        },
        {
          "id": "MDRW2100033504.1.4",
          "form": "전 위즐리 아버님",
          "original_form": "전 위즐리 아버님",
          "speaker_id": "2",
          "time": "20210824 08:18"
        },
        {
          "id": "MDRW2100033504.1.5",
          "form": "헐 너무 신기하다",
          "original_form": "헐 너무 신기하다",
          "speaker_id": "1",
          "time": "20210824 08:18"
        },
        {
          "id": "MDRW2100033504.1.6",
          "form": "해리포터 아버님 정모",
          "original_form": "해리포터 아버님 정모",
          "speaker_id": "1",
          "time": "20210824 08:19"
        },
        {
          "id": "MDRW2100033504.1.7",
          "form": "보면서 반가우셨겠어요",
          "original_form": "보면서 반가우셨겠어요",
          "speaker_id": "1",
          "time": "20210824 08:19"
        },
        {
          "id": "MDRW2100033504.1.8",
          "form": "그래서 가끔 걱정도해요",
          "original_form": "그래서 가끔 걱정도해요",
          "speaker_id": "2",
          "time": "20210824 08:19"
        },
        {
          "id": "MDRW2100033504.1.9",
          "form": "저분 고지혈증과 고혈압때매",
          "original_form": "저분 고지혈증과 고혈압때매",
          "speaker_id": "2",
          "time": "20210824 08:19"
        },
        {
          "id": "MDRW2100033504.1.10",
          "form": "어떤 부분이요?",
          "original_form": "어떤 부분이요?",
          "speaker_id": "1",
          "time": "20210824 08:19"
        },
        {
          "id": "MDRW2100033504.1.11",
          "form": "아.. 건강..",
          "original_form": "아.. 건강..",
          "speaker_id": "1",
          "time": "20210824 08:19"
        },
        {
          "id": "MDRW2100033504.1.12",
          "form": "담시즌 안나오면 어떻해 ㅋㅋ",
          "original_form": "담시즌 안나오면 어떻해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:20"
        },
        {
          "id": "MDRW2100033504.1.13",
          "form": "배우 일 오래 해주셔야하는데",
          "original_form": "배우 일 오래 해주셔야하는데",
          "speaker_id": "1",
          "time": "20210824 08:20"
        },
        {
          "id": "MDRW2100033504.1.14",
          "form": "아니 진짜 건강 괜찮으신건가 ㅋㅋ",
          "original_form": "아니 진짜 건강 괜찮으신건가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:20"
        },
        {
          "id": "MDRW2100033504.1.15",
          "form": "별걸 다 걱정하네요 ㅋㅋㅋㅋㅋ드라마가 재미있으니",
          "original_form": "별걸 다 걱정하네요 ㅋㅋㅋㅋㅋ드라마가 재미있으니",
          "speaker_id": "2",
          "time": "20210824 08:20"
        },
        {
          "id": "MDRW2100033504.1.16",
          "form": "시즌에 배우가 바뀌거나 안나오면 약간 섭섭한것도 있고",
          "original_form": "시즌에 배우가 바뀌거나 안나오면 약간 섭섭한것도 있고",
          "speaker_id": "1",
          "time": "20210824 08:20"
        },
        {
          "id": "MDRW2100033504.1.17",
          "form": "그렇죠 아무래도",
          "original_form": "그렇죠 아무래도",
          "speaker_id": "1",
          "time": "20210824 08:20"
        },
        {
          "id": "MDRW2100033504.1.18",
          "form": "드라마의 가장 인상적인 점은 무엇이었나요",
          "original_form": "드라마의 가장 인상적인 점은 무엇이었나요",
          "speaker_id": "1",
          "time": "20210824 08:21"
        },
        {
          "id": "MDRW2100033504.1.19",
          "form": "음 혹시 csi보셨어요?",
          "original_form": "음 혹시 csi보셨어요?",
          "speaker_id": "2",
          "time": "20210824 08:22"
        },
        {
          "id": "MDRW2100033504.1.20",
          "form": "어.. 들어는 봤어요",
          "original_form": "어.. 들어는 봤어요",
          "speaker_id": "1",
          "time": "20210824 08:22"
        },
        {
          "id": "MDRW2100033504.1.21",
          "form": "그건 진짜 초과학수사거든요",
          "original_form": "그건 진짜 초과학수사거든요",
          "speaker_id": "2",
          "time": "20210824 08:23"
        },
        {
          "id": "MDRW2100033504.1.22",
          "form": "가끔 티비에서 하는거 지나가다 짧게 봐서 큰 내용은 잘 몰라요..",
          "original_form": "가끔 티비에서 하는거 지나가다 짧게 봐서 큰 내용은 잘 몰라요..",
          "speaker_id": "1",
          "time": "20210824 08:23"
        },
        {
          "id": "MDRW2100033504.1.23",
          "form": "지문에 dna 에",
          "original_form": "지문에 dna 에",
          "speaker_id": "2",
          "time": "20210824 08:23"
        },
        {
          "id": "MDRW2100033504.1.24",
          "form": "뭐 그런? 느낌 아시죵?",
          "original_form": "뭐 그런? 느낌 아시죵?",
          "speaker_id": "2",
          "time": "20210824 08:23"
        },
        {
          "id": "MDRW2100033504.1.25",
          "form": "으음~ 진짜 모든걸 탈탈털어 범인을 잡겠다",
          "original_form": "으음~ 진짜 모든걸 탈탈털어 범인을 잡겠다",
          "speaker_id": "1",
          "time": "20210824 08:23"
        },
        {
          "id": "MDRW2100033504.1.26",
          "form": "근데 브라운신부는 50년대라 딱 정반대에요ㅋㅋ",
          "original_form": "근데 브라운신부는 50년대라 딱 정반대에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:24"
        },
        {
          "id": "MDRW2100033504.1.27",
          "form": "그런데 해당 드라마는요?",
          "original_form": "그런데 해당 드라마는요?",
          "speaker_id": "1",
          "time": "20210824 08:24"
        },
        {
          "id": "MDRW2100033504.1.28",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210824 08:24"
        },
        {
          "id": "MDRW2100033504.1.29",
          "form": "약간 야매식인가",
          "original_form": "약간 야매식인가",
          "speaker_id": "1",
          "time": "20210824 08:24"
        },
        {
          "id": "MDRW2100033504.1.30",
          "form": "고전적인?ㅋㅋ그리고 증거가 진짜 빈약해 가끔보면",
          "original_form": "고전적인?ㅋㅋ그리고 증거가 진짜 빈약해 가끔보면",
          "speaker_id": "2",
          "time": "20210824 08:24"
        },
        {
          "id": "MDRW2100033504.1.31",
          "form": "빈약한 증거와 야매식인 수사",
          "original_form": "빈약한 증거와 야매식인 수사",
          "speaker_id": "1",
          "time": "20210824 08:24"
        }
      ]
