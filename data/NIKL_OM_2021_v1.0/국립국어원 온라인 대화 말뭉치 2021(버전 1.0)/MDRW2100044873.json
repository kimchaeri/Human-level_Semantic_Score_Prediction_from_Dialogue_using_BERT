      "utterance": [
        {
          "id": "MDRW2100044873.1.1",
          "form": "ㅋㅋ 담 앨범이 잘나와야겠군요",
          "original_form": "ㅋㅋ 담 앨범이 잘나와야겠군요",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044873.1.2",
          "form": "포레스텔라는 계속 콘서트해요..",
          "original_form": "포레스텔라는 계속 콘서트해요..",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044873.1.3",
          "form": "저는 못가지만..ㅎㅜ",
          "original_form": "저는 못가지만..ㅎㅜ",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044873.1.4",
          "form": "직업때매 못가시는거예요?",
          "original_form": "직업때매 못가시는거예요?",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044873.1.5",
          "form": "네ㅠ 금지입니다..",
          "original_form": "네ㅠ 금지입니다..",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044873.1.6",
          "form": "온라인콘서트 해줬으면 좋겠어요",
          "original_form": "온라인콘서트 해줬으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044873.1.7",
          "form": "아.. 근데 관중없는 온라인은 진짜 노감동이에요ㅜ",
          "original_form": "아.. 근데 관중없는 온라인은 진짜 노감동이에요ㅜ",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044873.1.8",
          "form": "그쳐 그리고 포레스텔라는 이미 오프콘 하고있어서..ㅠ",
          "original_form": "그쳐 그리고 포레스텔라는 이미 오프콘 하고있어서..ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044873.1.9",
          "form": "유투브로만 만나고있어요ㅜ",
          "original_form": "유투브로만 만나고있어요ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044873.1.10",
          "form": "아 맞네요!!",
          "original_form": "아 맞네요!!",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044873.1.11",
          "form": "그럼 유투브랑 별차이없지않아요? 콘서트랑",
          "original_form": "그럼 유투브랑 별차이없지않아요? 콘서트랑",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044873.1.12",
          "form": "그런데 실시간이라는 점이 다르니까요ㅋㅋ",
          "original_form": "그런데 실시간이라는 점이 다르니까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044873.1.13",
          "form": "아소통!!!",
          "original_form": "아소통!!!",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044873.1.14",
          "form": "전 이시국되고 온콘 자주 했는데 오프콘과 진짜 천지차이...ㅜ",
          "original_form": "전 이시국되고 온콘 자주 했는데 오프콘과 진짜 천지차이...ㅜ",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044873.1.15",
          "form": "가서 소리는 못질러도 댓글로 박수는 치고싶어서요ㅋㅋ",
          "original_form": "가서 소리는 못질러도 댓글로 박수는 치고싶어서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044873.1.16",
          "form": "짝짝짝 ㅋㅋ",
          "original_form": "짝짝짝 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044873.1.17",
          "form": "찐 팬심ㅜㅜ",
          "original_form": "찐 팬심ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044873.1.18",
          "form": "감동ㅜㅠ",
          "original_form": "감동ㅜㅠ",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044873.1.19",
          "form": "온콘이랑 천지차이라니 그냥 오프 될때까지 유투브나 보면서 기다려야겠어요",
          "original_form": "온콘이랑 천지차이라니 그냥 오프 될때까지 유투브나 보면서 기다려야겠어요",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044873.1.20",
          "form": "네 제가 느끼기엔 그랬어요",
          "original_form": "네 제가 느끼기엔 그랬어요",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044873.1.21",
          "form": "공연장의 울림 그립네요",
          "original_form": "공연장의 울림 그립네요",
