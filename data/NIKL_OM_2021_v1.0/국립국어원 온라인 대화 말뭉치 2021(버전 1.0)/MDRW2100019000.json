      },
      "utterance": [
        {
          "id": "MDRW2100019000.1.1",
          "form": "뭐 얘기했는데",
          "original_form": "뭐 얘기했는데",
          "speaker_id": "2",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100019000.1.2",
          "form": "기억이 안나요",
          "original_form": "기억이 안나요",
          "speaker_id": "2",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100019000.1.3",
          "form": "그건 기록 안했는데..",
          "original_form": "그건 기록 안했는데..",
          "speaker_id": "2",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100019000.1.4",
          "form": "나 저장해놨었는데",
          "original_form": "나 저장해놨었는데",
          "speaker_id": "1",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100019000.1.5",
          "form": "어디있지",
          "original_form": "어디있지",
          "speaker_id": "1",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100019000.1.6",
          "form": "찾아주세요",
          "original_form": "찾아주세요",
          "speaker_id": "2",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100019000.1.7",
          "form": "저 경복궁인가 거기도 꼭",
          "original_form": "저 경복궁인가 거기도 꼭",
          "speaker_id": "2",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100019000.1.8",
          "form": "봄에가고싶음",
          "original_form": "봄에가고싶음",
          "speaker_id": "2",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100019000.1.9",
          "form": "헐 진짜 가고싶다",
          "original_form": "헐 진짜 가고싶다",
          "speaker_id": "1",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100019000.1.10",
          "form": "거기에 뭐..",
          "original_form": "거기에 뭐..",
          "speaker_id": "2",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100019000.1.11",
          "form": "차랑.. 전통",
          "original_form": "차랑.. 전통",
          "speaker_id": "2",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100019000.1.12",
          "form": "과자..?",
          "original_form": "과자..?",
          "speaker_id": "2",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100019000.1.13",
          "form": "저가 말한건 부산 국립해양박물관에서 하는",
          "original_form": "저가 말한건 부산 국립해양박물관에서 하는",
          "speaker_id": "1",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100019000.1.14",
          "form": "파는 유명한 거기도 좀 궁금하고",
          "original_form": "파는 유명한 거기도 좀 궁금하고",
          "speaker_id": "2",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100019000.1.15",
          "form": "심연의 상상이라는 전시네요",
          "original_form": "심연의 상상이라는 전시네요",
          "speaker_id": "1",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100019000.1.16",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "2",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100019000.1.17",
          "form": "헐.. 재밌겠다",
          "original_form": "헐.. 재밌겠다",
          "speaker_id": "1",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100019000.1.18",
          "form": "그건 전시가 언제까지에요?",
          "original_form": "그건 전시가 언제까지에요?",
          "speaker_id": "2",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100019000.1.19",
          "form": "10월10일",
          "original_form": "10월10일",
          "speaker_id": "1",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100019000.1.20",
          "form": "빠듯하네..",
          "original_form": "빠듯하네..",
          "speaker_id": "2",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100019000.1.21",
          "form": "첨 홍보 봤을 땐 널널했는데",
          "original_form": "첨 홍보 봤을 땐 널널했는데",
          "speaker_id": "1",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100019000.1.22",
          "form": "이제 종료가 곧이네",
          "original_form": "이제 종료가 곧이네",
          "speaker_id": "1",
          "time": "20210827 14:37"
        }
      ]
