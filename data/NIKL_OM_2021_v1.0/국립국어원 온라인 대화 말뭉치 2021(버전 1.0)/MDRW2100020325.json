      "utterance": [
        {
          "id": "MDRW2100020325.1.1",
          "form": "요즘 드시는 비타민 있으신가요??",
          "original_form": "요즘 드시는 비타민 있으신가요??",
          "speaker_id": "2",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020325.1.2",
          "form": "저는 딱히 없어요 ㅠㅠ 뭐드세요??",
          "original_form": "저는 딱히 없어요 ㅠㅠ 뭐드세요??",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020325.1.3",
          "form": "저는 멀티비타민이랑",
          "original_form": "저는 멀티비타민이랑",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020325.1.4",
          "form": "홍삼먹었는데",
          "original_form": "홍삼먹었는데",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020325.1.5",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020325.1.6",
          "form": "지금 다먹어서 아무것도 안먹고있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "지금 다먹어서 아무것도 안먹고있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020325.1.7",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020325.1.8",
          "form": "재구매예정은 없으신가요",
          "original_form": "재구매예정은 없으신가요",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020325.1.9",
          "form": "엄청 피곤할때는 꼭 사야지 하다가",
          "original_form": "엄청 피곤할때는 꼭 사야지 하다가",
          "speaker_id": "2",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020325.1.10",
          "form": "정작 제돈주고 사려니 아깝더라구요",
          "original_form": "정작 제돈주고 사려니 아깝더라구요",
          "speaker_id": "2",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020325.1.11",
          "form": "그럴수있져 ㅋㅋㅋ",
          "original_form": "그럴수있져 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020325.1.12",
          "form": "할인할때 사셨었나요??",
          "original_form": "할인할때 사셨었나요??",
          "speaker_id": "1",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020325.1.13",
          "form": "친구들이 생일선물로 줬어요 ㅋㅋ",
          "original_form": "친구들이 생일선물로 줬어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:28"
        },
        {
          "id": "MDRW2100020325.1.14",
          "form": "올해도 생일선물로 달라고 하려구요 ㅎㅎ",
          "original_form": "올해도 생일선물로 달라고 하려구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:28"
        },
        {
          "id": "MDRW2100020325.1.15",
          "form": "아아 ㅋㅋㅋㅋ 좋은 친구분들이시네용",
          "original_form": "아아 ㅋㅋㅋㅋ 좋은 친구분들이시네용",
          "speaker_id": "1",
          "time": "20210828 01:28"
        },
        {
          "id": "MDRW2100020325.1.16",
          "form": "생일선물 고를 수 있으면 최고죠 ㅋㅋㅋ",
          "original_form": "생일선물 고를 수 있으면 최고죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020325.1.17",
          "form": "제친구들도 점점 비타민사달라고 하더라구요 ㅋㅋㅋ",
          "original_form": "제친구들도 점점 비타민사달라고 하더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020325.1.18",
          "form": "다들 몸이 늙어서 ㅎㅎ",
          "original_form": "다들 몸이 늙어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020325.1.19",
          "form": "나이들면 여기저기 쑤시더라구요 ㅋㅋㅋ",
          "original_form": "나이들면 여기저기 쑤시더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020325.1.20",
          "form": "name1은 나이가 어떻게 되시나요 ㅎㅎ",
          "original_form": "&name1&은 나이가 어떻게 되시나요 ㅎㅎ",
