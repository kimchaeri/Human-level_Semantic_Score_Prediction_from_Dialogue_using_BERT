        },
        {
          "id": "MDRW2100021153.1.4",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100021153.1.5",
          "form": "서동요가원래없었고",
          "original_form": "서동요가원래없었고",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100021153.1.6",
          "form": "요즘에만든이론이라는얘기도있어",
          "original_form": "요즘에만든이론이라는얘기도있어",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100021153.1.7",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100021153.1.8",
          "form": "그게 무슨 소리임?",
          "original_form": "그게 무슨 소리임?",
          "speaker_id": "2",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100021153.1.9",
          "form": "그게설화잖아",
          "original_form": "그게설화잖아",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100021153.1.10",
          "form": "설화면옛날에있었다는얘긴데",
          "original_form": "설화면옛날에있었다는얘긴데",
          "speaker_id": "1",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100021153.1.11",
          "form": "그게아니라요즘세대에만들어낸거라는논문도있어",
          "original_form": "그게아니라요즘세대에만들어낸거라는논문도있어",
          "speaker_id": "1",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100021153.1.12",
          "form": "엥 그건 아님",
          "original_form": "엥 그건 아님",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100021153.1.13",
          "form": "삼국유사에 기록 되어 있음",
          "original_form": "삼국유사에 기록 되어 있음",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100021153.1.14",
          "form": "ㅇㅇㅇ그니까그게아닐수도있다는",
          "original_form": "ㅇㅇㅇ그니까그게아닐수도있다는",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021153.1.15",
          "form": "논문도있대",
          "original_form": "논문도있대",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021153.1.16",
          "form": "그런의견도있나봐",
          "original_form": "그런의견도있나봐",
