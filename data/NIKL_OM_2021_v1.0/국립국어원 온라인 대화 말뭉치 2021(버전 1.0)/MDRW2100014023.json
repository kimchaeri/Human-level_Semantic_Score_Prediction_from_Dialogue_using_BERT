      "utterance": [
        {
          "id": "MDRW2100014023.1.1",
          "form": "메로나는 어때?",
          "original_form": "메로나는 어때?",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014023.1.2",
          "form": "메로난 별로",
          "original_form": "메로난 별로",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014023.1.3",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014023.1.4",
          "form": "너무 많이 먹은거같아",
          "original_form": "너무 많이 먹은거같아",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014023.1.5",
          "form": "질려???",
          "original_form": "질려???",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014023.1.6",
          "form": "어",
          "original_form": "어",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014023.1.7",
          "form": "좀",
          "original_form": "좀",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014023.1.8",
          "form": "아쉽군",
          "original_form": "아쉽군",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014023.1.9",
          "form": "메로나 맛있는데",
          "original_form": "메로나 맛있는데",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014023.1.10",
          "form": "기프티콘 있어서 줄까 했는디",
          "original_form": "기프티콘 있어서 줄까 했는디",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100014023.1.11",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100014023.1.12",
          "form": "그러면 또 받아야지",
          "original_form": "그러면 또 받아야지",
          "speaker_id": "1",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100014023.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100014023.1.14",
          "form": "문자로 줄게",
          "original_form": "문자로 줄게",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100014023.1.15",
          "form": "진짜주는거지?",
          "original_form": "진짜주는거지?",
          "speaker_id": "1",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100014023.1.16",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100014023.1.17",
          "form": "고맙고맙",
          "original_form": "고맙고맙",
          "speaker_id": "1",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100014023.1.18",
          "form": "히히 멀 이정도가지구",
          "original_form": "히히 멀 이정도가지구",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100014023.1.19",
          "form": "이왕이면 다른아이스크림도줘",
          "original_form": "이왕이면 다른아이스크림도줘",
          "speaker_id": "1",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100014023.1.20",
          "form": "뭐?",
          "original_form": "뭐?",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100014023.1.21",
          "form": "그건 쫌",
          "original_form": "그건 쫌",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100014023.1.22",
          "form": "오늘 저녁 맛있었다",
          "original_form": "오늘 저녁 맛있었다",
