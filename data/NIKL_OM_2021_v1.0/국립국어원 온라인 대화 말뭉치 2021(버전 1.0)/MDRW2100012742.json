      "utterance": [
        {
          "id": "MDRW2100012742.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 00:33"
        },
        {
          "id": "MDRW2100012742.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012742.1.3",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012742.1.4",
          "form": "귀야운 댕댕이 한마리랑 같이 살고있어용",
          "original_form": "귀야운 댕댕이 한마리랑 같이 살고있어용",
          "speaker_id": "2",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100012742.1.5",
          "form": "너무 부러워요",
          "original_form": "너무 부러워요",
          "speaker_id": "1",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100012742.1.6",
          "form": "어떤 댕댕이 인가요?",
          "original_form": "어떤 댕댕이 인가요?",
          "speaker_id": "1",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100012742.1.7",
          "form": "자랑해주세요",
          "original_form": "자랑해주세요",
          "speaker_id": "1",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100012742.1.8",
          "form": "말티즈에요!!",
          "original_form": "말티즈에요!!",
          "speaker_id": "2",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100012742.1.9",
          "form": "근데 말티즈답지않게 엄청엄청 순하고 착해요ㅠㅜㅠ",
          "original_form": "근데 말티즈답지않게 엄청엄청 순하고 착해요ㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100012742.1.10",
          "form": "너무너무 귀야워요",
          "original_form": "너무너무 귀야워요",
          "speaker_id": "2",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100012742.1.11",
          "form": "강아지 좋아하시나보다",
          "original_form": "강아지 좋아하시나보다",
          "speaker_id": "2",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100012742.1.12",
          "form": "몇년 키우셨어요?",
          "original_form": "몇년 키우셨어요?",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100012742.1.13",
          "form": "강아지 나이는 12살이고 11년 키웠어요!",
          "original_form": "강아지 나이는 12살이고 11년 키웠어요!",
          "speaker_id": "2",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100012742.1.14",
          "form": "오 장수견이네요~",
          "original_form": "오 장수견이네요~",
          "speaker_id": "1",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100012742.1.15",
          "form": "히히 그래두 더 오래오래 살아야죠ㅠㅠ",
          "original_form": "히히 그래두 더 오래오래 살아야죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100012742.1.16",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100012742.1.17",
          "form": "아니요~ 예전에 닭 키운적있어요",
          "original_form": "아니요~ 예전에 닭 키운적있어요",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100012742.1.18",
          "form": "우와 닭이요?? 병아리때부터 기른건가요?",
          "original_form": "우와 닭이요?? 병아리때부터 기른건가요?",
          "speaker_id": "2",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100012742.1.19",
          "form": "네 초딩때 오백원 주고 데려왔어요",
          "original_form": "네 초딩때 오백원 주고 데려왔어요",
