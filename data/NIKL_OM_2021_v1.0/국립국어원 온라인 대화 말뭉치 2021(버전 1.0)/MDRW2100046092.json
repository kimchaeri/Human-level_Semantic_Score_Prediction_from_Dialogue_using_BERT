        },
        {
          "id": "MDRW2100046092.1.2",
          "form": "나 요즘 좋아하는 음료수있어",
          "original_form": "나 요즘 좋아하는 음료수있어",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046092.1.3",
          "form": "어떤 음료수야?",
          "original_form": "어떤 음료수야?",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046092.1.4",
          "form": "비타500",
          "original_form": "비타500",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046092.1.5",
          "form": "비타 오백 맛있긴 하지",
          "original_form": "비타 오백 맛있긴 하지",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046092.1.6",
          "form": "마시면 건강해지는느낌이야",
          "original_form": "마시면 건강해지는느낌이야",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046092.1.7",
          "form": "나는 스프라이트가 제일 좋아",
          "original_form": "나는 스프라이트가 제일 좋아",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046092.1.8",
          "form": "칠성꺼 사이다도맛있는데",
          "original_form": "칠성꺼 사이다도맛있는데",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046092.1.9",
          "form": "아 칠성사이다?",
          "original_form": "아 칠성사이다?",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046092.1.10",
          "form": "응응 사이다는 고기랑도어울려",
          "original_form": "응응 사이다는 고기랑도어울려",
          "speaker_id": "1",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046092.1.11",
          "form": "맞아 느끼한거에 딱이지",
          "original_form": "맞아 느끼한거에 딱이지",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046092.1.12",
          "form": "속이 시원하게뚤리지",
          "original_form": "속이 시원하게뚤리지",
          "speaker_id": "1",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046092.1.13",
          "form": "제로 사이다도 있대",
          "original_form": "제로 사이다도 있대",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046092.1.14",
          "form": "정말? 요즘 그런 건강한게많아서좋아",
          "original_form": "정말? 요즘 그런 건강한게많아서좋아",
