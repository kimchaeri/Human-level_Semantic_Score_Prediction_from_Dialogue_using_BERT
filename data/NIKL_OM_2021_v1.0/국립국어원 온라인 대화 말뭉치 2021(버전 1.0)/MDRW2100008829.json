      "utterance": [
        {
          "id": "MDRW2100008829.1.1",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008829.1.2",
          "form": "어릴땐 진짜 독서가 취미이자 특기였는데",
          "original_form": "어릴땐 진짜 독서가 취미이자 특기였는데",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008829.1.3",
          "form": "요즘은 실용서 위주로 본거같아요",
          "original_form": "요즘은 실용서 위주로 본거같아요",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008829.1.4",
          "form": "우와 책을 많이 읽으시나봐요",
          "original_form": "우와 책을 많이 읽으시나봐요",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008829.1.5",
          "form": "실용서 어떤거요?",
          "original_form": "실용서 어떤거요?",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008829.1.6",
          "form": "음 말이 거창했구요",
          "original_form": "음 말이 거창했구요",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008829.1.7",
          "form": "다이어트 책?",
          "original_form": "다이어트 책?",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008829.1.8",
          "form": "운동책? ㅋㅋㅋ",
          "original_form": "운동책? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008829.1.9",
          "form": "아하 뭐 좋은 책 있던가요?",
          "original_form": "아하 뭐 좋은 책 있던가요?",
          "speaker_id": "1",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100008829.1.10",
          "form": "저도 꿀팁좀 얻어가게요 ㅎ",
          "original_form": "저도 꿀팁좀 얻어가게요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100008829.1.11",
          "form": "음 결론은 적게 먹고 많이 움직여라 인거같아요 ㅋㅋㅋ",
          "original_form": "음 결론은 적게 먹고 많이 움직여라 인거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100008829.1.12",
          "form": "세상에 모두가 아는 그 대진리",
          "original_form": "세상에 모두가 아는 그 대진리",
          "speaker_id": "2",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100008829.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100008829.1.14",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "1",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100008829.1.15",
          "form": "모르는 사람없죠",
          "original_form": "모르는 사람없죠",
          "speaker_id": "1",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100008829.1.16",
          "form": "못할뿐 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "못할뿐 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100008829.1.17",
          "form": "아 굳이 따지면 살 안 찌는거는 많이먹어도된다?",
          "original_form": "아 굳이 따지면 살 안 찌는거는 많이먹어도된다?",
          "speaker_id": "2",
          "time": "20210822 20:55"
        },
        {
          "id": "MDRW2100008829.1.18",
          "form": "방울토마토 같은거요 ㅋㅋㅋ",
          "original_form": "방울토마토 같은거요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:55"
        },
        {
          "id": "MDRW2100008829.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100008829.1.20",
          "form": "맞는 말이라 너무 웃프네요",
          "original_form": "맞는 말이라 너무 웃프네요",
          "speaker_id": "1",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100008829.1.21",
          "form": "따로 인생 책 같은건 있으세요?",
          "original_form": "따로 인생 책 같은건 있으세요?",
          "speaker_id": "1",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100008829.1.22",
          "form": "전 다이어트책을 되게많이샀었는데 이게인생책이라보긴 좀슬프네요 ㅋㅋㅋ",
          "original_form": "전 다이어트책을 되게많이샀었는데 이게인생책이라보긴 좀슬프네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100008829.1.23",
          "form": "뭔가 정작다이어트는 실패하고 책만 산 느낌이 ㅋㅋ",
          "original_form": "뭔가 정작다이어트는 실패하고 책만 산 느낌이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100008829.1.24",
          "form": "ㅋㅋㅋㅋ다이어트 너무 어렵죠",
          "original_form": "ㅋㅋㅋㅋ다이어트 너무 어렵죠",
