      "utterance": [
        {
          "id": "MDRW2100038087.1.1",
          "form": "그냥 다이어리 꾸미는거야?",
          "original_form": "그냥 다이어리 꾸미는거야?",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038087.1.2",
          "form": "어머 너 다이어리 써?",
          "original_form": "어머 너 다이어리 써?",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038087.1.3",
          "form": "비싼 스티커를 붙이는거야",
          "original_form": "비싼 스티커를 붙이는거야",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038087.1.4",
          "form": "다이어리는 언제 산거야?",
          "original_form": "다이어리는 언제 산거야?",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038087.1.5",
          "form": "매년 다이어리를 구매하긴해",
          "original_form": "매년 다이어리를 구매하긴해",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038087.1.6",
          "form": "아까워서 스티커 못사겠어",
          "original_form": "아까워서 스티커 못사겠어",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038087.1.7",
          "form": "나도야",
          "original_form": "나도야",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038087.1.8",
          "form": "너는 올해도 다이어리를 샀니?",
          "original_form": "너는 올해도 다이어리를 샀니?",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038087.1.9",
          "form": "근데 요즘 일기안쓴지 오래됐어",
          "original_form": "근데 요즘 일기안쓴지 오래됐어",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038087.1.10",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038087.1.11",
          "form": "일기를 쓰눈게 즁욯9",
          "original_form": "일기를 쓰눈게 즁욯9",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038087.1.12",
          "form": "스티커 너무 비싸",
          "original_form": "스티커 너무 비싸",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038087.1.13",
          "form": "맞아 스티커가 금값이야",
          "original_form": "맞아 스티커가 금값이야",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038087.1.14",
          "form": "나는 그냥 귀여운 걸 소장하고싶거든",
          "original_form": "나는 그냥 귀여운 걸 소장하고싶거든",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038087.1.15",
          "form": "근데 스티커만 전문으로 파는 가게도 있더라",
          "original_form": "근데 스티커만 전문으로 파는 가게도 있더라",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038087.1.16",
          "form": "오늘 귀여운 강아지 스티커를 찾았어",
          "original_form": "오늘 귀여운 강아지 스티커를 찾았어",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038087.1.17",
          "form": "다음에 수성못을 가면 나랑 가보자",
          "original_form": "다음에 수성못을 가면 나랑 가보자",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038087.1.18",
          "form": "한장인데 육촌원이야",
          "original_form": "한장인데 육촌원이야",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038087.1.19",
          "form": "거기가면 눈이 막 돌아가",
          "original_form": "거기가면 눈이 막 돌아가",
          "speaker_id": "1",
          "time": "20210901 23:05"
        }
      ]
