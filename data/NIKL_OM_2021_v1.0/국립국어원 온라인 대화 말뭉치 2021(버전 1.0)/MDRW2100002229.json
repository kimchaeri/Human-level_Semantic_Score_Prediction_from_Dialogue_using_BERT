      "utterance": [
        {
          "id": "MDRW2100002229.1.1",
          "form": "민트초코 제품들 많더라",
          "original_form": "민트초코 제품들 많더라",
          "speaker_id": "2",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002229.1.2",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002229.1.3",
          "form": "오. 맞아. 너 민트초코를 좋아했었지",
          "original_form": "오. 맞아. 너 민트초코를 좋아했었지",
          "speaker_id": "1",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002229.1.4",
          "form": "나는 그냥 초코칩이 좋더라",
          "original_form": "나는 그냥 초코칩이 좋더라",
          "speaker_id": "2",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002229.1.5",
          "form": "아니 그건 잘못된 정보인거같아",
          "original_form": "아니 그건 잘못된 정보인거같아",
          "speaker_id": "2",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002229.1.6",
          "form": "예전에 네가 나에게 배라 민트초코를 알려준 후로 나는 그걸 즐겨먹게 되었단다.",
          "original_form": "예전에 네가 나에게 배라 민트초코를 알려준 후로 나는 그걸 즐겨먹게 되었단다.",
          "speaker_id": "1",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002229.1.7",
          "form": "아. 취향이 바뀌었구나.",
          "original_form": "아. 취향이 바뀌었구나.",
          "speaker_id": "1",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002229.1.8",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002229.1.9",
          "form": "사람은 바뀌니까",
          "original_form": "사람은 바뀌니까",
          "speaker_id": "2",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002229.1.10",
          "form": "서브웨이는 보통 어떤거 시켜먹어?",
          "original_form": "서브웨이는 보통 어떤거 시켜먹어?",
          "speaker_id": "2",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002229.1.11",
          "form": "나는 서브웨이 짜서 싫어해.",
          "original_form": "나는 서브웨이 짜서 싫어해.",
          "speaker_id": "1",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002229.1.12",
          "form": "서브웨이보다는 퀴즈노스를 좋아한단다.",
          "original_form": "서브웨이보다는 퀴즈노스를 좋아한단다.",
          "speaker_id": "1",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002229.1.13",
          "form": "언제한번 꼭 먹어보렴.",
          "original_form": "언제한번 꼭 먹어보렴.",
          "speaker_id": "1",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002229.1.14",
          "form": "아직 먹어본적이 없어서 비교를 못하겠어",
          "original_form": "아직 먹어본적이 없어서 비교를 못하겠어",
          "speaker_id": "2",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002229.1.15",
          "form": "알겠어 정말 너무 궁금해",
          "original_form": "알겠어 정말 너무 궁금해",
          "speaker_id": "2",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002229.1.16",
          "form": "그럼 언젠간 같이 먹으러 가자.",
          "original_form": "그럼 언젠간 같이 먹으러 가자.",
          "speaker_id": "1",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002229.1.17",
          "form": "근데 경대병원이 좀 멀긴하니까",
          "original_form": "근데 경대병원이 좀 멀긴하니까",
          "speaker_id": "2",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002229.1.18",
          "form": "거기까르보나라 어쩌구도 맛있고",
          "original_form": "거기까르보나라 어쩌구도 맛있고",
          "speaker_id": "1",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002229.1.19",
          "form": "트레디셔널도 맛있어.",
          "original_form": "트레디셔널도 맛있어.",
          "speaker_id": "1",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002229.1.20",
          "form": "맘단단히 먹고 가보자",
          "original_form": "맘단단히 먹고 가보자",
          "speaker_id": "2",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002229.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아.",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아.",
          "speaker_id": "1",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002229.1.22",
          "form": "까르보나라 맛있겠다",
          "original_form": "까르보나라 맛있겠다",
          "speaker_id": "2",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002229.1.23",
          "form": "식도략만남이 되겠구나.",
          "original_form": "식도략만남이 되겠구나.",
          "speaker_id": "1",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002229.1.24",
          "form": "지금 너무 땡기네",
          "original_form": "지금 너무 땡기네",
          "speaker_id": "2",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002229.1.25",
          "form": "나 어제 탐앤탐스 허니브레드를 구매했어",
          "original_form": "나 어제 탐앤탐스 허니브레드를 구매했어",
          "speaker_id": "1",
          "time": "20210820 14:55"
        }
      ]
    }
