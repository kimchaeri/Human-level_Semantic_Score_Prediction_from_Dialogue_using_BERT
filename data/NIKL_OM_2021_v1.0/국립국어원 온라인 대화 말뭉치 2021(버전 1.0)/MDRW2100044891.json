      "utterance": [
        {
          "id": "MDRW2100044891.1.1",
          "form": "최근에 어떤거 쇼핑하셨어요?",
          "original_form": "최근에 어떤거 쇼핑하셨어요?",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044891.1.2",
          "form": "무선이어폰 샀어요!",
          "original_form": "무선이어폰 샀어요!",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044891.1.3",
          "form": "앗! 혹시 버즈?",
          "original_form": "앗! 혹시 버즈?",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044891.1.4",
          "form": "네네 ㅋㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044891.1.5",
          "form": "근데 최신은 아녜요",
          "original_form": "근데 최신은 아녜요",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044891.1.6",
          "form": "저는 버즈 플러스 사용하고 있어요 :_)",
          "original_form": "저는 버즈 플러스 사용하고 있어요 :_)",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044891.1.7",
          "form": "저는 버즈라이브샀어요!",
          "original_form": "저는 버즈라이브샀어요!",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044891.1.8",
          "form": "라이브 부터는 노이즈캔슬링 되죠?",
          "original_form": "라이브 부터는 노이즈캔슬링 되죠?",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044891.1.9",
          "form": "된다고는 하는대",
          "original_form": "된다고는 하는대",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044891.1.10",
          "form": "커널형이 아니라 그런지 드라마틱 하진 않아요",
          "original_form": "커널형이 아니라 그런지 드라마틱 하진 않아요",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044891.1.11",
          "form": "아... 그런 느낌이군요",
          "original_form": "아... 그런 느낌이군요",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044891.1.12",
          "form": "이거는.. 좀 돼서 캔슬링 기능 자체가 없어요 ㅠ",
          "original_form": "이거는.. 좀 돼서 캔슬링 기능 자체가 없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044891.1.13",
          "form": "근데 캔슬링 전 거의 안써서...",
          "original_form": "근데 캔슬링 전 거의 안써서...",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044891.1.14",
          "form": "귀가 먹먹해지는 느낌이 불편하더라구요 ㅠ",
          "original_form": "귀가 먹먹해지는 느낌이 불편하더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044891.1.15",
          "form": "있어도 기능이 불편하군요 ㅠ퓨",
          "original_form": "있어도 기능이 불편하군요 ㅠ퓨",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044891.1.16",
          "form": "네네 아직까지는 한창 개발중인 기능이라고 느꼇어요 ㅠ",
