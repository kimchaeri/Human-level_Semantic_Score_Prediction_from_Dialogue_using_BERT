      "utterance": [
        {
          "id": "MDRW2100030449.1.1",
          "form": "조개나 새우 이런 건 무슨 맛인지도 모르겠어서 안 땡겨요",
          "original_form": "조개나 새우 이런 건 무슨 맛인지도 모르겠어서 안 땡겨요",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030449.1.2",
          "form": "다행이졓ㅎㅎㅎㅎ",
          "original_form": "다행이졓ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030449.1.3",
          "form": "아 진짜요?? 그정도면 안좋아하시는거다",
          "original_form": "아 진짜요?? 그정도면 안좋아하시는거다",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030449.1.4",
          "form": "저는 맛있어서 좋아하거든요",
          "original_form": "저는 맛있어서 좋아하거든요",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030449.1.5",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030449.1.6",
          "form": "특히 꼬막철엔",
          "original_form": "특히 꼬막철엔",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030449.1.7",
          "form": "무슨 맛인가요..?",
          "original_form": "무슨 맛인가요..?",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030449.1.8",
          "form": "전꼬막도 그냥 양념맛으로 먹어요..",
          "original_form": "전꼬막도 그냥 양념맛으로 먹어요..",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030449.1.9",
          "form": "그 쫄깃하면서 살짝 비린맛이 포인트??",
          "original_form": "그 쫄깃하면서 살짝 비린맛이 포인트??",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030449.1.10",
          "form": "ㅋㅋ 꼬막은 양념맛이긴 해여",
          "original_form": "ㅋㅋ 꼬막은 양념맛이긴 해여",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030449.1.11",
          "form": "새우를 안좋아하신다니 감바스 안좋아하시겠어요",
          "original_form": "새우를 안좋아하신다니 감바스 안좋아하시겠어요",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030449.1.12",
          "form": "헐 맞아요 맞아요",
          "original_form": "헐 맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100030449.1.13",
          "form": "감바스 밀키트도 그냥",
          "original_form": "감바스 밀키트도 그냥",
          "speaker_id": "2",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100030449.1.14",
          "form": "요즘 생새우 철이라는데 대하구이 같은거도 안땡기시겠어요",
          "original_form": "요즘 생새우 철이라는데 대하구이 같은거도 안땡기시겠어요",
          "speaker_id": "1",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100030449.1.15",
          "form": "가끔 땡겨서 사는데 먹고나면 늘 후회..",
          "original_form": "가끔 땡겨서 사는데 먹고나면 늘 후회..",
          "speaker_id": "2",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100030449.1.16",
          "form": "대하는 진심",
          "original_form": "대하는 진심",
          "speaker_id": "2",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100030449.1.17",
          "form": "부모님이 먹으러 가자고 해도 안 가고 해요",
          "original_form": "부모님이 먹으러 가자고 해도 안 가고 해요",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030449.1.18",
          "form": "와 그러시구나",
          "original_form": "와 그러시구나",
          "speaker_id": "1",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030449.1.19",
          "form": "진짜 몸에서 안받으시나보다",
          "original_form": "진짜 몸에서 안받으시나보다",
          "speaker_id": "1",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030449.1.20",
          "form": "진짜 남들은",
          "original_form": "진짜 남들은",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030449.1.21",
          "form": "맛잇어하는 거 맞죠..?",
          "original_form": "맛잇어하는 거 맞죠..?",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030449.1.22",
          "form": "네 대체로요 ㅋㅋㅋ",
          "original_form": "네 대체로요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030449.1.23",
          "form": "싫어하는 사람을 거의 못봤어요!!",
          "original_form": "싫어하는 사람을 거의 못봤어요!!",
          "speaker_id": "1",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030449.1.24",
          "form": "약간 쓴맛 나시진 않죠?",
          "original_form": "약간 쓴맛 나시진 않죠?",
          "speaker_id": "1",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100030449.1.25",
          "form": "알레르기 있으면 먹는거에서 쓴맛이 난다고 했던걸 들어서요",
          "original_form": "알레르기 있으면 먹는거에서 쓴맛이 난다고 했던걸 들어서요",
          "speaker_id": "1",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100030449.1.26",
          "form": "쓴맛까진 아니고",
          "original_form": "쓴맛까진 아니고",
          "speaker_id": "2",
          "time": "20210904 01:20"
        },
        {
          "id": "MDRW2100030449.1.27",
          "form": "그냥 무맛.....",
          "original_form": "그냥 무맛.....",
          "speaker_id": "2",
          "time": "20210904 01:20"
        },
        {
          "id": "MDRW2100030449.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 쓴맛이 나면 막 두드러기 일어나고 그런 정도인가요?ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 쓴맛이 나면 막 두드러기 일어나고 그런 정도인가요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 01:20"
        },
        {
          "id": "MDRW2100030449.1.29",
          "form": "그건 아닌거 같고 살짝 아린맛이 난대요",
          "original_form": "그건 아닌거 같고 살짝 아린맛이 난대요",
