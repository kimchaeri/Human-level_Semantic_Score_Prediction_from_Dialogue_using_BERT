      "utterance": [
        {
          "id": "MDRW2100030012.1.1",
          "form": "백신 예약 하셨나용?!?!",
          "original_form": "백신 예약 하셨나용?!?!",
          "speaker_id": "1",
          "time": "20210903 18:33"
        },
        {
          "id": "MDRW2100030012.1.2",
          "form": "네네 예약했어요! 예약하셨나요!",
          "original_form": "네네 예약했어요! 예약하셨나요!",
          "speaker_id": "2",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100030012.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 했습니당!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 했습니당!",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030012.1.4",
          "form": "전 잔여백신 잡았어요!",
          "original_form": "전 잔여백신 잡았어요!",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030012.1.5",
          "form": "이제 2차만 맞으면 되네용ㅎㅎ",
          "original_form": "이제 2차만 맞으면 되네용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030012.1.6",
          "form": "어떤거 맞으셨어요?",
          "original_form": "어떤거 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030012.1.7",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030012.1.8",
          "form": "전 화이자요!!",
          "original_form": "전 화이자요!!",
          "speaker_id": "1",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100030012.1.9",
          "form": "원래 2차도 8월달이었는데",
          "original_form": "원래 2차도 8월달이었는데",
          "speaker_id": "1",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100030012.1.10",
          "form": "엄청 밀렸네용ㅎㅎㅎ",
          "original_form": "엄청 밀렸네용ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100030012.1.11",
          "form": "4주가 6주로 미뤄지는 바람에ㅠㅠ",
          "original_form": "4주가 6주로 미뤄지는 바람에ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100030012.1.12",
          "form": "2차는 언제 맞으세요?",
          "original_form": "2차는 언제 맞으세요?",
          "speaker_id": "2",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100030012.1.13",
          "form": "1주일 뒤요!!",
          "original_form": "1주일 뒤요!!",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030012.1.14",
          "form": "가기전에",
          "original_form": "가기전에",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030012.1.15",
          "form": "삼겹살 먹구",
          "original_form": "삼겹살 먹구",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030012.1.16",
          "form": "가려구용ㅋㅋㅋ",
          "original_form": "가려구용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030012.1.17",
          "form": "얼마 안남았네요",
          "original_form": "얼마 안남았네요",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030012.1.18",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ 삼겹살 맛있죠,,",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ 삼겹살 맛있죠,,",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030012.1.19",
          "form": "저번에 회사분이 백신맞는 날 삼겹살 먹어야한다고 하시던데 ㅋㅋㅋ 이런 얘기가 돌았었나요?",
          "original_form": "저번에 회사분이 백신맞는 날 삼겹살 먹어야한다고 하시던데 ㅋㅋㅋ 이런 얘기가 돌았었나요?",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030012.1.20",
          "form": "ㅋㅋㅋㅋㅋ아 SNS에서 삼겹살 먹고 갔더니",
          "original_form": "ㅋㅋㅋㅋㅋ아 SNS에서 삼겹살 먹고 갔더니",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100030012.1.21",
          "form": "하나도 안 아팠다",
          "original_form": "하나도 안 아팠다",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100030012.1.22",
          "form": "이런 얘기를 하셔서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이런 얘기를 하셔서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100030012.1.23",
          "form": "사실 백신은 핑계에요 ㅎㅎㅎ",
          "original_form": "사실 백신은 핑계에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100030012.1.24",
          "form": "맞아여 ㅋㅋㅋㅋㅋㅋ 그런 핑계가 있어주면",
          "original_form": "맞아여 ㅋㅋㅋㅋㅋㅋ 그런 핑계가 있어주면",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100030012.1.25",
          "form": "맛있는 음식 먹기 죄책감 덜하고 행복해여 ㅋㅋㅋ",
          "original_form": "맛있는 음식 먹기 죄책감 덜하고 행복해여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100030012.1.26",
          "form": "ㅋㅋㅋㅋㅋ맞아용ㅋㅋㅋㅇ0ㅇ님은 언제 맞으세요??",
          "original_form": "ㅋㅋㅋㅋㅋ맞아용ㅋㅋㅋㅇ0ㅇ님은 언제 맞으세요??",
          "speaker_id": "1",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100030012.1.27",
          "form": "저는 9월말이요!",
          "original_form": "저는 9월말이요!",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030012.1.28",
          "form": "9월 말 이라고 보냈는데 나쁜말이라고 뜨네요 ㅋㅌ",
          "original_form": "9월 말 이라고 보냈는데 나쁜말이라고 뜨네요 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030012.1.29",
          "form": "ㅋㅋㅋㅋ뭐지ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ뭐지ㅋㅋㅋ",
