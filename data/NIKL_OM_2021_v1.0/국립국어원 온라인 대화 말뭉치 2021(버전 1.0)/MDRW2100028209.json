      "utterance": [
        {
          "id": "MDRW2100028209.1.1",
          "form": "안녕하세요! 게임 좋아하시나요?",
          "original_form": "안녕하세요! 게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028209.1.2",
          "form": "좋아하는편이긴한데 지금은 하는거 별로없어요",
          "original_form": "좋아하는편이긴한데 지금은 하는거 별로없어요",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028209.1.3",
          "form": "name1님은여",
          "original_form": "&name1&님은여",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028209.1.4",
          "form": "저도 예전에 오버워치 엄청 했는데",
          "original_form": "저도 예전에 오버워치 엄청 했는데",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028209.1.5",
          "form": "요새는 현생 때문에 많이 못하는 것 같아요",
          "original_form": "요새는 현생 때문에 많이 못하는 것 같아요",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028209.1.6",
          "form": "name2님은 무슨 게임 하셨었어요?",
          "original_form": "&name2&님은 무슨 게임 하셨었어요?",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028209.1.7",
          "form": "지금하고있는건 모바일겜하고있어요",
          "original_form": "지금하고있는건 모바일겜하고있어요",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028209.1.8",
          "form": "모바일 겜 한 번 하면 못빠져나온다는데",
          "original_form": "모바일 겜 한 번 하면 못빠져나온다는데",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028209.1.9",
          "form": "정말로 그런가요...",
          "original_form": "정말로 그런가요...",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028209.1.10",
          "form": "아녀아녀 ㅋㅋㅋㅋㅋ전 금방질리는타입이라",
          "original_form": "아녀아녀 ㅋㅋㅋㅋㅋ전 금방질리는타입이라",
          "speaker_id": "2",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028209.1.11",
          "form": "지금 쿠킹덤하고있는데 현질안하면서 그냥저냥하고있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "지금 쿠킹덤하고있는데 현질안하면서 그냥저냥하고있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028209.1.12",
          "form": "앗 어쩐지 쿠킹덤일것같았어요 ㅋㅋ",
          "original_form": "앗 어쩐지 쿠킹덤일것같았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028209.1.13",
          "form": "쿠키가 다 귀엽더라고요",
          "original_form": "쿠키가 다 귀엽더라고요",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028209.1.14",
          "form": "네 근데 메인에피가 재미없어요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 근데 메인에피가 재미없어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028209.1.15",
          "form": "와 그거 메인 에피도 따로 있어요??",
          "original_form": "와 그거 메인 에피도 따로 있어요??",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028209.1.16",
          "form": "그냥 쿠키들이.세상을 구하는 건 줄 알았는데..",
          "original_form": "그냥 쿠키들이.세상을 구하는 건 줄 알았는데..",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028209.1.17",
          "form": "네 메인에피소드가 사냥모드인데 자동사냥이고 걍똑같이진행하는게 반복이라서ㅜ",
          "original_form": "네 메인에피소드가 사냥모드인데 자동사냥이고 걍똑같이진행하는게 반복이라서ㅜ",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028209.1.18",
          "form": "헉 그럼 좀 질리시겠다",
          "original_form": "헉 그럼 좀 질리시겠다",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028209.1.19",
          "form": "모바일 겜은 자동 전투나 반복되는게 많은 것 같아요",
          "original_form": "모바일 겜은 자동 전투나 반복되는게 많은 것 같아요",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028209.1.20",
          "form": "네 그래서 걍 쿠뽑만하거있어요",
          "original_form": "네 그래서 걍 쿠뽑만하거있어요",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028209.1.21",
          "form": "캔디크러쉬사가라던가",
          "original_form": "캔디크러쉬사가라던가",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028209.1.22",
          "form": "현질안하신다니 인내력 대단하시다",
          "original_form": "현질안하신다니 인내력 대단하시다",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028209.1.23",
          "form": "그게 모바일게임 단점인것같아여 한계가있으니까 비슷비슷해요",
          "original_form": "그게 모바일게임 단점인것같아여 한계가있으니까 비슷비슷해요",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028209.1.24",
          "form": "제 친구들도 그 뭐지 대머리 아저씨 나오는",
          "original_form": "제 친구들도 그 뭐지 대머리 아저씨 나오는",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028209.1.25",
          "form": "돈 좀쓴적도 많은데...남는게없더라구여..특히모바일은 망겜되버리니까..",
          "original_form": "돈 좀쓴적도 많은데...남는게없더라구여..특히모바일은 망겜되버리니까..",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028209.1.26",
          "form": "아 대머리 ㅋㅋㅋㅋㅋㅋㅋㅋ꿈의집인가",
          "original_form": "아 대머리 ㅋㅋㅋㅋㅋㅋㅋㅋ꿈의집인가",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028209.1.27",
          "form": "무슨 행복의 집? 이런 겜 하던데..현질 많이 하더라고요",
          "original_form": "무슨 행복의 집? 이런 겜 하던데..현질 많이 하더라고요",
          "speaker_id": "1",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100028209.1.28",
          "form": "아맞아!!",
          "original_form": "아맞아!!",
          "speaker_id": "1",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100028209.1.29",
          "form": "꿈의집..ㅎㅎ",
          "original_form": "꿈의집..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100028209.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그거은근많이하시더라구여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그거은근많이하시더라구여",
          "speaker_id": "2",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100028209.1.31",
          "form": "그쵸 저도 처음엔 집 꾸미기인줄 알았는데",
          "original_form": "그쵸 저도 처음엔 집 꾸미기인줄 알았는데",
          "speaker_id": "1",
          "time": "20210902 18:21"
