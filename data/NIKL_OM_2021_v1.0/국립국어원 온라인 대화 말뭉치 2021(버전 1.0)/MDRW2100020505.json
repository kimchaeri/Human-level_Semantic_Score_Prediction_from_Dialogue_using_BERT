        },
        {
          "id": "MDRW2100020505.1.2",
          "form": "오대박",
          "original_form": "오대박",
          "speaker_id": "1",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020505.1.3",
          "form": "근데 진짜 샤머는 아니래",
          "original_form": "근데 진짜 샤머는 아니래",
          "speaker_id": "2",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020505.1.4",
          "form": "에이..뭐야",
          "original_form": "에이..뭐야",
          "speaker_id": "1",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020505.1.5",
          "form": "그니깐 ㅡㅡ어이없슈",
          "original_form": "그니깐 ㅡㅡ어이없슈",
          "speaker_id": "2",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020505.1.6",
          "form": "좀빡치네!!!",
          "original_form": "좀빡치네!!!",
          "speaker_id": "1",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020505.1.7",
          "form": "그냥 캔디같은 맛이래",
          "original_form": "그냥 캔디같은 맛이래",
          "speaker_id": "2",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020505.1.8",
          "form": "그럼안먹어",
          "original_form": "그럼안먹어",
          "speaker_id": "1",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020505.1.9",
          "form": "나두안먹어!",
          "original_form": "나두안먹어!",
          "speaker_id": "2",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020505.1.10",
          "form": "캔디는 좀그렇자나",
          "original_form": "캔디는 좀그렇자나",
