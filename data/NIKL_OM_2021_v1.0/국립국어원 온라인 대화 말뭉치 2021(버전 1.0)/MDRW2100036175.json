      "utterance": [
        {
          "id": "MDRW2100036175.1.1",
          "form": "제가 교환학생3 타과1 저 이렇게 한적이 있어서...",
          "original_form": "제가 교환학생3 타과1 저 이렇게 한적이 있어서...",
          "speaker_id": "2",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100036175.1.2",
          "form": "조별과제? 개인과제!였어요",
          "original_form": "조별과제? 개인과제!였어요",
          "speaker_id": "2",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100036175.1.3",
          "form": "교환학생 3명이었으면...",
          "original_form": "교환학생 3명이었으면...",
          "speaker_id": "1",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100036175.1.4",
          "form": "아찔하네요...",
          "original_form": "아찔하네요...",
          "speaker_id": "1",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100036175.1.5",
          "form": "어우 아찔해요...",
          "original_form": "어우 아찔해요...",
          "speaker_id": "2",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036175.1.6",
          "form": "그래도 조별을 졸업때까지 한 6번정도 밖에 안해서",
          "original_form": "그래도 조별을 졸업때까지 한 6번정도 밖에 안해서",
          "speaker_id": "2",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036175.1.7",
          "form": "다행이예요",
          "original_form": "다행이예요",
          "speaker_id": "2",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036175.1.8",
          "form": "그 교환학생이시면 외국 분이신 거죠?",
          "original_form": "그 교환학생이시면 외국 분이신 거죠?",
          "speaker_id": "1",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036175.1.9",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036175.1.10",
          "form": "저는 중국분이셨어요",
          "original_form": "저는 중국분이셨어요",
          "speaker_id": "2",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036175.1.11",
          "form": "절대ㅜ피해야겠네요...",
          "original_form": "절대ㅜ피해야겠네요...",
          "speaker_id": "1",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036175.1.12",
          "form": "그죠...",
          "original_form": "그죠...",
          "speaker_id": "2",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100036175.1.13",
          "form": "언어 쪽 전공이셨나요?",
          "original_form": "언어 쪽 전공이셨나요?",
          "speaker_id": "1",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100036175.1.14",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100036175.1.15",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100036175.1.16",
          "form": "이게 좀 늦게가네요...",
          "original_form": "이게 좀 늦게가네요...",
          "speaker_id": "2",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100036175.1.17",
          "form": "언어쪽이면 조별많지않나요?",
          "original_form": "언어쪽이면 조별많지않나요?",
          "speaker_id": "2",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100036175.1.18",
          "form": "교환학생 있으셔서 언어쪽인가 여쭤본 거였어요! 저는 공대라서 팀플이 거의 없긴 한데 이번에는 교양 과목 하나가 팀플 수업이더라고요",
          "original_form": "교환학생 있으셔서 언어쪽인가 여쭤본 거였어요! 저는 공대라서 팀플이 거의 없긴 한데 이번에는 교양 과목 하나가 팀플 수업이더라고요",
          "speaker_id": "1",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100036175.1.19",
          "form": "저도 공대쪽이여서 그나마 적게했어요",
          "original_form": "저도 공대쪽이여서 그나마 적게했어요",
          "speaker_id": "2",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100036175.1.20",
          "form": "실습수업은 없으신가요?",
          "original_form": "실습수업은 없으신가요?",
          "speaker_id": "2",
          "time": "20210830 18:05"
        },
        {
          "id": "MDRW2100036175.1.21",
          "form": "공대긴 한데 소프트웨어 쪽이라 하드웨어 실습은 없어요!",
          "original_form": "공대긴 한데 소프트웨어 쪽이라 하드웨어 실습은 없어요!",
          "speaker_id": "1",
          "time": "20210830 18:06"
        },
        {
          "id": "MDRW2100036175.1.22",
          "form": "그러시구나! 저희는 실습이 많아서 학교에서 밤새는 수업도 있었구요 배일주일타는 수업도 있었어요",
          "original_form": "그러시구나! 저희는 실습이 많아서 학교에서 밤새는 수업도 있었구요 배일주일타는 수업도 있었어요",
          "speaker_id": "2",
          "time": "20210830 18:15"
        },
        {
          "id": "MDRW2100036175.1.23",
          "form": "좀 독특한 수업이 있는 과였어요",
          "original_form": "좀 독특한 수업이 있는 과였어요",
          "speaker_id": "2",
          "time": "20210830 18:15"
        }
      ]
    }
  ]
