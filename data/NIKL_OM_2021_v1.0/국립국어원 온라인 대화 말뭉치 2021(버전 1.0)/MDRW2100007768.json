      "utterance": [
        {
          "id": "MDRW2100007768.1.1",
          "form": "저는 요즘 넷플릭스랑 유튜브 보는 시간이 엄청 늘었어요",
          "original_form": "저는 요즘 넷플릭스랑 유튜브 보는 시간이 엄청 늘었어요",
          "speaker_id": "2",
          "time": "20210822 10:23"
        },
        {
          "id": "MDRW2100007768.1.2",
          "form": "전 한참보다 넷플은 좀줄고",
          "original_form": "전 한참보다 넷플은 좀줄고",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007768.1.3",
          "form": "유튜브요",
          "original_form": "유튜브요",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007768.1.4",
          "form": "유튜브 보다보면",
          "original_form": "유튜브 보다보면",
          "speaker_id": "2",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007768.1.5",
          "form": "넷플은 요새 시리즈 보다 말고 해요",
          "original_form": "넷플은 요새 시리즈 보다 말고 해요",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007768.1.6",
          "form": "막 연관동영상 누르게 되고",
          "original_form": "막 연관동영상 누르게 되고",
          "speaker_id": "2",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007768.1.7",
          "form": "맞아욬ㅋ 자동인간 됐어요",
          "original_form": "맞아욬ㅋ 자동인간 됐어요",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007768.1.8",
          "form": "그러다보면 진짜 시간이 금방 흐르더라고요",
          "original_form": "그러다보면 진짜 시간이 금방 흐르더라고요",
          "speaker_id": "2",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007768.1.9",
          "form": "알아서 막나오는데로 봐요",
          "original_form": "알아서 막나오는데로 봐요",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007768.1.10",
          "form": "네 눈아프게 보네요 ㅋㅋ",
          "original_form": "네 눈아프게 보네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007768.1.11",
          "form": "넷플은 만화 보고",
          "original_form": "넷플은 만화 보고",
          "speaker_id": "1",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007768.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007768.1.13",
          "form": "왓챠도 애니요",
          "original_form": "왓챠도 애니요",
          "speaker_id": "1",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007768.1.14",
          "form": "오 왓챠도 구독하시는구나",
          "original_form": "오 왓챠도 구독하시는구나",
          "speaker_id": "2",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007768.1.15",
          "form": "저는 왓챠 잠깐 구독했는데",
          "original_form": "저는 왓챠 잠깐 구독했는데",
          "speaker_id": "2",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007768.1.16",
          "form": "그때 해리포터 시리즈 정주행했어요",
          "original_form": "그때 해리포터 시리즈 정주행했어요",
          "speaker_id": "2",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007768.1.17",
          "form": "그거 팝곤 있잖아요 ㅋㅋ",
          "original_form": "그거 팝곤 있잖아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007768.1.18",
          "form": "전 해리포터 영화관에서 다봤어요",
          "original_form": "전 해리포터 영화관에서 다봤어요",
          "speaker_id": "1",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007768.1.19",
          "form": "넘좋아 했었어요 ㅎㅎ",
          "original_form": "넘좋아 했었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007768.1.20",
          "form": "저도 영화관에서 4편인가까지는 봤는데",
          "original_form": "저도 영화관에서 4편인가까지는 봤는데",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007768.1.21",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007768.1.22",
          "form": "오랜만에 왓챠서 정주행하니 재미있더라고요ㅎㅎ",
          "original_form": "오랜만에 왓챠서 정주행하니 재미있더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007768.1.23",
          "form": "전 매번 가서 봤어요",
          "original_form": "전 매번 가서 봤어요",
          "speaker_id": "1",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007768.1.24",
          "form": "그럼요 잼나요",
          "original_form": "그럼요 잼나요",
          "speaker_id": "1",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007768.1.25",
          "form": "전 왓챠 주술회전 보려고 ㅋㅋ",
          "original_form": "전 왓챠 주술회전 보려고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007768.1.26",
          "form": "주술회전?",
          "original_form": "주술회전?",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007768.1.27",
          "form": "그러고 나니 볼게 없네요 ㅋㅋ",
          "original_form": "그러고 나니 볼게 없네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007768.1.28",
          "form": "중국드라마인가요?",
          "original_form": "중국드라마인가요?",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007768.1.29",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007768.1.30",
          "form": "뭐 보시는거 있으세요?",
          "original_form": "뭐 보시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007768.1.31",
          "form": "제 지인 중에도 중국드라마 좋아하는 분 계신데ㅎㅎ",
          "original_form": "제 지인 중에도 중국드라마 좋아하는 분 계신데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007768.1.32",
          "form": "전 향밀밀 삼생삼세에 빠져서 한참 봤었어요 ㅋㅋ",
          "original_form": "전 향밀밀 삼생삼세에 빠져서 한참 봤었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007768.1.33",
          "form": "중국드라마는 넷플릭스보다는 왓챠가 더 많은 것 같더라고요~",
          "original_form": "중국드라마는 넷플릭스보다는 왓챠가 더 많은 것 같더라고요~",
          "speaker_id": "2",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007768.1.34",
          "form": "채널이 ㅜㅜ사라져서 못본후 안보고 있어요",
          "original_form": "채널이 ㅜㅜ사라져서 못본후 안보고 있어요",
          "speaker_id": "1",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007768.1.35",
          "form": "네 그런가봐요",
          "original_form": "네 그런가봐요",
          "speaker_id": "1",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007768.1.36",
          "form": "두작품 남주가 서로 다른데 매력있네요 ㅋㅋ",
          "original_form": "두작품 남주가 서로 다른데 매력있네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007768.1.37",
          "form": "판타지 도 스릴러도  참 잼나요",
          "original_form": "판타지 도 스릴러도  참 잼나요",
          "speaker_id": "1",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007768.1.38",
          "form": "유투브는 주식하고 피지빼는거 내성발톱 관리보며 희열을 느껴요 ㅋㅋ",
          "original_form": "유투브는 주식하고 피지빼는거 내성발톱 관리보며 희열을 느껴요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:31"
        },
        {
          "id": "MDRW2100007768.1.39",
          "form": "ㅎㅎㅎ그런 영상 보다보면 희열이 느껴지죠",
          "original_form": "ㅎㅎㅎ그런 영상 보다보면 희열이 느껴지죠",
          "speaker_id": "2",
          "time": "20210822 10:31"
        },
        {
          "id": "MDRW2100007768.1.40",
          "form": "네 제가 막 같이 뽑고파요",
          "original_form": "네 제가 막 같이 뽑고파요",
          "speaker_id": "1",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007768.1.41",
          "form": "유튜브는 또 영상 한두개 보다보면",
          "original_form": "유튜브는 또 영상 한두개 보다보면",
          "speaker_id": "2",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007768.1.42",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007768.1.43",
          "form": "그런 시리즈로 계속 추천해주잖아요",
          "original_form": "그런 시리즈로 계속 추천해주잖아요",
          "speaker_id": "2",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007768.1.44",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007768.1.45",
          "form": "그래서 그런것만봐요 ㅋㅋ",
          "original_form": "그래서 그런것만봐요 ㅋㅋ",
          "speaker_id": "1",
