      "utterance": [
        {
          "id": "MDRW2100039382.1.1",
          "form": "책을 읽으면  단어의 의미나",
          "original_form": "책을 읽으면  단어의 의미나",
          "speaker_id": "2",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100039382.1.2",
          "form": "문장의 구조 이런거를 이해못한다대",
          "original_form": "문장의 구조 이런거를 이해못한다대",
          "speaker_id": "2",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100039382.1.3",
          "form": "갑자기 그 글 보니까",
          "original_form": "갑자기 그 글 보니까",
          "speaker_id": "2",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100039382.1.4",
          "form": "난독증 되는거 아닌가 싶어서 윽시 겁나는거야",
          "original_form": "난독증 되는거 아닌가 싶어서 윽시 겁나는거야",
          "speaker_id": "2",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100039382.1.5",
          "form": "아직 어리잖아요ㅠ",
          "original_form": "아직 어리잖아요ㅠ",
          "speaker_id": "1",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100039382.1.6",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100039382.1.7",
          "form": "엄마가 노력해야 되는구만요~",
          "original_form": "엄마가 노력해야 되는구만요~",
          "speaker_id": "1",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100039382.1.8",
          "form": "어 이제부터라도",
          "original_form": "어 이제부터라도",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039382.1.9",
          "form": "영상을 덜 보여주고",
          "original_form": "영상을 덜 보여주고",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039382.1.10",
          "form": "책을 좀 읽어주려고",
          "original_form": "책을 좀 읽어주려고",
          "speaker_id": "2",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100039382.1.11",
          "form": "대화할 때 뭔가 부족함을 늘 느낀다",
          "original_form": "대화할 때 뭔가 부족함을 늘 느낀다",
          "speaker_id": "2",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100039382.1.12",
          "form": "언닌 잘 할수 있을꺼여요~",
          "original_form": "언닌 잘 할수 있을꺼여요~",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039382.1.13",
          "form": "화이팅 입니데이~",
          "original_form": "화이팅 입니데이~",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039382.1.14",
          "form": "우리 둘찌는.짜증이 많이내서..나도 요즘 유튜브 자제 시키고있으요~",
          "original_form": "우리 둘찌는.짜증이 많이내서..나도 요즘 유튜브 자제 시키고있으요~",
          "speaker_id": "1",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100039382.1.15",
          "form": "도움되는기 없니여..",
          "original_form": "도움되는기 없니여..",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039382.1.16",
          "form": "보다가 안 보여주면",
          "original_form": "보다가 안 보여주면",
          "speaker_id": "2",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039382.1.17",
          "form": "더 짜증내는 듯",
          "original_form": "더 짜증내는 듯",
          "speaker_id": "2",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039382.1.18",
          "form": "우리 투도 엄청 짜증낸다",
          "original_form": "우리 투도 엄청 짜증낸다",
          "speaker_id": "2",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039382.1.19",
          "form": "오늘은 일찍 잠들었나보네",
          "original_form": "오늘은 일찍 잠들었나보네",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039382.1.20",
          "form": "일하고 있나??",
          "original_form": "일하고 있나??",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100039382.1.21",
          "form": "언니 잘 댕겨와여 ㅋㅋㅋㅋ",
          "original_form": "언니 잘 댕겨와여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:48"
        },
        {
          "id": "MDRW2100039382.1.22",
          "form": "어 오늘 원래 경미언니도 같이 가기로 했는데",
          "original_form": "어 오늘 원래 경미언니도 같이 가기로 했는데",
          "speaker_id": "2",
          "time": "20210904 13:49"
        },
        {
          "id": "MDRW2100039382.1.23",
          "form": "언니 못 온다고 해서",
          "original_form": "언니 못 온다고 해서",
          "speaker_id": "2",
          "time": "20210904 13:49"
        },
        {
          "id": "MDRW2100039382.1.24",
          "form": "수 엄마랑 뻘쭘하게 둘이 같이 가야 된다",
          "original_form": "수 엄마랑 뻘쭘하게 둘이 같이 가야 된다",
          "speaker_id": "2",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100039382.1.25",
          "form": "첫째가 아까 우리 원한테 전화 왔던데",
          "original_form": "첫째가 아까 우리 원한테 전화 왔던데",
          "speaker_id": "2",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100039382.1.26",
          "form": "우리 원이 문자 보냈는데 답이 없다 지금",
          "original_form": "우리 원이 문자 보냈는데 답이 없다 지금",
          "speaker_id": "2",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100039382.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210904 21:41"
        },
        {
          "id": "MDRW2100039382.1.28",
          "form": "아 언니~그거 서원이 엄마가 언니 원 전화번호 거르켜달라고 ㅋㅋㅋㅋㅋ",
          "original_form": "아 언니~그거 서원이 엄마가 언니 원 전화번호 거르켜달라고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 06:42"
        },
        {
          "id": "MDRW2100039382.1.29",
          "form": "우리 첫찌가 찾다가 전화 걸었어요 ㅋㅋㅋㅋ",
          "original_form": "우리 첫찌가 찾다가 전화 걸었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 06:42"
        },
        {
          "id": "MDRW2100039382.1.30",
          "form": "아니 전화기 좀 달라니 끝까지안줌..;;",
          "original_form": "아니 전화기 좀 달라니 끝까지안줌..;;",
          "speaker_id": "1",
          "time": "20210905 06:42"
        },
        {
          "id": "MDRW2100039382.1.31",
          "form": "아 그랬구만 ㅋㅋㅋㅋㅋㅋㄱㄱㄱ",
          "original_form": "아 그랬구만 ㅋㅋㅋㅋㅋㅋㄱㄱㄱ",
          "speaker_id": "2",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100039382.1.32",
          "form": "우린 또 간만에 연락왔다고 좋아했더니\r\nㅋㅋㅋㅋㅋㄱ",
          "original_form": "우린 또 간만에 연락왔다고 좋아했더니\r\nㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100039382.1.33",
          "form": "저녁까지 먹고 대구 오나??",
          "original_form": "저녁까지 먹고 대구 오나??",
          "speaker_id": "2",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100039382.1.34",
          "form": "우리 오늘 오늘도 서원이 만나고 왔다ㅋㅋㅋ",
          "original_form": "우리 오늘 오늘도 서원이 만나고 왔다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100039382.1.35",
          "form": "언니 ㅋㅋㅋㅋㅋ또 너무 자주 노는거 아녀요??",
          "original_form": "언니 ㅋㅋㅋㅋㅋ또 너무 자주 노는거 아녀요??",
          "speaker_id": "1",
          "time": "20210906 08:57"
        },
        {
          "id": "MDRW2100039382.1.36",
          "form": "잘 놀아도 걱정됨 ㅋㅋㅋㅋㅋ",
          "original_form": "잘 놀아도 걱정됨 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 08:58"
        },
        {
          "id": "MDRW2100039382.1.37",
          "form": "그러이 ㅋㅋㅋㅋㄱ",
          "original_form": "그러이 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 10:06"
        },
        {
          "id": "MDRW2100039382.1.38",
          "form": "나도 그래서 일주일에 한번만 놀으라고 했다 ㅋㅋㄱ",
          "original_form": "나도 그래서 일주일에 한번만 놀으라고 했다 ㅋㅋㄱ",
