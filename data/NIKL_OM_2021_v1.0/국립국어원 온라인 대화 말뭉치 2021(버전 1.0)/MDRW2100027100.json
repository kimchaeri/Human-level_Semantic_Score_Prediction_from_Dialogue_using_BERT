      "utterance": [
        {
          "id": "MDRW2100027100.1.1",
          "form": "백신은 맞으셨나요 ㅎㅎ",
          "original_form": "백신은 맞으셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027100.1.2",
          "form": "모더나 1차 맞았어요",
          "original_form": "모더나 1차 맞았어요",
          "speaker_id": "2",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027100.1.3",
          "form": "배신맞으셨어요?",
          "original_form": "배신맞으셨어요?",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027100.1.4",
          "form": "저도 1차까지 맞고 2차 기다리고있네요 ㅎㅎ",
          "original_form": "저도 1차까지 맞고 2차 기다리고있네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027100.1.5",
          "form": "2차는 언제에요?",
          "original_form": "2차는 언제에요?",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027100.1.6",
          "form": "저는 10월 2일이욥",
          "original_form": "저는 10월 2일이욥",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027100.1.7",
          "form": "저는 9월 6일이에요",
          "original_form": "저는 9월 6일이에요",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027100.1.8",
          "form": "담주 월욜",
          "original_form": "담주 월욜",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027100.1.9",
          "form": "얼마 안남아서 좋아요 ㅋㅋ",
          "original_form": "얼마 안남아서 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027100.1.10",
          "form": "와 2차 곧이군요",
          "original_form": "와 2차 곧이군요",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027100.1.11",
          "form": "빨리 맞으셨네요",
          "original_form": "빨리 맞으셨네요",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027100.1.12",
          "form": "백신맞으니 안심돼요ㅡ",
          "original_form": "백신맞으니 안심돼요ㅡ",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027100.1.13",
          "form": "심리적으로요",
          "original_form": "심리적으로요",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027100.1.14",
          "form": "건강이 최고에요",
          "original_form": "건강이 최고에요",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027100.1.15",
          "form": "저는 그 잔여백신 신청한거라서요",
          "original_form": "저는 그 잔여백신 신청한거라서요",
          "speaker_id": "1",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100027100.1.16",
          "form": "사실 이것도 미뤄진거에요ㅜㅜ",
          "original_form": "사실 이것도 미뤄진거에요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100027100.1.17",
          "form": "어떤백신 맞으셨나요",
          "original_form": "어떤백신 맞으셨나요",
          "speaker_id": "2",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100027100.1.18",
          "form": "저는 모더나요 ㅋㅋ",
          "original_form": "저는 모더나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027100.1.19",
          "form": "같네요. 1차는 무난히 지나갔지요?",
          "original_form": "같네요. 1차는 무난히 지나갔지요?",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027100.1.20",
          "form": "저는 별일 없었어요",
          "original_form": "저는 별일 없었어요",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027100.1.21",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027100.1.22",
          "form": "왼팔 붓기 이틀가고",
          "original_form": "왼팔 붓기 이틀가고",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027100.1.23",
          "form": "일주일후 증상이 나왔어요",
          "original_form": "일주일후 증상이 나왔어요",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027100.1.24",
          "form": "림프절이 부어서 겨드랑이통증",
          "original_form": "림프절이 부어서 겨드랑이통증",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027100.1.25",
          "form": "여자에겐 흔한 부작용이래요",
          "original_form": "여자에겐 흔한 부작용이래요",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027100.1.26",
          "form": "앗 저는 솔직히 팔붓기랑 피곤한거 빼고 별증상 없었거든요",
          "original_form": "앗 저는 솔직히 팔붓기랑 피곤한거 빼고 별증상 없었거든요",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027100.1.27",
          "form": "근데 그 부정출혈 있으실수도있어요",
          "original_form": "근데 그 부정출혈 있으실수도있어요",
          "speaker_id": "1",
          "time": "20210902 13:46"
        }
      ]
    }
  ]
