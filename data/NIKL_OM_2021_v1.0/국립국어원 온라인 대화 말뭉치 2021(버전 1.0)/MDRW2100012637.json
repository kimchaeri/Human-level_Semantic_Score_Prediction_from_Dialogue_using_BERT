      "utterance": [
        {
          "id": "MDRW2100012637.1.1",
          "form": "오늘 비가 오락가락해서",
          "original_form": "오늘 비가 오락가락해서",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012637.1.2",
          "form": "가을에도 장마와 태풍 \n넘 비가 많이 오네요",
          "original_form": "가을에도 장마와 태풍 \n넘 비가 많이 오네요",
          "speaker_id": "1",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012637.1.3",
          "form": "우산을 가지고 나갔는데",
          "original_form": "우산을 가지고 나갔는데",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012637.1.4",
          "form": "안쓰고 왔네여",
          "original_form": "안쓰고 왔네여",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012637.1.5",
          "form": "장루산인데 ㅋㅋ무겁고 짐되서 고생했네요",
          "original_form": "장루산인데 ㅋㅋ무겁고 짐되서 고생했네요",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012637.1.6",
          "form": "그렇죠 가을장마",
          "original_form": "그렇죠 가을장마",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012637.1.7",
          "form": "과수원 이런데 ㅠㅣ해 없어야 하는데",
          "original_form": "과수원 이런데 ㅠㅣ해 없어야 하는데",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012637.1.8",
          "form": "전 빨래가 너무 안마르고 집이 습해서 힘들어요",
          "original_form": "전 빨래가 너무 안마르고 집이 습해서 힘들어요",
          "speaker_id": "2",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100012637.1.9",
          "form": "제습기 쓰시나요?",
          "original_form": "제습기 쓰시나요?",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100012637.1.10",
          "form": "전 에어컨 어늘 오랜만에 틀어서 습기 말렸어요",
          "original_form": "전 에어컨 어늘 오랜만에 틀어서 습기 말렸어요",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100012637.1.11",
          "form": "참 장마는 이게 힘들어요",
          "original_form": "참 장마는 이게 힘들어요",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100012637.1.12",
          "form": "비오면 빨래가 넘 마르기 힘들죠",
          "original_form": "비오면 빨래가 넘 마르기 힘들죠",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100012637.1.13",
          "form": "네 말하도 눅눅해요",
          "original_form": "네 말하도 눅눅해요",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100012637.1.14",
          "form": "전 비올때 신발이 젖어서 안마르고 찝찝할때 젤 싫더라구요",
          "original_form": "전 비올때 신발이 젖어서 안마르고 찝찝할때 젤 싫더라구요",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012637.1.15",
          "form": "비를 좀 싫어해요",
          "original_form": "비를 좀 싫어해요",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012637.1.16",
          "form": "냄새도 나서 넘 싫어요",
          "original_form": "냄새도 나서 넘 싫어요",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012637.1.17",
          "form": "맞아요 그 꼬링꼬링한 냄새요 ㅋㅋ",
          "original_form": "맞아요 그 꼬링꼬링한 냄새요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100012637.1.18",
          "form": "전 비 알러지가 있어서 비오는날 진짜 싫어요",
          "original_form": "전 비 알러지가 있어서 비오는날 진짜 싫어요",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012637.1.19",
          "form": "어머 세상에 전 봄에 꽃알러지요",
          "original_form": "어머 세상에 전 봄에 꽃알러지요",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012637.1.20",
          "form": "꽃성분 많은 화장품 못써요",
          "original_form": "꽃성분 많은 화장품 못써요",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012637.1.21",
          "form": "비오는날은 외출금지 하고 있죠",
          "original_form": "비오는날은 외출금지 하고 있죠",
          "speaker_id": "1",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012637.1.22",
          "form": "네 나가지 마세요 비냄새 비려요 ㅜㅜ",
          "original_form": "네 나가지 마세요 비냄새 비려요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012637.1.23",
          "form": "전 비온날 차에 물도 맞아봤네요",
          "original_form": "전 비온날 차에 물도 맞아봤네요",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012637.1.24",
          "form": "그냥 쌩가요",
          "original_form": "그냥 쌩가요",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012637.1.25",
          "form": "그런거 빼면 가을은 시원하고 예쁜 계절이죠",
          "original_form": "그런거 빼면 가을은 시원하고 예쁜 계절이죠",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012637.1.26",
          "form": "단풍으로 산이 ㅎㅎ",
          "original_form": "단풍으로 산이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012637.1.27",
          "form": "그렇죠 비맞으면 두드러기가 엄청크게 올라와서",
          "original_form": "그렇죠 비맞으면 두드러기가 엄청크게 올라와서",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012637.1.28",
          "form": "사람들이 이상하게 쳐다봐요",
          "original_form": "사람들이 이상하게 쳐다봐요",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012637.1.29",
          "form": "아 근데 다들 내가 아니면 몰라요",
          "original_form": "아 근데 다들 내가 아니면 몰라요",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012637.1.30",
          "form": "저도 기침에 얼굴이 부어요",
          "original_form": "저도 기침에 얼굴이 부어요",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012637.1.31",
          "form": "나만아는 고통이죠",
          "original_form": "나만아는 고통이죠",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100012637.1.32",
          "form": "일일이 얘기 할수도 없고 두배로 힘들죠",
          "original_form": "일일이 얘기 할수도 없고 두배로 힘들죠",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012637.1.33",
          "form": "네 해명 힘들어요",
          "original_form": "네 해명 힘들어요",
          "speaker_id": "2",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012637.1.34",
          "form": "코로나에 기침 계속하니 의심받고 ㅋㅋ",
          "original_form": "코로나에 기침 계속하니 의심받고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012637.1.35",
          "form": "참 요샌 여러모로 힘들어요",
          "original_form": "참 요샌 여러모로 힘들어요",
          "speaker_id": "2",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012637.1.36",
          "form": "계절에 딸하 알레르기 완전히 사라짐 좋겠어요",
          "original_form": "계절에 딸하 알레르기 완전히 사라짐 좋겠어요",
          "speaker_id": "2",
          "time": "20210824 23:48"
        }
      ]
