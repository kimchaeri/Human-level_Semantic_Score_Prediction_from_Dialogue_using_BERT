        },
        {
          "id": "MDRW2100042313.1.3",
          "form": "치킨 돌려무야지",
          "original_form": "치킨 돌려무야지",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042313.1.4",
          "form": "자나",
          "original_form": "자나",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042313.1.5",
          "form": "돌려묵는다고?쏜살?",
          "original_form": "돌려묵는다고?쏜살?",
          "speaker_id": "1",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042313.1.6",
          "form": "왓드아",
          "original_form": "왓드아",
          "speaker_id": "1",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042313.1.7",
          "form": "안되네ㅠ",
          "original_form": "안되네ㅠ",
          "speaker_id": "1",
          "time": "20210906 23:27"
        },
        {
          "id": "MDRW2100042313.1.8",
          "form": "아니 닭껌질 튀김",
          "original_form": "아니 닭껌질 튀김",
          "speaker_id": "2",
          "time": "20210906 23:28"
        },
        {
          "id": "MDRW2100042313.1.9",
          "form": "맥주 이번에 종류별로 조금씩 샀으 ㅋㅋ",
          "original_form": "맥주 이번에 종류별로 조금씩 샀으 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:29"
        },
        {
          "id": "MDRW2100042313.1.10",
          "form": "어제산거.지",
          "original_form": "어제산거.지",
          "speaker_id": "2",
          "time": "20210906 23:32"
        },
        {
          "id": "MDRW2100042313.1.11",
          "form": "신랑이 말표 흙맥주 맛있다카드라 ㅋㅋ",
          "original_form": "신랑이 말표 흙맥주 맛있다카드라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:32"
        },
        {
          "id": "MDRW2100042313.1.12",
          "form": "흑맥주 먹고싶다ㅠㅋㅋ",
          "original_form": "흑맥주 먹고싶다ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:37"
        },
        {
          "id": "MDRW2100042313.1.13",
          "form": "술2주간 금지래",
          "original_form": "술2주간 금지래",
          "speaker_id": "1",
          "time": "20210906 23:38"
        },
        {
          "id": "MDRW2100042313.1.14",
          "form": "2주맞나여튼ㅋ",
          "original_form": "2주맞나여튼ㅋ",
          "speaker_id": "1",
          "time": "20210906 23:38"
        },
        {
          "id": "MDRW2100042313.1.15",
          "form": "응 안무야지 혹시 모르니깐 ㅎㅎ",
          "original_form": "응 안무야지 혹시 모르니깐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 23:41"
        },
        {
          "id": "MDRW2100042313.1.16",
          "form": "응 그래서 참아야해ㅠㅠ",
          "original_form": "응 그래서 참아야해ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100042313.1.17",
          "form": "응.ㅋㅋ 벌써 12시네 ㅎㅎ",
          "original_form": "응.ㅋㅋ 벌써 12시네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100042313.1.18",
          "form": "또 자야할시간이네",
          "original_form": "또 자야할시간이네",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042313.1.19",
          "form": "난 이제 시작인데ㅠ",
          "original_form": "난 이제 시작인데ㅠ",
          "speaker_id": "1",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042313.1.20",
          "form": "그래도 요즘 좀 늦게 자네 ㅎㅎ",
          "original_form": "그래도 요즘 좀 늦게 자네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042313.1.21",
          "form": "다시 패턴이 조금 돌아온듯",
