        },
        {
          "id": "MDRW2100001969.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001969.1.3",
          "form": "혹시 직업이 어떻게 되세요?",
          "original_form": "혹시 직업이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001969.1.4",
          "form": "저는 초등학생 가르치고있어요ㅎ",
          "original_form": "저는 초등학생 가르치고있어요ㅎ",
          "speaker_id": "2",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001969.1.5",
          "form": "님은 어떤 일 하시나요",
          "original_form": "님은 어떤 일 하시나요",
          "speaker_id": "2",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001969.1.6",
          "form": "전 아직 19살이라",
          "original_form": "전 아직 19살이라",
          "speaker_id": "1",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100001969.1.7",
          "form": "수능 준비중이에요..",
          "original_form": "수능 준비중이에요..",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001969.1.8",
          "form": "학생이시군요~지금은 힘들지만 좋은결과 있을거에요",
          "original_form": "학생이시군요~지금은 힘들지만 좋은결과 있을거에요",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100001969.1.9",
          "form": "근데 초등학생 가르치시면 힘들지 않으세요?",
          "original_form": "근데 초등학생 가르치시면 힘들지 않으세요?",
          "speaker_id": "1",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100001969.1.10",
          "form": "네 힘들기도하고 보람있을때도 많구요",
          "original_form": "네 힘들기도하고 보람있을때도 많구요",
          "speaker_id": "2",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001969.1.11",
          "form": "초등학교 저학년이 힘들어요 고학년이 힘들어요?",
          "original_form": "초등학교 저학년이 힘들어요 고학년이 힘들어요?",
          "speaker_id": "1",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100001969.1.12",
          "form": "성향에 따라다른데 저는 고학년이 더 힘들더라구요ㅎㅎ",
          "original_form": "성향에 따라다른데 저는 고학년이 더 힘들더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001969.1.13",
          "form": "저학년은 아기들같아서 기본습관같은거(앉아라 책꺼내라 등등)끊임없이 반복해서 말해야하는게 어렵구요",
          "original_form": "저학년은 아기들같아서 기본습관같은거(앉아라 책꺼내라 등등)끊임없이 반복해서 말해야하는게 어렵구요",
          "speaker_id": "2",
          "time": "20210819 22:57"
        },
        {
          "id": "MDRW2100001969.1.14",
          "form": "고학년은 수업시수가 많고 감정이 섬세한 시기다보니 그런부분 지도하는게 어려울때가 있어요~",
          "original_form": "고학년은 수업시수가 많고 감정이 섬세한 시기다보니 그런부분 지도하는게 어려울때가 있어요~",
          "speaker_id": "2",
          "time": "20210819 22:58"
        },
        {
          "id": "MDRW2100001969.1.15",
          "form": "저는 중학교 남중나왔는데",
          "original_form": "저는 중학교 남중나왔는데",
          "speaker_id": "1",
          "time": "20210819 22:59"
        },
        {
          "id": "MDRW2100001969.1.16",
          "form": "선생님이 참 힘드실거같더라구요..",
          "original_form": "선생님이 참 힘드실거같더라구요..",
          "speaker_id": "1",
          "time": "20210819 22:59"
        },
        {
          "id": "MDRW2100001969.1.17",
          "form": "그런생각 해주다니 기특하고 고맙네요^^ 마음 예쁜학생인 것 같아요",
          "original_form": "그런생각 해주다니 기특하고 고맙네요^^ 마음 예쁜학생인 것 같아요",
          "speaker_id": "2",
          "time": "20210819 23:02"
        },
        {
          "id": "MDRW2100001969.1.18",
          "form": "교권 바닥이라는 말 많던데 고등학교는 남녀공학이라 덜한데 남중때는 확실히 힘들거같더라구요..",
          "original_form": "교권 바닥이라는 말 많던데 고등학교는 남녀공학이라 덜한데 남중때는 확실히 힘들거같더라구요..",
          "speaker_id": "1",
          "time": "20210819 23:03"
        }
