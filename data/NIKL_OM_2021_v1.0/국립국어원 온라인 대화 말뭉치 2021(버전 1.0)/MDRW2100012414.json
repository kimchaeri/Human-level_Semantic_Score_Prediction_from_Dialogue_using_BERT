      "utterance": [
        {
          "id": "MDRW2100012414.1.1",
          "form": "새콤매콤한 수프같아요",
          "original_form": "새콤매콤한 수프같아요",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012414.1.2",
          "form": "밥 말아먹어도 좋고요",
          "original_form": "밥 말아먹어도 좋고요",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012414.1.3",
          "form": "음~~ 안에 건더기는 뭐가 들어가요?",
          "original_form": "음~~ 안에 건더기는 뭐가 들어가요?",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012414.1.4",
          "form": "새우라든지 내용물이 푸짐해서 그것만 먹기도?",
          "original_form": "새우라든지 내용물이 푸짐해서 그것만 먹기도?",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012414.1.5",
          "form": "새우 버섯 채소 이런 것들이요",
          "original_form": "새우 버섯 채소 이런 것들이요",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012414.1.6",
          "form": "디폴트는 새우인 듯ㅎ",
          "original_form": "디폴트는 새우인 듯ㅎ",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012414.1.7",
          "form": "아~ 감자 뭐 이런거 들어가나요?",
          "original_form": "아~ 감자 뭐 이런거 들어가나요?",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012414.1.8",
          "form": "새우. . 통새우... ",
          "original_form": "새우. . 통새우... ",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012414.1.9",
          "form": "감자는 안 들어가요",
          "original_form": "감자는 안 들어가요",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012414.1.10",
          "form": "네네 통새우요",
          "original_form": "네네 통새우요",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012414.1.11",
          "form": "살짝 새콤해서 호불호 갈릴수도 있는데",
          "original_form": "살짝 새콤해서 호불호 갈릴수도 있는데",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012414.1.12",
          "form": "입맛에 맞으면 천국",
          "original_form": "입맛에 맞으면 천국",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012414.1.13",
          "form": "따뜻하게 먹는 음식이죠?",
          "original_form": "따뜻하게 먹는 음식이죠?",
          "speaker_id": "1",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012414.1.14",
          "form": "약간 맛이 상상이가요 ㅎㅎㅎ",
          "original_form": "약간 맛이 상상이가요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012414.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012414.1.16",
          "form": "맛있을것 같아용 감칠맛나게 새콤할것 같은!!",
          "original_form": "맛있을것 같아용 감칠맛나게 새콤할것 같은!!",
          "speaker_id": "1",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012414.1.17",
          "form": "ㅎㅎ 기회되면 꼭 드세요",
          "original_form": "ㅎㅎ 기회되면 꼭 드세요",
          "speaker_id": "2",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012414.1.18",
          "form": "요샌 태국 요리점도 많으니",
          "original_form": "요샌 태국 요리점도 많으니",
          "speaker_id": "2",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012414.1.19",
          "form": "태국은 똠양꿍! 기억하겠습니다 ㅋㅋㅋ",
          "original_form": "태국은 똠양꿍! 기억하겠습니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012414.1.20",
          "form": "요즘은 진짜 다양한 나라음식을 쉽게 먹을 수 있는것 같아요~~",
          "original_form": "요즘은 진짜 다양한 나라음식을 쉽게 먹을 수 있는것 같아요~~",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012414.1.21",
          "form": "넵 ㅎ 단거 좋아하면 길거리 음식은 로띠",
          "original_form": "넵 ㅎ 단거 좋아하면 길거리 음식은 로띠",
          "speaker_id": "2",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012414.1.22",
          "form": "베트남음식점은 뭐 동네마다 기본이구",
          "original_form": "베트남음식점은 뭐 동네마다 기본이구",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012414.1.23",
          "form": "로띠?",
          "original_form": "로띠?",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012414.1.24",
          "form": "제입맛에는 달았지만 입맛에 맞으실듯요",
          "original_form": "제입맛에는 달았지만 입맛에 맞으실듯요",
          "speaker_id": "2",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012414.1.25",
          "form": "로티보이 번 생각나네요 ㅋㅋㅋㅋ 무슨음식이에요?",
          "original_form": "로티보이 번 생각나네요 ㅋㅋㅋㅋ 무슨음식이에요?",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012414.1.26",
          "form": "그냥 팬케이크에 바나나 초코크림 이느낌?",
          "original_form": "그냥 팬케이크에 바나나 초코크림 이느낌?",
          "speaker_id": "2",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012414.1.27",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012414.1.28",
          "form": "아아랑 먹으면 딱일듯!!",
          "original_form": "아아랑 먹으면 딱일듯!!",
          "speaker_id": "1",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012414.1.29",
          "form": "팬케이크가 아니고 크레페 비슷하겠네요",
          "original_form": "팬케이크가 아니고 크레페 비슷하겠네요",
          "speaker_id": "2",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012414.1.30",
          "form": "네 달고...달아요",
          "original_form": "네 달고...달아요",
          "speaker_id": "2",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012414.1.31",
          "form": "크레페!!",
          "original_form": "크레페!!",
          "speaker_id": "1",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012414.1.32",
          "form": "그렇다면 제 입에도 달것 같아요ㅜ",
          "original_form": "그렇다면 제 입에도 달것 같아요ㅜ",
