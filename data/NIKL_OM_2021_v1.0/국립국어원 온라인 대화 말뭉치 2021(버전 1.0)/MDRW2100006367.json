      "utterance": [
        {
          "id": "MDRW2100006367.1.1",
          "form": "맛집 투어 다니시나요?",
          "original_form": "맛집 투어 다니시나요?",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006367.1.2",
          "form": "투어는 아닌데 찾아보긴해요",
          "original_form": "투어는 아닌데 찾아보긴해요",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006367.1.3",
          "form": "만족하시나요 보토으",
          "original_form": "만족하시나요 보토으",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006367.1.4",
          "form": "보통??",
          "original_form": "보통??",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006367.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006367.1.6",
          "form": "인스타맛집은 보러가고",
          "original_form": "인스타맛집은 보러가고",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006367.1.7",
          "form": "그주에 맛있는집도있고",
          "original_form": "그주에 맛있는집도있고",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006367.1.8",
          "form": "저는.네이버리뷰보고가는데.실패가 대부분이었어요",
          "original_form": "저는.네이버리뷰보고가는데.실패가 대부분이었어요",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006367.1.9",
          "form": "아.. 네이버는 광고가 많아서 많이 비교해야해호",
          "original_form": "아.. 네이버는 광고가 많아서 많이 비교해야해호",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100006367.1.10",
          "form": "그 영수증 리뷰가 있길래 믿고갔거든요 ㅠㅠ",
          "original_form": "그 영수증 리뷰가 있길래 믿고갔거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006367.1.11",
          "form": "지원받는곳많어서.. 사진같은거나 메뉴 잘 비교해야 해요",
          "original_form": "지원받는곳많어서.. 사진같은거나 메뉴 잘 비교해야 해요",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006367.1.12",
          "form": "그렇군요 인스타.맛집은 믿을만한가요",
          "original_form": "그렇군요 인스타.맛집은 믿을만한가요",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100006367.1.13",
          "form": "쉽지않아요..ㅋㅋㅋㅋ 그것도 비교해야해옷",
          "original_form": "쉽지않아요..ㅋㅋㅋㅋ 그것도 비교해야해옷",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006367.1.14",
          "form": "그렇군요..이러하니 늘 가던 곳에 가는게 맘편하더라구요 ㅎㅎ",
          "original_form": "그렇군요..이러하니 늘 가던 곳에 가는게 맘편하더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100006367.1.15",
          "form": "그게 편하긴 하죠 ㅎㅎ",
          "original_form": "그게 편하긴 하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100006367.1.16",
          "form": "좋았던곳 있으실까요? 가보신곳중에?",
          "original_form": "좋았던곳 있으실까요? 가보신곳중에?",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100006367.1.17",
          "form": "저는 그냥 동네맛집이요ㅎㅎ 칼국수만두먹으러",
          "original_form": "저는 그냥 동네맛집이요ㅎㅎ 칼국수만두먹으러",
          "speaker_id": "1",
          "time": "20210908 00:22"
        }
      ]
