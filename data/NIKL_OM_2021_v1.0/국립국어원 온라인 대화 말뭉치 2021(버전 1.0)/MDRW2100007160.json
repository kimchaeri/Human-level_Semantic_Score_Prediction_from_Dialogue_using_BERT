      "utterance": [
        {
          "id": "MDRW2100007160.1.1",
          "form": "저도 맵찔이긴 한데",
          "original_form": "저도 맵찔이긴 한데",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007160.1.2",
          "form": "매운 거 잘 도전해요",
          "original_form": "매운 거 잘 도전해요",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007160.1.3",
          "form": "근데 유독 엽떡은 맵더라고요",
          "original_form": "근데 유독 엽떡은 맵더라고요",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007160.1.4",
          "form": "착한 맛 그 위로는... 아예 한입도 못 먹겠어요ㅋㅋ쿠ㅠㅠ",
          "original_form": "착한 맛 그 위로는... 아예 한입도 못 먹겠어요ㅋㅋ쿠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007160.1.5",
          "form": "맞아요 엽떡이 진짜 매워요..ㅠ",
          "original_form": "맞아요 엽떡이 진짜 매워요..ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007160.1.6",
          "form": "신전떡볶이도 좋아하세요?",
          "original_form": "신전떡볶이도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007160.1.7",
          "form": "네!! 신전 너무 좋아하죠ㅠ",
          "original_form": "네!! 신전 너무 좋아하죠ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007160.1.8",
          "form": "신전 시키실때 김밥이랑 오뎅튀김도 드세요??",
          "original_form": "신전 시키실때 김밥이랑 오뎅튀김도 드세요??",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007160.1.9",
          "form": "저는 꼭 시켜요 ㅠㅠ",
          "original_form": "저는 꼭 시켜요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007160.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007160.1.11",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007160.1.12",
          "form": "먹을 줄 아시네요",
          "original_form": "먹을 줄 아시네요",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007160.1.13",
          "form": "먹잘알~!!",
          "original_form": "먹잘알~!!",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007160.1.14",
          "form": "그쵸 쫌 통하시네요 먹잘알 ..~",
          "original_form": "그쵸 쫌 통하시네요 먹잘알 ..~",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007160.1.15",
          "form": "김밥이 진짜 너무 맛있어서 비법 좀 알고싶어요",
          "original_form": "김밥이 진짜 너무 맛있어서 비법 좀 알고싶어요",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007160.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007160.1.17",
          "form": "진짜요ㅜ",
          "original_form": "진짜요ㅜ",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007160.1.18",
          "form": "신전치즈김밥",
          "original_form": "신전치즈김밥",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007160.1.19",
          "form": "그렇게 맛있는 김밥 없어요 ...",
          "original_form": "그렇게 맛있는 김밥 없어요 ...",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007160.1.20",
          "form": "그니까요... 왜이렇게 맛있을까요..?",
          "original_form": "그니까요... 왜이렇게 맛있을까요..?",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007160.1.21",
          "form": "가끔 그것만 먹고싶은 날도 있었어요..",
          "original_form": "가끔 그것만 먹고싶은 날도 있었어요..",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007160.1.22",
          "form": "맞아요 떡볶이보다 김밥이 더 먹고 싶을 때 있어요...",
          "original_form": "맞아요 떡볶이보다 김밥이 더 먹고 싶을 때 있어요...",
          "speaker_id": "1",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100007160.1.23",
          "form": "신전만의 비법...",
          "original_form": "신전만의 비법...",
          "speaker_id": "1",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100007160.1.24",
          "form": "맞아요 그 김치랑 ..무슨 양념인지는 모르겠지만 너무 맛있어요",
          "original_form": "맞아요 그 김치랑 ..무슨 양념인지는 모르겠지만 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210822 15:19"
        },
        {
          "id": "MDRW2100007160.1.25",
          "form": "점심 뭐 드셨나요?",
          "original_form": "점심 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210822 15:19"
        },
        {
          "id": "MDRW2100007160.1.26",
          "form": "저... 놀랍게도 방금 일어나서 아직 밥을 안 먹었어요",
          "original_form": "저... 놀랍게도 방금 일어나서 아직 밥을 안 먹었어요",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100007160.1.27",
          "form": "드시면 점심겸 저녁이 되시겠네요..",
          "original_form": "드시면 점심겸 저녁이 되시겠네요..",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100007160.1.28",
          "form": "생각해두신 메뉴있으세요?",
          "original_form": "생각해두신 메뉴있으세요?",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100007160.1.29",
          "form": "그렇죠... 볶음밥 먹고 왔어요",
          "original_form": "그렇죠... 볶음밥 먹고 왔어요",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100007160.1.30",
          "form": "저도 점심 볶음밥 먹었어요 ㅎㅎ 간단한데 맛있어요~",
          "original_form": "저도 점심 볶음밥 먹었어요 ㅎㅎ 간단한데 맛있어요~",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100007160.1.31",
          "form": "오 저희 통했군요!ㅎㅎㅎ",
          "original_form": "오 저희 통했군요!ㅎㅎㅎ",
