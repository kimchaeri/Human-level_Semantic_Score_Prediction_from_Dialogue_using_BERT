      "utterance": [
        {
          "id": "MDRW2100032315.1.1",
          "form": "아니 그냥 공부하기 싫어서",
          "original_form": "아니 그냥 공부하기 싫어서",
          "speaker_id": "2",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032315.1.2",
          "form": "아하... 왜?",
          "original_form": "아하... 왜?",
          "speaker_id": "1",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032315.1.3",
          "form": "별로 재미가 없닪아",
          "original_form": "별로 재미가 없닪아",
          "speaker_id": "2",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032315.1.4",
          "form": "그렇구나... 수업도 잘 안들어?",
          "original_form": "그렇구나... 수업도 잘 안들어?",
          "speaker_id": "1",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032315.1.5",
          "form": "아니 슈업은 들으려고 해",
          "original_form": "아니 슈업은 들으려고 해",
          "speaker_id": "2",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032315.1.6",
          "form": "학교에서 자주 졸아?",
          "original_form": "학교에서 자주 졸아?",
          "speaker_id": "1",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032315.1.7",
          "form": "아니 안 졸려고 노력",
          "original_form": "아니 안 졸려고 노력",
          "speaker_id": "2",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032315.1.8",
          "form": "오오 그건 좀 대단한데",
          "original_form": "오오 그건 좀 대단한데",
          "speaker_id": "1",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032315.1.9",
          "form": "공부잘해?",
          "original_form": "공부잘해?",
          "speaker_id": "1",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032315.1.10",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032315.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032315.1.12",
          "form": "그냥 물어봤어",
          "original_form": "그냥 물어봤어",
          "speaker_id": "1",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032315.1.13",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032315.1.14",
