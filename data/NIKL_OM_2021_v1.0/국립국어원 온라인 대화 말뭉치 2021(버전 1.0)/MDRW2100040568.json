        {
          "id": "MDRW2100040568.1.2",
          "form": "집에 에어프라이어기 있잖아",
          "original_form": "집에 에어프라이어기 있잖아",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040568.1.3",
          "form": "시간 오래 걸린다",
          "original_form": "시간 오래 걸린다",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040568.1.4",
          "form": "그건그렇다 ㅎ",
          "original_form": "그건그렇다 ㅎ",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040568.1.5",
          "form": "설겆이 꺼리도 많이 나옴",
          "original_form": "설겆이 꺼리도 많이 나옴",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
