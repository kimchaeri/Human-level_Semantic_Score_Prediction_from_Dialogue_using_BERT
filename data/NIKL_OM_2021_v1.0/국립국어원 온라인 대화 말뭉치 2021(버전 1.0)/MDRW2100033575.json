      "utterance": [
        {
          "id": "MDRW2100033575.1.1",
          "form": "요즘 보신영화 있으신가요",
          "original_form": "요즘 보신영화 있으신가요",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100033575.1.2",
          "form": "영화 안본지 좀 되었네요",
          "original_form": "영화 안본지 좀 되었네요",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100033575.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100033575.1.4",
          "form": "저도 한동안안보다 얼마전 모가디슈봤어요",
          "original_form": "저도 한동안안보다 얼마전 모가디슈봤어요",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100033575.1.5",
          "form": "어떤 영화인가요?",
          "original_form": "어떤 영화인가요?",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100033575.1.6",
          "form": "실화바탕인데 소말리아  내전터져서 남북한 대사들 탈출하는 스토리요",
          "original_form": "실화바탕인데 소말리아  내전터져서 남북한 대사들 탈출하는 스토리요",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100033575.1.7",
          "form": "오 한번 볼게요",
          "original_form": "오 한번 볼게요",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100033575.1.8",
          "form": "탈출하니까 생각나는건데 쇼생크탈출 보셨나요?",
          "original_form": "탈출하니까 생각나는건데 쇼생크탈출 보셨나요?",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100033575.1.9",
          "form": "네 어릴적에 봤었죠ㅎㅅㄴ",
          "original_form": "네 어릴적에 봤었죠ㅎㅅㄴ",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100033575.1.10",
          "form": "톰행크스랑",
          "original_form": "톰행크스랑",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100033575.1.11",
          "form": "모건프리먼인가요?",
          "original_form": "모건프리먼인가요?",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100033575.1.12",
          "form": "팀 로빈스랑 모건프리먼이요",
          "original_form": "팀 로빈스랑 모건프리먼이요",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100033575.1.13",
          "form": "재밌게봤던 영화중 하나네요",
          "original_form": "재밌게봤던 영화중 하나네요",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100033575.1.14",
          "form": "아 팀로빈스  저도그건 잼나게 본거같아요",
          "original_form": "아 팀로빈스  저도그건 잼나게 본거같아요",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100033575.1.15",
          "form": "좋아하는 영화감독 있으신가요?",
          "original_form": "좋아하는 영화감독 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 15:34"
        },
        {
          "id": "MDRW2100033575.1.16",
          "form": "감독은 잘몰라서 ㅎ",
          "original_form": "감독은 잘몰라서 ㅎ",
          "speaker_id": "1",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100033575.1.17",
          "form": "주로 흥행하는 영화봐요",
          "original_form": "주로 흥행하는 영화봐요",
          "speaker_id": "1",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100033575.1.18",
          "form": "콘서트 가보신적있으신가요?",
          "original_form": "콘서트 가보신적있으신가요?",
          "speaker_id": "2",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100033575.1.19",
          "form": "친구가 티켓당첨돼서 힙합콘서트가봤어요",
          "original_form": "친구가 티켓당첨돼서 힙합콘서트가봤어요",
          "speaker_id": "1",
          "time": "20210824 15:37"
        }
