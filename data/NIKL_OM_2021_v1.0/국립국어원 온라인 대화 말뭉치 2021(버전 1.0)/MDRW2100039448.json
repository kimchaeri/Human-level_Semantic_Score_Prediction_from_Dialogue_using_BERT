        },
        {
          "id": "MDRW2100039448.1.3",
          "form": "백순대 사랑합니댜",
          "original_form": "백순대 사랑합니댜",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039448.1.4",
          "form": "아예 곱창 없는거???",
          "original_form": "아예 곱창 없는거???",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039448.1.5",
          "form": "신림 곱창타운st???",
          "original_form": "신림 곱창타운st???",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039448.1.6",
          "form": "신림 곱창타운있??",
          "original_form": "신림 곱창타운있??",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039448.1.7",
          "form": "순대타운 아닌가오!!!",
          "original_form": "순대타운 아닌가오!!!",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039448.1.8",
          "form": "아 아니다",
          "original_form": "아 아니다",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039448.1.9",
          "form": "거기에 곱창 들어가네 죄송합니다...",
          "original_form": "거기에 곱창 들어가네 죄송합니다...",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039448.1.10",
          "form": "순대만 순간 기억함ㅠㅠㅋ",
          "original_form": "순대만 순간 기억함ㅠㅠㅋ",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039448.1.11",
          "form": "아 순대타운 ㅇㅇㅇ",
          "original_form": "아 순대타운 ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039448.1.12",
          "form": "아냐 순대타운이 맞음ㅋㅋㅋㅋㅋㅋㅋㅋ곰창타운은 무슨 혼종인가 ㅋㅋㅋㅋ",
          "original_form": "아냐 순대타운이 맞음ㅋㅋㅋㅋㅋㅋㅋㅋ곰창타운은 무슨 혼종인가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039448.1.13",
          "form": "백순대 먹고싶다",
          "original_form": "백순대 먹고싶다",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039448.1.14",
          "form": "깻잎에 당면넣고",
          "original_form": "깻잎에 당면넣고",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039448.1.15",
          "form": "ㅎ ㅏ 신림 오세오",
          "original_form": "ㅎ ㅏ 신림 오세오",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039448.1.16",
          "form": "백순대의 묘미는 기름에 튀겨진 쫄면이죠",
          "original_form": "백순대의 묘미는 기름에 튀겨진 쫄면이죠",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039448.1.17",
          "form": "아 너무 좋아오",
          "original_form": "아 너무 좋아오",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039448.1.18",
          "form": "진짜 기름에 튀긴 쫄면2222222",
          "original_form": "진짜 기름에 튀긴 쫄면2222222",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039448.1.19",
          "form": "살짝 바삭한 그것",
          "original_form": "살짝 바삭한 그것",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039448.1.20",
          "form": "야채 많이 넣어도 맛나구욧",
          "original_form": "야채 많이 넣어도 맛나구욧",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039448.1.21",
          "form": "그리고 그 먹기전에 주시는 간도 쫀맛이죠",
          "original_form": "그리고 그 먹기전에 주시는 간도 쫀맛이죠",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039448.1.22",
          "form": "진짜 고소해",
          "original_form": "진짜 고소해",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039448.1.23",
          "form": "아 간을 주던가????",
          "original_form": "아 간을 주던가????",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039448.1.24",
          "form": "와 왜 기억이 안나지ㅠㅠㅠㅠ억울하다",
          "original_form": "와 왜 기억이 안나지ㅠㅠㅠㅠ억울하다",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039448.1.25",
          "form": "삶은 간?",
          "original_form": "삶은 간?",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039448.1.26",
          "form": "네넹 그 순대에 같이 나오는 간이오",
          "original_form": "네넹 그 순대에 같이 나오는 간이오",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039448.1.27",
          "form": "그거 진짜 고소합니댜",
          "original_form": "그거 진짜 고소합니댜",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039448.1.28",
          "form": "아 ㅇ0ㅇ",
          "original_form": "아 ㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039448.1.29",
          "form": "나 그것도 순대에 볶아먹은 것 같아",
          "original_form": "나 그것도 순대에 볶아먹은 것 같아",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039448.1.30",
          "form": "헐 맛잘알",
          "original_form": "헐 맛잘알",
          "speaker_id": "2",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100039448.1.31",
          "form": "와 뭐냐",
          "original_form": "와 뭐냐",
          "speaker_id": "2",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100039448.1.32",
          "form": "그생각은 못했네",
          "original_form": "그생각은 못했네",
          "speaker_id": "2",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100039448.1.33",
          "form": "기름에 튀깁시다",
          "original_form": "기름에 튀깁시다",
          "speaker_id": "1",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100039448.1.34",
          "form": "아 순대가 먹고싶네요",
          "original_form": "아 순대가 먹고싶네요",
          "speaker_id": "1",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100039448.1.35",
          "form": "저녁 메뉴 무엇?",
          "original_form": "저녁 메뉴 무엇?",
          "speaker_id": "1",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100039448.1.36",
          "form": "카레라이스 임니댜",
          "original_form": "카레라이스 임니댜",
          "speaker_id": "2",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100039448.1.37",
          "form": "그거랑 부침개 반죽 남은거 굽굽할거에오",
          "original_form": "그거랑 부침개 반죽 남은거 굽굽할거에오",
          "speaker_id": "2",
          "time": "20210903 18:08"
        },
        {
          "id": "MDRW2100039448.1.38",
          "form": "님은요??",
          "original_form": "님은요??",
          "speaker_id": "2",
          "time": "20210903 18:08"
        },
        {
          "id": "MDRW2100039448.1.39",
          "form": "오 부침개 ㅇ0ㅇ",
          "original_form": "오 부침개 ㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210903 18:13"
        },
        {
          "id": "MDRW2100039448.1.40",
          "form": "저는 메밀국수 먹을거에요 농심거",
