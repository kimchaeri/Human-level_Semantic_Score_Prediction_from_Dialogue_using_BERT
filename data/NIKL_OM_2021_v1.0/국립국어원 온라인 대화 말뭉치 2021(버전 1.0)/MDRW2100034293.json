      "utterance": [
        {
          "id": "MDRW2100034293.1.1",
          "form": "안녕하세요 요즘 비가 많이 오네요",
          "original_form": "안녕하세요 요즘 비가 많이 오네요",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034293.1.2",
          "form": "여름 내내 안오던 비가 몰아서 오나봐요",
          "original_form": "여름 내내 안오던 비가 몰아서 오나봐요",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034293.1.3",
          "form": "그러게요 너무 습하죠 ㅠ",
          "original_form": "그러게요 너무 습하죠 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034293.1.4",
          "form": "네 밖은 시원한데 실내는 습해서 덥네요ㅠ",
          "original_form": "네 밖은 시원한데 실내는 습해서 덥네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034293.1.5",
          "form": "저 그래서 에어컨 틀었잖아요",
          "original_form": "저 그래서 에어컨 틀었잖아요",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034293.1.6",
          "form": "밖은 시원한데두 ㅠㅠ",
          "original_form": "밖은 시원한데두 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034293.1.7",
          "form": "저두요! 이럴때 틀기 아깝긴한데 ...",
          "original_form": "저두요! 이럴때 틀기 아깝긴한데 ...",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034293.1.8",
          "form": "너무 습해서ㅠㅠ",
          "original_form": "너무 습해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034293.1.9",
          "form": "이럴땐 제습기 사고싶어져요",
          "original_form": "이럴땐 제습기 사고싶어져요",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034293.1.10",
          "form": "아 제습기 없어서 아쉽네요",
          "original_form": "아 제습기 없어서 아쉽네요",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034293.1.11",
          "form": "비오는날 좋아하세요??",
          "original_form": "비오는날 좋아하세요??",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034293.1.12",
          "form": "실내에 있을때는 좋아하는편이예요",
          "original_form": "실내에 있을때는 좋아하는편이예요",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034293.1.13",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034293.1.14",
          "form": "오 저는 양말젖고 우산무겁고해서 ㅠ 어두운것도 안좋아해서 ㅂㄹ예요 ㅠ",
          "original_form": "오 저는 양말젖고 우산무겁고해서 ㅠ 어두운것도 안좋아해서 ㅂㄹ예요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034293.1.15",
          "form": "빨리 가을이나 왔음 좋겠네요",
          "original_form": "빨리 가을이나 왔음 좋겠네요",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034293.1.16",
          "form": "그러시구나 가을 좋아하시나요?",
          "original_form": "그러시구나 가을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034293.1.17",
          "form": "네 전봄가을 좋아해요",
          "original_form": "네 전봄가을 좋아해요",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034293.1.18",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034293.1.19",
          "form": "저는 가을이 제일좋구요 그다음이 봄이요",
          "original_form": "저는 가을이 제일좋구요 그다음이 봄이요",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034293.1.20",
          "form": "오 빨리 긴팔입고싶지않나요??",
          "original_form": "오 빨리 긴팔입고싶지않나요??",
