      "utterance": [
        {
          "id": "MDRW2100022966.1.1",
          "form": "치타가 괜찮겠네요..",
          "original_form": "치타가 괜찮겠네요..",
          "speaker_id": "2",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100022966.1.2",
          "form": "치타는 그냥 고양이 수준이라.. 전투력도 약하고..",
          "original_form": "치타는 그냥 고양이 수준이라.. 전투력도 약하고..",
          "speaker_id": "2",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100022966.1.3",
          "form": "엥 치타가 전투력이 약하군요?",
          "original_form": "엥 치타가 전투력이 약하군요?",
          "speaker_id": "1",
          "time": "20210829 19:48"
        },
        {
          "id": "MDRW2100022966.1.4",
          "form": "빠른것만 알고있었는데 ㅋㅋㅋ",
          "original_form": "빠른것만 알고있었는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:48"
        },
        {
          "id": "MDRW2100022966.1.5",
          "form": "치타는 키우는 사람 엄처우많아요..",
          "original_form": "치타는 키우는 사람 엄처우많아요..",
          "speaker_id": "2",
          "time": "20210829 19:49"
        },
        {
          "id": "MDRW2100022966.1.6",
          "form": "고양이과 중에서 최약체라.. 여성도 쉽게 제압할수 잌ㅅ어요",
          "original_form": "고양이과 중에서 최약체라.. 여성도 쉽게 제압할수 잌ㅅ어요",
          "speaker_id": "2",
          "time": "20210829 19:49"
        },
        {
          "id": "MDRW2100022966.1.7",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "1",
          "time": "20210829 19:49"
        },
        {
          "id": "MDRW2100022966.1.8",
          "form": "오 좀 솔깃하는걸요",
          "original_form": "오 좀 솔깃하는걸요",
          "speaker_id": "1",
          "time": "20210829 19:49"
        },
        {
          "id": "MDRW2100022966.1.9",
          "form": "보통 사반나의 호구라고 하죠.. ㅋㅋㅋ",
          "original_form": "보통 사반나의 호구라고 하죠.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:49"
        },
        {
          "id": "MDRW2100022966.1.10",
          "form": "먹이고 맨날 하이에나한테 뺏기고 힘도 없고..",
          "original_form": "먹이고 맨날 하이에나한테 뺏기고 힘도 없고..",
          "speaker_id": "2",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100022966.1.11",
          "form": "아 ㅋㅋㅋ 아니 걔는 왜 야생에 있는거죠?",
          "original_form": "아 ㅋㅋㅋ 아니 걔는 왜 야생에 있는거죠?",
          "speaker_id": "1",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100022966.1.12",
          "form": "진정한 반려동물 ..",
          "original_form": "진정한 반려동물 ..",
          "speaker_id": "1",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100022966.1.13",
          "form": "멸종위기에여.. ㅋㅋㅋㅋ 불쌍한 치토스군..",
          "original_form": "멸종위기에여.. ㅋㅋㅋㅋ 불쌍한 치토스군..",
          "speaker_id": "2",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100022966.1.14",
          "form": "헐 최약체라서 그런건가요 ,,",
          "original_form": "헐 최약체라서 그런건가요 ,,",
          "speaker_id": "1",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022966.1.15",
          "form": "다 잡하먹히다니 ,,",
          "original_form": "다 잡하먹히다니 ,,",
          "speaker_id": "1",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022966.1.16",
          "form": "진짜.. 웃긴게.나무사이에 껴서 탈출못해서 죽거나..",
          "original_form": "진짜.. 웃긴게.나무사이에 껴서 탈출못해서 죽거나..",
          "speaker_id": "2",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022966.1.17",
          "form": "달리기도 단거리만 빨라서.. 지구력은 똥이구..",
          "original_form": "달리기도 단거리만 빨라서.. 지구력은 똥이구..",
          "speaker_id": "2",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100022966.1.18",
          "form": "아니 나무사이는 진짜 바보아니에요? ㅋㅋㅋ",
          "original_form": "아니 나무사이는 진짜 바보아니에요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100022966.1.19",
          "form": "아이큐가 금붕어급인데 ㅋㅋㅋ",
          "original_form": "아이큐가 금붕어급인데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100022966.1.20",
          "form": "ㅋㅋㅋ 대신 아랍형들이 많이 좋아해주니까..",
          "original_form": "ㅋㅋㅋ 대신 아랍형들이 많이 좋아해주니까..",
