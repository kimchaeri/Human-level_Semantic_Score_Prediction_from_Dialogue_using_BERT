      "utterance": [
        {
          "id": "MDRW2100007581.1.1",
          "form": "겨울 좋아해??",
          "original_form": "겨울 좋아해??",
          "speaker_id": "1",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007581.1.2",
          "form": "여름이졸아 겨울이좋아",
          "original_form": "여름이졸아 겨울이좋아",
          "speaker_id": "1",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007581.1.3",
          "form": "방금 여름 끝나서",
          "original_form": "방금 여름 끝나서",
          "speaker_id": "2",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007581.1.4",
          "form": "겨울 ㅋ",
          "original_form": "겨울 ㅋ",
          "speaker_id": "2",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007581.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나두",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나두",
          "speaker_id": "1",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007581.1.6",
          "form": "난벌레많아서 겨울..ㅎ",
          "original_form": "난벌레많아서 겨울..ㅎ",
          "speaker_id": "1",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007581.1.7",
          "form": "그리고 가을이 왔으면 좋겠어",
          "original_form": "그리고 가을이 왔으면 좋겠어",
          "speaker_id": "2",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007581.1.8",
          "form": "코트 샀걸랑",
          "original_form": "코트 샀걸랑",
          "speaker_id": "2",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007581.1.9",
          "form": "여름 넘 더웟",
          "original_form": "여름 넘 더웟",
          "speaker_id": "1",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007581.1.10",
          "form": "아 벌레;; 맞아",
          "original_form": "아 벌레;; 맞아",
          "speaker_id": "2",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007581.1.11",
          "form": "헐 트렌치??",
          "original_form": "헐 트렌치??",
          "speaker_id": "1",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007581.1.12",
          "form": "그리구 땀 나",
          "original_form": "그리구 땀 나",
          "speaker_id": "2",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007581.1.13",
          "form": "웅 트렌치!",
          "original_form": "웅 트렌치!",
          "speaker_id": "2",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007581.1.14",
          "form": "가을은 또 트렌치의 계절이지ㅠ",
          "original_form": "가을은 또 트렌치의 계절이지ㅠ",
          "speaker_id": "1",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007581.1.15",
          "form": "근데 얼마 못입자나ㅠㅠ",
          "original_form": "근데 얼마 못입자나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007581.1.16",
          "form": "크....",
          "original_form": "크....",
          "speaker_id": "2",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007581.1.17",
          "form": "가을 넘 순식간이야ㅠㅠ",
          "original_form": "가을 넘 순식간이야ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007581.1.18",
          "form": "그냥 더워 죽어도 코트입고",
          "original_form": "그냥 더워 죽어도 코트입고",
          "speaker_id": "2",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007581.1.19",
