      "utterance": [
        {
          "id": "MDRW2100027341.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027341.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027341.1.3",
          "form": "반려동물 있으신가요",
          "original_form": "반려동물 있으신가요",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027341.1.4",
          "form": "없어요 ㅠㅠㅠ",
          "original_form": "없어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027341.1.5",
          "form": "저는 있는데 혹시 관심은 있으실까여...?",
          "original_form": "저는 있는데 혹시 관심은 있으실까여...?",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027341.1.6",
          "form": "전 강아지 키워요",
          "original_form": "전 강아지 키워요",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027341.1.7",
          "form": "ㅎㅎ 관심은 있죠!",
          "original_form": "ㅎㅎ 관심은 있죠!",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027341.1.8",
          "form": "저는 고양이 키우고싶은데...",
          "original_form": "저는 고양이 키우고싶은데...",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027341.1.9",
          "form": "집에 알레르기 있는 사람이 많아서 ㅠ",
          "original_form": "집에 알레르기 있는 사람이 많아서 ㅠ",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027341.1.10",
          "form": "불가능하네요 ㅠㅠ",
          "original_form": "불가능하네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027341.1.11",
          "form": "강아지 귀엽겠어요!!",
          "original_form": "강아지 귀엽겠어요!!",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027341.1.12",
          "form": "아 그럼 어렵죠 ㅠㅠ 친구가 고양이 키우는데 털 많이 빠져서",
          "original_form": "아 그럼 어렵죠 ㅠㅠ 친구가 고양이 키우는데 털 많이 빠져서",
          "speaker_id": "1",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027341.1.13",
          "form": "꿈의고양이 있으신가여 ㅋㅋ",
          "original_form": "꿈의고양이 있으신가여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027341.1.14",
          "form": "치즈? 턱시도?",
          "original_form": "치즈? 턱시도?",
          "speaker_id": "1",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027341.1.15",
          "form": "러시안블루!!",
          "original_form": "러시안블루!!",
          "speaker_id": "2",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027341.1.16",
          "form": "너무 털색도 눈동자색도",
          "original_form": "너무 털색도 눈동자색도",
          "speaker_id": "2",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027341.1.17",
          "form": "성격두 넘 좋아요 ㅎㅎ",
          "original_form": "성격두 넘 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027341.1.18",
          "form": "강아지도 털 많이 빠지지않나요?",
          "original_form": "강아지도 털 많이 빠지지않나요?",
          "speaker_id": "2",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027341.1.19",
          "form": "악 러시안블루 넘이쁘죠",
          "original_form": "악 러시안블루 넘이쁘죠",
          "speaker_id": "1",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100027341.1.20",
          "form": "개냥이 대명사!!!",
          "original_form": "개냥이 대명사!!!",
          "speaker_id": "1",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100027341.1.21",
          "form": "맞아요. 근데 저희는 푸들 키우는데 푸들은 털이 안빠져요 ㅎㅎ",
          "original_form": "맞아요. 근데 저희는 푸들 키우는데 푸들은 털이 안빠져요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:50"
        },
        {
          "id": "MDRW2100027341.1.22",
          "form": "아 푸들은 진짜 안빠지더라구요 신기해요",
          "original_form": "아 푸들은 진짜 안빠지더라구요 신기해요",
          "speaker_id": "2",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100027341.1.23",
          "form": "러시안블루 진짜 이뻐요 ㅠㅠ",
          "original_form": "러시안블루 진짜 이뻐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100027341.1.24",
          "form": "바닥에 보면 제 (머리)털이 더 많아요 .ㅌㅋㅋㅋ",
          "original_form": "바닥에 보면 제 (머리)털이 더 많아요 .ㅌㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100027341.1.25",
          "form": "랜선집사도 충분하죠!!",
          "original_form": "랜선집사도 충분하죠!!",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027341.1.26",
          "form": "ㅎㅎㅎ 안키워도 ㅎㅎ",
          "original_form": "ㅎㅎㅎ 안키워도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100027341.1.27",
          "form": "제 머리털이 더 많긴해욬ㅋㅋㅋㅋㅌ",
          "original_form": "제 머리털이 더 많긴해욬ㅋㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100027341.1.28",
          "form": "친구네 고양이 가끔 보면서 랜선집사로만족합니다 ㅎㅎ",
          "original_form": "친구네 고양이 가끔 보면서 랜선집사로만족합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100027341.1.29",
          "form": "고양님 키우는 제 친구네 놀러가면",
          "original_form": "고양님 키우는 제 친구네 놀러가면",
          "speaker_id": "1",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100027341.1.30",
          "form": "물컵으로 물마시다가 털 뱉는 수준이에여 ㅠㅠㅋㅋ",
          "original_form": "물컵으로 물마시다가 털 뱉는 수준이에여 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:57"
        }
      ]
