      "utterance": [
        {
          "id": "MDRW2100009851.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100009851.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100009851.1.3",
          "form": "요즘 어떤 옷 입으세요?!",
          "original_form": "요즘 어떤 옷 입으세요?!",
          "speaker_id": "1",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100009851.1.4",
          "form": "오늘부터 날이 쌀쌀해서 긴 팔 입었어요!",
          "original_form": "오늘부터 날이 쌀쌀해서 긴 팔 입었어요!",
          "speaker_id": "2",
          "time": "20210823 12:01"
        },
        {
          "id": "MDRW2100009851.1.5",
          "form": "저도 날이 쌀쌀해서 내일부터 긴팔 입을까 고민중이었어요",
          "original_form": "저도 날이 쌀쌀해서 내일부터 긴팔 입을까 고민중이었어요",
          "speaker_id": "1",
          "time": "20210823 12:02"
        },
        {
          "id": "MDRW2100009851.1.6",
          "form": "날이 너무 변화무쌍한 것같아요",
          "original_form": "날이 너무 변화무쌍한 것같아요",
          "speaker_id": "1",
          "time": "20210823 12:03"
        },
        {
          "id": "MDRW2100009851.1.7",
          "form": "맞아요.. 또 태풍이 온다고 해서 옷을 어떻게 입어야하나 걱정이에요 ㅠㅠㅠ",
          "original_form": "맞아요.. 또 태풍이 온다고 해서 옷을 어떻게 입어야하나 걱정이에요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 12:03"
        },
        {
          "id": "MDRW2100009851.1.8",
          "form": "맞아요 저는 추위를 많이타서 외투필수인것같아요",
          "original_form": "맞아요 저는 추위를 많이타서 외투필수인것같아요",
          "speaker_id": "1",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100009851.1.9",
          "form": "맞아요 더우면 벗고 추우면 입을 수 있게 가디건 같은거 챙겨야겠어요",
          "original_form": "맞아요 더우면 벗고 추우면 입을 수 있게 가디건 같은거 챙겨야겠어요",
          "speaker_id": "2",
          "time": "20210823 12:19"
        },
        {
          "id": "MDRW2100009851.1.10",
          "form": "운동은하시나요?",
          "original_form": "운동은하시나요?",
          "speaker_id": "1",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009851.1.11",
          "form": "저는 지금 딱히 하고있는 운동은 없고 런데이 할까 생각중이에요!",
          "original_form": "저는 지금 딱히 하고있는 운동은 없고 런데이 할까 생각중이에요!",
          "speaker_id": "2",
          "time": "20210823 12:33"
        },
        {
          "id": "MDRW2100009851.1.12",
          "form": "런데이가 그 애플워치로하는건가요?",
          "original_form": "런데이가 그 애플워치로하는건가요?",
          "speaker_id": "1",
          "time": "20210823 12:35"
        },
        {
          "id": "MDRW2100009851.1.13",
          "form": "워치로도 되고 핸드폰 어플로도 깔아서 할 수 있어요!",
          "original_form": "워치로도 되고 핸드폰 어플로도 깔아서 할 수 있어요!",
          "speaker_id": "2",
          "time": "20210823 12:38"
        },
        {
          "id": "MDRW2100009851.1.14",
          "form": "아하 달리기하는거 측정하는건가요?",
          "original_form": "아하 달리기하는거 측정하는건가요?",
          "speaker_id": "1",
          "time": "20210823 12:55"
        },
        {
          "id": "MDRW2100009851.1.15",
          "form": "네 그런것도 있고 음성으로 알려줘요 ㅋㅋㅋ 뛸때 뛰라는거랑 걸으면서 쉬라고 할 때 전부다요~",
          "original_form": "네 그런것도 있고 음성으로 알려줘요 ㅋㅋㅋ 뛸때 뛰라는거랑 걸으면서 쉬라고 할 때 전부다요~",
          "speaker_id": "2",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100009851.1.16",
          "form": "와 신기해요 저도 해봐야겠어요!",
          "original_form": "와 신기해요 저도 해봐야겠어요!",
          "speaker_id": "1",
          "time": "20210823 13:31"
        }
      ]
