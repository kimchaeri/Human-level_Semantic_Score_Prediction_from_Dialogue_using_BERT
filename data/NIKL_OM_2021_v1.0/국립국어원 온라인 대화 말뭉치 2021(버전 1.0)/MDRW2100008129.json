      "utterance": [
        {
          "id": "MDRW2100008129.1.1",
          "form": "모바일게임하세요?",
          "original_form": "모바일게임하세요?",
          "speaker_id": "2",
          "time": "20210822 14:04"
        },
        {
          "id": "MDRW2100008129.1.2",
          "form": "쿠킹덤 좀 하다가 요즘에는 미해결사건부라는 게임 하고 있어요",
          "original_form": "쿠킹덤 좀 하다가 요즘에는 미해결사건부라는 게임 하고 있어요",
          "speaker_id": "1",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100008129.1.3",
          "form": "name2 님은 게임 좋아하세요?",
          "original_form": "&name2& 님은 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100008129.1.4",
          "form": "저는 가디언테일즈 하고 있어요ㅎㅎ",
          "original_form": "저는 가디언테일즈 하고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008129.1.5",
          "form": "미해결사건부는",
          "original_form": "미해결사건부는",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008129.1.6",
          "form": "어떤 게임인가요?",
          "original_form": "어떤 게임인가요?",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008129.1.7",
          "form": "주인공이 변호사인데 증거 모아서 재판하는 게임이에요",
          "original_form": "주인공이 변호사인데 증거 모아서 재판하는 게임이에요",
          "speaker_id": "1",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008129.1.8",
          "form": "추리요소가 조금 있고 어렵지 않아서 재밌더라고요",
          "original_form": "추리요소가 조금 있고 어렵지 않아서 재밌더라고요",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008129.1.9",
          "form": "와 저 추리게임 좋아하는데 한번 해봐야겠어요ㅋㅋㅋ",
          "original_form": "와 저 추리게임 좋아하는데 한번 해봐야겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008129.1.10",
          "form": "예전에 검은방이라고 아세요...????ㅎㅎ",
          "original_form": "예전에 검은방이라고 아세요...????ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008129.1.11",
          "form": "처음 들어보는데",
          "original_form": "처음 들어보는데",
          "speaker_id": "1",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008129.1.12",
          "form": "추리게임인가요?",
          "original_form": "추리게임인가요?",
          "speaker_id": "1",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008129.1.13",
          "form": "너무너무 옛날꺼라서ㅋㅋ",
          "original_form": "너무너무 옛날꺼라서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008129.1.14",
          "form": "잘 모르실수도있어요ㅋㅋㅋ",
          "original_form": "잘 모르실수도있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008129.1.15",
          "form": "피쳐폰 쓸때 있었던 게임이라서ㅋㅋ",
          "original_form": "피쳐폰 쓸때 있었던 게임이라서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008129.1.16",
          "form": "아하ㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008129.1.17",
          "form": "미해결사건부 한 번 해보세요 서브 콘텐츠도 꽤 있어서 시간 때우기 좋아요",
          "original_form": "미해결사건부 한 번 해보세요 서브 콘텐츠도 꽤 있어서 시간 때우기 좋아요",
          "speaker_id": "1",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008129.1.18",
          "form": "가디언테일즈는 어떤 게임인가요?",
          "original_form": "가디언테일즈는 어떤 게임인가요?",
          "speaker_id": "1",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008129.1.19",
          "form": "저도 게임은 잘 모르는데ㅋㅋㅋ",
          "original_form": "저도 게임은 잘 모르는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008129.1.20",
          "form": "아마 쿠킹덤? 이랑 비슷할거에요??",
          "original_form": "아마 쿠킹덤? 이랑 비슷할거에요??",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008129.1.21",
          "form": "아닌가ㅋㅋ",
          "original_form": "아닌가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008129.1.22",
          "form": "걍 영웅 키우면서 스토리 진행하고",
          "original_form": "걍 영웅 키우면서 스토리 진행하고",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008129.1.23",
          "form": "서브로 여러 퍼즐이나 게임이 있는??",
          "original_form": "서브로 여러 퍼즐이나 게임이 있는??",
          "speaker_id": "2",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008129.1.24",
          "form": "그것도 재밌겠네요",
          "original_form": "그것도 재밌겠네요",
          "speaker_id": "1",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008129.1.25",
          "form": "PC 게임도 하시나요?",
          "original_form": "PC 게임도 하시나요?",
          "speaker_id": "1",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008129.1.26",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210822 14:14"
        },
        {
          "id": "MDRW2100008129.1.27",
          "form": "예전에 리듬게임 하다가 요즘은 아무것도 안해요ㅎㅎ",
          "original_form": "예전에 리듬게임 하다가 요즘은 아무것도 안해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:14"
        },
        {
          "id": "MDRW2100008129.1.28",
          "form": "잘 못해서ㅎㅎ",
          "original_form": "잘 못해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:14"
        },
        {
          "id": "MDRW2100008129.1.29",
          "form": "저도 게임 정말 좋아하는데 실력이 잘 안 느는 편이에요",
          "original_form": "저도 게임 정말 좋아하는데 실력이 잘 안 느는 편이에요",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008129.1.30",
          "form": "그래서 즐길만큼 하면 질려서 못하겠더라고요",
          "original_form": "그래서 즐길만큼 하면 질려서 못하겠더라고요",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008129.1.31",
          "form": "어떤 리듬게임 하셨었나요?",
          "original_form": "어떤 리듬게임 하셨었나요?",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008129.1.32",
          "form": "옛날거라서 기억이 안나네요ㅋㅋㅋ",
          "original_form": "옛날거라서 기억이 안나네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008129.1.33",
          "form": "오투잼? 알투비트 였나?",
          "original_form": "오투잼? 알투비트 였나?",
          "speaker_id": "2",
          "time": "20210822 14:17"
        },
        {
          "id": "MDRW2100008129.1.34",
          "form": "알투비트는 저도 재밌게 했었어요",
          "original_form": "알투비트는 저도 재밌게 했었어요",
