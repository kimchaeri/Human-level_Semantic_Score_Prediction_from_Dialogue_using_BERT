      "utterance": [
        {
          "id": "MDRW2100014382.1.1",
          "form": "공연좋아하시나?",
          "original_form": "공연좋아하시나?",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014382.1.2",
          "form": "콘서트 잘안가시죠..",
          "original_form": "콘서트 잘안가시죠..",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014382.1.3",
          "form": "나 최근에 프리가이봤다",
          "original_form": "나 최근에 프리가이봤다",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014382.1.4",
          "form": "메가박스에서",
          "original_form": "메가박스에서",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014382.1.5",
          "form": "뮤지컬보러가고싶다",
          "original_form": "뮤지컬보러가고싶다",
          "speaker_id": "1",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100014382.1.6",
          "form": "소극장으로",
          "original_form": "소극장으로",
          "speaker_id": "1",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100014382.1.7",
          "form": "우리맨날얘기하는데 가질못허네..",
          "original_form": "우리맨날얘기하는데 가질못허네..",
          "speaker_id": "2",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100014382.1.8",
          "form": "500에 30인가 그걸 마지막으로 못갔어",
          "original_form": "500에 30인가 그걸 마지막으로 못갔어",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100014382.1.9",
          "form": "소극장연극은 또 그 맛이 있는데",
          "original_form": "소극장연극은 또 그 맛이 있는데",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100014382.1.10",
          "form": "나 소극장 가본적없어서가보고싶다",
          "original_form": "나 소극장 가본적없어서가보고싶다",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100014382.1.11",
          "form": "근데 너무 가까우면 부담도ㅑ...",
          "original_form": "근데 너무 가까우면 부담도ㅑ...",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100014382.1.12",
          "form": "헐 그래?",
          "original_form": "헐 그래?",
          "speaker_id": "1",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100014382.1.13",
          "form": "아 아니다 나 청주에서도 연극본적있어",
          "original_form": "아 아니다 나 청주에서도 연극본적있어",
          "speaker_id": "1",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100014382.1.14",
          "form": "거기 자리 잘뽑아서 무대우ㅣ로 올라갔다",
          "original_form": "거기 자리 잘뽑아서 무대우ㅣ로 올라갔다",
          "speaker_id": "1",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100014382.1.15",
          "form": "ㅋㅋ오",
          "original_form": "ㅋㅋ오",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100014382.1.16",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100014382.1.17",
          "form": "안돼",
          "original_form": "안돼",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100014382.1.18",
          "form": "인프피는 수치사가능",
          "original_form": "인프피는 수치사가능",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100014382.1.19",
          "form": "뮤지컬도 보고싶어",
          "original_form": "뮤지컬도 보고싶어",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100014382.1.20",
          "form": "나는 관종이라 아주 좋았어",
          "original_form": "나는 관종이라 아주 좋았어",
          "speaker_id": "1",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100014382.1.21",
          "form": "뮤지컬 진짜 보고싶어",
          "original_form": "뮤지컬 진짜 보고싶어",
          "speaker_id": "1",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100014382.1.22",
          "form": "가자 뮤지컬보러",
          "original_form": "가자 뮤지컬보러",
          "speaker_id": "1",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100014382.1.23",
          "form": "좋아 나도 뮤지컬가고파",
          "original_form": "좋아 나도 뮤지컬가고파",
          "speaker_id": "2",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100014382.1.24",
          "form": "돈모았다가 대형뮤지컬도 보자",
          "original_form": "돈모았다가 대형뮤지컬도 보자",
          "speaker_id": "1",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100014382.1.25",
          "form": "시카고보고싶어",
          "original_form": "시카고보고싶어",
          "speaker_id": "1",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100014382.1.26",
          "form": "완전조아 나두",
          "original_form": "완전조아 나두",
          "speaker_id": "2",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100014382.1.27",
          "form": "영화재밌었어",
          "original_form": "영화재밌었어",
          "speaker_id": "2",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100014382.1.28",
          "form": "맞아 영화만 봐도 심장 떨리는데 뮤지컬 보면 소름끼릴듯",
          "original_form": "맞아 영화만 봐도 심장 떨리는데 뮤지컬 보면 소름끼릴듯",
          "speaker_id": "1",
          "time": "20210827 10:38"
        }
      ]
    }
