      "utterance": [
        {
          "id": "MDRW2100020873.1.1",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020873.1.2",
          "form": "네 먹었어요! 만두쪄먹엇어요 ㅋㅋㅋㅋ",
          "original_form": "네 먹었어요! 만두쪄먹엇어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020873.1.3",
          "form": "점심드셨어요?",
          "original_form": "점심드셨어요?",
          "speaker_id": "2",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020873.1.4",
          "form": "오오! 네!",
          "original_form": "오오! 네!",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020873.1.5",
          "form": "엄청 먹었어요!",
          "original_form": "엄청 먹었어요!",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020873.1.6",
          "form": "오 무슨 메뉴 드셧어요",
          "original_form": "오 무슨 메뉴 드셧어요",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020873.1.7",
          "form": "떡볶이랑 냉면이랑 빈대떡이요!",
          "original_form": "떡볶이랑 냉면이랑 빈대떡이요!",
          "speaker_id": "1",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100020873.1.8",
          "form": "오 조합이 뭔가 특이한데요??? 일단 다 맛있는거군요...",
          "original_form": "오 조합이 뭔가 특이한데요??? 일단 다 맛있는거군요...",
          "speaker_id": "2",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020873.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020873.1.10",
          "form": "버릴게 없었어요",
          "original_form": "버릴게 없었어요",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020873.1.11",
          "form": "배터지게 먹었네요ㅋㅋ",
          "original_form": "배터지게 먹었네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020873.1.12",
          "form": "크 배달 시키셧어요?",
          "original_form": "크 배달 시키셧어요?",
          "speaker_id": "2",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100020873.1.13",
          "form": "식당가서 먹엇어요!",
          "original_form": "식당가서 먹엇어요!",
          "speaker_id": "1",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100020873.1.14",
          "form": "친구가 맛집이래서 같이 갔는데 근처에 있다는게 신기하고 좋더라구요",
          "original_form": "친구가 맛집이래서 같이 갔는데 근처에 있다는게 신기하고 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020873.1.15",
          "form": "오 그렇군요 맛집!!",
          "original_form": "오 그렇군요 맛집!!",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020873.1.16",
          "form": "최애 음식 있으세요?",
          "original_form": "최애 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020873.1.17",
          "form": "갈비요!",
          "original_form": "갈비요!",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020873.1.18",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020873.1.19",
          "form": "저는 치킨 좋아해요!",
          "original_form": "저는 치킨 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020873.1.20",
          "form": "갈비도 좋아해요 ㅠㅠㅠ",
          "original_form": "갈비도 좋아해요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020873.1.21",
          "form": "치킨 진짜 맛있죠",
          "original_form": "치킨 진짜 맛있죠",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020873.1.22",
          "form": "고기는 배신을 안해요",
          "original_form": "고기는 배신을 안해요",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020873.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020873.1.24",
          "form": "갈비말고 또 어느 부위 좋아하세요?",
          "original_form": "갈비말고 또 어느 부위 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020873.1.25",
          "form": "사실 고기를 다 좋아해요ㅋㅋㅋ",
          "original_form": "사실 고기를 다 좋아해요ㅋㅋㅋ",
