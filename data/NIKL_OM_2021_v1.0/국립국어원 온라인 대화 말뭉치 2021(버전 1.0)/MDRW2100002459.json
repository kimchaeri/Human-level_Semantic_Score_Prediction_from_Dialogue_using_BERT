      "utterance": [
        {
          "id": "MDRW2100002459.1.1",
          "form": "최근에 뭐 구매하신 거 있으세요?",
          "original_form": "최근에 뭐 구매하신 거 있으세요?",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002459.1.2",
          "form": "아 저는 최근에 운동화를샀어요!",
          "original_form": "아 저는 최근에 운동화를샀어요!",
          "speaker_id": "2",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002459.1.3",
          "form": "구매하신거있으신가요?",
          "original_form": "구매하신거있으신가요?",
          "speaker_id": "2",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002459.1.4",
          "form": "오 어떤 운동화 사셨어요?",
          "original_form": "오 어떤 운동화 사셨어요?",
          "speaker_id": "1",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002459.1.5",
          "form": "저는 최근에 지플립 사전예약했어요!!",
          "original_form": "저는 최근에 지플립 사전예약했어요!!",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002459.1.6",
          "form": "자랑하고싶었어요,,ㅎ",
          "original_form": "자랑하고싶었어요,,ㅎ",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002459.1.7",
          "form": "근데 폰이 안와요..",
          "original_form": "근데 폰이 안와요..",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002459.1.8",
          "form": "저 나이키운동화요ㅎㅎㅎ",
          "original_form": "저 나이키운동화요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002459.1.9",
          "form": "와 너무해요ㅠㅠ저도그거사고싶은데",
          "original_form": "와 너무해요ㅠㅠ저도그거사고싶은데",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002459.1.10",
          "form": "무슨색사셨나요?",
          "original_form": "무슨색사셨나요?",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002459.1.11",
          "form": "저 크림색이요..!!",
          "original_form": "저 크림색이요..!!",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002459.1.12",
          "form": "색 다 예쁘게 나온 거 같아요ㅠㅠ",
          "original_form": "색 다 예쁘게 나온 거 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002459.1.13",
          "form": "저는 나이키 데이브레이크 갖고싶어요..",
          "original_form": "저는 나이키 데이브레이크 갖고싶어요..",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002459.1.14",
          "form": "오 크림색좋네요",
          "original_form": "오 크림색좋네요",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002459.1.15",
          "form": "데이브레이크는 뭐죠?!",
          "original_form": "데이브레이크는 뭐죠?!",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002459.1.16",
          "form": "나이키 운동화인데 좀 유명한 라인같더라고요!!",
          "original_form": "나이키 운동화인데 좀 유명한 라인같더라고요!!",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002459.1.17",
          "form": "abc마트에서 봤는데 반했어요ㅠ",
          "original_form": "abc마트에서 봤는데 반했어요ㅠ",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002459.1.18",
          "form": "오 좋네요ㅋㅋㅋ",
          "original_form": "오 좋네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002459.1.19",
          "form": "저는최근에",
          "original_form": "저는최근에",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002459.1.20",
          "form": "갤럭시워치 사고싶더라구요",
          "original_form": "갤럭시워치 사고싶더라구요",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002459.1.21",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002459.1.22",
          "form": "인바디 된다는 게 너무 신기해요",
          "original_form": "인바디 된다는 게 너무 신기해요",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002459.1.23",
          "form": "평소에 스마트워치 사용하시나요?",
          "original_form": "평소에 스마트워치 사용하시나요?",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002459.1.24",
          "form": "저 갤워치 액티브2있거든요ㅋㅋㅋ",
          "original_form": "저 갤워치 액티브2있거든요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002459.1.25",
          "form": "근데그거40마넌주고산건데",
          "original_form": "근데그거40마넌주고산건데",
