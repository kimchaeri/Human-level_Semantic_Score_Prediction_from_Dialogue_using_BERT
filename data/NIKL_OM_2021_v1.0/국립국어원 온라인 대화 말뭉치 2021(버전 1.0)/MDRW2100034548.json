      "utterance": [
        {
          "id": "MDRW2100034548.1.1",
          "form": "일본에서 탈출해 레바논으로 갔네",
          "original_form": "일본에서 탈출해 레바논으로 갔네",
          "speaker_id": "2",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100034548.1.2",
          "form": "역시 돈이네",
          "original_form": "역시 돈이네",
          "speaker_id": "1",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100034548.1.3",
          "form": "그러게 돈으로 안되는게 없는 세상인가보다",
          "original_form": "그러게 돈으로 안되는게 없는 세상인가보다",
          "speaker_id": "2",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100034548.1.4",
          "form": "100억 넘게 받는 사람인데 그게 돈이겠나",
          "original_form": "100억 넘게 받는 사람인데 그게 돈이겠나",
          "speaker_id": "1",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100034548.1.5",
          "form": "그러게 협박을 당했다고 하지만",
          "original_form": "그러게 협박을 당했다고 하지만",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034548.1.6",
          "form": "탈출한 사람은 아무렇지도 않고ㅋ",
          "original_form": "탈출한 사람은 아무렇지도 않고ㅋ",
          "speaker_id": "1",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100034548.1.7",
          "form": "레바논에서 당사자는 잘 살고 탈출을 도운 사람들만 처벌당했네",
          "original_form": "레바논에서 당사자는 잘 살고 탈출을 도운 사람들만 처벌당했네",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100034548.1.8",
          "form": "이해가 안가지만 어쩌겠어",
          "original_form": "이해가 안가지만 어쩌겠어",
          "speaker_id": "1",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100034548.1.9",
          "form": "범인인도조약이 없는갑다",
          "original_form": "범인인도조약이 없는갑다",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034548.1.10",
          "form": "그런듯..  레바논에서 절대 안나갈듯",
          "original_form": "그런듯..  레바논에서 절대 안나갈듯",
          "speaker_id": "1",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034548.1.11",
          "form": "크리스마스트리에서 불이....금방 크게 번지네",
          "original_form": "크리스마스트리에서 불이....금방 크게 번지네",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034548.1.12",
          "form": "뛰어 내리다 보네",
          "original_form": "뛰어 내리다 보네",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100034548.1.13",
          "form": "그런갑다 헐 동생을 던진다",
          "original_form": "그런갑다 헐 동생을 던진다",
          "speaker_id": "2",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100034548.1.14",
          "form": "와 받았다. 다행이다",
          "original_form": "와 받았다. 다행이다",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100034548.1.15",
          "form": "다행히 아래있는 어른들이 동생을 잘 받았네",
          "original_form": "다행히 아래있는 어른들이 동생을 잘 받았네",
          "speaker_id": "2",
          "time": "20210828 14:58"
