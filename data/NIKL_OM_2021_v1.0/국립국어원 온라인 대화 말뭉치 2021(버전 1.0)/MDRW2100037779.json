          "original_form": "왕!",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037779.1.17",
          "form": "어디지!?",
          "original_form": "어디지!?",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037779.1.18",
          "form": "유니버셜 스튜디오!",
          "original_form": "유니버셜 스튜디오!",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037779.1.19",
          "form": "유니버셜 슽ㄷ어!",
          "original_form": "유니버셜 슽ㄷ어!",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037779.1.20",
          "form": "마자!",
          "original_form": "마자!",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037779.1.21",
          "form": "지브리 있는곳도 그고",
          "original_form": "지브리 있는곳도 그고",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037779.1.22",
          "form": "짱구 막 거기도 가고",
          "original_form": "짱구 막 거기도 가고",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037779.1.23",
          "form": "다 가!",
          "original_form": "다 가!",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037779.1.24",
          "form": "막 다 가!!",
          "original_form": "막 다 가!!",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037779.1.25",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
