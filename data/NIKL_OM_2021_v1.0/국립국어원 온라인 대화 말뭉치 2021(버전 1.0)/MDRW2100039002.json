      "utterance": [
        {
          "id": "MDRW2100039002.1.1",
          "form": "나한ㄷㅔ",
          "original_form": "나한ㄷㅔ",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.2",
          "form": "전화 왔어요",
          "original_form": "전화 왔어요",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.3",
          "form": "다시 만나보면 안되겠냐고",
          "original_form": "다시 만나보면 안되겠냐고",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.4",
          "form": "왜 그러냐고",
          "original_form": "왜 그러냐고",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.5",
          "form": "아는 이모님통해서",
          "original_form": "아는 이모님통해서",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.6",
          "form": "계속 말했나봐",
          "original_form": "계속 말했나봐",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.7",
          "form": "아이고 아이고 아이고 아이고",
          "original_form": "아이고 아이고 아이고 아이고",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.8",
          "form": "안만난다했지",
          "original_form": "안만난다했지",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.9",
          "form": "마마보이는 진짜 별로죠",
          "original_form": "마마보이는 진짜 별로죠",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.10",
          "form": "내친구한테",
          "original_form": "내친구한테",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.11",
          "form": "아들 일에 간섭하는 엄마도 싫어",
          "original_form": "아들 일에 간섭하는 엄마도 싫어",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.12",
          "form": "그분을 만나보라고",
          "original_form": "그분을 만나보라고",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100039002.1.13",
          "form": "그건 또 뭐하는지 쉬고",
          "original_form": "그건 또 뭐하는지 쉬고",
          "speaker_id": "1",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100039002.1.14",
          "form": "이모님이 하도 사정을 했나봐",
          "original_form": "이모님이 하도 사정을 했나봐",
          "speaker_id": "2",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100039002.1.15",
          "form": "그 엄마가",
          "original_form": "그 엄마가",
          "speaker_id": "2",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100039002.1.16",
          "form": "그 남자도 참 안타깝네",
          "original_form": "그 남자도 참 안타깝네",
          "speaker_id": "1",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100039002.1.17",
          "form": "계속 소개시켜달라했나보지",
          "original_form": "계속 소개시켜달라했나보지",
          "speaker_id": "2",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100039002.1.18",
          "form": "디게 결혼하고 싶었나 보다",
          "original_form": "디게 결혼하고 싶었나 보다",
          "speaker_id": "1",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100039002.1.19",
          "form": "이모님 체면도 있공",
          "original_form": "이모님 체면도 있공",
          "speaker_id": "2",
          "time": "20210903 01:02"
        },
