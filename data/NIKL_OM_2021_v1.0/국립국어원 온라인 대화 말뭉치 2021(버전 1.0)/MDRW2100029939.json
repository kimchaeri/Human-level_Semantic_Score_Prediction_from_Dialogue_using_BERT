      "utterance": [
        {
          "id": "MDRW2100029939.1.1",
          "form": "혹시 게불도 드시나요",
          "original_form": "혹시 게불도 드시나요",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029939.1.2",
          "form": "전 개불도 참기름 안찍습닏ㄴ",
          "original_form": "전 개불도 참기름 안찍습닏ㄴ",
          "speaker_id": "2",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029939.1.3",
          "form": "간장에 조집니다",
          "original_form": "간장에 조집니다",
          "speaker_id": "2",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029939.1.4",
          "form": "저 이거 진짜 좋아하는데",
          "original_form": "저 이거 진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029939.1.5",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029939.1.6",
          "form": "멍게보다 더 같이 먹기 난이도가 높아요",
          "original_form": "멍게보다 더 같이 먹기 난이도가 높아요",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029939.1.7",
          "form": "오독오독한거 좋아하나요",
          "original_form": "오독오독한거 좋아하나요",
          "speaker_id": "2",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029939.1.8",
          "form": "네네 근데 사람들이 조리하기전",
          "original_form": "네네 근데 사람들이 조리하기전",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029939.1.9",
          "form": "생김새만 보고 기피해서ㅜ",
          "original_form": "생김새만 보고 기피해서ㅜ",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029939.1.10",
          "form": "게다가 가격에 비해 양도 적어서",
          "original_form": "게다가 가격에 비해 양도 적어서",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029939.1.11",
          "form": "같이 감 시키기 어려워요",
          "original_form": "같이 감 시키기 어려워요",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029939.1.12",
          "form": "개불 진짜 달고 맛있는데!",
          "original_form": "개불 진짜 달고 맛있는데!",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100029939.1.13",
          "form": "맞있죠 ㅋㅋ",
          "original_form": "맞있죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029939.1.14",
          "form": "죵아요",
          "original_form": "죵아요",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029939.1.15",
          "form": "소주각!",
          "original_form": "소주각!",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029939.1.16",
          "form": "조리도 해요?",
          "original_form": "조리도 해요?",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029939.1.17",
          "form": "ㅎㅎ 요리는.. 제게 허락된 영역이 아닙니다",
          "original_form": "ㅎㅎ 요리는.. 제게 허락된 영역이 아닙니다",
          "speaker_id": "1",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100029939.1.18",
          "form": "요리가 아닌 도전과 모험을 하죠",
          "original_form": "요리가 아닌 도전과 모험을 하죠",
          "speaker_id": "1",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100029939.1.19",
          "form": "아 개불은 조리 말고 채 써는 거요ㅎ",
          "original_form": "아 개불은 조리 말고 채 써는 거요ㅎ",
          "speaker_id": "1",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100029939.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100029939.1.21",
          "form": "조리라고하셔서",
          "original_form": "조리라고하셔서",
          "speaker_id": "2",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100029939.1.22",
          "form": "내 소중한 개불로",
          "original_form": "내 소중한 개불로",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029939.1.23",
          "form": "칼질 무시하시나욧!",
          "original_form": "칼질 무시하시나욧!",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029939.1.24",
          "form": "뭔 작당을 하시나",
          "original_form": "뭔 작당을 하시나",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029939.1.25",
          "form": "생각했습니다 ㅋㅋ",
          "original_form": "생각했습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029939.1.26",
          "form": "ㅋㅋㅋ 개불과 절친이신가요ㅎㅎ",
          "original_form": "ㅋㅋㅋ 개불과 절친이신가요ㅎㅎ",
