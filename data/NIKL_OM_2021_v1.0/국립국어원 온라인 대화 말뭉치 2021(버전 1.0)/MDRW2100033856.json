      "utterance": [
        {
          "id": "MDRW2100033856.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100033856.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100033856.1.3",
          "form": "최근에 사신것있나요?",
          "original_form": "최근에 사신것있나요?",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100033856.1.4",
          "form": "양산을 하나샀어요.",
          "original_form": "양산을 하나샀어요.",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100033856.1.5",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100033856.1.6",
          "form": "아주 좋은선택이시네요",
          "original_form": "아주 좋은선택이시네요",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100033856.1.7",
          "form": "이번주는 비가 와서 못 쓰고 있지만요ㅋㅋ",
          "original_form": "이번주는 비가 와서 못 쓰고 있지만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100033856.1.8",
          "form": "쇼핑 자주 하시나요?",
          "original_form": "쇼핑 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100033856.1.9",
          "form": "으음",
          "original_form": "으음",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033856.1.10",
          "form": "자주는 안하는것같아요",
          "original_form": "자주는 안하는것같아요",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033856.1.11",
          "form": "맞아요 장마라서요ㅠ",
          "original_form": "맞아요 장마라서요ㅠ",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033856.1.12",
          "form": "구매는 안하는데 아이쇼핑은 하게 되요",
          "original_form": "구매는 안하는데 아이쇼핑은 하게 되요",
          "speaker_id": "1",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100033856.1.13",
          "form": "요새 사고 싶은 물건은 없으세요?",
          "original_form": "요새 사고 싶은 물건은 없으세요?",
          "speaker_id": "1",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100033856.1.14",
          "form": "저 요즘",
          "original_form": "저 요즘",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033856.1.15",
          "form": "시계사고싶어요ㅋㅋㅋ",
          "original_form": "시계사고싶어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033856.1.16",
          "form": "ㅋㅋㅋ시계는 한번 살때 비싼거로 사라던데요",
          "original_form": "ㅋㅋㅋ시계는 한번 살때 비싼거로 사라던데요",
          "speaker_id": "1",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100033856.1.17",
          "form": "근데 워치가 있긴해요ㅠ",
          "original_form": "근데 워치가 있긴해요ㅠ",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033856.1.18",
          "form": "요즘은 스마트워치 진짜많이차더라고요 시계대용으로",
          "original_form": "요즘은 스마트워치 진짜많이차더라고요 시계대용으로",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033856.1.19",
          "form": "맞아요. 그치만 시계만의 감성이 있는데..ㅎ",
          "original_form": "맞아요. 그치만 시계만의 감성이 있는데..ㅎ",
          "speaker_id": "1",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100033856.1.20",
          "form": "그렇긴하죠ㅋㅋㅋ",
          "original_form": "그렇긴하죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100033856.1.21",
          "form": "시계매출이 조금줄었을라나요",
          "original_form": "시계매출이 조금줄었을라나요",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100033856.1.22",
          "form": "영향이 없으려나",
          "original_form": "영향이 없으려나",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
