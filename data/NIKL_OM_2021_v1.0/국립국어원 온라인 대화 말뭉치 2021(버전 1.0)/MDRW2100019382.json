      "utterance": [
        {
          "id": "MDRW2100019382.1.1",
          "form": "얇은피는 뭐가 좋아?",
          "original_form": "얇은피는 뭐가 좋아?",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019382.1.2",
          "form": "난 맛있는 만두ㅋㅋ",
          "original_form": "난 맛있는 만두ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019382.1.3",
          "form": "맛있는만두?",
          "original_form": "맛있는만두?",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019382.1.4",
          "form": "그런만두가 있어?",
          "original_form": "그런만두가 있어?",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019382.1.5",
          "form": "얇은피에서는 다들 김치랑 오짬? 추천하는거같았어",
          "original_form": "얇은피에서는 다들 김치랑 오짬? 추천하는거같았어",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019382.1.6",
          "form": "나 오짬 안먹어봄 ㅠㅠ 내가 보는 딜에는 안팔더라",
          "original_form": "나 오짬 안먹어봄 ㅠㅠ 내가 보는 딜에는 안팔더라",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019382.1.7",
          "form": "아니아니\r\n내가 만두 종류 구분없이 맛있음 좋다고ㅋㅋ",
          "original_form": "아니아니\r\n내가 만두 종류 구분없이 맛있음 좋다고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019382.1.8",
          "form": "김치는 난 그냥 그렇더라",
          "original_form": "김치는 난 그냥 그렇더라",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019382.1.9",
          "form": "나도 오짬 안먹어봄\r\n뭔가 인의적인 MSG맛일거같아서 도전안돼더라",
          "original_form": "나도 오짬 안먹어봄\r\n뭔가 인의적인 MSG맛일거같아서 도전안돼더라",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019382.1.10",
          "form": "난 피가 얇다는거에 만족했는데",
          "original_form": "난 피가 얇다는거에 만족했는데",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019382.1.11",
          "form": "울엄마는 비비고 커다란만두 이름뭐더라 그거 좋아하시는데",
          "original_form": "울엄마는 비비고 커다란만두 이름뭐더라 그거 좋아하시는데",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019382.1.12",
          "form": "그렇구나 난 오짬만 안 먹어봐서 먹어보고싶다",
          "original_form": "그렇구나 난 오짬만 안 먹어봐서 먹어보고싶다",
          "speaker_id": "1",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019382.1.13",
          "form": "비비고 왕교자? 왕만두?",
          "original_form": "비비고 왕교자? 왕만두?",
          "speaker_id": "1",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019382.1.14",
          "form": "굽는건데",
          "original_form": "굽는건데",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019382.1.15",
          "form": "왕교자",
          "original_form": "왕교자",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019382.1.16",
          "form": "이게 크고 먹을맛난데ㅎ",
          "original_form": "이게 크고 먹을맛난데ㅎ",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019382.1.17",
          "form": "아 비비고도 괜찮긴하지",
          "original_form": "아 비비고도 괜찮긴하지",
          "speaker_id": "1",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019382.1.18",
          "form": "우리집은 육향이 많이 난다고 안좋아하셔",
          "original_form": "우리집은 육향이 많이 난다고 안좋아하셔",
          "speaker_id": "1",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019382.1.19",
          "form": "비비고 고기 부위랑 비율 변경하고는\r\n옛만못한다하긴하던데",
          "original_form": "비비고 고기 부위랑 비율 변경하고는\r\n옛만못한다하긴하던데",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019382.1.20",
          "form": "얇피를 그나마 잘 드시는듯",
          "original_form": "얇피를 그나마 잘 드시는듯",
          "speaker_id": "1",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019382.1.21",
          "form": "난 비비고 왕교자도 좋고 얇은피도 좋아",
          "original_form": "난 비비고 왕교자도 좋고 얇은피도 좋아",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019382.1.22",
          "form": "요즘 대세는 얇피같도라",
          "original_form": "요즘 대세는 얇피같도라",
          "speaker_id": "1",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019382.1.23",
          "form": "내가 안구울때ㅋㅋ",
          "original_form": "내가 안구울때ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019382.1.24",
          "form": "내가 구움 맛없어ㅋㅋ",
          "original_form": "내가 구움 맛없어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019382.1.25",
          "form": "굽는 만두는 비비고가 나은듯",
          "original_form": "굽는 만두는 비비고가 나은듯",
          "speaker_id": "1",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019382.1.26",
          "form": "기름 많이 두르고 구워 ㅋㅋ",
          "original_form": "기름 많이 두르고 구워 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019382.1.27",
          "form": "불은 중불",
          "original_form": "불은 중불",
          "speaker_id": "1",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019382.1.28",
          "form": "그게 잘안돼\r\n막상 구울땐ㅋ",
          "original_form": "그게 잘안돼\r\n막상 구울땐ㅋ",
          "speaker_id": "2",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019382.1.29",
          "form": "맞나 ㅋㅋㅋ 뭐든 하다보면 된다 너도 해보면 늘걸",
          "original_form": "맞나 ㅋㅋㅋ 뭐든 하다보면 된다 너도 해보면 늘걸",
          "speaker_id": "1",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019382.1.30",
          "form": "늘정도로 할 기회는 드물어ㅎㅎ",
          "original_form": "늘정도로 할 기회는 드물어ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100019382.1.31",
          "form": "1년에 2번 구워먹을까싶어서ㅎ",
          "original_form": "1년에 2번 구워먹을까싶어서ㅎ",
          "speaker_id": "2",
          "time": "20210827 18:40"
        }
      ]
    }
