      "utterance": [
        {
          "id": "MDRW2100018684.1.1",
          "form": "강아지랑 고양이 어느쪽이세요ㅡ",
          "original_form": "강아지랑 고양이 어느쪽이세요ㅡ",
          "speaker_id": "1",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100018684.1.2",
          "form": "둘 다 좋지만 고양이요!",
          "original_form": "둘 다 좋지만 고양이요!",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100018684.1.3",
          "form": "역시 고양이죠..ㅋㅋㅋㅋ \r\n왜 고양이에용?",
          "original_form": "역시 고양이죠..ㅋㅋㅋㅋ \r\n왜 고양이에용?",
          "speaker_id": "1",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018684.1.4",
          "form": "ㅋㅋname1님도 고양이시군요?!",
          "original_form": "ㅋㅋ&name1&님도 고양이시군요?!",
          "speaker_id": "2",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018684.1.5",
          "form": "지금 함께 지내고 있어서 더 그런거같아요!",
          "original_form": "지금 함께 지내고 있어서 더 그런거같아요!",
          "speaker_id": "2",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018684.1.6",
          "form": "와~~~~~ 얼마나 됫어요?",
          "original_form": "와~~~~~ 얼마나 됫어요?",
          "speaker_id": "1",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018684.1.7",
          "form": "이제 3년이요!",
          "original_form": "이제 3년이요!",
          "speaker_id": "2",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018684.1.8",
          "form": "완전 어른이네요.. 쫌 얌전해 졌겠네?",
          "original_form": "완전 어른이네요.. 쫌 얌전해 졌겠네?",
          "speaker_id": "1",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018684.1.9",
          "form": "넹ㅋㅋ 그래도 아직은 애기라 활발해요!",
          "original_form": "넹ㅋㅋ 그래도 아직은 애기라 활발해요!",
          "speaker_id": "2",
          "time": "20210827 10:44"
        },
        {
          "id": "MDRW2100018684.1.10",
          "form": "ㅋㅋㅋ 무슨색이에요?",
          "original_form": "ㅋㅋㅋ 무슨색이에요?",
          "speaker_id": "1",
          "time": "20210827 10:45"
        },
        {
          "id": "MDRW2100018684.1.11",
          "form": "백호요!",
          "original_form": "백호요!",
          "speaker_id": "2",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100018684.1.12",
          "form": "걷는것도 아기호랑이 같아요ㅋㅋ",
          "original_form": "걷는것도 아기호랑이 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100018684.1.13",
          "form": "아메숏?",
          "original_form": "아메숏?",
          "speaker_id": "1",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100018684.1.14",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "2",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100018684.1.15",
          "form": "맞아용ㅋㅋ",
          "original_form": "맞아용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:47"
        },
        {
          "id": "MDRW2100018684.1.16",
          "form": "엄청 멋지죠...ㅋㅋㅋㅋ 일나오면 엄청 보고싶겟어여...",
          "original_form": "엄청 멋지죠...ㅋㅋㅋㅋ 일나오면 엄청 보고싶겟어여...",
          "speaker_id": "1",
          "time": "20210827 10:47"
        },
        {
          "id": "MDRW2100018684.1.17",
          "form": "저는 누렁호랑무늬 키우고 싶어여",
          "original_form": "저는 누렁호랑무늬 키우고 싶어여",
          "speaker_id": "1",
          "time": "20210827 10:47"
        },
        {
          "id": "MDRW2100018684.1.18",
          "form": "네ㅠ",
          "original_form": "네ㅠ",
          "speaker_id": "2",
          "time": "20210827 10:47"
        },
        {
          "id": "MDRW2100018684.1.19",
          "form": "꺄 엄청 예쁘죠",
          "original_form": "꺄 엄청 예쁘죠",
          "speaker_id": "2",
          "time": "20210827 10:47"
        },
        {
          "id": "MDRW2100018684.1.20",
          "form": "지금 키우시는 동물은 없으신가요?",
          "original_form": "지금 키우시는 동물은 없으신가요?",
          "speaker_id": "2",
          "time": "20210827 10:48"
        },
        {
          "id": "MDRW2100018684.1.21",
          "form": "네.. 생계가 불안정해서..",
          "original_form": "네.. 생계가 불안정해서..",
          "speaker_id": "1",
          "time": "20210827 10:49"
        },
        {
          "id": "MDRW2100018684.1.22",
          "form": "자리잡음 고양이 먼저 키우고 시퍼여",
          "original_form": "자리잡음 고양이 먼저 키우고 시퍼여",
          "speaker_id": "1",
          "time": "20210827 10:49"
        },
        {
          "id": "MDRW2100018684.1.23",
          "form": "참된분이시네욥!",
          "original_form": "참된분이시네욥!",
          "speaker_id": "2",
          "time": "20210827 10:50"
        },
        {
          "id": "MDRW2100018684.1.24",
          "form": "저도 얼른 자리잡아서 더 좋은 환경 마련해주고 싶어요",
          "original_form": "저도 얼른 자리잡아서 더 좋은 환경 마련해주고 싶어요",
          "speaker_id": "2",
          "time": "20210827 10:50"
        },
        {
          "id": "MDRW2100018684.1.25",
          "form": "ㅋㅋㅋ 한마리에요?",
          "original_form": "ㅋㅋㅋ 한마리에요?",
          "speaker_id": "1",
          "time": "20210827 10:50"
        },
        {
          "id": "MDRW2100018684.1.26",
          "form": "2마리요!",
          "original_form": "2마리요!",
          "speaker_id": "2",
          "time": "20210827 10:51"
        },
        {
          "id": "MDRW2100018684.1.27",
          "form": "남매예요",
          "original_form": "남매예요",
          "speaker_id": "2",
