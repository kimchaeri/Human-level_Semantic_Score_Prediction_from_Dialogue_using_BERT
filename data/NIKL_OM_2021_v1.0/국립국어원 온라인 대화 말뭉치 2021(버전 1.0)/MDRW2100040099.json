      "utterance": [
        {
          "id": "MDRW2100040099.1.1",
          "form": "오늘의메뉸 무엇일까나요",
          "original_form": "오늘의메뉸 무엇일까나요",
          "speaker_id": "1",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100040099.1.2",
          "form": "그러니까용ㅋㅋㅋㅋ뭐드실거에요?",
          "original_form": "그러니까용ㅋㅋㅋㅋ뭐드실거에요?",
          "speaker_id": "2",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100040099.1.3",
          "form": "글쎄용... 오늘 외식합니다!",
          "original_form": "글쎄용... 오늘 외식합니다!",
          "speaker_id": "1",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100040099.1.4",
          "form": "꺄악 외식 자주하시나요?",
          "original_form": "꺄악 외식 자주하시나요?",
          "speaker_id": "2",
          "time": "20210904 09:51"
        },
        {
          "id": "MDRW2100040099.1.5",
          "form": "올만입니다!",
          "original_form": "올만입니다!",
          "speaker_id": "1",
          "time": "20210904 09:51"
        },
        {
          "id": "MDRW2100040099.1.6",
          "form": "카페도 갈거거등요",
          "original_form": "카페도 갈거거등요",
          "speaker_id": "1",
          "time": "20210904 09:52"
        },
        {
          "id": "MDRW2100040099.1.7",
          "form": "우왕 카페 좋죠! 오늘 날씨가 아이스아메리카노 먹고싶은 날씬데",
          "original_form": "우왕 카페 좋죠! 오늘 날씨가 아이스아메리카노 먹고싶은 날씬데",
          "speaker_id": "2",
          "time": "20210904 09:54"
        },
        {
          "id": "MDRW2100040099.1.8",
          "form": "전 커피를 잘 못마셔서 슬퍼요ㅠ",
          "original_form": "전 커피를 잘 못마셔서 슬퍼요ㅠ",
          "speaker_id": "2",
          "time": "20210904 09:54"
        },
        {
          "id": "MDRW2100040099.1.9",
          "form": "흐엉 ㅜㅜ 카페인때문요?",
          "original_form": "흐엉 ㅜㅜ 카페인때문요?",
          "speaker_id": "1",
          "time": "20210904 09:57"
        },
        {
          "id": "MDRW2100040099.1.10",
          "form": "넹.... ㅠㅠㅠ name1님는 커피 잘 드시나요?",
          "original_form": "넹.... ㅠㅠㅠ &name1&님는 커피 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210904 09:58"
        },
        {
          "id": "MDRW2100040099.1.11",
          "form": "저도 마니 줄엇어용..",
          "original_form": "저도 마니 줄엇어용..",
          "speaker_id": "1",
          "time": "20210904 10:01"
        },
        {
          "id": "MDRW2100040099.1.12",
          "form": "아아최곤데 슬프네요",
          "original_form": "아아최곤데 슬프네요",
          "speaker_id": "1",
          "time": "20210904 10:02"
        },
        {
          "id": "MDRW2100040099.1.13",
          "form": "그쵸...흑흑... 아빠는 막 밤늦게도 커피마시던뎈ㅋㅋㅋㅋㅋㅋ부러워요...",
          "original_form": "그쵸...흑흑... 아빠는 막 밤늦게도 커피마시던뎈ㅋㅋㅋㅋㅋㅋ부러워요...",
          "speaker_id": "2",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100040099.1.14",
          "form": "전..이제무리요 그건",
          "original_form": "전..이제무리요 그건",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100040099.1.15",
          "form": "어제 그래서 잠못잣네유",
          "original_form": "어제 그래서 잠못잣네유",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100040099.1.16",
          "form": "에궁 ㅜㅜㅜ 피곤하시겠어요",
          "original_form": "에궁 ㅜㅜㅜ 피곤하시겠어요",
          "speaker_id": "2",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100040099.1.17",
          "form": "졸립다아 그래도 룰루랄라",
          "original_form": "졸립다아 그래도 룰루랄라",
          "speaker_id": "1",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100040099.1.18",
          "form": "맞아요 눈누난나 북촌에 좋은 카페 아시나요?",
          "original_form": "맞아요 눈누난나 북촌에 좋은 카페 아시나요?",
          "speaker_id": "2",
          "time": "20210904 10:06"
        }
      ]
