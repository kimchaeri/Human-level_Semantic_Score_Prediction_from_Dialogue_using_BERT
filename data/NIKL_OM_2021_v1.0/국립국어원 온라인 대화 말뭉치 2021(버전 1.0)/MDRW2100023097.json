      "utterance": [
        {
          "id": "MDRW2100023097.1.1",
          "form": "옛날에 한국지리 배울 때는 가고 싶은 곳 정말 많았는데",
          "original_form": "옛날에 한국지리 배울 때는 가고 싶은 곳 정말 많았는데",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023097.1.2",
          "form": "난 옛날에 가본 적도 별로 없어가지고",
          "original_form": "난 옛날에 가본 적도 별로 없어가지고",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023097.1.3",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023097.1.4",
          "form": "너무 슬프다 코로나 끝나면 많이 가",
          "original_form": "너무 슬프다 코로나 끝나면 많이 가",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023097.1.5",
          "form": "그래야 되는데",
          "original_form": "그래야 되는데",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023097.1.6",
          "form": "친구들이랑도 가고 가족들이랑도 가고",
          "original_form": "친구들이랑도 가고 가족들이랑도 가고",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023097.1.7",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023097.1.8",
          "form": "가보고 싶은 곳 제주도 말고 없어?",
          "original_form": "가보고 싶은 곳 제주도 말고 없어?",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023097.1.9",
          "form": "강릉이나 부산?",
          "original_form": "강릉이나 부산?",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023097.1.10",
          "form": "나도 부산 좋아",
          "original_form": "나도 부산 좋아",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023097.1.11",
          "form": "바닷가에서 해산물 같은거 먹고",
          "original_form": "바닷가에서 해산물 같은거 먹고",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023097.1.12",
          "form": "조갸구이 먹고 싶다",
          "original_form": "조갸구이 먹고 싶다",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023097.1.13",
          "form": "조개",
          "original_form": "조개",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023097.1.14",
          "form": "조개구이 맛있지",
          "original_form": "조개구이 맛있지",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023097.1.15",
          "form": "부산가서 먹었었는데",
          "original_form": "부산가서 먹었었는데",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023097.1.16",
          "form": "맞아 기억난다",
          "original_form": "맞아 기억난다",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023097.1.17",
          "form": "등산 좋아해?",
          "original_form": "등산 좋아해?",
          "speaker_id": "2",
          "time": "20210829 22:11"
        }
      ]
