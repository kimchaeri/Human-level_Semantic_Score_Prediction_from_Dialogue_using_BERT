      "utterance": [
        {
          "id": "MDRW2100019885.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019885.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019885.1.3",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019885.1.4",
          "form": "넹 마라탕 먹었어요",
          "original_form": "넹 마라탕 먹었어요",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019885.1.5",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019885.1.6",
          "form": "네! 저는 바질페스토파스타먹었어요~",
          "original_form": "네! 저는 바질페스토파스타먹었어요~",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019885.1.7",
          "form": "직접 해드신건가요",
          "original_form": "직접 해드신건가요",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019885.1.8",
          "form": "네~ 되게간단해서 직접해먹어요",
          "original_form": "네~ 되게간단해서 직접해먹어요",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019885.1.9",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019885.1.10",
          "form": "맛잇엇어여 ㅋㅋ 파스타좋아하시나여?",
          "original_form": "맛잇엇어여 ㅋㅋ 파스타좋아하시나여?",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019885.1.11",
          "form": "넹 근데 요새다이어트한다고",
          "original_form": "넹 근데 요새다이어트한다고",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019885.1.12",
          "form": "면을 두부면으로 먹어요...",
          "original_form": "면을 두부면으로 먹어요...",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019885.1.13",
          "form": "아 이런... 두부면이라... ㅋㅋㅋ 좋아하시나요?",
          "original_form": "아 이런... 두부면이라... ㅋㅋㅋ 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019885.1.14",
          "form": "두부를 원래 좋아해서 그런가",
          "original_form": "두부를 원래 좋아해서 그런가",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019885.1.15",
          "form": "나쁘진않긴한데",
          "original_form": "나쁘진않긴한데",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019885.1.16",
          "form": "그래도 탄수화물이 짱이에요ㅠㅠ",
          "original_form": "그래도 탄수화물이 짱이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019885.1.17",
          "form": "두부면 드셔보셨어요?",
          "original_form": "두부면 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019885.1.18",
          "form": "아 그래요?? 전 너무좋아하는데 두부면 넘 맛없더라구요 ㅠ",
          "original_form": "아 그래요?? 전 너무좋아하는데 두부면 넘 맛없더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019885.1.19",
          "form": "탄수화물이 최고!",
          "original_form": "탄수화물이 최고!",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019885.1.20",
          "form": "맞아요 그냥파스타면은",
          "original_form": "맞아요 그냥파스타면은",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100019885.1.21",
          "form": "이길수없어요",
          "original_form": "이길수없어요",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100019885.1.22",
          "form": "맞아요 ㅋㅋㅋㅋ 통밀이 최고죠~~",
          "original_form": "맞아요 ㅋㅋㅋㅋ 통밀이 최고죠~~",
          "speaker_id": "1",
          "time": "20210828 00:04"
        }
      ]
