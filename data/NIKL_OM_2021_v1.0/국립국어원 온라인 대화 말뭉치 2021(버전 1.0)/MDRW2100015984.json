      "utterance": [
        {
          "id": "MDRW2100015984.1.1",
          "form": "안녕하세용!",
          "original_form": "안녕하세용!",
          "speaker_id": "1",
          "time": "20210826 05:05"
        },
        {
          "id": "MDRW2100015984.1.2",
          "form": "오늘 서울 비온다고하네용",
          "original_form": "오늘 서울 비온다고하네용",
          "speaker_id": "1",
          "time": "20210826 05:06"
        },
        {
          "id": "MDRW2100015984.1.3",
          "form": "어제도 비오다 잠깐 안와서 좋았는데 말이에요",
          "original_form": "어제도 비오다 잠깐 안와서 좋았는데 말이에요",
          "speaker_id": "2",
          "time": "20210826 05:06"
        },
        {
          "id": "MDRW2100015984.1.4",
          "form": "네네ㅎㅎ 비오고 온도가 내려갈것같아요",
          "original_form": "네네ㅎㅎ 비오고 온도가 내려갈것같아요",
          "speaker_id": "1",
          "time": "20210826 05:06"
        },
        {
          "id": "MDRW2100015984.1.5",
          "form": "그건 좀 반갑네요 ㅎㅎㅎ무슨 계절 좋아하세요?",
          "original_form": "그건 좀 반갑네요 ㅎㅎㅎ무슨 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 05:07"
        },
        {
          "id": "MDRW2100015984.1.6",
          "form": "저는 겨울을 제일 좋아해요",
          "original_form": "저는 겨울을 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210826 05:07"
        },
        {
          "id": "MDRW2100015984.1.7",
          "form": "끝과 시작이 있는 계절이라서요ㅎㅎ",
          "original_form": "끝과 시작이 있는 계절이라서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 05:08"
        },
        {
          "id": "MDRW2100015984.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 05:08"
        },
        {
          "id": "MDRW2100015984.1.9",
          "form": "엇 겨울 좋아하는 분은 좀 낯서네요",
          "original_form": "엇 겨울 좋아하는 분은 좀 낯서네요",
          "speaker_id": "2",
          "time": "20210826 05:08"
        },
        {
          "id": "MDRW2100015984.1.10",
          "form": "전 봄이요!",
          "original_form": "전 봄이요!",
          "speaker_id": "2",
          "time": "20210826 05:08"
        },
        {
          "id": "MDRW2100015984.1.11",
          "form": "ㅎㅎ 봄 좋죠 봄에 피어나는 꽃들이 예뻐요",
          "original_form": "ㅎㅎ 봄 좋죠 봄에 피어나는 꽃들이 예뻐요",
          "speaker_id": "1",
          "time": "20210826 05:08"
        },
        {
          "id": "MDRW2100015984.1.12",
          "form": "맞아요 벚꽃 보면 힐링이되요",
          "original_form": "맞아요 벚꽃 보면 힐링이되요",
          "speaker_id": "2",
          "time": "20210826 05:08"
        },
        {
          "id": "MDRW2100015984.1.13",
          "form": "옷도 봄옷이 가장 예쁜것같아요",
          "original_form": "옷도 봄옷이 가장 예쁜것같아요",
          "speaker_id": "1",
          "time": "20210826 05:09"
        },
        {
          "id": "MDRW2100015984.1.14",
          "form": "색감이 예뻐요",
          "original_form": "색감이 예뻐요",
          "speaker_id": "1",
          "time": "20210826 05:09"
        },
        {
          "id": "MDRW2100015984.1.15",
          "form": "맞아요 ㅎㅎㅎ전 겨울은 눈 올 때가 좋아요",
          "original_form": "맞아요 ㅎㅎㅎ전 겨울은 눈 올 때가 좋아요",
          "speaker_id": "2",
          "time": "20210826 05:09"
        },
        {
          "id": "MDRW2100015984.1.16",
          "form": "저도 눈밟는 느낌이랑 소리가 좋더라구요ㅎㅎ",
          "original_form": "저도 눈밟는 느낌이랑 소리가 좋더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 05:10"
        },
        {
          "id": "MDRW2100015984.1.17",
          "form": "맞아요 아무도 안 밟은 눈 밟을 때 행복해요",
          "original_form": "맞아요 아무도 안 밟은 눈 밟을 때 행복해요",
          "speaker_id": "2",
          "time": "20210826 05:10"
        },
        {
          "id": "MDRW2100015984.1.18",
          "form": "ㅎㅎ 이제 가을이 지나고 곧 겨울이 오겠네요~",
          "original_form": "ㅎㅎ 이제 가을이 지나고 곧 겨울이 오겠네요~",
          "speaker_id": "1",
          "time": "20210826 05:10"
        }
      ]
