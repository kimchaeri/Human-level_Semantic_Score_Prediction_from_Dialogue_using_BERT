      "utterance": [
        {
          "id": "MDRW2100039782.1.1",
          "form": "안녕하세요 동물키우시나요?",
          "original_form": "안녕하세요 동물키우시나요?",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100039782.1.2",
          "form": "아니요 키우진 않고 있어요",
          "original_form": "아니요 키우진 않고 있어요",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039782.1.3",
          "form": "키우고 계신가요?",
          "original_form": "키우고 계신가요?",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039782.1.4",
          "form": "네 진돗개 키우고 있습니다",
          "original_form": "네 진돗개 키우고 있습니다",
          "speaker_id": "1",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039782.1.5",
          "form": "반려동물 키우고 싶지는 않으세요?",
          "original_form": "반려동물 키우고 싶지는 않으세요?",
          "speaker_id": "1",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039782.1.6",
          "form": "우와 진돗개라면 마당이나 그런 것도 있겠네요!\r\n밖에서 키우시나요?\r\n키우고 싶다는 생각도 했었는데 생명을 책임지는 일이 거대해서...ㅎㅎ 당장 급한 저부터 키우려구요.",
          "original_form": "우와 진돗개라면 마당이나 그런 것도 있겠네요!\r\n밖에서 키우시나요?\r\n키우고 싶다는 생각도 했었는데 생명을 책임지는 일이 거대해서...ㅎㅎ 당장 급한 저부터 키우려구요.",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039782.1.7",
          "form": "네 마당에서요~마자요",
          "original_form": "네 마당에서요~마자요",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039782.1.8",
          "form": "저를 키우는게 제일 좋죠",
          "original_form": "저를 키우는게 제일 좋죠",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039782.1.9",
          "form": "동물 알레르기 있으세요?",
          "original_form": "동물 알레르기 있으세요?",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039782.1.10",
          "form": "저밖에 못 키워서 저만 돌보고 있어요ㅋㅋㅋ 그래서 반려동물 키우시거나 아이 키우시는 분들 참 대단하더라구요!!",
          "original_form": "저밖에 못 키워서 저만 돌보고 있어요ㅋㅋㅋ 그래서 반려동물 키우시거나 아이 키우시는 분들 참 대단하더라구요!!",
          "speaker_id": "2",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039782.1.11",
          "form": "동물 알레르기는 딱히 없는데 털이 많이 날리면 힘들어하긴 해요",
          "original_form": "동물 알레르기는 딱히 없는데 털이 많이 날리면 힘들어하긴 해요",
          "speaker_id": "2",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039782.1.12",
          "form": "아 저도 비염이 있어서",
          "original_form": "아 저도 비염이 있어서",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039782.1.13",
          "form": "고양이 털은 진짜 힘들더라고요",
          "original_form": "고양이 털은 진짜 힘들더라고요",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039782.1.14",
          "form": "그래서 만약 반려동물을 만나게? 된다면 털은 많이 안 날리는 동물을 만나게 될 것 같아요ㅎㅎ",
          "original_form": "그래서 만약 반려동물을 만나게? 된다면 털은 많이 안 날리는 동물을 만나게 될 것 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039782.1.15",
          "form": "키우시는 진돗개는 이름이 뭔가요?",
          "original_form": "키우시는 진돗개는 이름이 뭔가요?",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039782.1.16",
          "form": "그죠 푸들이 많이 안날린대요",
          "original_form": "그죠 푸들이 많이 안날린대요",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039782.1.17",
          "form": "저는 통통이요",
          "original_form": "저는 통통이요",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039782.1.18",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039782.1.19",
          "form": "통통이 이름이 너무 귀엽네요!",
          "original_form": "통통이 이름이 너무 귀엽네요!",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039782.1.20",
          "form": "어떻게 해서 지어지게 된 거예요~?",
          "original_form": "어떻게 해서 지어지게 된 거예요~?",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039782.1.21",
          "form": "그냥 단순해요",
          "original_form": "그냥 단순해요",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039782.1.22",
          "form": "통통하게 잘 자라라 해서",
          "original_form": "통통하게 잘 자라라 해서",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039782.1.23",
          "form": "진짜 통통해요",
          "original_form": "진짜 통통해요",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039782.1.24",
          "form": "그렇군요 이름대로 건강하게 자라고 있네요ㅎㅎ",
          "original_form": "그렇군요 이름대로 건강하게 자라고 있네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100039782.1.25",
          "form": "통통이는 어떤 놀이?를 제일 좋아해요?",
          "original_form": "통통이는 어떤 놀이?를 제일 좋아해요?",
          "speaker_id": "2",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100039782.1.26",
          "form": "음 터그놀이요",
          "original_form": "음 터그놀이요",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100039782.1.27",
          "form": "간식으로 노는 놀이를 제일 좋아하긴해요..",
          "original_form": "간식으로 노는 놀이를 제일 좋아하긴해요..",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100039782.1.28",
          "form": "먹는거 좋아해서ㅋㅋㅋㅋ",
          "original_form": "먹는거 좋아해서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100039782.1.29",
          "form": "이름이랑 잘 어울리는 놀이를 좋아하네요ㅋㅋ",
          "original_form": "이름이랑 잘 어울리는 놀이를 좋아하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100039782.1.30",
          "form": "그니까요ㅋㅋㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:57"
        },
        {
          "id": "MDRW2100039782.1.31",
          "form": "귀엽죻ㅎㅎ",
          "original_form": "귀엽죻ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:57"
        },
        {
          "id": "MDRW2100039782.1.32",
          "form": "통통이는 가족이 된지 몇 년 되었어요?",
          "original_form": "통통이는 가족이 된지 몇 년 되었어요?",
          "speaker_id": "2",
          "time": "20210905 15:44"
        },
        {
          "id": "MDRW2100039782.1.33",
