      "utterance": [
        {
          "id": "MDRW2100021488.1.1",
          "form": "치킨 좋아하세요?",
          "original_form": "치킨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100021488.1.2",
          "form": "치킨 진짜 다이어트계의 원수잖아요",
          "original_form": "치킨 진짜 다이어트계의 원수잖아요",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021488.1.3",
          "form": "애증의 결정체",
          "original_form": "애증의 결정체",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021488.1.4",
          "form": "전 옛날통닭도 좋아해요",
          "original_form": "전 옛날통닭도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021488.1.5",
          "form": "어느브랜드 무슨치킨 좋아하세용?",
          "original_form": "어느브랜드 무슨치킨 좋아하세용?",
          "speaker_id": "1",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021488.1.6",
          "form": "ㅋㅋㅋㅋ 저두요",
          "original_form": "ㅋㅋㅋㅋ 저두요",
          "speaker_id": "1",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021488.1.7",
          "form": "처갓집이나 맥시칸같이",
          "original_form": "처갓집이나 맥시칸같이",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021488.1.8",
          "form": "옛날브랜드는 추억의 맛이예요",
          "original_form": "옛날브랜드는 추억의 맛이예요",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021488.1.9",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021488.1.10",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021488.1.11",
          "form": "ㅋ페리카나 양념치킨 좋아해요",
          "original_form": "ㅋ페리카나 양념치킨 좋아해요",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021488.1.12",
          "form": "양념은 이집 못따라오는것같아요",
          "original_form": "양념은 이집 못따라오는것같아요",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021488.1.13",
          "form": "헐 먹어보고싶어요 페리카나도",
          "original_form": "헐 먹어보고싶어요 페리카나도",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021488.1.14",
          "form": "전 치킨 사이들ᆢ",
          "original_form": "전 치킨 사이들ᆢ",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021488.1.15",
          "form": "사이드로 감튀가 제일 좋아요",
          "original_form": "사이드로 감튀가 제일 좋아요",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021488.1.16",
          "form": "name1은 어떠세요?",
          "original_form": "&name1&은 어떠세요?",
          "speaker_id": "2",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100021488.1.17",
          "form": "저두요ㅋㅋㅋ 교촌시킬때",
          "original_form": "저두요ㅋㅋㅋ 교촌시킬때",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100021488.1.18",
          "form": "웨지감자 꼭 시켜요",
          "original_form": "웨지감자 꼭 시켜요",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100021488.1.19",
          "form": "웨지감자 짱이죠 진짜",
          "original_form": "웨지감자 짱이죠 진짜",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021488.1.20",
          "form": "그리고 치즈볼도",
          "original_form": "그리고 치즈볼도",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021488.1.21",
          "form": "대박인것같아요",
          "original_form": "대박인것같아요",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021488.1.22",
          "form": "치즈볼은 bhc가 맛있더라구요ㅋㅋㅋㅋ",
          "original_form": "치즈볼은 bhc가 맛있더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021488.1.23",
          "form": "우와 치킨브랜드 전반을 꿰고 계신것같아요",
          "original_form": "우와 치킨브랜드 전반을 꿰고 계신것같아요",
          "speaker_id": "2",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100021488.1.24",
          "form": "콜라나 치킨무도 드시나요",
          "original_form": "콜라나 치킨무도 드시나요",
          "speaker_id": "2",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100021488.1.25",
          "form": "전 콜라는 안먹어욛",
          "original_form": "전 콜라는 안먹어욛",
          "speaker_id": "2",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100021488.1.26",
          "form": "몸에 안좋은거 다 먹는 느낌이네요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "몸에 안좋은거 다 먹는 느낌이네요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100021488.1.27",
          "form": "콜라도 먹어용",
          "original_form": "콜라도 먹어용",
          "speaker_id": "1",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100021488.1.28",
          "form": "근데 제로콜라는 안먹어용",
          "original_form": "근데 제로콜라는 안먹어용",
          "speaker_id": "1",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100021488.1.29",
          "form": "무슨차이인가요???",
          "original_form": "무슨차이인가요???",
          "speaker_id": "2",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021488.1.30",
          "form": "제로콜라는 맛이 없나요",
          "original_form": "제로콜라는 맛이 없나요",
          "speaker_id": "2",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021488.1.31",
          "form": "한번사는 인생 맛있는거 잔뜩 먹고가는거죠",
          "original_form": "한번사는 인생 맛있는거 잔뜩 먹고가는거죠",
          "speaker_id": "2",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021488.1.32",
          "form": "제로콜라는 설탕대신 액상과당이 들어갔고",
          "original_form": "제로콜라는 설탕대신 액상과당이 들어갔고",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021488.1.33",
          "form": "콜라는 설탕이 들어가있어용!",
          "original_form": "콜라는 설탕이 들어가있어용!",
          "speaker_id": "1",
