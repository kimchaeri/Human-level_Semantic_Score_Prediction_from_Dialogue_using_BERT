      "utterance": [
        {
          "id": "MDRW2100033510.1.1",
          "form": "넷플릭스 보고싶은 거 있을 때 구독하는 편이에요",
          "original_form": "넷플릭스 보고싶은 거 있을 때 구독하는 편이에요",
          "speaker_id": "1",
          "time": "20210824 09:49"
        },
        {
          "id": "MDRW2100033510.1.2",
          "form": "정기구독은 안하고있구요",
          "original_form": "정기구독은 안하고있구요",
          "speaker_id": "1",
          "time": "20210824 09:49"
        },
        {
          "id": "MDRW2100033510.1.3",
          "form": "저는 그 4명 모으는거 깨진 이후 안봤어요 ㅋㅋ",
          "original_form": "저는 그 4명 모으는거 깨진 이후 안봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:50"
        },
        {
          "id": "MDRW2100033510.1.4",
          "form": "그거 모으는게 은근 일이던 ㅋㅋㅋ",
          "original_form": "그거 모으는게 은근 일이던 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:51"
        },
        {
          "id": "MDRW2100033510.1.5",
          "form": "아 맞아요 스트레스 받을 거 같더라구요",
          "original_form": "아 맞아요 스트레스 받을 거 같더라구요",
          "speaker_id": "1",
          "time": "20210824 09:51"
        },
        {
          "id": "MDRW2100033510.1.6",
          "form": "넷플릭스에서 재밌게 본 거 있으신가요",
          "original_form": "넷플릭스에서 재밌게 본 거 있으신가요",
          "speaker_id": "1",
          "time": "20210824 09:51"
        },
        {
          "id": "MDRW2100033510.1.7",
          "form": "뭔가 진짜 별거아닌데 스트레스 받더라고요",
          "original_form": "뭔가 진짜 별거아닌데 스트레스 받더라고요",
          "speaker_id": "2",
          "time": "20210824 09:52"
        },
        {
          "id": "MDRW2100033510.1.8",
          "form": "포기하고 나니 차라리 짐을 벗어던진것처럼 편하더라고요",
          "original_form": "포기하고 나니 차라리 짐을 벗어던진것처럼 편하더라고요",
          "speaker_id": "2",
          "time": "20210824 09:52"
        },
        {
          "id": "MDRW2100033510.1.9",
          "form": "아무래도 총대면 신경 쓸 일이 많으니까요",
          "original_form": "아무래도 총대면 신경 쓸 일이 많으니까요",
          "speaker_id": "1",
          "time": "20210824 09:53"
        },
        {
          "id": "MDRW2100033510.1.10",
          "form": "갑자기 제목 기억안나는데",
          "original_form": "갑자기 제목 기억안나는데",
          "speaker_id": "2",
          "time": "20210824 09:55"
        },
        {
          "id": "MDRW2100033510.1.11",
          "form": "70년대 배경 의",
          "original_form": "70년대 배경 의",
          "speaker_id": "2",
          "time": "20210824 09:55"
        },
        {
          "id": "MDRW2100033510.1.12",
          "form": "외계인",
          "original_form": "외계인",
          "speaker_id": "2",
          "time": "20210824 09:55"
        },
        {
          "id": "MDRW2100033510.1.13",
          "form": "불가사의 현상 나오는 드라마가 있었ㄴㄷㄴ데",
          "original_form": "불가사의 현상 나오는 드라마가 있었ㄴㄷㄴ데",
          "speaker_id": "2",
          "time": "20210824 09:56"
        },
        {
          "id": "MDRW2100033510.1.14",
          "form": "었는데",
          "original_form": "었는데",
          "speaker_id": "2",
          "time": "20210824 09:56"
        },
        {
          "id": "MDRW2100033510.1.15",
          "form": "외국거였나요",
          "original_form": "외국거였나요",
          "speaker_id": "1",
          "time": "20210824 09:56"
        },
        {
          "id": "MDRW2100033510.1.16",
          "form": "음.. 뭐지 영화였나요 드라마였나요",
          "original_form": "음.. 뭐지 영화였나요 드라마였나요",
          "speaker_id": "1",
          "time": "20210824 09:57"
        },
        {
          "id": "MDRW2100033510.1.17",
          "form": "외국꺼요",
          "original_form": "외국꺼요",
          "speaker_id": "2",
          "time": "20210824 10:01"
        },
        {
          "id": "MDRW2100033510.1.18",
          "form": "그거랑",
          "original_form": "그거랑",
          "speaker_id": "2",
          "time": "20210824 10:01"
        },
        {
          "id": "MDRW2100033510.1.19",
          "form": "킹덤이 유명한거 같았어요",
          "original_form": "킹덤이 유명한거 같았어요",
          "speaker_id": "2",
          "time": "20210824 10:01"
        },
        {
          "id": "MDRW2100033510.1.20",
          "form": "킹덤 유명하더라구요",
          "original_form": "킹덤 유명하더라구요",
          "speaker_id": "1",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100033510.1.21",
          "form": "전 좀 잔인할걸 못 봐서",
          "original_form": "전 좀 잔인할걸 못 봐서",
          "speaker_id": "1",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100033510.1.22",
          "form": "킹덤은 아직 안봤어요",
          "original_form": "킹덤은 아직 안봤어요",
          "speaker_id": "1",
          "time": "20210824 10:03"
        },
        {
          "id": "MDRW2100033510.1.23",
          "form": "근데 개별결제면",
          "original_form": "근데 개별결제면",
          "speaker_id": "2",
          "time": "20210824 10:04"
        },
        {
          "id": "MDRW2100033510.1.24",
          "form": "굳이 안보는게",
          "original_form": "굳이 안보는게",
          "speaker_id": "2",
          "time": "20210824 10:04"
        },
        {
          "id": "MDRW2100033510.1.25",
          "form": "킹덤 보셨나요",
          "original_form": "킹덤 보셨나요",
          "speaker_id": "1",
          "time": "20210824 10:04"
        },
        {
          "id": "MDRW2100033510.1.26",
          "form": "저는 유투브에서 영상보니까",
          "original_form": "저는 유투브에서 영상보니까",
          "speaker_id": "2",
          "time": "20210824 10:05"
        },
        {
          "id": "MDRW2100033510.1.27",
          "form": "아 이렇게 보면 되겠구나 싶었어요 ㅋㅋ",
          "original_form": "아 이렇게 보면 되겠구나 싶었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:05"
        },
        {
          "id": "MDRW2100033510.1.28",
          "form": "아하..",
          "original_form": "아하..",
          "speaker_id": "1",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100033510.1.29",
          "form": "유튜브에도 클립이 올라오니까요",
          "original_form": "유튜브에도 클립이 올라오니까요",
          "speaker_id": "1",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100033510.1.30",
          "form": "네 물론 풀버전도 정작 보면 시간 가는줄 몰랐을지도 모르지만요",
          "original_form": "네 물론 풀버전도 정작 보면 시간 가는줄 몰랐을지도 모르지만요",
          "speaker_id": "2",
          "time": "20210824 10:19"
        },
        {
          "id": "MDRW2100033510.1.31",
          "form": "시간 날 때 보면 좋겠어요",
          "original_form": "시간 날 때 보면 좋겠어요",
