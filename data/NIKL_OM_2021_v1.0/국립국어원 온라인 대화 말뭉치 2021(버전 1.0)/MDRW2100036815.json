        },
        {
          "id": "MDRW2100036815.1.2",
          "form": "입벌리셨어여?",
          "original_form": "입벌리셨어여?",
          "speaker_id": "2",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100036815.1.3",
          "form": "ㅋㅋㅋ기",
          "original_form": "ㅋㅋㅋ기",
          "speaker_id": "2",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100036815.1.4",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100036815.1.5",
          "form": "입 지금 열심히",
          "original_form": "입 지금 열심히",
          "speaker_id": "1",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100036815.1.6",
          "form": "아 하고 있어요",
          "original_form": "아 하고 있어요",
          "speaker_id": "1",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100036815.1.7",
          "form": "지금 점심 먹고있어요 ㅎㅎ",
          "original_form": "지금 점심 먹고있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100036815.1.8",
          "form": "갑니다욧ㅋㅋ",
          "original_form": "갑니다욧ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100036815.1.9",
          "form": "어떤거 드셔용?",
          "original_form": "어떤거 드셔용?",
          "speaker_id": "2",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100036815.1.10",
          "form": "저 오리불고기 먹었습니당 ㅎㅎㅅ !!",
          "original_form": "저 오리불고기 먹었습니당 ㅎㅎㅅ !!",
          "speaker_id": "1",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100036815.1.11",
          "form": "엇.. 엄청 맛있는거 드셨네용",
          "original_form": "엇.. 엄청 맛있는거 드셨네용",
          "speaker_id": "2",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100036815.1.12",
          "form": "야무지게 볶음밥두 먹었어요...!",
          "original_form": "야무지게 볶음밥두 먹었어요...!",
          "speaker_id": "1",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100036815.1.13",
          "form": "크으 불고기에 볶음밥 필수져!!",
          "original_form": "크으 불고기에 볶음밥 필수져!!",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100036815.1.14",
          "form": "전 점심 먹고 배불러서 졸았어요ㅠㅜㅋㅋ",
          "original_form": "전 점심 먹고 배불러서 졸았어요ㅠㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036815.1.15",
          "form": "저두ㅋㅋㅋ 잠깐 잤어욬ㅋㅋㅋ",
          "original_form": "저두ㅋㅋㅋ 잠깐 잤어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100036815.1.16",
          "form": "저녁은 뭐드셔요!",
          "original_form": "저녁은 뭐드셔요!",
          "speaker_id": "1",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100036815.1.17",
          "form": "전하고 된장국이욥!",
          "original_form": "전하고 된장국이욥!",
          "speaker_id": "2",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100036815.1.18",
          "form": "점심에서 진화했슴닷ㅋㅋㅋ",
          "original_form": "점심에서 진화했슴닷ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100036815.1.19",
