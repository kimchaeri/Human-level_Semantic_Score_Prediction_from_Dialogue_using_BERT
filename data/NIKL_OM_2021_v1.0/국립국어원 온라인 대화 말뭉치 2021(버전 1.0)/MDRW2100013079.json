          "speaker_id": "2",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100013079.1.12",
          "form": "약간 망함",
          "original_form": "약간 망함",
          "speaker_id": "2",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100013079.1.13",
          "form": "앜ㅋㅋㅋㅋㅋ아니",
          "original_form": "앜ㅋㅋㅋㅋㅋ아니",
          "speaker_id": "1",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100013079.1.14",
          "form": "망했다니",
          "original_form": "망했다니",
          "speaker_id": "1",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100013079.1.15",
          "form": "노잼이었음?",
          "original_form": "노잼이었음?",
          "speaker_id": "1",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100013079.1.16",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013079.1.17",
          "form": "개연성이 약한 부분도 있었고",
          "original_form": "개연성이 약한 부분도 있었고",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013079.1.18",
          "form": "설정이 어려워서",
          "original_form": "설정이 어려워서",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013079.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ아아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아아",
          "speaker_id": "1",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013079.1.20",
          "form": "약간 그런",
          "original_form": "약간 그런",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013079.1.21",
          "form": "설정이어려우면",
          "original_form": "설정이어려우면",
          "speaker_id": "1",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013079.1.22",
          "form": "개언성이",
          "original_form": "개언성이",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013079.1.23",
          "form": "좋기힘들더라",
          "original_form": "좋기힘들더라",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013079.1.24",
          "form": "작가역량이좋지않은이상",
          "original_form": "작가역량이좋지않은이상",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013079.1.25",
          "form": "그니까 설정이 어려우면 다른건 최대한 간단히 했었어야 하는데",
          "original_form": "그니까 설정이 어려우면 다른건 최대한 간단히 했었어야 하는데",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013079.1.26",
          "form": "그게 안돼서 망함",
          "original_form": "그게 안돼서 망함",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013079.1.27",
          "form": "그리고 주연이",
          "original_form": "그리고 주연이",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013079.1.28",
          "form": "시청자들도조금힘들어함",
          "original_form": "시청자들도조금힘들어함",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013079.1.29",
          "form": "ㅇㅇ주연?",
          "original_form": "ㅇㅇ주연?",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013079.1.30",
          "form": "누구였는데",
          "original_form": "누구였는데",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013079.1.31",
          "form": "조승우 박신혜였어",
          "original_form": "조승우 박신혜였어",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013079.1.32",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013079.1.33",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013079.1.34",
          "form": "아님?",
          "original_form": "아님?",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013079.1.35",
          "form": "미래어쩌고하는거",
          "original_form": "미래어쩌고하는거",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013079.1.36",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013079.1.37",
          "form": "엄마가보는거봤음",
          "original_form": "엄마가보는거봤음",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013079.1.38",
          "form": "타임 머신을 만든 조승우와 그 타임머신을 타고 넘어와서 조승우 지키는 박신혜",
          "original_form": "타임 머신을 만든 조승우와 그 타임머신을 타고 넘어와서 조승우 지키는 박신혜",
          "speaker_id": "2",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100013079.1.39",
          "form": "둘이 10살 차이임",
          "original_form": "둘이 10살 차이임",
          "speaker_id": "2",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100013079.1.40",
          "form": "아 맞음",
          "original_form": "아 맞음",
          "speaker_id": "1",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100013079.1.41",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100013079.1.42",
          "form": "둘이러브라인이었음?",
          "original_form": "둘이러브라인이었음?",
          "speaker_id": "1",
          "time": "20210825 12:23"
