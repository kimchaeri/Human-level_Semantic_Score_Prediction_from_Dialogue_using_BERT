      "utterance": [
        {
          "id": "MDRW2100002849.1.1",
          "form": "가족구성이 어떻게 되시나요",
          "original_form": "가족구성이 어떻게 되시나요",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002849.1.2",
          "form": "저는 남편하고 둘이요",
          "original_form": "저는 남편하고 둘이요",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002849.1.3",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002849.1.4",
          "form": "저도요 ㅎㅎ",
          "original_form": "저도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002849.1.5",
          "form": "가사일 힘들어요",
          "original_form": "가사일 힘들어요",
          "speaker_id": "1",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002849.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002849.1.7",
          "form": "청소 매일 해야하니까 ㅋㅋ",
          "original_form": "청소 매일 해야하니까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002849.1.8",
          "form": "먼지는 매일 쌓이더라구요",
          "original_form": "먼지는 매일 쌓이더라구요",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002849.1.9",
          "form": "집안일은 역시 힘들어요",
          "original_form": "집안일은 역시 힘들어요",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002849.1.10",
          "form": "일이 한도 끝도 없죠",
          "original_form": "일이 한도 끝도 없죠",
          "speaker_id": "2",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002849.1.11",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002849.1.12",
          "form": "빨래 돌려야하는데 ㅋㅋ",
          "original_form": "빨래 돌려야하는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002849.1.13",
          "form": "빨래 자주 돌리세요?",
          "original_form": "빨래 자주 돌리세요?",
          "speaker_id": "2",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002849.1.14",
          "form": "아뇨 저는 일주일에 한 두번만 ㅋㅋ",
          "original_form": "아뇨 저는 일주일에 한 두번만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002849.1.15",
          "form": "다들 그렇죠 ㅎㅎ",
          "original_form": "다들 그렇죠 ㅎㅎ",
