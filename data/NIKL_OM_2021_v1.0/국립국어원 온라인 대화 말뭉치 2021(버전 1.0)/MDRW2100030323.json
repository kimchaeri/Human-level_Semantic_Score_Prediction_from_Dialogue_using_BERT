      "utterance": [
        {
          "id": "MDRW2100030323.1.1",
          "form": "3삼일정도는 몸조리 조심",
          "original_form": "3삼일정도는 몸조리 조심",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030323.1.2",
          "form": "네!  그래서 최대한 집에서 요양하듯",
          "original_form": "네!  그래서 최대한 집에서 요양하듯",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030323.1.3",
          "form": "쉬면서 있으려구요",
          "original_form": "쉬면서 있으려구요",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030323.1.4",
          "form": "ㅜㅜ너무 무리하지마세여",
          "original_form": "ㅜㅜ너무 무리하지마세여",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030323.1.5",
          "form": "네네! 감사해요 \r\n무리하지 말고 푹 쉬어야죠 \r\n내일은 어떨지 걱정이 되기 시작하네요",
          "original_form": "네네! 감사해요 \r\n무리하지 말고 푹 쉬어야죠 \r\n내일은 어떨지 걱정이 되기 시작하네요",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030323.1.6",
          "form": "말 좀 적게해도되여",
          "original_form": "말 좀 적게해도되여",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030323.1.7",
          "form": "팔아픈데",
          "original_form": "팔아픈데",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030323.1.8",
          "form": "네 그럴게요\r\n줄여야겠어요",
          "original_form": "네 그럴게요\r\n줄여야겠어요",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030323.1.9",
          "form": "뭐좀 받치고해여",
          "original_form": "뭐좀 받치고해여",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030323.1.10",
          "form": "최대한 오름손으로",
          "original_form": "최대한 오름손으로",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030323.1.11",
          "form": "다리를 받침대 삼고 있어요",
          "original_form": "다리를 받침대 삼고 있어요",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030323.1.12",
          "form": "그걸로는..붕편",
          "original_form": "그걸로는..붕편",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030323.1.13",
          "form": "더 좋은 방법을 찾아야겠네요",
          "original_form": "더 좋은 방법을 찾아야겠네요",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030323.1.14",
          "form": "뭔가  받침대옶내여",
          "original_form": "뭔가  받침대옶내여",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030323.1.15",
          "form": "잠시 책상에 올려놓는걸로 바꿨어요",
          "original_form": "잠시 책상에 올려놓는걸로 바꿨어요",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030323.1.16",
          "form": "네 잘햇어여",
          "original_form": "네 잘햇어여",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030323.1.17",
          "form": "새로운 자세를 찾았어요",
          "original_form": "새로운 자세를 찾았어요",
          "speaker_id": "2",
          "time": "20210904 00:25"
        }
      ]
