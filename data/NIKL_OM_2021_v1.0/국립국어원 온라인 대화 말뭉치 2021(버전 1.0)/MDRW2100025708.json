      "utterance": [
        {
          "id": "MDRW2100025708.1.1",
          "form": "마스크가드 말고 샀다는 거 얘기 좀 더 해줘ㅋㅋㅋ",
          "original_form": "마스크가드 말고 샀다는 거 얘기 좀 더 해줘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025708.1.2",
          "form": "음 아 바람막이샀어 이거 런데이용",
          "original_form": "음 아 바람막이샀어 이거 런데이용",
          "speaker_id": "2",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025708.1.3",
          "form": "아하 바람막이는 어디거 샀어??",
          "original_form": "아하 바람막이는 어디거 샀어??",
          "speaker_id": "1",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025708.1.4",
          "form": "행텐알아? 거기껀데 여름용이라 되게 얇고 저렴해",
          "original_form": "행텐알아? 거기껀데 여름용이라 되게 얇고 저렴해",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025708.1.5",
          "form": "아 알아알아 얼마에 샀는데??",
          "original_form": "아 알아알아 얼마에 샀는데??",
          "speaker_id": "1",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025708.1.6",
          "form": "어 만이천원 ㅋㅋ 싸지?",
          "original_form": "어 만이천원 ㅋㅋ 싸지?",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025708.1.7",
          "form": "완전 여름용",
          "original_form": "완전 여름용",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025708.1.8",
          "form": "오 되게싸다 진짜루",
          "original_form": "오 되게싸다 진짜루",
          "speaker_id": "1",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025708.1.9",
          "form": "ㅋㅋㅋㅋ 그치 근데 아직 안와서 오늘 이ㅏ따 입어봐야돼 ㅋㅋㅋ 후기는 좋더라",
          "original_form": "ㅋㅋㅋㅋ 그치 근데 아직 안와서 오늘 이ㅏ따 입어봐야돼 ㅋㅋㅋ 후기는 좋더라",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025708.1.10",
          "form": "ㅋㅋㅋ오고나면 여름 다갔겠는데...",
          "original_form": "ㅋㅋㅋ오고나면 여름 다갔겠는데...",
          "speaker_id": "1",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025708.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ..그래도 비와서 좀 습하고 더우니깐...ㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ..그래도 비와서 좀 습하고 더우니깐...ㅋ",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025708.1.12",
          "form": "그니까 주말부터 런데이해야한다구",
          "original_form": "그니까 주말부터 런데이해야한다구",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025708.1.13",
          "form": "ㅋㅋㅋㅋㅋ 하긴.. 한.. 한달은 더 입어도 되겠따",
          "original_form": "ㅋㅋㅋㅋㅋ 하긴.. 한.. 한달은 더 입어도 되겠따",
          "speaker_id": "1",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025708.1.14",
          "form": "또 다른거 어떤거???",
          "original_form": "또 다른거 어떤거???",
          "speaker_id": "1",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100025708.1.15",
          "form": "다른거 뭐샀냐구?",
          "original_form": "다른거 뭐샀냐구?",
          "speaker_id": "2",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100025708.1.16",
          "form": "어 어떤거 샀어??",
          "original_form": "어 어떤거 샀어??",
