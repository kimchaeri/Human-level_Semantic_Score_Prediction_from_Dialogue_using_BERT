      "utterance": [
        {
          "id": "MDRW2100034672.1.1",
          "form": "섞으면 좀 느끼해져서",
          "original_form": "섞으면 좀 느끼해져서",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034672.1.2",
          "form": "쭈우욱 늘어나는걸 보는게 제맛 ㅠㅠ",
          "original_form": "쭈우욱 늘어나는걸 보는게 제맛 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034672.1.3",
          "form": "오맞아요 ㅜㅜ",
          "original_form": "오맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034672.1.4",
          "form": "섞으면 좀.. 사라지지 않나요 녹아서!?",
          "original_form": "섞으면 좀.. 사라지지 않나요 녹아서!?",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034672.1.5",
          "form": "맞아요 ㅋㅋㅋㅋ 있는줄도 모르겠고",
          "original_form": "맞아요 ㅋㅋㅋㅋ 있는줄도 모르겠고",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034672.1.6",
          "form": "다른건 뭐 젛아하세요?",
          "original_form": "다른건 뭐 젛아하세요?",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034672.1.7",
          "form": "흠 저는 단거는 다 좋아해요!",
          "original_form": "흠 저는 단거는 다 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034672.1.8",
          "form": "빵순이에여 ㅋㅋㅋ",
          "original_form": "빵순이에여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034672.1.9",
          "form": "무슨 빵이 좋아요?",
          "original_form": "무슨 빵이 좋아요?",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034672.1.10",
          "form": "저도 단거 사랑해요 ㅎㅎ",
          "original_form": "저도 단거 사랑해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034672.1.11",
          "form": "흠 저는 시나몬 환장해섴ㅋㅋ",
          "original_form": "흠 저는 시나몬 환장해섴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034672.1.12",
          "form": "시나몬롤이나 마늘빵이요",
          "original_form": "시나몬롤이나 마늘빵이요",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034672.1.13",
          "form": "아 시나몬 너무 좋져",
          "original_form": "아 시나몬 너무 좋져",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034672.1.14",
          "form": "그 향이 ㅠㅠ",
          "original_form": "그 향이 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034672.1.15",
          "form": "저는 밀크티에 뿌려진게 잴 맛있더라고요",
          "original_form": "저는 밀크티에 뿌려진게 잴 맛있더라고요",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034672.1.16",
          "form": "아 전 밀크티는 불호라 못 먹어봤어요 ㅠㅠ",
          "original_form": "아 전 밀크티는 불호라 못 먹어봤어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100034672.1.17",
          "form": "밀크티에도 뿌려주는지는 몰랐네요 ㅋㅋㅋ",
          "original_form": "밀크티에도 뿌려주는지는 몰랐네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100034672.1.18",
          "form": "가끔 카페에서 뿌려주는 곳이 있더하고요",
          "original_form": "가끔 카페에서 뿌려주는 곳이 있더하고요",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100034672.1.19",
          "form": "저는 그런곳만 열심히 찾아가는 ㅎㅎ",
          "original_form": "저는 그런곳만 열심히 찾아가는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100034672.1.20",
          "form": "밀크티도 호불호가 많이 갈리죠",
          "original_form": "밀크티도 호불호가 많이 갈리죠",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034672.1.21",
          "form": "이왕이면 뿌려주는 데로가야죠 ㅋㅋㅋ",
          "original_form": "이왕이면 뿌려주는 데로가야죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034672.1.22",
          "form": "아 원래 호불호갈리는 음식이었군요 ㅋㅋㅋ",
          "original_form": "아 원래 호불호갈리는 음식이었군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034672.1.23",
          "form": "저도 원래는 불호였거든요",
          "original_form": "저도 원래는 불호였거든요",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100034672.1.24",
          "form": "근데 요즘은..",
          "original_form": "근데 요즘은..",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100034672.1.25",
          "form": "카페인 중독..?",
          "original_form": "카페인 중독..?",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100034672.1.26",
          "form": "카페요??",
          "original_form": "카페요??",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100034672.1.27",
          "form": "카페인 들어잇는 음료 다 너무 젛아요 ㅎㅎ..",
          "original_form": "카페인 들어잇는 음료 다 너무 젛아요 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100034672.1.28",
          "form": "맛있더라고요",
          "original_form": "맛있더라고요",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100034672.1.29",
          "form": "아 ㅋㅋㅋㅋㅋ 카페이름 말하시는줄 알았네요",
          "original_form": "아 ㅋㅋㅋㅋㅋ 카페이름 말하시는줄 알았네요",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100034672.1.30",
          "form": "저희집 근처 장사 잘되는 집이름이거든요 ㅋㅋㅋ",
          "original_form": "저희집 근처 장사 잘되는 집이름이거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:32"
        }
      ]
