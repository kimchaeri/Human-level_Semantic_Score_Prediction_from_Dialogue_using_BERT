      "utterance": [
        {
          "id": "MDRW2100018620.1.1",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018620.1.2",
          "form": "음 너무 심하게 얼마전 한후로",
          "original_form": "음 너무 심하게 얼마전 한후로",
          "speaker_id": "1",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018620.1.3",
          "form": "요샌 걷기만요",
          "original_form": "요샌 걷기만요",
          "speaker_id": "1",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018620.1.4",
          "form": "하기가 싫으네요",
          "original_form": "하기가 싫으네요",
          "speaker_id": "1",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018620.1.5",
          "form": "요요와서 다 때려쳤어요 ㅋㅋ",
          "original_form": "요요와서 다 때려쳤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018620.1.6",
          "form": "비도 와서 걷기도 쉽지않죠ㅠ",
          "original_form": "비도 와서 걷기도 쉽지않죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018620.1.7",
          "form": "앗..요요!",
          "original_form": "앗..요요!",
          "speaker_id": "2",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018620.1.8",
          "form": "최악이에요 왜 요요가 오는건지ㅠ",
          "original_form": "최악이에요 왜 요요가 오는건지ㅠ",
          "speaker_id": "2",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018620.1.9",
          "form": "네 일러거면 뭐하러하나",
          "original_form": "네 일러거면 뭐하러하나",
          "speaker_id": "1",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018620.1.10",
          "form": "현타왔어요",
          "original_form": "현타왔어요",
          "speaker_id": "1",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018620.1.11",
          "form": "알죠ㅠ 전 그순간 탁 놔버렸더니 지금 돌이킬수없어져서 이번엔 건강하게 해보려해요ㅠ",
          "original_form": "알죠ㅠ 전 그순간 탁 놔버렸더니 지금 돌이킬수없어져서 이번엔 건강하게 해보려해요ㅠ",
          "speaker_id": "2",
          "time": "20210827 09:29"
        },
        {
          "id": "MDRW2100018620.1.12",
          "form": "네 꼭 건강하게요",
          "original_form": "네 꼭 건강하게요",
          "speaker_id": "1",
          "time": "20210827 09:30"
        },
        {
          "id": "MDRW2100018620.1.13",
          "form": "전 20을 2달만에 빼고했더니 ㅜㅜ",
          "original_form": "전 20을 2달만에 빼고했더니 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018620.1.14",
          "form": "다시 돌아오는데도 두달이네여",
          "original_form": "다시 돌아오는데도 두달이네여",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018620.1.15",
          "form": "지금은 천천히 걷기하게요",
          "original_form": "지금은 천천히 걷기하게요",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018620.1.16",
          "form": "너무 그때 질려서 사실 운동이 싫어요 ㅋㅋ",
          "original_form": "너무 그때 질려서 사실 운동이 싫어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018620.1.17",
          "form": "힉.. 대단하세요",
          "original_form": "힉.. 대단하세요",
          "speaker_id": "2",
          "time": "20210827 09:36"
        },
        {
          "id": "MDRW2100018620.1.18",
          "form": "저 이제 한달해서 4ㅡ5키로정도 뺐어요ㅠ",
          "original_form": "저 이제 한달해서 4ㅡ5키로정도 뺐어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 09:36"
        },
        {
          "id": "MDRW2100018620.1.19",
          "form": "쉽지않다는걸 느껴요",
          "original_form": "쉽지않다는걸 느껴요",
          "speaker_id": "2",
          "time": "20210827 09:36"
        },
        {
          "id": "MDRW2100018620.1.20",
          "form": "오 성공적이예요 그정도면요",
          "original_form": "오 성공적이예요 그정도면요",
          "speaker_id": "1",
          "time": "20210827 09:37"
        },
        {
          "id": "MDRW2100018620.1.21",
          "form": "오? 그런가요??",
          "original_form": "오? 그런가요??",
          "speaker_id": "2",
          "time": "20210827 09:40"
        },
        {
          "id": "MDRW2100018620.1.22",
          "form": "그럼요 적당하죠",
          "original_form": "그럼요 적당하죠",
          "speaker_id": "1",
          "time": "20210827 09:41"
        },
        {
          "id": "MDRW2100018620.1.23",
          "form": "고수입니다 다욧",
          "original_form": "고수입니다 다욧",
          "speaker_id": "1",
          "time": "20210827 09:41"
        },
        {
          "id": "MDRW2100018620.1.24",
          "form": "캬아 저녁을 절제 못해서 살이 안빠진다고 자책 했는데ㅜㅠ 감동이에요",
          "original_form": "캬아 저녁을 절제 못해서 살이 안빠진다고 자책 했는데ㅜㅠ 감동이에요",
          "speaker_id": "2",
          "time": "20210827 09:46"
        },
        {
          "id": "MDRW2100018620.1.25",
          "form": "인정받은 느낌이 최고네요",
          "original_form": "인정받은 느낌이 최고네요",
          "speaker_id": "2",
          "time": "20210827 09:46"
        },
        {
          "id": "MDRW2100018620.1.26",
          "form": "인정해야죠",
          "original_form": "인정해야죠",
          "speaker_id": "1",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100018620.1.27",
          "form": "사실 4키로 빼기 얼마나 어려운데요",
          "original_form": "사실 4키로 빼기 얼마나 어려운데요",
          "speaker_id": "1",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100018620.1.28",
          "form": "4키로 빼려고 다들 약먹고 하잖아요",
          "original_form": "4키로 빼려고 다들 약먹고 하잖아요",
          "speaker_id": "1",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100018620.1.29",
          "form": "맞아요ㅠ 체중이 많이 나가도 쉽지않단말이죠ㅠ",
          "original_form": "맞아요ㅠ 체중이 많이 나가도 쉽지않단말이죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100018620.1.30",
          "form": "제가 다 부럽네요",
          "original_form": "제가 다 부럽네요",
          "speaker_id": "1",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100018620.1.31",
          "form": "평소 먹는양 줄이는건 진짜 힘들어요",
          "original_form": "평소 먹는양 줄이는건 진짜 힘들어요",
          "speaker_id": "2",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100018620.1.32",
          "form": "추카추카 입니다",
          "original_form": "추카추카 입니다",
          "speaker_id": "1",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100018620.1.33",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100018620.1.34",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100018620.1.35",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100018620.1.36",
          "form": "ㅎㅎ 정말 고생 많으셨어요",
          "original_form": "ㅎㅎ 정말 고생 많으셨어요",
          "speaker_id": "1",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100018620.1.37",
          "form": "다들 알아줘야해요",
          "original_form": "다들 알아줘야해요",
          "speaker_id": "1",
          "time": "20210827 09:49"
        },
        {
          "id": "MDRW2100018620.1.38",
          "form": "그게 얼마나 힘든지",
          "original_form": "그게 얼마나 힘든지",
          "speaker_id": "1",
          "time": "20210827 09:49"
        },
        {
          "id": "MDRW2100018620.1.39",
          "form": "맞아요! name1이 알아주셔서 감동입니닷!",
          "original_form": "맞아요! &name1&이 알아주셔서 감동입니닷!",
