      "utterance": [
        {
          "id": "MDRW2100041200.1.1",
          "form": "포카칩에 빼빼로 맛있겠다",
          "original_form": "포카칩에 빼빼로 맛있겠다",
          "speaker_id": "1",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100041200.1.2",
          "form": "과자 땡기는걸 보니 생리할때가 곧 오겠군",
          "original_form": "과자 땡기는걸 보니 생리할때가 곧 오겠군",
          "speaker_id": "1",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100041200.1.3",
          "form": "응 포카칩 빼빼로",
          "original_form": "응 포카칩 빼빼로",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100041200.1.4",
          "form": "단짠조합",
          "original_form": "단짠조합",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100041200.1.5",
          "form": "역시 다음주네요",
          "original_form": "역시 다음주네요",
          "speaker_id": "1",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100041200.1.6",
          "form": "생리전주 간식땡기기 완전 공식임..",
          "original_form": "생리전주 간식땡기기 완전 공식임..",
          "speaker_id": "1",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100041200.1.7",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041200.1.8",
          "form": "나는 백신펄슨이라 간식 먹음ㅋㅋ",
          "original_form": "나는 백신펄슨이라 간식 먹음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041200.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041200.1.10",
          "form": "백신펄슨은 초코가 땡기시는군요",
          "original_form": "백신펄슨은 초코가 땡기시는군요",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041200.1.11",
          "form": "네 단거",
          "original_form": "네 단거",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041200.1.12",
          "form": "당깁니다",
          "original_form": "당깁니다",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041200.1.13",
          "form": "저는 바삭바삭이 땡기네요",
          "original_form": "저는 바삭바삭이 땡기네요",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041200.1.14",
          "form": "식감쪽이시군요",
          "original_form": "식감쪽이시군요",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041200.1.15",
          "form": "아버지가 빼빼로 종류가 많아 당황하셨군",
          "original_form": "아버지가 빼빼로 종류가 많아 당황하셨군",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041200.1.16",
          "form": "엉 아홉가지라고",
          "original_form": "엉 아홉가지라고",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041200.1.17",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041200.1.18",
          "form": "요즘 콜라보된 것까지 나와서",
          "original_form": "요즘 콜라보된 것까지 나와서",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041200.1.19",
          "form": "아니 어느틈에 빼빼로 종류가 그리 많아졌댜~",
          "original_form": "아니 어느틈에 빼빼로 종류가 그리 많아졌댜~",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041200.1.20",
          "form": "꼬깔콘 빼빼로까지 나왔다니까",
          "original_form": "꼬깔콘 빼빼로까지 나왔다니까",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100041200.1.21",
          "form": "꼬깔콘 빼빼로 머임",
          "original_form": "꼬깔콘 빼빼로 머임",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100041200.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100041200.1.23",
          "form": "도전해보고 싶진 않아",
          "original_form": "도전해보고 싶진 않아",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041200.1.24",
          "form": "빼빼로데이 다가오면 또 기상천외한 빼빼로들이 나올거야",
          "original_form": "빼빼로데이 다가오면 또 기상천외한 빼빼로들이 나올거야",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041200.1.25",
          "form": "ㅋㅋㅋ요거트 선물 감사합니다",
          "original_form": "ㅋㅋㅋ요거트 선물 감사합니다",
          "speaker_id": "1",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041200.1.26",
          "form": "오면 짜치키 만들자",
          "original_form": "오면 짜치키 만들자",
