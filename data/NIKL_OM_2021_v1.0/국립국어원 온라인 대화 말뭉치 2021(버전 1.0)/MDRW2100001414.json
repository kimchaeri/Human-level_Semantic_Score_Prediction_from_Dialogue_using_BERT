      "utterance": [
        {
          "id": "MDRW2100001414.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 13:58"
        },
        {
          "id": "MDRW2100001414.1.2",
          "form": "안녕하세요~ 영화 보는거 좋아하시나요",
          "original_form": "안녕하세요~ 영화 보는거 좋아하시나요",
          "speaker_id": "2",
          "time": "20211015 14:00"
        },
        {
          "id": "MDRW2100001414.1.3",
          "form": "네 좋아해요~",
          "original_form": "네 좋아해요~",
          "speaker_id": "1",
          "time": "20211015 14:00"
        },
        {
          "id": "MDRW2100001414.1.4",
          "form": "그런데 코로나때문에 영화관은 못가고",
          "original_form": "그런데 코로나때문에 영화관은 못가고",
          "speaker_id": "1",
          "time": "20211015 14:00"
        },
        {
          "id": "MDRW2100001414.1.5",
          "form": "요즘엔 집에서 보네요",
          "original_form": "요즘엔 집에서 보네요",
          "speaker_id": "1",
          "time": "20211015 14:01"
        },
        {
          "id": "MDRW2100001414.1.6",
          "form": "오 저도 그래요 ",
          "original_form": "오 저도 그래요 ",
          "speaker_id": "2",
          "time": "20211015 14:01"
        },
        {
          "id": "MDRW2100001414.1.7",
          "form": "넷플릭스 왓챠 번갈아가면서 보고있어요",
          "original_form": "넷플릭스 왓챠 번갈아가면서 보고있어요",
          "speaker_id": "2",
          "time": "20211015 14:01"
        },
        {
          "id": "MDRW2100001414.1.8",
          "form": "어떤 장르를 주로 보시나요?",
          "original_form": "어떤 장르를 주로 보시나요?",
          "speaker_id": "2",
          "time": "20211015 14:01"
        },
        {
          "id": "MDRW2100001414.1.9",
          "form": "저는 주로 범죄스릴러 좋아해요",
          "original_form": "저는 주로 범죄스릴러 좋아해요",
          "speaker_id": "1",
          "time": "20211015 14:10"
        },
        {
          "id": "MDRW2100001414.1.10",
          "form": "범죄의 재구성이나 도둑들",
          "original_form": "범죄의 재구성이나 도둑들",
          "speaker_id": "1",
          "time": "20211015 14:10"
        },
        {
          "id": "MDRW2100001414.1.11",
          "form": "넷플릭스 보시면 오징어게임 보셨겠네요?",
          "original_form": "넷플릭스 보시면 오징어게임 보셨겠네요?",
          "speaker_id": "1",
          "time": "20211015 14:10"
        },
        {
          "id": "MDRW2100001414.1.12",
          "form": "영화는 아니고 드라마지만 ㅎㅎ",
          "original_form": "영화는 아니고 드라마지만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 14:11"
        },
        {
          "id": "MDRW2100001414.1.13",
          "form": "ㅋㅋ넵! 요즘 오징어게임 안보면 대화를 못해요",
          "original_form": "ㅋㅋ넵! 요즘 오징어게임 안보면 대화를 못해요",
          "speaker_id": "2",
          "time": "20211015 14:11"
        },
        {
          "id": "MDRW2100001414.1.14",
          "form": "보셨나요? 오징어게임",
          "original_form": "보셨나요? 오징어게임",
          "speaker_id": "2",
          "time": "20211015 14:11"
        },
        {
          "id": "MDRW2100001414.1.15",
          "form": "그건 그래요 ㅎㅎ",
          "original_form": "그건 그래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 14:11"
        },
        {
          "id": "MDRW2100001414.1.16",
          "form": "네 저는 하루에 몰아서 다 봤어요",
          "original_form": "네 저는 하루에 몰아서 다 봤어요",
          "speaker_id": "1",
          "time": "20211015 14:11"
        },
        {
          "id": "MDRW2100001414.1.17",
          "form": "어떠셨어요? 전 처음에 잔인한줄 모르고 봐서",
          "original_form": "어떠셨어요? 전 처음에 잔인한줄 모르고 봐서",
          "speaker_id": "2",
          "time": "20211015 14:13"
        },
        {
          "id": "MDRW2100001414.1.18",
          "form": "깜짝 놀랐어요 ㅋㅋ",
          "original_form": "깜짝 놀랐어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 14:13"
        },
        {
          "id": "MDRW2100001414.1.19",
          "form": "저도 아무내용 모르고 봐서",
          "original_form": "저도 아무내용 모르고 봐서",
          "speaker_id": "1",
          "time": "20211015 14:13"
        },
        {
          "id": "MDRW2100001414.1.20",
          "form": "좀 충격적이었는데",
          "original_form": "좀 충격적이었는데",
          "speaker_id": "1",
          "time": "20211015 14:13"
        },
        {
          "id": "MDRW2100001414.1.21",
          "form": "그런면이 더 매력적이었던것 같아요",
          "original_form": "그런면이 더 매력적이었던것 같아요",
          "speaker_id": "1",
          "time": "20211015 14:13"
        },
        {
          "id": "MDRW2100001414.1.22",
          "form": "영화나 드라마는 픽션이다보니까 그런점이 있어야 빠져들더라구요 ㅎㅎ",
          "original_form": "영화나 드라마는 픽션이다보니까 그런점이 있어야 빠져들더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 14:13"
        },
        {
          "id": "MDRW2100001414.1.23",
          "form": "맞아요 너무 자극적이지 않나했는데",
          "original_form": "맞아요 너무 자극적이지 않나했는데",
          "speaker_id": "2",
          "time": "20211015 14:14"
        },
        {
          "id": "MDRW2100001414.1.24",
          "form": "이렇게 인기를 얻을 줄이야..",
          "original_form": "이렇게 인기를 얻을 줄이야..",
          "speaker_id": "2",
          "time": "20211015 14:15"
        },
        {
          "id": "MDRW2100001414.1.25",
          "form": "전 세계적인 인기는 아무도 예상 못했겠죠 ㅎㅎ",
          "original_form": "전 세계적인 인기는 아무도 예상 못했겠죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 14:15"
        },
        {
          "id": "MDRW2100001414.1.26",
          "form": "외국인들이 오징어게임에 나오는 게임들을 따라하더라구요",
          "original_form": "외국인들이 오징어게임에 나오는 게임들을 따라하더라구요",
          "speaker_id": "2",
          "time": "20211015 14:16"
        },
        {
          "id": "MDRW2100001414.1.27",
          "form": "맞아요 달고나게임이 제일 인기 많다고 뉴스에서 봤어요 ㅋㅋ",
          "original_form": "맞아요 달고나게임이 제일 인기 많다고 뉴스에서 봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211015 14:18"
        },
        {
          "id": "MDRW2100001414.1.28",
          "form": "ㅋㅋㅋ달고나가 수출될줄이야...",
          "original_form": "ㅋㅋㅋ달고나가 수출될줄이야...",
          "speaker_id": "2",
          "time": "20211015 14:20"
        }
      ]
    }
