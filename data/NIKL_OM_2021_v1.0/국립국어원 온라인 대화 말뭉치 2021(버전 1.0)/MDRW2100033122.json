        },
        {
          "id": "MDRW2100033122.1.2",
          "form": "장래희망이 머죠?",
          "original_form": "장래희망이 머죠?",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033122.1.3",
          "form": "음 전 꿈이 없어요 name2님은요?",
          "original_form": "음 전 꿈이 없어요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033122.1.4",
          "form": "저는 약사요",
          "original_form": "저는 약사요",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033122.1.5",
          "form": "오 멋진 직업",
          "original_form": "오 멋진 직업",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033122.1.6",
          "form": "근디 그럴러면 공부를..",
          "original_form": "근디 그럴러면 공부를..",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033122.1.7",
          "form": "에이 하면 되죠",
          "original_form": "에이 하면 되죠",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033122.1.8",
          "form": "할 수 있다!",
          "original_form": "할 수 있다!",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033122.1.9",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033122.1.10",
          "form": "아녜요 ㅋㅋ 약사 하면 뭐가 좋아요?",
          "original_form": "아녜요 ㅋㅋ 약사 하면 뭐가 좋아요?",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033122.1.11",
          "form": "돈을 많이 벌죠",
          "original_form": "돈을 많이 벌죠",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033122.1.12",
          "form": "아 돈.. 중요하죠",
          "original_form": "아 돈.. 중요하죠",
          "speaker_id": "1",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100033122.1.13",
          "form": "약사 되기 어렵나요?",
          "original_form": "약사 되기 어렵나요?",
