      "utterance": [
        {
          "id": "MDRW2100007342.1.1",
          "form": "어떤 음식 좋아해요?",
          "original_form": "어떤 음식 좋아해요?",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007342.1.2",
          "form": "저는 자극적인거 좋아해요",
          "original_form": "저는 자극적인거 좋아해요",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007342.1.3",
          "form": "그래서 문제네요 건강 걱정도 되고..",
          "original_form": "그래서 문제네요 건강 걱정도 되고..",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007342.1.4",
          "form": "그렇죠ㅜ 아무래도 몸에 안좋으니까ㅜㅜ",
          "original_form": "그렇죠ㅜ 아무래도 몸에 안좋으니까ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007342.1.5",
          "form": "매운 걸 되게 잘 먹는데 이게 건강측면에선",
          "original_form": "매운 걸 되게 잘 먹는데 이게 건강측면에선",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007342.1.6",
          "form": "결코 좋지 않은것 같아요",
          "original_form": "결코 좋지 않은것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007342.1.7",
          "form": "우와 전 맵찔이라....ㅋㅋㅋㅋㅋㅋ 부러워요!",
          "original_form": "우와 전 맵찔이라....ㅋㅋㅋㅋㅋㅋ 부러워요!",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007342.1.8",
          "form": "위는 힘들겠죠ㅜㅜㅜ",
          "original_form": "위는 힘들겠죠ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007342.1.9",
          "form": "어제도 엽떡 먹었는데",
          "original_form": "어제도 엽떡 먹었는데",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007342.1.10",
          "form": "오랜만에 먹으니까 위가 아파서 혼났어요 ㅋㅋㅋ",
          "original_form": "오랜만에 먹으니까 위가 아파서 혼났어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007342.1.11",
          "form": "매운건 안먹으면 점점 못먹는거 같아요ㅜ",
          "original_form": "매운건 안먹으면 점점 못먹는거 같아요ㅜ",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007342.1.12",
          "form": "네 아무래도 고통에 강해져야하기 때문에..... ㅎ",
          "original_form": "네 아무래도 고통에 강해져야하기 때문에..... ㅎ",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007342.1.13",
          "form": "저도 옆에있는 사람이 맵찔이라 안먹다보니 더더더 못먹게되더라구요ㅠ",
          "original_form": "저도 옆에있는 사람이 맵찔이라 안먹다보니 더더더 못먹게되더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007342.1.14",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007342.1.15",
          "form": "그럼 어떤거 선호하시나요?",
          "original_form": "그럼 어떤거 선호하시나요?",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007342.1.16",
          "form": "뭔가 고통에 강해진다니 운동같아요!ㅋㅋㅋㅋ",
          "original_form": "뭔가 고통에 강해진다니 운동같아요!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007342.1.17",
          "form": "전 고기요 육류 러버",
          "original_form": "전 고기요 육류 러버",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007342.1.18",
          "form": "이것도 건강엔 나쁘겠죠",
          "original_form": "이것도 건강엔 나쁘겠죠",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007342.1.19",
          "form": "돼지 닭 소",
          "original_form": "돼지 닭 소",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007342.1.20",
          "form": "순위를 매긴다면 어떻게 되나요?ㅋㅋ",
          "original_form": "순위를 매긴다면 어떻게 되나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007342.1.21",
          "form": "뭐 오리 양 다른것도 좋구요",
          "original_form": "뭐 오리 양 다른것도 좋구요",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007342.1.22",
          "form": "아ㅜㅜ 우열을 가릴수없어요ㅠㅠ",
          "original_form": "아ㅜㅜ 우열을 가릴수없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007342.1.23",
          "form": "그날 분위기 장소에 따라서 다 좋은데ㅜㅜ",
          "original_form": "그날 분위기 장소에 따라서 다 좋은데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007342.1.24",
          "form": "너무 어려운거같아요ㅜㅜㅜ",
          "original_form": "너무 어려운거같아요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007342.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 고기는 다 맛있죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 고기는 다 맛있죠",
          "speaker_id": "1",
          "time": "20210822 01:36"
        }
      ]
    }
