      "utterance": [
        {
          "id": "MDRW2100026068.1.1",
          "form": "길고양이 너무 이쁘디",
          "original_form": "길고양이 너무 이쁘디",
          "speaker_id": "1",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100026068.1.2",
          "form": "친화적이지",
          "original_form": "친화적이지",
          "speaker_id": "2",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100026068.1.3",
          "form": "보송보송하고",
          "original_form": "보송보송하고",
          "speaker_id": "2",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100026068.1.4",
          "form": "이쁘자낭 야들야들",
          "original_form": "이쁘자낭 야들야들",
          "speaker_id": "1",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100026068.1.5",
          "form": "고양이는 깨끗해",
          "original_form": "고양이는 깨끗해",
          "speaker_id": "1",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100026068.1.6",
          "form": "개는 그럼 꼬질꼬질하냐?",
          "original_form": "개는 그럼 꼬질꼬질하냐?",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026068.1.7",
          "form": "냥이가 깨끗한거지",
          "original_form": "냥이가 깨끗한거지",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026068.1.8",
          "form": "간식이라도 줄까",
          "original_form": "간식이라도 줄까",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026068.1.9",
          "form": "사료든 젤리든 냥이들이  좋아하는거",
          "original_form": "사료든 젤리든 냥이들이  좋아하는거",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026068.1.10",
          "form": "우린 나중에 유기견 키워야지",
          "original_form": "우린 나중에 유기견 키워야지",
          "speaker_id": "2",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026068.1.11",
          "form": "시골가면",
          "original_form": "시골가면",
          "speaker_id": "2",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026068.1.12",
          "form": "당근 여러마리 키우자 농장에다가ㅎㅎ",
          "original_form": "당근 여러마리 키우자 농장에다가ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026068.1.13",
          "form": "고양이도 있으면 좋겠다",
          "original_form": "고양이도 있으면 좋겠다",
          "speaker_id": "2",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026068.1.14",
          "form": "강아지랑 고양이는 어울리기 힘들어",
          "original_form": "강아지랑 고양이는 어울리기 힘들어",
          "speaker_id": "1",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100026068.1.15",
          "form": "요새 유투브보면 잘어울려",
          "original_form": "요새 유투브보면 잘어울려",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100026068.1.16",
          "form": "일반적인건 아니지",
          "original_form": "일반적인건 아니지",
          "speaker_id": "1",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100026068.1.17",
          "form": "무튼 귀여워",
          "original_form": "무튼 귀여워",
          "speaker_id": "1",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100026068.1.18",
          "form": "동물들은 다 귀여워",
          "original_form": "동물들은 다 귀여워",
          "speaker_id": "1",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100026068.1.19",
