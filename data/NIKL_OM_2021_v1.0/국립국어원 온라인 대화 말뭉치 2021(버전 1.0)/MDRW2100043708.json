      "utterance": [
        {
          "id": "MDRW2100043708.1.1",
          "form": "그럼 캐나다 가서 김치 없이 어떡하냐 너",
          "original_form": "그럼 캐나다 가서 김치 없이 어떡하냐 너",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043708.1.2",
          "form": "그래서 한인마트에서 김치를 사지않을까...?",
          "original_form": "그래서 한인마트에서 김치를 사지않을까...?",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043708.1.3",
          "form": "라면이랑 김치 쌓아놔야지",
          "original_form": "라면이랑 김치 쌓아놔야지",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043708.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ값이 장난 아니겠는걸",
          "original_form": "ㅋㅋㅋㅋㅋㅋ값이 장난 아니겠는걸",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043708.1.5",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043708.1.6",
          "form": "여기서 쟁여가지고 가야겠네",
          "original_form": "여기서 쟁여가지고 가야겠네",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043708.1.7",
          "form": "아니다 반입 안 된댔지",
          "original_form": "아니다 반입 안 된댔지",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043708.1.8",
          "form": "한달 사는 생활비가 얼마려나",
          "original_form": "한달 사는 생활비가 얼마려나",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043708.1.9",
          "form": "캐나다는 식재료 물가가 좀 저렴하지 않나?",
          "original_form": "캐나다는 식재료 물가가 좀 저렴하지 않나?",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043708.1.10",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043708.1.11",
          "form": "그럼 다행이려나",
          "original_form": "그럼 다행이려나",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043708.1.12",
          "form": "캐나다 음식은 메이플시럽밖에 몰라서리~",
          "original_form": "캐나다 음식은 메이플시럽밖에 몰라서리~",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043708.1.13",
          "form": "넌 빵좋아하니",
          "original_form": "넌 빵좋아하니",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043708.1.14",
          "form": "웅 빵 좋아해",
          "original_form": "웅 빵 좋아해",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043708.1.15",
          "form": "무슨 빵좋아해?",
          "original_form": "무슨 빵좋아해?",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043708.1.16",
          "form": "난 요즘 에그타르트가 너무 맛있어",
          "original_form": "난 요즘 에그타르트가 너무 맛있어",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043708.1.17",
          "form": "너 타르트 좋아하니?",
          "original_form": "너 타르트 좋아하니?",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043708.1.18",
          "form": "아 타르트 진짜 좋아해",
          "original_form": "아 타르트 진짜 좋아해",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043708.1.19",
          "form": "나도 진짜 타르트 사랑해",
          "original_form": "나도 진짜 타르트 사랑해",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043708.1.20",
          "form": "바삭한 빵이랑 부드러운 필링이라 너무 좋아",
          "original_form": "바삭한 빵이랑 부드러운 필링이라 너무 좋아",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043708.1.21",
          "form": "호두타르트 진짜 좋아",
          "original_form": "호두타르트 진짜 좋아",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043708.1.22",
          "form": "호두 맛있지",
          "original_form": "호두 맛있지",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043708.1.23",
          "form": "식감이 오독오독해서 중독성 있어",
          "original_form": "식감이 오독오독해서 중독성 있어",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043708.1.24",
          "form": "응 호두 너무 맛있어",
          "original_form": "응 호두 너무 맛있어",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043708.1.25",
          "form": "근데 생호두는 안먹어ㅎ",
          "original_form": "근데 생호두는 안먹어ㅎ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043708.1.26",
          "form": "헐 왜죠",
          "original_form": "헐 왜죠",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043708.1.27",
          "form": "호두계의 이단아시군요",
          "original_form": "호두계의 이단아시군요",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043708.1.28",
          "form": "아몬드나 호두나",
          "original_form": "아몬드나 호두나",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043708.1.29",
          "form": "생 견과류 맛이 싫어",
          "original_form": "생 견과류 맛이 싫어",
          "speaker_id": "1",
          "time": "20210907 20:11"
        }
      ]
    }
