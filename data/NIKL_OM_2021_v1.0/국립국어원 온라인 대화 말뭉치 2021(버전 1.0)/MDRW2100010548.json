      "utterance": [
        {
          "id": "MDRW2100010548.1.1",
          "form": "즐겨하시는 게임 있으신가요",
          "original_form": "즐겨하시는 게임 있으신가요",
          "speaker_id": "2",
          "time": "20210823 19:28"
        },
        {
          "id": "MDRW2100010548.1.2",
          "form": "스팀으로 캐주얼 게임 좀 해요!",
          "original_form": "스팀으로 캐주얼 게임 좀 해요!",
          "speaker_id": "1",
          "time": "20210823 19:30"
        },
        {
          "id": "MDRW2100010548.1.3",
          "form": "헐헐!!",
          "original_form": "헐헐!!",
          "speaker_id": "2",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010548.1.4",
          "form": "게임 좋아하시는 군요!!!",
          "original_form": "게임 좋아하시는 군요!!!",
          "speaker_id": "2",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010548.1.5",
          "form": "스팀 게임 즐겨하시나요!!",
          "original_form": "스팀 게임 즐겨하시나요!!",
          "speaker_id": "1",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010548.1.6",
          "form": "스팀이 그 게임 모아져 있는 사이트죠?!",
          "original_form": "스팀이 그 게임 모아져 있는 사이트죠?!",
          "speaker_id": "2",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010548.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010548.1.8",
          "form": "아뇨 저 게임 좋아하는 사람 첨만났어요",
          "original_form": "아뇨 저 게임 좋아하는 사람 첨만났어요",
          "speaker_id": "2",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010548.1.9",
          "form": "게임 좋아해요",
          "original_form": "게임 좋아해요",
          "speaker_id": "1",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010548.1.10",
          "form": "갑자기 궁금해지네여",
          "original_form": "갑자기 궁금해지네여",
          "speaker_id": "2",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010548.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010548.1.12",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010548.1.13",
          "form": "스팀 세일할 때 사람들이 달려가던데",
          "original_form": "스팀 세일할 때 사람들이 달려가던데",
          "speaker_id": "2",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010548.1.14",
          "form": "세일을 많이하는 편인가요 맨날 달려간다고 올라와서(?)요",
          "original_form": "세일을 많이하는 편인가요 맨날 달려간다고 올라와서(?)요",
          "speaker_id": "2",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010548.1.15",
          "form": "맞아요 1년에 4번 정도 하는 거 같아요",
          "original_form": "맞아요 1년에 4번 정도 하는 거 같아요",
          "speaker_id": "1",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010548.1.16",
          "form": "방학 시즌이랑 연초나 연말에",
          "original_form": "방학 시즌이랑 연초나 연말에",
          "speaker_id": "1",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010548.1.17",
          "form": "게임은 잘 안 하세요?",
          "original_form": "게임은 잘 안 하세요?",
          "speaker_id": "1",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010548.1.18",
          "form": "저 오디션 해봤어요",
          "original_form": "저 오디션 해봤어요",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010548.1.19",
          "form": "크아랑",
          "original_form": "크아랑",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010548.1.20",
          "form": "제가 게임을 못하더라고요. 저도 크면서 알았답니다.",
          "original_form": "제가 게임을 못하더라고요. 저도 크면서 알았답니다.",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010548.1.21",
          "form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010548.1.22",
          "form": "게인 잘하는 사람들이 너무 부러워요",
          "original_form": "게인 잘하는 사람들이 너무 부러워요",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010548.1.23",
          "form": "저도 진짜 못해요",
          "original_form": "저도 진짜 못해요",
          "speaker_id": "1",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010548.1.24",
          "form": "재능이 없달까",
          "original_form": "재능이 없달까",
          "speaker_id": "1",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010548.1.25",
          "form": "즐기는 것이 재능.",
          "original_form": "즐기는 것이 재능.",
          "speaker_id": "2",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010548.1.26",
          "form": "그건 맞죠!",
          "original_form": "그건 맞죠!",
          "speaker_id": "1",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010548.1.27",
          "form": "어떤류 게임을 젤 좋아하세요?",
          "original_form": "어떤류 게임을 젤 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010548.1.28",
          "form": "ㅊ..총쏘는거?!",
          "original_form": "ㅊ..총쏘는거?!",
          "speaker_id": "2",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010548.1.29",
          "form": "아뇨 그런 건 잘 못하고",
          "original_form": "아뇨 그런 건 잘 못하고",
          "speaker_id": "1",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010548.1.30",
          "form": "노동(?) 하는 거 좋아해요",
          "original_form": "노동(?) 하는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010548.1.31",
          "form": "농장 키우거나 생존 게임 같은 거 즐겨해요",
          "original_form": "농장 키우거나 생존 게임 같은 거 즐겨해요",
          "speaker_id": "1",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010548.1.32",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "2",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010548.1.33",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010548.1.34",
          "form": "그러면 약간 동물의 숲같은 게임인가요?!",
          "original_form": "그러면 약간 동물의 숲같은 게임인가요?!",
          "speaker_id": "2",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010548.1.35",
          "form": "그런 거 좋아해요",
          "original_form": "그런 거 좋아해요",
          "speaker_id": "1",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010548.1.36",
          "form": "캐주얼 게임은 아니지만 제5인격 아세요? 그것도 재밌게 했는데",
          "original_form": "캐주얼 게임은 아니지만 제5인격 아세요? 그것도 재밌게 했는데",
          "speaker_id": "1",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100010548.1.37",
          "form": "헉 그건 뭔가요?!!",
          "original_form": "헉 그건 뭔가요?!!",
          "speaker_id": "2",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100010548.1.38",
          "form": "술래잡기 같은 게임이었어요",
          "original_form": "술래잡기 같은 게임이었어요",
          "speaker_id": "1",
          "time": "20210823 19:37"
        },
        {
          "id": "MDRW2100010548.1.39",
          "form": "4명이 미션하는 동안 1명이 4명 잡으러 다니는",
          "original_form": "4명이 미션하는 동안 1명이 4명 잡으러 다니는",
          "speaker_id": "1",
          "time": "20210823 19:38"
        },
        {
          "id": "MDRW2100010548.1.40",
          "form": "지금 보니 특별히 장르 안 가리고 게임을 좋아하는 거 같아요ㅋㅋ",
          "original_form": "지금 보니 특별히 장르 안 가리고 게임을 좋아하는 거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:38"
        }
      ]
    }
