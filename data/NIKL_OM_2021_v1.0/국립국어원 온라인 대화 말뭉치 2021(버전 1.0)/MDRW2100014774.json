      "utterance": [
        {
          "id": "MDRW2100014774.1.1",
          "form": "말말님은 최근에 구매한 물건이 있나요??",
          "original_form": "말말님은 최근에 구매한 물건이 있나요??",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014774.1.2",
          "form": "저는 머리집게 산거같네요",
          "original_form": "저는 머리집게 산거같네요",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014774.1.3",
          "form": "name1님은요?? ㅋㅋ",
          "original_form": "&name1&님은요?? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014774.1.4",
          "form": "저는 버킷햇 샀어요",
          "original_form": "저는 버킷햇 샀어요",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014774.1.5",
          "form": "오늘 배송왔는데",
          "original_form": "오늘 배송왔는데",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014774.1.6",
          "form": "살짝 후회중 ㅋㅋ",
          "original_form": "살짝 후회중 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014774.1.7",
          "form": "오 왜요? 버킷햇 좋죠",
          "original_form": "오 왜요? 버킷햇 좋죠",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014774.1.8",
          "form": "가을에쓰기딱인데",
          "original_form": "가을에쓰기딱인데",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014774.1.9",
          "form": "요구르트 아줌마같아요...",
          "original_form": "요구르트 아줌마같아요...",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014774.1.10",
          "form": "ㅠㅠ아니면 낚시하러 갈것같기도하고..",
          "original_form": "ㅠㅠ아니면 낚시하러 갈것같기도하고..",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014774.1.11",
          "form": "옷을 잘 입어야할것같아요 ㅋㅋㅋ",
          "original_form": "옷을 잘 입어야할것같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014774.1.12",
          "form": "ㅋㅋㅋ좀 힙합스럽게 입어야할듯요",
          "original_form": "ㅋㅋㅋ좀 힙합스럽게 입어야할듯요",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014774.1.13",
          "form": "저는 최근에 캡모자 샀는데 잘안쓰게되더라고요",
          "original_form": "저는 최근에 캡모자 샀는데 잘안쓰게되더라고요",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014774.1.14",
          "form": "ㅋㅋㅋ캡모자는 무슨 색이에요??",
          "original_form": "ㅋㅋㅋ캡모자는 무슨 색이에요??",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014774.1.15",
          "form": "보라색이라서 ㅋㅋ 뭔가 애매하네요",
          "original_form": "보라색이라서 ㅋㅋ 뭔가 애매하네요",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014774.1.16",
          "form": "버킷햇 살걸 그랬나봐요",
          "original_form": "버킷햇 살걸 그랬나봐요",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014774.1.17",
          "form": "뭔가 포인트로 쓰긴 좋을 것 같아요 ㅎㅎ",
          "original_form": "뭔가 포인트로 쓰긴 좋을 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014774.1.18",
          "form": "그런가요? 이제 가을 되어서 추워지면 잘쓸거같아요",
          "original_form": "그런가요? 이제 가을 되어서 추워지면 잘쓸거같아요",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014774.1.19",
          "form": "버킷햇도 이것 저것 써보고 사는게 좋을 것 같아요!",
          "original_form": "버킷햇도 이것 저것 써보고 사는게 좋을 것 같아요!",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014774.1.20",
          "form": "가을에 패션을 뽐낼 시기기도 하죠",
          "original_form": "가을에 패션을 뽐낼 시기기도 하죠",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014774.1.21",
          "form": "맞아요 모자는 직접써보고 사는게 좋은거같기도해요",
          "original_form": "맞아요 모자는 직접써보고 사는게 좋은거같기도해요",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014774.1.22",
          "form": "실제로보면 달라서",
          "original_form": "실제로보면 달라서",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014774.1.23",
          "form": "맞아요!!저는 직접 써보고도 구매했는데도",
          "original_form": "맞아요!!저는 직접 써보고도 구매했는데도",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014774.1.24",
          "form": "후회가 되네요!",
          "original_form": "후회가 되네요!",
          "speaker_id": "1",
          "time": "20210825 20:45"
        }
      ]
    }
