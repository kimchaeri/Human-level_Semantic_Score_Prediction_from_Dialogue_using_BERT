      "utterance": [
        {
          "id": "MDRW2100039070.1.1",
          "form": "가족들이랑 다같이 해서",
          "original_form": "가족들이랑 다같이 해서",
          "speaker_id": "1",
          "time": "20210903 08:59"
        },
        {
          "id": "MDRW2100039070.1.2",
          "form": "5만보는 채우게 되더라구요",
          "original_form": "5만보는 채우게 되더라구요",
          "speaker_id": "1",
          "time": "20210903 08:59"
        },
        {
          "id": "MDRW2100039070.1.3",
          "form": "가족들이랑 같이 했구나......되게 효율적이죵그러면",
          "original_form": "가족들이랑 같이 했구나......되게 효율적이죵그러면",
          "speaker_id": "2",
          "time": "20210903 09:00"
        },
        {
          "id": "MDRW2100039070.1.4",
          "form": "우리가족은 그런거 안해서",
          "original_form": "우리가족은 그런거 안해서",
          "speaker_id": "2",
          "time": "20210903 09:00"
        },
        {
          "id": "MDRW2100039070.1.5",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 09:00"
        },
        {
          "id": "MDRW2100039070.1.6",
          "form": "ㅠ 그러면 어쩔수 없죠",
          "original_form": "ㅠ 그러면 어쩔수 없죠",
          "speaker_id": "1",
          "time": "20210903 09:02"
        },
        {
          "id": "MDRW2100039070.1.7",
          "form": "토스 원래 옛날에는 1만보달성하면 백원줬다던데 그때도 했었나요?",
          "original_form": "토스 원래 옛날에는 1만보달성하면 백원줬다던데 그때도 했었나요?",
          "speaker_id": "2",
          "time": "20210903 09:02"
        },
        {
          "id": "MDRW2100039070.1.8",
          "form": "저는 올해시작해서",
          "original_form": "저는 올해시작해서",
          "speaker_id": "2",
          "time": "20210903 09:02"
        },
        {
          "id": "MDRW2100039070.1.9",
          "form": "5만보 백원이 너무 빡셔요ㅠㅠ",
          "original_form": "5만보 백원이 너무 빡셔요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100039070.1.10",
          "form": "저도 그때는 안했어요",
          "original_form": "저도 그때는 안했어요",
          "speaker_id": "1",
          "time": "20210903 09:04"
        },
        {
          "id": "MDRW2100039070.1.11",
          "form": "토스옛날이 되게 좋았데요ㅠㅠ",
          "original_form": "토스옛날이 되게 좋았데요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 09:05"
        },
        {
          "id": "MDRW2100039070.1.12",
          "form": "너무 늦게 알아서....ㅜㅜ",
          "original_form": "너무 늦게 알아서....ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100039070.1.13",
          "form": "ㅠ 일찍 알았으면 돈 더 버는건데ㅠ",
          "original_form": "ㅠ 일찍 알았으면 돈 더 버는건데ㅠ",
          "speaker_id": "1",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100039070.1.14",
          "form": "그니깐요ㅠㅠ정보가 되게 중요한것같아요",
          "original_form": "그니깐요ㅠㅠ정보가 되게 중요한것같아요",
          "speaker_id": "2",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100039070.1.15",
          "form": "올해 앱테크를해서 1만보도 운동도 앱테크ㅋㅋㅋ",
          "original_form": "올해 앱테크를해서 1만보도 운동도 앱테크ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100039070.1.16",
          "form": "앱테크 중에 설문조사 참여로 돈버는 것도 있던데",
          "original_form": "앱테크 중에 설문조사 참여로 돈버는 것도 있던데",
          "speaker_id": "1",
          "time": "20210903 09:28"
        },
        {
          "id": "MDRW2100039070.1.17",
          "form": "그것도 하세요?",
          "original_form": "그것도 하세요?",
          "speaker_id": "1",
          "time": "20210903 09:28"
        },
        {
          "id": "MDRW2100039070.1.18",
          "form": "설문조사면 엠브레인하고있어요",
          "original_form": "설문조사면 엠브레인하고있어요",
          "speaker_id": "2",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100039070.1.19",
          "form": "저두요ㅋㅋ",
          "original_form": "저두요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100039070.1.20",
          "form": "오늘저녁운동하나요?",
          "original_form": "오늘저녁운동하나요?",
          "speaker_id": "2",
          "time": "20210903 17:05"
        },
        {
          "id": "MDRW2100039070.1.21",
          "form": "이따가 저녁 먹고 하려구요",
          "original_form": "이따가 저녁 먹고 하려구요",
          "speaker_id": "1",
