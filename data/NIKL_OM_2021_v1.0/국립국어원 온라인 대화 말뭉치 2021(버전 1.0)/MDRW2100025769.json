      "utterance": [
        {
          "id": "MDRW2100025769.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025769.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100025769.1.3",
          "form": "요즘 날씨가 너무 안좋죠",
          "original_form": "요즘 날씨가 너무 안좋죠",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100025769.1.4",
          "form": "네 좀 물고기가 된 거 같은 기분",
          "original_form": "네 좀 물고기가 된 거 같은 기분",
          "speaker_id": "2",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025769.1.5",
          "form": "습해서 은근히 찜찜해요",
          "original_form": "습해서 은근히 찜찜해요",
          "speaker_id": "2",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025769.1.6",
          "form": "올해는 너무 습해서 탈이었죠...",
          "original_form": "올해는 너무 습해서 탈이었죠...",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025769.1.7",
          "form": "불쾌지수도 올라가구요",
          "original_form": "불쾌지수도 올라가구요",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025769.1.8",
          "form": "네, 비맞은것도 아닌데 괜히 축축한거 같고",
          "original_form": "네, 비맞은것도 아닌데 괜히 축축한거 같고",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025769.1.9",
          "form": "차라리 지금은 시원해서 다행인거 같아요",
          "original_form": "차라리 지금은 시원해서 다행인거 같아요",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025769.1.10",
          "form": "그쵸, 근데 이렇게 비 한두번 오고나면 확 추워질거 같아서 두려워요 ㅎㅎ",
          "original_form": "그쵸, 근데 이렇게 비 한두번 오고나면 확 추워질거 같아서 두려워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025769.1.11",
          "form": "이제는 9월이고 가을 시작이겠죠",
          "original_form": "이제는 9월이고 가을 시작이겠죠",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025769.1.12",
          "form": "가을옷 미리 준비해야하는데",
          "original_form": "가을옷 미리 준비해야하는데",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025769.1.13",
          "form": "네, 지금은 일단 가디건을 챙기고 다니는데",
          "original_form": "네, 지금은 일단 가디건을 챙기고 다니는데",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025769.1.14",
          "form": "슬슬 옷장에서 긴옷 꺼내야겠어요",
          "original_form": "슬슬 옷장에서 긴옷 꺼내야겠어요",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025769.1.15",
          "form": "가을은 훅훅 지나가던데 또 곧 겨울이 오겠네요",
          "original_form": "가을은 훅훅 지나가던데 또 곧 겨울이 오겠네요",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025769.1.16",
          "form": "그쵸 그럼 이제 패딩도 꺼내야하는...ㅋㅋ",
          "original_form": "그쵸 그럼 이제 패딩도 꺼내야하는...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025769.1.17",
          "form": "올해 겨울 패션도 롱패딩 군단이겠죠?",
          "original_form": "올해 겨울 패션도 롱패딩 군단이겠죠?",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025769.1.18",
          "form": "아무래도 편하고 따뜻하니까요",
          "original_form": "아무래도 편하고 따뜻하니까요",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025769.1.19",
          "form": "어디 두기가 곤혹스러운거만 빼면요",
          "original_form": "어디 두기가 곤혹스러운거만 빼면요",
          "speaker_id": "2",
          "time": "20210901 17:14"
        }
      ]
