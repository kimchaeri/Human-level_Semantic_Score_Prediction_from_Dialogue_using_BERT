      "utterance": [
        {
          "id": "MDRW2100011829.1.1",
          "form": "대학교 cc 도 해야하고 교복데이트에",
          "original_form": "대학교 cc 도 해야하고 교복데이트에",
          "speaker_id": "1",
          "time": "20210824 16:22"
        },
        {
          "id": "MDRW2100011829.1.2",
          "form": "교복입고 놀이공원데이트",
          "original_form": "교복입고 놀이공원데이트",
          "speaker_id": "2",
          "time": "20210824 16:22"
        },
        {
          "id": "MDRW2100011829.1.3",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210824 16:22"
        },
        {
          "id": "MDRW2100011829.1.4",
          "form": "무슨 대국민 버킷리스트도아니고",
          "original_form": "무슨 대국민 버킷리스트도아니고",
          "speaker_id": "1",
          "time": "20210824 16:22"
        },
        {
          "id": "MDRW2100011829.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:22"
        },
        {
          "id": "MDRW2100011829.1.6",
          "form": "회전목마앞에서 뽀뽀하는 사진찍고",
          "original_form": "회전목마앞에서 뽀뽀하는 사진찍고",
          "speaker_id": "1",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.7",
          "form": "사실상 교복입고 놀이공원가면",
          "original_form": "사실상 교복입고 놀이공원가면",
          "speaker_id": "2",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.8",
          "form": "럽스타그램하고",
          "original_form": "럽스타그램하고",
          "speaker_id": "1",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.9",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.10",
          "form": "정말 망ㅎ이 봣어요!!",
          "original_form": "정말 망ㅎ이 봣어요!!",
          "speaker_id": "2",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.11",
          "form": "진짜 수만번",
          "original_form": "진짜 수만번",
          "speaker_id": "1",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.12",
          "form": "어두워진 배경에 불켜진 회전목마",
          "original_form": "어두워진 배경에 불켜진 회전목마",
          "speaker_id": "2",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.13",
          "form": "올림픽공원 외톨이나무앞에서도 커플사진 엄청찍고",
          "original_form": "올림픽공원 외톨이나무앞에서도 커플사진 엄청찍고",
          "speaker_id": "1",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.14",
          "form": "아 맞아요 꼭 야경",
          "original_form": "아 맞아요 꼭 야경",
          "speaker_id": "1",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.16",
          "form": "뒷모습도 찍고",
          "original_form": "뒷모습도 찍고",
          "speaker_id": "2",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.17",
          "form": "근데 교복도 치마 교복 입고가잖아요",
          "original_form": "근데 교복도 치마 교복 입고가잖아요",
          "speaker_id": "2",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011829.1.18",
          "form": "어떻게 놀이기구 타는건지",
          "original_form": "어떻게 놀이기구 타는건지",
          "speaker_id": "2",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011829.1.19",
          "form": "아 무슨 교복치마입고 놀이기구타요?",
          "original_form": "아 무슨 교복치마입고 놀이기구타요?",
          "speaker_id": "1",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011829.1.20",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011829.1.21",
          "form": "회전목마도 불편할거같은데",
          "original_form": "회전목마도 불편할거같은데",
          "speaker_id": "1",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011829.1.22",
          "form": "그런것 같던데요?",
          "original_form": "그런것 같던데요?",
          "speaker_id": "2",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011829.1.23",
          "form": "맞아요 회전목마도 어떻게 타는건지",
          "original_form": "맞아요 회전목마도 어떻게 타는건지",
          "speaker_id": "2",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011829.1.24",
          "form": "뭐 외투로 가려도 한계가 있을텐데",
          "original_form": "뭐 외투로 가려도 한계가 있을텐데",
          "speaker_id": "2",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011829.1.25",
          "form": "이런 불편함도 여자들만 겪는거겠죠",
          "original_form": "이런 불편함도 여자들만 겪는거겠죠",
