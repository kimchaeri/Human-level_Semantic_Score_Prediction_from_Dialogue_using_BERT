      "utterance": [
        {
          "id": "MDRW2100018598.1.1",
          "form": "언니는 저거 방송 봤어요",
          "original_form": "언니는 저거 방송 봤어요",
          "speaker_id": "2",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018598.1.2",
          "form": "아니 방송은 못 봤는데",
          "original_form": "아니 방송은 못 봤는데",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018598.1.3",
          "form": "어제 자기 전에 카페 보니까 난리더라고",
          "original_form": "어제 자기 전에 카페 보니까 난리더라고",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018598.1.4",
          "form": "종 이태우는 거 아니고",
          "original_form": "종 이태우는 거 아니고",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018598.1.5",
          "form": "아 난리 났길래 몽 공 싶어서 다시 봤죠",
          "original_form": "아 난리 났길래 몽 공 싶어서 다시 봤죠",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018598.1.6",
          "form": "그거 전에도 산업 폐기물 태우는 거라고 얘기 나왔는데",
          "original_form": "그거 전에도 산업 폐기물 태우는 거라고 얘기 나왔는데",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018598.1.7",
          "form": "어느 순간 또 조용해졌죠",
          "original_form": "어느 순간 또 조용해졌죠",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018598.1.8",
          "form": "폐기물",
          "original_form": "폐기물",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018598.1.9",
          "form": "이라고 하더만",
          "original_form": "이라고 하더만",
          "speaker_id": "1",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100018598.1.10",
          "form": "내 맞아요 쓰레기 태워요",
          "original_form": "내 맞아요 쓰레기 태워요",
          "speaker_id": "2",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100018598.1.11",
          "form": "애들 지우기 힘드네요",
          "original_form": "애들 지우기 힘드네요",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018598.1.12",
          "form": "애들 키우기",
          "original_form": "애들 키우기",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018598.1.13",
          "form": "그렇다",
          "original_form": "그렇다",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018598.1.14",
          "form": "이사 갈 수도 없고",
          "original_form": "이사 갈 수도 없고",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018598.1.15",
          "form": "한동안은 또 카페가 시끌벅적하겠네",
          "original_form": "한동안은 또 카페가 시끌벅적하겠네",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018598.1.16",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018598.1.17",
          "form": "이러다 또 잊혀질 듯",
          "original_form": "이러다 또 잊혀질 듯",
          "speaker_id": "2",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018598.1.18",
          "form": "밑에는 꾸준히 냄새가 많이 나요",
          "original_form": "밑에는 꾸준히 냄새가 많이 나요",
          "speaker_id": "2",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018598.1.19",
          "form": "아 진짜??",
          "original_form": "아 진짜??",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018598.1.20",
          "form": "나도 가끔 엄마 집 가면 무슨 냄새 나긴 나더라",
          "original_form": "나도 가끔 엄마 집 가면 무슨 냄새 나긴 나더라",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018598.1.21",
          "form": "그게 그 냄새 겠지??",
          "original_form": "그게 그 냄새 겠지??",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018598.1.22",
          "form": "맞아요 밑에는 냄새가 나요",
          "original_form": "맞아요 밑에는 냄새가 나요",
          "speaker_id": "2",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018598.1.23",
          "form": "우리도 아빠 집 가면 깜짝 놀라요",
          "original_form": "우리도 아빠 집 가면 깜짝 놀라요",
          "speaker_id": "2",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018598.1.24",
          "form": "비 오는 날은 더하고",
          "original_form": "비 오는 날은 더하고",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018598.1.25",
          "form": "글타",
          "original_form": "글타",
          "speaker_id": "1",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018598.1.26",
          "form": "저게 없어지겠나...",
          "original_form": "저게 없어지겠나...",
          "speaker_id": "1",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018598.1.27",
          "form": "권영진을 없애는게 빠르지",
          "original_form": "권영진을 없애는게 빠르지",
          "speaker_id": "1",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018598.1.28",
          "form": "저거 돈을 어마어마하게 번 데요",
          "original_form": "저거 돈을 어마어마하게 번 데요",
          "speaker_id": "2",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018598.1.29",
          "form": "그래서 군에서도 어떻게 할 수가 없대요",
          "original_form": "그래서 군에서도 어떻게 할 수가 없대요",
          "speaker_id": "2",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018598.1.30",
          "form": "권영진은 아직 살아 있나요",
          "original_form": "권영진은 아직 살아 있나요",
          "speaker_id": "2",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018598.1.31",
          "form": "아하 그러면 여기다가 신도시라 왜 지은거야",
          "original_form": "아하 그러면 여기다가 신도시라 왜 지은거야",
          "speaker_id": "1",
          "time": "20210827 09:52"
        },
        {
          "id": "MDRW2100018598.1.32",
          "form": "나쁜놈들",
          "original_form": "나쁜놈들",
          "speaker_id": "1",
          "time": "20210827 09:52"
        },
        {
          "id": "MDRW2100018598.1.33",
          "form": "이래놓고 애들을 키우라고...",
          "original_form": "이래놓고 애들을 키우라고...",
          "speaker_id": "1",
          "time": "20210827 09:52"
        },
        {
          "id": "MDRW2100018598.1.34",
          "form": "맨날 폐렴걸리는것도 열받는데....",
          "original_form": "맨날 폐렴걸리는것도 열받는데....",
          "speaker_id": "1",
          "time": "20210827 09:52"
        },
        {
          "id": "MDRW2100018598.1.35",
          "form": "그니까요 신도시를 지금은 안됐는데",
          "original_form": "그니까요 신도시를 지금은 안됐는데",
          "speaker_id": "2",
          "time": "20210827 09:57"
        },
        {
          "id": "MDRW2100018598.1.36",
          "form": "여기에 살던 사람조차도",
          "original_form": "여기에 살던 사람조차도",
          "speaker_id": "2",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018598.1.37",
          "form": "집 싼 거 찾아왔으니 그냥 살아 라면서",
          "original_form": "집 싼 거 찾아왔으니 그냥 살아 라면서",
          "speaker_id": "2",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018598.1.38",
          "form": "텍사모에서 그랬어요",
          "original_form": "텍사모에서 그랬어요",
          "speaker_id": "2",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018598.1.39",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018598.1.40",
          "form": "집싼거 라니...",
          "original_form": "집싼거 라니...",
          "speaker_id": "1",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018598.1.41",
          "form": "나 그래서 텍사모 탈퇴 요",
          "original_form": "나 그래서 텍사모 탈퇴 요",
          "speaker_id": "2",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018598.1.42",
          "form": "지같은면",
          "original_form": "지같은면",
          "speaker_id": "1",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018598.1.43",
          "form": "있던 거를 쫓아 보낼 수는 없지만",
          "original_form": "있던 거를 쫓아 보낼 수는 없지만",
          "speaker_id": "2",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018598.1.44",
          "form": "발암물질 나오는데",
          "original_form": "발암물질 나오는데",
          "speaker_id": "1",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018598.1.45",
          "form": "그래도 제 재는 해줄 수 있잖아요",
          "original_form": "그래도 제 재는 해줄 수 있잖아요",
          "speaker_id": "2",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018598.1.46",
          "form": "집값 싸다고 왔겠나",
          "original_form": "집값 싸다고 왔겠나",
          "speaker_id": "1",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018598.1.47",
          "form": "미를 치는 사람이네",
          "original_form": "미를 치는 사람이네",
          "speaker_id": "1",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018598.1.48",
          "form": "그리고 여기 제지 공장에서 일하는 사람들도 많아요",
          "original_form": "그리고 여기 제지 공장에서 일하는 사람들도 많아요",
          "speaker_id": "2",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018598.1.49",
          "form": "그래서 더 시시하는 듯",
          "original_form": "그래서 더 시시하는 듯",
          "speaker_id": "2",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018598.1.50",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "1",
          "time": "20210827 10:00"
        },
        {
          "id": "MDRW2100018598.1.51",
          "form": "이제 애들 아프기만 하면",
          "original_form": "이제 애들 아프기만 하면",
          "speaker_id": "1",
          "time": "20210827 10:00"
        },
        {
          "id": "MDRW2100018598.1.52",
          "form": "제지공장때문이라 생각할듯",
          "original_form": "제지공장때문이라 생각할듯",
          "speaker_id": "1",
          "time": "20210827 10:00"
        },
        {
          "id": "MDRW2100018598.1.53",
          "form": "제지공장 옮기는건 안될거같고",
          "original_form": "제지공장 옮기는건 안될거같고",
          "speaker_id": "1",
          "time": "20210827 10:01"
        }
      ]
