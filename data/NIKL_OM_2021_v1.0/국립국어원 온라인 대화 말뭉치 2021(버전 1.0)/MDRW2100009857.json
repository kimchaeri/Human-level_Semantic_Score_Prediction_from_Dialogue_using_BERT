      "utterance": [
        {
          "id": "MDRW2100009857.1.1",
          "form": "안녕하세요 게임 하시나요?",
          "original_form": "안녕하세요 게임 하시나요?",
          "speaker_id": "1",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100009857.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100009857.1.3",
          "form": "저 스타듀밸리 합니다 후후후후",
          "original_form": "저 스타듀밸리 합니다 후후후후",
          "speaker_id": "2",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009857.1.4",
          "form": "name1님은 어떠세요?",
          "original_form": "&name1&님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009857.1.5",
          "form": "오 스타듀밸리 재밌죠! 전 롤을 하고 있어요",
          "original_form": "오 스타듀밸리 재밌죠! 전 롤을 하고 있어요",
          "speaker_id": "1",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009857.1.6",
          "form": "오오 저 며칠 전에 롤 배웠습니다!",
          "original_form": "오오 저 며칠 전에 롤 배웠습니다!",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009857.1.7",
          "form": "생각보다 재미지던데요",
          "original_form": "생각보다 재미지던데요",
          "speaker_id": "2",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100009857.1.8",
          "form": "맨날 왜하나싶었는뎈ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맨날 왜하나싶었는뎈ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100009857.1.9",
          "form": "맞아요ㅋㅋㅋ 어떤 캐릭터 주로 하시나요?",
          "original_form": "맞아요ㅋㅋㅋ 어떤 캐릭터 주로 하시나요?",
          "speaker_id": "1",
          "time": "20210823 11:05"
        },
        {
          "id": "MDRW2100009857.1.10",
          "form": "저는 럭스요 ㅋㅋㅋㅋㅋㅋ 그 노란머리 여캐맞죠...?",
          "original_form": "저는 럭스요 ㅋㅋㅋㅋㅋㅋ 그 노란머리 여캐맞죠...?",
          "speaker_id": "2",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009857.1.11",
          "form": "애인이랑 애인친구가 같이 알려줬는데 제일 편해보였슴다...",
          "original_form": "애인이랑 애인친구가 같이 알려줬는데 제일 편해보였슴다...",
          "speaker_id": "2",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100009857.1.12",
          "form": "(물론 저는 미니언이랑 놀고 탑 뿌시는건 애인이 다함)",
          "original_form": "(물론 저는 미니언이랑 놀고 탑 뿌시는건 애인이 다함)",
          "speaker_id": "2",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100009857.1.13",
          "form": "아 럭스로 서포터 가시는군요!ㅋㅋㅋㅋ",
          "original_form": "아 럭스로 서포터 가시는군요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009857.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 롤린이는 쫄래쫄래 따라다니며 미니언이랑 놀기만 합니다 히히",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 롤린이는 쫄래쫄래 따라다니며 미니언이랑 놀기만 합니다 히히",
          "speaker_id": "2",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009857.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ서포터시면 룰루도 추천해드릴게요 엄청 귀엽거든요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ서포터시면 룰루도 추천해드릴게요 엄청 귀엽거든요",
          "speaker_id": "1",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009857.1.16",
          "form": "오 저 웬만한 캐릭터들 다 한번씩만(ㅋㅋㅋㅋㅋ) 해봤는데 룰루도 했던 것 같아요!",
          "original_form": "오 저 웬만한 캐릭터들 다 한번씩만(ㅋㅋㅋㅋㅋ) 해봤는데 룰루도 했던 것 같아요!",
          "speaker_id": "2",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009857.1.17",
          "form": "그 쪼꼬미 아닌가욧",
          "original_form": "그 쪼꼬미 아닌가욧",
          "speaker_id": "2",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009857.1.18",
          "form": "맞아요 엄청 귀여운데 1티어예요 ㅋㅋ",
          "original_form": "맞아요 엄청 귀여운데 1티어예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:39"
        },
        {
          "id": "MDRW2100009857.1.19",
          "form": "오잉 1티어가 뭔가요!",
          "original_form": "오잉 1티어가 뭔가요!",
          "speaker_id": "2",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009857.1.20",
          "form": "음 제일 사람들이 많이하는 캐릭터요!",
          "original_form": "음 제일 사람들이 많이하는 캐릭터요!",
          "speaker_id": "1",
          "time": "20210823 12:15"
        }
      ]
    }
  ]
