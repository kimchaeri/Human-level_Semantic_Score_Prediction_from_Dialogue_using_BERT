      "utterance": [
        {
          "id": "MDRW2100039660.1.1",
          "form": "ㅇㅇ된다 그룹",
          "original_form": "ㅇㅇ된다 그룹",
          "speaker_id": "2",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039660.1.2",
          "form": "신기한 세상이네..",
          "original_form": "신기한 세상이네..",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039660.1.3",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039660.1.4",
          "form": "엄마 집 다른집에서 하라는거지?",
          "original_form": "엄마 집 다른집에서 하라는거지?",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039660.1.5",
          "form": "아니 그냥 받을것만 그집에 받자는거",
          "original_form": "아니 그냥 받을것만 그집에 받자는거",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039660.1.6",
          "form": "응 그거 말고 다른 부분은...딴 곳에서",
          "original_form": "응 그거 말고 다른 부분은...딴 곳에서",
          "speaker_id": "1",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039660.1.7",
          "form": "다른집에서 받았다가 위에서 돈안주면 어쩔건데그러네 안서방이..",
          "original_form": "다른집에서 받았다가 위에서 돈안주면 어쩔건데그러네 안서방이..",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039660.1.8",
          "form": "그걸 윗집이랑 이야기해야지...어디서 하든 마음대로 하라했잖아",
          "original_form": "그걸 윗집이랑 이야기해야지...어디서 하든 마음대로 하라했잖아",
          "speaker_id": "1",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039660.1.9",
          "form": "인테리어가 안쉽다 스트레스만땅",
          "original_form": "인테리어가 안쉽다 스트레스만땅",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039660.1.10",
          "form": "그걸누가 얘기하노",
          "original_form": "그걸누가 얘기하노",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039660.1.11",
          "form": "그렇지 집지으면 장난아니라잖아",
          "original_form": "그렇지 집지으면 장난아니라잖아",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039660.1.12",
          "form": "머리아프게됐다",
          "original_form": "머리아프게됐다",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039660.1.13",
          "form": "그렇네....",
          "original_form": "그렇네....",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039660.1.14",
          "form": "현정이는 밥도 못 먹나보던데",
          "original_form": "현정이는 밥도 못 먹나보던데",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039660.1.15",
          "form": "스트레스받아서?",
          "original_form": "스트레스받아서?",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039660.1.16",
          "form": "이사가는것도 그렇고 이것저것..다 죽어가던데",
