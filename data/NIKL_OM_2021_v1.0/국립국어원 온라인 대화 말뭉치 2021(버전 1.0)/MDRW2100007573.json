      "utterance": [
        {
          "id": "MDRW2100007573.1.1",
          "form": "옵치해?",
          "original_form": "옵치해?",
          "speaker_id": "1",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007573.1.2",
          "form": "옵치 예전에 하다가 어려워서 접었어.",
          "original_form": "옵치 예전에 하다가 어려워서 접었어.",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007573.1.3",
          "form": "단호하네....",
          "original_form": "단호하네....",
          "speaker_id": "1",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007573.1.4",
          "form": "지금 하는 게임은??",
          "original_form": "지금 하는 게임은??",
          "speaker_id": "1",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007573.1.5",
          "form": "스듀하거나 망겜이긴 하지만..좀비고 하고 있어.",
          "original_form": "스듀하거나 망겜이긴 하지만..좀비고 하고 있어.",
          "speaker_id": "2",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007573.1.6",
          "form": "넌 게임하는 거 뭐 있어?",
          "original_form": "넌 게임하는 거 뭐 있어?",
          "speaker_id": "2",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007573.1.7",
          "form": "스듀!!!!",
          "original_form": "스듀!!!!",
          "speaker_id": "1",
          "time": "20210822 03:31"
        },
        {
          "id": "MDRW2100007573.1.8",
          "form": "어제도했지",
          "original_form": "어제도했지",
          "speaker_id": "1",
          "time": "20210822 03:31"
        },
        {
          "id": "MDRW2100007573.1.9",
          "form": "굶지마두 해",
          "original_form": "굶지마두 해",
          "speaker_id": "1",
          "time": "20210822 03:31"
        },
        {
          "id": "MDRW2100007573.1.10",
          "form": "굶지마 재밌어?",
          "original_form": "굶지마 재밌어?",
          "speaker_id": "2",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007573.1.11",
          "form": "해볼까 고민 중이었는데",
          "original_form": "해볼까 고민 중이었는데",
          "speaker_id": "2",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007573.1.12",
          "form": "존잼존잼",
          "original_form": "존잼존잼",
          "speaker_id": "1",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007573.1.13",
          "form": "무조건 멀티로 ㄱㄱ",
          "original_form": "무조건 멀티로 ㄱㄱ",
          "speaker_id": "1",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007573.1.14",
          "form": "친구 꼬셔서 해야겠다 ><",
          "original_form": "친구 꼬셔서 해야겠다 ><",
          "speaker_id": "2",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007573.1.15",
          "form": "굶지마 말고도 추천해줄 게임 있어?",
          "original_form": "굶지마 말고도 추천해줄 게임 있어?",
          "speaker_id": "2",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007573.1.16",
          "form": "너무좋지",
          "original_form": "너무좋지",
          "speaker_id": "1",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007573.1.17",
          "form": "많이 꼬셩",
          "original_form": "많이 꼬셩",
          "speaker_id": "1",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007573.1.18",
          "form": "음 프린세스메이커 2?",
          "original_form": "음 프린세스메이커 2?",
          "speaker_id": "1",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007573.1.19",
          "form": "억 그거 완전 고전게임 아녀? 요즘도 할 수 있는 거였어?????",
          "original_form": "억 그거 완전 고전게임 아녀? 요즘도 할 수 있는 거였어?????",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007573.1.20",
          "form": "당근이지!!!!!",
          "original_form": "당근이지!!!!!",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007573.1.21",
          "form": "스팀에 있어",
          "original_form": "스팀에 있어",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007573.1.22",
          "form": "세일 때 사면 완존 싸다",
          "original_form": "세일 때 사면 완존 싸다",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007573.1.23",
          "form": "헐 스팀 세일할때 질러야겠다",
          "original_form": "헐 스팀 세일할때 질러야겠다",
          "speaker_id": "2",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007573.1.24",
          "form": "이렇게 플레이 타임 1시간도 안되고 쌓여가는 게임들",
          "original_form": "이렇게 플레이 타임 1시간도 안되고 쌓여가는 게임들",
          "speaker_id": "2",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007573.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아",
          "speaker_id": "1",
          "time": "20210822 03:49"
        },
        {
          "id": "MDRW2100007573.1.26",
          "form": "사놓고 까보지도 않은 게임들이 수두룩...",
          "original_form": "사놓고 까보지도 않은 게임들이 수두룩...",
          "speaker_id": "1",
          "time": "20210822 03:50"
        },
        {
          "id": "MDRW2100007573.1.27",
          "form": "하..... 토끼같은 게임들이 날 기다리는데",
          "original_form": "하..... 토끼같은 게임들이 날 기다리는데",
          "speaker_id": "2",
          "time": "20210822 03:51"
        },
        {
          "id": "MDRW2100007573.1.28",
          "form": "키는 순간 귀찮아서 벌러덩 누워버려 ㅋㅋ ㅠㅠ",
          "original_form": "키는 순간 귀찮아서 벌러덩 누워버려 ㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:51"
        },
        {
          "id": "MDRW2100007573.1.29",
          "form": "토끼같은 게임들이 있는데...",
          "original_form": "토끼같은 게임들이 있는데...",
          "speaker_id": "1",
          "time": "20210822 04:01"
        },
        {
          "id": "MDRW2100007573.1.30",
          "form": "맨날 하던 게임만 해....",
          "original_form": "맨날 하던 게임만 해....",
