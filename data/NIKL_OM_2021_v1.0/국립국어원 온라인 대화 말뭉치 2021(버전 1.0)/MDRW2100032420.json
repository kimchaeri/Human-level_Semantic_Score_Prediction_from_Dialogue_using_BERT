      "utterance": [
        {
          "id": "MDRW2100032420.1.1",
          "form": "싫어하는 음식은 뭐예요?",
          "original_form": "싫어하는 음식은 뭐예요?",
          "speaker_id": "1",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100032420.1.2",
          "form": "가지튀김을 제외한 가지, 콩팥이 피처링된 밥, 코다리튀김, 급 식 스타일의 느끼한 스프",
          "original_form": "가지튀김을 제외한 가지, 콩팥이 피처링된 밥, 코다리튀김, 급 식 스타일의 느끼한 스프",
          "speaker_id": "2",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100032420.1.3",
          "form": "등이 있습니다",
          "original_form": "등이 있습니다",
          "speaker_id": "2",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100032420.1.4",
          "form": "코다리튀김 급식에서",
          "original_form": "코다리튀김 급식에서",
          "speaker_id": "1",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100032420.1.5",
          "form": "최악이었죠",
          "original_form": "최악이었죠",
          "speaker_id": "1",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100032420.1.6",
          "form": "치킨인줄 착각하고 ㅎ",
          "original_form": "치킨인줄 착각하고 ㅎ",
          "speaker_id": "1",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100032420.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 밥경찰이에요 푸드폴리스",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 밥경찰이에요 푸드폴리스",
          "speaker_id": "2",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100032420.1.8",
          "form": "아, 해파리냉채도 싫어해요",
          "original_form": "아, 해파리냉채도 싫어해요",
          "speaker_id": "2",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100032420.1.9",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100032420.1.10",
          "form": "냉채 겨자냄세 심하죠",
          "original_form": "냉채 겨자냄세 심하죠",
          "speaker_id": "1",
          "time": "20210905 17:17"
        },
        {
          "id": "MDRW2100032420.1.11",
          "form": "저는 민초랑 고수랑 외국향신료 센거 못먹겠어요",
          "original_form": "저는 민초랑 고수랑 외국향신료 센거 못먹겠어요",
          "speaker_id": "1",
          "time": "20210905 17:17"
        },
        {
          "id": "MDRW2100032420.1.12",
          "form": "오, 저도 고수 못 먹어요",
          "original_form": "오, 저도 고수 못 먹어요",
          "speaker_id": "2",
          "time": "20210905 17:18"
        },
        {
          "id": "MDRW2100032420.1.13",
          "form": "ㄹㅇ 비누맛이 이런걸까 싶더라구요",
          "original_form": "ㄹㅇ 비누맛이 이런걸까 싶더라구요",
          "speaker_id": "2",
          "time": "20210905 17:18"
        },
        {
          "id": "MDRW2100032420.1.14",
          "form": "아 진짜요 쌀국수 처음 먹을때",
          "original_form": "아 진짜요 쌀국수 처음 먹을때",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032420.1.15",
          "form": "뭣모르고 넣었다가",
          "original_form": "뭣모르고 넣었다가",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032420.1.16",
          "form": "면만 겨우 다 먹었어요",
          "original_form": "면만 겨우 다 먹었어요",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032420.1.17",
          "form": "ㅋㅋㅋㅋㅋ 면 구출 대작전",
          "original_form": "ㅋㅋㅋㅋㅋ 면 구출 대작전",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032420.1.18",
          "form": "오염된 고수물에서 국수를 구출하라",
          "original_form": "오염된 고수물에서 국수를 구출하라",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032420.1.19",
          "form": "그뒤로 쌀국수 싫어하다가",
          "original_form": "그뒤로 쌀국수 싫어하다가",
          "speaker_id": "1",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100032420.1.20",
          "form": "나중에 고수빼고 먹었는데 맛있더라구요 ㅋㅋㅋ",
          "original_form": "나중에 고수빼고 먹었는데 맛있더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100032420.1.21",
          "form": "그쵸ㅋㅋㅋ\r\n저는 베트남하면 반미 샌드위치도 맛있더라구요",
          "original_form": "그쵸ㅋㅋㅋ\r\n저는 베트남하면 반미 샌드위치도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210905 17:24"
        },
        {
          "id": "MDRW2100032420.1.22",
          "form": "반미도 맛있죠 그것도 고수빼고 먹어요 ㅋㅋㄱ",
          "original_form": "반미도 맛있죠 그것도 고수빼고 먹어요 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210905 17:24"
        },
        {
          "id": "MDRW2100032420.1.23",
          "form": "아, 얘기하니 군침 도네요 ",
          "original_form": "아, 얘기하니 군침 도네요 ",
          "speaker_id": "2",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100032420.1.24",
          "form": "고수는 우리의 적",
          "original_form": "고수는 우리의 적",
          "speaker_id": "2",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100032420.1.25",
          "form": "그쵸 ㅜㅜ 얘기하니 먹고싶네요 저녁시간인데",
          "original_form": "그쵸 ㅜㅜ 얘기하니 먹고싶네요 저녁시간인데",
          "speaker_id": "1",
          "time": "20210905 17:27"
        }
      ]
    }
