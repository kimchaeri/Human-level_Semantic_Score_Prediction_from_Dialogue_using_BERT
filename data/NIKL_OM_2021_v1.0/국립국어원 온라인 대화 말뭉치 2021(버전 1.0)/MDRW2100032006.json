      "utterance": [
        {
          "id": "MDRW2100032006.1.1",
          "form": "아 역시 기상청이야",
          "original_form": "아 역시 기상청이야",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100032006.1.2",
          "form": "비가 내리네 날씨 좋다고 했는데...",
          "original_form": "비가 내리네 날씨 좋다고 했는데...",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100032006.1.3",
          "form": "맞아. 예보 너무 잘 틀려서 싫어",
          "original_form": "맞아. 예보 너무 잘 틀려서 싫어",
          "speaker_id": "1",
          "time": "20210905 11:03"
        },
        {
          "id": "MDRW2100032006.1.4",
          "form": "비오니까 몸이 힘들어진다",
          "original_form": "비오니까 몸이 힘들어진다",
          "speaker_id": "2",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100032006.1.5",
          "form": "ㅠㅠ 맛있는 거라도 먹고 힘내",
          "original_form": "ㅠㅠ 맛있는 거라도 먹고 힘내",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032006.1.6",
          "form": "사 줘~ 아 이런 날씨는 국물이 최고지 ㅎㅎ",
          "original_form": "사 줘~ 아 이런 날씨는 국물이 최고지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032006.1.7",
          "form": "나도 국물음식 먹고 싶어지네",
          "original_form": "나도 국물음식 먹고 싶어지네",
          "speaker_id": "1",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032006.1.8",
          "form": "뭐 설렁탕 곰탕 떡국? 아 쌀국수는 최근 먹었네",
          "original_form": "뭐 설렁탕 곰탕 떡국? 아 쌀국수는 최근 먹었네",
          "speaker_id": "2",
          "time": "20210905 11:13"
        },
        {
          "id": "MDRW2100032006.1.9",
          "form": "설렁탕이나 떡국이 좋아",
          "original_form": "설렁탕이나 떡국이 좋아",
          "speaker_id": "1",
          "time": "20210905 11:14"
        },
        {
          "id": "MDRW2100032006.1.10",
          "form": "비 오는 날에 최고지 ㅋㅋ",
          "original_form": "비 오는 날에 최고지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:15"
        },
        {
          "id": "MDRW2100032006.1.11",
          "form": "아 가을이긴 가을이야 비오면 춥다",
          "original_form": "아 가을이긴 가을이야 비오면 춥다",
          "speaker_id": "2",
          "time": "20210905 11:16"
        },
        {
          "id": "MDRW2100032006.1.12",
          "form": "나만 별로 안 춥나? 춥기보다는 조금 서늘한데",
          "original_form": "나만 별로 안 춥나? 춥기보다는 조금 서늘한데",
          "speaker_id": "1",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032006.1.13",
          "form": "내가 추위 약하잖아",
          "original_form": "내가 추위 약하잖아",
          "speaker_id": "2",
          "time": "20210905 11:20"
        },
        {
          "id": "MDRW2100032006.1.14",
          "form": "그렇긴 하지. 벌써 추워해서 큰일이네",
          "original_form": "그렇긴 하지. 벌써 추워해서 큰일이네",
          "speaker_id": "1",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100032006.1.15",
          "form": "그래서 겨울이 싫어 여름리 낫지",
          "original_form": "그래서 겨울이 싫어 여름리 낫지",
          "speaker_id": "2",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100032006.1.16",
          "form": "나도 여름이 더 좋아",
          "original_form": "나도 여름이 더 좋아",
          "speaker_id": "1",
          "time": "20210905 11:25"
        },
        {
          "id": "MDRW2100032006.1.17",
          "form": "여름 좋지 여름 더위습기 빼곤 좋아",
          "original_form": "여름 좋지 여름 더위습기 빼곤 좋아",
          "speaker_id": "2",
          "time": "20210905 11:28"
        }
