      "utterance": [
        {
          "id": "MDRW2100014178.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014178.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014178.1.3",
          "form": "보는것만 좋아해요",
          "original_form": "보는것만 좋아해요",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014178.1.4",
          "form": "제일 좋아하는 친구는 누군가요?",
          "original_form": "제일 좋아하는 친구는 누군가요?",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014178.1.5",
          "form": "저는 책임감이 없기 때문에",
          "original_form": "저는 책임감이 없기 때문에",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014178.1.6",
          "form": "저는 아무래도 개냥이요",
          "original_form": "저는 아무래도 개냥이요",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014178.1.7",
          "form": "그냥 고양이 말고",
          "original_form": "그냥 고양이 말고",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014178.1.8",
          "form": "개냥이요",
          "original_form": "개냥이요",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014178.1.9",
          "form": "그런 친구들이 오히려 더 잘 챙겨줘야해요",
          "original_form": "그런 친구들이 오히려 더 잘 챙겨줘야해요",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014178.1.10",
          "form": "맞아요 그래서 보기만할래요",
          "original_form": "맞아요 그래서 보기만할래요",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014178.1.11",
          "form": "쥬라기님은",
          "original_form": "쥬라기님은",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014178.1.12",
          "form": "두마리 키우실꺼같은데",
          "original_form": "두마리 키우실꺼같은데",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014178.1.13",
          "form": "맞나요?",
          "original_form": "맞나요?",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014178.1.14",
          "form": "ㅋㅋㅋ 족집게시네요",
          "original_form": "ㅋㅋㅋ 족집게시네요",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014178.1.15",
          "form": "강아지 두마리 키워요",
          "original_form": "강아지 두마리 키워요",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014178.1.16",
          "form": "그쵸 제가 좀 타고났어요",
          "original_form": "그쵸 제가 좀 타고났어요",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014178.1.17",
          "form": "혹시 강이지",
          "original_form": "혹시 강이지",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014178.1.18",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014178.1.19",
          "form": "방금 강아지 두마리라고 했는데요..",
          "original_form": "방금 강아지 두마리라고 했는데요..",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014178.1.20",
          "form": "집중을 안한거는 저였네요",
          "original_form": "집중을 안한거는 저였네요",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014178.1.21",
          "form": "죄송합니다",
          "original_form": "죄송합니다",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014178.1.22",
          "form": "그럴수도있죠",
          "original_form": "그럴수도있죠",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014178.1.23",
          "form": "강아지 이름은 무엇인가요?",
          "original_form": "강아지 이름은 무엇인가요?",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014178.1.24",
          "form": "비밀이라서 알려줄수 없어요",
          "original_form": "비밀이라서 알려줄수 없어요",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014178.1.25",
          "form": "그럼 반려동물을 한번도 안키워 본건가요?",
          "original_form": "그럼 반려동물을 한번도 안키워 본건가요?",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014178.1.26",
          "form": "아 여기에는",
          "original_form": "아 여기에는",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014178.1.27",
          "form": "아주 슬픈 이야기가 있답니다",
          "original_form": "아주 슬픈 이야기가 있답니다",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014178.1.28",
          "form": "들을준비되셨나요?",
          "original_form": "들을준비되셨나요?",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014178.1.29",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014178.1.30",
          "form": "시작합니다",
          "original_form": "시작합니다",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014178.1.31",
          "form": "8282",
          "original_form": "8282",
          "speaker_id": "1",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014178.1.32",
          "form": "때는 제가 중학생때였어요",
          "original_form": "때는 제가 중학생때였어요",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014178.1.33",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.34",
          "form": "재촉하지마세요",
          "original_form": "재촉하지마세요",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.35",
          "form": "암튼 저때 엄마가 시장에서 토끼를",
          "original_form": "암튼 저때 엄마가 시장에서 토끼를",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.36",
          "form": "아 네; 타자가 느리신가봐요",
          "original_form": "아 네; 타자가 느리신가봐요",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.37",
          "form": "사오셨어요",
          "original_form": "사오셨어요",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.38",
          "form": "어허",
          "original_form": "어허",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.39",
          "form": "오 토끼~",
          "original_form": "오 토끼~",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.40",
          "form": "그토끼 이름은",
          "original_form": "그토끼 이름은",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.41",
          "form": "name3",
          "original_form": "&name3&",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.42",
          "form": "name3요",
          "original_form": "&name3&요",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.43",
          "form": "근데 어린나이에",
          "original_form": "근데 어린나이에",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.44",
          "form": "네이밍센스가 구리시네요",
          "original_form": "네이밍센스가 구리시네요",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014178.1.45",
          "form": "맞이할 준비도",
          "original_form": "맞이할 준비도",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014178.1.46",
          "form": "안해놨는데 갑자기 온거죠!",
          "original_form": "안해놨는데 갑자기 온거죠!",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014178.1.47",
          "form": "상자에 풀깔고",
          "original_form": "상자에 풀깔고",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014178.1.48",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014178.1.49",
          "form": "구경하다가",
          "original_form": "구경하다가",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014178.1.50",
          "form": "배고픈거 같아서 상추를 줬어요",
          "original_form": "배고픈거 같아서 상추를 줬어요",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014178.1.51",
          "form": "애가 죽었어요",
          "original_form": "애가 죽었어요",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014178.1.52",
          "form": "상자면 갉아 먹지 않을까요",
          "original_form": "상자면 갉아 먹지 않을까요",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014178.1.53",
          "form": "그건 햄스터인가",
          "original_form": "그건 햄스터인가",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014178.1.54",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014178.1.55",
          "form": "엥??",
          "original_form": "엥??",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014178.1.56",
          "form": "어린토끼가",
          "original_form": "어린토끼가",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.57",
          "form": "토끼 상추 먹으면 죽나요",
          "original_form": "토끼 상추 먹으면 죽나요",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.58",
          "form": "스트레스 받아서",
          "original_form": "스트레스 받아서",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.59",
          "form": "밤에 끼악!소리지르고",
          "original_form": "밤에 끼악!소리지르고",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.60",
          "form": "죽은거였어요..",
          "original_form": "죽은거였어요..",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.61",
          "form": "토끼가 소리를 질러요?",
          "original_form": "토끼가 소리를 질러요?",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.62",
          "form": "눈물 좀 닦고 올게요",
          "original_form": "눈물 좀 닦고 올게요",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.63",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.64",
          "form": "토끼는",
          "original_form": "토끼는",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.65",
          "form": "건초를 먹어야했대요",
          "original_form": "건초를 먹어야했대요",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.66",
          "form": "저도 토끼 키웠었는데 걔가 발성이 있는 친구였군요",
          "original_form": "저도 토끼 키웠었는데 걔가 발성이 있는 친구였군요",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.67",
          "form": "아무 소리도 안내던데",
          "original_form": "아무 소리도 안내던데",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014178.1.68",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014178.1.69",
          "form": "마지막 목소리였어요",
          "original_form": "마지막 목소리였어요",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014178.1.70",
          "form": "자고일어났는데",
          "original_form": "자고일어났는데",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014178.1.71",
          "form": "동생이 문구점에서 뽑기를 하다가 얼결에 토끼를 뽑아왔어요",
          "original_form": "동생이 문구점에서 뽑기를 하다가 얼결에 토끼를 뽑아왔어요",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014178.1.72",
          "form": "토리가 없고",
          "original_form": "토리가 없고",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014178.1.73",
          "form": "지금 생각하면 동물학대예요",
