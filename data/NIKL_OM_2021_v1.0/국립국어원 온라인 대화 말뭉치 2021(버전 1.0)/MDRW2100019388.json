      "utterance": [
        {
          "id": "MDRW2100019388.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100019388.1.2",
          "form": "아니요 ㅜㅜ name2님은요?",
          "original_form": "아니요 ㅜㅜ &name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100019388.1.3",
          "form": "저도 안키워요 ㅜㅜ",
          "original_form": "저도 안키워요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100019388.1.4",
          "form": "강아지 좋아하는데ㅜㅜ",
          "original_form": "강아지 좋아하는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100019388.1.5",
          "form": "ㅠ 오 저는 고양이 좋아해요",
          "original_form": "ㅠ 오 저는 고양이 좋아해요",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100019388.1.6",
          "form": "고양이도 좋아요 ㅜㅜ 매력있어요 ㅋㅋㄱ",
          "original_form": "고양이도 좋아요 ㅜㅜ 매력있어요 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100019388.1.7",
          "form": "저희동네 1층집중에 고양이키우는집이 세곳잇는데 부럽더라고요",
          "original_form": "저희동네 1층집중에 고양이키우는집이 세곳잇는데 부럽더라고요",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100019388.1.8",
          "form": "으아 완전 귀엽겠어요 !!",
          "original_form": "으아 완전 귀엽겠어요 !!",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100019388.1.9",
          "form": "어떤종이에요?",
          "original_form": "어떤종이에요?",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100019388.1.10",
          "form": "한곳은 샴\r\n두집은 코숏이요 ㅋㅋ",
          "original_form": "한곳은 샴\r\n두집은 코숏이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100019388.1.11",
          "form": "아 셋 다 넘 좋아하는 친구들이네요 ㅋㅋㅋ",
          "original_form": "아 셋 다 넘 좋아하는 친구들이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100019388.1.12",
          "form": "제 친구도 코숏 키우는데 은근 애교쟁이던데요 ㅎㅎ",
          "original_form": "제 친구도 코숏 키우는데 은근 애교쟁이던데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100019388.1.13",
          "form": "너무 귀엽네요. \r\n그래서 가끔 그  중 한집 고양이들이 창가에서 사람구경하거든요.너무 귀엽더라고요ㅠㅠ",
          "original_form": "너무 귀엽네요. \r\n그래서 가끔 그  중 한집 고양이들이 창가에서 사람구경하거든요.너무 귀엽더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100019388.1.14",
          "form": "으아 짱 귀엽겠어요 ㅜㅜ",
          "original_form": "으아 짱 귀엽겠어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100019388.1.15",
          "form": "뭔가 동네 마스코트 느낌 ㅋㅋ ㅠㅠㅠ",
          "original_form": "뭔가 동네 마스코트 느낌 ㅋㅋ ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100019388.1.16",
          "form": "고양이 보러 놀러갈수잇어서 부럽네요 ㅠ",
          "original_form": "고양이 보러 놀러갈수잇어서 부럽네요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100019388.1.17",
          "form": "저 가끔 놀러가는데 하 넘 부러워요 키우는거 ㅋㅋ",
          "original_form": "저 가끔 놀러가는데 하 넘 부러워요 키우는거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100019388.1.18",
          "form": "근데 털이 진짜 많이 묻더라구요 ㅋㅋ",
