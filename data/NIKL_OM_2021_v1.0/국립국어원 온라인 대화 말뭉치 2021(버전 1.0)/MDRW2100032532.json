        },
        {
          "id": "MDRW2100032532.1.2",
          "form": "제가 한박자 먼저 질러서",
          "original_form": "제가 한박자 먼저 질러서",
          "speaker_id": "2",
          "time": "20210906 08:38"
        },
        {
          "id": "MDRW2100032532.1.3",
          "form": "관객들 다 빵터졌어요ㅋㅋ",
          "original_form": "관객들 다 빵터졌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 08:38"
        },
        {
          "id": "MDRW2100032532.1.4",
          "form": "ㅋㅋㅋㅋ부끄러웠겠네요",
          "original_form": "ㅋㅋㅋㅋ부끄러웠겠네요",
          "speaker_id": "1",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100032532.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ후다닥 나왔죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ후다닥 나왔죠",
          "speaker_id": "2",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100032532.1.6",
          "form": "예능은 뭐 보시나요",
          "original_form": "예능은 뭐 보시나요",
          "speaker_id": "2",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100032532.1.7",
          "form": "저 유퀴즈랑 대탈출 좋아해요",
          "original_form": "저 유퀴즈랑 대탈출 좋아해요",
          "speaker_id": "1",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100032532.1.8",
          "form": "와우 대탈출!!!",
          "original_form": "와우 대탈출!!!",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100032532.1.9",
          "form": "ㅋㅋㅋ 세트가 흥미진진해요",
          "original_form": "ㅋㅋㅋ 세트가 흥미진진해요",
          "speaker_id": "1",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100032532.1.10",
          "form": "전 이제 이 대화를 마지막으로",
          "original_form": "전 이제 이 대화를 마지막으로",
          "speaker_id": "1",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100032532.1.11",
          "form": "500개 이상을 채웠기 땜에",
          "original_form": "500개 이상을 채웠기 땜에",
          "speaker_id": "1",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100032532.1.12",
          "form": "지난주 대탈출 보러 떠나겠습니다 ㅎㅎㅎ",
          "original_form": "지난주 대탈출 보러 떠나겠습니다 ㅎㅎㅎ",
