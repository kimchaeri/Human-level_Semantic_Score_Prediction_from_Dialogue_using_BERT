      "utterance": [
        {
          "id": "MDRW2100012564.1.1",
          "form": "까페좋아하시죠?",
          "original_form": "까페좋아하시죠?",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012564.1.2",
          "form": "메뉴이야기해볼까요?ㅎㅎ",
          "original_form": "메뉴이야기해볼까요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012564.1.3",
          "form": "저  커피좋아해요",
          "original_form": "저  커피좋아해요",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012564.1.4",
          "form": "커피 무슨종류요? 라떼?",
          "original_form": "커피 무슨종류요? 라떼?",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012564.1.5",
          "form": "마끼아또나 바닐라라떼 요",
          "original_form": "마끼아또나 바닐라라떼 요",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012564.1.6",
          "form": "달달커피",
          "original_form": "달달커피",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012564.1.7",
          "form": "오오ㅎ 저도 달달이좋음ㅎ",
          "original_form": "오오ㅎ 저도 달달이좋음ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012564.1.8",
          "form": "아메리카노는 좋아하세요?",
          "original_form": "아메리카노는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012564.1.9",
          "form": "아뇨ㅎ 물을 잘안마시는 체질이라ㅋ",
          "original_form": "아뇨ㅎ 물을 잘안마시는 체질이라ㅋ",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012564.1.10",
          "form": "아메는 사실 무슨맛으로 먹는지 모르겠어요",
          "original_form": "아메는 사실 무슨맛으로 먹는지 모르겠어요",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012564.1.11",
          "form": "담뱃재맛안나요?ㅎ",
          "original_form": "담뱃재맛안나요?ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012564.1.12",
          "form": "담배는안피는데ㅎ처음에먹을때 그런맛이나는것같앴어요ㅎ",
          "original_form": "담배는안피는데ㅎ처음에먹을때 그런맛이나는것같앴어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012564.1.13",
          "form": "그렇게 느낄수도있겠어요",
          "original_form": "그렇게 느낄수도있겠어요",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012564.1.14",
          "form": "아메안드시나요?",
          "original_form": "아메안드시나요?",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012564.1.15",
          "form": "가끔먹어요ㅎㅎ",
          "original_form": "가끔먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012564.1.16",
          "form": "전 달달한거ㅎ",
          "original_form": "전 달달한거ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100012564.1.17",
          "form": "스벅으로따지면 자바칩?ㅎ",
          "original_form": "스벅으로따지면 자바칩?ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100012564.1.18",
          "form": "저도 무조건 달달",
          "original_form": "저도 무조건 달달",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100012564.1.19",
          "form": "무조건 초코ㅎ",
          "original_form": "무조건 초코ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100012564.1.20",
          "form": "전 더블에쏘",
          "original_form": "전 더블에쏘",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100012564.1.21",
          "form": "까페는 달달한거드시면ㅎ",
          "original_form": "까페는 달달한거드시면ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100012564.1.22",
          "form": "수다하는건 좋아하세요?ㅎ",
          "original_form": "수다하는건 좋아하세요?ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100012564.1.23",
          "form": "까페많이가면 수다 좋아하시는분일지도ㅎㄹ",
          "original_form": "까페많이가면 수다 좋아하시는분일지도ㅎㄹ",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100012564.1.24",
          "form": "네ㅎ 주로 스트레스는 수다로",
          "original_form": "네ㅎ 주로 스트레스는 수다로",
          "speaker_id": "1",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100012564.1.25",
          "form": "친구셋이 만남 스벅에서 5시간씩 앉아있어요",
          "original_form": "친구셋이 만남 스벅에서 5시간씩 앉아있어요",
          "speaker_id": "1",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100012564.1.26",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012564.1.27",
          "form": "역시ㅎ 수다쟁이는 수다쟁이를 알아보죠ㅎ",
          "original_form": "역시ㅎ 수다쟁이는 수다쟁이를 알아보죠ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012564.1.28",
          "form": "그러실것같애요ㅎ",
          "original_form": "그러실것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012564.1.29",
          "form": "name2도 남자분치곤 수다스러우신듯",
          "original_form": "&name2&도 남자분치곤 수다스러우신듯",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012564.1.30",
          "form": "전 예전에 달콤이 24시간 할때 친구랑 단둘이서 밤새이야기헸어요ㅎ",
          "original_form": "전 예전에 달콤이 24시간 할때 친구랑 단둘이서 밤새이야기헸어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012564.1.31",
          "form": "뭐이렇게 이야기할께많은지",
          "original_form": "뭐이렇게 이야기할께많은지",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012564.1.32",
          "form": "아무래도 쉴때라 그랬나봐요ㅎㅎ",
          "original_form": "아무래도 쉴때라 그랬나봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012564.1.33",
          "form": "저 수다쟁이같애요?",
          "original_form": "저 수다쟁이같애요?",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012564.1.34",
          "form": "별거아닌걸로도 끝이없죠ㅋ",
          "original_form": "별거아닌걸로도 끝이없죠ㅋ",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012564.1.35",
          "form": "ㅎㅎ 수다떠는게 세상에서 제일재밌어요ㅎ",
          "original_form": "ㅎㅎ 수다떠는게 세상에서 제일재밌어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012564.1.36",
          "form": "그사람을 알아가는것도 재미고",
          "original_form": "그사람을 알아가는것도 재미고",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012564.1.37",
          "form": "그사람이 이야기하는 삶에 대한 재미도 있고ㅎ",
          "original_form": "그사람이 이야기하는 삶에 대한 재미도 있고ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012564.1.38",
          "form": "세상 궁굼한게 많아서 그런가봐요ㅎ",
          "original_form": "세상 궁굼한게 많아서 그런가봐요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:32"
        }
      ]
