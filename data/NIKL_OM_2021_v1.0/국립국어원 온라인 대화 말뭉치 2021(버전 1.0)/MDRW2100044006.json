      "utterance": [
        {
          "id": "MDRW2100044006.1.1",
          "form": "하.. 개불 좋아하시는분",
          "original_form": "하.. 개불 좋아하시는분",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044006.1.2",
          "form": "너무좋네욬ㅋㅋㅋㅋ",
          "original_form": "너무좋네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044006.1.3",
          "form": "저도 엄청 좋아해서요",
          "original_form": "저도 엄청 좋아해서요",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044006.1.4",
          "form": "그쵸? 드물죠?",
          "original_form": "그쵸? 드물죠?",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044006.1.5",
          "form": "잘없어요",
          "original_form": "잘없어요",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044006.1.6",
          "form": "저만 개불먹어요",
          "original_form": "저만 개불먹어요",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044006.1.7",
          "form": "저도 가족들중에 저만먹어요 ㅠ",
          "original_form": "저도 가족들중에 저만먹어요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044006.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044006.1.9",
          "form": "저도...뭐 주변에 아무도안먹어요",
          "original_form": "저도...뭐 주변에 아무도안먹어요",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044006.1.10",
          "form": "친구도 가족도",
          "original_form": "친구도 가족도",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044006.1.11",
          "form": "오직 저를 위한 개불",
          "original_form": "오직 저를 위한 개불",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044006.1.12",
          "form": "저희 입맛 비슷하네요",
          "original_form": "저희 입맛 비슷하네요",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044006.1.13",
          "form": "개불먹는 친구를 심심이에서 보네요 ㅎㅎ",
          "original_form": "개불먹는 친구를 심심이에서 보네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044006.1.14",
          "form": "ㅋㅋ네네 매운건 좋아하세요?",
          "original_form": "ㅋㅋ네네 매운건 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044006.1.15",
          "form": "좋아하는데",
          "original_form": "좋아하는데",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044006.1.16",
          "form": "못먹어요",
          "original_form": "못먹어요",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044006.1.17",
          "form": "못먹는데 왜시키냐는데",
          "original_form": "못먹는데 왜시키냐는데",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044006.1.18",
          "form": "먹고싶어서 맨날 시켜요",
          "original_form": "먹고싶어서 맨날 시켜요",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044006.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그럴수있죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그럴수있죠",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044006.1.20",
          "form": "전 적당히 잘먹고",
          "original_form": "전 적당히 잘먹고",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044006.1.21",
          "form": "많이좋아합니닼ㅋㅋ",
          "original_form": "많이좋아합니닼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044006.1.22",
          "form": "저도 좋아해요 ㅠㅠㅠ먹고 배고플뿐...",
          "original_form": "저도 좋아해요 ㅠㅠㅠ먹고 배고플뿐...",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044006.1.23",
          "form": "배아플뿐 ?!",
          "original_form": "배아플뿐 ?!",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044006.1.24",
          "form": "아뇨 배고파요...",
          "original_form": "아뇨 배고파요...",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044006.1.25",
          "form": "못먹어서",
          "original_form": "못먹어서",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044006.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044006.1.27",
          "form": "뭘 먹은게없는듯이 먹어서 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭘 먹은게없는듯이 먹어서 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044006.1.28",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044006.1.29",
          "form": "잘못이해했네요",
          "original_form": "잘못이해했네요",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044006.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ보통 배아프죸ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ보통 배아프죸ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044006.1.31",
          "form": "커피도 좋아하세요?",
          "original_form": "커피도 좋아하세요?",
