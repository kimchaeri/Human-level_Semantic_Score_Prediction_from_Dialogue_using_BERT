      "utterance": [
        {
          "id": "MDRW2100046134.1.1",
          "form": "건강을 위해 무엇을 하나요",
          "original_form": "건강을 위해 무엇을 하나요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046134.1.2",
          "form": "가끔 유튜브서 타바타 따라해요",
          "original_form": "가끔 유튜브서 타바타 따라해요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046134.1.3",
          "form": "타바타가 먼가요",
          "original_form": "타바타가 먼가요",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046134.1.4",
          "form": "이삼십초 운동하고 십초쉬고 그렇게 반복하더라고요ㅋ",
          "original_form": "이삼십초 운동하고 십초쉬고 그렇게 반복하더라고요ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046134.1.5",
          "form": "헬스종류 인가요",
          "original_form": "헬스종류 인가요",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046134.1.6",
          "form": "운동기구는 없고요 에어로빅느낌인가 그래요ㅋ",
          "original_form": "운동기구는 없고요 에어로빅느낌인가 그래요ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046134.1.7",
          "form": "에어로빅요",
          "original_form": "에어로빅요",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046134.1.8",
          "form": "조카가 학교에서 한다고해서 봣는데 재미잇더라고요",
          "original_form": "조카가 학교에서 한다고해서 봣는데 재미잇더라고요",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046134.1.9",
          "form": "저도 찾아보고 싶네요",
          "original_form": "저도 찾아보고 싶네요",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046134.1.10",
          "form": "운동되요ㅋㅋ",
          "original_form": "운동되요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046134.1.11",
          "form": "재미도 있다고 하니 그것도 좋은것 같네요",
          "original_form": "재미도 있다고 하니 그것도 좋은것 같네요",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046134.1.12",
          "form": "담날 몸 쑤시더라고요ㅋㅋ",
          "original_form": "담날 몸 쑤시더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046134.1.13",
          "form": "하다보면 아픈것도 덜하겠죠",
          "original_form": "하다보면 아픈것도 덜하겠죠",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046134.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046134.1.15",
          "form": "하다보면 익숙해지겠죠",
          "original_form": "하다보면 익숙해지겠죠",
          "speaker_id": "1",
          "time": "20210908 11:13"
