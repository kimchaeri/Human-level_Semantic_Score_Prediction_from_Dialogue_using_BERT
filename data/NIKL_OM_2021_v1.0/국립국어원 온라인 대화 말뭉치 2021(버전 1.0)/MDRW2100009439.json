      "utterance": [
        {
          "id": "MDRW2100009439.1.1",
          "form": "절이랑 성당 야기했어요 ㅎㅎ",
          "original_form": "절이랑 성당 야기했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009439.1.2",
          "form": "오 감사해요 안 그래도 헷갈려요ㅠㅠ",
          "original_form": "오 감사해요 안 그래도 헷갈려요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009439.1.3",
          "form": "지금은 다 방학이죠 수강신청하셨어요?",
          "original_form": "지금은 다 방학이죠 수강신청하셨어요?",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009439.1.4",
          "form": "내일 수강신청ㅇ에요ㅋㅋ",
          "original_form": "내일 수강신청ㅇ에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009439.1.5",
          "form": "name2님은 하셨나요?",
          "original_form": "&name2&님은 하셨나요?",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009439.1.6",
          "form": "헉 내일이에요? 올클 기원해요!!",
          "original_form": "헉 내일이에요? 올클 기원해요!!",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009439.1.7",
          "form": "저는 다음학기 휴학이에요ㅎㅎ",
          "original_form": "저는 다음학기 휴학이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009439.1.8",
          "form": "다 노잼강의라서 ㅋㅋㅋㅋ 괜찮을것같아요",
          "original_form": "다 노잼강의라서 ㅋㅋㅋㅋ 괜찮을것같아요",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009439.1.9",
          "form": "헉 휴학하새요?",
          "original_form": "헉 휴학하새요?",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009439.1.10",
          "form": "솔직히 ㅠㅠ 넘 부럽내요 ㅠㅠ",
          "original_form": "솔직히 ㅠㅠ 넘 부럽내요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009439.1.11",
          "form": "앗 그렇군요ㅋㅋㅋㅋ",
          "original_form": "앗 그렇군요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009439.1.12",
          "form": "아니에요.. 취업공부해야 한답니다.. ㅎㅎ 전 학교 다니고 싶어요",
          "original_form": "아니에요.. 취업공부해야 한답니다.. ㅎㅎ 전 학교 다니고 싶어요",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009439.1.13",
          "form": "아앗 ㅠ 힘드시갰어요...",
          "original_form": "아앗 ㅠ 힘드시갰어요...",
          "speaker_id": "1",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009439.1.14",
          "form": "근데 저도 ..막학기라 취업공부 병행해야하내요 ㅠㅠ",
          "original_form": "근데 저도 ..막학기라 취업공부 병행해야하내요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009439.1.15",
          "form": "병행하는 게 더 힘들죠ㅠㅠ 화이팅이에요",
          "original_form": "병행하는 게 더 힘들죠ㅠㅠ 화이팅이에요",
          "speaker_id": "2",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100009439.1.16",
          "form": "수강신청은 성공하셨어요?",
          "original_form": "수강신청은 성공하셨어요?",
          "speaker_id": "2",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100009439.1.17",
          "form": "서버 터져서 ㅋㅋㅋㅋ",
          "original_form": "서버 터져서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100009439.1.18",
          "form": "전공만 주점주점 담았어요",
          "original_form": "전공만 주점주점 담았어요",
          "speaker_id": "1",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100009439.1.19",
          "form": "순탄치 않은 막학기가 될것같네요 ㅎ",
          "original_form": "순탄치 않은 막학기가 될것같네요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100009439.1.20",
          "form": "헉 서버 터졌나요..!",
          "original_form": "헉 서버 터졌나요..!",
          "speaker_id": "2",
          "time": "20210823 21:50"
        },
        {
          "id": "MDRW2100009439.1.21",
          "form": "그래도 전공은 채워서 다행이에요ㅜㅠ 전공이 중요하니까요ㅠㅜㅜ",
          "original_form": "그래도 전공은 채워서 다행이에요ㅜㅠ 전공이 중요하니까요ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 21:51"
        },
        {
          "id": "MDRW2100009439.1.22",
          "form": "맞아요 ㅠㅠ 졸업은 해야지..",
          "original_form": "맞아요 ㅠㅠ 졸업은 해야지..",
          "speaker_id": "1",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100009439.1.23",
          "form": "타교양신청일에 가서 주섬주섬 줏어오려고요",
          "original_form": "타교양신청일에 가서 주섬주섬 줏어오려고요",
          "speaker_id": "1",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100009439.1.24",
          "form": "그래.. 꿀교양은 일이학년이 들어야지",
          "original_form": "그래.. 꿀교양은 일이학년이 들어야지",
          "speaker_id": "1",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100009439.1.25",
          "form": "저도 꿀교양은 못 들어봤어요.. ㅋㅋㅋㅋ 꿀교양이 어떨 땐 전공보다 어려워요",
          "original_form": "저도 꿀교양은 못 들어봤어요.. ㅋㅋㅋㅋ 꿀교양이 어떨 땐 전공보다 어려워요",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100009439.1.26",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100009439.1.27",
          "form": "저희 영화보는 과목있는데",
          "original_form": "저희 영화보는 과목있는데",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100009439.1.28",
          "form": "진짜 맨날 일초컷이더라고요",
          "original_form": "진짜 맨날 일초컷이더라고요",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100009439.1.29",
          "form": "저희두 영화 관련 교양 인기 많아요! 다 똑같은가봐요ㅋㅋㅋㅋㅋㅋ",
