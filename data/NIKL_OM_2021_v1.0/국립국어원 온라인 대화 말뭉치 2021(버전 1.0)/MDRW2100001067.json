      "utterance": [
        {
          "id": "MDRW2100001067.1.1",
          "form": "요즘 학교생활 어때요??",
          "original_form": "요즘 학교생활 어때요??",
          "speaker_id": "1",
          "time": "20210722 22:59"
        },
        {
          "id": "MDRW2100001067.1.2",
          "form": "요즘 학교생활 걱정이 좀 많은 것 같아요",
          "original_form": "요즘 학교생활 걱정이 좀 많은 것 같아요",
          "speaker_id": "2",
          "time": "20210722 23:00"
        },
        {
          "id": "MDRW2100001067.1.3",
          "form": "무슨일이에요??",
          "original_form": "무슨일이에요??",
          "speaker_id": "1",
          "time": "20210722 23:00"
        },
        {
          "id": "MDRW2100001067.1.4",
          "form": "털어 놀 곳 없으면 저한테 털어놓으셔도 돼요!!",
          "original_form": "털어 놀 곳 없으면 저한테 털어놓으셔도 돼요!!",
          "speaker_id": "1",
          "time": "20210722 23:01"
        },
        {
          "id": "MDRW2100001067.1.5",
          "form": "헉 고마워요ㅠㅠ",
          "original_form": "헉 고마워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 23:01"
        },
        {
          "id": "MDRW2100001067.1.6",
          "form": "최근에 친구들이랑 좀 많이 멀어진 느낌이라서 걱정이 많은 것 같아요",
          "original_form": "최근에 친구들이랑 좀 많이 멀어진 느낌이라서 걱정이 많은 것 같아요",
          "speaker_id": "2",
          "time": "20210722 23:02"
        },
        {
          "id": "MDRW2100001067.1.7",
          "form": "선생님들 문제도 있고ㅠㅠㅠ",
          "original_form": "선생님들 문제도 있고ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 23:02"
        },
        {
          "id": "MDRW2100001067.1.8",
          "form": "아하.....",
          "original_form": "아하.....",
          "speaker_id": "1",
          "time": "20210722 23:02"
        },
        {
          "id": "MDRW2100001067.1.9",
          "form": "친구들이랑 싸웠나요??",
          "original_form": "친구들이랑 싸웠나요??",
          "speaker_id": "1",
          "time": "20210722 23:03"
        },
        {
          "id": "MDRW2100001067.1.10",
          "form": "음 한 친구랑 싸웠는데 그 친구가 무리를 끼고 있어서 약간 소외감들게 만들고 있어요..ㅠ",
          "original_form": "음 한 친구랑 싸웠는데 그 친구가 무리를 끼고 있어서 약간 소외감들게 만들고 있어요..ㅠ",
          "speaker_id": "2",
          "time": "20210722 23:04"
        },
        {
          "id": "MDRW2100001067.1.11",
          "form": "아.......",
          "original_form": "아.......",
          "speaker_id": "1",
          "time": "20210722 23:04"
        },
        {
          "id": "MDRW2100001067.1.12",
          "form": "너무하네요 그친구....",
          "original_form": "너무하네요 그친구....",
          "speaker_id": "1",
          "time": "20210722 23:04"
        },
        {
          "id": "MDRW2100001067.1.13",
          "form": "지금은 그럼 학교에서 누구랑지내요..??",
          "original_form": "지금은 그럼 학교에서 누구랑지내요..??",
          "speaker_id": "1",
          "time": "20210722 23:05"
        },
        {
          "id": "MDRW2100001067.1.14",
          "form": "학교에서 그냥 혼자 지내고 있어요",
          "original_form": "학교에서 그냥 혼자 지내고 있어요",
          "speaker_id": "2",
          "time": "20210722 23:05"
        },
        {
          "id": "MDRW2100001067.1.15",
          "form": "그래서 좀 많이 힘들었던 것 같아요",
          "original_form": "그래서 좀 많이 힘들었던 것 같아요",
          "speaker_id": "2",
          "time": "20210722 23:05"
        },
        {
          "id": "MDRW2100001067.1.16",
          "form": "아......",
          "original_form": "아......",
          "speaker_id": "1",
          "time": "20210722 23:05"
        },
        {
          "id": "MDRW2100001067.1.17",
          "form": "너무하네요 그친구.....",
          "original_form": "너무하네요 그친구.....",
          "speaker_id": "1",
          "time": "20210722 23:05"
        },
        {
          "id": "MDRW2100001067.1.18",
          "form": "그 친구가 나쁜거라 생각하며 지내고 있긴 한데",
          "original_form": "그 친구가 나쁜거라 생각하며 지내고 있긴 한데",
          "speaker_id": "2",
          "time": "20210722 23:06"
        },
        {
          "id": "MDRW2100001067.1.19",
          "form": "제가 해줄수있는건같이 욕해주는것 뿐이네요....",
          "original_form": "제가 해줄수있는건같이 욕해주는것 뿐이네요....",
          "speaker_id": "1",
          "time": "20210722 23:06"
        },
        {
          "id": "MDRW2100001067.1.20",
          "form": "그래도 학교 내에서 생활 하기가 좀 힘들어서 걱정이네요",
          "original_form": "그래도 학교 내에서 생활 하기가 좀 힘들어서 걱정이네요",
          "speaker_id": "2",
          "time": "20210722 23:06"
        },
        {
          "id": "MDRW2100001067.1.21",
          "form": "그 친구가 나쁜거에요!!!!",
          "original_form": "그 친구가 나쁜거에요!!!!",
          "speaker_id": "1",
          "time": "20210722 23:06"
        },
        {
          "id": "MDRW2100001067.1.22",
          "form": "앜ㅋㅋㅋㅋ 고마워요 이렇게 공감해주니 제가 다 속이 시원하네요",
          "original_form": "앜ㅋㅋㅋㅋ 고마워요 이렇게 공감해주니 제가 다 속이 시원하네요",
          "speaker_id": "2",
          "time": "20210722 23:06"
        },
        {
          "id": "MDRW2100001067.1.23",
          "form": "혹시 남자이신가요??",
          "original_form": "혹시 남자이신가요??",
          "speaker_id": "1",
          "time": "20210722 23:07"
        },
        {
          "id": "MDRW2100001067.1.24",
          "form": "그쪽은 학교내에서 걱정 같은거 없어요?",
          "original_form": "그쪽은 학교내에서 걱정 같은거 없어요?",
          "speaker_id": "2",
          "time": "20210722 23:07"
        },
        {
          "id": "MDRW2100001067.1.25",
          "form": "아뇨 전 여자예요",
          "original_form": "아뇨 전 여자예요",
          "speaker_id": "2",
          "time": "20210722 23:07"
        },
        {
          "id": "MDRW2100001067.1.26",
          "form": "저는 성적이......",
          "original_form": "저는 성적이......",
          "speaker_id": "1",
          "time": "20210722 23:07"
        },
        {
          "id": "MDRW2100001067.1.27",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210722 23:07"
        },
        {
          "id": "MDRW2100001067.1.28",
          "form": "아 학업문제ㅠㅠ",
          "original_form": "아 학업문제ㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 23:07"
        },
        {
          "id": "MDRW2100001067.1.29",
          "form": "성적이 저를 너무 힘들게해요...",
          "original_form": "성적이 저를 너무 힘들게해요...",
          "speaker_id": "1",
          "time": "20210722 23:07"
        },
        {
          "id": "MDRW2100001067.1.30",
          "form": "진짜 어쩔 수 없는 공통의 걱정이죠..",
          "original_form": "진짜 어쩔 수 없는 공통의 걱정이죠..",
          "speaker_id": "2",
          "time": "20210722 23:07"
        },
        {
          "id": "MDRW2100001067.1.31",
          "form": "성적이 잘 안나와요?",
          "original_form": "성적이 잘 안나와요?",
          "speaker_id": "2",
          "time": "20210722 23:08"
        },
        {
          "id": "MDRW2100001067.1.32",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "1",
          "time": "20210722 23:08"
        },
        {
          "id": "MDRW2100001067.1.33",
          "form": "학원도 다니는데....",
          "original_form": "학원도 다니는데....",
          "speaker_id": "1",
          "time": "20210722 23:08"
        },
        {
          "id": "MDRW2100001067.1.34",
          "form": "노력한것만큼 안나오네요.....",
          "original_form": "노력한것만큼 안나오네요.....",
          "speaker_id": "1",
          "time": "20210722 23:08"
        },
        {
          "id": "MDRW2100001067.1.35",
          "form": "아이고ㅠㅠ 힘들겠네요",
          "original_form": "아이고ㅠㅠ 힘들겠네요",
          "speaker_id": "2",
          "time": "20210722 23:09"
        },
        {
          "id": "MDRW2100001067.1.36",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210722 23:09"
        },
        {
          "id": "MDRW2100001067.1.37",
          "form": "감사해요... 이런거 들어주셔서.....",
          "original_form": "감사해요... 이런거 들어주셔서.....",
          "speaker_id": "1",
          "time": "20210722 23:10"
        },
        {
          "id": "MDRW2100001067.1.38",
          "form": "저도 그런적 많았는데 진짜 공부는 차근차근 꾸준하 하다가 훅 오른다고",
          "original_form": "저도 그런적 많았는데 진짜 공부는 차근차근 꾸준하 하다가 훅 오른다고",
          "speaker_id": "2",
          "time": "20210722 23:10"
        },
        {
          "id": "MDRW2100001067.1.39",
          "form": "중간고사에서 망했다고 기멀고사에서 훅 올라갔던 적 많았어요",
          "original_form": "중간고사에서 망했다고 기멀고사에서 훅 올라갔던 적 많았어요",
          "speaker_id": "2",
          "time": "20210722 23:10"
        },
        {
          "id": "MDRW2100001067.1.40",
          "form": "오오ㅗㅇ",
          "original_form": "오오ㅗㅇ",
          "speaker_id": "1",
          "time": "20210722 23:10"
        },
        {
          "id": "MDRW2100001067.1.41",
          "form": "저도 꾸준히 노력해야겠네요!!",
          "original_form": "저도 꾸준히 노력해야겠네요!!",
          "speaker_id": "1",
          "time": "20210722 23:10"
        },
        {
          "id": "MDRW2100001067.1.42",
          "form": "혹시 그냥 여쭤만 보는건데",
          "original_form": "혹시 그냥 여쭤만 보는건데",
          "speaker_id": "1",
          "time": "20210722 23:10"
        },
        {
          "id": "MDRW2100001067.1.43",
          "form": "음음 그럼요! 아직 시험 많으니까요",
          "original_form": "음음 그럼요! 아직 시험 많으니까요",
          "speaker_id": "2",
          "time": "20210722 23:11"
        },
        {
          "id": "MDRW2100001067.1.44",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210722 23:11"
        },
        {
          "id": "MDRW2100001067.1.45",
          "form": "SNS뭐뭐 하시나요??",
          "original_form": "SNS뭐뭐 하시나요??",
          "speaker_id": "1",
          "time": "20210722 23:11"
        },
