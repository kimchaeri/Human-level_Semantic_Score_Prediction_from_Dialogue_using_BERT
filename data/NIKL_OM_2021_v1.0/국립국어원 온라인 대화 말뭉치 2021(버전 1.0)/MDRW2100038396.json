      "utterance": [
        {
          "id": "MDRW2100038396.1.1",
          "form": "장보러 나갈건데",
          "original_form": "장보러 나갈건데",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038396.1.2",
          "form": "저녁 메뉴를 안정했어요",
          "original_form": "저녁 메뉴를 안정했어요",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038396.1.3",
          "form": "저두요ㅠㅠ",
          "original_form": "저두요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100038396.1.4",
          "form": "모먹지요?",
          "original_form": "모먹지요?",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100038396.1.5",
          "form": "그냥 이마트에서 양장피 사오는것도 생각하고 있어요",
          "original_form": "그냥 이마트에서 양장피 사오는것도 생각하고 있어요",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100038396.1.6",
          "form": "전 갈비탕요ㅋ",
          "original_form": "전 갈비탕요ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100038396.1.7",
          "form": "으아 갈비탕도 좋죠",
          "original_form": "으아 갈비탕도 좋죠",
          "speaker_id": "2",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100038396.1.8",
          "form": "네 맛나요ㅎ",
          "original_form": "네 맛나요ㅎ",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100038396.1.9",
          "form": "주로 어디서 사드세요?",
          "original_form": "주로 어디서 사드세요?",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100038396.1.10",
          "form": "주변에 맛집이 있는건가요?",
          "original_form": "주변에 맛집이 있는건가요?",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100038396.1.11",
          "form": "인터넷으로 사서요ㅎ",
          "original_form": "인터넷으로 사서요ㅎ",
          "speaker_id": "1",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100038396.1.12",
          "form": "아하 인터넷은 생각도 못했네요",
          "original_form": "아하 인터넷은 생각도 못했네요",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100038396.1.13",
          "form": "그냥 저냥 맛나요",
          "original_form": "그냥 저냥 맛나요",
          "speaker_id": "1",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100038396.1.14",
          "form": "밖에서 사먹는거 보단 아닌가봐요",
          "original_form": "밖에서 사먹는거 보단 아닌가봐요",
          "speaker_id": "2",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100038396.1.15",
          "form": "네 아무래도ㅠ",
          "original_form": "네 아무래도ㅠ",
          "speaker_id": "1",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100038396.1.16",
          "form": "코로나 끝나면 가고싶은 맛집이 참 많아요",
          "original_form": "코로나 끝나면 가고싶은 맛집이 참 많아요",
          "speaker_id": "2",
