      "utterance": [
        {
          "id": "MDRW2100022320.1.1",
          "form": "오오 해피엔딩이네요",
          "original_form": "오오 해피엔딩이네요",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022320.1.2",
          "form": "다 자기 짝이 있긴 있나봐요",
          "original_form": "다 자기 짝이 있긴 있나봐요",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022320.1.3",
          "form": "결혼을 하려면 그래도 어느정도 연애를 하긴 해야해요",
          "original_form": "결혼을 하려면 그래도 어느정도 연애를 하긴 해야해요",
          "speaker_id": "1",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022320.1.4",
          "form": "마자마자요 아예 모르고 하는 건 좀 위험한 거 같아요",
          "original_form": "마자마자요 아예 모르고 하는 건 좀 위험한 거 같아요",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022320.1.5",
          "form": "짧게하고 결혼해서 잘사는사람도 있지먀",
          "original_form": "짧게하고 결혼해서 잘사는사람도 있지먀",
          "speaker_id": "1",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022320.1.6",
          "form": "아닌사람도 많더라구요",
          "original_form": "아닌사람도 많더라구요",
          "speaker_id": "1",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022320.1.7",
          "form": "연애 잘 안하는 사람들은 환상갖기도 쉬우니 ㅎㅎ",
          "original_form": "연애 잘 안하는 사람들은 환상갖기도 쉬우니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022320.1.8",
          "form": "좀 와장창하는 시기가 오더라구요",
          "original_form": "좀 와장창하는 시기가 오더라구요",
          "speaker_id": "2",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022320.1.9",
          "form": "환상이 크던데요 ㅋㅋ",
          "original_form": "환상이 크던데요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022320.1.10",
          "form": "ㅋㅋㅋㅋ현실 자각하는 순간부터 싸움시작",
          "original_form": "ㅋㅋㅋㅋ현실 자각하는 순간부터 싸움시작",
          "speaker_id": "2",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022320.1.11",
          "form": "여친분이랑 좀 싸우시나요",
          "original_form": "여친분이랑 좀 싸우시나요",
          "speaker_id": "2",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022320.1.12",
          "form": "제가 2년차때",
          "original_form": "제가 2년차때",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022320.1.13",
          "form": "엄청싸웠어요",
          "original_form": "엄청싸웠어요",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022320.1.14",
          "form": "오 왜여",
          "original_form": "오 왜여",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022320.1.15",
          "form": "1년차땐 좋아서 그냥 내가 맞춰주자 했던것들이 서로 있었던거 같은데",
          "original_form": "1년차땐 좋아서 그냥 내가 맞춰주자 했던것들이 서로 있었던거 같은데",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022320.1.16",
          "form": "그게 2년차때 서로 터진거죠",
          "original_form": "그게 2년차때 서로 터진거죠",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022320.1.17",
          "form": "오 2년차부터 드러났군요ㅋㅋㅋ",
          "original_form": "오 2년차부터 드러났군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022320.1.18",
          "form": "진짜 헤어질수도 있겠다",
          "original_form": "진짜 헤어질수도 있겠다",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022320.1.19",
          "form": "싶을정도로 싸웠는데",
          "original_form": "싶을정도로 싸웠는데",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022320.1.20",
          "form": "거의 만나면 싸우는수준이라",
          "original_form": "거의 만나면 싸우는수준이라",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022320.1.21",
          "form": "헛ㅋㅋㅋㅋ",
          "original_form": "헛ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022320.1.22",
          "form": "그래도 안 헤어지고 잘 극복해 내셨네요ㅋㅋㅋ",
          "original_form": "그래도 안 헤어지고 잘 극복해 내셨네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022320.1.23",
          "form": "그럼 결혼해서도 잘 극복하겠어여",
          "original_form": "그럼 결혼해서도 잘 극복하겠어여",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022320.1.24",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022320.1.25",
          "form": "전 그전 연애 6년동안",
          "original_form": "전 그전 연애 6년동안",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022320.1.26",
          "form": "한번도 안 싸웠는데",
          "original_form": "한번도 안 싸웠는데",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022320.1.27",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022320.1.28",
          "form": "한번도요?",
          "original_form": "한번도요?",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022320.1.29",
          "form": "그냥 소소한 말싸움정도만",
          "original_form": "그냥 소소한 말싸움정도만",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022320.1.30",
          "form": "싸울일이 없는 연애햇다가",
          "original_form": "싸울일이 없는 연애햇다가",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022320.1.31",
          "form": "또 너무 안 싸우니까",
          "original_form": "또 너무 안 싸우니까",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022320.1.32",
          "form": "구냥 좀 애정이 사그라든고같기도 해요",
          "original_form": "구냥 좀 애정이 사그라든고같기도 해요",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022320.1.33",
          "form": "자연스럽게 헤어지신거에요?",
          "original_form": "자연스럽게 헤어지신거에요?",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022320.1.34",
          "form": "불편한 상황에 맞닥뜨리니까 그냥 이제 그만해야겠다 안녕~하고",
          "original_form": "불편한 상황에 맞닥뜨리니까 그냥 이제 그만해야겠다 안녕~하고",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022320.1.35",
          "form": "네네 그렇게 자연스럽게 헤어졌어요",
          "original_form": "네네 그렇게 자연스럽게 헤어졌어요",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022320.1.36",
          "form": "마지막 데이트도 엄청 즐겁게 하고",
          "original_form": "마지막 데이트도 엄청 즐겁게 하고",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022320.1.37",
          "form": "헛...",
          "original_form": "헛...",
          "speaker_id": "1",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022320.1.38",
          "form": "한 며칠뒤에 이제 그만하자 안녕 하고 헤어져서",
          "original_form": "한 며칠뒤에 이제 그만하자 안녕 하고 헤어져서",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022320.1.39",
          "form": "뭔가 아메리칸스타일인가...",
          "original_form": "뭔가 아메리칸스타일인가...",
          "speaker_id": "1",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022320.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022320.1.41",
          "form": "힘들지 않았어요?",
          "original_form": "힘들지 않았어요?",
          "speaker_id": "1",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022320.1.42",
          "form": "장거리 6년이 쉬운게 아니어서",
          "original_form": "장거리 6년이 쉬운게 아니어서",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022320.1.43",
          "form": "6년인데 그래도",
          "original_form": "6년인데 그래도",
          "speaker_id": "1",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022320.1.44",
          "form": "사실 그정도면 만날만큼 햇다",
          "original_form": "사실 그정도면 만날만큼 햇다",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022320.1.45",
          "form": "둘다 애썼다 이렇게 헤어졌네요",
          "original_form": "둘다 애썼다 이렇게 헤어졌네요",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022320.1.46",
          "form": "장거리연애에 지친건가요?",
          "original_form": "장거리연애에 지친건가요?",
          "speaker_id": "1",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022320.1.47",
          "form": "둘다 마음이 식어서 그런지 좀 허전햇어도 너무 슬픈것도아니엇고",
          "original_form": "둘다 마음이 식어서 그런지 좀 허전햇어도 너무 슬픈것도아니엇고",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022320.1.48",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022320.1.49",
          "form": "네 장거리연애도 지쳣고 결혼할 상황도 아닌거 같고 ㅋㅋ",
          "original_form": "네 장거리연애도 지쳣고 결혼할 상황도 아닌거 같고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022320.1.50",
          "form": "금융치료 한번에 잘 잊혀졌어요",
          "original_form": "금융치료 한번에 잘 잊혀졌어요",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022320.1.51",
          "form": "ㅋㅋㅋㅋ 헤어지고 항공권을 바로 주문해서",
          "original_form": "ㅋㅋㅋㅋ 헤어지고 항공권을 바로 주문해서",
          "speaker_id": "2",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022320.1.52",
          "form": "금융치료요?",
          "original_form": "금융치료요?",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022320.1.53",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022320.1.54",
          "form": "여행 다녀오고 깔끔하게 잘 잊었다! 이렇게",
          "original_form": "여행 다녀오고 깔끔하게 잘 잊었다! 이렇게",
          "speaker_id": "2",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022320.1.55",
          "form": "ㅋㅋㅋㅋ그런 이별도 되더라구요?",
          "original_form": "ㅋㅋㅋㅋ그런 이별도 되더라구요?",
          "speaker_id": "2",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022320.1.56",
          "form": "눈물이 진짜 안났어요",
          "original_form": "눈물이 진짜 안났어요",
          "speaker_id": "2",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022320.1.57",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022320.1.58",
          "form": "자만추만의 이별방식인가",
          "original_form": "자만추만의 이별방식인가",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022320.1.59",
          "form": "아니 그건 아니에요 ㅋㅋㅋㅋ",
          "original_form": "아니 그건 아니에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022320.1.60",
          "form": "5년은 좀 썸남처럼 살았고 6년은 연애한거라",
          "original_form": "5년은 좀 썸남처럼 살았고 6년은 연애한거라",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022320.1.61",
          "form": "제 20대의 전부였긴 했는데",
          "original_form": "제 20대의 전부였긴 했는데",
          "speaker_id": "2",
          "time": "20210829 12:46"
