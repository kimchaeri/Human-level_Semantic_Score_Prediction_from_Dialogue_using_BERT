      "utterance": [
        {
          "id": "MDRW2100042469.1.1",
          "form": "예능 좋아하는거 있으신가요??",
          "original_form": "예능 좋아하는거 있으신가요??",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042469.1.2",
          "form": "저는 요즘 대탈출이랑 심야괴담회 보고있어요",
          "original_form": "저는 요즘 대탈출이랑 심야괴담회 보고있어요",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100042469.1.3",
          "form": "어떤거 보시나요?",
          "original_form": "어떤거 보시나요?",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100042469.1.4",
          "form": "저는 런닝맨 대탈출이요!!",
          "original_form": "저는 런닝맨 대탈출이요!!",
          "speaker_id": "2",
          "time": "20210907 17:34"
        },
        {
          "id": "MDRW2100042469.1.5",
          "form": "런닝맨도 재밌죠ㅋㅋ",
          "original_form": "런닝맨도 재밌죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:35"
        },
        {
          "id": "MDRW2100042469.1.6",
          "form": "대탈출 이번주편도 보셨나요?",
          "original_form": "대탈출 이번주편도 보셨나요?",
          "speaker_id": "1",
          "time": "20210907 17:36"
        },
        {
          "id": "MDRW2100042469.1.7",
          "form": "이번주 지났죠 제일 최근편이요!",
          "original_form": "이번주 지났죠 제일 최근편이요!",
          "speaker_id": "1",
          "time": "20210907 17:36"
        },
        {
          "id": "MDRW2100042469.1.8",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210907 17:37"
        },
        {
          "id": "MDRW2100042469.1.9",
          "form": "크레이지 하우스 말씀하시는건가요??",
          "original_form": "크레이지 하우스 말씀하시는건가요??",
          "speaker_id": "2",
          "time": "20210907 17:37"
        },
        {
          "id": "MDRW2100042469.1.10",
          "form": "그것도 재밌었고 제일 최근에 한 폭탄해제하는 편이요!",
          "original_form": "그것도 재밌었고 제일 최근에 한 폭탄해제하는 편이요!",
          "speaker_id": "1",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100042469.1.11",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100042469.1.12",
          "form": "봤어요!",
          "original_form": "봤어요!",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100042469.1.13",
          "form": "피오 넘 멋있어...요",
          "original_form": "피오 넘 멋있어...요",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100042469.1.14",
          "form": "피오 요즘 잘하더라구요!",
          "original_form": "피오 요즘 잘하더라구요!",
          "speaker_id": "1",
          "time": "20210907 17:41"
        },
        {
          "id": "MDRW2100042469.1.15",
          "form": "저는 김종민 아래로 떨어지는거 너무 웃겼어요...ㅋㅋ",
          "original_form": "저는 김종민 아래로 떨어지는거 너무 웃겼어요...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100042469.1.16",
          "form": "전 너무 놀랬어요....ㅋㅋㅋㅋ",
          "original_form": "전 너무 놀랬어요....ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:50"
        },
        {
          "id": "MDRW2100042469.1.17",
          "form": "웃기기도 했는데ㅋㅋㅋ",
          "original_form": "웃기기도 했는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:50"
        },
        {
          "id": "MDRW2100042469.1.18",
          "form": "진짜 저였으면 눈물 찔끔 났을 것 같아요",
          "original_form": "진짜 저였으면 눈물 찔끔 났을 것 같아요",
          "speaker_id": "1",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100042469.1.19",
          "form": "진짜 훅 하고 떨어지더라구요 ..",
          "original_form": "진짜 훅 하고 떨어지더라구요 ..",
          "speaker_id": "1",
          "time": "20210907 18:00"
        },
        {
          "id": "MDRW2100042469.1.20",
          "form": "정말 일자로ㅋㅋㅋ",
          "original_form": "정말 일자로ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100042469.1.21",
          "form": "그 와중에 김종민 기절한척 장난치고ㅋㅋㅋ",
          "original_form": "그 와중에 김종민 기절한척 장난치고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100042469.1.22",
          "form": "맞아요 그래서 진지해지지 않고 웃겼어요ㅋㅋㅋ",
          "original_form": "맞아요 그래서 진지해지지 않고 웃겼어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100042469.1.23",
          "form": "김종민 진짜 웃긴것같아요 ㅠㅠ",
          "original_form": "김종민 진짜 웃긴것같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100042469.1.24",
          "form": "10년 넘어서 주말예능 고정인 이유가ㅋㅋ",
          "original_form": "10년 넘어서 주말예능 고정인 이유가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100042469.1.25",
          "form": "대탈출 보면 진짜 저도 같이 한번 체험 해보고 싶더라구요ㅜ",
          "original_form": "대탈출 보면 진짜 저도 같이 한번 체험 해보고 싶더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100042469.1.26",
          "form": "진짜 꼭 체험가고싶어요 세트장 진짜 스케일이 대단해요",
          "original_form": "진짜 꼭 체험가고싶어요 세트장 진짜 스케일이 대단해요",
          "speaker_id": "1",
          "time": "20210907 18:42"
        },
        {
          "id": "MDRW2100042469.1.27",
          "form": "진짜ㅜ 스포 당했지만",
          "original_form": "진짜ㅜ 스포 당했지만",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100042469.1.28",
          "form": "가보고싶어요ㅜ",
          "original_form": "가보고싶어요ㅜ",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100042469.1.29",
          "form": "1회용인게 너무 아쉬워요ㅜ",
          "original_form": "1회용인게 너무 아쉬워요ㅜ",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100042469.1.30",
          "form": "그러니까요 ㅠㅠ 체험으로 제작비좀 버시지..",
          "original_form": "그러니까요 ㅠㅠ 체험으로 제작비좀 버시지..",
          "speaker_id": "1",
          "time": "20210907 19:22"
        }
      ]
