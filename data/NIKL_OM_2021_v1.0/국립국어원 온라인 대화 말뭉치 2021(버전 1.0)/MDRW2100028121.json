      "utterance": [
        {
          "id": "MDRW2100028121.1.1",
          "form": "허얼 멈멍이 자랑해줘요",
          "original_form": "허얼 멈멍이 자랑해줘요",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028121.1.2",
          "form": "우리강아지가 얼마나 귀여운지 온세상사람이 알아야함",
          "original_form": "우리강아지가 얼마나 귀여운지 온세상사람이 알아야함",
          "speaker_id": "1",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028121.1.3",
          "form": "근데 뭘 자랑하지",
          "original_form": "근데 뭘 자랑하지",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028121.1.4",
          "form": "발바닥 꼬순내가 귀여워요",
          "original_form": "발바닥 꼬순내가 귀여워요",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028121.1.5",
          "form": "강아지 발냄새 맡아봤나요",
          "original_form": "강아지 발냄새 맡아봤나요",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028121.1.6",
          "form": "헐 나 강아지 사랑하는데 꼬순내는 안좋아해",
          "original_form": "헐 나 강아지 사랑하는데 꼬순내는 안좋아해",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028121.1.7",
          "form": "SAD",
          "original_form": "SAD",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028121.1.8",
          "form": "슬퍼요",
          "original_form": "슬퍼요",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028121.1.9",
          "form": "그치만 저희 강강쥐도 싫어하는듯",
          "original_form": "그치만 저희 강강쥐도 싫어하는듯",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028121.1.10",
          "form": "발잡고 코 가까이 가져가서 킁킁하면 으르르릉해",
          "original_form": "발잡고 코 가까이 가져가서 킁킁하면 으르르릉해",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028121.1.11",
          "form": "까칠하고 귀여운 친구네",
          "original_form": "까칠하고 귀여운 친구네",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028121.1.12",
          "form": "난 친구 강아지가 너무 귀여워",
          "original_form": "난 친구 강아지가 너무 귀여워",
          "speaker_id": "2",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028121.1.13",
          "form": "포메라니안이야",
          "original_form": "포메라니안이야",
          "speaker_id": "2",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028121.1.14",
          "form": "포메라니안의 피가 절반 흐르긴하지",
          "original_form": "포메라니안의 피가 절반 흐르긴하지",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028121.1.15",
          "form": "나머지 절반은 뭐게용",
          "original_form": "나머지 절반은 뭐게용",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028121.1.16",
          "form": "푸들인가",
          "original_form": "푸들인가",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028121.1.17",
          "form": "땡 아닙니다 곧은 털이예요",
          "original_form": "땡 아닙니다 곧은 털이예요",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028121.1.18",
          "form": "털이 짧고...어 장모종도 있긴하다",
          "original_form": "털이 짧고...어 장모종도 있긴하다",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028121.1.19",
          "form": "흐음.............",
          "original_form": "흐음.............",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028121.1.20",
          "form": "띵킹중",
          "original_form": "띵킹중",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028121.1.21",
          "form": "정답! 모르겠다!",
          "original_form": "정답! 모르겠다!",
          "speaker_id": "2",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028121.1.22",
          "form": "퇴근했다!",
          "original_form": "퇴근했다!",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028121.1.23",
          "form": "정답 공개 두구두구두구",
          "original_form": "정답 공개 두구두구두구",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028121.1.24",
          "form": "치와와입니다~~~!",
          "original_form": "치와와입니다~~~!",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028121.1.25",
          "form": "치와와 포메라니안 합쳐서 포메치와~!",
          "original_form": "치와와 포메라니안 합쳐서 포메치와~!",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028121.1.26",
          "form": "치와와 생각도 했는데 아깝다 흐아악",
