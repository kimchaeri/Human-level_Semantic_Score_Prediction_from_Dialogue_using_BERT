      "utterance": [
        {
          "id": "MDRW2100003826.1.1",
          "form": "정확히는 신혼초에",
          "original_form": "정확히는 신혼초에",
          "speaker_id": "2",
          "time": "20210821 12:33"
        },
        {
          "id": "MDRW2100003826.1.2",
          "form": "아윽 신혼초ㅠ",
          "original_form": "아윽 신혼초ㅠ",
          "speaker_id": "1",
          "time": "20210821 12:33"
        },
        {
          "id": "MDRW2100003826.1.3",
          "form": "부인네 집안에서 남친네 집 무작정 찾아와서",
          "original_form": "부인네 집안에서 남친네 집 무작정 찾아와서",
          "speaker_id": "2",
          "time": "20210821 12:33"
        },
        {
          "id": "MDRW2100003826.1.4",
          "form": "헐 맙소사",
          "original_form": "헐 맙소사",
          "speaker_id": "1",
          "time": "20210821 12:33"
        },
        {
          "id": "MDRW2100003826.1.5",
          "form": "부모님 다 있으신데서 난리쳤대요",
          "original_form": "부모님 다 있으신데서 난리쳤대요",
          "speaker_id": "2",
          "time": "20210821 12:33"
        },
        {
          "id": "MDRW2100003826.1.6",
          "form": "좀 긴데",
          "original_form": "좀 긴데",
          "speaker_id": "2",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003826.1.7",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003826.1.8",
          "form": "부인쪽에서 남친네 집안 기 잡겠다고 그런거예요",
          "original_form": "부인쪽에서 남친네 집안 기 잡겠다고 그런거예요",
          "speaker_id": "2",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003826.1.9",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "1",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003826.1.10",
          "form": "부인쪽 아버지가 강원도에서 유명한 조폭인가 깡패래요",
          "original_form": "부인쪽 아버지가 강원도에서 유명한 조폭인가 깡패래요",
          "speaker_id": "2",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003826.1.11",
          "form": "헐!!! 맙소사",
          "original_form": "헐!!! 맙소사",
          "speaker_id": "1",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003826.1.12",
          "form": "아무튼 그래서 그 일로 어머니 쓰러지셨다가 암까지 얻으셔서",
          "original_form": "아무튼 그래서 그 일로 어머니 쓰러지셨다가 암까지 얻으셔서",
          "speaker_id": "2",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003826.1.13",
          "form": "서로 남남처럼 살다가",
          "original_form": "서로 남남처럼 살다가",
          "speaker_id": "2",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003826.1.14",
          "form": "결혼 전에는 그런 집안인지 모르셨겠죠?",
          "original_form": "결혼 전에는 그런 집안인지 모르셨겠죠?",
          "speaker_id": "1",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003826.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003826.1.16",
          "form": "속았대요ㅋㅋㅋ",
          "original_form": "속았대요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003826.1.17",
          "form": "심지어 살인 전과도 있는데",
          "original_form": "심지어 살인 전과도 있는데",
          "speaker_id": "2",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003826.1.18",
          "form": "입 싹 씻은거죠",
          "original_form": "입 싹 씻은거죠",
          "speaker_id": "2",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003826.1.19",
          "form": "헉 살인",
          "original_form": "헉 살인",
          "speaker_id": "1",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003826.1.20",
          "form": "부인 어머니쪽은 무당쪽이래요",
          "original_form": "부인 어머니쪽은 무당쪽이래요",
          "speaker_id": "2",
          "time": "20210821 12:35"
        }
      ]
