      "utterance": [
        {
          "id": "MDRW2100042490.1.1",
          "form": "비 오는 날 긴 옷은 힘들죠",
          "original_form": "비 오는 날 긴 옷은 힘들죠",
          "speaker_id": "1",
          "time": "20210907 08:29"
        },
        {
          "id": "MDRW2100042490.1.2",
          "form": "맞아요 영국 가보셨어요?",
          "original_form": "맞아요 영국 가보셨어요?",
          "speaker_id": "2",
          "time": "20210907 08:29"
        },
        {
          "id": "MDRW2100042490.1.3",
          "form": "네 영국 가봤어요",
          "original_form": "네 영국 가봤어요",
          "speaker_id": "1",
          "time": "20210907 08:29"
        },
        {
          "id": "MDRW2100042490.1.4",
          "form": "비 많이 오던가요?",
          "original_form": "비 많이 오던가요?",
          "speaker_id": "2",
          "time": "20210907 08:30"
        },
        {
          "id": "MDRW2100042490.1.5",
          "form": "제가 갔던 때는 그렇게 많이 내리진 않았던 것 같아요",
          "original_form": "제가 갔던 때는 그렇게 많이 내리진 않았던 것 같아요",
          "speaker_id": "1",
          "time": "20210907 08:30"
        },
        {
          "id": "MDRW2100042490.1.6",
          "form": "8월쯤 갔는데",
          "original_form": "8월쯤 갔는데",
          "speaker_id": "1",
          "time": "20210907 08:30"
        },
        {
          "id": "MDRW2100042490.1.7",
          "form": "하루이틀? 비왔어요",
          "original_form": "하루이틀? 비왔어요",
          "speaker_id": "1",
          "time": "20210907 08:30"
        },
        {
          "id": "MDRW2100042490.1.8",
          "form": "아 영국만 가신거예요?",
          "original_form": "아 영국만 가신거예요?",
          "speaker_id": "2",
          "time": "20210907 08:31"
        },
        {
          "id": "MDRW2100042490.1.9",
          "form": "아 서유럽쪽 쭉 돌았어요",
          "original_form": "아 서유럽쪽 쭉 돌았어요",
          "speaker_id": "1",
          "time": "20210907 08:31"
        },
        {
          "id": "MDRW2100042490.1.10",
          "form": "런던 인 로마 아웃으로",
          "original_form": "런던 인 로마 아웃으로",
          "speaker_id": "1",
          "time": "20210907 08:32"
        },
        {
          "id": "MDRW2100042490.1.11",
          "form": "아 자유로 가신거죠?",
          "original_form": "아 자유로 가신거죠?",
          "speaker_id": "2",
          "time": "20210907 08:32"
        },
        {
          "id": "MDRW2100042490.1.12",
          "form": "제일 좋았던 곳이 어디에요?",
          "original_form": "제일 좋았던 곳이 어디에요?",
          "speaker_id": "2",
          "time": "20210907 08:32"
        },
        {
          "id": "MDRW2100042490.1.13",
          "form": "네 친구랑 자유여행으로요",
          "original_form": "네 친구랑 자유여행으로요",
          "speaker_id": "1",
          "time": "20210907 08:32"
        },
        {
          "id": "MDRW2100042490.1.14",
          "form": "파리 올림픽때 가고싶네요",
          "original_form": "파리 올림픽때 가고싶네요",
          "speaker_id": "2",
          "time": "20210907 08:34"
        },
        {
          "id": "MDRW2100042490.1.15",
          "form": "파리도 좋았고",
          "original_form": "파리도 좋았고",
          "speaker_id": "1",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042490.1.16",
          "form": "편안하게 다닌 건 영국이요",
          "original_form": "편안하게 다닌 건 영국이요",
          "speaker_id": "1",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042490.1.17",
          "form": "소매치기 걱정이 없어서",
          "original_form": "소매치기 걱정이 없어서",
          "speaker_id": "1",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042490.1.18",
          "form": "다른가요?",
          "original_form": "다른가요?",
          "speaker_id": "2",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042490.1.19",
          "form": "파리랑 이탈리아는 소매치기 조심하라고 하도 얘기를 들어서요",
          "original_form": "파리랑 이탈리아는 소매치기 조심하라고 하도 얘기를 들어서요",
          "speaker_id": "1",
          "time": "20210907 08:42"
        },
        {
          "id": "MDRW2100042490.1.20",
          "form": "치안이 같은 유럽인데도 차이가 많나봐요",
          "original_form": "치안이 같은 유럽인데도 차이가 많나봐요",
          "speaker_id": "2",
          "time": "20210907 08:42"
        }
      ]
