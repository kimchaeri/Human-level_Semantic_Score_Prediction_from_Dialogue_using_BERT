      "utterance": [
        {
          "id": "MDRW2100034374.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100034374.1.2",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100034374.1.3",
          "form": "아직이요. 다음달에 예약 해놨어요. 님은요?",
          "original_form": "아직이요. 다음달에 예약 해놨어요. 님은요?",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100034374.1.4",
          "form": "전오늘 2차맞았어요",
          "original_form": "전오늘 2차맞았어요",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100034374.1.5",
          "form": "벌써 2차까지! 부럽네요",
          "original_form": "벌써 2차까지! 부럽네요",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100034374.1.6",
          "form": "슬슬 팔뚝이 아프네요 담달이면 곧 맞겠네요",
          "original_form": "슬슬 팔뚝이 아프네요 담달이면 곧 맞겠네요",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100034374.1.7",
          "form": "팔 다들 아프다 그러더라구요",
          "original_form": "팔 다들 아프다 그러더라구요",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100034374.1.8",
          "form": "저는 불안해서 발빠르게 예약해가지구",
          "original_form": "저는 불안해서 발빠르게 예약해가지구",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100034374.1.9",
          "form": "남들보다 빨리 맞았어요ㅎㅎ",
          "original_form": "남들보다 빨리 맞았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100034374.1.10",
          "form": "머맞으시나요 저는 Az 맞았어요",
          "original_form": "머맞으시나요 저는 Az 맞았어요",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100034374.1.11",
          "form": "화이자라고 뜨더라구요",
          "original_form": "화이자라고 뜨더라구요",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100034374.1.12",
          "form": "저는 2차 화이자로 교차접종 맞았어요",
          "original_form": "저는 2차 화이자로 교차접종 맞았어요",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100034374.1.13",
          "form": "교차접종이 좋다고 하더라구요",
          "original_form": "교차접종이 좋다고 하더라구요",
          "speaker_id": "2",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100034374.1.14",
          "form": "교차가 예방 효과가 높대요 추천드려요",
          "original_form": "교차가 예방 효과가 높대요 추천드려요",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100034374.1.15",
          "form": "사람들이 AZ회피하더라구요ㅜ",
          "original_form": "사람들이 AZ회피하더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100034374.1.16",
          "form": "병원 가서 물어봐야겠어요",
          "original_form": "병원 가서 물어봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100034374.1.17",
          "form": "병원에 가니까 좀 한산해졌더라구요 제생각에",
          "original_form": "병원에 가니까 좀 한산해졌더라구요 제생각에",
          "speaker_id": "1",
          "time": "20210828 01:21"
        }
      ]
