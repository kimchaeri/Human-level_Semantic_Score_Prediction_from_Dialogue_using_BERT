      "utterance": [
        {
          "id": "MDRW2100008239.1.1",
          "form": "키우는 애긔 있으세요? ㅎ",
          "original_form": "키우는 애긔 있으세요? ㅎ",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008239.1.2",
          "form": "아녀 저는 없어요 키우는 동물 있으세요?",
          "original_form": "아녀 저는 없어요 키우는 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008239.1.3",
          "form": "저도 없엉ㆍ",
          "original_form": "저도 없엉ㆍ",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008239.1.4",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008239.1.5",
          "form": "키울 예정이나 키우고 싶은? 동물 있으세요?",
          "original_form": "키울 예정이나 키우고 싶은? 동물 있으세요?",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008239.1.6",
          "form": "나중에 강아지 키우고 싶은데 키울수 있을지...",
          "original_form": "나중에 강아지 키우고 싶은데 키울수 있을지...",
          "speaker_id": "1",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100008239.1.7",
          "form": "저도 한때 실버푸들이 너무 귀여워서 사진도 찾아보고 막 그랬답니다  ㅋㅋ",
          "original_form": "저도 한때 실버푸들이 너무 귀여워서 사진도 찾아보고 막 그랬답니다  ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008239.1.8",
          "form": "지금은 자신없네요ㅜ",
          "original_form": "지금은 자신없네요ㅜ",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008239.1.9",
          "form": "맞아요 저도 지금은 책임못질거같구..",
          "original_form": "맞아요 저도 지금은 책임못질거같구..",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008239.1.10",
          "form": "대신 웹툰봐요 랜선집사? 랄까",
          "original_form": "대신 웹툰봐요 랜선집사? 랄까",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008239.1.11",
          "form": "아 저는 유투브로 보고 힐링해요ㅎㅎ",
          "original_form": "아 저는 유투브로 보고 힐링해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008239.1.12",
          "form": "저는 네이버 웹툰이요 완결전 무료연재일때 봐야한다는 소시민 마인드로 ㅋㅋ",
          "original_form": "저는 네이버 웹툰이요 완결전 무료연재일때 봐야한다는 소시민 마인드로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008239.1.13",
          "form": "보기만 하는게 부담스럽지않고 좋아요ㅎㅎ",
          "original_form": "보기만 하는게 부담스럽지않고 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008239.1.14",
          "form": "아 동물은 아닌데",
          "original_form": "아 동물은 아닌데",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008239.1.15",
          "form": "마리모? 좀 키워보고 싶더라고요",
          "original_form": "마리모? 좀 키워보고 싶더라고요",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008239.1.16",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008239.1.17",
          "form": "아 네 은근 키우기 어렵다던데ㅎㅎ",
          "original_form": "아 네 은근 키우기 어렵다던데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008239.1.18",
          "form": "뭔가 보고만있어도 힐링될거같은데",
          "original_form": "뭔가 보고만있어도 힐링될거같은데",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008239.1.19",
          "form": "똥손이라 시도를 못하고있어요",
          "original_form": "똥손이라 시도를 못하고있어요",
          "speaker_id": "2",
          "time": "20210822 15:31"
        }
      ]
