      "utterance": [
        {
          "id": "MDRW2100015375.1.1",
          "form": "동아리나 동호회 가신적 있으세요?",
          "original_form": "동아리나 동호회 가신적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015375.1.2",
          "form": "간적없어요ㅜㅜ 안간거후회해요..ㅜㅜ",
          "original_form": "간적없어요ㅜㅜ 안간거후회해요..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015375.1.3",
          "form": "어디든적있어용?",
          "original_form": "어디든적있어용?",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015375.1.4",
          "form": "약간 그렇죠? 저도 그래요",
          "original_form": "약간 그렇죠? 저도 그래요",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015375.1.5",
          "form": "아 저는 등산동호회입니다",
          "original_form": "아 저는 등산동호회입니다",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015375.1.6",
          "form": "와 등산동호회..",
          "original_form": "와 등산동호회..",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015375.1.7",
          "form": "세상제일싫은등산쓰..",
          "original_form": "세상제일싫은등산쓰..",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015375.1.8",
          "form": "가입하고 코로나라 간 적 없어요",
          "original_form": "가입하고 코로나라 간 적 없어요",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015375.1.9",
          "form": "사실 상품권 때뭄에 가입했어요",
          "original_form": "사실 상품권 때뭄에 가입했어요",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015375.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015375.1.11",
          "form": "상품권ㅋㅋㅋㅋㅋ",
          "original_form": "상품권ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015375.1.12",
          "form": "재테크용입니다",
          "original_form": "재테크용입니다",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015375.1.13",
          "form": "역시뭔가걸려야...",
          "original_form": "역시뭔가걸려야...",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015375.1.14",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015375.1.15",
          "form": "등산은 너무 힘들거같아요...",
          "original_form": "등산은 너무 힘들거같아요...",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015375.1.16",
          "form": "근데사실 제가 낸 회비 그대로에요",
          "original_form": "근데사실 제가 낸 회비 그대로에요",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015375.1.17",
          "form": "우리집 뒤에 백산은 동산이라 갈만해요",
          "original_form": "우리집 뒤에 백산은 동산이라 갈만해요",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015375.1.18",
          "form": "같이갈래요?",
          "original_form": "같이갈래요?",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015375.1.19",
          "form": "오 그정도는좋아요",
          "original_form": "오 그정도는좋아요",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015375.1.20",
          "form": "날씨좋아지면가요",
          "original_form": "날씨좋아지면가요",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015375.1.21",
          "form": "너무좋아요",
          "original_form": "너무좋아요",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015375.1.22",
          "form": "가을에 우리 어디 많이가요 약속 진짜ㅠㅠ오지치즈아니고ㅠㅠ",
          "original_form": "가을에 우리 어디 많이가요 약속 진짜ㅠㅠ오지치즈아니고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015375.1.23",
          "form": "ㅠㅡㅠ진짜진짜..",
          "original_form": "ㅠㅡㅠ{emoji:♡}진짜진짜..{emoji:♡}",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015375.1.24",
          "form": "동호회사람들은좋나요",
          "original_form": "동호회사람들은좋나요",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015375.1.25",
          "form": "안나가봐서모르나요..",
          "original_form": "안나가봐서모르나요..",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015375.1.26",
          "form": "맞아요 하지만 멤버들이 다좋아요",
          "original_form": "맞아요 하지만 멤버들이 다좋아요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        }
      ]
    }
