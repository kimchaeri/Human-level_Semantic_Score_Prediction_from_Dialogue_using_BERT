      "utterance": [
        {
          "id": "MDRW2100043081.1.1",
          "form": "배달음식 먹고싶다",
          "original_form": "배달음식 먹고싶다",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043081.1.2",
          "form": "귀찮지...밥 매번하는거",
          "original_form": "귀찮지...밥 매번하는거",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043081.1.3",
          "form": "그니까...이렇게 비오는날에는 더 시켜먹고싶어",
          "original_form": "그니까...이렇게 비오는날에는 더 시켜먹고싶어",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043081.1.4",
          "form": "뭐먹고싶은데?",
          "original_form": "뭐먹고싶은데?",
          "speaker_id": "2",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043081.1.5",
          "form": "나는 새우찜...",
          "original_form": "나는 새우찜...",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043081.1.6",
          "form": "꾸준하네 새우찜쳐돌이",
          "original_form": "꾸준하네 새우찜쳐돌이",
          "speaker_id": "2",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043081.1.7",
          "form": "찜류먹으면 한끼 든든하게 너무 잘먹은거 같아서 좋아",
          "original_form": "찜류먹으면 한끼 든든하게 너무 잘먹은거 같아서 좋아",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043081.1.8",
          "form": "좀 그런거 있는거 같아 보쌈 찜류 이런게 든든하긴해",
          "original_form": "좀 그런거 있는거 같아 보쌈 찜류 이런게 든든하긴해",
          "speaker_id": "2",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100043081.1.9",
          "form": "나는 치킨먹고싶다",
          "original_form": "나는 치킨먹고싶다",
          "speaker_id": "2",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100043081.1.10",
          "form": "치킨도 좋지...",
          "original_form": "치킨도 좋지...",
          "speaker_id": "1",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100043081.1.11",
          "form": "뿌링클 좋아하잖아 너",
          "original_form": "뿌링클 좋아하잖아 너",
          "speaker_id": "1",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100043081.1.12",
          "form": "응 뿌링클 먹고싶네",
          "original_form": "응 뿌링클 먹고싶네",
          "speaker_id": "2",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100043081.1.13",
          "form": "매번 시켜먹기엔 돈도...배달료도 부담스러워",
          "original_form": "매번 시켜먹기엔 돈도...배달료도 부담스러워",
          "speaker_id": "1",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100043081.1.14",
          "form": "그니까....시켰다하면 이만원은 훌쩍넘어가니까",
          "original_form": "그니까....시켰다하면 이만원은 훌쩍넘어가니까",
          "speaker_id": "2",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100043081.1.15",
          "form": "너무 부담이지",
          "original_form": "너무 부담이지",
          "speaker_id": "2",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100043081.1.16",
          "form": "양도 많고...",
          "original_form": "양도 많고...",
          "speaker_id": "2",
