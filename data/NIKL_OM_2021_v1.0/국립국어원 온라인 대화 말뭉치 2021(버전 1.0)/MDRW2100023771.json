      },
      "utterance": [
        {
          "id": "MDRW2100023771.1.1",
          "form": "할 수 있을 때 해볼라고",
          "original_form": "할 수 있을 때 해볼라고",
          "speaker_id": "2",
          "time": "20210830 17:01"
        },
        {
          "id": "MDRW2100023771.1.2",
          "form": "나도 탈색을 해보고 싶었는데",
          "original_form": "나도 탈색을 해보고 싶었는데",
          "speaker_id": "1",
          "time": "20210830 17:01"
        },
        {
          "id": "MDRW2100023771.1.3",
          "form": "한 번 하지 않았어?",
          "original_form": "한 번 하지 않았어?",
          "speaker_id": "2",
          "time": "20210830 17:01"
        },
        {
          "id": "MDRW2100023771.1.4",
          "form": "아니야 밝은 색으로 염색한 적은 있는데 탈색을 한 적은 한 번도 없었어",
          "original_form": "아니야 밝은 색으로 염색한 적은 있는데 탈색을 한 적은 한 번도 없었어",
          "speaker_id": "1",
          "time": "20210830 17:01"
        },
        {
          "id": "MDRW2100023771.1.5",
          "form": "난 밝은 색은 거의 다 탈색해야 되는줄 알았는데",
          "original_form": "난 밝은 색은 거의 다 탈색해야 되는줄 알았는데",
          "speaker_id": "2",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100023771.1.6",
          "form": "이게 탈색을 해야 나오는 색깔 범주가 있고 탈색 안하고 좀 밝게 나오는 색깔 범주가 있더라고",
          "original_form": "이게 탈색을 해야 나오는 색깔 범주가 있고 탈색 안하고 좀 밝게 나오는 색깔 범주가 있더라고",
          "speaker_id": "1",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100023771.1.7",
          "form": "아 그런게 있구나",
          "original_form": "아 그런게 있구나",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023771.1.8",
          "form": "탈색이 부담스러우면 좀 밝게 나오는 컬러로 염색하는 것도 가능해",
          "original_form": "탈색이 부담스러우면 좀 밝게 나오는 컬러로 염색하는 것도 가능해",
          "speaker_id": "1",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023771.1.9",
          "form": "예를 들면?",
          "original_form": "예를 들면?",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023771.1.10",
          "form": "처음에 오렌지 브라운 염색했었는데",
          "original_form": "처음에 오렌지 브라운 염색했었는데",
          "speaker_id": "1",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023771.1.11",
          "form": "나중에 엄청 밝아졌어",
          "original_form": "나중에 엄청 밝아졌어",
          "speaker_id": "1",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023771.1.12",
          "form": "나도 예전에 비슷한 색 했던거 같은데",
          "original_form": "나도 예전에 비슷한 색 했던거 같은데",
          "speaker_id": "2",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100023771.1.13",
          "form": "그랬었나?",
          "original_form": "그랬었나?",
          "speaker_id": "1",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100023771.1.14",
          "form": "어 친구랑 같이",
          "original_form": "어 친구랑 같이",
          "speaker_id": "2",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100023771.1.15",
          "form": "아 맞아 친구랑 같은 데 가서 했다고 얘기했던 거 같아",
          "original_form": "아 맞아 친구랑 같은 데 가서 했다고 얘기했던 거 같아",
          "speaker_id": "1",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100023771.1.16",
          "form": "그때 갈색 비슷한 색이었던거 같은데",
          "original_form": "그때 갈색 비슷한 색이었던거 같은데",
          "speaker_id": "2",
          "time": "20210830 17:06"
        },
        {
          "id": "MDRW2100023771.1.17",
          "form": "응 그 색깔도 예뻤던 거 같은데",
          "original_form": "응 그 색깔도 예뻤던 거 같은데",
          "speaker_id": "1",
          "time": "20210830 17:06"
        },
        {
          "id": "MDRW2100023771.1.18",
          "form": "그니까 괜찮았던거 같아",
          "original_form": "그니까 괜찮았던거 같아",
          "speaker_id": "2",
          "time": "20210830 17:07"
        },
        {
          "id": "MDRW2100023771.1.19",
          "form": "나도 탈색을 해보고 싶었었는데 머리카락이 너무 약해서 불가능",
          "original_form": "나도 탈색을 해보고 싶었었는데 머리카락이 너무 약해서 불가능",
          "speaker_id": "1",
          "time": "20210830 17:07"
        }
