      "utterance": [
        {
          "id": "MDRW2100027183.1.1",
          "form": "임플란트 시술 하시고 양파까지드시다니 대단쓰!",
          "original_form": "임플란트 시술 하시고 양파까지드시다니 대단쓰!",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027183.1.2",
          "form": "치과 진료하시고 어디 식당가실건가요",
          "original_form": "치과 진료하시고 어디 식당가실건가요",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027183.1.3",
          "form": "양파절임은 필수쥬ㅎ",
          "original_form": "양파절임은 필수쥬ㅎ",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027183.1.4",
          "form": "집 가서 회나 시킬까 했는데",
          "original_form": "집 가서 회나 시킬까 했는데",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027183.1.5",
          "form": "전에 시켰던 곳 전어 가격이 50프로가 뛰었네요ㅜ",
          "original_form": "전에 시켰던 곳 전어 가격이 50프로가 뛰었네요ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027183.1.6",
          "form": "캬 회에 소주인가요?",
          "original_form": "캬 회에 소주인가요?",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027183.1.7",
          "form": "음주 언제부터 되냐고 물었더니",
          "original_form": "음주 언제부터 되냐고 물었더니",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027183.1.8",
          "form": "고주망태 보는 눈으로 일주일간 자제하래요ㅎㅎ",
          "original_form": "고주망태 보는 눈으로 일주일간 자제하래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027183.1.9",
          "form": "아 오늘은 안되세요",
          "original_form": "아 오늘은 안되세요",
          "speaker_id": "2",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027183.1.10",
          "form": "접종하셔서",
          "original_form": "접종하셔서",
          "speaker_id": "2",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027183.1.11",
          "form": "큰일납니다",
          "original_form": "큰일납니다",
          "speaker_id": "2",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027183.1.12",
          "form": "인터넷 뒤지니 이틀? 얘기가 주이가 한데",
          "original_form": "인터넷 뒤지니 이틀? 얘기가 주이가 한데",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027183.1.13",
          "form": "저는 죽다살아서",
          "original_form": "저는 죽다살아서",
          "speaker_id": "2",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027183.1.14",
          "form": "얀센맞앗거든요",
          "original_form": "얀센맞앗거든요",
          "speaker_id": "2",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027183.1.15",
          "form": "오늘 일찍 맞았으니 낼 밤은 괜찮지 않을까요",
          "original_form": "오늘 일찍 맞았으니 낼 밤은 괜찮지 않을까요",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027183.1.16",
          "form": "헉ㅜ",
          "original_form": "헉ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027183.1.17",
          "form": "사람마다 다르니까",
          "original_form": "사람마다 다르니까",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027183.1.18",
          "form": "맞고 많이 앓으셨군요",
          "original_form": "맞고 많이 앓으셨군요",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027183.1.19",
          "form": "괜찮으실겁니다",
          "original_form": "괜찮으실겁니다",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027183.1.20",
          "form": "네 열도 많이나고 힘들어서 죽 배달 시켜먹엇서요",
          "original_form": "네 열도 많이나고 힘들어서 죽 배달 시켜먹엇서요",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027183.1.21",
          "form": "오늘 죽 어떠세요?",
          "original_form": "오늘 죽 어떠세요?",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027183.1.22",
          "form": "ㅜ 맞고 언제부터 아프셨어요?",
          "original_form": "ㅜ 맞고 언제부터 아프셨어요?",
          "speaker_id": "1",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100027183.1.23",
          "form": "한 6시간지나고 나서부터 슬슬 안좋아지더라궇",
          "original_form": "한 6시간지나고 나서부터 슬슬 안좋아지더라궇",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027183.1.24",
          "form": "구요",
          "original_form": "구요",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027183.1.25",
          "form": "열도 38.5도까지 치고",
          "original_form": "열도 38.5도까지 치고",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027183.1.26",
          "form": "그 뭐냐 타이레놀 ER",
          "original_form": "그 뭐냐 타이레놀 ER",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027183.1.27",
          "form": "이알?그거 먹고",
          "original_form": "이알?그거 먹고",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027183.1.28",
          "form": "아이고; 열이 많이 났네요ㅜ",
          "original_form": "아이고; 열이 많이 났네요ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027183.1.29",
          "form": "고통의 30퍼 수준까지 내리고 8시간후에 100으로 올라가고 반복해씁니다",
          "original_form": "고통의 30퍼 수준까지 내리고 8시간후에 100으로 올라가고 반복해씁니다",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027183.1.30",
          "form": "총 이틀간 고생했네요 ㅠ",
          "original_form": "총 이틀간 고생했네요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027183.1.31",
          "form": "아찔합니다",
          "original_form": "아찔합니다",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027183.1.32",
          "form": "그래도 그후 큰 부작용이 안나 다행이에요",
          "original_form": "그래도 그후 큰 부작용이 안나 다행이에요",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027183.1.33",
          "form": "네 다행입니다",
          "original_form": "네 다행입니다",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027183.1.34",
          "form": "저 이번 접종을 계기로",
          "original_form": "저 이번 접종을 계기로",
          "speaker_id": "2",
          "time": "20210902 10:42"
        }
      ]
