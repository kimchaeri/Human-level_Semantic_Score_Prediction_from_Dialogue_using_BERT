      "utterance": [
        {
          "id": "MDRW2100011136.1.1",
          "form": "하 맨날 밤새는 게 일상이라 힘들어ㅠㅠ",
          "original_form": "하 맨날 밤새는 게 일상이라 힘들어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 01:34"
        },
        {
          "id": "MDRW2100011136.1.2",
          "form": "새벽 3 4시인데 학과사람들이 짱 많아!",
          "original_form": "새벽 3 4시인데 학과사람들이 짱 많아!",
          "speaker_id": "2",
          "time": "20210824 01:34"
        },
        {
          "id": "MDRW2100011136.1.3",
          "form": "헉 그렇구나 ㅠㅠㅠㅠ 과제가 많아서 그런거야??",
          "original_form": "헉 그렇구나 ㅠㅠㅠㅠ 과제가 많아서 그런거야??",
          "speaker_id": "1",
          "time": "20210824 01:35"
        },
        {
          "id": "MDRW2100011136.1.4",
          "form": "대단해...",
          "original_form": "대단해...",
          "speaker_id": "1",
          "time": "20210824 01:35"
        },
        {
          "id": "MDRW2100011136.1.5",
          "form": "초등교육과가 더 멋있는데!?!",
          "original_form": "초등교육과가 더 멋있는데!?!",
          "speaker_id": "2",
          "time": "20210824 01:35"
        },
        {
          "id": "MDRW2100011136.1.6",
          "form": "보통 뭐 배워?",
          "original_form": "보통 뭐 배워?",
          "speaker_id": "2",
          "time": "20210824 01:35"
        },
        {
          "id": "MDRW2100011136.1.7",
          "form": "단소 불고 장구 치고,, 서예도 해",
          "original_form": "단소 불고 장구 치고,, 서예도 해",
          "speaker_id": "1",
          "time": "20210824 01:35"
        },
        {
          "id": "MDRW2100011136.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ우와 재밌겠다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ우와 재밌겠다",
          "speaker_id": "2",
          "time": "20210824 01:36"
        },
        {
          "id": "MDRW2100011136.1.9",
          "form": "적성이랑 잘 맞아?",
          "original_form": "적성이랑 잘 맞아?",
          "speaker_id": "2",
          "time": "20210824 01:36"
        },
        {
          "id": "MDRW2100011136.1.10",
          "form": "앜ㅋㅋㅋ 맞아 무용도 배워!",
          "original_form": "앜ㅋㅋㅋ 맞아 무용도 배워!",
          "speaker_id": "1",
          "time": "20210824 01:36"
        },
        {
          "id": "MDRW2100011136.1.11",
          "form": "무용?",
          "original_form": "무용?",
          "speaker_id": "2",
          "time": "20210824 01:36"
        },
        {
          "id": "MDRW2100011136.1.12",
          "form": "웅웅 현대무용 이런거~",
          "original_form": "웅웅 현대무용 이런거~",
          "speaker_id": "1",
          "time": "20210824 01:36"
        },
        {
          "id": "MDRW2100011136.1.13",
          "form": "산디과 다시 생각해도 멋지다~~ 원래 꿈이었어?",
          "original_form": "산디과 다시 생각해도 멋지다~~ 원래 꿈이었어?",
          "speaker_id": "1",
          "time": "20210824 01:37"
        },
        {
          "id": "MDRW2100011136.1.14",
          "form": "응응 난 디자인과 가고싶었어!",
          "original_form": "응응 난 디자인과 가고싶었어!",
          "speaker_id": "2",
          "time": "20210824 01:37"
        },
        {
          "id": "MDRW2100011136.1.15",
          "form": "요새 장애인미술교사로 일하고 있어!",
          "original_form": "요새 장애인미술교사로 일하고 있어!",
          "speaker_id": "2",
          "time": "20210824 01:37"
        },
        {
          "id": "MDRW2100011136.1.16",
          "form": "우와~~ 이미 선생님이네! 어떤 일이야?",
          "original_form": "우와~~ 이미 선생님이네! 어떤 일이야?",
          "speaker_id": "1",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011136.1.17",
          "form": "장애인분들 미술 가르치는 거얌!",
          "original_form": "장애인분들 미술 가르치는 거얌!",
          "speaker_id": "2",
          "time": "20210824 01:39"
        },
        {
          "id": "MDRW2100011136.1.18",
          "form": "아 학교 동기들은 어때?",
          "original_form": "아 학교 동기들은 어때?",
          "speaker_id": "2",
          "time": "20210824 01:40"
        },
        {
          "id": "MDRW2100011136.1.19",
          "form": "다 좋지~~  아무래도 목적이 같아서 동질감이 많이 들어!",
          "original_form": "다 좋지~~  아무래도 목적이 같아서 동질감이 많이 들어!",
          "speaker_id": "1",
          "time": "20210824 01:41"
        },
        {
          "id": "MDRW2100011136.1.20",
          "form": "으와 친구 많구나!!",
          "original_form": "으와 친구 많구나!!",
          "speaker_id": "2",
          "time": "20210824 01:42"
        },
        {
          "id": "MDRW2100011136.1.21",
          "form": "교대는 좁아서 약간 강제 친구 느낌이야 ㅋㅋㅋㅋ!",
          "original_form": "교대는 좁아서 약간 강제 친구 느낌이야 ㅋㅋㅋㅋ!",
          "speaker_id": "1",
          "time": "20210824 01:43"
        }
      ]
