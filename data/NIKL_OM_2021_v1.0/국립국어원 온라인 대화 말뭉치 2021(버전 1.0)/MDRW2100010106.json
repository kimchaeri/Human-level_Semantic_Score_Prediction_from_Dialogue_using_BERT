      "utterance": [
        {
          "id": "MDRW2100010106.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100010106.1.2",
          "form": "안녕하세요! 여긴 비 오는데 거긴 어떤가요?",
          "original_form": "안녕하세요! 여긴 비 오는데 거긴 어떤가요?",
          "speaker_id": "2",
          "time": "20210823 14:28"
        },
        {
          "id": "MDRW2100010106.1.3",
          "form": "여기도 비가 많이 와요",
          "original_form": "여기도 비가 많이 와요",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010106.1.4",
          "form": "우산 챙기셨나요",
          "original_form": "우산 챙기셨나요",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010106.1.5",
          "form": "망했어요",
          "original_form": "망했어요",
          "speaker_id": "2",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010106.1.6",
          "form": "요즘 되게 동남아 날씨같아요 갑자기 비 쏟아지구",
          "original_form": "요즘 되게 동남아 날씨같아요 갑자기 비 쏟아지구",
          "speaker_id": "2",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010106.1.7",
          "form": "헉 안 챙기셨나요",
          "original_form": "헉 안 챙기셨나요",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010106.1.8",
          "form": "맞아요 기후 변화 때문에 그렇대요",
          "original_form": "맞아요 기후 변화 때문에 그렇대요",
          "speaker_id": "1",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010106.1.9",
          "form": "회사에 하나쯤 버려진 우산이 있을줄알았어요..ㅎ",
          "original_form": "회사에 하나쯤 버려진 우산이 있을줄알았어요..ㅎ",
          "speaker_id": "2",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010106.1.10",
          "form": "세상에 이럴 수가",
          "original_form": "세상에 이럴 수가",
          "speaker_id": "1",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010106.1.11",
          "form": "나중에 2050년되면 서울인천 일부 잠기고 한다는데",
          "original_form": "나중에 2050년되면 서울인천 일부 잠기고 한다는데",
          "speaker_id": "2",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010106.1.12",
          "form": "그럼 집에 가실 때 비 맞고 가시나요ㅠㅠ",
          "original_form": "그럼 집에 가실 때 비 맞고 가시나요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010106.1.13",
          "form": "그럴수도 있겠다 믿음이 가더라구요",
          "original_form": "그럴수도 있겠다 믿음이 가더라구요",
          "speaker_id": "2",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010106.1.14",
          "form": "집에 뒹굴고 있을 동생 찬스 쓰려구요ㅎㅎ",
          "original_form": "집에 뒹굴고 있을 동생 찬스 쓰려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010106.1.15",
          "form": "앗 동생 찬스가 있었군요",
          "original_form": "앗 동생 찬스가 있었군요",
          "speaker_id": "1",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010106.1.16",
          "form": "이번 주 내내 비올거래요",
          "original_form": "이번 주 내내 비올거래요",
          "speaker_id": "1",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010106.1.17",
          "form": "태풍 온다는 말도 있고요",
          "original_form": "태풍 온다는 말도 있고요",
          "speaker_id": "1",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010106.1.18",
          "form": "여름엔 장마 전혀 없더니 난리네유",
          "original_form": "여름엔 장마 전혀 없더니 난리네유",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010106.1.19",
          "form": "진짜 모기도 엄청 나오더라구요",
          "original_form": "진짜 모기도 엄청 나오더라구요",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010106.1.20",
          "form": "그러게 말이에요",
          "original_form": "그러게 말이에요",
          "speaker_id": "1",
          "time": "20210823 14:32"
        }
