      "utterance": [
        {
          "id": "MDRW2100021884.1.1",
          "form": "티비프로그램 보는거 있으세요?",
          "original_form": "티비프로그램 보는거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021884.1.2",
          "form": "동상이몽. 돌싱글즈. 등등요",
          "original_form": "동상이몽. 돌싱글즈. 등등요",
          "speaker_id": "2",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021884.1.3",
          "form": "돌싱글즈는 무슨 프로그램인가요?",
          "original_form": "돌싱글즈는 무슨 프로그램인가요?",
          "speaker_id": "1",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100021884.1.4",
          "form": "처음 들어봐요",
          "original_form": "처음 들어봐요",
          "speaker_id": "1",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100021884.1.5",
          "form": "돌싱 일반인들..4대4 소개팅이요 ㅎ",
          "original_form": "돌싱 일반인들..4대4 소개팅이요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021884.1.6",
          "form": "아하 일반인들 나오는 프로그램이군요!",
          "original_form": "아하 일반인들 나오는 프로그램이군요!",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021884.1.7",
          "form": "드라마는 잘 안보시나요?",
          "original_form": "드라마는 잘 안보시나요?",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021884.1.8",
          "form": "요즘은 볼게없더라구요..얼마전까지..마인..결혼작사..봤어요 ㅋ",
          "original_form": "요즘은 볼게없더라구요..얼마전까지..마인..결혼작사..봤어요 ㅋ",
          "speaker_id": "2",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021884.1.9",
          "form": "오 마인 저도 봤어요!",
          "original_form": "오 마인 저도 봤어요!",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021884.1.10",
          "form": "최근에 넷플로 나의아저씨 너무 재미있고 감동있게 봤어요",
          "original_form": "최근에 넷플로 나의아저씨 너무 재미있고 감동있게 봤어요",
          "speaker_id": "2",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021884.1.11",
          "form": "아그거 아직 안봤는데 재밌나보네요!",
          "original_form": "아그거 아직 안봤는데 재밌나보네요!",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021884.1.12",
          "form": "마인 재미있죠..이보영은 나이들어도 이쁘드라구요",
          "original_form": "마인 재미있죠..이보영은 나이들어도 이쁘드라구요",
          "speaker_id": "2",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021884.1.13",
          "form": "네 완전 감동있어요 울다가 웃다가..꼭보세요",
          "original_form": "네 완전 감동있어요 울다가 웃다가..꼭보세요",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021884.1.14",
          "form": "다음에 찾아봐야겠어요!",
          "original_form": "다음에 찾아봐야겠어요!",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021884.1.15",
          "form": "네 완전 강추요",
          "original_form": "네 완전 강추요",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100021884.1.16",
          "form": "name2은 좋아하는 연예인 있으신가요?",
          "original_form": "&name2&은 좋아하는 연예인 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100021884.1.17",
          "form": "정해인이 좋더라구요",
          "original_form": "정해인이 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021884.1.18",
