      "utterance": [
        {
          "id": "MDRW2100023425.1.1",
          "form": "그때가 41~2개월 이었을꺼에요ㅎ",
          "original_form": "그때가 41~2개월 이었을꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:07"
        },
        {
          "id": "MDRW2100023425.1.2",
          "form": "조금 늦긴 했네요 걱정 하실만",
          "original_form": "조금 늦긴 했네요 걱정 하실만",
          "speaker_id": "1",
          "time": "20210830 02:07"
        },
        {
          "id": "MDRW2100023425.1.3",
          "form": "아빠 엄마 밖에 말 못했었어요 ㅡㅎ 진짜 그때 엄청 걱정많이해서ㅠ",
          "original_form": "아빠 엄마 밖에 말 못했었어요 ㅡㅎ 진짜 그때 엄청 걱정많이해서ㅠ",
          "speaker_id": "2",
          "time": "20210830 02:08"
        },
        {
          "id": "MDRW2100023425.1.4",
          "form": "전  조카들이 넘빨리 말해서",
          "original_form": "전  조카들이 넘빨리 말해서",
          "speaker_id": "1",
          "time": "20210830 02:08"
        },
        {
          "id": "MDRW2100023425.1.5",
          "form": "언어치료도 생각했엇어요",
          "original_form": "언어치료도 생각했엇어요",
          "speaker_id": "2",
          "time": "20210830 02:08"
        },
        {
          "id": "MDRW2100023425.1.6",
          "form": "5세오빠는 30개월에 했는데 늦은편이였어요",
          "original_form": "5세오빠는 30개월에 했는데 늦은편이였어요",
          "speaker_id": "1",
          "time": "20210830 02:08"
        },
        {
          "id": "MDRW2100023425.1.7",
          "form": "아마 주위에서 빨리 말하면 따라하면서 느는것같더라구요ㅎ",
          "original_form": "아마 주위에서 빨리 말하면 따라하면서 느는것같더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:08"
        },
        {
          "id": "MDRW2100023425.1.8",
          "form": "오삐네 애기가 22개월에 해서 더빨리 할머니 했거든요",
          "original_form": "오삐네 애기가 22개월에 해서 더빨리 할머니 했거든요",
          "speaker_id": "1",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100023425.1.9",
          "form": "아아ㅎ",
          "original_form": "아아ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100023425.1.10",
          "form": "그렇구나 진짜 느렸구나ㅠ",
          "original_form": "그렇구나 진짜 느렸구나ㅠ",
          "speaker_id": "2",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100023425.1.11",
          "form": "ㅎㅎ 근데 보통",
          "original_form": "ㅎㅎ 근데 보통",
          "speaker_id": "1",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100023425.1.12",
          "form": "인지 되면 걱정 안해도 된데요",
          "original_form": "인지 되면 걱정 안해도 된데요",
          "speaker_id": "1",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100023425.1.13",
          "form": "코로나라 거의 집에만 있어서 그랬나봐요ㅍ",
          "original_form": "코로나라 거의 집에만 있어서 그랬나봐요ㅍ",
          "speaker_id": "2",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100023425.1.14",
          "form": "어린이지 가면 확실히 빨리.늘죠",
          "original_form": "어린이지 가면 확실히 빨리.늘죠",
          "speaker_id": "1",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100023425.1.15",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100023425.1.16",
          "form": "왜나면 놀이를 하려면 이야기릉 해야하니",
          "original_form": "왜나면 놀이를 하려면 이야기릉 해야하니",
          "speaker_id": "1",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100023425.1.17",
          "form": "말지금 이렇게 하는것도 어린이집때문이에요ㅎ",
          "original_form": "말지금 이렇게 하는것도 어린이집때문이에요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:10"
        },
        {
          "id": "MDRW2100023425.1.18",
          "form": "하루는 존댓말을 쓰는데 얼마나 귀엽덥지",
          "original_form": "하루는 존댓말을 쓰는데 얼마나 귀엽덥지",
          "speaker_id": "2",
          "time": "20210830 02:10"
        },
        {
          "id": "MDRW2100023425.1.19",
          "form": "ㅎㅎ티비 볼때도 혼자 보게 하지말고",
          "original_form": "ㅎㅎ티비 볼때도 혼자 보게 하지말고",
          "speaker_id": "1",
          "time": "20210830 02:10"
        },
        {
          "id": "MDRW2100023425.1.20",
          "form": "같이 이야기 하면서 보는게 좋아요",
          "original_form": "같이 이야기 하면서 보는게 좋아요",
          "speaker_id": "1",
          "time": "20210830 02:10"
        },
        {
          "id": "MDRW2100023425.1.21",
          "form": "아빠 핸드폰으로 유튜브 보면 안될까요? name3는 보고싶어요~ 보여주실꺼죠?",
          "original_form": "아빠 핸드폰으로 유튜브 보면 안될까요? &name3&는 보고싶어요~ 보여주실꺼죠?",
          "speaker_id": "2",
          "time": "20210830 02:10"
        },
        {
          "id": "MDRW2100023425.1.22",
          "form": "아ㅎㅎ 그렇구나ㅎ",
          "original_form": "아ㅎㅎ 그렇구나ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:11"
        },
        {
          "id": "MDRW2100023425.1.23",
          "form": "아 크롱이 울엇구나 슬펐겠다 이런식으로ㅠㅎㅎ",
          "original_form": "아 크롱이 울엇구나 슬펐겠다 이런식으로ㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:11"
        },
        {
          "id": "MDRW2100023425.1.24",
          "form": "아아ㅎ 많이 배웁니다ㅎ",
          "original_form": "아아ㅎ 많이 배웁니다ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:11"
        },
        {
          "id": "MDRW2100023425.1.25",
          "form": "ㅎㅎ 귀엽네요 ㅎㅎ",
          "original_form": "ㅎㅎ 귀엽네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:11"
        },
        {
          "id": "MDRW2100023425.1.26",
          "form": "전 그냥 아이가 보고있으면ㅎ",
          "original_form": "전 그냥 아이가 보고있으면ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:11"
        },
        {
          "id": "MDRW2100023425.1.27",
          "form": "끓어안기바뻐서ㅎㅎ 신경을 안썼내요ㅎㅎ",
          "original_form": "끓어안기바뻐서ㅎㅎ 신경을 안썼내요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:11"
        },
        {
          "id": "MDRW2100023425.1.28",
          "form": "근데 거의 티비는 안보여주고 맨날 몸으로 놀아요ㅎㅎ 아이가 너무 좋아해서ㅎ 안할수가없네요ㅎ",
          "original_form": "근데 거의 티비는 안보여주고 맨날 몸으로 놀아요ㅎㅎ 아이가 너무 좋아해서ㅎ 안할수가없네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:12"
        },
        {
          "id": "MDRW2100023425.1.29",
          "form": "저도 느려서 여기저기 조언을 ㅎㅎ",
          "original_form": "저도 느려서 여기저기 조언을 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:12"
        },
        {
          "id": "MDRW2100023425.1.30",
          "form": "말타기 아빠 놀이공원ㅎㅎ",
          "original_form": "말타기 아빠 놀이공원ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:12"
        },
        {
          "id": "MDRW2100023425.1.31",
          "form": "전 제가 해서 ㅋㅋ",
          "original_form": "전 제가 해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:12"
        },
        {
          "id": "MDRW2100023425.1.32",
          "form": "아.... 그래요?ㅎㅎ",
          "original_form": "아.... 그래요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:12"
        },
        {
          "id": "MDRW2100023425.1.33",
          "form": "엄마가 하기가 쉽지않는데",
          "original_form": "엄마가 하기가 쉽지않는데",
          "speaker_id": "2",
          "time": "20210830 02:12"
        },
        {
          "id": "MDRW2100023425.1.34",
          "form": "저희아내는 아빠가 당연히 해야한다고ㅎ 자기는누워있고하던데",
          "original_form": "저희아내는 아빠가 당연히 해야한다고ㅎ 자기는누워있고하던데",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023425.1.35",
          "form": "ㅎㅎㅎ 보통은 아빠가 해주는게 좋죠",
          "original_form": "ㅎㅎㅎ 보통은 아빠가 해주는게 좋죠",
          "speaker_id": "1",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023425.1.36",
          "form": "이제 주무셔요ㅎㅎㅎ 너무 피곤하실듯ㄹㅎ",
          "original_form": "이제 주무셔요ㅎㅎㅎ 너무 피곤하실듯ㄹㅎ",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023425.1.37",
          "form": "유대감 생기고",
          "original_form": "유대감 생기고",
          "speaker_id": "1",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023425.1.38",
          "form": "전 같이 놀렆자주 가요",
          "original_form": "전 같이 놀렆자주 가요",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023425.1.39",
          "form": "데이트ㅎㅎㅎ",
          "original_form": "데이트ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023425.1.40",
          "form": "나중에 커서도 많이 데리고 가고ㅎ",
          "original_form": "나중에 커서도 많이 데리고 가고ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023425.1.41",
          "form": "전 주로 둘이 가요  ㅎㅎ",
          "original_form": "전 주로 둘이 가요  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023425.1.42",
          "form": "아까도 말했지만ㅎ 이벤트로 차가지고싶다고ㅎ",
          "original_form": "아까도 말했지만ㅎ 이벤트로 차가지고싶다고ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023425.1.43",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023425.1.44",
          "form": "그게 아이를 위해서ㅎ",
          "original_form": "그게 아이를 위해서ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023425.1.45",
          "form": "좀더 넒은차로 아이와 함께",
          "original_form": "좀더 넒은차로 아이와 함께",
