          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021841.1.33",
          "form": "와 그럼 화염마녀는",
          "original_form": "와 그럼 화염마녀는",
          "speaker_id": "2",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021841.1.34",
          "form": "이번에 공개된",
          "original_form": "이번에 공개된",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021841.1.35",
          "form": "시뇨라가 쓰던거",
          "original_form": "시뇨라가 쓰던거",
          "speaker_id": "2",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021841.1.36",
          "form": "절연의 기치는 꼭두각시 검귀임",
          "original_form": "절연의 기치는 꼭두각시 검귀임",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021841.1.37",
          "form": "들고와서 쓰는거야?",
          "original_form": "들고와서 쓰는거야?",
          "speaker_id": "2",
          "time": "20210828 23:13"
