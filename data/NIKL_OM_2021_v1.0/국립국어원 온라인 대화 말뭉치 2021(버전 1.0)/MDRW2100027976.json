        {
          "id": "MDRW2100027976.1.1",
          "form": "집안일 하시나요?",
          "original_form": "집안일 하시나요?",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027976.1.2",
          "form": "넹 자주 해요",
          "original_form": "넹 자주 해요",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027976.1.3",
          "form": "집안일 저도 매일해요",
          "original_form": "집안일 저도 매일해요",
          "speaker_id": "1",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100027976.1.4",
          "form": "주로 뭐하세요",
          "original_form": "주로 뭐하세요",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100027976.1.5",
          "form": "모든 집안일요",
          "original_form": "모든 집안일요",
          "speaker_id": "1",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100027976.1.6",
          "form": "전 청소를 좋아해요",
          "original_form": "전 청소를 좋아해요",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100027976.1.7",
          "form": "진리는 로봇청소기인데",
          "original_form": "진리는 로봇청소기인데",
          "speaker_id": "1",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100027976.1.8",
          "form": "빨래는 건조기죠",
          "original_form": "빨래는 건조기죠",
          "speaker_id": "2",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100027976.1.9",
          "form": "로봇 하나장만하세요",
          "original_form": "로봇 하나장만하세요",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100027976.1.10",
          "form": "넹 룸메가 집안일을 안해서좀 힘들어요",
          "original_form": "넹 룸메가 집안일을 안해서좀 힘들어요",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100027976.1.11",
          "form": "흠 그럼 님이하세요",
          "original_form": "흠 그럼 님이하세요",
          "speaker_id": "1",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100027976.1.12",
          "form": "로봇청소기라도 사주면 덜 서운할텐데",
          "original_form": "로봇청소기라도 사주면 덜 서운할텐데",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100027976.1.13",
          "form": "똑같이하겠다는게욕심이에요",
          "original_form": "똑같이하겠다는게욕심이에요",
          "speaker_id": "1",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100027976.1.14",
          "form": "넹 감내해야죠",
          "original_form": "넹 감내해야죠",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100027976.1.15",
          "form": "생활비나 냈으면..",
          "original_form": "생활비나 냈으면..",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100027976.1.16",
          "form": "네 이해하세요",
          "original_form": "네 이해하세요",
