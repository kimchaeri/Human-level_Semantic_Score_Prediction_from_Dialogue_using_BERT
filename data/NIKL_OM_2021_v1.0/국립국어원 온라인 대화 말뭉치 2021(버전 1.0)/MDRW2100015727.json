      "utterance": [
        {
          "id": "MDRW2100015727.1.1",
          "form": "뭐샀다고?",
          "original_form": "뭐샀다고?",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015727.1.2",
          "form": "과자",
          "original_form": "과자",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015727.1.3",
          "form": "무슨과자?",
          "original_form": "무슨과자?",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015727.1.4",
          "form": "쿠크다스 비슷",
          "original_form": "쿠크다스 비슷",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015727.1.5",
          "form": "이름이뭔데",
          "original_form": "이름이뭔데",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015727.1.6",
          "form": "위메이드 밀크쿠키",
          "original_form": "위메이드 밀크쿠키",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015727.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015727.1.8",
          "form": "우유맛많이나?",
          "original_form": "우유맛많이나?",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015727.1.9",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015727.1.10",
          "form": "요번에 그런과자 많이삼ㅋㅋ",
          "original_form": "요번에 그런과자 많이삼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015727.1.11",
          "form": "버터링도 삼ㅋㅋ",
          "original_form": "버터링도 삼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015727.1.12",
          "form": "아빠가 임플란트 한다고 몇개 이빨을 뽑아놓음ㅋㅋ",
          "original_form": "아빠가 임플란트 한다고 몇개 이빨을 뽑아놓음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015727.1.13",
          "form": "아그래서 부드러운거 사는거?",
          "original_form": "아그래서 부드러운거 사는거?",
          "speaker_id": "1",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015727.1.14",
          "form": "응응 내가 좋아하기도 하고ㅋㅋ아빠핑계대고 삿음",
          "original_form": "응응 내가 좋아하기도 하고ㅋㅋ아빠핑계대고 삿음",
          "speaker_id": "2",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015727.1.15",
          "form": "그 뭐냐 버터쿠키는 안좋아함?",
          "original_form": "그 뭐냐 버터쿠키는 안좋아함?",
          "speaker_id": "1",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015727.1.16",
          "form": "버터쿠키 좋아함",
          "original_form": "버터쿠키 좋아함",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015727.1.17",
          "form": "사고싶었는데 비슷한거 세개나 사서ㅜㅜ",
          "original_form": "사고싶었는데 비슷한거 세개나 사서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015727.1.18",
          "form": "난 그거좋아래 워커스쇼트브래드 완전살찌는맛 ㅋㅋ",
          "original_form": "난 그거좋아래 워커스쇼트브래드 완전살찌는맛 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015727.1.19",
          "form": "생긴거 그거같애",
          "original_form": "생긴거 그거같애",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015727.1.20",
          "form": "너너 잘사먹엇던거ㅋㅋ",
          "original_form": "너너 잘사먹엇던거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015727.1.21",
          "form": "칼로리바란스? ㅋㅋㅋ",
          "original_form": "칼로리바란스? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015727.1.22",
          "form": "ㅇㅇㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:53"
        },
        {
          "id": "MDRW2100015727.1.23",
          "form": "그런맛이야??",
          "original_form": "그런맛이야??",
          "speaker_id": "2",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015727.1.24",
          "form": "ㄴㄴ 그렇게 뻑뻑한느낌은 아니야 조금단단한 버터쿠키?",
          "original_form": "ㄴㄴ 그렇게 뻑뻑한느낌은 아니야 조금단단한 버터쿠키?",
          "speaker_id": "1",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015727.1.25",
          "form": "오홍 담에 도전ㅋㅋㅋ쿠팡에 로켓배송이 없엉ㅋㅋㅋㅋ",
          "original_form": "오홍 담에 도전ㅋㅋㅋ쿠팡에 로켓배송이 없엉ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015727.1.26",
          "form": "엥 있지않아?",
          "original_form": "엥 있지않아?",
          "speaker_id": "1",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015727.1.27",
          "form": "없엉ㅜㅜ 방금확인함ㅜㅜ",
          "original_form": "없엉ㅜㅜ 방금확인함ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015727.1.28",
          "form": "품절인가봐 원래 있음 나 자주사먹음ㅋㅋ",
