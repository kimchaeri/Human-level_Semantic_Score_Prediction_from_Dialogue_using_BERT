      "utterance": [
        {
          "id": "MDRW2100010512.1.1",
          "form": "흑흡",
          "original_form": "흑흡",
          "speaker_id": "1",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010512.1.2",
          "form": "하시는 운동 있으신가요?!",
          "original_form": "하시는 운동 있으신가요?!",
          "speaker_id": "1",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010512.1.3",
          "form": "요즘 1~2시간 정도 러닝합니다!",
          "original_form": "요즘 1~2시간 정도 러닝합니다!",
          "speaker_id": "2",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010512.1.4",
          "form": "오 실내에서요?!",
          "original_form": "오 실내에서요?!",
          "speaker_id": "1",
          "time": "20210823 19:04"
        },
        {
          "id": "MDRW2100010512.1.5",
          "form": "아니면 밖에서 러닝하시나요?",
          "original_form": "아니면 밖에서 러닝하시나요?",
          "speaker_id": "1",
          "time": "20210823 19:04"
        },
        {
          "id": "MDRW2100010512.1.6",
          "form": "집 근처에 공원이 있어서 거기서 하는 편이에요!",
          "original_form": "집 근처에 공원이 있어서 거기서 하는 편이에요!",
          "speaker_id": "2",
          "time": "20210823 19:05"
        },
        {
          "id": "MDRW2100010512.1.7",
          "form": "크으으 집 근처 공원 최고죠",
          "original_form": "크으으 집 근처 공원 최고죠",
          "speaker_id": "1",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100010512.1.8",
          "form": "러닝이 체력 향상에 좋다는 이야기 듣고",
          "original_form": "러닝이 체력 향상에 좋다는 이야기 듣고",
          "speaker_id": "1",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100010512.1.9",
          "form": "런데이도 깔았지만",
          "original_form": "런데이도 깔았지만",
          "speaker_id": "1",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100010512.1.10",
          "form": "켜보지도 못했답니다",
          "original_form": "켜보지도 못했답니다",
          "speaker_id": "1",
          "time": "20210823 19:08"
        },
        {
          "id": "MDRW2100010512.1.11",
          "form": "시작이 어렵죠",
          "original_form": "시작이 어렵죠",
          "speaker_id": "2",
          "time": "20210823 19:08"
        },
        {
          "id": "MDRW2100010512.1.12",
          "form": "저도 방학 시작한 김에 시작했어요",
          "original_form": "저도 방학 시작한 김에 시작했어요",
          "speaker_id": "2",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100010512.1.13",
          "form": "해야 하는 걸 알지만 계기나 동기가 없으면 운동도 정말 하기 귀찮아요",
          "original_form": "해야 하는 걸 알지만 계기나 동기가 없으면 운동도 정말 하기 귀찮아요",
          "speaker_id": "2",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100010512.1.14",
          "form": "건강해지는게 최고죠",
          "original_form": "건강해지는게 최고죠",
          "speaker_id": "1",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100010512.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100010512.1.16",
          "form": "작심 3일도 저는 솔직히 대단한 거라고 생각해요",
          "original_form": "작심 3일도 저는 솔직히 대단한 거라고 생각해요",
          "speaker_id": "1",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100010512.1.17",
          "form": "그니까요 진짜 운동만큼 시작하기 힘든 건 없어요",
          "original_form": "그니까요 진짜 운동만큼 시작하기 힘든 건 없어요",
          "speaker_id": "2",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100010512.1.18",
          "form": "마음 먹는데만 한참 걸려요ㅋㅋㅋ",
          "original_form": "마음 먹는데만 한참 걸려요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:12"
        },
        {
          "id": "MDRW2100010512.1.19",
          "form": "글고 꾸준히 하기 운동만큼 힘든 것도 없는 것 같어요",
          "original_form": "글고 꾸준히 하기 운동만큼 힘든 것도 없는 것 같어요",
          "speaker_id": "1",
          "time": "20210823 19:12"
        },
        {
          "id": "MDRW2100010512.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010512.1.21",
          "form": "혼자하기도 힘들어요",
          "original_form": "혼자하기도 힘들어요",
          "speaker_id": "2",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010512.1.22",
          "form": "저는 그래서 피티끊었어욬ㅋ큐ㅠㅠ",
          "original_form": "저는 그래서 피티끊었어욬ㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 19:14"
        },
        {
          "id": "MDRW2100010512.1.23",
          "form": "저도 내년에는 피티 받으려고요",
          "original_form": "저도 내년에는 피티 받으려고요",
          "speaker_id": "2",
          "time": "20210823 19:14"
        },
        {
          "id": "MDRW2100010512.1.24",
          "form": "일단 혼자서 체지방만 우선적으로 빼고 있어요",
          "original_form": "일단 혼자서 체지방만 우선적으로 빼고 있어요",
          "speaker_id": "2",
          "time": "20210823 19:14"
        },
        {
          "id": "MDRW2100010512.1.25",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210823 19:15"
        },
        {
          "id": "MDRW2100010512.1.26",
          "form": "혼자 체지방 빼는 게 진짜 대단한겁니다",
          "original_form": "혼자 체지방 빼는 게 진짜 대단한겁니다",
          "speaker_id": "1",
          "time": "20210823 19:15"
        }
      ]
