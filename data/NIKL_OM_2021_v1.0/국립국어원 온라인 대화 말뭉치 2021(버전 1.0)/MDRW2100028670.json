      "utterance": [
        {
          "id": "MDRW2100028670.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028670.1.2",
          "form": "안녕하세요~ 반려동물있으신가요",
          "original_form": "안녕하세요~ 반려동물있으신가요",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028670.1.3",
          "form": "지금은 없고 키운적 있습니다ㅠ",
          "original_form": "지금은 없고 키운적 있습니다ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028670.1.4",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028670.1.5",
          "form": "헉 어쩌다가...",
          "original_form": "헉 어쩌다가...",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028670.1.6",
          "form": "저도 지금 강아지 키워요",
          "original_form": "저도 지금 강아지 키워요",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028670.1.7",
          "form": "많이 아팠었네요ㅠ",
          "original_form": "많이 아팠었네요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028670.1.8",
          "form": "아이고 ㅠㅠ 아가 지금 하늘에서 name1님 기다리고 있을 거에요",
          "original_form": "아이고 ㅠㅠ 아가 지금 하늘에서 &name1&님 기다리고 있을 거에요",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028670.1.9",
          "form": "천천히, 시간 지나 만나게 되면 같이 놀려구요.",
          "original_form": "천천히, 시간 지나 만나게 되면 같이 놀려구요.",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028670.1.10",
          "form": "제가 뭐라 썼는지ㅠㅠ",
          "original_form": "제가 뭐라 썼는지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028670.1.11",
          "form": "강아지 좋아하는 간식이 있나요?",
          "original_form": "강아지 좋아하는 간식이 있나요?",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028670.1.12",
          "form": "아 강아지 무지개다리 건넜다고 하신 거라고 이해했어요",
          "original_form": "아 강아지 무지개다리 건넜다고 하신 거라고 이해했어요",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028670.1.13",
          "form": "아 네! 강아지 키운지 오래 되셨나요?",
          "original_form": "아 네! 강아지 키운지 오래 되셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028670.1.14",
          "form": "넵 올해 여섯살이에여 ㅎㅎㅎㅎ",
          "original_form": "넵 올해 여섯살이에여 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028670.1.15",
          "form": "푸들이에요",
          "original_form": "푸들이에요",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028670.1.16",
          "form": "제일 귀여울때군요",
          "original_form": "제일 귀여울때군요",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028670.1.17",
          "form": "푸들 넘 귀여워요",
          "original_form": "푸들 넘 귀여워요",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028670.1.18",
          "form": "이름 물어봐도 되나요?",
          "original_form": "이름 물어봐도 되나요?",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028670.1.19",
          "form": "제가 오늘 야근해서 지금 집에 왔는데",
          "original_form": "제가 오늘 야근해서 지금 집에 왔는데",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028670.1.20",
          "form": "펄쩍거리다가 혼자 비명까지지르네욬ㅋㅋㅋ 넘웃김...",
          "original_form": "펄쩍거리다가 혼자 비명까지지르네욬ㅋㅋㅋ 넘웃김...",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028670.1.21",
          "form": "이름 name3에요!!",
          "original_form": "이름 &name3&에요!!",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028670.1.22",
          "form": "집사 오니깐 넘  기분 좋은가 보군여 귀여워라ㅠㅠㅠ",
          "original_form": "집사 오니깐 넘  기분 좋은가 보군여 귀여워라ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028670.1.23",
          "form": "이름도 귀여워요ㅠㅠ",
          "original_form": "이름도 귀여워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028670.1.24",
          "form": "산책도 매일 두번씩 시켜요",
          "original_form": "산책도 매일 두번씩 시켜요",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028670.1.25",
          "form": "거의 상전+막내아들 ㅋㅋㅌ",
          "original_form": "거의 상전+막내아들 ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028670.1.26",
          "form": "집에 댕댕이 있으면 가장 1순위 되죠ㅠㅋㅋ",
          "original_form": "집에 댕댕이 있으면 가장 1순위 되죠ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028670.1.27",
          "form": "와 산책도 두번 가시는 군요",
          "original_form": "와 산책도 두번 가시는 군요",
          "speaker_id": "1",
          "time": "20210902 22:11"
        }
      ]
