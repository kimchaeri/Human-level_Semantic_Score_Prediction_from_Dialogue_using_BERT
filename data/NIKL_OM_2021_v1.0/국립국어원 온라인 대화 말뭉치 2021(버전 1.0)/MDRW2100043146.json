      "utterance": [
        {
          "id": "MDRW2100043146.1.1",
          "form": "생줄기 너무 웃겨요",
          "original_form": "생줄기 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043146.1.2",
          "form": "야채를 마니먹어여 건강한데",
          "original_form": "야채를 마니먹어여 건강한데",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043146.1.3",
          "form": "샘표 그양념 괜찮아요 오이소박이는",
          "original_form": "샘표 그양념 괜찮아요 오이소박이는",
          "speaker_id": "2",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043146.1.4",
          "form": "먹을만해용",
          "original_form": "먹을만해용",
          "speaker_id": "2",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043146.1.5",
          "form": "오오 오이소박이 해볼래요",
          "original_form": "오오 오이소박이 해볼래요",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043146.1.6",
          "form": "오이도 몸에 좋자나여",
          "original_form": "오이도 몸에 좋자나여",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043146.1.7",
          "form": "네네 진짜 그건 갠찮더라구요 ㅎㅎ  금방 먹어도 맛있고",
          "original_form": "네네 진짜 그건 갠찮더라구요 ㅎㅎ  금방 먹어도 맛있고",
          "speaker_id": "2",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043146.1.8",
          "form": "우와우와 궁금했는데 오늘 바로 사야겠다",
          "original_form": "우와우와 궁금했는데 오늘 바로 사야겠다",
          "speaker_id": "1",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043146.1.9",
          "form": "건강을 위해서",
          "original_form": "건강을 위해서",
          "speaker_id": "1",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043146.1.10",
          "form": "백신맞으셨나요??",
          "original_form": "백신맞으셨나요??",
          "speaker_id": "2",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100043146.1.11",
          "form": "1차 맞았어요",
          "original_form": "1차 맞았어요",
          "speaker_id": "1",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100043146.1.12",
          "form": "금욜날 2차인데 무서워요",
          "original_form": "금욜날 2차인데 무서워요",
          "speaker_id": "1",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100043146.1.13",
          "form": "아 벌써ㅠ",
          "original_form": "아 벌써ㅠ",
          "speaker_id": "2",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043146.1.14",
          "form": "1차는안아프셨죠??",
          "original_form": "1차는안아프셨죠??",
          "speaker_id": "2",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043146.1.15",
          "form": "1차는 두통이 좀 있어서",
          "original_form": "1차는 두통이 좀 있어서",
          "speaker_id": "1",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043146.1.16",
          "form": "타이레놀 2알 먹고 나았어요",
          "original_form": "타이레놀 2알 먹고 나았어요",
          "speaker_id": "1",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043146.1.17",
          "form": "화이자에여??ㅠㅠ",
          "original_form": "화이자에여??ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043146.1.18",
          "form": "네네 화이자에요",
          "original_form": "네네 화이자에요",
          "speaker_id": "1",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043146.1.19",
          "form": "2차 무서웡7",
          "original_form": "2차 무서웡7",
          "speaker_id": "1",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043146.1.20",
          "form": "ㅠㅠ 2차가 그렇게 아프다던데ㅠㅠ 저도 무섭 저는 지난주토욜에 1차맞았는데ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠ 2차가 그렇게 아프다던데ㅠㅠ 저도 무섭 저는 지난주토욜에 1차맞았는데ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043146.1.21",
          "form": "그니까요ㅠㅠ 걱정이에요",
          "original_form": "그니까요ㅠㅠ 걱정이에요",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043146.1.22",
          "form": "지난주 별 탈 없으셨나요???",
          "original_form": "지난주 별 탈 없으셨나요???",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043146.1.23",
          "form": "근육통만여 열도 안나구.. 대자연의날과 겹쳐서그런지 뻐근한 느낌??",
          "original_form": "근육통만여 열도 안나구.. 대자연의날과 겹쳐서그런지 뻐근한 느낌??",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043146.1.24",
          "form": "와오 맞다",
          "original_form": "와오 맞다",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043146.1.25",
          "form": "화이자 부작용 중에 부정출혈도 있고",
          "original_form": "화이자 부작용 중에 부정출혈도 있고",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043146.1.26",
          "form": "대자연 통증도 심해진 사람 많대",
          "original_form": "대자연 통증도 심해진 사람 많대",
          "speaker_id": "1",
          "time": "20210907 15:17"
        }
      ]
    }
