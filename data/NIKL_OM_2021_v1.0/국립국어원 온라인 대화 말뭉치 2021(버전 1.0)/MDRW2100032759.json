      "utterance": [
        {
          "id": "MDRW2100032759.1.1",
          "form": "ㅎㅎㅎ결혼은",
          "original_form": "ㅎㅎㅎ결혼은",
          "speaker_id": "1",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100032759.1.2",
          "form": "음아직여러모로준비가안됬지만",
          "original_form": "음아직여러모로준비가안됬지만",
          "speaker_id": "1",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100032759.1.3",
          "form": "하고싶은사람이생기면하려고요ㅎㅎ",
          "original_form": "하고싶은사람이생기면하려고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100032759.1.4",
          "form": "그죠? 적당한 때 적당한 사람이 있으면 결혼하는거라고 많이들 하시니까ㅎㅎ",
          "original_form": "그죠? 적당한 때 적당한 사람이 있으면 결혼하는거라고 많이들 하시니까ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100032759.1.5",
          "form": "ㅎㅎㅎ그죠",
          "original_form": "ㅎㅎㅎ그죠",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100032759.1.6",
          "form": "생겼으면해요",
          "original_form": "생겼으면해요",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100032759.1.7",
          "form": "칭구들 중엔 결혼 안하려는 친구느",
          "original_form": "칭구들 중엔 결혼 안하려는 친구느",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100032759.1.8",
          "form": "애 안가지려는 친구나도 많도라구요",
          "original_form": "애 안가지려는 친구나도 많도라구요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100032759.1.9",
          "form": "ㅜㅠ이해해요",
          "original_form": "ㅜㅠ이해해요",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100032759.1.10",
          "form": "이해되죠 완전",
          "original_form": "이해되죠 완전",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100032759.1.11",
          "form": "근데 또 애기들 보면 너무 귀여워서 맘 바뀌지 않으려나 싶기도 하고",
          "original_form": "근데 또 애기들 보면 너무 귀여워서 맘 바뀌지 않으려나 싶기도 하고",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100032759.1.12",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100032759.1.13",
          "form": "이상형은 있어요?",
          "original_form": "이상형은 있어요?",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100032759.1.14",
          "form": "부드러운스탈이요",
          "original_form": "부드러운스탈이요",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100032759.1.15",
          "form": "성격이? 다정다감한*",
          "original_form": "성격이? 다정다감한*",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100032759.1.16",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100032759.1.17",
          "form": "저는 만나고 보면 까칠남들이 좀 있었던거 같아요ㅋㅋㅋ",
          "original_form": "저는 만나고 보면 까칠남들이 좀 있었던거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100032759.1.18",
          "form": "부드러운 스탈 좋아한다고 생각했는데",
          "original_form": "부드러운 스탈 좋아한다고 생각했는데",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100032759.1.19",
          "form": "그런스타일이매력적일수있죠",
          "original_form": "그런스타일이매력적일수있죠",
