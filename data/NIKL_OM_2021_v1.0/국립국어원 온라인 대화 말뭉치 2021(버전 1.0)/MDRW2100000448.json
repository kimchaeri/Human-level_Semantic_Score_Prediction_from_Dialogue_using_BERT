      "utterance": [
        {
          "id": "MDRW2100000448.1.1",
          "form": "무슨 게임 좋아해?",
          "original_form": "무슨 게임 좋아해?",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000448.1.2",
          "form": "와우 그랜드체이스알아? 예전에 서비스종료했다가 최근에 스팀에 다시 출시됐어!",
          "original_form": "와우 그랜드체이스알아? 예전에 서비스종료했다가 최근에 스팀에 다시 출시됐어!",
          "speaker_id": "2",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000448.1.3",
          "form": "ㅜㅠ",
          "original_form": "ㅜㅠ",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000448.1.4",
          "form": "모르겠엉",
          "original_form": "모르겠엉",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000448.1.5",
          "form": "어떤 게임이야?",
          "original_form": "어떤 게임이야?",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000448.1.6",
          "form": "사람들끼지 싸우는 대전도 있고 다같이 몬스터 잡는 던전도 있는데 난 대전파야ㅋㅋ",
          "original_form": "사람들끼지 싸우는 대전도 있고 다같이 몬스터 잡는 던전도 있는데 난 대전파야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000448.1.7",
          "form": "너는 무슨게임 좋아해?",
          "original_form": "너는 무슨게임 좋아해?",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000448.1.8",
          "form": "난 게임 안해 ㅜ",
          "original_form": "난 게임 안해 ㅜ",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000448.1.9",
          "form": "난 밖에서 노는거 좋아해",
          "original_form": "난 밖에서 노는거 좋아해",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000448.1.10",
          "form": "앗 그래? 밖에서 뭐하고 놀아? 요즘엔 날도 덥자낭",
          "original_form": "앗 그래? 밖에서 뭐하고 놀아? 요즘엔 날도 덥자낭",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000448.1.11",
          "form": "요즘엔 잘 안놀아 ㅜ",
          "original_form": "요즘엔 잘 안놀아 ㅜ",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000448.1.12",
          "form": "너무 바뻐서",
          "original_form": "너무 바뻐서",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000448.1.13",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000448.1.14",
          "form": "나도 게임이나 할까?",
          "original_form": "나도 게임이나 할까?",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000448.1.15",
          "form": "근데 안하던 사람들은 안하더라구 하는사람만 하는듯ㅋㅋ\n게임 한번도 해본적 없어?",
          "original_form": "근데 안하던 사람들은 안하더라구 하는사람만 하는듯ㅋㅋ\n게임 한번도 해본적 없어?",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000448.1.16",
          "form": "피파 ㅎㅎㅎ",
          "original_form": "피파 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000448.1.17",
          "form": "학교다닝때 피파 좋아했어 ㅎㅎㅎ",
          "original_form": "학교다닝때 피파 좋아했어 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000448.1.18",
          "form": "아~ 피파는 플스인가? 컴으로도 가능해?",
          "original_form": "아~ 피파는 플스인가? 컴으로도 가능해?",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000448.1.19",
          "form": "컴으로 했었어 ㅎㅎㅎ",
          "original_form": "컴으로 했었어 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000448.1.20",
          "form": "요즘엔 폰으로도 할 수 있을거같은데! 피파없나?ㅋㅋ",
          "original_form": "요즘엔 폰으로도 할 수 있을거같은데! 피파없나?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000448.1.21",
          "form": "피파도 폰 가능인데",
          "original_form": "피파도 폰 가능인데",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000448.1.22",
          "form": "너무 현징 느낌이라",
          "original_form": "너무 현징 느낌이라",
          "speaker_id": "1",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000448.1.23",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000448.1.24",
          "form": "그럼 하면 되겠네! 아 현질?",
          "original_form": "그럼 하면 되겠네! 아 현질?",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000448.1.25",
          "form": "맞아 게임은 현질을 빼놓을수가 없지.. 현질 안하는 파?",
          "original_form": "맞아 게임은 현질을 빼놓을수가 없지.. 현질 안하는 파?",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000448.1.26",
          "form": "ㅇㅇㅇ 돈아까워서 ㅜ",
          "original_form": "ㅇㅇㅇ 돈아까워서 ㅜ",
          "speaker_id": "1",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000448.1.27",
          "form": "그랜드체이스더 현질 가능이야??",
          "original_form": "그랜드체이스더 현질 가능이야??",
          "speaker_id": "1",
          "time": "20210819 16:39"
        }
      ]
    }
