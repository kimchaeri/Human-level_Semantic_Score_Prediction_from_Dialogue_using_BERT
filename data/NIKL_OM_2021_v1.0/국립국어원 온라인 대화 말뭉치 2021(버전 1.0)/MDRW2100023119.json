      "utterance": [
        {
          "id": "MDRW2100023119.1.1",
          "form": "반려동물 기르세요?",
          "original_form": "반려동물 기르세요?",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023119.1.2",
          "form": "아니요;;동물은 없어요 name1은요?",
          "original_form": "아니요;;동물은 없어요 &name1&은요?",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023119.1.3",
          "form": "저도 안 키워요 ㅋㅋ",
          "original_form": "저도 안 키워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023119.1.4",
          "form": "근데 키우고 싶어요",
          "original_form": "근데 키우고 싶어요",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023119.1.5",
          "form": "아ㅎ 구피는있어요",
          "original_form": "아ㅎ 구피는있어요",
          "speaker_id": "2",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023119.1.6",
          "form": "관리 안 어려운가요?",
          "original_form": "관리 안 어려운가요?",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023119.1.7",
          "form": "네 잘크고있네요  생가보다 키우기쉬워요",
          "original_form": "네 잘크고있네요  생가보다 키우기쉬워요",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023119.1.8",
          "form": "어렸을 때 금붕어 키웠는데 다 죽여버려서.....ㅠ..",
          "original_form": "어렸을 때 금붕어 키웠는데 다 죽여버려서.....ㅠ..",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023119.1.9",
          "form": "ㅜㅜ 구피는 관리많이안해도 잘커서편하게키울수있어요",
          "original_form": "ㅜㅜ 구피는 관리많이안해도 잘커서편하게키울수있어요",
          "speaker_id": "2",
          "time": "20210830 08:22"
        },
        {
          "id": "MDRW2100023119.1.10",
          "form": "오래 기르셨나요?",
          "original_form": "오래 기르셨나요?",
          "speaker_id": "1",
          "time": "20210830 10:00"
        },
        {
          "id": "MDRW2100023119.1.11",
          "form": "지금있어요ㅋ 키우고ㅎ",
          "original_form": "지금있어요ㅋ 키우고ㅎ",
          "speaker_id": "2",
          "time": "20210830 11:25"
        },
        {
          "id": "MDRW2100023119.1.12",
          "form": "그러니까 언제부터 기르셨냔 얘기였어요",
          "original_form": "그러니까 언제부터 기르셨냔 얘기였어요",
          "speaker_id": "1",
          "time": "20210830 11:28"
        },
        {
          "id": "MDRW2100023119.1.13",
          "form": "아;; 1년쯤됐네요ㅎ",
          "original_form": "아;; 1년쯤됐네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100023119.1.14",
          "form": "걔네들은 수명이 어느정도 돼요?",
          "original_form": "걔네들은 수명이 어느정도 돼요?",
          "speaker_id": "1",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100023119.1.15",
          "form": "글쎄요 지금키우고있는 애들도 잘크고있는데1년지났어도 건강해요",
