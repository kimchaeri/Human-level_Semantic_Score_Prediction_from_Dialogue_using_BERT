      "utterance": [
        {
          "id": "MDRW2100033249.1.1",
          "form": "여수도 좋죠",
          "original_form": "여수도 좋죠",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100033249.1.2",
          "form": "가보셨나요?",
          "original_form": "가보셨나요?",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100033249.1.3",
          "form": "네네  나라 이동할 때는요",
          "original_form": "네네  나라 이동할 때는요",
          "speaker_id": "1",
          "time": "20210907 08:44"
        },
        {
          "id": "MDRW2100033249.1.4",
          "form": "진짜 동선을 잘짜야할거같아요 그 짐들고 다닐 생각에 아득하네요ㅜ",
          "original_form": "진짜 동선을 잘짜야할거같아요 그 짐들고 다닐 생각에 아득하네요ㅜ",
          "speaker_id": "2",
          "time": "20210907 08:49"
        },
        {
          "id": "MDRW2100033249.1.5",
          "form": "네 동선짜기가 핵심이예요",
          "original_form": "네 동선짜기가 핵심이예요",
          "speaker_id": "1",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100033249.1.6",
          "form": "가급적 효율적으로 움직이기",
          "original_form": "가급적 효율적으로 움직이기",
          "speaker_id": "1",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100033249.1.7",
          "form": "캐리어 2개는 챙겨야죠?",
          "original_form": "캐리어 2개는 챙겨야죠?",
          "speaker_id": "2",
          "time": "20210907 10:34"
        },
        {
          "id": "MDRW2100033249.1.8",
          "form": "저는 1개 챙겼어요",
          "original_form": "저는 1개 챙겼어요",
          "speaker_id": "1",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100033249.1.9",
          "form": "며칠여행인데요?",
          "original_form": "며칠여행인데요?",
          "speaker_id": "2",
          "time": "20210907 10:37"
        },
        {
          "id": "MDRW2100033249.1.10",
          "form": "전 25일이었는데",
          "original_form": "전 25일이었는데",
          "speaker_id": "1",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100033249.1.11",
          "form": "여름이라 짐을 최소화했어요",
          "original_form": "여름이라 짐을 최소화했어요",
          "speaker_id": "1",
          "time": "20210907 10:39"
        },
        {
          "id": "MDRW2100033249.1.12",
          "form": "기념품도 다 소화가능하셨나요?",
          "original_form": "기념품도 다 소화가능하셨나요?",
          "speaker_id": "2",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100033249.1.13",
          "form": "작은 배낭 하나 더 가져갔고",
          "original_form": "작은 배낭 하나 더 가져갔고",
          "speaker_id": "1",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100033249.1.14",
          "form": "캐리어에 꾹꾹 눌러담았어요",
          "original_form": "캐리어에 꾹꾹 눌러담았어요",
          "speaker_id": "1",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100033249.1.15",
          "form": "저 캐나다 귀국때 캐리어하나가득 쿠키랑 시럽 화장품ㅋㅋ",
          "original_form": "저 캐나다 귀국때 캐리어하나가득 쿠키랑 시럽 화장품ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100033249.1.16",
          "form": "멍청한 짓이었죠",
          "original_form": "멍청한 짓이었죠",
          "speaker_id": "2",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100033249.1.17",
          "form": "아 저도 면세품이랑 기념품 땜에 귀국 때마다 캐리어 눌러담느라",
          "original_form": "아 저도 면세품이랑 기념품 땜에 귀국 때마다 캐리어 눌러담느라",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100033249.1.18",
          "form": "고생했어요ㅎㅎ",
          "original_form": "고생했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100033249.1.19",
          "form": "캐리어도 헌거가져가서 바퀴가 빠질뻔해서ㅜㅋㅋ 진짜 식겁했어요",
          "original_form": "캐리어도 헌거가져가서 바퀴가 빠질뻔해서ㅜㅋㅋ 진짜 식겁했어요",
          "speaker_id": "2",
          "time": "20210907 11:09"
        },
        {
          "id": "MDRW2100033249.1.20",
          "form": "아이고 캐리어 바퀴 망가지면 난감하죠",
          "original_form": "아이고 캐리어 바퀴 망가지면 난감하죠",
          "speaker_id": "1",
          "time": "20210907 11:18"
        },
        {
          "id": "MDRW2100033249.1.21",
          "form": "전 비행기 수화물 무게 넘겨서 공항에서 난리난적도 있어요ㅎㅎ",
          "original_form": "전 비행기 수화물 무게 넘겨서 공항에서 난리난적도 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:19"
        }
      ]
