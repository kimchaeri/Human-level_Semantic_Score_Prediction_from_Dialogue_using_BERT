      "utterance": [
        {
          "id": "MDRW2100017652.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017652.1.2",
          "form": "여행 좋아하는데 코로나 때문에 못가고 있어요..",
          "original_form": "여행 좋아하는데 코로나 때문에 못가고 있어요..",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017652.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017652.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017652.1.5",
          "form": "가본 여행지중에 제일 좋앗던 곳 잇나요?",
          "original_form": "가본 여행지중에 제일 좋앗던 곳 잇나요?",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017652.1.6",
          "form": "제주도가 좋았어요!",
          "original_form": "제주도가 좋았어요!",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017652.1.7",
          "form": "제주도 음식이 맛있어서 행복했어요~",
          "original_form": "제주도 음식이 맛있어서 행복했어요~",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017652.1.8",
          "form": "뭘 제일 맛잇게 드셧어요?",
          "original_form": "뭘 제일 맛잇게 드셧어요?",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017652.1.9",
          "form": "흑돼지구이가 진짜 맛있더라구요!",
          "original_form": "흑돼지구이가 진짜 맛있더라구요!",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017652.1.10",
          "form": "흑돼지로 만든 돈까스도 좋았어요~",
          "original_form": "흑돼지로 만든 돈까스도 좋았어요~",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017652.1.11",
          "form": "오 흑돼지구이만 먹어보고 돈까스는 안먹어봣는데",
          "original_form": "오 흑돼지구이만 먹어보고 돈까스는 안먹어봣는데",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017652.1.12",
          "form": "다음에 제주 가면 먹어봐야겟네요",
          "original_form": "다음에 제주 가면 먹어봐야겟네요",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017652.1.13",
          "form": "다음주에 제주도 여행 있어요?",
          "original_form": "다음주에 제주도 여행 있어요?",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017652.1.14",
          "form": "아니요 다음에 제주에 가게되면 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요 다음에 제주에 가게되면 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017652.1.15",
          "form": "코로나 이후로 제주에 가본적 없어요 ㅋㅋㅋㅋ",
          "original_form": "코로나 이후로 제주에 가본적 없어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017652.1.16",
          "form": "아 ㅎㅎ 사람들 제주에 그렇게 많이 간데요",
          "original_form": "아 ㅎㅎ 사람들 제주에 그렇게 많이 간데요",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017652.1.17",
          "form": "청정구역이라는 이미지가 있나봐요...",
          "original_form": "청정구역이라는 이미지가 있나봐요...",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017652.1.18",
          "form": "여행 정말 좋아햇엇는데 코로나 이후로 여행가본적이 없어요 ㅋㅋ큐ㅠㅠㅜ",
          "original_form": "여행 정말 좋아햇엇는데 코로나 이후로 여행가본적이 없어요 ㅋㅋ큐ㅠㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017652.1.19",
          "form": "젤 기억에 남는 여행지는 어디였어요?",
          "original_form": "젤 기억에 남는 여행지는 어디였어요?",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017652.1.20",
          "form": "저는 청산도가 너무 좋앗어요",
          "original_form": "저는 청산도가 너무 좋앗어요",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017652.1.21",
          "form": "청산도는 어디에 있는 데에요?",
          "original_form": "청산도는 어디에 있는 데에요?",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017652.1.22",
          "form": "전라남도에 있어요",
          "original_form": "전라남도에 있어요",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017652.1.23",
          "form": "완도에서 배타고 좀더 가면 있어요",
          "original_form": "완도에서 배타고 좀더 가면 있어요",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017652.1.24",
          "form": "아~ 그래서 제가 잘 몰랐나봐요.",
          "original_form": "아~ 그래서 제가 잘 몰랐나봐요.",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017652.1.25",
          "form": "배 타고 얼마나 걸려요?",
          "original_form": "배 타고 얼마나 걸려요?",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017652.1.26",
          "form": "완도에서 배타면 한시간정도 걸려요",
          "original_form": "완도에서 배타면 한시간정도 걸려요",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017652.1.27",
          "form": "요즘 여행 가고 싶다는 생각이 드네요",
          "original_form": "요즘 여행 가고 싶다는 생각이 드네요",
          "speaker_id": "2",
          "time": "20210828 14:40"
        }
      ]
    }
  ]
