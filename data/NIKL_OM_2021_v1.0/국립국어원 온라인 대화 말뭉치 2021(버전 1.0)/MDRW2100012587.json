      "utterance": [
        {
          "id": "MDRW2100012587.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012587.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012587.1.3",
          "form": "치킨좋아하세요?",
          "original_form": "치킨좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012587.1.4",
          "form": "너무 좋아하죠",
          "original_form": "너무 좋아하죠",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012587.1.5",
          "form": "요리하는거 좋아하세요?",
          "original_form": "요리하는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012587.1.6",
          "form": "네네 저좋아해요",
          "original_form": "네네 저좋아해요",
          "speaker_id": "1",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012587.1.7",
          "form": "좋아하나요?",
          "original_form": "좋아하나요?",
          "speaker_id": "1",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012587.1.8",
          "form": "네 최근에 집에감자가 많아서 감자수프했어요",
          "original_form": "네 최근에 집에감자가 많아서 감자수프했어요",
          "speaker_id": "2",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012587.1.9",
          "form": "최근에요리 뭐힌셨어요",
          "original_form": "최근에요리 뭐힌셨어요",
          "speaker_id": "2",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012587.1.10",
          "form": "음 기억은안나네요ㅎㅎㅎ",
          "original_form": "음 기억은안나네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:22"
        },
        {
          "id": "MDRW2100012587.1.11",
          "form": "와우 감자스프",
          "original_form": "와우 감자스프",
          "speaker_id": "1",
          "time": "20210825 00:22"
        },
        {
          "id": "MDRW2100012587.1.12",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "1",
          "time": "20210825 00:22"
        },
        {
          "id": "MDRW2100012587.1.13",
          "form": "네 맛있었어요",
          "original_form": "네 맛있었어요",
          "speaker_id": "2",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100012587.1.14",
          "form": "좋아하는 음식이 뭐에요?",
          "original_form": "좋아하는 음식이 뭐에요?",
          "speaker_id": "2",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100012587.1.15",
          "form": "전 초밥이요",
          "original_form": "전 초밥이요",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100012587.1.16",
          "form": "회나 참치 해산물도 전부요!",
          "original_form": "회나 참치 해산물도 전부요!",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100012587.1.17",
          "form": "오 저도 초밥 좋아해요",
          "original_form": "오 저도 초밥 좋아해요",
          "speaker_id": "2",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012587.1.18",
          "form": "작년에 오마카세 처음 가봤는데 좋았어요",
          "original_form": "작년에 오마카세 처음 가봤는데 좋았어요",
          "speaker_id": "2",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012587.1.19",
          "form": "일식 좋아하는거네요?",
          "original_form": "일식 좋아하는거네요?",
          "speaker_id": "2",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100012587.1.20",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100012587.1.21",
          "form": "날거는 대부분 다좋아하죠",
          "original_form": "날거는 대부분 다좋아하죠",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100012587.1.22",
          "form": "회 초밥 육회 참치 우니",
          "original_form": "회 초밥 육회 참치 우니",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100012587.1.23",
          "form": "저는 회는 이상하게 안끌리는데 초밥은 좋아요",
          "original_form": "저는 회는 이상하게 안끌리는데 초밥은 좋아요",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100012587.1.24",
          "form": "역시 탄수화물인가",
          "original_form": "역시 탄수화물인가",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100012587.1.25",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100012587.1.26",
          "form": "회는 싫나요?",
          "original_form": "회는 싫나요?",
          "speaker_id": "1",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100012587.1.27",
          "form": "먹기는하는데 좋아하지는 않아요",
          "original_form": "먹기는하는데 좋아하지는 않아요",
          "speaker_id": "2",
          "time": "20210825 12:16"
        }
      ]
    }
  ]
