      "utterance": [
        {
          "id": "MDRW2100003505.1.1",
          "form": "집에 고기왔어? 보냇는데",
          "original_form": "집에 고기왔어? 보냇는데",
          "speaker_id": "1",
          "time": "20210820 23:58"
        },
        {
          "id": "MDRW2100003505.1.2",
          "form": "어 왔어 2개보냈니?",
          "original_form": "어 왔어 2개보냈니?",
          "speaker_id": "2",
          "time": "20210820 23:59"
        },
        {
          "id": "MDRW2100003505.1.3",
          "form": "2개? 뭐가 간건지 봐야겟어",
          "original_form": "2개? 뭐가 간건지 봐야겟어",
          "speaker_id": "1",
          "time": "20210820 23:59"
        },
        {
          "id": "MDRW2100003505.1.4",
          "form": "집에 고기 2개 있든데",
          "original_form": "집에 고기 2개 있든데",
          "speaker_id": "2",
          "time": "20210821 00:00"
        },
        {
          "id": "MDRW2100003505.1.5",
          "form": "그래? 주소잘못썻나봐",
          "original_form": "그래? 주소잘못썻나봐",
          "speaker_id": "1",
          "time": "20210821 00:00"
        },
        {
          "id": "MDRW2100003505.1.6",
          "form": "가지러 올꺼니?",
          "original_form": "가지러 올꺼니?",
          "speaker_id": "2",
          "time": "20210821 00:00"
        },
        {
          "id": "MDRW2100003505.1.7",
          "form": "아냐 그냥먹어",
          "original_form": "아냐 그냥먹어",
          "speaker_id": "1",
          "time": "20210821 00:00"
        },
        {
          "id": "MDRW2100003505.1.8",
          "form": "알았어",
          "original_form": "알았어",
          "speaker_id": "2",
          "time": "20210821 00:00"
        },
        {
          "id": "MDRW2100003505.1.9",
          "form": "응 스테이크 니까 맛있게먹어",
          "original_form": "응 스테이크 니까 맛있게먹어",
          "speaker_id": "1",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003505.1.10",
          "form": "이따 온다면서",
          "original_form": "이따 온다면서",
          "speaker_id": "2",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003505.1.11",
          "form": "응 그럼 이따 먹을까",
          "original_form": "응 그럼 이따 먹을까",
          "speaker_id": "1",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003505.1.12",
          "form": "그래 이따 구워먹어",
          "original_form": "그래 이따 구워먹어",
          "speaker_id": "2",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003505.1.13",
          "form": "알았어 냉장에 넣어줘",
          "original_form": "알았어 냉장에 넣어줘",
          "speaker_id": "1",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003505.1.14",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "2",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003505.1.15",
          "form": "고기 먹을꺼 생각나니 좋다",
          "original_form": "고기 먹을꺼 생각나니 좋다",
          "speaker_id": "1",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003505.1.16",
          "form": "자주먹으면서 그러니",
          "original_form": "자주먹으면서 그러니",
          "speaker_id": "2",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003505.1.17",
          "form": "먹어도 고기는 맛있잖아",
          "original_form": "먹어도 고기는 맛있잖아",
          "speaker_id": "1",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003505.1.18",
          "form": "너만그래",
          "original_form": "너만그래",
          "speaker_id": "2",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003505.1.19",
          "form": "ㅋㅋ 그런가 ? 택배하나 더 올거 있는데",
          "original_form": "ㅋㅋ 그런가 ? 택배하나 더 올거 있는데",
          "speaker_id": "1",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003505.1.20",
          "form": "여기로 시켯니",
          "original_form": "여기로 시켯니",
          "speaker_id": "2",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003505.1.21",
          "form": "응 그건 같이먹을려고 주문햇지",
          "original_form": "응 그건 같이먹을려고 주문햇지",
          "speaker_id": "1",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003505.1.22",
          "form": "뭐니",
          "original_form": "뭐니",
          "speaker_id": "2",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003505.1.23",
          "form": "곱창전골",
          "original_form": "곱창전골",
          "speaker_id": "1",
          "time": "20210821 00:04"
        },
        {
          "id": "MDRW2100003505.1.24",
          "form": "맛있는집이라고 추천받았어",
          "original_form": "맛있는집이라고 추천받았어",
          "speaker_id": "1",
          "time": "20210821 00:04"
        },
        {
          "id": "MDRW2100003505.1.25",
          "form": "잘안먹잖아",
          "original_form": "잘안먹잖아",
          "speaker_id": "2",
          "time": "20210821 00:04"
        },
        {
          "id": "MDRW2100003505.1.26",
          "form": "맛있데 오면 먹어봐야지",
          "original_form": "맛있데 오면 먹어봐야지",
          "speaker_id": "1",
          "time": "20210821 00:04"
        },
        {
          "id": "MDRW2100003505.1.27",
          "form": "알았다고",
          "original_form": "알았다고",
          "speaker_id": "2",
          "time": "20210821 00:05"
        },
        {
          "id": "MDRW2100003505.1.28",
          "form": "생일선물 준 가방은 어때?",
          "original_form": "생일선물 준 가방은 어때?",
          "speaker_id": "1",
          "time": "20210821 00:06"
        },
        {
          "id": "MDRW2100003505.1.29",
          "form": "옆의 아줌마들에게 자랑햇어",
          "original_form": "옆의 아줌마들에게 자랑햇어",
          "speaker_id": "2",
          "time": "20210821 00:06"
        },
        {
          "id": "MDRW2100003505.1.30",
          "form": "언니 그거 뭐야 하드라",
          "original_form": "언니 그거 뭐야 하드라",
          "speaker_id": "2",
          "time": "20210821 00:06"
        },
        {
          "id": "MDRW2100003505.1.31",
          "form": "ㅋㅋㅋ 자랑했다고",
          "original_form": "ㅋㅋㅋ 자랑했다고",
          "speaker_id": "1",
          "time": "20210821 00:07"
        }
      ]
