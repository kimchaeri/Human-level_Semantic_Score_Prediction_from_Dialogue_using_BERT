      "utterance": [
        {
          "id": "MDRW2100040649.1.1",
          "form": "와 검색하고왔어요 ㅋㅋㅋㅋ",
          "original_form": "와 검색하고왔어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040649.1.2",
          "form": "투게더 크림브륄레!",
          "original_form": "투게더 크림브륄레!",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040649.1.3",
          "form": "오잉? 그건 처음들어봐요!",
          "original_form": "오잉? 그건 처음들어봐요!",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040649.1.4",
          "form": "크림브륄레 진짜맛있어요",
          "original_form": "크림브륄레 진짜맛있어요",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100040649.1.5",
          "form": "커스타드 푸딩을 좋아하신다면 이건 안좋아할 수가 없어요",
          "original_form": "커스타드 푸딩을 좋아하신다면 이건 안좋아할 수가 없어요",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100040649.1.6",
          "form": "크림브륄레 맛이 나는 유사 식품들은 먹어봤지만",
          "original_form": "크림브륄레 맛이 나는 유사 식품들은 먹어봤지만",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100040649.1.7",
          "form": "진짜 크림브륄레는 먹어본적이 없어요 ㅠ",
          "original_form": "진짜 크림브륄레는 먹어본적이 없어요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100040649.1.8",
          "form": "그렇게 말씀하신다면 꼭 먹어봐야겠는걸요",
          "original_form": "그렇게 말씀하신다면 꼭 먹어봐야겠는걸요",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100040649.1.9",
          "form": "진짜꼭먹어보세요 제가사는지역에",
          "original_form": "진짜꼭먹어보세요 제가사는지역에",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100040649.1.10",
          "form": "어떤 라멘집에서 크림브륄레를 팔았거든요? ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어떤 라멘집에서 크림브륄레를 팔았거든요? ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100040649.1.11",
          "form": "그거먹으러 라멘먹으러가고 그랬어요 ㅋㅋㅋ",
          "original_form": "그거먹으러 라멘먹으러가고 그랬어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100040649.1.12",
          "form": "우와 라멘집에서요?",
          "original_form": "우와 라멘집에서요?",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100040649.1.13",
          "form": "되게 신기한라멘 가게에요!",
          "original_form": "되게 신기한라멘 가게에요!",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100040649.1.14",
          "form": "옹... 진짜 맛집 발굴을 시도해봐야겟어요",
          "original_form": "옹... 진짜 맛집 발굴을 시도해봐야겟어요",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100040649.1.15",
          "form": "일본인이 라는 라멘가게였는데 크림브륄레가 오히려 유명했죠 ㅋㅋㅋ 한번 드셔보시고",
          "original_form": "일본인이 라는 라멘가게였는데 크림브륄레가 오히려 유명했죠 ㅋㅋㅋ 한번 드셔보시고",
          "speaker_id": "1",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040649.1.16",
          "form": "집에서 만드실수있으실거에요!",
          "original_form": "집에서 만드실수있으실거에요!",
          "speaker_id": "1",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040649.1.17",
          "form": "name1님은 어떤 디저트 좋아하세요?",
          "original_form": "&name1&님은 어떤 디저트 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100040649.1.18",
          "form": "사실 전 거의 다 좋아합니다",
          "original_form": "사실 전 거의 다 좋아합니다",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100040649.1.19",
          "form": "어떤 디저트를 고르는건 또다른 어떤 디저트에게 실례에요",
          "original_form": "어떤 디저트를 고르는건 또다른 어떤 디저트에게 실례에요",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100040649.1.20",
          "form": "꺄르륵 ㅋㅋㅋ 역시 배우신분",
          "original_form": "꺄르륵 ㅋㅋㅋ 역시 배우신분",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100040649.1.21",
          "form": "새로운게 나오면 먹어보고싶은 타입입니다",
          "original_form": "새로운게 나오면 먹어보고싶은 타입입니다",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100040649.1.22",
          "form": "버터바드셔보셨어요?",
          "original_form": "버터바드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100040649.1.23",
          "form": "네! 먹어봤어요",
          "original_form": "네! 먹어봤어요",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100040649.1.24",
          "form": "진짜 새로운 맛이더라구요",
          "original_form": "진짜 새로운 맛이더라구요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100040649.1.25",
          "form": "이런걸 또 어떻게 만들 생각을 했을까요?",
          "original_form": "이런걸 또 어떻게 만들 생각을 했을까요?",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100040649.1.26",
          "form": "저아직 못먹어봤어요 ㅡㅜㅜㅜ 어떤맛인가요 ㅜ",
          "original_form": "저아직 못먹어봤어요 ㅡㅜㅜㅜ 어떤맛인가요 ㅜ",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100040649.1.27",
          "form": "이걸 어떻게 표현을 하면 좋을까요",
          "original_form": "이걸 어떻게 표현을 하면 좋을까요",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100040649.1.28",
          "form": "버터를 왕창 넣은 뻑뻑하지 않은 스콘?",
          "original_form": "버터를 왕창 넣은 뻑뻑하지 않은 스콘?",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100040649.1.29",
          "form": "아...버터바... 아... 먹어본적없지만",
          "original_form": "아...버터바... 아... 먹어본적없지만",
          "speaker_id": "1",
          "time": "20210904 23:53"
        }
