        },
        {
          "id": "MDRW2100020361.1.3",
          "form": "책보는거좋아하세요?",
          "original_form": "책보는거좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 02:06"
        },
        {
          "id": "MDRW2100020361.1.4",
          "form": "좋아합니다!! 근데 요즘 코로나라 도서관가기가 좀 그래서",
          "original_form": "좋아합니다!! 근데 요즘 코로나라 도서관가기가 좀 그래서",
          "speaker_id": "2",
          "time": "20210828 02:06"
        },
        {
          "id": "MDRW2100020361.1.5",
          "form": "예..한 2년간 잘 못보고있어요 ..^^",
          "original_form": "예..한 2년간 잘 못보고있어요 ..^^",
          "speaker_id": "2",
          "time": "20210828 02:07"
        },
        {
          "id": "MDRW2100020361.1.6",
          "form": "ㅋㄱㄱㅋㅋㅋㅋㅋㅋ꽤오래,,,",
          "original_form": "ㅋㄱㄱㅋㅋㅋㅋㅋㅋ꽤오래,,,",
          "speaker_id": "1",
          "time": "20210828 02:07"
        },
        {
          "id": "MDRW2100020361.1.7",
          "form": "그쳐..ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쳐..ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:07"
        },
        {
          "id": "MDRW2100020361.1.8",
          "form": "책은 한번읽으면 계속읽는데",
          "original_form": "책은 한번읽으면 계속읽는데",
          "speaker_id": "1",
          "time": "20210828 02:07"
        },
        {
          "id": "MDRW2100020361.1.9",
          "form": "한번놓으면",
          "original_form": "한번놓으면",
          "speaker_id": "1",
          "time": "20210828 02:07"
        },
        {
          "id": "MDRW2100020361.1.10",
          "form": "다시잡기가",
          "original_form": "다시잡기가",
          "speaker_id": "1",
          "time": "20210828 02:07"
        },
        {
          "id": "MDRW2100020361.1.11",
          "form": "쉽지않더라고요",
          "original_form": "쉽지않더라고요",
          "speaker_id": "1",
          "time": "20210828 02:07"
        },
        {
          "id": "MDRW2100020361.1.12",
          "form": "아니 근데 저 진짜 원래는 독서 좋아합니다!!",
          "original_form": "아니 근데 저 진짜 원래는 독서 좋아합니다!!",
          "speaker_id": "2",
          "time": "20210828 02:07"
        },
        {
          "id": "MDRW2100020361.1.13",
          "form": "그니깐요 요즘에 걍 넷플 유툽보다버면 시간이 넘 훅훅가서 ㅋㅋㅋㅋㅋ",
          "original_form": "그니깐요 요즘에 걍 넷플 유툽보다버면 시간이 넘 훅훅가서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:08"
        },
        {
          "id": "MDRW2100020361.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 자극적",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 자극적",
          "speaker_id": "1",
          "time": "20210828 02:08"
        },
        {
          "id": "MDRW2100020361.1.15",
          "form": "최근에 읽으신 책 있으신가요?! 아님 좋아하는 책??",
          "original_form": "최근에 읽으신 책 있으신가요?! 아님 좋아하는 책??",
          "speaker_id": "2",
          "time": "20210828 02:08"
        },
        {
          "id": "MDRW2100020361.1.16",
          "form": "양귀자작가님꺼\r\n나는소망한다내게금지된것을",
          "original_form": "양귀자작가님꺼\r\n나는소망한다내게금지된것을",
          "speaker_id": "1",
          "time": "20210828 02:09"
        },
        {
          "id": "MDRW2100020361.1.17",
          "form": "최근에 재밌게봤고",
          "original_form": "최근에 재밌게봤고",
          "speaker_id": "1",
          "time": "20210828 02:09"
        },
        {
          "id": "MDRW2100020361.1.18",
          "form": "누가추천물어보면 그냥",
          "original_form": "누가추천물어보면 그냥",
          "speaker_id": "1",
          "time": "20210828 02:09"
        },
        {
          "id": "MDRW2100020361.1.19",
          "form": "김초엽작가 우빛속 추천해요ㅋㅋ",
          "original_form": "김초엽작가 우빛속 추천해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:09"
        },
        {
          "id": "MDRW2100020361.1.20",
          "form": "오 저는 양귀자 작가님꺼 모순 하나 읽어봤어요!!",
          "original_form": "오 저는 양귀자 작가님꺼 모순 하나 읽어봤어요!!",
          "speaker_id": "2",
          "time": "20210828 02:09"
        },
        {
          "id": "MDRW2100020361.1.21",
          "form": "그것도 알아요!! 저도 공대출신이거든요",
          "original_form": "그것도 알아요!! 저도 공대출신이거든요",
          "speaker_id": "2",
          "time": "20210828 02:09"
        },
        {
          "id": "MDRW2100020361.1.22",
          "form": "모순도좋죠!",
          "original_form": "모순도좋죠!",
          "speaker_id": "1",
          "time": "20210828 02:10"
        },
        {
          "id": "MDRW2100020361.1.23",
          "form": "우와공대",
          "original_form": "우와공대",
          "speaker_id": "1",
          "time": "20210828 02:10"
        },
        {
          "id": "MDRW2100020361.1.24",
          "form": "포공나온사람이 썼다고 들었어요 ㅋㅋㅋ",
          "original_form": "포공나온사람이 썼다고 들었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:10"
        },
        {
          "id": "MDRW2100020361.1.25",
          "form": "아 헐 그렇구나",
          "original_form": "아 헐 그렇구나",
          "speaker_id": "1",
          "time": "20210828 02:10"
        },
        {
          "id": "MDRW2100020361.1.26",
          "form": "어쩐지 파워 이과같은",
          "original_form": "어쩐지 파워 이과같은",
          "speaker_id": "1",
          "time": "20210828 02:10"
        },
        {
          "id": "MDRW2100020361.1.27",
          "form": "글이거든요",
          "original_form": "글이거든요",
          "speaker_id": "1",
          "time": "20210828 02:10"
        },
        {
          "id": "MDRW2100020361.1.28",
          "form": "악 ㅋㅋㅋㅋㅋㅋ 저도 작가 추천해보자면",
          "original_form": "악 ㅋㅋㅋㅋㅋㅋ 저도 작가 추천해보자면",
          "speaker_id": "2",
          "time": "20210828 02:11"
        },
        {
          "id": "MDRW2100020361.1.29",
          "form": "저는 한명꽂히면 쭉 다 읽는편인데",
          "original_form": "저는 한명꽂히면 쭉 다 읽는편인데",
          "speaker_id": "2",
          "time": "20210828 02:11"
        },
        {
          "id": "MDRW2100020361.1.30",
          "form": "장강명 작가님꺼 거진 다 읽었어요",
          "original_form": "장강명 작가님꺼 거진 다 읽었어요",
          "speaker_id": "2",
          "time": "20210828 02:11"
        },
        {
          "id": "MDRW2100020361.1.31",
          "form": "그분거도읽었어요!",
          "original_form": "그분거도읽었어요!",
          "speaker_id": "1",
          "time": "20210828 02:11"
        },
        {
          "id": "MDRW2100020361.1.32",
          "form": "오오!! name1님도 독서 많이하시는군욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오오!! &name1&님도 독서 많이하시는군욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:12"
        },
        {
          "id": "MDRW2100020361.1.33",
          "form": "아 아니다",
          "original_form": "아 아니다",
          "speaker_id": "1",
          "time": "20210828 02:12"
        },
        {
          "id": "MDRW2100020361.1.34",
          "form": "아니에요!!",
          "original_form": "아니에요!!",
          "speaker_id": "1",
          "time": "20210828 02:12"
        },
        {
          "id": "MDRW2100020361.1.35",
          "form": "저분꺼 어떤책이 재밌나요?!",
          "original_form": "저분꺼 어떤책이 재밌나요?!",
          "speaker_id": "1",
          "time": "20210828 02:12"
        },
        {
          "id": "MDRW2100020361.1.36",
          "form": "꼭읽어볼게요",
          "original_form": "꼭읽어볼게요",
          "speaker_id": "1",
          "time": "20210828 02:12"
        },
        {
          "id": "MDRW2100020361.1.37",
          "form": "뤼미에르피플 한국이싫어서 댓글부대 우리의소원은전쟁",
          "original_form": "뤼미에르피플 한국이싫어서 댓글부대 우리의소원은전쟁",
          "speaker_id": "2",
          "time": "20210828 02:13"
        },
        {
          "id": "MDRW2100020361.1.38",
          "form": "지금생각나는것들은 요정도?!?!",
          "original_form": "지금생각나는것들은 요정도?!?!",
          "speaker_id": "2",
          "time": "20210828 02:13"
        },
        {
          "id": "MDRW2100020361.1.39",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 02:13"
        },
        {
          "id": "MDRW2100020361.1.40",
          "form": "한국이싫어서!!!",
          "original_form": "한국이싫어서!!!",
          "speaker_id": "1",
          "time": "20210828 02:13"
        },
        {
          "id": "MDRW2100020361.1.41",
          "form": "저거때문에 작가이름기억해요",
          "original_form": "저거때문에 작가이름기억해요",
          "speaker_id": "1",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020361.1.42",
          "form": "저거빌리려다 못빌렸던책이라",
          "original_form": "저거빌리려다 못빌렸던책이라",
          "speaker_id": "1",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020361.1.43",
          "form": "아!! 저책이 유명한가요 ㅋㅋㅋㅋ",
          "original_form": "아!! 저책이 유명한가요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020361.1.44",
          "form": "저는 저 책 걍 하루만에 다읽었어요 ㅋㅋㅋ",
          "original_form": "저는 저 책 걍 하루만에 다읽었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020361.1.45",
          "form": "재밌을것같아요 민음사에 저표지시리즈가 재밌더라고요",
          "original_form": "재밌을것같아요 민음사에 저표지시리즈가 재밌더라고요",
          "speaker_id": "1",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020361.1.46",
          "form": "진짜 쑥쑥 읽혀요",
          "original_form": "진짜 쑥쑥 읽혀요",
          "speaker_id": "2",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020361.1.47",
          "form": "댓글부대도 재밌을느낌!",
          "original_form": "댓글부대도 재밌을느낌!",
          "speaker_id": "1",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020361.1.48",
          "form": "저는 넘 ㅋㅋㅋ소설만 읽어요 ㅋㅋㅋㅋ",
          "original_form": "저는 넘 ㅋㅋㅋ소설만 읽어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:17"
        },
        {
          "id": "MDRW2100020361.1.49",
          "form": "다양하게 읽어야하는데..",
          "original_form": "다양하게 읽어야하는데..",
