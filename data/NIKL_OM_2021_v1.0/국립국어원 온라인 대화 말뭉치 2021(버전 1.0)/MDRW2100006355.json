      "utterance": [
        {
          "id": "MDRW2100006355.1.1",
          "form": "동아리 많이 해보셨나요?",
          "original_form": "동아리 많이 해보셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006355.1.2",
          "form": "음 동아리는 두개정도 했었어요",
          "original_form": "음 동아리는 두개정도 했었어요",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100006355.1.3",
          "form": "오 어떤 동아린가요",
          "original_form": "오 어떤 동아린가요",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100006355.1.4",
          "form": "1학년때 봉사동아리 했었어요",
          "original_form": "1학년때 봉사동아리 했었어요",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006355.1.5",
          "form": "아ㅋㅋ주변에 연계해서 봉사하는 동아린가요",
          "original_form": "아ㅋㅋ주변에 연계해서 봉사하는 동아린가요",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006355.1.6",
          "form": "과동아리였었어요",
          "original_form": "과동아리였었어요",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100006355.1.7",
          "form": "ㅋㅋㅋ농활은 가보셨어요?",
          "original_form": "ㅋㅋㅋ농활은 가보셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006355.1.8",
          "form": "아뇨 ㅋㅋ 농활쪽 동아리는 아니었어요",
          "original_form": "아뇨 ㅋㅋ 농활쪽 동아리는 아니었어요",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006355.1.9",
          "form": "아 농활은 보통 총학생회나 단대학생회에서 보통 하잖아요ㅋㅋㅋㅋ",
          "original_form": "아 농활은 보통 총학생회나 단대학생회에서 보통 하잖아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006355.1.10",
          "form": "그렇죠 그런데는 좀 큰동아리여서 부담스럽더라구여",
          "original_form": "그렇죠 그런데는 좀 큰동아리여서 부담스럽더라구여",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006355.1.11",
          "form": "아 거기서 주관하고 일반 학생들이 참여하고 햌ㅅ었어요 저희는",
          "original_form": "아 거기서 주관하고 일반 학생들이 참여하고 햌ㅅ었어요 저희는",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006355.1.12",
          "form": "저도 학생회는 안했는데",
          "original_form": "저도 학생회는 안했는데",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006355.1.13",
          "form": "다녀왔었거든요ㅋㅋㅋ",
          "original_form": "다녀왔었거든요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006355.1.14",
          "form": "아 어떠셨어요?",
          "original_form": "아 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006355.1.15",
          "form": "대존잼이요ㅋㅋㅋ",
          "original_form": "대존잼이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006355.1.16",
          "form": "ㅋㅋㅋㅋㅋ중간중간 야참도 많이먹구",
          "original_form": "ㅋㅋㅋㅋㅋ중간중간 야참도 많이먹구",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006355.1.17",
          "form": "농사일도 첨으로 해보고 하니꺼",
          "original_form": "농사일도 첨으로 해보고 하니꺼",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006355.1.18",
          "form": "추억도 되구ㅋㅋㅋ",
          "original_form": "추억도 되구ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006355.1.19",
          "form": "다른 과 학생들이랑 교류도 되고! 재미있었어요ㅋㅋ",
          "original_form": "다른 과 학생들이랑 교류도 되고! 재미있었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006355.1.20",
          "form": "저도 한번해보고싶은데",
          "original_form": "저도 한번해보고싶은데",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006355.1.21",
          "form": "코로나땜에 어렵겠네요 ㅋㅋㅋ",
          "original_form": "코로나땜에 어렵겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006355.1.22",
          "form": "저도 매년 못가본게 아쉬워요ㅋㅋ",
          "original_form": "저도 매년 못가본게 아쉬워요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006355.1.23",
          "form": "한번 밖에 못가봐서 ㅣㅋㅋㅋ",
          "original_form": "한번 밖에 못가봐서 ㅣㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:29"
        }
      ]
    }
