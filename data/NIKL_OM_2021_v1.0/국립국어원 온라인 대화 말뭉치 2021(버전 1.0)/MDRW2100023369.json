      "utterance": [
        {
          "id": "MDRW2100023369.1.1",
          "form": "name2이 뭐예요?",
          "original_form": "&name2&이 뭐예요?",
          "speaker_id": "1",
          "time": "20210830 00:59"
        },
        {
          "id": "MDRW2100023369.1.2",
          "form": "이태원 클라스 가게이름여",
          "original_form": "이태원 클라스 가게이름여",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023369.1.3",
          "form": "밤이 달달했으면 좋겠다",
          "original_form": "밤이 달달했으면 좋겠다",
          "speaker_id": "1",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023369.1.4",
          "form": "그거요",
          "original_form": "그거요",
          "speaker_id": "1",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023369.1.5",
          "form": "네네 ㅎㅎ 아시네요",
          "original_form": "네네 ㅎㅎ 아시네요",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023369.1.6",
          "form": "재밌게봐서요 기억나요",
          "original_form": "재밌게봐서요 기억나요",
          "speaker_id": "1",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023369.1.7",
          "form": "근데 마지막엔 좀 노잼화 됐어요",
          "original_form": "근데 마지막엔 좀 노잼화 됐어요",
          "speaker_id": "1",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023369.1.8",
          "form": "저도 같은 생각요 ㅎㅎㅎ",
          "original_form": "저도 같은 생각요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:30"
        },
        {
          "id": "MDRW2100023369.1.9",
          "form": "왕좌의게임이랑 비슷하죠",
          "original_form": "왕좌의게임이랑 비슷하죠",
          "speaker_id": "1",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023369.1.10",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023369.1.11",
          "form": "처음엔 이거 명작이다 했는데",
          "original_form": "처음엔 이거 명작이다 했는데",
          "speaker_id": "1",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023369.1.12",
          "form": "뒤로갈수록",
          "original_form": "뒤로갈수록",
          "speaker_id": "1",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023369.1.13",
          "form": "엥 스러운",
          "original_form": "엥 스러운",
          "speaker_id": "1",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023369.1.14",
          "form": "ㅋㅋㅋ 그쵸",
          "original_form": "ㅋㅋㅋ 그쵸",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023369.1.15",
          "form": "dp보셨어요?",
          "original_form": "dp보셨어요?",
          "speaker_id": "1",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023369.1.16",
          "form": "넷플신작인데 요즘 핫하던데요",
          "original_form": "넷플신작인데 요즘 핫하던데요",
          "speaker_id": "1",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023369.1.17",
          "form": "아뇨 못봣어요",
          "original_form": "아뇨 못봣어요",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023369.1.18",
          "form": "군대얘기라서 여성분은 좀 싫으실수도",
          "original_form": "군대얘기라서 여성분은 좀 싫으실수도",
          "speaker_id": "1",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100023369.1.19",
          "form": "아 전 군대 이야기 좋아해여 ㄹㅎ",
          "original_form": "아 전 군대 이야기 좋아해여 ㄹㅎ",
          "speaker_id": "2",
          "time": "20210830 02:07"
        },
        {
          "id": "MDRW2100023369.1.20",
          "form": "그럼 한번 봐보세요",
          "original_form": "그럼 한번 봐보세요",
          "speaker_id": "1",
          "time": "20210830 02:08"
        },
        {
          "id": "MDRW2100023369.1.21",
          "form": "재밌어요",
          "original_form": "재밌어요",
          "speaker_id": "1",
          "time": "20210830 02:08"
        },
        {
          "id": "MDRW2100023369.1.22",
          "form": "넵 한번 봐바야ㅜ겟네요 ㅎㅎ",
