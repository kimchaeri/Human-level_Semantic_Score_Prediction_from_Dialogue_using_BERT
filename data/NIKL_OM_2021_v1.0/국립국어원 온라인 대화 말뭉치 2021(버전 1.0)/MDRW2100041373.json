      "utterance": [
        {
          "id": "MDRW2100041373.1.1",
          "form": "저도 로코 좋아하고 추리물같은것도 좋아해요",
          "original_form": "저도 로코 좋아하고 추리물같은것도 좋아해요",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041373.1.2",
          "form": "오 저두요",
          "original_form": "오 저두요",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041373.1.3",
          "form": "추리물류는 뭐보셨어용?",
          "original_form": "추리물류는 뭐보셨어용?",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041373.1.4",
          "form": "박보영 진짜 여자가봐도 사랑스럽게 연기하더라구요",
          "original_form": "박보영 진짜 여자가봐도 사랑스럽게 연기하더라구요",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041373.1.5",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041373.1.6",
          "form": "추리물인지는 모르겠는데",
          "original_form": "추리물인지는 모르겠는데",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041373.1.7",
          "form": "로맨스류는 그 일단뜨겁게청소하라인가",
          "original_form": "로맨스류는 그 일단뜨겁게청소하라인가",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041373.1.8",
          "form": "그것도 잼나용 ㅎㅎ",
          "original_form": "그것도 잼나용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041373.1.9",
          "form": "아 그래요? 그건 안봤어서 한번봐야겠네오ㅡ",
          "original_form": "아 그래요? 그건 안봤어서 한번봐야겠네오ㅡ",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041373.1.10",
          "form": "네네 김유정 성인되고",
          "original_form": "네네 김유정 성인되고",
          "speaker_id": "1",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041373.1.11",
          "form": "예전에 너를기억해 재밌게 봤어요 추리물인지는 모르겠지만ㅋㅋ",
          "original_form": "예전에 너를기억해 재밌게 봤어요 추리물인지는 모르겠지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041373.1.12",
          "form": "어린이미지가 있어서 잘어울릴까했는데",
          "original_form": "어린이미지가 있어서 잘어울릴까했는데",
          "speaker_id": "1",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041373.1.13",
          "form": "하는 드라마마다 잼나더라구용",
          "original_form": "하는 드라마마다 잼나더라구용",
          "speaker_id": "1",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041373.1.14",
          "form": "너를기억해 ?",
          "original_form": "너를기억해 ?",
          "speaker_id": "1",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041373.1.15",
          "form": "함 찾아볼게용 !",
          "original_form": "함 찾아볼게용 !",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100041373.1.16",
          "form": "마자요! 연기력이 되니까 다 잘하더라구요!",
          "original_form": "마자요! 연기력이 되니까 다 잘하더라구요!",
          "speaker_id": "2",
          "time": "20210906 00:16"
        },
        {
          "id": "MDRW2100041373.1.17",
          "form": "넹! 한번 봐보세요!",
          "original_form": "넹! 한번 봐보세요!",
          "speaker_id": "2",
          "time": "20210906 00:16"
        },
        {
          "id": "MDRW2100041373.1.18",
          "form": "찾아봤는데 잼날거같아용 ㅎㅎ 쉬는날에 정주행해볼게용",
          "original_form": "찾아봤는데 잼날거같아용 ㅎㅎ 쉬는날에 정주행해볼게용",
          "speaker_id": "1",
          "time": "20210906 08:49"
        },
        {
          "id": "MDRW2100041373.1.19",
          "form": "오!  네네!",
          "original_form": "오!  네네!",
          "speaker_id": "2",
          "time": "20210906 12:01"
        }
      ]
