      "utterance": [
        {
          "id": "MDRW2100022131.1.1",
          "form": "아침식사는 하셨나용?",
          "original_form": "아침식사는 하셨나용?",
          "speaker_id": "1",
          "time": "20210829 09:16"
        },
        {
          "id": "MDRW2100022131.1.2",
          "form": "아침먹었어요 ㅡㅎㅎ",
          "original_form": "아침먹었어요 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022131.1.3",
          "form": "김치찌개에다가밥말아먹었네요ㅎㅎ",
          "original_form": "김치찌개에다가밥말아먹었네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100022131.1.4",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100022131.1.5",
          "form": "저는 된장찌개해서 먹었어요:)",
          "original_form": "저는 된장찌개해서 먹었어요:)",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022131.1.6",
          "form": "오오ㅎ 점심은요?",
          "original_form": "오오ㅎ 점심은요?",
          "speaker_id": "2",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100022131.1.7",
          "form": "점심은 아이가 생선 먹고싶다해서\r\n굴ㅂㅣ 구워먹었어요^^",
          "original_form": "점심은 아이가 생선 먹고싶다해서\r\n굴ㅂㅣ 구워먹었어요^^",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022131.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022131.1.9",
          "form": "오오ㅎ 점심은저 그냥 패쓰했어요ㅎㅎ",
          "original_form": "오오ㅎ 점심은저 그냥 패쓰했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022131.1.10",
          "form": "저녁은드셨어요?ㅎ",
          "original_form": "저녁은드셨어요?ㅎ",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022131.1.11",
          "form": "저는 치킨 시켰어요:)",
          "original_form": "저는 치킨 시켰어요:)",
          "speaker_id": "1",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022131.1.12",
          "form": "name2님은 점심 왜 안드셨어용",
          "original_form": "&name2&님은 점심 왜 안드셨어용",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022131.1.13",
          "form": "우와우와ㅠ",
          "original_form": "우와우와ㅠ",
          "speaker_id": "2",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022131.1.14",
          "form": "다이어트 중이에요ㅠ 생각보다 많이 쪘더라구요ㅠㅠ",
          "original_form": "다이어트 중이에요ㅠ 생각보다 많이 쪘더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022131.1.15",
          "form": "아항ㅠ 그럼 다이어트\r\n식단으로라도 드시쥬 ㅠ",
          "original_form": "아항ㅠ 그럼 다이어트\r\n식단으로라도 드시쥬 ㅠ",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022131.1.16",
          "form": "ㅎㅎㅎ 괜찮아요ㅎ",
          "original_form": "ㅎㅎㅎ 괜찮아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022131.1.17",
          "form": "다이어트한다고 밥은안먹고 지금 계속 믹스커피마시는중입니다ㅠ",
          "original_form": "다이어트한다고 밥은안먹고 지금 계속 믹스커피마시는중입니다ㅠ",
          "speaker_id": "2",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022131.1.18",
          "form": "믹스커피가 칼로리가 50인데ㅎ 그러면 오늘 전 저녁을 다먹었네요ㅎㅎㅎㄹㄹㄹ",
          "original_form": "믹스커피가 칼로리가 50인데ㅎ 그러면 오늘 전 저녁을 다먹었네요ㅎㅎㅎㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210829 19:57"
        },
        {
          "id": "MDRW2100022131.1.19",
          "form": "와 믹스커피 칼로리가\r\n50이나 되나요? ㄷㄷ",
          "original_form": "와 믹스커피 칼로리가\r\n50이나 되나요? ㄷㄷ",
          "speaker_id": "1",
          "time": "20210829 20:59"
        },
        {
          "id": "MDRW2100022131.1.20",
          "form": "네네ㅎㅎ 그냥 볼께 아니에요ㅎㅎ",
          "original_form": "네네ㅎㅎ 그냥 볼께 아니에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100022131.1.21",
          "form": "그냥 먹으면 완전 낭패봐요ㅎ",
          "original_form": "그냥 먹으면 완전 낭패봐요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100022131.1.22",
          "form": "감히 믹스커피따위가\r\n어마무시한 칼로리를ㅋㅋ",
          "original_form": "감히 믹스커피따위가\r\n어마무시한 칼로리를ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100022131.1.23",
          "form": "그따위가 나를 살찌게 하더라구요ㅎㅎㅎ",
          "original_form": "그따위가 나를 살찌게 하더라구요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100022131.1.24",
          "form": "감히ㅎㅎㅎ",
          "original_form": "감히ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100022131.1.25",
          "form": "근데 일하면서 주섬주섬 안먹으면 당떨어져서ㅠ",
          "original_form": "근데 일하면서 주섬주섬 안먹으면 당떨어져서ㅠ",
