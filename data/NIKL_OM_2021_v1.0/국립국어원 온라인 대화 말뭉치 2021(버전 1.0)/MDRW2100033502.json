      "utterance": [
        {
          "id": "MDRW2100033502.1.1",
          "form": "즐겨보는 드라마가있나요",
          "original_form": "즐겨보는 드라마가있나요",
          "speaker_id": "1",
          "time": "20210824 08:04"
        },
        {
          "id": "MDRW2100033502.1.2",
          "form": "저는 영드 정주행 중이에요",
          "original_form": "저는 영드 정주행 중이에요",
          "speaker_id": "2",
          "time": "20210824 08:06"
        },
        {
          "id": "MDRW2100033502.1.3",
          "form": "영드 어떤거 보고있나요?",
          "original_form": "영드 어떤거 보고있나요?",
          "speaker_id": "1",
          "time": "20210824 08:07"
        },
        {
          "id": "MDRW2100033502.1.4",
          "form": "파더 브라운 이라고 브라운신부  추천이요",
          "original_form": "파더 브라운 이라고 브라운신부  추천이요",
          "speaker_id": "2",
          "time": "20210824 08:07"
        },
        {
          "id": "MDRW2100033502.1.5",
          "form": "처음 들어보는데 넷플릭스에서 볼 수 있나요?",
          "original_form": "처음 들어보는데 넷플릭스에서 볼 수 있나요?",
          "speaker_id": "1",
          "time": "20210824 08:07"
        },
        {
          "id": "MDRW2100033502.1.6",
          "form": "앗 웨이브 사용하세요?",
          "original_form": "앗 웨이브 사용하세요?",
          "speaker_id": "2",
          "time": "20210824 08:10"
        },
        {
          "id": "MDRW2100033502.1.7",
          "form": "웨이브에 있나요?",
          "original_form": "웨이브에 있나요?",
          "speaker_id": "1",
          "time": "20210824 08:10"
        },
        {
          "id": "MDRW2100033502.1.8",
          "form": "저 따로 정기구독하는건 없고 보고싶을 때 구독해서 보는 편이에요",
          "original_form": "저 따로 정기구독하는건 없고 보고싶을 때 구독해서 보는 편이에요",
          "speaker_id": "1",
          "time": "20210824 08:11"
        },
        {
          "id": "MDRW2100033502.1.9",
          "form": "네네 웨이브 영드 카테고리에서 시즌1~2 볼수있어요",
          "original_form": "네네 웨이브 영드 카테고리에서 시즌1~2 볼수있어요",
          "speaker_id": "2",
          "time": "20210824 08:11"
        },
        {
          "id": "MDRW2100033502.1.10",
          "form": "핸드폰 sk세요?",
          "original_form": "핸드폰 sk세요?",
          "speaker_id": "2",
          "time": "20210824 08:11"
        },
        {
          "id": "MDRW2100033502.1.11",
          "form": "안타깝게도 Kt이용하고 있어요ㅎ",
          "original_form": "안타깝게도 Kt이용하고 있어요ㅎ",
          "speaker_id": "1",
          "time": "20210824 08:12"
        },
        {
          "id": "MDRW2100033502.1.12",
          "form": "웨이브에 있다니 한 번 찾아봐야겠어요",
          "original_form": "웨이브에 있다니 한 번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 08:12"
        },
        {
          "id": "MDRW2100033502.1.13",
          "form": "저는 비싼 요금제 쓰니까 웨이브 공짜더라고요 ㅋㅋ",
          "original_form": "저는 비싼 요금제 쓰니까 웨이브 공짜더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:14"
        },
        {
          "id": "MDRW2100033502.1.14",
          "form": "1950년대 영국이 배경이에요 ㅎㅎ",
          "original_form": "1950년대 영국이 배경이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 08:15"
        },
        {
          "id": "MDRW2100033502.1.15",
          "form": "듣기만 해도 궁금해지는걸요",
          "original_form": "듣기만 해도 궁금해지는걸요",
          "speaker_id": "1",
          "time": "20210824 08:15"
        },
        {
          "id": "MDRW2100033502.1.16",
          "form": "꼭 봐봐야 겠어요",
          "original_form": "꼭 봐봐야 겠어요",
          "speaker_id": "1",
          "time": "20210824 08:15"
        },
        {
          "id": "MDRW2100033502.1.17",
          "form": "해리포터 위즐리 아빠 역할했던 배우가 주인공 신부님 역할로 나와요 ㅎㅎ",
          "original_form": "해리포터 위즐리 아빠 역할했던 배우가 주인공 신부님 역할로 나와요 ㅎㅎ",
