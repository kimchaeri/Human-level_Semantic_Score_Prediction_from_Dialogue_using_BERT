      "utterance": [
        {
          "id": "MDRW2100013038.1.1",
          "form": "오늘 점심엔 뭐 먹어?",
          "original_form": "오늘 점심엔 뭐 먹어?",
          "speaker_id": "2",
          "time": "20210825 11:27"
        },
        {
          "id": "MDRW2100013038.1.2",
          "form": "오늘은 순두부찌개랑 돈까스 먹으려고 ㅎㅎ",
          "original_form": "오늘은 순두부찌개랑 돈까스 먹으려고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 11:27"
        },
        {
          "id": "MDRW2100013038.1.3",
          "form": "맛있겠네",
          "original_form": "맛있겠네",
          "speaker_id": "2",
          "time": "20210825 11:27"
        },
        {
          "id": "MDRW2100013038.1.4",
          "form": "근데 작은 아들은 순두부는 보통이래 ㅋㅋ",
          "original_form": "근데 작은 아들은 순두부는 보통이래 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:27"
        },
        {
          "id": "MDRW2100013038.1.5",
          "form": "주는대로 먹으라 해",
          "original_form": "주는대로 먹으라 해",
          "speaker_id": "2",
          "time": "20210825 11:27"
        },
        {
          "id": "MDRW2100013038.1.6",
          "form": "뭐 먹고 싶은 게 있는지 메뉴 듣고는 우물쭈물 하던데 ㅎㅎ",
          "original_form": "뭐 먹고 싶은 게 있는지 메뉴 듣고는 우물쭈물 하던데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 11:27"
        },
        {
          "id": "MDRW2100013038.1.7",
          "form": "그냥 물어보지 말고 줘",
          "original_form": "그냥 물어보지 말고 줘",
          "speaker_id": "2",
          "time": "20210825 11:28"
        },
        {
          "id": "MDRW2100013038.1.8",
          "form": "이미 다 만들어놔서 바꿀 수도 없어",
          "original_form": "이미 다 만들어놔서 바꿀 수도 없어",
          "speaker_id": "1",
          "time": "20210825 11:28"
        },
        {
          "id": "MDRW2100013038.1.9",
          "form": "12시 땡하면 먹겠네",
          "original_form": "12시 땡하면 먹겠네",
          "speaker_id": "2",
          "time": "20210825 11:28"
        },
        {
          "id": "MDRW2100013038.1.10",
          "form": "ㅇㅇ 그 시간에 맞춰 준비해놔야지",
          "original_form": "ㅇㅇ 그 시간에 맞춰 준비해놔야지",
          "speaker_id": "1",
          "time": "20210825 11:28"
        },
        {
          "id": "MDRW2100013038.1.11",
          "form": "난 벌써 배고프다 ㅎㄹ",
          "original_form": "난 벌써 배고프다 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 11:28"
        },
        {
          "id": "MDRW2100013038.1.12",
          "form": "ㅎㅎ 나도~ 뭐 먹을지 정했어?",
          "original_form": "ㅎㅎ 나도~ 뭐 먹을지 정했어?",
          "speaker_id": "1",
          "time": "20210825 11:29"
        },
        {
          "id": "MDRW2100013038.1.13",
          "form": "난 오늘 짬뽕이나 먹을까",
          "original_form": "난 오늘 짬뽕이나 먹을까",
          "speaker_id": "2",
          "time": "20210825 11:29"
        },
        {
          "id": "MDRW2100013038.1.14",
          "form": "ㅎㅎ 그라던지~ 뜨끈하고 맛있겠네",
          "original_form": "ㅎㅎ 그라던지~ 뜨끈하고 맛있겠네",
          "speaker_id": "1",
          "time": "20210825 11:29"
        }
      ]
