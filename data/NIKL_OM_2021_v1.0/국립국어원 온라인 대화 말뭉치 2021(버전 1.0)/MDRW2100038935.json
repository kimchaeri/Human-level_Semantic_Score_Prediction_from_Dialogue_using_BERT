      "utterance": [
        {
          "id": "MDRW2100038935.1.1",
          "form": "저도 앱 깔고 쿠폰 존버해봐야겠네요",
          "original_form": "저도 앱 깔고 쿠폰 존버해봐야겠네요",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100038935.1.2",
          "form": "전 쿠폰소식들리면 깔앗다가 끝나면 삭졔해요ㅋㅋ",
          "original_form": "전 쿠폰소식들리면 깔앗다가 끝나면 삭졔해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100038935.1.3",
          "form": "진짜 쿠폰만 쓰시는군요",
          "original_form": "진짜 쿠폰만 쓰시는군요",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100038935.1.4",
          "form": "전엔 그냥햇는데 써보니까 없으면 안하게 되더라고요",
          "original_form": "전엔 그냥햇는데 써보니까 없으면 안하게 되더라고요",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100038935.1.5",
          "form": "삐빅 쿠폰 중독입니다",
          "original_form": "삐빅 쿠폰 중독입니다",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100038935.1.6",
          "form": "맞아요 ㅋㅋㅋ\r\n쿠폰써야 더맛있어요",
          "original_form": "맞아요 ㅋㅋㅋ\r\n쿠폰써야 더맛있어요",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038935.1.7",
          "form": "그건 그렇죠 남이 대신 사주는거니까요",
          "original_form": "그건 그렇죠 남이 대신 사주는거니까요",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100038935.1.8",
          "form": "네 맞아요ㅋㅋ",
          "original_form": "네 맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100038935.1.9",
          "form": "저도 오랜만에 쿠폰 헌팅이나 다닐까요",
          "original_form": "저도 오랜만에 쿠폰 헌팅이나 다닐까요",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100038935.1.10",
          "form": "곧 치킨쿠폰이 생기겠지만요",
          "original_form": "곧 치킨쿠폰이 생기겠지만요",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100038935.1.11",
          "form": "하나로는 모자라요ㅋ",
          "original_form": "하나로는 모자라요ㅋ",
          "speaker_id": "1",
          "time": "20210903 07:30"
        },
        {
          "id": "MDRW2100038935.1.12",
          "form": "맞아요... 치킨은 1인 1닭이기 때문에ㅎㅎ",
          "original_form": "맞아요... 치킨은 1인 1닭이기 때문에ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 08:21"
        },
        {
          "id": "MDRW2100038935.1.13",
          "form": "많은거 같지만 금세 없어져 버리져",
          "original_form": "많은거 같지만 금세 없어져 버리져",
          "speaker_id": "1",
          "time": "20210903 08:24"
        },
        {
          "id": "MDRW2100038935.1.14",
          "form": "공감합니다...",
          "original_form": "공감합니다...",
          "speaker_id": "2",
          "time": "20210903 08:26"
        },
        {
          "id": "MDRW2100038935.1.15",
          "form": "근데 그게 다 뱃속에 들어갔을거 생각하면",
          "original_form": "근데 그게 다 뱃속에 들어갔을거 생각하면",
          "speaker_id": "2",
          "time": "20210903 08:26"
        },
        {
          "id": "MDRW2100038935.1.16",
          "form": "급하게 운동하게 되더라구요",
          "original_form": "급하게 운동하게 되더라구요",
