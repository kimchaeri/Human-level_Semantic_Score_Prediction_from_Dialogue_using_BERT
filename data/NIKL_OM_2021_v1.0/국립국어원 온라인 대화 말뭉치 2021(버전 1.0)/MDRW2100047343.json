      "utterance": [
        {
          "id": "MDRW2100047343.1.1",
          "form": "보고싶은 공연이",
          "original_form": "보고싶은 공연이",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100047343.1.2",
          "form": "있나요?",
          "original_form": "있나요?",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100047343.1.3",
          "form": "저 이소라 콘서트 가보고싶어요!",
          "original_form": "저 이소라 콘서트 가보고싶어요!",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100047343.1.4",
          "form": "라이브가 궁금해요",
          "original_form": "라이브가 궁금해요",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100047343.1.5",
          "form": "오.. 이소라 콘서트",
          "original_form": "오.. 이소라 콘서트",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100047343.1.6",
          "form": "뭔가 상상 못했는데",
          "original_form": "뭔가 상상 못했는데",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100047343.1.7",
          "form": "좋을 거 같네여ㅋㅋㅋㅋ",
          "original_form": "좋을 거 같네여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100047343.1.8",
          "form": "ㅋㅋㅋㅋㅋ 공연가면 사람들 많이 울지 그것도 궁금해요",
          "original_form": "ㅋㅋㅋㅋㅋ 공연가면 사람들 많이 울지 그것도 궁금해요",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100047343.1.9",
          "form": "저 갔던 콘서트들은",
          "original_form": "저 갔던 콘서트들은",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100047343.1.10",
          "form": "뭐 노래가 좋아서 눈물 흘리는 건",
          "original_form": "뭐 노래가 좋아서 눈물 흘리는 건",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100047343.1.11",
          "form": "없던 거 같아요ㅋㅋㅋㅋㅋ",
          "original_form": "없던 거 같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100047343.1.12",
          "form": "아! 그렇구낰ㅋㅋㅋ콘서트 어디 다녀오셧나요",
          "original_form": "아! 그렇구낰ㅋㅋㅋ콘서트 어디 다녀오셧나요",
          "speaker_id": "1",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100047343.1.13",
          "form": "저 아이돌 콘서트요ㅋㅋㅋㅋㅋ",
          "original_form": "저 아이돌 콘서트요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100047343.1.14",
          "form": "아하!!! 아이돌 누구 좋아하세요?",
          "original_form": "아하!!! 아이돌 누구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 19:27"
        },
        {
          "id": "MDRW2100047343.1.15",
          "form": "저도 아이돌 콘서트 가면 신날거같은데",
          "original_form": "저도 아이돌 콘서트 가면 신날거같은데",
          "speaker_id": "1",
          "time": "20210827 19:27"
        },
        {
          "id": "MDRW2100047343.1.16",
          "form": "티켓팅 엄두도 못 내겠어ㅏ요",
          "original_form": "티켓팅 엄두도 못 내겠어ㅏ요",
          "speaker_id": "1",
          "time": "20210827 19:27"
        },
        {
          "id": "MDRW2100047343.1.17",
          "form": "티켓팅 힘들죠.....",
          "original_form": "티켓팅 힘들죠.....",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100047343.1.18",
          "form": "짐짜 빡세요!!!",
          "original_form": "짐짜 빡세요!!!",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100047343.1.19",
          "form": "코로나 백신도 저는 겨우예약해서",
          "original_form": "코로나 백신도 저는 겨우예약해서",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100047343.1.20",
          "form": "아이돌 콘서트는 꿈도안꿉니닼ㅋㅋ",
          "original_form": "아이돌 콘서트는 꿈도안꿉니닼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100047343.1.21",
          "form": "아ㅋㅋㅋㅋㅋㅋ백신 ㅠ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ백신 ㅠ",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100047343.1.22",
          "form": "피켓팅ㅋㅋㅋㅋ",
          "original_form": "피켓팅ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100047343.1.23",
          "form": "요새는 그나마 쉽잖아요",
          "original_form": "요새는 그나마 쉽잖아요",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100047343.1.24",
          "form": "맞아요 요즘은 좀 쉬워보이던데",
          "original_form": "맞아요 요즘은 좀 쉬워보이던데",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100047343.1.25",
          "form": "저는 예약 잘 못해서 월요일에 맞아요 ㅋㅋㅋㅋ",
          "original_form": "저는 예약 잘 못해서 월요일에 맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100047343.1.26",
          "form": "담주요??? 빨리 맞으시네요 !!!",
          "original_form": "담주요??? 빨리 맞으시네요 !!!",
