        },
        {
          "id": "MDRW2100029429.1.2",
          "form": "네~~프리랜서요",
          "original_form": "네~~프리랜서요",
          "speaker_id": "2",
          "time": "20210903 08:04"
        },
        {
          "id": "MDRW2100029429.1.3",
          "form": "부럽다요",
          "original_form": "부럽다요",
          "speaker_id": "1",
          "time": "20210903 08:07"
        },
        {
          "id": "MDRW2100029429.1.4",
          "form": "시간이 좀 자유로워요",
          "original_form": "시간이 좀 자유로워요",
          "speaker_id": "2",
          "time": "20210903 08:12"
        },
        {
          "id": "MDRW2100029429.1.5",
          "form": "전 시간 압박이",
          "original_form": "전 시간 압박이",
          "speaker_id": "1",
          "time": "20210903 08:14"
        },
        {
          "id": "MDRW2100029429.1.6",
          "form": "지금이제 일나가시려고 준비중이시겠네요",
          "original_form": "지금이제 일나가시려고 준비중이시겠네요",
          "speaker_id": "2",
          "time": "20210903 08:29"
        },
        {
          "id": "MDRW2100029429.1.7",
          "form": "힘들어요",
          "original_form": "힘들어요",
          "speaker_id": "1",
          "time": "20210903 09:32"
        },
        {
          "id": "MDRW2100029429.1.8",
          "form": "힘내세요..",
          "original_form": "힘내세요..",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100029429.1.9",
          "form": "직업이 있는 모든사람은 다 힘들듯하네요",
          "original_form": "직업이 있는 모든사람은 다 힘들듯하네요",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100029429.1.10",
          "form": "사람이문제죠",
          "original_form": "사람이문제죠",
          "speaker_id": "1",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100029429.1.11",
          "form": "월급날생각하며 일하는거죠",
          "original_form": "월급날생각하며 일하는거죠",
          "speaker_id": "2",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100029429.1.12",
          "form": "힘들어도 참고",
          "original_form": "힘들어도 참고",
          "speaker_id": "2",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100029429.1.13",
          "form": "전 오후만일해서 시간이 좀여유로워요",
          "original_form": "전 오후만일해서 시간이 좀여유로워요",
          "speaker_id": "2",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100029429.1.14",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210903 10:06"
        },
        {
          "id": "MDRW2100029429.1.15",
          "form": "코로나로 쉬는날도 많아요",
          "original_form": "코로나로 쉬는날도 많아요",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
