      "utterance": [
        {
          "id": "MDRW2100041096.1.1",
          "form": "물론 찾기 힘들겠지만",
          "original_form": "물론 찾기 힘들겠지만",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041096.1.2",
          "form": "바로불태웠으면",
          "original_form": "바로불태웠으면",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041096.1.3",
          "form": "함",
          "original_form": "함",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041096.1.4",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041096.1.5",
          "form": "최소한로맨스는아니야",
          "original_form": "최소한로맨스는아니야",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041096.1.6",
          "form": "차라리 알탕영화나 해",
          "original_form": "차라리 알탕영화나 해",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041096.1.7",
          "form": "안보이게",
          "original_form": "안보이게",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041096.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ그러니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러니까",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041096.1.9",
          "form": "그 발음 안좋은 애들이랑 몰려있으라고",
          "original_form": "그 발음 안좋은 애들이랑 몰려있으라고",
          "speaker_id": "2",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041096.1.10",
          "form": "진심 어휴",
          "original_form": "진심 어휴",
          "speaker_id": "2",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041096.1.11",
          "form": "ㅋㅋㅋㅋㅋ알탕영화만",
          "original_form": "ㅋㅋㅋㅋㅋ알탕영화만",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041096.1.12",
          "form": "찍으면",
          "original_form": "찍으면",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041096.1.13",
          "form": "그냥안보고말지",
          "original_form": "그냥안보고말지",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041096.1.14",
          "form": "ㅇㅈ 안보기 가능",
          "original_form": "ㅇㅈ 안보기 가능",
          "speaker_id": "2",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041096.1.15",
          "form": "여주가좋은배우면",
          "original_form": "여주가좋은배우면",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041096.1.16",
          "form": "짜증난다고",
          "original_form": "짜증난다고",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041096.1.17",
          "form": "ㅇㅈㅇㅈ 아니 진심",
          "original_form": "ㅇㅈㅇㅈ 아니 진심",
          "speaker_id": "2",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041096.1.18",
          "form": "어떻게 전도연이 원톱 주연을 못맡냐",
          "original_form": "어떻게 전도연이 원톱 주연을 못맡냐",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.19",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "1",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.20",
          "form": "왜 거기에 류를 비벼",
          "original_form": "왜 거기에 류를 비벼",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.21",
          "form": "걔는누가써주는거임?",
          "original_form": "걔는누가써주는거임?",
          "speaker_id": "1",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.22",
          "form": "드라마 제작사가",
          "original_form": "드라마 제작사가",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.23",
          "form": "그런애들써주니가",
          "original_form": "그런애들써주니가",
          "speaker_id": "1",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.24",
          "form": "요즘로맨스가",
          "original_form": "요즘로맨스가",
          "speaker_id": "1",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.25",
          "form": "다망하지",
          "original_form": "다망하지",
          "speaker_id": "1",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.26",
          "form": "류준열 소속사임",
          "original_form": "류준열 소속사임",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.27",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.28",
          "form": "하는족족망하잖아",
          "original_form": "하는족족망하잖아",
          "speaker_id": "1",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.29",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.30",
          "form": "로맨스 제대로 하던가 최소한",
          "original_form": "로맨스 제대로 하던가 최소한",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.31",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "1",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041096.1.32",
          "form": "얼굴합이라도 맞춰",
          "original_form": "얼굴합이라도 맞춰",
          "speaker_id": "2",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100041096.1.33",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100041096.1.34",
          "form": "류를",
          "original_form": "류를",
          "speaker_id": "1",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100041096.1.35",
          "form": "상대로",
          "original_form": "상대로",
          "speaker_id": "1",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100041096.1.36",
          "form": "얼굴맞는",
          "original_form": "얼굴맞는",
          "speaker_id": "1",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100041096.1.37",
          "form": "ㅂ9우를찾을수없음",
          "original_form": "ㅂ9우를찾을수없음",
          "speaker_id": "1",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100041096.1.38",
          "form": "같은남배우면모를까",
          "original_form": "같은남배우면모를까",
