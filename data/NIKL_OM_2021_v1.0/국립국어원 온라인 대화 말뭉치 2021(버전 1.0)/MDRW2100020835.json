      "utterance": [
        {
          "id": "MDRW2100020835.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100020835.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100020835.1.3",
          "form": "배달음식 자주 드시나요?",
          "original_form": "배달음식 자주 드시나요?",
          "speaker_id": "2",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100020835.1.4",
          "form": "네네 자주 먹는편이에요 ㅋㅋㅋㅋ",
          "original_form": "네네 자주 먹는편이에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100020835.1.5",
          "form": "배달 자주 드세요?",
          "original_form": "배달 자주 드세요?",
          "speaker_id": "1",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100020835.1.6",
          "form": "전 배달은 가끔 하고 포장해서 먹어요",
          "original_form": "전 배달은 가끔 하고 포장해서 먹어요",
          "speaker_id": "2",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020835.1.7",
          "form": "어떤거 주로 사드세요?",
          "original_form": "어떤거 주로 사드세요?",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020835.1.8",
          "form": "샤브샤브.곱창전골.해물칼국수. 중국 음식요",
          "original_form": "샤브샤브.곱창전골.해물칼국수. 중국 음식요",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020835.1.9",
          "form": "그렇군요!",
          "original_form": "그렇군요!",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020835.1.10",
          "form": "저는 치킨 자주 먹어요 ㅋㅋㅋㅋ",
          "original_form": "저는 치킨 자주 먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020835.1.11",
          "form": "치킨은 별로 잘 안드세요?",
          "original_form": "치킨은 별로 잘 안드세요?",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020835.1.12",
          "form": "치킨은. 배달이죠 ㅋ 한달에 한번정도 먹어요. 피자도 배달하구요",
          "original_form": "치킨은. 배달이죠 ㅋ 한달에 한번정도 먹어요. 피자도 배달하구요",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020835.1.13",
          "form": "ㅋㅋㅋㅋ네 둘다 사먹는거긴하잖아요",
          "original_form": "ㅋㅋㅋㅋ네 둘다 사먹는거긴하잖아요",
          "speaker_id": "1",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020835.1.14",
          "form": "좋아하는 치킨 브랜드있으세요?",
          "original_form": "좋아하는 치킨 브랜드있으세요?",
          "speaker_id": "1",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020835.1.15",
          "form": "그렇긴 하죠. 집근처는 픽업하고 좀먼곳은 배달",
          "original_form": "그렇긴 하죠. 집근처는 픽업하고 좀먼곳은 배달",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020835.1.16",
          "form": "비비큐.",
          "original_form": "비비큐.",
          "speaker_id": "2",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100020835.1.17",
          "form": "저두 비비큐 좋아해요 ㅋㅋㅋㅋ후라이드맛잇더라구요",
          "original_form": "저두 비비큐 좋아해요 ㅋㅋㅋㅋ후라이드맛잇더라구요",
          "speaker_id": "1",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020835.1.18",
          "form": "황금올리브 맛있어요",
          "original_form": "황금올리브 맛있어요",
          "speaker_id": "2",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020835.1.19",
          "form": "60계치킨도 맛있어요",
          "original_form": "60계치킨도 맛있어요",
          "speaker_id": "2",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020835.1.20",
          "form": "오 60계 안그래두 귱금햇는데 어느 메뉴가 맛잇나요?",
          "original_form": "오 60계 안그래두 귱금햇는데 어느 메뉴가 맛잇나요?",
          "speaker_id": "1",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020835.1.21",
          "form": "매콤한 맛이었는데 생각보다 맛있더라구요",
          "original_form": "매콤한 맛이었는데 생각보다 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020835.1.22",
          "form": "참고할게요!",
          "original_form": "참고할게요!",
          "speaker_id": "1",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100020835.1.23",
          "form": "저는 최근에 바른치킨 먹어봤는데 후라이드 고소하고 맛잇더라구요",
          "original_form": "저는 최근에 바른치킨 먹어봤는데 후라이드 고소하고 맛잇더라구요",
          "speaker_id": "1",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100020835.1.24",
          "form": "추천드립니다!매운거좋아하시면 대세레드",
          "original_form": "추천드립니다!매운거좋아하시면 대세레드",
          "speaker_id": "1",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100020835.1.25",
          "form": "바른치킨 은 처음들어보는 브랜드에요",
          "original_form": "바른치킨 은 처음들어보는 브랜드에요",
          "speaker_id": "2",
          "time": "20210828 13:26"
        },
        {
          "id": "MDRW2100020835.1.26",
          "form": "요즘 피피엘많이해서 궁금해서 먹어봣는데",
          "original_form": "요즘 피피엘많이해서 궁금해서 먹어봣는데",
          "speaker_id": "1",
          "time": "20210828 13:26"
        },
        {
          "id": "MDRW2100020835.1.27",
          "form": "맛있어요! 대세레드는 큰새우 올라가는데 잘 어울려요",
          "original_form": "맛있어요! 대세레드는 큰새우 올라가는데 잘 어울려요",
          "speaker_id": "1",
          "time": "20210828 13:27"
        },
        {
          "id": "MDRW2100020835.1.28",
          "form": "오~~맛있겠네요. 대세레드",
          "original_form": "오~~맛있겠네요. 대세레드",
          "speaker_id": "2",
          "time": "20210828 13:28"
        }
      ]
    }
