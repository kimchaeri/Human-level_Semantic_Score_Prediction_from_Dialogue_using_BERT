      "utterance": [
        {
          "id": "MDRW2100012798.1.1",
          "form": "아르바이트 해보신적 있으신가요",
          "original_form": "아르바이트 해보신적 있으신가요",
          "speaker_id": "1",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100012798.1.2",
          "form": "네 편의점과 과외알바해봤어요",
          "original_form": "네 편의점과 과외알바해봤어요",
          "speaker_id": "2",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100012798.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100012798.1.4",
          "form": "사무보조 알바해봤어요",
          "original_form": "사무보조 알바해봤어요",
          "speaker_id": "1",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100012798.1.5",
          "form": "단기간에 일 익히기 어렵지 않나요",
          "original_form": "단기간에 일 익히기 어렵지 않나요",
          "speaker_id": "2",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100012798.1.6",
          "form": "단순업무라서",
          "original_form": "단순업무라서",
          "speaker_id": "1",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100012798.1.7",
          "form": "쉬웠어요",
          "original_form": "쉬웠어요",
          "speaker_id": "1",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100012798.1.8",
          "form": "공부잘하셨나보네요",
          "original_form": "공부잘하셨나보네요",
          "speaker_id": "1",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100012798.1.9",
          "form": "과외알바 어떠셨나요?",
          "original_form": "과외알바 어떠셨나요?",
          "speaker_id": "1",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100012798.1.10",
          "form": "편해데...이렇게 돈벌어도 되나 싶어요",
          "original_form": "편해데...이렇게 돈벌어도 되나 싶어요",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100012798.1.11",
          "form": "약간 학부모에게 미안하기도 하고",
          "original_form": "약간 학부모에게 미안하기도 하고",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100012798.1.12",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100012798.1.13",
          "form": "그래도 편하긴 진짜 편해요",
          "original_form": "그래도 편하긴 진짜 편해요",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100012798.1.14",
          "form": "학부모들이 깐깐하게 요구안하던가요?",
          "original_form": "학부모들이 깐깐하게 요구안하던가요?",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100012798.1.15",
          "form": "커리큘럼 한번 짜두면 앵무새처럼 반복하면 되니까요",
          "original_form": "커리큘럼 한번 짜두면 앵무새처럼 반복하면 되니까요",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100012798.1.16",
          "form": "그런 경우는 전혀 없었어요",
          "original_form": "그런 경우는 전혀 없었어요",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100012798.1.17",
          "form": "대신 처음 과외 시작할때 얘가 이 과목이 너무 취약하다할때 대충 목표선은 암묵적으로 잡아두죠",
          "original_form": "대신 처음 과외 시작할때 얘가 이 과목이 너무 취약하다할때 대충 목표선은 암묵적으로 잡아두죠",
          "speaker_id": "2",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100012798.1.18",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100012798.1.19",
          "form": "직장 다니시나요?",
          "original_form": "직장 다니시나요?",
          "speaker_id": "1",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100012798.1.20",
          "form": "네 지금은 회사  노비입니다ㅜ",
          "original_form": "네 지금은 회사  노비입니다ㅜ",
          "speaker_id": "2",
          "time": "20210825 01:36"
        },
        {
          "id": "MDRW2100012798.1.21",
          "form": "name1님은 학생이신가요?",
          "original_form": "&name1&님은 학생이신가요?",
          "speaker_id": "2",
          "time": "20210825 01:37"
        },
        {
          "id": "MDRW2100012798.1.22",
