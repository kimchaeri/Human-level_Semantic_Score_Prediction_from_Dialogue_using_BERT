      "utterance": [
        {
          "id": "MDRW2100020311.1.1",
          "form": "오 저는 가디건이요 ㅋㅋㅋ",
          "original_form": "오 저는 가디건이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020311.1.2",
          "form": "한번도 안 입은 거면 팔기에도 아까울거같아요 ㅠㅠㅠ 새건데",
          "original_form": "한번도 안 입은 거면 팔기에도 아까울거같아요 ㅠㅠㅠ 새건데",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020311.1.3",
          "form": "마냥 쟁여두기엔 사놓은지도 좀 됐고, 안입을옷이라서 그냥 마음 편히 팔아버렸어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "마냥 쟁여두기엔 사놓은지도 좀 됐고, 안입을옷이라서 그냥 마음 편히 팔아버렸어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020311.1.4",
          "form": "요즘 사고싶은 물건 있으세요 ?",
          "original_form": "요즘 사고싶은 물건 있으세요 ?",
          "speaker_id": "2",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020311.1.5",
          "form": "차라리 그게 낫겠어요 ㅋㅋㅋ",
          "original_form": "차라리 그게 낫겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020311.1.6",
          "form": "저는 반지 ,,",
          "original_form": "저는 반지 ,,",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020311.1.7",
          "form": "흠 저는 옷욕심은 사도사도 끝이 없는거같아요",
          "original_form": "흠 저는 옷욕심은 사도사도 끝이 없는거같아요",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020311.1.8",
          "form": "저도 반지 좋아해요 ㅠㅠ 아 목걸이도 사고 싶어요!",
          "original_form": "저도 반지 좋아해요 ㅠㅠ 아 목걸이도 사고 싶어요!",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020311.1.9",
          "form": "아 저도 까마귀에요 반짝거리는게 좋아요",
          "original_form": "아 저도 까마귀에요 반짝거리는게 좋아요",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020311.1.10",
          "form": "동지를 만났네요 ㅋㅋㅋ",
          "original_form": "동지를 만났네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020311.1.11",
          "form": "전 목걸이 살때 줄조차 더 반짝이는걸로 골라요 ㅋㅋㅋㅋ",
          "original_form": "전 목걸이 살때 줄조차 더 반짝이는걸로 골라요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020311.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그저 반짝!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그저 반짝!",
          "speaker_id": "2",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020311.1.13",
          "form": "네 ㅋㅋㅋ화장품조차도 반짝이 좋아해요",
          "original_form": "네 ㅋㅋㅋ화장품조차도 반짝이 좋아해요",
          "speaker_id": "1",
          "time": "20210828 01:22"
        },
        {
          "id": "MDRW2100020311.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ화려한 조명이 감쌀거에요 ,,",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ화려한 조명이 감쌀거에요 ,,",
          "speaker_id": "2",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100020311.1.15",
          "form": "앜ㅋㅋㅋㅋㅋ 아이돌같네여 ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 아이돌같네여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100020311.1.16",
          "form": "최근에 선물받은거 있으세요 ??",
          "original_form": "최근에 선물받은거 있으세요 ??",
          "speaker_id": "2",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020311.1.17",
          "form": "저는 책선물받았어요",
          "original_form": "저는 책선물받았어요",
          "speaker_id": "2",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020311.1.18",
          "form": "저는 제가 산거밖에 없네욬 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 제가 산거밖에 없네욬 ㅋㅋㅋㅋㅋ",
