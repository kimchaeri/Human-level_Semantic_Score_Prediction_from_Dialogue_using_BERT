        },
        {
          "id": "MDRW2100022798.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022798.1.3",
          "form": "좋아하는 음식 있으신가요~",
          "original_form": "좋아하는 음식 있으신가요~",
          "speaker_id": "2",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022798.1.4",
          "form": "전 떡볶이랑 빵 좋아해요",
          "original_form": "전 떡볶이랑 빵 좋아해요",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022798.1.5",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022798.1.6",
          "form": "헐 저도 빵 완전 좋아해요",
          "original_form": "헐 저도 빵 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022798.1.7",
          "form": "오늘도 마들렌 하나 먹었어요!!",
          "original_form": "오늘도 마들렌 하나 먹었어요!!",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022798.1.8",
          "form": "무슨 빵 좋아하세요?",
          "original_form": "무슨 빵 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022798.1.9",
          "form": "지금은 소보로가 먹고 싶어요",
          "original_form": "지금은 소보로가 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022798.1.10",
          "form": "저는 카스테라 가루 묻혀져있는 빵이 최애입니댱",
          "original_form": "저는 카스테라 가루 묻혀져있는 빵이 최애입니댱",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022798.1.11",
          "form": "카스테라 가루만 있다면.. 모든 빵을 먹을수있어요",
          "original_form": "카스테라 가루만 있다면.. 모든 빵을 먹을수있어요",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022798.1.12",
          "form": "헐 저도 카스테라 가루 묻은 빵 진짜 좋아해요",
          "original_form": "헐 저도 카스테라 가루 묻은 빵 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022798.1.13",
          "form": "카스테라가루가 역시.. 세상을 지배한다..!!!",
          "original_form": "카스테라가루가 역시.. 세상을 지배한다..!!!",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022798.1.14",
          "form": "저녁은 뭐 드셨나요?",
          "original_form": "저녁은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022798.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022798.1.16",
          "form": "전 밥이랑 반찬 먹었어요",
          "original_form": "전 밥이랑 반찬 먹었어요",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022798.1.17",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022798.1.18",
          "form": "저는 방금 돈까스 시켜서 먹고왔습니다!!",
          "original_form": "저는 방금 돈까스 시켜서 먹고왔습니다!!",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022798.1.19",
          "form": "돈까스 좋아하세요?",
          "original_form": "돈까스 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022798.1.20",
          "form": "아뇨 전 고기는 잘 안 먹어요",
          "original_form": "아뇨 전 고기는 잘 안 먹어요",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022798.1.21",
          "form": "튀김요리는 먹고 싶네요",
          "original_form": "튀김요리는 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022798.1.22",
          "form": "아~ 혹시 채식하세요?",
          "original_form": "아~ 혹시 채식하세요?",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022798.1.23",
          "form": "지향하는 편이에요",
          "original_form": "지향하는 편이에요",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022798.1.24",
          "form": "저도 채식 시도하려고 했지만... 생각보다 고기들어간 음식이 너무 많아서 어렵더라고요..",
          "original_form": "저도 채식 시도하려고 했지만... 생각보다 고기들어간 음식이 너무 많아서 어렵더라고요..",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022798.1.25",
          "form": "실천하기가 워낙 어려워요",
          "original_form": "실천하기가 워낙 어려워요",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022798.1.26",
          "form": "그쵸 다른 사람들이랑 같이 먹어야 할 때는 아예 못 하겠더라고요",
          "original_form": "그쵸 다른 사람들이랑 같이 먹어야 할 때는 아예 못 하겠더라고요",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022798.1.27",
          "form": "맞아요 도시락을 가지고 다니는게 가장 좋은 방법인거 같은데",
          "original_form": "맞아요 도시락을 가지고 다니는게 가장 좋은 방법인거 같은데",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022798.1.28",
          "form": "채식이 주류가 되는 사회가 아니라 어렵죠..!!",
          "original_form": "채식이 주류가 되는 사회가 아니라 어렵죠..!!",
          "speaker_id": "2",
          "time": "20210829 18:55"
        }
      ]
