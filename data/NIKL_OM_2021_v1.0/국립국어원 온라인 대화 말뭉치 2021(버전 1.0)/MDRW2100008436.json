      "utterance": [
        {
          "id": "MDRW2100008436.1.1",
          "form": "오늘 돼지김치찌개 먹을 것같아요",
          "original_form": "오늘 돼지김치찌개 먹을 것같아요",
          "speaker_id": "2",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008436.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008436.1.3",
          "form": "길거리 도너츠",
          "original_form": "길거리 도너츠",
          "speaker_id": "1",
          "time": "20210822 20:00"
        },
        {
          "id": "MDRW2100008436.1.4",
          "form": "저녁은 자장면",
          "original_form": "저녁은 자장면",
          "speaker_id": "1",
          "time": "20210822 20:00"
        },
        {
          "id": "MDRW2100008436.1.5",
          "form": "많이도 먹었죠ㅋㅋ",
          "original_form": "많이도 먹었죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:00"
        },
        {
          "id": "MDRW2100008436.1.6",
          "form": "헉 저 오늘 자장면 먹을려다 말았는데",
          "original_form": "헉 저 오늘 자장면 먹을려다 말았는데",
          "speaker_id": "2",
          "time": "20210822 20:07"
        },
        {
          "id": "MDRW2100008436.1.7",
          "form": "맛있었겠어요ㅠ",
          "original_form": "맛있었겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210822 20:07"
        },
        {
          "id": "MDRW2100008436.1.8",
          "form": "집에서 해먹었어요!",
          "original_form": "집에서 해먹었어요!",
          "speaker_id": "1",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100008436.1.9",
          "form": "오오 소스도 처음부터 다 만들어서 드신 건가요?",
          "original_form": "오오 소스도 처음부터 다 만들어서 드신 건가요?",
          "speaker_id": "2",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100008436.1.10",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210824 08:07"
        },
        {
          "id": "MDRW2100008436.1.11",
          "form": "엄마가 만들어주셨어요ㅋㅋ",
          "original_form": "엄마가 만들어주셨어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 08:07"
        },
        {
          "id": "MDRW2100008436.1.12",
          "form": "오늘 아침드셨어요?",
          "original_form": "오늘 아침드셨어요?",
          "speaker_id": "1",
          "time": "20210824 08:07"
        },
        {
          "id": "MDRW2100008436.1.13",
          "form": "네 오늘 그냥 간단하게 과일이랑 떡 먹었어요",
          "original_form": "네 오늘 그냥 간단하게 과일이랑 떡 먹었어요",
          "speaker_id": "2",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100008436.1.14",
          "form": "아침 드셨나요? 이제 점심 시간이긴 하지만요ㅋㅋㅋㅋ",
          "original_form": "아침 드셨나요? 이제 점심 시간이긴 하지만요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:44"
        },
        {
          "id": "MDRW2100008436.1.15",
          "form": "아침은 바나나2개",
          "original_form": "아침은 바나나2개",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100008436.1.16",
          "form": "점심은 치즈돈까스",
          "original_form": "점심은 치즈돈까스",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100008436.1.17",
          "form": "후식은 바닐라라떼!",
          "original_form": "후식은 바닐라라떼!",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100008436.1.18",
          "form": "오늘 저녁은 뭐 드실거예요",
          "original_form": "오늘 저녁은 뭐 드실거예요",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100008436.1.19",
          "form": "점심을 늦게먹었더니 배부르네요",
          "original_form": "점심을 늦게먹었더니 배부르네요",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100008436.1.20",
          "form": "전 냉장고에 있는 재료 볶아서 볶음밥 먹었어요!",
          "original_form": "전 냉장고에 있는 재료 볶아서 볶음밥 먹었어요!",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100008436.1.21",
          "form": "치즈돈까스 맛있었겠네요ㅠ",
          "original_form": "치즈돈까스 맛있었겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100008436.1.22",
          "form": "전 개인적으로 고치돈 좋아해요",
          "original_form": "전 개인적으로 고치돈 좋아해요",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100008436.1.23",
          "form": "요리잘하세요?",
          "original_form": "요리잘하세요?",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100008436.1.24",
          "form": "똥손은 아니지만",
          "original_form": "똥손은 아니지만",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100008436.1.25",
          "form": "금손은 아니에요.....",
          "original_form": "금손은 아니에요.....",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100008436.1.26",
          "form": "저는똥손입니다ㅋㅋ",
          "original_form": "저는똥손입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100008436.1.27",
          "form": "일단 귀찮음이 엄청..ㅋㅋ",
          "original_form": "일단 귀찮음이 엄청..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100008436.1.28",
          "form": "맞아요ㅋㅋㅋ요리하는건 괜찮은데 뒷처리가 너무 귀찮아서",
          "original_form": "맞아요ㅋㅋㅋ요리하는건 괜찮은데 뒷처리가 너무 귀찮아서",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100008436.1.29",
          "form": "시작하는것도 쉽지 않은ㅋㅋㅋ",
          "original_form": "시작하는것도 쉽지 않은ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100008436.1.30",
          "form": "설거지 진짜 귀찮은거같아요",
          "original_form": "설거지 진짜 귀찮은거같아요",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100008436.1.31",
          "form": "요리는 금방하는데",
          "original_form": "요리는 금방하는데",
          "speaker_id": "1",
          "time": "20210825 14:07"
        }
      ]
    }
