        {
          "id": "MDRW2100004525.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004525.1.2",
          "form": "지금 안 오는 거 같아요",
          "original_form": "지금 안 오는 거 같아요",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004525.1.3",
          "form": "아까 비 엄청오ㅓㅆ어요",
          "original_form": "아까 비 엄청오ㅓㅆ어요",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004525.1.4",
          "form": "무서워서",
          "original_form": "무서워서",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004525.1.5",
          "form": "비와서 무서우시다구요??????",
          "original_form": "비와서 무서우시다구요??????",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004525.1.6",
          "form": "네 앞이 안보이더라구요",
          "original_form": "네 앞이 안보이더라구요",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004525.1.7",
          "form": "오우 비오는데 운전하셨나요",
          "original_form": "오우 비오는데 운전하셨나요",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004525.1.8",
          "form": "요즘 주말에 비가 많이오네요",
          "original_form": "요즘 주말에 비가 많이오네요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004525.1.9",
          "form": "네 나갈일이 있어서",
          "original_form": "네 나갈일이 있어서",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004525.1.10",
          "form": "비올때는 운전 조심하세요",
          "original_form": "비올때는 운전 조심하세요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004525.1.11",
          "form": "다시 좀 쌀쌀해져서 옷도 잘 챙겨입어야겠도라구요",
          "original_form": "다시 좀 쌀쌀해져서 옷도 잘 챙겨입어야겠도라구요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004525.1.12",
          "form": "네.그래야겠어요",
          "original_form": "네.그래야겠어요",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004525.1.13",
          "form": "어떤 계절 제일 좋아하세요",
          "original_form": "어떤 계절 제일 좋아하세요",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004525.1.14",
          "form": "봄이요",
          "original_form": "봄이요",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004525.1.15",
          "form": "봄 좋죠~!",
          "original_form": "봄 좋죠~!",
