      "utterance": [
        {
          "id": "MDRW2100039599.1.1",
          "form": "안녕하세요? 집안일하실때 즐거우시나요?",
          "original_form": "안녕하세요? 집안일하실때 즐거우시나요?",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039599.1.2",
          "form": "화장실청소는 할만한거 같아요",
          "original_form": "화장실청소는 할만한거 같아요",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039599.1.3",
          "form": "깨끗해지는거 보는맛",
          "original_form": "깨끗해지는거 보는맛",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039599.1.4",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039599.1.5",
          "form": "청소를 즐기시는 편이시군요",
          "original_form": "청소를 즐기시는 편이시군요",
          "speaker_id": "1",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039599.1.6",
          "form": "전 빨래나 설거지는 그나마 좀 해요",
          "original_form": "전 빨래나 설거지는 그나마 좀 해요",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039599.1.7",
          "form": "저는 설거지 너무 귀찮아요 ㅜㅜ",
          "original_form": "저는 설거지 너무 귀찮아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039599.1.8",
          "form": "식기세척기 갖고싶네요",
          "original_form": "식기세척기 갖고싶네요",
          "speaker_id": "2",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039599.1.9",
          "form": "전 화장실 청소 넘 싫은데요 ㅠ",
          "original_form": "전 화장실 청소 넘 싫은데요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039599.1.10",
          "form": "전 식기세척기 못 믿을것 같아요 왠지 손으로 해야 깨끗할것 같고 ㅋㅋ",
          "original_form": "전 식기세척기 못 믿을것 같아요 왠지 손으로 해야 깨끗할것 같고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039599.1.11",
          "form": "세제 투척해놓고 기다렸다 물뿌리면 씻겨요 ㅎㅎ",
          "original_form": "세제 투척해놓고 기다렸다 물뿌리면 씻겨요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039599.1.12",
          "form": "화장실요??",
          "original_form": "화장실요??",
          "speaker_id": "1",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039599.1.13",
          "form": "유투브보니 좋아보이는 느낌 ㅋㅋㅋ 광고일지",
          "original_form": "유투브보니 좋아보이는 느낌 ㅋㅋㅋ 광고일지",
          "speaker_id": "2",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039599.1.14",
          "form": "마자요 화장실",
          "original_form": "마자요 화장실",
          "speaker_id": "2",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039599.1.15",
          "form": "어떤 세제 쓰시는데요?",
          "original_form": "어떤 세제 쓰시는데요?",
          "speaker_id": "1",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039599.1.16",
          "form": "뚫어뻥용액? 그게 때를 녹이더라구요",
          "original_form": "뚫어뻥용액? 그게 때를 녹이더라구요",
          "speaker_id": "2",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039599.1.17",
          "form": "그거 변기에 붓는거 아니예요??",
          "original_form": "그거 변기에 붓는거 아니예요??",
          "speaker_id": "1",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039599.1.18",
          "form": "그걸로 가스렌지 후드 기름때도 녹인데요",
          "original_form": "그걸로 가스렌지 후드 기름때도 녹인데요",
          "speaker_id": "2",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039599.1.19",
          "form": "아!! 다용도군요~",
          "original_form": "아!! 다용도군요~",
          "speaker_id": "1",
