      "utterance": [
        {
          "id": "MDRW2100009694.1.1",
          "form": "안녕\r\n무슨 일 하나요?",
          "original_form": "안녕\r\n무슨 일 하나요?",
          "speaker_id": "2",
          "time": "20210823 04:28"
        },
        {
          "id": "MDRW2100009694.1.2",
          "form": "눈을 뜨면 월요일 출근이라는게 믿기지 않아요",
          "original_form": "눈을 뜨면 월요일 출근이라는게 믿기지 않아요",
          "speaker_id": "1",
          "time": "20210823 04:28"
        },
        {
          "id": "MDRW2100009694.1.3",
          "form": "그냥 단순 사무알바요! 학생이라서요",
          "original_form": "그냥 단순 사무알바요! 학생이라서요",
          "speaker_id": "1",
          "time": "20210823 04:28"
        },
        {
          "id": "MDRW2100009694.1.4",
          "form": "전 이직 알아보고 있어요 짬짬이 아르바이트도 하구요",
          "original_form": "전 이직 알아보고 있어요 짬짬이 아르바이트도 하구요",
          "speaker_id": "2",
          "time": "20210823 04:29"
        },
        {
          "id": "MDRW2100009694.1.5",
          "form": "월요일은 일어나기 힘들어요",
          "original_form": "월요일은 일어나기 힘들어요",
          "speaker_id": "2",
          "time": "20210823 04:29"
        },
        {
          "id": "MDRW2100009694.1.6",
          "form": "머리가 복잡하실 때 같아요 이따 아침에 출근하시나요?",
          "original_form": "머리가 복잡하실 때 같아요 이따 아침에 출근하시나요?",
          "speaker_id": "1",
          "time": "20210823 04:30"
        },
        {
          "id": "MDRW2100009694.1.7",
          "form": "아니요 오전에 자격증 공부 할 생각이에요",
          "original_form": "아니요 오전에 자격증 공부 할 생각이에요",
          "speaker_id": "2",
          "time": "20210823 04:30"
        },
        {
          "id": "MDRW2100009694.1.8",
          "form": "영어 공부도 해야하고",
          "original_form": "영어 공부도 해야하고",
          "speaker_id": "2",
          "time": "20210823 04:30"
        },
        {
          "id": "MDRW2100009694.1.9",
          "form": "사무 아르바이트면 언제 출근하나요?",
          "original_form": "사무 아르바이트면 언제 출근하나요?",
          "speaker_id": "2",
          "time": "20210823 04:31"
        },
        {
          "id": "MDRW2100009694.1.10",
          "form": "와 멋져요",
          "original_form": "와 멋져요",
          "speaker_id": "1",
          "time": "20210823 04:31"
        },
        {
          "id": "MDRW2100009694.1.11",
          "form": "저 9시 출근이요!",
          "original_form": "저 9시 출근이요!",
          "speaker_id": "1",
          "time": "20210823 04:31"
        },
        {
          "id": "MDRW2100009694.1.12",
          "form": "망했어요ㅎ",
          "original_form": "망했어요ㅎ",
          "speaker_id": "1",
          "time": "20210823 04:31"
        },
        {
          "id": "MDRW2100009694.1.13",
          "form": "잠 자기엔 너무 늦은 시간이네요",
          "original_form": "잠 자기엔 너무 늦은 시간이네요",
          "speaker_id": "2",
          "time": "20210823 04:31"
        },
        {
          "id": "MDRW2100009694.1.14",
          "form": "피곤하겠어요",
          "original_form": "피곤하겠어요",
          "speaker_id": "2",
          "time": "20210823 04:31"
        },
        {
          "id": "MDRW2100009694.1.15",
          "form": "일은 힘들지 않나요?",
          "original_form": "일은 힘들지 않나요?",
          "speaker_id": "2",
          "time": "20210823 04:32"
        },
        {
          "id": "MDRW2100009694.1.16",
          "form": "출근하면 비타민이랑 카페인 때려넣고 정신차리려구요..",
          "original_form": "출근하면 비타민이랑 카페인 때려넣고 정신차리려구요..",
          "speaker_id": "1",
          "time": "20210823 04:32"
        },
        {
          "id": "MDRW2100009694.1.17",
          "form": "구청에서 하는 단순 알바라 힘들진않아요!",
          "original_form": "구청에서 하는 단순 알바라 힘들진않아요!",
          "speaker_id": "1",
          "time": "20210823 04:33"
        },
        {
          "id": "MDRW2100009694.1.18",
          "form": "역시 카페인이 최고에요",
          "original_form": "역시 카페인이 최고에요",
          "speaker_id": "2",
          "time": "20210823 04:33"
        },
        {
          "id": "MDRW2100009694.1.19",
          "form": "평일에 그거 하고 주말에 편의점 알바하구",
          "original_form": "평일에 그거 하고 주말에 편의점 알바하구",
          "speaker_id": "1",
          "time": "20210823 04:33"
        },
        {
          "id": "MDRW2100009694.1.20",
          "form": "마치 포션같은 ㅋㅋㅋ 없으면 안되요",
          "original_form": "마치 포션같은 ㅋㅋㅋ 없으면 안되요",
          "speaker_id": "2",
          "time": "20210823 04:33"
        },
        {
          "id": "MDRW2100009694.1.21",
          "form": "와우 바쁘게 사네요",
          "original_form": "와우 바쁘게 사네요",
          "speaker_id": "2",
          "time": "20210823 04:33"
        },
        {
          "id": "MDRW2100009694.1.22",
          "form": "맞아요 어릴땐 커피우유도 쓰다고 못먹었는데 이제 아아없음 못살아요ㅋㅎ",
          "original_form": "맞아요 어릴땐 커피우유도 쓰다고 못먹었는데 이제 아아없음 못살아요ㅋㅎ",
          "speaker_id": "1",
          "time": "20210823 04:34"
        }
      ]
    }
