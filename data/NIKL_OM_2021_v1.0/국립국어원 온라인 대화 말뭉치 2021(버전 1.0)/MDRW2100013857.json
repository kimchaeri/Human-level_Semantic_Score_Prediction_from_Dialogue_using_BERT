      "utterance": [
        {
          "id": "MDRW2100013857.1.1",
          "form": "요즘 비가 자주 와서 습하네요",
          "original_form": "요즘 비가 자주 와서 습하네요",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013857.1.2",
          "form": "그러게요 요즘 온도가 좀 내려갔는데도 습해서 에어컨 가끔 켜요",
          "original_form": "그러게요 요즘 온도가 좀 내려갔는데도 습해서 에어컨 가끔 켜요",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013857.1.3",
          "form": "요즘 날씨가 에어컨 끄면 덥고 켜면 춥네요",
          "original_form": "요즘 날씨가 에어컨 끄면 덥고 켜면 춥네요",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013857.1.4",
          "form": "그쵸 습해서 빨래도 뽀송하지가 않아요",
          "original_form": "그쵸 습해서 빨래도 뽀송하지가 않아요",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013857.1.5",
          "form": "한여름이면 습해도 잘 마르던데 가을장마 야속하네요",
          "original_form": "한여름이면 습해도 잘 마르던데 가을장마 야속하네요",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013857.1.6",
          "form": "이번주 내내 비온다고 하더라구요",
          "original_form": "이번주 내내 비온다고 하더라구요",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013857.1.7",
          "form": "당분간은 그렇겠어요ㅠ",
          "original_form": "당분간은 그렇겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013857.1.8",
          "form": "이번주 내내요? 우산 잘 챙겨야겠네요",
          "original_form": "이번주 내내요? 우산 잘 챙겨야겠네요",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013857.1.9",
          "form": "차라리 비가 확 오면 좀 덜 습할 거 같은데 시원하게 내리지도 않네요 ㅋㅋ",
          "original_form": "차라리 비가 확 오면 좀 덜 습할 거 같은데 시원하게 내리지도 않네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013857.1.10",
          "form": "날씨가 애매해요ㅋㅋ 일교차도 커졌어요",
          "original_form": "날씨가 애매해요ㅋㅋ 일교차도 커졌어요",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013857.1.11",
          "form": "그러게요 그래도 열대야는 끝나서 다행이에요 한동안 밤에도 힘들었는데",
          "original_form": "그러게요 그래도 열대야는 끝나서 다행이에요 한동안 밤에도 힘들었는데",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013857.1.12",
          "form": "요즘은 누워있음 추워서 창문 아예 닫고 자요",
          "original_form": "요즘은 누워있음 추워서 창문 아예 닫고 자요",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013857.1.13",
          "form": "맞아요 밤에 창문열고 선풍기 틀면 춥더라구요",
          "original_form": "맞아요 밤에 창문열고 선풍기 틀면 춥더라구요",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013857.1.14",
          "form": "그래서 잘땐 닫고자요 아침에 넘 추워요 ㅋㅋ",
          "original_form": "그래서 잘땐 닫고자요 아침에 넘 추워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013857.1.15",
          "form": "가을이 오긴 오나봐요",
          "original_form": "가을이 오긴 오나봐요",
          "speaker_id": "1",
          "time": "20210825 16:46"
        }
      ]
    }
