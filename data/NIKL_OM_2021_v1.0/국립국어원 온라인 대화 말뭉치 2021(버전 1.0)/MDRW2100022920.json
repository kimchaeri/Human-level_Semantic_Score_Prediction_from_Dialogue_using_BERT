      "utterance": [
        {
          "id": "MDRW2100022920.1.1",
          "form": "없어",
          "original_form": "없어",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022920.1.2",
          "form": "없어?",
          "original_form": "없어?",
          "speaker_id": "1",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100022920.1.3",
          "form": "응 소개시켜줘",
          "original_form": "응 소개시켜줘",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100022920.1.4",
          "form": "나어때?",
          "original_form": "나어때?",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022920.1.5",
          "form": "너 어떤데?",
          "original_form": "너 어떤데?",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022920.1.6",
          "form": "나 착하고 잘생기고 섹시하고 멋지고 매너있고 개념있고 게임잘하고 운동도 잘해",
          "original_form": "나 착하고 잘생기고 섹시하고 멋지고 매너있고 개념있고 게임잘하고 운동도 잘해",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022920.1.7",
          "form": "나 게임잘하고 운동 잘하는 사람이 이상형이야",
          "original_form": "나 게임잘하고 운동 잘하는 사람이 이상형이야",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022920.1.8",
          "form": "그게 나야",
          "original_form": "그게 나야",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022920.1.9",
          "form": "게임잘하고 운동잘해",
          "original_form": "게임잘하고 운동잘해",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022920.1.10",
          "form": "무슨 게임을 잘하는데?",
          "original_form": "무슨 게임을 잘하는데?",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022920.1.11",
          "form": "담주부터 운동 다시할거야 아야아야하거든",
          "original_form": "담주부터 운동 다시할거야 아야아야하거든",
          "speaker_id": "1",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022920.1.12",
          "form": "나 다잘해",
          "original_form": "나 다잘해",
          "speaker_id": "1",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022920.1.13",
          "form": "일단 fps게임",
          "original_form": "일단 fps게임",
          "speaker_id": "1",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022920.1.14",
          "form": "총게임 좋아해?",
          "original_form": "총게임 좋아해?",
          "speaker_id": "2",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022920.1.15",
          "form": "총게임도 총게임인데 그치 총게임이지",
          "original_form": "총게임도 총게임인데 그치 총게임이지",
          "speaker_id": "1",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022920.1.16",
          "form": "운동은 무슨 운동해?",
          "original_form": "운동은 무슨 운동해?",
          "speaker_id": "2",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022920.1.17",
          "form": "운동은 웨이트! 축구!",
          "original_form": "운동은 웨이트! 축구!",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022920.1.18",
          "form": "배드민턴! 은 안친지 오래됐다",
          "original_form": "배드민턴! 은 안친지 오래됐다",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022920.1.19",
          "form": "아 축구 잘해?",
          "original_form": "아 축구 잘해?",
          "speaker_id": "2",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022920.1.20",
          "form": "잘해잘해",
          "original_form": "잘해잘해",
          "speaker_id": "1",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022920.1.21",
          "form": "너는 이상형이 어떻게 돼?",
          "original_form": "너는 이상형이 어떻게 돼?",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022920.1.22",
          "form": "난 이쁘고 섹시하고 귀엽고 재롱둥이인애",
          "original_form": "난 이쁘고 섹시하고 귀엽고 재롱둥이인애",
          "speaker_id": "1",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022920.1.23",
          "form": "바라는게많네",
          "original_form": "바라는게많네",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022920.1.24",
          "form": "바라는게 많으면 어때 이상형은 이상형일뿐",
          "original_form": "바라는게 많으면 어때 이상형은 이상형일뿐",
          "speaker_id": "1",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022920.1.25",
          "form": "외모말고 능력은? 게임이나 운동이나 공부 같은거",
          "original_form": "외모말고 능력은? 게임이나 운동이나 공부 같은거",
          "speaker_id": "2",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022920.1.26",
          "form": "도라에몽처럼 이것저것 잘 챙겨주는 스타일",
          "original_form": "도라에몽처럼 이것저것 잘 챙겨주는 스타일",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022920.1.27",
          "form": "다른건?",
          "original_form": "다른건?",
          "speaker_id": "2",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022920.1.28",
          "form": "운동좀 하면 좋을거같은데",
          "original_form": "운동좀 하면 좋을거같은데",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022920.1.29",
          "form": "아 나네",
          "original_form": "아 나네",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100022920.1.30",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "1",
          "time": "20210829 19:17"
        }
      ]
    }
  ]
