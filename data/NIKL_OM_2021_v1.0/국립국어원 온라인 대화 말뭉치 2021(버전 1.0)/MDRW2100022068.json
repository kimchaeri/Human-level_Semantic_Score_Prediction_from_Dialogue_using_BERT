      "utterance": [
        {
          "id": "MDRW2100022068.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 01:59"
        },
        {
          "id": "MDRW2100022068.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100022068.1.3",
          "form": "싸이월드 부활하는거 알고 계셨나요??",
          "original_form": "싸이월드 부활하는거 알고 계셨나요??",
          "speaker_id": "1",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100022068.1.4",
          "form": "네! 알고 있었어요ㅎㅎ",
          "original_form": "네! 알고 있었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100022068.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ 예전에 싸이월드 하셨었나요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 예전에 싸이월드 하셨었나요??",
          "speaker_id": "1",
          "time": "20210829 02:05"
        },
        {
          "id": "MDRW2100022068.1.6",
          "form": "저는 중학생때 잠깐 했던거 같아요~",
          "original_form": "저는 중학생때 잠깐 했던거 같아요~",
          "speaker_id": "1",
          "time": "20210829 02:05"
        },
        {
          "id": "MDRW2100022068.1.7",
          "form": "저도 중학생때 잠시 했었는데, 미니홈피 만들어두기만하고 사진이나 글 같은건 하나도 안 올렸었어요~",
          "original_form": "저도 중학생때 잠시 했었는데, 미니홈피 만들어두기만하고 사진이나 글 같은건 하나도 안 올렸었어요~",
          "speaker_id": "2",
          "time": "20210829 02:06"
        },
        {
          "id": "MDRW2100022068.1.8",
          "form": "오 조금 부럽네요...!!",
          "original_form": "오 조금 부럽네요...!!",
          "speaker_id": "1",
          "time": "20210829 02:07"
        },
        {
          "id": "MDRW2100022068.1.9",
          "form": "저는 많이는 아니지만 올렸었는데 나중에 지우느라 진땀뺐던 기억이 있어요 ㅋㅋㅋㅋ",
          "original_form": "저는 많이는 아니지만 올렸었는데 나중에 지우느라 진땀뺐던 기억이 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:07"
        },
        {
          "id": "MDRW2100022068.1.10",
          "form": "그래서 지금은 아마 하나도 없을텐데",
          "original_form": "그래서 지금은 아마 하나도 없을텐데",
          "speaker_id": "1",
          "time": "20210829 02:08"
        },
        {
          "id": "MDRW2100022068.1.11",
          "form": "아ㅋㅋㅋㅋ 그래서 부럽다고 하신거군요ㅎㅎ",
          "original_form": "아ㅋㅋㅋㅋ 그래서 부럽다고 하신거군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:08"
        },
        {
          "id": "MDRW2100022068.1.12",
          "form": "내용이 나빠서라기보단...",
          "original_form": "내용이 나빠서라기보단...",
          "speaker_id": "1",
          "time": "20210829 02:08"
        },
        {
          "id": "MDRW2100022068.1.13",
          "form": "그냥 오글거리는게 많아서?? 그랬던거 같아요ㅜㅜ",
          "original_form": "그냥 오글거리는게 많아서?? 그랬던거 같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 02:08"
        },
        {
          "id": "MDRW2100022068.1.14",
          "form": "뭔가 흑역사 같은 느낌?",
          "original_form": "뭔가 흑역사 같은 느낌?",
          "speaker_id": "2",
          "time": "20210829 02:08"
        },
        {
          "id": "MDRW2100022068.1.15",
          "form": "약간 옛날 지식인 질문 보는 느낌...",
          "original_form": "약간 옛날 지식인 질문 보는 느낌...",
          "speaker_id": "1",
          "time": "20210829 02:08"
        },
        {
          "id": "MDRW2100022068.1.16",
          "form": "맞아요... 그런거죠...ㅜㅜ",
          "original_form": "맞아요... 그런거죠...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100022068.1.17",
          "form": "저라도 별로 남겨두고 싶지 않았을거 같아요",
          "original_form": "저라도 별로 남겨두고 싶지 않았을거 같아요",
          "speaker_id": "2",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100022068.1.18",
          "form": "사실 지식인 질문은 삭제가 안돼서 아직도 고통받고 있답니다",
          "original_form": "사실 지식인 질문은 삭제가 안돼서 아직도 고통받고 있답니다",
          "speaker_id": "1",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100022068.1.19",
          "form": "싸이월드가 그나마 나았던 거에요...",
          "original_form": "싸이월드가 그나마 나았던 거에요...",
          "speaker_id": "1",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100022068.1.20",
          "form": "그래도 그때 사진 보면 추억 생각나기도 하고",
          "original_form": "그래도 그때 사진 보면 추억 생각나기도 하고",
          "speaker_id": "1",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100022068.1.21",
          "form": "싸이월드 부활하고서 다시 들어가보셨어요?",
          "original_form": "싸이월드 부활하고서 다시 들어가보셨어요?",
          "speaker_id": "2",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100022068.1.22",
          "form": "이렇게 화질이 안좋았던 적이 있었나... 생각도 들어요",
          "original_form": "이렇게 화질이 안좋았던 적이 있었나... 생각도 들어요",
          "speaker_id": "1",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100022068.1.23",
          "form": "사진들보면 추억 떠오르죠~",
          "original_form": "사진들보면 추억 떠오르죠~",
          "speaker_id": "2",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100022068.1.24",
          "form": "아니요! 저는 이미 다 지워서 지울 때 사진 몇개 갤러리에 남겨뒀거든요 ㅋㅋㅋㅋㅋ",
          "original_form": "아니요! 저는 이미 다 지워서 지울 때 사진 몇개 갤러리에 남겨뒀거든요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:13"
        },
        {
          "id": "MDRW2100022068.1.25",
          "form": "나름 디카로 찍었던 건데도 화질이 웬만한 스마트폰보다 못하더라구요...!",
          "original_form": "나름 디카로 찍었던 건데도 화질이 웬만한 스마트폰보다 못하더라구요...!",
          "speaker_id": "1",
          "time": "20210829 02:13"
        },
        {
          "id": "MDRW2100022068.1.26",
          "form": "시간 많이 흘렀다 싶다가도... 10년정도만에 기술도 많이 발전하고 세상도 많이 바뀌었어요...!!",
          "original_form": "시간 많이 흘렀다 싶다가도... 10년정도만에 기술도 많이 발전하고 세상도 많이 바뀌었어요...!!",
          "speaker_id": "1",
          "time": "20210829 02:14"
        },
        {
          "id": "MDRW2100022068.1.27",
          "form": "예전 사진들과 지금 사진들의 화질 차이는 크죠\r\n요즘은 스마트폰 화질도 넘 좋죠",
          "original_form": "예전 사진들과 지금 사진들의 화질 차이는 크죠\r\n요즘은 스마트폰 화질도 넘 좋죠",
          "speaker_id": "2",
          "time": "20210829 02:14"
        },
        {
          "id": "MDRW2100022068.1.28",
          "form": "맞아요 제 핸드폰은 이제 하늘에 구름까지 잡더라고요!",
          "original_form": "맞아요 제 핸드폰은 이제 하늘에 구름까지 잡더라고요!",
          "speaker_id": "1",
          "time": "20210829 02:15"
        }
      ]
    }
