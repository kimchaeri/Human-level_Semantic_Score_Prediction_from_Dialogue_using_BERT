      "utterance": [
        {
          "id": "MDRW2100034474.1.1",
          "form": "전 집에서는 영화를 잘 못보겟더라구요 ㅠㅠ가끔 보긴 하는데",
          "original_form": "전 집에서는 영화를 잘 못보겟더라구요 ㅠㅠ가끔 보긴 하는데",
          "speaker_id": "2",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100034474.1.2",
          "form": "집중력이 약한가봐욬ㅋㅋㅋㅋ",
          "original_form": "집중력이 약한가봐욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100034474.1.3",
          "form": "저도 거의 배경음악 수준으러 틀어놓게되더라구요 ㅋㅋㅋ",
          "original_form": "저도 거의 배경음악 수준으러 틀어놓게되더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100034474.1.4",
          "form": "얼마전에 미드소마를 집에서 봤는데",
          "original_form": "얼마전에 미드소마를 집에서 봤는데",
          "speaker_id": "1",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100034474.1.5",
          "form": "너무 집중이 안되었어요 ㅋㅋㅋ",
          "original_form": "너무 집중이 안되었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100034474.1.6",
          "form": "맞아요 영화관에 앉아서 강제로 집중하게 되는 그게 좋아요",
          "original_form": "맞아요 영화관에 앉아서 강제로 집중하게 되는 그게 좋아요",
          "speaker_id": "2",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100034474.1.7",
          "form": "집에서 영화보다가 조금 늘어진다 싶으면 저도모르게 폰 보게되고",
          "original_form": "집에서 영화보다가 조금 늘어진다 싶으면 저도모르게 폰 보게되고",
          "speaker_id": "2",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100034474.1.8",
          "form": "마자여 그래서 뭐 데이트 하는 거 아니고서는 걍 예능만 보게되네여ㅋㅋㅋ",
          "original_form": "마자여 그래서 뭐 데이트 하는 거 아니고서는 걍 예능만 보게되네여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034474.1.9",
          "form": "근데 또 드라마는 잘 봐요 저는ㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 또 드라마는 잘 봐요 저는ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100034474.1.10",
          "form": "예능은 뭐보세요?",
          "original_form": "예능은 뭐보세요?",
          "speaker_id": "2",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100034474.1.11",
          "form": "저 유퀴즈랑 대탈출!",
          "original_form": "저 유퀴즈랑 대탈출!",
          "speaker_id": "1",
          "time": "20210828 13:54"
        },
        {
          "id": "MDRW2100034474.1.12",
          "form": "유퀴즈 저도 봐요 ㅋㅋㅋㅋㅋ",
          "original_form": "유퀴즈 저도 봐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:54"
        },
        {
          "id": "MDRW2100034474.1.13",
          "form": "대탈출이 그렇게 재밋다면서요??",
          "original_form": "대탈출이 그렇게 재밋다면서요??",
          "speaker_id": "2",
          "time": "20210828 13:54"
        },
        {
          "id": "MDRW2100034474.1.14",
          "form": "네ㅋㅋㅋ 진짜 피디의 연출력이 너무 감탄나와요",
          "original_form": "네ㅋㅋㅋ 진짜 피디의 연출력이 너무 감탄나와요",
          "speaker_id": "1",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100034474.1.15",
          "form": "처음부터 각잡고 보려고 벼르고 잇는중인데 선뜻 엄두가 안나요 ㅋㅋㅋㅋㅋ",
          "original_form": "처음부터 각잡고 보려고 벼르고 잇는중인데 선뜻 엄두가 안나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:57"
        },
        {
          "id": "MDRW2100034474.1.16",
          "form": "근데 보기 시작하면 금방 봐요",
          "original_form": "근데 보기 시작하면 금방 봐요",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100034474.1.17",
          "form": "아까워요 그래서ㅋㅋㅋㅋ",
          "original_form": "아까워요 그래서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100034474.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ조만간 봐야겟어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ조만간 봐야겟어요",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100034474.1.19",
          "form": "계속 봐야지 봐야지 하면서 크라임씬만 재탕햇지뭐예요",
          "original_form": "계속 봐야지 봐야지 하면서 크라임씬만 재탕햇지뭐예요",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100034474.1.20",
          "form": "크 저는 크라임씬을 안봤어요 그것도 꽤 마니아가 많던대",
          "original_form": "크 저는 크라임씬을 안봤어요 그것도 꽤 마니아가 많던대",
          "speaker_id": "1",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100034474.1.21",
          "form": "데..",
          "original_form": "데..",
          "speaker_id": "1",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100034474.1.22",
          "form": "그 매니아가 접니다....ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 매니아가 접니다....ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100034474.1.23",
          "form": "ㅋㅋㅋㅋ저도 크라임씬을 봐야겠다고 맨날 생각만 하고 있어요",
          "original_form": "ㅋㅋㅋㅋ저도 크라임씬을 봐야겠다고 맨날 생각만 하고 있어요",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100034474.1.24",
          "form": "다음시즌 기약도 없어서 ㅠㅠ",
          "original_form": "다음시즌 기약도 없어서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100034474.1.25",
          "form": "봣던거 재탕하고 ㅠㅠ",
          "original_form": "봣던거 재탕하고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100034474.1.26",
          "form": "그러게요 대탈출이랑 같은 감독이죠?",
          "original_form": "그러게요 대탈출이랑 같은 감독이죠?",
          "speaker_id": "1",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100034474.1.27",
          "form": "대탈출이랑 여고추리반에 집중하시는 것 같네요ㅋㅋ",
          "original_form": "대탈출이랑 여고추리반에 집중하시는 것 같네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100034474.1.28",
          "form": "아 대탈출감독은",
          "original_form": "아 대탈출감독은",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100034474.1.29",
          "form": "지니어스 감독으로 알고잇구요",
          "original_form": "지니어스 감독으로 알고잇구요",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100034474.1.30",
          "form": "크라임씬 감독은 슈가맨감독으로 알고잇어요",
          "original_form": "크라임씬 감독은 슈가맨감독으로 알고잇어요",
          "speaker_id": "2",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100034474.1.31",
          "form": "아아 지니어스구나",
          "original_form": "아아 지니어스구나",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100034474.1.32",
          "form": "그럼 왜 안 하시는 ㅜㅜ",
          "original_form": "그럼 왜 안 하시는 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100034474.1.33",
          "form": "제작진이 너무 고생한다더라구요 ㅠㅠ",
          "original_form": "제작진이 너무 고생한다더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100034474.1.34",
          "form": "크라임씬도 준비할게 많긴 하겠네요",
          "original_form": "크라임씬도 준비할게 많긴 하겠네요",
          "speaker_id": "1",
          "time": "20210828 14:18"
        }
      ]
