      "utterance": [
        {
          "id": "MDRW2100016304.1.1",
          "form": "방탄소년단의 어떤 점이 가장 좋은가요",
          "original_form": "방탄소년단의 어떤 점이 가장 좋은가요",
          "speaker_id": "2",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100016304.1.2",
          "form": "강아지 영상 주로 어떤거 보세요",
          "original_form": "강아지 영상 주로 어떤거 보세요",
          "speaker_id": "2",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016304.1.3",
          "form": "그냥 다 보는데",
          "original_form": "그냥 다 보는데",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016304.1.4",
          "form": "모카우유는 구독중이에요",
          "original_form": "모카우유는 구독중이에요",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016304.1.5",
          "form": "모카우유님은 어떤 종을 기르시나요",
          "original_form": "모카우유님은 어떤 종을 기르시나요",
          "speaker_id": "2",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016304.1.6",
          "form": "다른 채널은 그냥 알고리즘에 뜨는걸로만 봐요",
          "original_form": "다른 채널은 그냥 알고리즘에 뜨는걸로만 봐요",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016304.1.7",
          "form": "아 찾아보니 스피치  맞나요",
          "original_form": "아 찾아보니 스피치  맞나요",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016304.1.8",
          "form": "모카는 갈색포메라니안이고 우유는 사모예드인데 외국에 살아요",
          "original_form": "모카는 갈색포메라니안이고 우유는 사모예드인데 외국에 살아요",
          "speaker_id": "1",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016304.1.9",
          "form": "아니었구나",
          "original_form": "아니었구나",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016304.1.10",
          "form": "ㅋㅋㅋㅋㅋ헷갈릴수 있죠",
          "original_form": "ㅋㅋㅋㅋㅋ헷갈릴수 있죠",
          "speaker_id": "1",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016304.1.11",
          "form": "앗..저의 이렇게 얕은지식이..",
          "original_form": "앗..저의 이렇게 얕은지식이..",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016304.1.12",
          "form": "너무 귀엽겠다",
          "original_form": "너무 귀엽겠다",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016304.1.13",
          "form": "약간 복슬복슬 털 많은 거 좋아하시나봐요",
          "original_form": "약간 복슬복슬 털 많은 거 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016304.1.14",
          "form": "진짜 귀여워요 네",
          "original_form": "진짜 귀여워요 네",
          "speaker_id": "1",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016304.1.15",
          "form": "복실복실 좋아합니다",
          "original_form": "복실복실 좋아합니다",
          "speaker_id": "1",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016304.1.16",
          "form": "검색했더니 사모예드 가장먼저 뜨는데 너무 귀엽네요",
          "original_form": "검색했더니 사모예드 가장먼저 뜨는데 너무 귀엽네요",
          "speaker_id": "2",
