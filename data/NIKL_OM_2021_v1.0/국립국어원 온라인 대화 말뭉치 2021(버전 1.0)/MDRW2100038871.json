      "utterance": [
        {
          "id": "MDRW2100038871.1.1",
          "form": "600년동안 사는 요괴는 이진욱인갑다",
          "original_form": "600년동안 사는 요괴는 이진욱인갑다",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100038871.1.2",
          "form": "아 이진욱 괜찮은데",
          "original_form": "아 이진욱 괜찮은데",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100038871.1.3",
          "form": "이 요괴가 구미호뎐에서 잠깐 나오 캐릭터엿데",
          "original_form": "이 요괴가 구미호뎐에서 잠깐 나오 캐릭터엿데",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100038871.1.4",
          "form": "권나라랑 안 어울릴거 같아요",
          "original_form": "권나라랑 안 어울릴거 같아요",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100038871.1.5",
          "form": "쇠를 먹고 살며 불로 죽 일 수 있는 요괴 불가살",
          "original_form": "쇠를 먹고 살며 불로 죽 일 수 있는 요괴 불가살",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100038871.1.6",
          "form": "그럼 사극인거예요?",
          "original_form": "그럼 사극인거예요?",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100038871.1.7",
          "form": "그런가본데",
          "original_form": "그런가본데",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100038871.1.8",
          "form": "재밌을거 같기도 아닐거 같기도 하네요",
          "original_form": "재밌을거 같기도 아닐거 같기도 하네요",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100038871.1.9",
          "form": "무관이 되어 왕조를 지키다가 요괴가 되었다니까",
          "original_form": "무관이 되어 왕조를 지키다가 요괴가 되었다니까",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100038871.1.10",
          "form": "그럼 사극이네요",
          "original_form": "그럼 사극이네요",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100038871.1.11",
          "form": "처음에는 원빈한테 캐스팅 제안을 했데",
          "original_form": "처음에는 원빈한테 캐스팅 제안을 했데",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100038871.1.12",
          "form": "어머?  또 거절한거네요",
          "original_form": "어머?  또 거절한거네요",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100038871.1.13",
          "form": "뭐 맨날 거절이지 ㅋㅋ",
          "original_form": "뭐 맨날 거절이지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100038871.1.14",
          "form": "광고만 찍고 있네요.",
          "original_form": "광고만 찍고 있네요.",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100038871.1.15",
          "form": "똑같은 요괴가 또 나오네",
          "original_form": "똑같은 요괴가 또 나오네",
          "speaker_id": "2",
