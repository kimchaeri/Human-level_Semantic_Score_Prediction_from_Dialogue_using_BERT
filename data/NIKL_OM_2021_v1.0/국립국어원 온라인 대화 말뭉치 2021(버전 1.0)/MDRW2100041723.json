      "utterance": [
        {
          "id": "MDRW2100041723.1.1",
          "form": "맞아요 그래서 못시켜요ㅋㅋ",
          "original_form": "맞아요 그래서 못시켜요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:01"
        },
        {
          "id": "MDRW2100041723.1.2",
          "form": "배송료도 삼천원인데 너무해욬ㅋㅋㅋ",
          "original_form": "배송료도 삼천원인데 너무해욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:02"
        },
        {
          "id": "MDRW2100041723.1.3",
          "form": "그러니까요 배민1이라는거 생겨서 더비싸자나요",
          "original_form": "그러니까요 배민1이라는거 생겨서 더비싸자나요",
          "speaker_id": "2",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100041723.1.4",
          "form": "그건 또 뭔가요??",
          "original_form": "그건 또 뭔가요??",
          "speaker_id": "1",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100041723.1.5",
          "form": "그런게 생겼나보네요 제가 요즘은 요기요에서 시키느라정신이 없는 사이에",
          "original_form": "그런게 생겼나보네요 제가 요즘은 요기요에서 시키느라정신이 없는 사이에",
          "speaker_id": "1",
          "time": "20210906 15:04"
        },
        {
          "id": "MDRW2100041723.1.6",
          "form": "한번에 한집만 간다고 배달비 더비싸요",
          "original_form": "한번에 한집만 간다고 배달비 더비싸요",
          "speaker_id": "2",
          "time": "20210906 15:05"
        },
        {
          "id": "MDRW2100041723.1.7",
          "form": "근데 그배달원이 가게에 오는게 늦어서 실제론 더늦어여ㅋㅋㄲ",
          "original_form": "근데 그배달원이 가게에 오는게 늦어서 실제론 더늦어여ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210906 15:05"
        },
        {
          "id": "MDRW2100041723.1.8",
          "form": "ㅋㅋㅋㅋㅋ 그거 과연 한집만 올까요",
          "original_form": "ㅋㅋㅋㅋㅋ 그거 과연 한집만 올까요",
          "speaker_id": "1",
          "time": "20210906 15:05"
        },
        {
          "id": "MDRW2100041723.1.9",
          "form": "아마 근처 갈일 있는거 끼리 모아서 갈거 같은데욬ㅋㅋ",
          "original_form": "아마 근처 갈일 있는거 끼리 모아서 갈거 같은데욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100041723.1.10",
          "form": "근데 위치가 바로 표시되긴하더라그요",
          "original_form": "근데 위치가 바로 표시되긴하더라그요",
          "speaker_id": "2",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100041723.1.11",
          "form": "아 그래요??",
          "original_form": "아 그래요??",
          "speaker_id": "1",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100041723.1.12",
          "form": "네 근데저번에 시켯는데 두시간지나도 안와서 보니까",
          "original_form": "네 근데저번에 시켯는데 두시간지나도 안와서 보니까",
          "speaker_id": "2",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100041723.1.13",
          "form": "아직 배달원이 가게에 안왓대요",
          "original_form": "아직 배달원이 가게에 안왓대요",
          "speaker_id": "2",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100041723.1.14",
          "form": "ㅋㅋㅋ 배달 바로 안오는게 많으니까 그런거까지 생기네요",
          "original_form": "ㅋㅋㅋ 배달 바로 안오는게 많으니까 그런거까지 생기네요",
          "speaker_id": "1",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100041723.1.15",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100041723.1.16",
          "form": "두시간???",
          "original_form": "두시간???",
          "speaker_id": "1",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100041723.1.17",
          "form": "진짜 오래 기다리셨네요",
          "original_form": "진짜 오래 기다리셨네요",
          "speaker_id": "1",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100041723.1.18",
          "form": "취소한댓는데 퀵으로 보낸대서",
          "original_form": "취소한댓는데 퀵으로 보낸대서",
          "speaker_id": "2",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100041723.1.19",
          "form": "드셨어요??",
          "original_form": "드셨어요??",
          "speaker_id": "1",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100041723.1.20",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100041723.1.21",
          "form": "다 식어서 온거 아닌가여 ㅠㅠ",
          "original_form": "다 식어서 온거 아닌가여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100041723.1.22",
          "form": "괜히 기다리느라 고생하셨네요",
          "original_form": "괜히 기다리느라 고생하셨네요",
          "speaker_id": "1",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100041723.1.23",
          "form": "그러니까요 배민 실망",
          "original_form": "그러니까요 배민 실망",
          "speaker_id": "2",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100041723.1.24",
          "form": "진짜 실망하실만 해요",
          "original_form": "진짜 실망하실만 해요",
          "speaker_id": "1",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100041723.1.25",
          "form": "그냥 시키는게 낫겠네요",
          "original_form": "그냥 시키는게 낫겠네요",
          "speaker_id": "1",
          "time": "20210906 15:09"
        }
      ]
    }
