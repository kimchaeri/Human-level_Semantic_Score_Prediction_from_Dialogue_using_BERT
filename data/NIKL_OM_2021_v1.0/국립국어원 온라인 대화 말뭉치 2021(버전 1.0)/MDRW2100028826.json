      "utterance": [
        {
          "id": "MDRW2100028826.1.1",
          "form": "아 근데 그분이",
          "original_form": "아 근데 그분이",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028826.1.2",
          "form": "모솔이었대요",
          "original_form": "모솔이었대요",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028826.1.3",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028826.1.4",
          "form": "34에..?",
          "original_form": "34에..?",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028826.1.5",
          "form": "그럼문제잇는거다",
          "original_form": "그럼문제잇는거다",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028826.1.6",
          "form": "코스프렌지 뭔지는 모르겠지만",
          "original_form": "코스프렌지 뭔지는 모르겠지만",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028826.1.7",
          "form": "어디하나 모자란거에요 ㅋㅋㅋ",
          "original_form": "어디하나 모자란거에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.8",
          "form": "암튼 따로 6번 만났는데",
          "original_form": "암튼 따로 6번 만났는데",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.9",
          "form": "이 누나가 카톡을 잘 안함..",
          "original_form": "이 누나가 카톡을 잘 안함..",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.10",
          "form": "전화를잘하나?",
          "original_form": "전화를잘하나?",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.11",
          "form": "원래 안하는 스타일이라는데",
          "original_form": "원래 안하는 스타일이라는데",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.12",
          "form": "그래야했나..",
          "original_form": "그래야했나..",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.13",
          "form": "나도남친이랑 카톡별류안함",
          "original_form": "나도남친이랑 카톡별류안함",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.14",
          "form": "전화많이험",
          "original_form": "전화많이험",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.15",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.16",
          "form": "글쿤",
          "original_form": "글쿤",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.17",
          "form": "시도때도없이전화함",
          "original_form": "시도때도없이전화함",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028826.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028826.1.20",
          "form": "전화는 급하거나 먼일 있거나 할때했고",
          "original_form": "전화는 급하거나 먼일 있거나 할때했고",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028826.1.21",
          "form": "카톡이야 뭐 해달라고하면되는거고",
          "original_form": "카톡이야 뭐 해달라고하면되는거고",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028826.1.22",
          "form": "좋으면 하게되어잇음",
          "original_form": "좋으면 하게되어잇음",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028826.1.23",
          "form": "잠만요",
          "original_form": "잠만요",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028826.1.24",
          "form": "엄마가 커피랑",
          "original_form": "엄마가 커피랑",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028826.1.25",
          "form": "디저트사옴",
          "original_form": "디저트사옴",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028826.1.26",
          "form": "아ㅡㅡ배고파쥭겟",
          "original_form": "아ㅡㅡ배고파쥭겟",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028826.1.27",
          "form": "부럽",
          "original_form": "부럽",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028826.1.28",
          "form": "아 배터짐요",
          "original_form": "아 배터짐요",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028826.1.29",
          "form": "뭐이리 많이 사와가지고",
          "original_form": "뭐이리 많이 사와가지고",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028826.1.30",
          "form": "엄마 후배가 커피집 열어서 거 가따온건데",
          "original_form": "엄마 후배가 커피집 열어서 거 가따온건데",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028826.1.31",
          "form": "누텔라 토스트 2개를 서비스로 줬다고 갖고오고",
          "original_form": "누텔라 토스트 2개를 서비스로 줬다고 갖고오고",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028826.1.32",
          "form": "배고프댔으니 그만함 ㅈㅅ",
          "original_form": "배고프댔으니 그만함 ㅈㅅ",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028826.1.33",
          "form": "뭔얘기했더라",
          "original_form": "뭔얘기했더라",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028826.1.34",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028826.1.35",
          "form": "자기전에 전화는 꼭 함",
          "original_form": "자기전에 전화는 꼭 함",
          "speaker_id": "2",
          "time": "20210902 22:26"
