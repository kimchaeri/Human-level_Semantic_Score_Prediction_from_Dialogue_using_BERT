        {
          "id": "MDRW2100016506.1.1",
          "form": "뭐하노??ㅋㅋ 바쁘나??",
          "original_form": "뭐하노??ㅋㅋ 바쁘나??",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016506.1.2",
          "form": "이제 두찌데리러 ㅋㅋㅋㅋ",
          "original_form": "이제 두찌데리러 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016506.1.3",
          "form": "우리은행 안갔음 오늘 통장도 몬만들었다 ㅋㅋㅋㅋ",
          "original_form": "우리은행 안갔음 오늘 통장도 몬만들었다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016506.1.4",
          "form": "속이다 시원하네~",
          "original_form": "속이다 시원하네~",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016506.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ아 웃겨 처음부터 우리은행가지 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 웃겨 처음부터 우리은행가지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016506.1.6",
          "form": "거기는 사람 많이 없드제?",
          "original_form": "거기는 사람 많이 없드제?",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016506.1.7",
          "form": "웅~통장도 핑크퐁 늠 이쁨 ㅋㅋㅋㅋ",
          "original_form": "웅~통장도 핑크퐁 늠 이쁨 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016506.1.8",
          "form": "선물도 주더라 ㅋㅋㅋㅋㅋ",
          "original_form": "선물도 주더라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016506.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아 핑크퐁이더라 ㅋㅋ 둘째 좋아하더라 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아 핑크퐁이더라 ㅋㅋ 둘째 좋아하더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016506.1.10",
          "form": "고생했네이",
          "original_form": "고생했네이",
          "speaker_id": "1",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016506.1.11",
          "form": "진짜 맴이 든든하군 ㅋㅋㅋㅋ",
          "original_form": "진짜 맴이 든든하군 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016506.1.12",
          "form": "절대 해약안한다 ㅋㅋㅋㅋ다시는 못만들겠다 ㅋㅋㅋ",
          "original_form": "절대 해약안한다 ㅋㅋㅋㅋ다시는 못만들겠다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016506.1.13",
          "form": "우린 애들 다 출근시켰다 ㅋㅋㅋㅋㅋ",
          "original_form": "우린 애들 다 출근시켰다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:23"
        },
        {
          "id": "MDRW2100016506.1.14",
          "form": "와 대박 멋지다ㅋㅋ",
          "original_form": "와 대박 멋지다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100016506.1.15",
          "form": "부럽구만ㅋㅋ",
          "original_form": "부럽구만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100016506.1.16",
          "form": "밖에 시원한데 땀 줄줄 흘림",
          "original_form": "밖에 시원한데 땀 줄줄 흘림",
