      "utterance": [
        {
          "id": "MDRW2100008705.1.1",
          "form": "공연,, 코로나때메 넘 못본지 오래됐네요",
          "original_form": "공연,, 코로나때메 넘 못본지 오래됐네요",
          "speaker_id": "2",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008705.1.2",
          "form": "최근에 극장 가신 적 잇으세요?",
          "original_form": "최근에 극장 가신 적 잇으세요?",
          "speaker_id": "2",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008705.1.3",
          "form": "극장은 간적있어요",
          "original_form": "극장은 간적있어요",
          "speaker_id": "1",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008705.1.4",
          "form": "혹시 sk세요? 전 휴대폰 멤버십 쓰러 갔었어요",
          "original_form": "혹시 sk세요? 전 휴대폰 멤버십 쓰러 갔었어요",
          "speaker_id": "1",
          "time": "20210822 17:43"
        },
        {
          "id": "MDRW2100008705.1.5",
          "form": "아! 저는 lg인데 멤버십 있는지 모르겠네요",
          "original_form": "아! 저는 lg인데 멤버십 있는지 모르겠네요",
          "speaker_id": "2",
          "time": "20210822 17:52"
        },
        {
          "id": "MDRW2100008705.1.6",
          "form": "뭐 보셨어요?",
          "original_form": "뭐 보셨어요?",
          "speaker_id": "2",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008705.1.7",
          "form": "저는 블랙 위도우요",
          "original_form": "저는 블랙 위도우요",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008705.1.8",
          "form": "others 엄마랑 같이봤는데 다행히 재미있게 보시더라고요",
          "original_form": "&others& 엄마랑 같이봤는데 다행히 재미있게 보시더라고요",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008705.1.9",
          "form": "오 저도 젤 최근에 본게 그거에요 ㅋㅋㅋㅋ",
          "original_form": "오 저도 젤 최근에 본게 그거에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008705.1.10",
          "form": "마블에서 블위 그렇게 내다버린거 참을수없습니다..",
          "original_form": "마블에서 블위 그렇게 내다버린거 참을수없습니다..",
          "speaker_id": "2",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008705.1.11",
          "form": "진짜 저도 그생각 했어요ㅡㅡ",
          "original_form": "진짜 저도 그생각 했어요ㅡㅡ",
          "speaker_id": "1",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008705.1.12",
          "form": "최근엔 조인성영화였나 그거 잼나다던데",
          "original_form": "최근엔 조인성영화였나 그거 잼나다던데",
          "speaker_id": "1",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008705.1.13",
          "form": "개국공신인데 그렇게 보내버리다니..",
          "original_form": "개국공신인데 그렇게 보내버리다니..",
          "speaker_id": "2",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008705.1.14",
          "form": "오 조인성 영화찍었나보네요",
          "original_form": "오 조인성 영화찍었나보네요",
          "speaker_id": "2",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008705.1.15",
          "form": "모가디슈인가?",
          "original_form": "모가디슈인가?",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008705.1.16",
          "form": "실화를 바탕으로 했다더라고요",
          "original_form": "실화를 바탕으로 했다더라고요",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008705.1.17",
          "form": "아! 그 무슨 탈출하는 그거죠",
          "original_form": "아! 그 무슨 탈출하는 그거죠",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008705.1.18",
          "form": "요즘 아프간 사태랑 겹쳐서 더 생생할거같아요",
          "original_form": "요즘 아프간 사태랑 겹쳐서 더 생생할거같아요",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008705.1.19",
          "form": "네네 그거요",
          "original_form": "네네 그거요",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008705.1.20",
          "form": "저는 좀 내 현생도 힘든데",
          "original_form": "저는 좀 내 현생도 힘든데",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008705.1.21",
          "form": "슬프거나 잔인한거 잘 안보거든요",
          "original_form": "슬프거나 잔인한거 잘 안보거든요",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008705.1.22",
          "form": "근데 이건 그정도 아니고 볼만 하다더라고요",
          "original_form": "근데 이건 그정도 아니고 볼만 하다더라고요",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008705.1.23",
          "form": "오 맞아요 저도 그런거 잘 못봐요 무조건 오락영화만 보고",
          "original_form": "오 맞아요 저도 그런거 잘 못봐요 무조건 오락영화만 보고",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008705.1.24",
          "form": "추천 감사합니다 ㅎㅎ",
          "original_form": "추천 감사합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008705.1.25",
          "form": "저  크루엘라 봤었는데 보셨나요 혹시",
          "original_form": "저  크루엘라 봤었는데 보셨나요 혹시",
          "speaker_id": "2",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008705.1.26",
          "form": "보고싶었는데 못봤어요",
          "original_form": "보고싶었는데 못봤어요",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008705.1.27",
          "form": "어떠셨어요",
          "original_form": "어떠셨어요",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008705.1.28",
          "form": "음악도 좋고 미장셴도 좋고 다 좋았어요",
          "original_form": "음악도 좋고 미장셴도 좋고 다 좋았어요",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008705.1.29",
          "form": "완전 대기업의 맛",
          "original_form": "완전 대기업의 맛",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008705.1.30",
          "form": "완전 대기업 맛이었어요",
          "original_form": "완전 대기업 맛이었어요",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008705.1.31",
          "form": "음악 미장셴 다 멋져서",
          "original_form": "음악 미장셴 다 멋져서",
