      "utterance": [
        {
          "id": "MDRW2100036147.1.1",
          "form": "배달음식은 어떤거자주시켜드세요",
          "original_form": "배달음식은 어떤거자주시켜드세요",
          "speaker_id": "1",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100036147.1.2",
          "form": "전치킨이요주로치킨",
          "original_form": "전치킨이요주로치킨",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100036147.1.3",
          "form": "name1님은 어떤걸 시켜드세요?",
          "original_form": "&name1&님은 어떤걸 시켜드세요?",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100036147.1.4",
          "form": "저도치킨 자주시켜먹어요 오늘은짜장면 시켜먹었네요",
          "original_form": "저도치킨 자주시켜먹어요 오늘은짜장면 시켜먹었네요",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100036147.1.5",
          "form": "어디 치킨자주드세요",
          "original_form": "어디 치킨자주드세요",
          "speaker_id": "1",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036147.1.6",
          "form": "전 교촌이요",
          "original_form": "전 교촌이요",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036147.1.7",
          "form": "교촌 레드콤보를 주로 먹어요",
          "original_form": "교촌 레드콤보를 주로 먹어요",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036147.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036147.1.9",
          "form": "아 저도 교촌자주먹어요 허니콤보 약간데자뷰가....",
          "original_form": "아 저도 교촌자주먹어요 허니콤보 약간데자뷰가....",
          "speaker_id": "1",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100036147.1.10",
          "form": "노랑통닭에 알마치도좋아하구여",
          "original_form": "노랑통닭에 알마치도좋아하구여",
          "speaker_id": "1",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100036147.1.11",
          "form": "네 배달음식 질문에 저도 데자뷰가 ㅋㅋㅋ",
          "original_form": "네 배달음식 질문에 저도 데자뷰가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100036147.1.12",
          "form": "전 노량통닭은 닭똥집 튀김때매 가끔 가요",
          "original_form": "전 노량통닭은 닭똥집 튀김때매 가끔 가요",
          "speaker_id": "2",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100036147.1.13",
          "form": "ㅋㅋㅋㅋ마자요 닭똥집도 맛있드라구여",
          "original_form": "ㅋㅋㅋㅋ마자요 닭똥집도 맛있드라구여",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100036147.1.14",
          "form": "가끔 땡길때있어요 바삭하고 쫄깃하고",
          "original_form": "가끔 땡길때있어요 바삭하고 쫄깃하고",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100036147.1.15",
          "form": "네네 원래는 시장 닭집에서나 팔던건데",
          "original_form": "네네 원래는 시장 닭집에서나 팔던건데",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100036147.1.16",
          "form": "노랑통닭은 배달되는 체인점이라 먹기 편해요 ㅋㅋ",
          "original_form": "노랑통닭은 배달되는 체인점이라 먹기 편해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100036147.1.17",
          "form": "ㅋㅋㅋ맞아요 배달의장점이 원래똥집은 치킨집에서 잘안팔았잖아요 호프집이나 시장에나",
          "original_form": "ㅋㅋㅋ맞아요 배달의장점이 원래똥집은 치킨집에서 잘안팔았잖아요 호프집이나 시장에나",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100036147.1.18",
          "form": "네네 그죠 거기서나 먹었죠",
          "original_form": "네네 그죠 거기서나 먹었죠",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100036147.1.19",
          "form": "네 그래서 저렴한데 잘못사먹었었는데 요즘은 넘편하네요",
          "original_form": "네 그래서 저렴한데 잘못사먹었었는데 요즘은 넘편하네요",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100036147.1.20",
          "form": "편의점도 배달이되니까 안된는게없어요",
          "original_form": "편의점도 배달이되니까 안된는게없어요",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100036147.1.21",
          "form": "맞아요 돈만 있음 참 좋은 세상인데  돈안쓰고 직접 만들라면 힘들고 그렇네요",
          "original_form": "맞아요 돈만 있음 참 좋은 세상인데  돈안쓰고 직접 만들라면 힘들고 그렇네요",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036147.1.22",
          "form": "네ㅜㅜ맞아요ㅜ 그래도여름지나면 요리하기 좀 수월해질거같아요",
          "original_form": "네ㅜㅜ맞아요ㅜ 그래도여름지나면 요리하기 좀 수월해질거같아요",
          "speaker_id": "1",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100036147.1.23",
          "form": "여름엔 너무더워서ㅠ가스불가까이가기도 싫었는데",
          "original_form": "여름엔 너무더워서ㅠ가스불가까이가기도 싫었는데",
          "speaker_id": "1",
          "time": "20210830 21:57"
        }
      ]
    }
