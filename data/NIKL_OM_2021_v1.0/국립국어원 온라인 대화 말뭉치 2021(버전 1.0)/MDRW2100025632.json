        {
          "id": "MDRW2100025632.1.2",
          "form": "맞나여?",
          "original_form": "맞나여?",
          "speaker_id": "1",
          "time": "20210901 14:40"
        },
        {
          "id": "MDRW2100025632.1.3",
          "form": "네 안녕하세요 최근에 어떤상품 구매하셨나요",
          "original_form": "네 안녕하세요 최근에 어떤상품 구매하셨나요",
          "speaker_id": "2",
          "time": "20210901 14:40"
        },
        {
          "id": "MDRW2100025632.1.4",
          "form": "게롤슈타이너 탄산수를 구매했어요",
          "original_form": "게롤슈타이너 탄산수를 구매했어요",
          "speaker_id": "1",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100025632.1.5",
          "form": "그외에도 가방스트랩...",
          "original_form": "그외에도 가방스트랩...",
          "speaker_id": "1",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100025632.1.6",
          "form": "또...",
          "original_form": "또...",
          "speaker_id": "1",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100025632.1.7",
          "form": "와 뭘 많이사셨네요",
          "original_form": "와 뭘 많이사셨네요",
          "speaker_id": "2",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100025632.1.8",
          "form": "탄산수는 몇개삿나요",
          "original_form": "탄산수는 몇개삿나요",
          "speaker_id": "2",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100025632.1.9",
          "form": "디퓨저도 샀어요",
          "original_form": "디퓨저도 샀어요",
          "speaker_id": "1",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100025632.1.10",
          "form": "24개 샀어요",
          "original_form": "24개 샀어요",
          "speaker_id": "1",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100025632.1.11",
          "form": "저번에 패트로 먹었는데",
          "original_form": "저번에 패트로 먹었는데",
          "speaker_id": "1",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100025632.1.12",
          "form": "탄산이 영 그래서 병으로 샀어요",
          "original_form": "탄산이 영 그래서 병으로 샀어요",
          "speaker_id": "1",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100025632.1.13",
          "form": "name2님은 뭐 산거있어요?",
          "original_form": "&name2&님은 뭐 산거있어요?",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025632.1.14",
          "form": "오늘 이어폰와요",
          "original_form": "오늘 이어폰와요",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025632.1.15",
          "form": "멜론왔오요",
          "original_form": "멜론왔오요",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025632.1.16",
          "form": "멜론 후숙중",
          "original_form": "멜론 후숙중",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025632.1.17",
          "form": "와 멜론..!!",
          "original_form": "와 멜론..!!",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025632.1.18",
          "form": "몇개사셨어요????",
          "original_form": "몇개사셨어요????",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025632.1.19",
          "form": "한박스?",
          "original_form": "한박스?",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025632.1.20",
          "form": "2개삿어요",
          "original_form": "2개삿어요",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025632.1.21",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025632.1.22",
          "form": "흐흐",
          "original_form": "흐흐",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025632.1.23",
          "form": "맛있을거같아요!",
          "original_form": "맛있을거같아요!",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025632.1.24",
          "form": "멜론멜론-",
          "original_form": "멜론멜론-",
          "speaker_id": "1",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100025632.1.25",
          "form": "스트랩은 뭐삿나요",
          "original_form": "스트랩은 뭐삿나요",
          "speaker_id": "2",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100025632.1.26",
          "form": "아 계속 살려고 보고 있었는데",
          "original_form": "아 계속 살려고 보고 있었는데",
          "speaker_id": "1",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100025632.1.27",
          "form": "맘에들면 비싸거나 이미 완판이라",
          "original_form": "맘에들면 비싸거나 이미 완판이라",
          "speaker_id": "1",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100025632.1.28",
          "form": "엄청 뒤적거리다가",
          "original_form": "엄청 뒤적거리다가",
          "speaker_id": "1",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100025632.1.29",
          "form": "디아이블랑꺼가 좀 싸고 이쁘길태",
          "original_form": "디아이블랑꺼가 좀 싸고 이쁘길태",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100025632.1.30",
          "form": "래",
          "original_form": "래",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100025632.1.31",
          "form": "흰색에 글씨 써 있는거",
          "original_form": "흰색에 글씨 써 있는거",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100025632.1.32",
          "form": "샀어요",
          "original_form": "샀어요",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100025632.1.33",
          "form": "생각보다 뭐 이뻐요 ㅋㅋ",
          "original_form": "생각보다 뭐 이뻐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100025632.1.34",
          "form": "가방끈이 죽어서 어쩔수없이 샀네요 ㅠ",
          "original_form": "가방끈이 죽어서 어쩔수없이 샀네요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100025632.1.35",
          "form": "아하 그런 슬픈일이",
          "original_form": "아하 그런 슬픈일이",
          "speaker_id": "2",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100025632.1.36",
          "form": "이어폰은 몇개 사셨어요???",
          "original_form": "이어폰은 몇개 사셨어요???",
          "speaker_id": "1",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100025632.1.37",
          "form": "맘에들길바라요",
          "original_form": "맘에들길바라요",
          "speaker_id": "2",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100025632.1.38",
          "form": "이어폰하나삿어요",
          "original_form": "이어폰하나삿어요",
          "speaker_id": "2",
          "time": "20210901 14:46"
