        },
        {
          "id": "MDRW2100027064.1.3",
          "form": "스포츠 경기 보는거 좋아하세요?",
          "original_form": "스포츠 경기 보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100027064.1.4",
          "form": "평소엔 잘 안봤는데",
          "original_form": "평소엔 잘 안봤는데",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100027064.1.5",
          "form": "저번 올림픽은 재밌게봤어요",
          "original_form": "저번 올림픽은 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100027064.1.6",
          "form": "저는 스포츠 보는거 좋아해서",
          "original_form": "저는 스포츠 보는거 좋아해서",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100027064.1.7",
          "form": "올림픽도 재밌게 봤어요",
          "original_form": "올림픽도 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027064.1.8",
          "form": "오 뭐 보셨나요?",
          "original_form": "오 뭐 보셨나요?",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027064.1.9",
          "form": "양궁이랑 배구 위주로",
          "original_form": "양궁이랑 배구 위주로",
          "speaker_id": "1",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027064.1.10",
          "form": "거의 다 봤어요",
          "original_form": "거의 다 봤어요",
          "speaker_id": "1",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027064.1.11",
          "form": "오 저도 양궁, 배구, 펜싱 등 많이 봤어요",
          "original_form": "오 저도 양궁, 배구, 펜싱 등 많이 봤어요",
          "speaker_id": "2",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100027064.1.12",
          "form": "진짜 다 재밌었어요",
          "original_form": "진짜 다 재밌었어요",
          "speaker_id": "2",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100027064.1.13",
          "form": "배구는 한 게임빼고 다 봤을정도ㅋㅋㅋ",
          "original_form": "배구는 한 게임빼고 다 봤을정도ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100027064.1.14",
          "form": "한일전 진짜 재밌었죠",
          "original_form": "한일전 진짜 재밌었죠",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100027064.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100027064.1.16",
          "form": "진짜 대박",
          "original_form": "진짜 대박",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100027064.1.17",
          "form": "일본이 수비를 잘해서",
          "original_form": "일본이 수비를 잘해서",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100027064.1.18",
          "form": "서로 수비로 싸우는게 눈에 보였죠",
          "original_form": "서로 수비로 싸우는게 눈에 보였죠",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027064.1.19",
          "form": "저까지 보면서 힘들었어요",
          "original_form": "저까지 보면서 힘들었어요",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027064.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027064.1.21",
          "form": "한국인이지만 일본이 진짜 잘하긴 했어요",
          "original_form": "한국인이지만 일본이 진짜 잘하긴 했어요",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027064.1.22",
          "form": "진짜 끈적끈적한 게임이었어요",
          "original_form": "진짜 끈적끈적한 게임이었어요",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027064.1.23",
          "form": "너무 좋은 상대였습니다",
          "original_form": "너무 좋은 상대였습니다",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027064.1.24",
          "form": "맞아요 진짜 명경기",
          "original_form": "맞아요 진짜 명경기",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027064.1.25",
          "form": "이겨서 기분도 좋구요ㅋㅋㅋ",
          "original_form": "이겨서 기분도 좋구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027064.1.26",
          "form": "맞아요 이긴게 크죠ㅋㅋ",
          "original_form": "맞아요 이긴게 크죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027064.1.27",
          "form": "네 진짜",
          "original_form": "네 진짜",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027064.1.28",
          "form": "선수들이 끝까지 임하는 모습보고",
          "original_form": "선수들이 끝까지 임하는 모습보고",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027064.1.29",
          "form": "감동, 감명받았어요",
          "original_form": "감동, 감명받았어요",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027064.1.30",
          "form": "집중력이 엄청났죠",
          "original_form": "집중력이 엄청났죠",
