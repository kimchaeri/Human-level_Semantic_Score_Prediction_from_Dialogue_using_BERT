      "utterance": [
        {
          "id": "MDRW2100020141.1.1",
          "form": "ㅋㅋㅋㅋㅋ그러기 위해서 저는",
          "original_form": "ㅋㅋㅋㅋㅋ그러기 위해서 저는",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020141.1.2",
          "form": "그런날이 오기만을 소망합니다",
          "original_form": "그런날이 오기만을 소망합니다",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020141.1.3",
          "form": "우정사랑허상설을",
          "original_form": "우정사랑허상설을",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020141.1.4",
          "form": "퍼뜨리고 다닐랍니다",
          "original_form": "퍼뜨리고 다닐랍니다",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020141.1.5",
          "form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020141.1.6",
          "form": "앜ㅋㅋㅋㅋㅋ 실천하는 지식인",
          "original_form": "앜ㅋㅋㅋㅋㅋ 실천하는 지식인",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020141.1.7",
          "form": "뭐랄까 우정과 사랑 말고",
          "original_form": "뭐랄까 우정과 사랑 말고",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020141.1.8",
          "form": "다양한 사랑을 통틀어 부르는 다른 단어가",
          "original_form": "다양한 사랑을 통틀어 부르는 다른 단어가",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020141.1.9",
          "form": "생겼으면 싶기도 해요",
          "original_form": "생겼으면 싶기도 해요",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020141.1.10",
          "form": "그르게요 그 외의 영역..",
          "original_form": "그르게요 그 외의 영역..",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020141.1.11",
          "form": "따지면 어머니와 딸간의 사랑도 사랑이고",
          "original_form": "따지면 어머니와 딸간의 사랑도 사랑이고",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020141.1.12",
          "form": "친구간의 사랑도 사랑이고",
          "original_form": "친구간의 사랑도 사랑이고",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020141.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020141.1.14",
          "form": "밥 먹었는데 맛있어서 오늘 내 하루를 사랑하게 된 것도",
          "original_form": "밥 먹었는데 맛있어서 오늘 내 하루를 사랑하게 된 것도",
          "speaker_id": "2",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020141.1.15",
          "form": "사랑??이고???",
          "original_form": "사랑??이고???",
          "speaker_id": "2",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020141.1.16",
          "form": "ㅋㅋㅋㅋㅋ아 너무 행복한 사랑이군요",
          "original_form": "ㅋㅋㅋㅋㅋ아 너무 행복한 사랑이군요",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020141.1.17",
          "form": "사랑에 성애가 필수가 아닌디",
          "original_form": "사랑에 성애가 필수가 아닌디",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020141.1.18",
          "form": "옳소",
          "original_form": "옳소",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020141.1.19",
          "form": "저는 그냥 절 지지해줄 사람이 필요하고",
          "original_form": "저는 그냥 절 지지해줄 사람이 필요하고",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020141.1.20",
          "form": "제 일상을 공유할 사람ㅇ리 있으면 좋겟는건데",
          "original_form": "제 일상을 공유할 사람ㅇ리 있으면 좋겟는건데",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020141.1.21",
          "form": "그게 성애는 아닌거같아요!!",
          "original_form": "그게 성애는 아닌거같아요!!",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020141.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020141.1.23",
          "form": "친구들이랑도 얘기할수잇는건데\n감정노동을 요구하는 순간부터 그 연애의 영역같기도하고..",
          "original_form": "친구들이랑도 얘기할수잇는건데\n감정노동을 요구하는 순간부터 그 연애의 영역같기도하고..",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020141.1.24",
          "form": "리터럴리 동반자나 지지자",
          "original_form": "리터럴리 동반자나 지지자",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020141.1.25",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020141.1.26",
          "form": "아니면 일상공유자",
          "original_form": "아니면 일상공유자",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020141.1.27",
          "form": "언어의 연금술사..",
          "original_form": "언어의 연금술사..",
