      "utterance": [
        {
          "id": "MDRW2100041862.1.1",
          "form": "저녁에 뭐 먹을까용",
          "original_form": "저녁에 뭐 먹을까용",
          "speaker_id": "1",
          "time": "20210906 17:15"
        },
        {
          "id": "MDRW2100041862.1.2",
          "form": "난 백신 맞아서 힘이 없다",
          "original_form": "난 백신 맞아서 힘이 없다",
          "speaker_id": "2",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041862.1.3",
          "form": "구래 내가 차리려고했징~",
          "original_form": "구래 내가 차리려고했징~",
          "speaker_id": "1",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041862.1.4",
          "form": "스파게티 먹을랭?",
          "original_form": "스파게티 먹을랭?",
          "speaker_id": "1",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041862.1.5",
          "form": "맛난 거 해 봐라",
          "original_form": "맛난 거 해 봐라",
          "speaker_id": "2",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041862.1.6",
          "form": "스파게티 좋네",
          "original_form": "스파게티 좋네",
          "speaker_id": "2",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041862.1.7",
          "form": "웅웅 ㅋㅋ 해줄게용",
          "original_form": "웅웅 ㅋㅋ 해줄게용",
          "speaker_id": "1",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041862.1.8",
          "form": "토마토 스파게티",
          "original_form": "토마토 스파게티",
          "speaker_id": "2",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041862.1.9",
          "form": "웅~ 집에 소스 있다",
          "original_form": "웅~ 집에 소스 있다",
          "speaker_id": "1",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041862.1.10",
          "form": "양파랑 버섯 많이 넣고",
          "original_form": "양파랑 버섯 많이 넣고",
          "speaker_id": "2",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041862.1.11",
          "form": "알았다 ㅎㅎ 면은 적당히만~",
          "original_form": "알았다 ㅎㅎ 면은 적당히만~",
          "speaker_id": "1",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041862.1.12",
          "form": "그래 살 안 찌게",
          "original_form": "그래 살 안 찌게",
