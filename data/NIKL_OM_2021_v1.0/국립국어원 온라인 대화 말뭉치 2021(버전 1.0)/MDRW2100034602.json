      "utterance": [
        {
          "id": "MDRW2100034602.1.1",
          "form": "저는 뉴스를 잘 안 봐서 몰랐네요 ㅜㅜ",
          "original_form": "저는 뉴스를 잘 안 봐서 몰랐네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100034602.1.2",
          "form": "국가가 추석 전에 맞추려고 노력하나봐요",
          "original_form": "국가가 추석 전에 맞추려고 노력하나봐요",
          "speaker_id": "2",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100034602.1.3",
          "form": "기사로 언뜻 본거같기도 해요",
          "original_form": "기사로 언뜻 본거같기도 해요",
          "speaker_id": "1",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100034602.1.4",
          "form": "맞아요 위험해서요 ㅠㅜ",
          "original_form": "맞아요 위험해서요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034602.1.5",
          "form": "추석에 대이동하면 힘들거같아요",
          "original_form": "추석에 대이동하면 힘들거같아요",
          "speaker_id": "1",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034602.1.6",
          "form": "ㅠ 휴가철에도 은근 다들 놀러가서 확 올라갔으니",
          "original_form": "ㅠ 휴가철에도 은근 다들 놀러가서 확 올라갔으니",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034602.1.7",
          "form": "추석때도 그럴거에요",
          "original_form": "추석때도 그럴거에요",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034602.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034602.1.9",
          "form": "첫 행해엔 그래도 조심하느라 안갔는데",
          "original_form": "첫 행해엔 그래도 조심하느라 안갔는데",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034602.1.10",
          "form": "하 방역수칙 다들 잘 지키면 좋겠어요 ㅜㅜ",
          "original_form": "하 방역수칙 다들 잘 지키면 좋겠어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034602.1.11",
          "form": "지금 거의 2년인가요 3년인가요",
          "original_form": "지금 거의 2년인가요 3년인가요",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034602.1.12",
          "form": "계속 안갈수 없으니 슬를 움직일때죠",
          "original_form": "계속 안갈수 없으니 슬를 움직일때죠",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034602.1.13",
          "form": "2년째같아요",
          "original_form": "2년째같아요",
          "speaker_id": "1",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034602.1.14",
          "form": "이젠 햇수도 헷갈려요 10년째 감금된 느낌이에요",
          "original_form": "이젠 햇수도 헷갈려요 10년째 감금된 느낌이에요",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034602.1.15",
          "form": "맞아욬ㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034602.1.16",
          "form": "올해 끝날줄 알았는데..아닐거같죠",
          "original_form": "올해 끝날줄 알았는데..아닐거같죠",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034602.1.17",
          "form": "장기수가 된 기분ㅋㅋㅋ",
          "original_form": "장기수가 된 기분ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034602.1.18",
          "form": "위드코로나하라고 하는데. 이게 말이 쉽지..",
          "original_form": "위드코로나하라고 하는데. 이게 말이 쉽지..",
          "speaker_id": "2",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034602.1.19",
          "form": "위드라니..!!!",
          "original_form": "위드라니..!!!",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034602.1.20",
          "form": "이리 심해질줄 몰랐어요",
          "original_form": "이리 심해질줄 몰랐어요",
          "speaker_id": "1",
          "time": "20210828 16:09"
