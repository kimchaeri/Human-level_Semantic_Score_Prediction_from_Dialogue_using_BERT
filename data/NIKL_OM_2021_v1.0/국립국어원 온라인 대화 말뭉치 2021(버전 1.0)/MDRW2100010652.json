      "utterance": [
        {
          "id": "MDRW2100010652.1.1",
          "form": "요즘 노래 뭐들으세요?",
          "original_form": "요즘 노래 뭐들으세요?",
          "speaker_id": "1",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010652.1.2",
          "form": "이무진의 신호등이란 노래요",
          "original_form": "이무진의 신호등이란 노래요",
          "speaker_id": "2",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010652.1.3",
          "form": "발매됐을때도 들었는데 요새 1위 하고있더라구요",
          "original_form": "발매됐을때도 들었는데 요새 1위 하고있더라구요",
          "speaker_id": "2",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100010652.1.4",
          "form": "안들어봤어요 검색해봐야겠네요",
          "original_form": "안들어봤어요 검색해봐야겠네요",
          "speaker_id": "1",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010652.1.5",
          "form": "전 넥스트레벨? 그 노래가 중독성있더라고요",
          "original_form": "전 넥스트레벨? 그 노래가 중독성있더라고요",
          "speaker_id": "1",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010652.1.6",
          "form": "그것도 많이 들어봤어요",
          "original_form": "그것도 많이 들어봤어요",
          "speaker_id": "2",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010652.1.7",
          "form": "춤도 멋있잖아요",
          "original_form": "춤도 멋있잖아요",
          "speaker_id": "2",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010652.1.8",
          "form": "아진짜요 그건 못보고 노래만 들어봤어요",
          "original_form": "아진짜요 그건 못보고 노래만 들어봤어요",
          "speaker_id": "1",
          "time": "20210823 20:25"
        },
        {
          "id": "MDRW2100010652.1.9",
          "form": "관절을 움직이는 춤이었는데 멋있더라구요",
          "original_form": "관절을 움직이는 춤이었는데 멋있더라구요",
          "speaker_id": "2",
          "time": "20210823 20:29"
        },
        {
          "id": "MDRW2100010652.1.10",
          "form": "최근엔 여름 노래만 찾아들었던거 같네요",
          "original_form": "최근엔 여름 노래만 찾아들었던거 같네요",
          "speaker_id": "2",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010652.1.11",
          "form": "네네 저도요 유튜브에서 노동요도 찾아들어요",
          "original_form": "네네 저도요 유튜브에서 노동요도 찾아들어요",
          "speaker_id": "1",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010652.1.12",
          "form": "추천하는 곡 있으세요?",
          "original_form": "추천하는 곡 있으세요?",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010652.1.13",
          "form": "아이유 노동요,여름에 듣기좋은 노동요 이런식으로 골라요 ㅋㅋ노동요치면요",
          "original_form": "아이유 노동요,여름에 듣기좋은 노동요 이런식으로 골라요 ㅋㅋ노동요치면요",
          "speaker_id": "1",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100010652.1.14",
          "form": "전 밴드곡이나 신나는 곡 위주로요ㅋㅋ",
          "original_form": "전 밴드곡이나 신나는 곡 위주로요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010652.1.15",
          "form": "밴드 곡 추천해주세요",
          "original_form": "밴드 곡 추천해주세요",
          "speaker_id": "1",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010652.1.16",
          "form": "요샌 이승윤의 들려주고 싶었던이란 곡 잘 들어요",
          "original_form": "요샌 이승윤의 들려주고 싶었던이란 곡 잘 들어요",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010652.1.17",
          "form": "국카스텐 곡은 다 좋구요ㅋㅋ",
          "original_form": "국카스텐 곡은 다 좋구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010652.1.18",
          "form": "와 노랠 좋아하시네요 나이들며 노래를 듣던거만들어요",
          "original_form": "와 노랠 좋아하시네요 나이들며 노래를 듣던거만들어요",
          "speaker_id": "1",
          "time": "20210823 23:14"
        }
