      "utterance": [
        {
          "id": "MDRW2100018159.1.1",
          "form": "저는 만달라키조명 갖고싶어요",
          "original_form": "저는 만달라키조명 갖고싶어요",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018159.1.2",
          "form": "미니멀은개뿔",
          "original_form": "미니멀은개뿔",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018159.1.3",
          "form": "검샏해보고 올게요",
          "original_form": "검샏해보고 올게요",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018159.1.4",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018159.1.5",
          "form": "완잔 갬성갬성",
          "original_form": "완잔 갬성갬성",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018159.1.6",
          "form": "저 얼마전에 저녁먹으러 간 싣당에 이거 있었어요",
          "original_form": "저 얼마전에 저녁먹으러 간 싣당에 이거 있었어요",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018159.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018159.1.8",
          "form": "헐진짜요? 전파란계열말고",
          "original_form": "헐진짜요? 전파란계열말고",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018159.1.9",
          "form": "노란계열이요..(선긋기(",
          "original_form": "노란계열이요..(선긋기(",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018159.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ취향확고하신 편이시군",
          "original_form": "ㅋㅋㅋㅋㅋㅋ취향확고하신 편이시군",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018159.1.11",
          "form": "엄연히 다른 계열이라구욧 호호",
          "original_form": "엄연히 다른 계열이라구욧 호호",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018159.1.12",
          "form": "저는 이제 사고싶은 게 없어요.....",
          "original_form": "저는 이제 사고싶은 게 없어요.....",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018159.1.13",
          "form": "너무 다 샀나봐요",
          "original_form": "너무 다 샀나봐요",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018159.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018159.1.15",
          "form": "멋진데요",
          "original_form": "멋진데요",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018159.1.16",
          "form": "젤 유용한탬은 뭔가요",
          "original_form": "젤 유용한탬은 뭔가요",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018159.1.17",
          "form": "하... 유용템이라....",
          "original_form": "하... 유용템이라....",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018159.1.18",
          "form": "제일은 아니고",
          "original_form": "제일은 아니고",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018159.1.19",
          "form": "soso 제품최근에 산거",
          "original_form": "soso 제품최근에 산거",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018159.1.20",
          "form": "1. 경추베개",
          "original_form": "1. 경추베개",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018159.1.21",
          "form": "이건 하루만 써봄... ㅎ 아직은 불편해요 근데 거북목에 좋대요",
          "original_form": "이건 하루만 써봄... ㅎ 아직은 불편해요 근데 거북목에 좋대요",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018159.1.22",
          "form": "원통모양으로 샀어요 ㅌㅋㅋ",
          "original_form": "원통모양으로 샀어요 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018159.1.23",
          "form": "2. 리무버브라",
          "original_form": "2. 리무버브라",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018159.1.24",
          "form": "오 확실히 불편할 거닽은데요?!",
          "original_form": "오 확실히 불편할 거닽은데요?!",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018159.1.25",
          "form": "맞나... 이건진짜 안붙인거같은 니플패치",
          "original_form": "맞나... 이건진짜 안붙인거같은 니플패치",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018159.1.26",
          "form": "오 메모메모",
          "original_form": "오 메모메모",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018159.1.27",
          "form": "올리브영에 팔아요",
          "original_form": "올리브영에 팔아요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018159.1.28",
          "form": "롭스랑",
          "original_form": "롭스랑",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018159.1.29",
          "form": "3. 고운발크림",
          "original_form": "3. 고운발크림",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018159.1.30",
          "form": "이것도 좋아요 촠촠",
          "original_form": "이것도 좋아요 촠촠",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018159.1.31",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018159.1.32",
          "form": "앗 안도ㅑ 정신차려",
          "original_form": "앗 안도ㅑ 정신차려",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018159.1.33",
          "form": "그만사야햐요",
          "original_form": "그만사야햐요",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018159.1.34",
