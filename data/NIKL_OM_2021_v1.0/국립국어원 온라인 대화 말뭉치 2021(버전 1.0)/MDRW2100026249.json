      "utterance": [
        {
          "id": "MDRW2100026249.1.1",
          "form": "그쵸ㅠㅠ 저 진짜 병원 가면서",
          "original_form": "그쵸ㅠㅠ 저 진짜 병원 가면서",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026249.1.2",
          "form": "매운닭발은 안먹어야지 했어요",
          "original_form": "매운닭발은 안먹어야지 했어요",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026249.1.3",
          "form": "지금도 절대 안 먹어요",
          "original_form": "지금도 절대 안 먹어요",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026249.1.4",
          "form": "아이고ㅠㅠ",
          "original_form": "아이고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026249.1.5",
          "form": "저도 마라탕 3단계는 이제 안 먹기로 했어요ㅋㅋㅋ",
          "original_form": "저도 마라탕 3단계는 이제 안 먹기로 했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026249.1.6",
          "form": "안 드시는 음식 있으세요?",
          "original_form": "안 드시는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026249.1.7",
          "form": "아 저두 ㅋㅋ 3단계는 무리예요",
          "original_form": "아 저두 ㅋㅋ 3단계는 무리예요",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026249.1.8",
          "form": "제가 시키는 곳에서는 2단계까지가 제가 먹을 수 있는 한계였어요",
          "original_form": "제가 시키는 곳에서는 2단계까지가 제가 먹을 수 있는 한계였어요",
          "speaker_id": "1",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026249.1.9",
          "form": "딱히 안 먹는 건 없는데 너무 단 음식은 잘 못 먹어요",
          "original_form": "딱히 안 먹는 건 없는데 너무 단 음식은 잘 못 먹어요",
          "speaker_id": "1",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026249.1.10",
          "form": "오 그러시구나 저는 초콜렛 먹으면서 초코우유 마시기 가능해요",
          "original_form": "오 그러시구나 저는 초콜렛 먹으면서 초코우유 마시기 가능해요",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026249.1.11",
          "form": "초코 너무 좋아요~ 딸기도 좋고~",
          "original_form": "초코 너무 좋아요~ 딸기도 좋고~",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026249.1.12",
          "form": "맛있죠~ 근데 언젠가부터 잘 안 들어가더라고요ㅜㅠ",
          "original_form": "맛있죠~ 근데 언젠가부터 잘 안 들어가더라고요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026249.1.13",
          "form": "투썸 아이스박스 이런 거 잘 못 먹어요ㅋㅋㅋㅋ",
          "original_form": "투썸 아이스박스 이런 거 잘 못 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026249.1.14",
          "form": "헉 그 맛있는 걸...!",
          "original_form": "헉 그 맛있는 걸...!",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026249.1.15",
          "form": "ㅋㅋㅋㅋ저는 막상 투썸 안간지 오래네요",
          "original_form": "ㅋㅋㅋㅋ저는 막상 투썸 안간지 오래네요",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026249.1.16",
          "form": "스벅만 주로 가게 돼요",
          "original_form": "스벅만 주로 가게 돼요",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026249.1.17",
          "form": "저는 집 앞에 투썸 매장 큰 게 있어서 친구들이랑 카공하러 가거든요ㅎㅎ 친구들은 케이크 먹는데 저혼자 커피나 차만 마셔요ㅋㅋㅋ",
          "original_form": "저는 집 앞에 투썸 매장 큰 게 있어서 친구들이랑 카공하러 가거든요ㅎㅎ 친구들은 케이크 먹는데 저혼자 커피나 차만 마셔요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026249.1.18",
          "form": "가까운 매장 자연스럽게 가는 거 같아요",
          "original_form": "가까운 매장 자연스럽게 가는 거 같아요",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026249.1.19",
          "form": "그쵸 ㅋㅋ 주변에 투썸이 없어요 전 ㅠㅠ",
          "original_form": "그쵸 ㅋㅋ 주변에 투썸이 없어요 전 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026249.1.20",
          "form": "스벅은 좀 있는데 의외로 없더라구요",
          "original_form": "스벅은 좀 있는데 의외로 없더라구요",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026249.1.21",
          "form": "맞아요 저희 집 근처에는 스벅이 없어서ㅎㅎ",
          "original_form": "맞아요 저희 집 근처에는 스벅이 없어서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026249.1.22",
          "form": "스벅 가시면 음료는 주로 뭐 주문하세요?",
          "original_form": "스벅 가시면 음료는 주로 뭐 주문하세요?",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026249.1.23",
          "form": "저는 좀 신메뉴 파예요 ㅋㅋㅋ",
          "original_form": "저는 좀 신메뉴 파예요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026249.1.24",
          "form": "가장 크게 걸려있는거 주문해요",
          "original_form": "가장 크게 걸려있는거 주문해요",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026249.1.25",
          "form": "사라지는 메뉴들 도전해보고 그럽니당",
          "original_form": "사라지는 메뉴들 도전해보고 그럽니당",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026249.1.26",
          "form": "도전을 즐기시는 군요! 저는 매일 마시던 것만 마시고 먹는 편이에요ㅋㅋㅋ",
          "original_form": "도전을 즐기시는 군요! 저는 매일 마시던 것만 마시고 먹는 편이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026249.1.27",
          "form": "스벅 디저트도 맛있나요?",
          "original_form": "스벅 디저트도 맛있나요?",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026249.1.28",
          "form": "네! 저는 하트쿠키 최애예요",
          "original_form": "네! 저는 하트쿠키 최애예요",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026249.1.29",
          "form": "하트쿠키 진짜 맛나요! 강추!",
          "original_form": "하트쿠키 진짜 맛나요! 강추!",
          "speaker_id": "2",
          "time": "20210901 23:35"
        }
      ]
    }
  ]
