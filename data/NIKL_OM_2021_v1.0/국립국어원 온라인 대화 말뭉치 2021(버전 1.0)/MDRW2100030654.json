      "utterance": [
        {
          "id": "MDRW2100030654.1.1",
          "form": "어제 목이 말라서 편의점에 갔는데",
          "original_form": "어제 목이 말라서 편의점에 갔는데",
          "speaker_id": "2",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030654.1.2",
          "form": "백김치가 없어서 신김치를",
          "original_form": "백김치가 없어서 신김치를",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030654.1.3",
          "form": "녹차 종류 중에 저도 녹차라고 들어 보셨나요",
          "original_form": "녹차 종류 중에 저도 녹차라고 들어 보셨나요",
          "speaker_id": "2",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030654.1.4",
          "form": "물에 씻어서 먹습니다",
          "original_form": "물에 씻어서 먹습니다",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030654.1.5",
          "form": "편의점 음료인데 정말 맛있네요 자주 마시나요",
          "original_form": "편의점 음료인데 정말 맛있네요 자주 마시나요",
          "speaker_id": "2",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030654.1.6",
          "form": "처음 들어 봅니다",
          "original_form": "처음 들어 봅니다",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030654.1.7",
          "form": "무엇이 다른가요",
          "original_form": "무엇이 다른가요",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030654.1.8",
          "form": "자두녹차 맛잇습니다 다음에 드셔 보세요",
          "original_form": "자두녹차 맛잇습니다 다음에 드셔 보세요",
          "speaker_id": "2",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030654.1.9",
          "form": "신김치 밥 싸 먹으면 진짜 맛있어요",
          "original_form": "신김치 밥 싸 먹으면 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030654.1.10",
          "form": "자두 맛이 난다고요",
          "original_form": "자두 맛이 난다고요",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030654.1.11",
          "form": "묵은 김치가 더 맛있어요",
          "original_form": "묵은 김치가 더 맛있어요",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030654.1.12",
          "form": "복숭아 녹차나 레몬 녹차는 드셔보셔야 됐지요",
          "original_form": "복숭아 녹차나 레몬 녹차는 드셔보셔야 됐지요",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030654.1.13",
          "form": "신맛이 나지 않거든요",
          "original_form": "신맛이 나지 않거든요",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030654.1.14",
          "form": "자두 맛이 나는 녹차입니다",
          "original_form": "자두 맛이 나는 녹차입니다",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030654.1.15",
          "form": "녹차랑 어울릴 거 같지가 않네요",
          "original_form": "녹차랑 어울릴 거 같지가 않네요",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030654.1.16",
          "form": "이름이 녹차 지 사실은 음료수 같아요",
          "original_form": "이름이 녹차 지 사실은 음료수 같아요",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030654.1.17",
          "form": "녹차는 그저 쓴 맛이납니다",
          "original_form": "녹차는 그저 쓴 맛이납니다",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030654.1.18",
          "form": "달달한 가요",
          "original_form": "달달한 가요",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030654.1.19",
          "form": "가향 된 녹차 인의 녹차 성분은 약간 있을 것 같고 맛이 있어서 그게 중요한 거죠",
          "original_form": "가향 된 녹차 인의 녹차 성분은 약간 있을 것 같고 맛이 있어서 그게 중요한 거죠",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030654.1.20",
          "form": "녹차 맛이 나는 음료 이군요",
          "original_form": "녹차 맛이 나는 음료 이군요",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030654.1.21",
          "form": "조금 달콤합니다",
          "original_form": "조금 달콤합니다",
          "speaker_id": "2",
          "time": "20210904 11:05"
        }
      ]
