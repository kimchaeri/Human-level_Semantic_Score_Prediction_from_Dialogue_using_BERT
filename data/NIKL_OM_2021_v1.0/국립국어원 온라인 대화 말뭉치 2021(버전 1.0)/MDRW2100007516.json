      "utterance": [
        {
          "id": "MDRW2100007516.1.1",
          "form": "결혼한집이요? 원래가족이요?",
          "original_form": "결혼한집이요? 원래가족이요?",
          "speaker_id": "2",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007516.1.2",
          "form": "엇 원래가족이요!",
          "original_form": "엇 원래가족이요!",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007516.1.3",
          "form": "제가 결혼 안해서 그걸 생각 못했어요ㅠㅋㅋㅋ",
          "original_form": "제가 결혼 안해서 그걸 생각 못했어요ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007516.1.4",
          "form": "딸3명이예요",
          "original_form": "딸3명이예요",
          "speaker_id": "2",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007516.1.5",
          "form": "다 크고나니 효도도 많이하고",
          "original_form": "다 크고나니 효도도 많이하고",
          "speaker_id": "2",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007516.1.6",
          "form": "아무래도 성별이 같다보니 공감대가 같아서 많이 도와주고",
          "original_form": "아무래도 성별이 같다보니 공감대가 같아서 많이 도와주고",
          "speaker_id": "2",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007516.1.7",
          "form": "어릴때는 많이싸웠어요",
          "original_form": "어릴때는 많이싸웠어요",
          "speaker_id": "2",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007516.1.8",
          "form": "헉 저도 공감해요",
          "original_form": "헉 저도 공감해요",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007516.1.9",
          "form": "전 위로 언니 한명 있는데",
          "original_form": "전 위로 언니 한명 있는데",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007516.1.10",
          "form": "많이 싸웠어요ㅋㅋㅋㅋㅋ",
          "original_form": "많이 싸웠어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007516.1.11",
          "form": "나이차많이나요?",
          "original_form": "나이차많이나요?",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007516.1.12",
          "form": "저는둘째인데 위에4살 밑에3살이예요",
          "original_form": "저는둘째인데 위에4살 밑에3살이예요",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007516.1.13",
          "form": "언니랑 막내랑7살차이나서\r\n저랑 막내랑 엄청 치고박고싸웠어요",
          "original_form": "언니랑 막내랑7살차이나서\r\n저랑 막내랑 엄청 치고박고싸웠어요",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007516.1.14",
          "form": "근데 동생이 중학교때부터 배구선수로 활동해서 힘으로는 졌어요ㅋㅋ",
          "original_form": "근데 동생이 중학교때부터 배구선수로 활동해서 힘으로는 졌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007516.1.15",
          "form": "저는 막말만 엄청하고",
          "original_form": "저는 막말만 엄청하고",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007516.1.16",
          "form": "헉 배구선수면...ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헉 배구선수면...ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007516.1.17",
          "form": "전 위로 3살이에요 별로 차이 안나서 더 싸운 것같아요",
          "original_form": "전 위로 3살이에요 별로 차이 안나서 더 싸운 것같아요",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007516.1.18",
          "form": "언니는 결혼했어요?",
          "original_form": "언니는 결혼했어요?",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007516.1.19",
          "form": "언니도 아직 어려서 결혼 안했어요ㅎㅎ",
          "original_form": "언니도 아직 어려서 결혼 안했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007516.1.20",
          "form": "몇살이예요?",
          "original_form": "몇살이예요?",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007516.1.21",
          "form": "언니는 20대 중반이에요",
          "original_form": "언니는 20대 중반이에요",
          "speaker_id": "1",
          "time": "20210822 02:55"
        },
        {
          "id": "MDRW2100007516.1.22",
          "form": "딱 예쁠 나이예요",
          "original_form": "딱 예쁠 나이예요",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007516.1.23",
          "form": "어머님아버님 언니분은 같이사세요?",
          "original_form": "어머님아버님 언니분은 같이사세요?",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007516.1.24",
          "form": "딸만있는 집은 아빠가 외롭지 않았어요?",
          "original_form": "딸만있는 집은 아빠가 외롭지 않았어요?",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007516.1.25",
          "form": "목욕탕가고그럴때",
          "original_form": "목욕탕가고그럴때",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007516.1.26",
          "form": "네 다같이 사는데",
          "original_form": "네 다같이 사는데",
          "speaker_id": "1",
          "time": "20210822 02:58"
        },
        {
          "id": "MDRW2100007516.1.27",
          "form": "목욕탕가면 아빠 혼자 들어갈때 좀 외로워보이긴 하더라구요",
          "original_form": "목욕탕가면 아빠 혼자 들어갈때 좀 외로워보이긴 하더라구요",
          "speaker_id": "1",
          "time": "20210822 02:58"
        },
        {
          "id": "MDRW2100007516.1.28",
          "form": "그래도 별말 없으셔서 혼자이신걸 즐기시나...생각도 했어요ㅋㅋㅋ",
          "original_form": "그래도 별말 없으셔서 혼자이신걸 즐기시나...생각도 했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:59"
        },
        {
          "id": "MDRW2100007516.1.29",
          "form": "등산가고 지금생각해보면 아빠랑 같이한 시간이 적더라고요",
          "original_form": "등산가고 지금생각해보면 아빠랑 같이한 시간이 적더라고요",
          "speaker_id": "2",
          "time": "20210822 02:59"
        },
        {
          "id": "MDRW2100007516.1.30",
          "form": "많이 보냈어요?",
          "original_form": "많이 보냈어요?",
          "speaker_id": "2",
          "time": "20210822 02:59"
        },
        {
          "id": "MDRW2100007516.1.31",
          "form": "좋은딸이라고 생각했는데 시간을 많이 못보냈어요",
          "original_form": "좋은딸이라고 생각했는데 시간을 많이 못보냈어요",
          "speaker_id": "2",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007516.1.32",
          "form": "고등학교때부터 기숙사살고\r\n대학교도 멀리가서 기숙사살고\r\n일도 멀리가서 자취하고\r\n결혼도 멀리했네요",
          "original_form": "고등학교때부터 기숙사살고\r\n대학교도 멀리가서 기숙사살고\r\n일도 멀리가서 자취하고\r\n결혼도 멀리했네요",
          "speaker_id": "2",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007516.1.33",
          "form": "저도 아빠랑 시간을 많이 보내야겠어요",
          "original_form": "저도 아빠랑 시간을 많이 보내야겠어요",
          "speaker_id": "1",
          "time": "20210822 03:01"
        }
      ]
    }
