      "utterance": [
        {
          "id": "MDRW2100017152.1.1",
          "form": "오늘 뭔가 날씨가 흐리흐리해요",
          "original_form": "오늘 뭔가 날씨가 흐리흐리해요",
          "speaker_id": "1",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017152.1.2",
          "form": "내일 비오려나요?",
          "original_form": "내일 비오려나요?",
          "speaker_id": "2",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017152.1.3",
          "form": "그러게요 언제 비가와도 놀라지않을것같은 날씨네요 습하구",
          "original_form": "그러게요 언제 비가와도 놀라지않을것같은 날씨네요 습하구",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017152.1.4",
          "form": "비오는거 넘 싫어요ㅠㅠ 눈도 싫고...",
          "original_form": "비오는거 넘 싫어요ㅠㅠ 눈도 싫고...",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017152.1.5",
          "form": "땅이 질척질척",
          "original_form": "땅이 질척질척",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017152.1.6",
          "form": "맞아요ㅠ 아까 산책하고왔는데 넘 꿉꿉해서 짱났어요",
          "original_form": "맞아요ㅠ 아까 산책하고왔는데 넘 꿉꿉해서 짱났어요",
          "speaker_id": "1",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100017152.1.7",
          "form": "날 조금 선선해지니까 바로 미세먼지도 안좋아지구ㅜ",
          "original_form": "날 조금 선선해지니까 바로 미세먼지도 안좋아지구ㅜ",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017152.1.8",
          "form": "선선하진건 좋아요 ㅋㅋㅋ오늘 아침에 추워서 긴팔 입고 나갔어요 ㅋㅋㅋㅋ",
          "original_form": "선선하진건 좋아요 ㅋㅋㅋ오늘 아침에 추워서 긴팔 입고 나갔어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017152.1.9",
          "form": "그랬더니 ㅣ 지금은 넘 더움",
          "original_form": "그랬더니 ㅣ 지금은 넘 더움",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017152.1.10",
          "form": "오 벌써 긴팔입었어요? ㅋㅋ 점심먹으러나갈땐 덥지않으셨어요?",
          "original_form": "오 벌써 긴팔입었어요? ㅋㅋ 점심먹으러나갈땐 덥지않으셨어요?",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017152.1.11",
          "form": "앜ㅋㅋㅋ역시 ㅋㅋㅋ 그치만 정말 새벽엔 추워요",
          "original_form": "앜ㅋㅋㅋ역시 ㅋㅋㅋ 그치만 정말 새벽엔 추워요",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017152.1.12",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017152.1.13",
          "form": "점심 시켜먹었어용 키키",
          "original_form": "점심 시켜먹었어용 키키",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017152.1.14",
          "form": "밤에 추워요 진짜!!",
          "original_form": "밤에 추워요 진짜!!",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017152.1.15",
          "form": "감기걸릴거같아요",
          "original_form": "감기걸릴거같아요",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017152.1.16",
          "form": "환절기 건강 조심하세요",
          "original_form": "환절기 건강 조심하세요",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017152.1.17",
          "form": "맞아요 진짜... 제가그래서 감기걸릴까봐 밤엔 안나가요",
          "original_form": "맞아요 진짜... 제가그래서 감기걸릴까봐 밤엔 안나가요",
          "speaker_id": "1",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017152.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017152.1.19",
          "form": "건강을 위한 집순이 생활 ㅋㅋㅋㅋㅋ",
          "original_form": "건강을 위한 집순이 생활 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017152.1.20",
          "form": "요즘엔 근데 정말 밤엔 선선해서 저 두꺼운이불꺼냈어요",
          "original_form": "요즘엔 근데 정말 밤엔 선선해서 저 두꺼운이불꺼냈어요",
