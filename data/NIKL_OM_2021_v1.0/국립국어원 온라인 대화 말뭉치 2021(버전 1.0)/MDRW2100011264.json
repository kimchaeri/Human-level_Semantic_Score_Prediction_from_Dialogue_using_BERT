      "utterance": [
        {
          "id": "MDRW2100011264.1.1",
          "form": "비가 정말 많이 오네요~",
          "original_form": "비가 정말 많이 오네요~",
          "speaker_id": "1",
          "time": "20210824 10:22"
        },
        {
          "id": "MDRW2100011264.1.2",
          "form": "거기도 많이 오나요~?",
          "original_form": "거기도 많이 오나요~?",
          "speaker_id": "1",
          "time": "20210824 10:22"
        },
        {
          "id": "MDRW2100011264.1.3",
          "form": "다행히 지금은 완전",
          "original_form": "다행히 지금은 완전",
          "speaker_id": "2",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100011264.1.4",
          "form": "멈춘 거 같아요!!!",
          "original_form": "멈춘 거 같아요!!!",
          "speaker_id": "2",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100011264.1.5",
          "form": "여기는 남부지방인데 안쉬고 계속 비가오네요ㅠ",
          "original_form": "여기는 남부지방인데 안쉬고 계속 비가오네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100011264.1.6",
          "form": "ㅠ ㅠㅠ 비가 너무 와서 싫죠",
          "original_form": "ㅠ ㅠㅠ 비가 너무 와서 싫죠",
          "speaker_id": "2",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100011264.1.7",
          "form": "정말 습해요",
          "original_form": "정말 습해요",
          "speaker_id": "2",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100011264.1.8",
          "form": "제 완전 싫어오ㅠㅠ",
          "original_form": "제 완전 싫어오ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011264.1.9",
          "form": "비가 다행히 멈췄는데 정말 습해서",
          "original_form": "비가 다행히 멈췄는데 정말 습해서",
          "speaker_id": "1",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011264.1.10",
          "form": "제습틀어놓고 있네요ㅠ",
          "original_form": "제습틀어놓고 있네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011264.1.11",
          "form": "저두요.. 습해서 죽음입니다",
          "original_form": "저두요.. 습해서 죽음입니다",
          "speaker_id": "2",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011264.1.12",
          "form": "이래서 여름이 싫은것같아요 저는 더운건 괜챦은데 습한거는 못참겠여요ㅠ",
          "original_form": "이래서 여름이 싫은것같아요 저는 더운건 괜챦은데 습한거는 못참겠여요ㅠ",
          "speaker_id": "1",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100011264.1.13",
          "form": "진짜요.......",
          "original_form": "진짜요.......",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100011264.1.14",
          "form": "습기가 최악이죠",
          "original_form": "습기가 최악이죠",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100011264.1.15",
          "form": "얼른 습기없는 가을이 왔으면 좋겠어요ㅠ",
          "original_form": "얼른 습기없는 가을이 왔으면 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100011264.1.16",
          "form": "곧 점차 없어지겠죠????",
          "original_form": "곧 점차 없어지겠죠????",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100011264.1.17",
          "form": "가을이 오고는 있으니 없어질거에요!!",
          "original_form": "가을이 오고는 있으니 없어질거에요!!",
          "speaker_id": "1",
          "time": "20210824 22:38"
        },
        {
          "id": "MDRW2100011264.1.18",
          "form": "근데 빨리왔음 좋겠어용ㅎㅎ",
          "original_form": "근데 빨리왔음 좋겠어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:38"
        },
        {
          "id": "MDRW2100011264.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ저듀요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저듀요",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100011264.1.20",
          "form": "무슨계절 좋아하세요~?",
          "original_form": "무슨계절 좋아하세요~?",
          "speaker_id": "1",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100011264.1.21",
          "form": "저 가을이요 ㅎ",
          "original_form": "저 가을이요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100011264.1.22",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100011264.1.23",
          "form": "오오 저는 봄입니다#",
          "original_form": "오오 저는 봄입니다#",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100011264.1.24",
          "form": "벚꽃보는게 너무 좋아요~",
          "original_form": "벚꽃보는게 너무 좋아요~",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100011264.1.25",
          "form": "대부분의 분들이 가을 선택하시더라구요~",
          "original_form": "대부분의 분들이 가을 선택하시더라구요~",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100011264.1.26",
          "form": "봄 좋긴한데",
          "original_form": "봄 좋긴한데",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100011264.1.27",
          "form": "꽃가루가 흑",
          "original_form": "꽃가루가 흑",
          "speaker_id": "2",
