      "utterance": [
        {
          "id": "MDRW2100012900.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 07:43"
        },
        {
          "id": "MDRW2100012900.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210825 07:47"
        },
        {
          "id": "MDRW2100012900.1.3",
          "form": "치킨 좋아하세요?",
          "original_form": "치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 07:48"
        },
        {
          "id": "MDRW2100012900.1.4",
          "form": "네ㅋㅋ 양념파입니다",
          "original_form": "네ㅋㅋ 양념파입니다",
          "speaker_id": "1",
          "time": "20210825 07:49"
        },
        {
          "id": "MDRW2100012900.1.5",
          "form": "최애 치킨은 어디껀가요? ㅋ",
          "original_form": "최애 치킨은 어디껀가요? ㅋ",
          "speaker_id": "2",
          "time": "20210825 07:52"
        },
        {
          "id": "MDRW2100012900.1.6",
          "form": "저 바비큐보스",
          "original_form": "저 바비큐보스",
          "speaker_id": "1",
          "time": "20210825 07:53"
        },
        {
          "id": "MDRW2100012900.1.7",
          "form": "이거 진짜 좋아하는데",
          "original_form": "이거 진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210825 07:53"
        },
        {
          "id": "MDRW2100012900.1.8",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "1",
          "time": "20210825 07:53"
        },
        {
          "id": "MDRW2100012900.1.9",
          "form": "망한듯",
          "original_form": "망한듯",
          "speaker_id": "1",
          "time": "20210825 07:53"
        },
        {
          "id": "MDRW2100012900.1.10",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "1",
          "time": "20210825 07:54"
        },
        {
          "id": "MDRW2100012900.1.11",
          "form": "오 저는 처음 들어봐요ㅠ",
          "original_form": "오 저는 처음 들어봐요ㅠ",
          "speaker_id": "2",
          "time": "20210825 07:55"
        },
        {
          "id": "MDRW2100012900.1.12",
          "form": "왜 없어졌을까요",
          "original_form": "왜 없어졌을까요",
          "speaker_id": "2",
          "time": "20210825 07:55"
        },
        {
          "id": "MDRW2100012900.1.13",
          "form": "처음 들어보셔서?ㅋ ㄱ",
          "original_form": "처음 들어보셔서?ㅋ ㄱ",
          "speaker_id": "1",
          "time": "20210825 08:02"
        },
        {
          "id": "MDRW2100012900.1.14",
          "form": "앗... 그런건가요ㅋㅋㅋ",
          "original_form": "앗... 그런건가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:07"
        },
        {
          "id": "MDRW2100012900.1.15",
          "form": "그럼 말씀하신 브랜드\r\n없어지고는 어디에\r\n정착하셨어요?",
          "original_form": "그럼 말씀하신 브랜드\r\n없어지고는 어디에\r\n정착하셨어요?",
          "speaker_id": "2",
          "time": "20210825 08:08"
        },
        {
          "id": "MDRW2100012900.1.16",
          "form": "지금은 방랑중입니다",
          "original_form": "지금은 방랑중입니다",
          "speaker_id": "1",
          "time": "20210825 08:31"
        },
        {
          "id": "MDRW2100012900.1.17",
          "form": "숯불바베큐같은게",
          "original_form": "숯불바베큐같은게",
          "speaker_id": "1",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012900.1.18",
          "form": "저랑 잘 맞던데",
          "original_form": "저랑 잘 맞던데",
          "speaker_id": "1",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012900.1.19",
          "form": "불로만? 코리안?ㅋㅋ 지코바?",
          "original_form": "불로만? 코리안?ㅋㅋ 지코바?",
          "speaker_id": "2",
          "time": "20210825 08:38"
        },
        {
          "id": "MDRW2100012900.1.20",
          "form": "지코바가 바비큐보스랑 비슷하댔는데",
          "original_form": "지코바가 바비큐보스랑 비슷하댔는데",
          "speaker_id": "1",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100012900.1.21",
          "form": "저는 아니더라고요ㅋㅋ",
          "original_form": "저는 아니더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100012900.1.22",
          "form": "지코바는 먹어봐서 아는데ㅋㅋ",
          "original_form": "지코바는 먹어봐서 아는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100012900.1.23",
          "form": "그 맛이 아니군용ㅋㅋ",
          "original_form": "그 맛이 아니군용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100012900.1.24",
          "form": "얼른 유목민 벗어나셔야할텐데요",
          "original_form": "얼른 유목민 벗어나셔야할텐데요",
          "speaker_id": "2",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100012900.1.25",
          "form": "굽네",
          "original_form": "굽네",
          "speaker_id": "1",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100012900.1.26",
          "form": "괜찮더라고요",
          "original_form": "괜찮더라고요",
          "speaker_id": "1",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100012900.1.27",
          "form": "아 굽네!",
          "original_form": "아 굽네!",
          "speaker_id": "2",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100012900.1.28",
          "form": "저는 고.추바사삭 좋아합니다ㅋㅋ",
          "original_form": "저는 고.추바사삭 좋아합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:46"
        },
        {
          "id": "MDRW2100012900.1.29",
          "form": "먹어도 죄를 짓지 않는 느낌ㅋㅋ",
          "original_form": "먹어도 죄를 짓지 않는 느낌ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:46"
        }
      ]
    }
  ]
