      "utterance": [
        {
          "id": "MDRW2100032967.1.1",
          "form": "최근 구매하신 물건 있나요",
          "original_form": "최근 구매하신 물건 있나요",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100032967.1.2",
          "form": "전 옷이랑 가방이여",
          "original_form": "전 옷이랑 가방이여",
          "speaker_id": "1",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100032967.1.3",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100032967.1.4",
          "form": "좋으셨겠어요 옷은 가을옷인가요ㅡl?",
          "original_form": "좋으셨겠어요 옷은 가을옷인가요ㅡl?",
          "speaker_id": "2",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100032967.1.5",
          "form": "여름옷이여ㅋㅋ",
          "original_form": "여름옷이여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100032967.1.6",
          "form": "가을옷 사야했나바요",
          "original_form": "가을옷 사야했나바요",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100032967.1.7",
          "form": "금세 가을같기도 하네요 근데 가을은 짧으니 이제 겨울옷을...준비...?",
          "original_form": "금세 가을같기도 하네요 근데 가을은 짧으니 이제 겨울옷을...준비...?",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100032967.1.8",
          "form": "ㅎㅎ 맞아욥",
          "original_form": "ㅎㅎ 맞아욥",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100032967.1.9",
          "form": "패딩의 계절ㅎ",
          "original_form": "패딩의 계절ㅎ",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100032967.1.10",
          "form": "가을옷으로 사고싶은것 있나요ㅏ",
          "original_form": "가을옷으로 사고싶은것 있나요ㅏ",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100032967.1.11",
          "form": "전 가디건이여ㅎ",
          "original_form": "전 가디건이여ㅎ",
          "speaker_id": "1",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100032967.1.12",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100032967.1.13",
          "form": "저는 티셔츠만 늘 입네요..가을 티셔트 사면 좋을듯요..ㅋ",
          "original_form": "저는 티셔츠만 늘 입네요..가을 티셔트 사면 좋을듯요..ㅋ",
          "speaker_id": "2",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100032967.1.14",
          "form": "곧 내복도 사야할듯요ㅎ",
          "original_form": "곧 내복도 사야할듯요ㅎ",
          "speaker_id": "1",
          "time": "20210906 20:11"
        },
        {
          "id": "MDRW2100032967.1.15",
          "form": "ㅎㅎ 나이들수록 내복이 필수네요",
          "original_form": "ㅎㅎ 나이들수록 내복이 필수네요",
          "speaker_id": "2",
          "time": "20210906 20:12"
        },
