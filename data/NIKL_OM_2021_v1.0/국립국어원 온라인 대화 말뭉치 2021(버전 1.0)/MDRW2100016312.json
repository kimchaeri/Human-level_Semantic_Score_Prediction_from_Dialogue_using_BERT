      "utterance": [
        {
          "id": "MDRW2100016312.1.1",
          "form": "안녕하세요 하시는 커뮤니티 있으세요?",
          "original_form": "안녕하세요 하시는 커뮤니티 있으세요?",
          "speaker_id": "1",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100016312.1.2",
          "form": "네 팬카페 활동 하고있어요",
          "original_form": "네 팬카페 활동 하고있어요",
          "speaker_id": "2",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100016312.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100016312.1.4",
          "form": "저는 덕후라는 사이트 를 방문해요ㅎㅎ",
          "original_form": "저는 덕후라는 사이트 를 방문해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100016312.1.5",
          "form": "어떤 팬카페 인가요?",
          "original_form": "어떤 팬카페 인가요?",
          "speaker_id": "1",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100016312.1.6",
          "form": "연예인 팬카페예요!",
          "original_form": "연예인 팬카페예요!",
          "speaker_id": "2",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100016312.1.7",
          "form": "덕후라는 사이트에서는 어떤 활동을 하시나요?",
          "original_form": "덕후라는 사이트에서는 어떤 활동을 하시나요?",
          "speaker_id": "2",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100016312.1.8",
          "form": "팬레터 같은 것도 쓰시겠네요ㅎㅎ",
          "original_form": "팬레터 같은 것도 쓰시겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016312.1.9",
          "form": "주로 핫딜 정보를 얻어요ㅎㅎ",
          "original_form": "주로 핫딜 정보를 얻어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016312.1.10",
          "form": "눈팅을 주로 합니다",
          "original_form": "눈팅을 주로 합니다",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016312.1.11",
          "form": "유용하네요",
          "original_form": "유용하네요",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016312.1.12",
          "form": "직접 만나기도 하시나요?",
          "original_form": "직접 만나기도 하시나요?",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016312.1.13",
          "form": "익명 커뮤니티라 주로 만나지는 않고",
          "original_form": "익명 커뮤니티라 주로 만나지는 않고",
          "speaker_id": "1",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016312.1.14",
          "form": "정보만 주고받아용ㅎㅎ",
          "original_form": "정보만 주고받아용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016312.1.15",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016312.1.16",
          "form": "가입도 할 필요 없나요 그럼?",
          "original_form": "가입도 할 필요 없나요 그럼?",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016312.1.17",
          "form": "가입은 해야 댓글 작성이나 글을 볼 수 있어요",
          "original_form": "가입은 해야 댓글 작성이나 글을 볼 수 있어요",
          "speaker_id": "1",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100016312.1.18",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100016312.1.19",
          "form": "정보 얻으셔서 샀던 거중에 제일 좋았던건 뭔가요?",
          "original_form": "정보 얻으셔서 샀던 거중에 제일 좋았던건 뭔가요?",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016312.1.20",
          "form": "최근에는 현대패션 몰에서 포인트 줘서 구매 하는게 있었는데 포인트로 저렴하게 결제했어요!ㅋㅋ",
          "original_form": "최근에는 현대패션 몰에서 포인트 줘서 구매 하는게 있었는데 포인트로 저렴하게 결제했어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016312.1.21",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016312.1.22",
          "form": "요즘 이런 커뮤니티 많이 생겼더라고요 한번 가입해 보세용!",
          "original_form": "요즘 이런 커뮤니티 많이 생겼더라고요 한번 가입해 보세용!",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
