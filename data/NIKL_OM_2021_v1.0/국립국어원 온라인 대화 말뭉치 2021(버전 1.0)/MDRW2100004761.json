      "utterance": [
        {
          "id": "MDRW2100004761.1.1",
          "form": "설거지 하는 거 어떠세요?",
          "original_form": "설거지 하는 거 어떠세요?",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004761.1.2",
          "form": "전 정말 싫어해요",
          "original_form": "전 정말 싫어해요",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004761.1.3",
          "form": "저도 싫어해요ㅠㅠ",
          "original_form": "저도 싫어해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004761.1.4",
          "form": "설거지는 하는데 음쓰 버릴때 눈물나요",
          "original_form": "설거지는 하는데 음쓰 버릴때 눈물나요",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004761.1.5",
          "form": "아 저도 알죠",
          "original_form": "아 저도 알죠",
          "speaker_id": "2",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004761.1.6",
          "form": "진짜 음쓰 너무 싫어요",
          "original_form": "진짜 음쓰 너무 싫어요",
          "speaker_id": "2",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004761.1.7",
          "form": "그쵸 진짜 눈물나요 엉엉",
          "original_form": "그쵸 진짜 눈물나요 엉엉",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004761.1.8",
          "form": "분리수거를 하는게 낫지",
          "original_form": "분리수거를 하는게 낫지",
          "speaker_id": "1",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004761.1.9",
          "form": "음쓰는 못버리겠어요",
          "original_form": "음쓰는 못버리겠어요",
          "speaker_id": "1",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004761.1.10",
          "form": "네 ... 음쓰 냄새가",
          "original_form": "네 ... 음쓰 냄새가",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004761.1.11",
          "form": "진짜 뇌까지 진동햐요",
          "original_form": "진짜 뇌까지 진동햐요",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004761.1.12",
          "form": "맞아요 근데 분리수거도 너무 복잡하지 않아요?",
          "original_form": "맞아요 근데 분리수거도 너무 복잡하지 않아요?",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004761.1.13",
          "form": "지역마다도 다르다고 하던데",
          "original_form": "지역마다도 다르다고 하던데",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004761.1.14",
          "form": "네.. 근데 이게 지역마다 다르다니 신기하네요",
          "original_form": "네.. 근데 이게 지역마다 다르다니 신기하네요",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004761.1.15",
          "form": "그러니까요 생각보다 너무 복잡해요ㅠㅠ",
          "original_form": "그러니까요 생각보다 너무 복잡해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004761.1.16",
          "form": "name2님은 가족이랑 같이 사세요?",
          "original_form": "&name2&님은 가족이랑 같이 사세요?",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004761.1.17",
          "form": "네 ㅎ name1님은ㅇ6?",
          "original_form": "네 ㅎ &name1&님은ㅇ6?",
          "speaker_id": "2",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100004761.1.18",
          "form": "저도 가족들이랑 살아요",
          "original_form": "저도 가족들이랑 살아요",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100004761.1.19",
          "form": "원랜 자취했었는데 가족들이랑 다시 살게 됐어요",
          "original_form": "원랜 자취했었는데 가족들이랑 다시 살게 됐어요",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100004761.1.20",
          "form": "자취  할때  어땠어요?",
          "original_form": "자취  할때  어땠어요?",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100004761.1.21",
          "form": "좋았어요ㅋㅋㅋㅋ장점은 돈 빼고 다",
          "original_form": "좋았어요ㅋㅋㅋㅋ장점은 돈 빼고 다",
          "speaker_id": "1",
          "time": "20210822 09:39"
        },
        {
          "id": "MDRW2100004761.1.22",
          "form": "단점은 돈!이었어요",
          "original_form": "단점은 돈!이었어요",
          "speaker_id": "1",
          "time": "20210822 09:39"
        },
        {
          "id": "MDRW2100004761.1.23",
          "form": "집안일도 제것만 하면 되니까 오히려 편한",
          "original_form": "집안일도 제것만 하면 되니까 오히려 편한",
          "speaker_id": "1",
          "time": "20210822 09:39"
        },
        {
          "id": "MDRW2100004761.1.24",
          "form": "아 ㅋㅋㅋㅋ 역시나 현실적인",
          "original_form": "아 ㅋㅋㅋㅋ 역시나 현실적인",
          "speaker_id": "2",
          "time": "20210822 11:58"
        },
        {
          "id": "MDRW2100004761.1.25",
          "form": "돈이 문제군요",
