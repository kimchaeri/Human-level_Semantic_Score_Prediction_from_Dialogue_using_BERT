      "utterance": [
        {
          "id": "MDRW2100007159.1.1",
          "form": "야식먹어야겠어요",
          "original_form": "야식먹어야겠어요",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007159.1.2",
          "form": "매운거 좋아하시면",
          "original_form": "매운거 좋아하시면",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007159.1.3",
          "form": "내일또 음식얘기해주실래요?",
          "original_form": "내일또 음식얘기해주실래요?",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007159.1.4",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007159.1.5",
          "form": "편의점에 틈새라면",
          "original_form": "편의점에 틈새라면",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007159.1.6",
          "form": "매운김치맛 새로 나온거",
          "original_form": "매운김치맛 새로 나온거",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007159.1.7",
          "form": "헉!!! 그거 진짜 맵다던데",
          "original_form": "헉!!! 그거 진짜 맵다던데",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007159.1.8",
          "form": "불닭이랑비슷하겠죠",
          "original_form": "불닭이랑비슷하겠죠",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007159.1.9",
          "form": "드셔보세요 ㅋㅋㅋㅋ 가지마세요 먹지말고 놀아옄ㅋㅋㅋ",
          "original_form": "드셔보세요 ㅋㅋㅋㅋ 가지마세요 먹지말고 놀아옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007159.1.10",
          "form": "ㅋㅋㅋㅋ저진짜 너무배고파요 음식얘기하니까",
          "original_form": "ㅋㅋㅋㅋ저진짜 너무배고파요 음식얘기하니까",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007159.1.11",
          "form": "저도 아직 매운김치맛은 못 먹어봤는데",
          "original_form": "저도 아직 매운김치맛은 못 먹어봤는데",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007159.1.12",
          "form": "낼 도전해보려구요",
          "original_form": "낼 도전해보려구요",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007159.1.13",
          "form": "사실 치킨받으려고시작했는데치킨받기전에 5마리는시키겠어요",
          "original_form": "사실 치킨받으려고시작했는데치킨받기전에 5마리는시키겠어요",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007159.1.14",
          "form": "저녁 안드셨어요???",
          "original_form": "저녁 안드셨어요???",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007159.1.15",
          "form": "오 미니스톱에도 팔겠죠?",
          "original_form": "오 미니스톱에도 팔겠죠?",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007159.1.16",
          "form": "먹었어요 제가 먹성이좋아요",
          "original_form": "먹었어요 제가 먹성이좋아요",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007159.1.17",
          "form": "먹방찍어도될듯요",
          "original_form": "먹방찍어도될듯요",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007159.1.18",
          "form": "아 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007159.1.19",
          "form": "저도 ㅋㅋㅋㅋ먹성좋아서ㅋㅋㅋ",
          "original_form": "저도 ㅋㅋㅋㅋ먹성좋아서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007159.1.20",
          "form": "소화가잘돼서ㅎ일안풀리면 먹방비제이하게요...",
          "original_form": "소화가잘돼서ㅎ일안풀리면 먹방비제이하게요...",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
