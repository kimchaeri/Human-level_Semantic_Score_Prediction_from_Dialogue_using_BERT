      "utterance": [
        {
          "id": "MDRW2100040259.1.1",
          "form": "자두맛 녹차 좀 마십니다",
          "original_form": "자두맛 녹차 좀 마십니다",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040259.1.2",
          "form": "환희 점에서 뭐 먹어 봤냐구요",
          "original_form": "환희 점에서 뭐 먹어 봤냐구요",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040259.1.3",
          "form": "편의점에 파나요",
          "original_form": "편의점에 파나요",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040259.1.4",
          "form": "편의점에서 파는 녹차 중에 자두맛 드셔본 적 있나요",
          "original_form": "편의점에서 파는 녹차 중에 자두맛 드셔본 적 있나요",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040259.1.5",
          "form": "먹고 싶지 않은 이름인데요",
          "original_form": "먹고 싶지 않은 이름인데요",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040259.1.6",
          "form": "자두 녹차 이름이 자두 녹차에요",
          "original_form": "자두 녹차 이름이 자두 녹차에요",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040259.1.7",
          "form": "복숭아 맛 녹차나 레몬 맛 녹차는 드셔보셨어요",
          "original_form": "복숭아 맛 녹차나 레몬 맛 녹차는 드셔보셨어요",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040259.1.8",
          "form": "자두맛 녹차인데 진짜 새콤달콤 맛있어요",
          "original_form": "자두맛 녹차인데 진짜 새콤달콤 맛있어요",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040259.1.9",
          "form": "아니요 녹차에다가 무슨 짓을 한 거예요",
          "original_form": "아니요 녹차에다가 무슨 짓을 한 거예요",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040259.1.10",
          "form": "제일 제 취향입니다",
          "original_form": "제일 제 취향입니다",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040259.1.11",
          "form": "저보다 심하시네요",
          "original_form": "저보다 심하시네요",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040259.1.12",
          "form": "이름만 녹차이지 자두맛 음료랑 다를바가 없어요",
          "original_form": "이름만 녹차이지 자두맛 음료랑 다를바가 없어요",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040259.1.13",
          "form": "그럼 그냥 자두 음료를 드시는 게 낫지 않을까요",
          "original_form": "그럼 그냥 자두 음료를 드시는 게 낫지 않을까요",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040259.1.14",
          "form": "전 그런 거 잘 시도해서 먹어 보는 편입니다",
          "original_form": "전 그런 거 잘 시도해서 먹어 보는 편입니다",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040259.1.15",
          "form": "시중에 자두맛 음료 별로 없잖아요",
          "original_form": "시중에 자두맛 음료 별로 없잖아요",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040259.1.16",
          "form": "대단하신 용기 입니다",
          "original_form": "대단하신 용기 입니다",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040259.1.17",
          "form": "존맛탱이 나쁜 말인가요",
          "original_form": "존맛탱이 나쁜 말인가요",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040259.1.18",
          "form": "그러고 보니 없네요",
          "original_form": "그러고 보니 없네요",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040259.1.19",
          "form": "네ㅋㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040259.1.20",
          "form": "자두맛 음료 꼭 드셔 보시길 바래요",
          "original_form": "자두맛 음료 꼭 드셔 보시길 바래요",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040259.1.21",
          "form": "강추합니다",
          "original_form": "강추합니다",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040259.1.22",
          "form": "어제 된장찌개 끓여 놨어요",
          "original_form": "어제 된장찌개 끓여 놨어요",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040259.1.23",
          "form": "조금 짠 감이 없지 않아 있긴 한데",
          "original_form": "조금 짠 감이 없지 않아 있긴 한데",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040259.1.24",
          "form": "끓여 놓았으니 어쩌겠어요 먹어야죠",
          "original_form": "끓여 놓았으니 어쩌겠어요 먹어야죠",
          "speaker_id": "2",
          "time": "20210904 15:13"
        }
      ]
    }
