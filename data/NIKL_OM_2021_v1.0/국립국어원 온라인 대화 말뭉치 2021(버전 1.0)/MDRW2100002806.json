      "utterance": [
        {
          "id": "MDRW2100002806.1.1",
          "form": "다이어트하고싶은데 ㅠ",
          "original_form": "다이어트하고싶은데 ㅠ",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002806.1.2",
          "form": "나는 매일 퇴근하고 gx하러가는데",
          "original_form": "나는 매일 퇴근하고 gx하러가는데",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002806.1.3",
          "form": "혹시 매일하는 운동있어?",
          "original_form": "혹시 매일하는 운동있어?",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002806.1.4",
          "form": "지금은 아니고 한때 매일 등산했어",
          "original_form": "지금은 아니고 한때 매일 등산했어",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002806.1.5",
          "form": "한두시간씩",
          "original_form": "한두시간씩",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002806.1.6",
          "form": "gx 하면 안빠지고 더 잘할수 잇겠다\r\n여럿이서 하니까\r\n아닌가?",
          "original_form": "gx 하면 안빠지고 더 잘할수 잇겠다\r\n여럿이서 하니까\r\n아닌가?",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002806.1.7",
          "form": "아무래도 비용을 지불하고 강제성이 있어야 꾸준히 하게되는것 같아",
          "original_form": "아무래도 비용을 지불하고 강제성이 있어야 꾸준히 하게되는것 같아",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002806.1.8",
          "form": "나는 gx 재미있고 좋아",
          "original_form": "나는 gx 재미있고 좋아",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002806.1.9",
          "form": "그건 그래 아까워서 안빠져야할것 같아 ㅎ",
          "original_form": "그건 그래 아까워서 안빠져야할것 같아 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002806.1.10",
          "form": "효과있어?",
          "original_form": "효과있어?",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002806.1.11",
          "form": "근력이랑 체력은 늘었는데 역시 다이어트는 90%가 식단",
          "original_form": "근력이랑 체력은 늘었는데 역시 다이어트는 90%가 식단",
          "speaker_id": "1",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002806.1.12",
          "form": "10%가 운동",
          "original_form": "10%가 운동",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002806.1.13",
          "form": "식단조절안하니까 체중변화는 없었어",
          "original_form": "식단조절안하니까 체중변화는 없었어",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002806.1.14",
          "form": "ㅋㅋㅋ그래도 운동하니 유지되는걸거야",
          "original_form": "ㅋㅋㅋ그래도 운동하니 유지되는걸거야",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002806.1.15",
          "form": "오늘 불금인데 운동만 하려고?",
          "original_form": "오늘 불금인데 운동만 하려고?",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002806.1.16",
          "form": "응 운동하고 끝나면 아내랑 산책하거나 아니면 근처 카페갔다올 계획이야",
          "original_form": "응 운동하고 끝나면 아내랑 산책하거나 아니면 근처 카페갔다올 계획이야",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002806.1.17",
          "form": "아내랑 같이 운동다녀?",
          "original_form": "아내랑 같이 운동다녀?",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002806.1.18",
          "form": "보기좋다",
          "original_form": "보기좋다",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002806.1.19",
          "form": "같이 다녔는데 아내는 지금 몸이 안좋아서 쉬는 중",
          "original_form": "같이 다녔는데 아내는 지금 몸이 안좋아서 쉬는 중",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002806.1.20",
          "form": "아, 그렇구나",
          "original_form": "아, 그렇구나",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002806.1.21",
          "form": "신혼냄새가 나는데 ㅎ",
          "original_form": "신혼냄새가 나는데 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002806.1.22",
          "form": "서로 건강도 챙기고",
          "original_form": "서로 건강도 챙기고",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002806.1.23",
          "form": "아직 신혼이야",
          "original_form": "아직 신혼이야",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002806.1.24",
          "form": "같이 운동가면 아주머니들께서 보기 좋다고 해주셨어",
          "original_form": "같이 운동가면 아주머니들께서 보기 좋다고 해주셨어",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002806.1.25",
          "form": "같이 운동하는거 즐겁고 좋아",
          "original_form": "같이 운동하는거 즐겁고 좋아",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002806.1.26",
          "form": "서로 자극도 되고",
          "original_form": "서로 자극도 되고",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002806.1.27",
          "form": "그럴것 같아",
          "original_form": "그럴것 같아",
          "speaker_id": "2",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002806.1.28",
          "form": "나도 남편이랑 등산다니고파서 등산화 같이 샀는데",
          "original_form": "나도 남편이랑 등산다니고파서 등산화 같이 샀는데",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002806.1.29",
          "form": "이젠 내가 게을러졌어 ㅎ",
          "original_form": "이젠 내가 게을러졌어 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
