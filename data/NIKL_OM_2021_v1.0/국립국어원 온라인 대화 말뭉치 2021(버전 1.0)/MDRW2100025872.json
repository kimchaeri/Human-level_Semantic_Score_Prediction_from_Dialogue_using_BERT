      "utterance": [
        {
          "id": "MDRW2100025872.1.1",
          "form": "최근에 어디 여행가셨나요",
          "original_form": "최근에 어디 여행가셨나요",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025872.1.2",
          "form": "대만이었습니다",
          "original_form": "대만이었습니다",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025872.1.3",
          "form": "2년쯤 전이라 가옥이 잘 안나요",
          "original_form": "2년쯤 전이라 가옥이 잘 안나요",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025872.1.4",
          "form": "어디 여행을 가셨나요?",
          "original_form": "어디 여행을 가셨나요?",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025872.1.5",
          "form": "기억에 남는 여행지가 있나요",
          "original_form": "기억에 남는 여행지가 있나요",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025872.1.6",
          "form": "저는 최근에 촌캉스다녀왔습니다 울진이요",
          "original_form": "저는 최근에 촌캉스다녀왔습니다 울진이요",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025872.1.7",
          "form": "오키나와 입니다 마음의 고향입니다",
          "original_form": "오키나와 입니다 마음의 고향입니다",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025872.1.8",
          "form": "오키나와 몇 번 가셧나요",
          "original_form": "오키나와 몇 번 가셧나요",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025872.1.9",
          "form": "울진이면 제 거주지 근처군요",
          "original_form": "울진이면 제 거주지 근처군요",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025872.1.10",
          "form": "오키나와 3번 간것같습니다 2번인가?",
          "original_form": "오키나와 3번 간것같습니다 2번인가?",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025872.1.11",
          "form": "울진에서 뭐했나요?",
          "original_form": "울진에서 뭐했나요?",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025872.1.12",
          "form": "울진에서 바베큐하고 전구워먹었습니다",
          "original_form": "울진에서 바베큐하고 전구워먹었습니다",
          "speaker_id": "1",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025872.1.13",
          "form": "휴가안가셧나요",
          "original_form": "휴가안가셧나요",
          "speaker_id": "1",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025872.1.14",
          "form": "누구랑 갔습니까",
          "original_form": "누구랑 갔습니까",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025872.1.15",
          "form": "아직 안갔습니다",
          "original_form": "아직 안갔습니다",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025872.1.16",
          "form": "남편이랑 갔네요 ㅎㅎ 계곡도 갔습니다",
          "original_form": "남편이랑 갔네요 ㅎㅎ 계곡도 갔습니다",
          "speaker_id": "1",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025872.1.17",
          "form": "언제가시나요",
          "original_form": "언제가시나요",
          "speaker_id": "1",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025872.1.18",
          "form": "함께 여행할수있는 남편이 있어서 좋으시겠어요",
          "original_form": "함께 여행할수있는 남편이 있어서 좋으시겠어요",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025872.1.19",
          "form": "아직 계획없습니다",
          "original_form": "아직 계획없습니다",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025872.1.20",
          "form": "휴가는",
          "original_form": "휴가는",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025872.1.21",
          "form": "대구나 경주는 자주 갑니다",
          "original_form": "대구나 경주는 자주 갑니다",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025872.1.22",
          "form": "경주는 어디 여행가시나요",
          "original_form": "경주는 어디 여행가시나요",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025872.1.23",
          "form": "동생 원룸에서 시간 보내다가",
          "original_form": "동생 원룸에서 시간 보내다가",
          "speaker_id": "2",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025872.1.24",
          "form": "카페가는 정도입니다",
          "original_form": "카페가는 정도입니다",
          "speaker_id": "2",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025872.1.25",
          "form": "가을되면 관광지 가볼게요",
          "original_form": "가을되면 관광지 가볼게요",
          "speaker_id": "2",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025872.1.26",
          "form": "여행 꼭 가고싶네요",
          "original_form": "여행 꼭 가고싶네요",
          "speaker_id": "1",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025872.1.27",
          "form": "언제 대구 옵니까",
          "original_form": "언제 대구 옵니까",
          "speaker_id": "2",
          "time": "20210901 17:40"
        }
      ]
    }
  ]
