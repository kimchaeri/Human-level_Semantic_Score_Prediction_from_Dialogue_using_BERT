      "utterance": [
        {
          "id": "MDRW2100035236.1.1",
          "form": "박하선이 성격은 좋은듯",
          "original_form": "박하선이 성격은 좋은듯",
          "speaker_id": "2",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100035236.1.2",
          "form": "결혼도 잘했고",
          "original_form": "결혼도 잘했고",
          "speaker_id": "1",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100035236.1.3",
          "form": "엉 어남선이 요리를 엄청 잘 해주겠지",
          "original_form": "엉 어남선이 요리를 엄청 잘 해주겠지",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100035236.1.4",
          "form": "그래 펀스토랑에서 엄청 잘하더만",
          "original_form": "그래 펀스토랑에서 엄청 잘하더만",
          "speaker_id": "1",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100035236.1.5",
          "form": "응 근데 부부가 그렇게 확 뜨지를 못해서",
          "original_form": "응 근데 부부가 그렇게 확 뜨지를 못해서",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100035236.1.6",
          "form": "뭐 그정도 까지는 아닐거 같고",
          "original_form": "뭐 그정도 까지는 아닐거 같고",
          "speaker_id": "1",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100035236.1.7",
          "form": "그렇게 많이 벌은거 같지는 않더라 집을 보니까",
          "original_form": "그렇게 많이 벌은거 같지는 않더라 집을 보니까",
          "speaker_id": "2",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100035236.1.8",
          "form": "매니저 착해 보인다",
          "original_form": "매니저 착해 보인다",
          "speaker_id": "1",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100035236.1.9",
          "form": "엉 나이도 어려보인다",
          "original_form": "엉 나이도 어려보인다",
          "speaker_id": "2",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100035236.1.10",
          "form": "그래도 우리보다는 훨 낫자나",
          "original_form": "그래도 우리보다는 훨 낫자나",
          "speaker_id": "1",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100035236.1.11",
          "form": "드라마 감독이 되게 열정적이다",
          "original_form": "드라마 감독이 되게 열정적이다",
          "speaker_id": "2",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100035236.1.12",
          "form": "그러네. 귀를 막네",
          "original_form": "그러네. 귀를 막네",
          "speaker_id": "1",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100035236.1.13",
          "form": "우리보다 낫지 당연히",
          "original_form": "우리보다 낫지 당연히",
          "speaker_id": "2",
          "time": "20210829 12:37"
