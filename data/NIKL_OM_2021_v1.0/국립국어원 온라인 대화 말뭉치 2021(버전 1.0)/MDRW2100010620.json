      "utterance": [
        {
          "id": "MDRW2100010620.1.1",
          "form": "요새 마케팅 죄다 유튜브로 당하고 있음",
          "original_form": "요새 마케팅 죄다 유튜브로 당하고 있음",
          "speaker_id": "1",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010620.1.2",
          "form": "플립도 유재석 찾아보다가 그만...",
          "original_form": "플립도 유재석 찾아보다가 그만...",
          "speaker_id": "1",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010620.1.3",
          "form": "유느님 팬이시군",
          "original_form": "유느님 팬이시군",
          "speaker_id": "2",
          "time": "20210823 20:10"
        },
        {
          "id": "MDRW2100010620.1.4",
          "form": "원래 좋았는데 요새 더 그럼",
          "original_form": "원래 좋았는데 요새 더 그럼",
          "speaker_id": "1",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010620.1.5",
          "form": "뭔가 다양하게 나오기도 했구",
          "original_form": "뭔가 다양하게 나오기도 했구",
          "speaker_id": "1",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010620.1.6",
          "form": "덕질은 좋지",
          "original_form": "덕질은 좋지",
          "speaker_id": "2",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100010620.1.7",
          "form": "요샌 티빙 결제하고 계속 틀어놓는거 같음",
          "original_form": "요샌 티빙 결제하고 계속 틀어놓는거 같음",
          "speaker_id": "1",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010620.1.8",
          "form": "메이저라 좋겠어요",
          "original_form": "메이저라 좋겠어요",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010620.1.9",
          "form": "메이저 좋아요",
          "original_form": "메이저 좋아요",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010620.1.10",
          "form": "몇년에 한번 음원도 내주고",
          "original_form": "몇년에 한번 음원도 내주고",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010620.1.11",
          "form": "좋아하는 연예인 없나바여",
          "original_form": "좋아하는 연예인 없나바여",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010620.1.12",
          "form": "전 게임이 취미라",
          "original_form": "전 게임이 취미라",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010620.1.13",
          "form": "잘 몰라여 ㅋㅋ",
          "original_form": "잘 몰라여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010620.1.14",
          "form": "티비나 유튜브도 안보나?",
          "original_form": "티비나 유튜브도 안보나?",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010620.1.15",
          "form": "티비는 야구만 보고",
          "original_form": "티비는 야구만 보고",
          "speaker_id": "2",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100010620.1.16",
          "form": "유튭도 게임 방송위주로 보는듯?",
          "original_form": "유튭도 게임 방송위주로 보는듯?",
          "speaker_id": "2",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100010620.1.17",
          "form": "짬타수아라고 재미있는 유튜버 있어",
          "original_form": "짬타수아라고 재미있는 유튜버 있어",
          "speaker_id": "2",
          "time": "20210823 20:22"
        },
        {
          "id": "MDRW2100010620.1.18",
          "form": "함봐봐 ㅋㅋ",
          "original_form": "함봐봐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:22"
        },
        {
          "id": "MDRW2100010620.1.19",
          "form": "조아.. 난 게임 유튜버는 잘 모르겠고",
          "original_form": "조아.. 난 게임 유튜버는 잘 모르겠고",
          "speaker_id": "1",
          "time": "20210823 20:22"
        },
        {
          "id": "MDRW2100010620.1.20",
          "form": "리듬겜은 가끔 찾아봄",
          "original_form": "리듬겜은 가끔 찾아봄",
          "speaker_id": "1",
          "time": "20210823 20:22"
        },
        {
          "id": "MDRW2100010620.1.21",
          "form": "리겜 머해여",
          "original_form": "리겜 머해여",
          "speaker_id": "2",
          "time": "20210823 20:22"
        },
        {
          "id": "MDRW2100010620.1.22",
          "form": "리겜 플레이 영상은 카타르시스가 있어",
          "original_form": "리겜 플레이 영상은 카타르시스가 있어",
          "speaker_id": "1",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010620.1.23",
          "form": "리겜 하진 않아여",
          "original_form": "리겜 하진 않아여",
          "speaker_id": "1",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010620.1.24",
          "form": "아 맞져 다른사람이 잘하는거 보는게 재미잇어",
          "original_form": "아 맞져 다른사람이 잘하는거 보는게 재미잇어",
          "speaker_id": "2",
          "time": "20210823 20:25"
        }
      ]
    }
