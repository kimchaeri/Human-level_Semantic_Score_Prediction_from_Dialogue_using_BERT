        },
        {
          "id": "MDRW2100042242.1.7",
          "form": "먼가글이야",
          "original_form": "먼가글이야",
          "speaker_id": "1",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100042242.1.8",
          "form": "리스테린?",
          "original_form": "리스테린?",
          "speaker_id": "1",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100042242.1.9",
          "form": "어!!!",
          "original_form": "어!!!",
          "speaker_id": "2",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100042242.1.10",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042242.1.11",
          "form": "대충 사는 여자 외우지 않고 ㅋㅋㅋ",
          "original_form": "대충 사는 여자 외우지 않고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042242.1.12",
          "form": "입이 까질거같다고",
          "original_form": "입이 까질거같다고",
          "speaker_id": "1",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042242.1.13",
          "form": "1리터에 육천얼마자나",
          "original_form": "1리터에 육천얼마자나",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042242.1.14",
          "form": "사야햇어 ㅋㅋㅋㅋ",
          "original_form": "사야햇어 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042242.1.15",
          "form": "아침에 이딱고도 뭔가 짱나면 그거 하고 가면 좋아",
          "original_form": "아침에 이딱고도 뭔가 짱나면 그거 하고 가면 좋아",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042242.1.16",
          "form": "가솔린 헹군 기분",
          "original_form": "가솔린 헹군 기분",
          "speaker_id": "2",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100042242.1.17",
          "form": "난 테라브레스아님못쓰겟더라구",
          "original_form": "난 테라브레스아님못쓰겟더라구",
