      "utterance": [
        {
          "id": "MDRW2100002706.1.1",
          "form": "우와 포장해온거야?",
          "original_form": "우와 포장해온거야?",
          "speaker_id": "2",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002706.1.2",
          "form": "탐앤탐스 허니브레드 먹어봤니?",
          "original_form": "탐앤탐스 허니브레드 먹어봤니?",
          "speaker_id": "1",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002706.1.3",
          "form": "아니 카카오톡에서 세일을 하더라구.",
          "original_form": "아니 카카오톡에서 세일을 하더라구.",
          "speaker_id": "1",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002706.1.4",
          "form": "4개 주문해서 집에서 해먹을 수 있어.",
          "original_form": "4개 주문해서 집에서 해먹을 수 있어.",
          "speaker_id": "1",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002706.1.5",
          "form": "아 그럼 기프티콘으로 산거야?",
          "original_form": "아 그럼 기프티콘으로 산거야?",
          "speaker_id": "2",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002706.1.6",
          "form": "아하아하",
          "original_form": "아하아하",
          "speaker_id": "2",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002706.1.7",
          "form": "아니.",
          "original_form": "아니.",
          "speaker_id": "1",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002706.1.8",
          "form": "4개 다 먹었니 어제?",
          "original_form": "4개 다 먹었니 어제?",
          "speaker_id": "2",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002706.1.9",
          "form": "오늘 와서 아직 하나도 안먿었어.",
          "original_form": "오늘 와서 아직 하나도 안먿었어.",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002706.1.10",
          "form": "집에 시나몬 가루도 있고 휘핑크림도 있어",
          "original_form": "집에 시나몬 가루도 있고 휘핑크림도 있어",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002706.1.11",
          "form": "올려먹으면 참 맛있을 거 같아.",
          "original_form": "올려먹으면 참 맛있을 거 같아.",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002706.1.12",
          "form": "그렇지?",
          "original_form": "그렇지?",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002706.1.13",
          "form": "우와 휘핑크림이랑 아주 잘어울려",
          "original_form": "우와 휘핑크림이랑 아주 잘어울려",
          "speaker_id": "2",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002706.1.14",
          "form": "먹는 사진 나중에 보여줘!",
          "original_form": "먹는 사진 나중에 보여줘!",
          "speaker_id": "2",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002706.1.15",
          "form": "얼려뒀다가 녹여먹는거야?",
          "original_form": "얼려뒀다가 녹여먹는거야?",
          "speaker_id": "2",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002706.1.16",
          "form": "좋아.",
          "original_form": "좋아.",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002706.1.17",
          "form": "보관은 얼려두면되고",
          "original_form": "보관은 얼려두면되고",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002706.1.18",
          "form": "에어프라이어나 오븐에 구우면 돼",
          "original_form": "에어프라이어나 오븐에 구우면 돼",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002706.1.19",
          "form": "간단하지?",
          "original_form": "간단하지?",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002706.1.20",
          "form": "에어프라이어는 정말 만능이구나",
          "original_form": "에어프라이어는 정말 만능이구나",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002706.1.21",
          "form": "허니브레드 네개를 구매하면 아이스아메리카노 쿠폰 하나를 주더라. 대단하지 않니?",
          "original_form": "허니브레드 네개를 구매하면 아이스아메리카노 쿠폰 하나를 주더라. 대단하지 않니?",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002706.1.22",
          "form": "카페를 간단하게 집에서 즐길 수 있겠다",
          "original_form": "카페를 간단하게 집에서 즐길 수 있겠다",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002706.1.23",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002706.1.24",
          "form": "맞아. 너도 기회가 된다면 에어프라이기를 사염.",
          "original_form": "맞아. 너도 기회가 된다면 에어프라이기를 사염.",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002706.1.25",
          "form": "ㅠㅠ하지만 너무 크고 비싸",
          "original_form": "ㅠㅠ하지만 너무 크고 비싸",
          "speaker_id": "2",
          "time": "20210820 15:01"
        }
      ]
    }
  ]
