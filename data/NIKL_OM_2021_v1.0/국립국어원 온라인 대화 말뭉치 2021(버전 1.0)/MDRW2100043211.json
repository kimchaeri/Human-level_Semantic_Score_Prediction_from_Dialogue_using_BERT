      "utterance": [
        {
          "id": "MDRW2100043211.1.1",
          "form": "저도안사용ㅎㅎ..",
          "original_form": "저도안사용ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043211.1.2",
          "form": "음선물은많이사요",
          "original_form": "음선물은많이사요",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043211.1.3",
          "form": "주로먹는거사서주변에뿌리죵ㅎㅎ",
          "original_form": "주로먹는거사서주변에뿌리죵ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043211.1.4",
          "form": "그러시구나 그게 제일 편하죠",
          "original_form": "그러시구나 그게 제일 편하죠",
          "speaker_id": "2",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043211.1.5",
          "form": "보면 바디제품 같은거 선물로 많이들 하더라고요",
          "original_form": "보면 바디제품 같은거 선물로 많이들 하더라고요",
          "speaker_id": "2",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043211.1.6",
          "form": "ㅎㅎ비누같은게사이즈도작아서좋죵",
          "original_form": "ㅎㅎ비누같은게사이즈도작아서좋죵",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043211.1.7",
          "form": "부피 크면 힘드니까!!",
          "original_form": "부피 크면 힘드니까!!",
          "speaker_id": "2",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100043211.1.8",
          "form": "마그넷 모으시는 분들 보면 와",
          "original_form": "마그넷 모으시는 분들 보면 와",
          "speaker_id": "2",
          "time": "20210907 15:51"
        },
        {
          "id": "MDRW2100043211.1.9",
          "form": "모아놓으면 진짜 예쁘더라고요",
          "original_form": "모아놓으면 진짜 예쁘더라고요",
          "speaker_id": "2",
          "time": "20210907 15:51"
        },
        {
          "id": "MDRW2100043211.1.10",
          "form": "아하 맞아요 많이사죵ㅎㅎ",
          "original_form": "아하 맞아요 많이사죵ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043211.1.11",
          "form": "저는면세찬스는열심히써요",
          "original_form": "저는면세찬스는열심히써요",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043211.1.12",
          "form": "name1님 사놓으셨으면 화려했을거 같아요!!",
          "original_form": "&name1&님 사놓으셨으면 화려했을거 같아요!!",
          "speaker_id": "2",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043211.1.13",
          "form": "아 역시 면세!",
          "original_form": "아 역시 면세!",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043211.1.14",
          "form": "헤헤 그리많이다니진않았어용",
          "original_form": "헤헤 그리많이다니진않았어용",
          "speaker_id": "1",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043211.1.15",
          "form": "최근구매하신거있나요",
          "original_form": "최근구매하신거있나요",
          "speaker_id": "1",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043211.1.16",
          "form": "저 요즘에 믹순이라고 콩 에센스 샀어요",
          "original_form": "저 요즘에 믹순이라고 콩 에센스 샀어요",
          "speaker_id": "2",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043211.1.17",
          "form": "화장품이죵?",
          "original_form": "화장품이죵?",
          "speaker_id": "1",
          "time": "20210907 15:59"
        },
        {
          "id": "MDRW2100043211.1.18",
          "form": "네 화장품이요!",
          "original_form": "네 화장품이요!",
          "speaker_id": "2",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100043211.1.19",
          "form": "기초제품은 오히려 더 사게 돼요 마스크 써서",
          "original_form": "기초제품은 오히려 더 사게 돼요 마스크 써서",
          "speaker_id": "2",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100043211.1.20",
          "form": "그춍 저도팩샀어요",
          "original_form": "그춍 저도팩샀어요",
          "speaker_id": "1",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100043211.1.21",
          "form": "팩이 정말 필요해지는 날씨기도 하고요",
