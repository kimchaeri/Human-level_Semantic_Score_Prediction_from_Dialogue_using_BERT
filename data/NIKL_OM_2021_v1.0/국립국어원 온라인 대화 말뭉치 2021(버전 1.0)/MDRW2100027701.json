      "utterance": [
        {
          "id": "MDRW2100027701.1.1",
          "form": "미얀마는 어땠어",
          "original_form": "미얀마는 어땠어",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027701.1.2",
          "form": "인레호수 이쁘지않았니",
          "original_form": "인레호수 이쁘지않았니",
          "speaker_id": "2",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027701.1.3",
          "form": "교통편이 불편했어",
          "original_form": "교통편이 불편했어",
          "speaker_id": "1",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027701.1.4",
          "form": "버스가 고산지대로 빙빙돌다보니 멀미가 심했어",
          "original_form": "버스가 고산지대로 빙빙돌다보니 멀미가 심했어",
          "speaker_id": "2",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027701.1.5",
          "form": "양곤은 생각보다 특징이 없어",
          "original_form": "양곤은 생각보다 특징이 없어",
          "speaker_id": "1",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027701.1.6",
          "form": "사원이멋있었어",
          "original_form": "사원이멋있었어",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027701.1.7",
          "form": "황금사원이 곳곳에 있지",
          "original_form": "황금사원이 곳곳에 있지",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027701.1.8",
          "form": "웅장해",
          "original_form": "웅장해",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027701.1.9",
          "form": "그것만이 유일한 특징이야",
          "original_form": "그것만이 유일한 특징이야",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027701.1.10",
          "form": "그게 있어서 관광지다웠어",
          "original_form": "그게 있어서 관광지다웠어",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027701.1.11",
          "form": "사람들이 순박했어",
          "original_form": "사람들이 순박했어",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027701.1.12",
          "form": "까만데 이빨은 하얗고",
          "original_form": "까만데 이빨은 하얗고",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027701.1.13",
          "form": "밍글라바가 인상적이었다",
          "original_form": "밍글라바가 인상적이었다",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027701.1.14",
          "form": "언어가 부드러웠어",
          "original_form": "언어가 부드러웠어",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027701.1.15",
          "form": "다시가고싶다",
          "original_form": "다시가고싶다",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027701.1.16",
          "form": "카야토스트 기억나",
          "original_form": "카야토스트 기억나",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027701.1.17",
          "form": "음식은 무난햏지",
          "original_form": "음식은 무난햏지",
          "speaker_id": "2",
          "time": "20210902 14:27"
        }
