      "utterance": [
        {
          "id": "MDRW2100002135.1.1",
          "form": "건조기도 사야하는데",
          "original_form": "건조기도 사야하는데",
          "speaker_id": "2",
          "time": "20210819 23:52"
        },
        {
          "id": "MDRW2100002135.1.2",
          "form": "주변에 무료나눔해줄사람 없나?",
          "original_form": "주변에 무료나눔해줄사람 없나?",
          "speaker_id": "2",
          "time": "20210819 23:52"
        },
        {
          "id": "MDRW2100002135.1.3",
          "form": "건조대겠지.",
          "original_form": "건조대겠지.",
          "speaker_id": "1",
          "time": "20210819 23:52"
        },
        {
          "id": "MDRW2100002135.1.4",
          "form": "건조기일수도 있잖아.",
          "original_form": "건조기일수도 있잖아.",
          "speaker_id": "2",
          "time": "20210819 23:52"
        },
        {
          "id": "MDRW2100002135.1.5",
          "form": "사실 맞아 건조대야",
          "original_form": "사실 맞아 건조대야",
          "speaker_id": "2",
          "time": "20210819 23:52"
        },
        {
          "id": "MDRW2100002135.1.6",
          "form": "건조대.. 당근에 걸어놔봤나 키워드알림",
          "original_form": "건조대.. 당근에 걸어놔봤나 키워드알림",
          "speaker_id": "1",
          "time": "20210819 23:52"
        },
        {
          "id": "MDRW2100002135.1.7",
          "form": "알아. 니 방에 건조기는 놓을 데도 없던데",
          "original_form": "알아. 니 방에 건조기는 놓을 데도 없던데",
          "speaker_id": "1",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.8",
          "form": "아니 근데 왠지~ 모르겠는데",
          "original_form": "아니 근데 왠지~ 모르겠는데",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.9",
          "form": "중고는 싫음??",
          "original_form": "중고는 싫음??",
          "speaker_id": "1",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.10",
          "form": "건조대는 새거 사고싶기도하고",
          "original_form": "건조대는 새거 사고싶기도하고",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.11",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.12",
          "form": "아 그건 그엏다",
          "original_form": "아 그건 그엏다",
          "speaker_id": "1",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.13",
          "form": "일단 이마트를 한번 가보려고",
          "original_form": "일단 이마트를 한번 가보려고",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.14",
          "form": "남의 축축한 빨래 널어놨을거잖아",
          "original_form": "남의 축축한 빨래 널어놨을거잖아",
          "speaker_id": "1",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.15",
          "form": "이마트까지요?!",
          "original_form": "이마트까지요?!",
          "speaker_id": "1",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.16",
          "form": "ㅋㅋㅋㅋㅋ아 직접적으로 말하니까",
          "original_form": "ㅋㅋㅋㅋㅋ아 직접적으로 말하니까",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.17",
          "form": "더 새로 사고싶다",
          "original_form": "더 새로 사고싶다",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.18",
          "form": "어 근데 내가 걸어서말고",
          "original_form": "어 근데 내가 걸어서말고",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.19",
          "form": "근데 왜 쿠팡 이런데서 안사고",
          "original_form": "근데 왜 쿠팡 이런데서 안사고",
          "speaker_id": "1",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.20",
          "form": "이마트에서 사서 가려고?",
          "original_form": "이마트에서 사서 가려고?",
          "speaker_id": "1",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002135.1.21",
          "form": "이마트에 14000원짜리 있다는 소문을 들어서",
          "original_form": "이마트에 14000원짜리 있다는 소문을 들어서",
          "speaker_id": "2",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002135.1.22",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002135.1.23",
          "form": "아 쿠팡도 한번 찾아봐야겠네",
          "original_form": "아 쿠팡도 한번 찾아봐야겠네",
          "speaker_id": "2",
          "time": "20210819 23:54"
        }
      ]
