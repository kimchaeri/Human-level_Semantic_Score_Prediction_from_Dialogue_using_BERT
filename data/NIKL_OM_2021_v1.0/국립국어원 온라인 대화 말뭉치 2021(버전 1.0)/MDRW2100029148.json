      "utterance": [
        {
          "id": "MDRW2100029148.1.1",
          "form": "name2은 학생이신가요?",
          "original_form": "&name2&은 학생이신가요?",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029148.1.2",
          "form": "네 아직 학생이에요.",
          "original_form": "네 아직 학생이에요.",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029148.1.3",
          "form": "방학때 여행이 꿈이었는데",
          "original_form": "방학때 여행이 꿈이었는데",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029148.1.4",
          "form": "망했어요ㅠㅠ",
          "original_form": "망했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029148.1.5",
          "form": "아 코로나때문에 힘들겠어요",
          "original_form": "아 코로나때문에 힘들겠어요",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029148.1.6",
          "form": "내년에도 해외여행 어렵겠죠",
          "original_form": "내년에도 해외여행 어렵겠죠",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029148.1.7",
          "form": "아무래도 그렇겠죠",
          "original_form": "아무래도 그렇겠죠",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029148.1.8",
          "form": "우리나라보다 심각한 나라들도 있다고하고...",
          "original_form": "우리나라보다 심각한 나라들도 있다고하고...",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029148.1.9",
          "form": "어머니 지인분이 터키에 계시는데",
          "original_form": "어머니 지인분이 터키에 계시는데",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029148.1.10",
          "form": "장 보러도 못나가서",
          "original_form": "장 보러도 못나가서",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029148.1.11",
          "form": "거기는 더 심한가보네요",
          "original_form": "거기는 더 심한가보네요",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029148.1.12",
          "form": "여기서 식료품 챙겨서 보내드렸어요",
          "original_form": "여기서 식료품 챙겨서 보내드렸어요",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029148.1.13",
          "form": "네 한인마트를 못가신대요",
          "original_form": "네 한인마트를 못가신대요",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029148.1.14",
          "form": "아 대단하시다",
          "original_form": "아 대단하시다",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029148.1.15",
          "form": "다들 고생이네요",
          "original_form": "다들 고생이네요",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029148.1.16",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029148.1.17",
          "form": "이번여름에 휴가다녀오셨어요?",
          "original_form": "이번여름에 휴가다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029148.1.18",
          "form": "요양병원에서 일하시는 지인은",
          "original_form": "요양병원에서 일하시는 지인은",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029148.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029148.1.20",
          "form": "작년 봄이었나 한창 심해질때",
          "original_form": "작년 봄이었나 한창 심해질때",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029148.1.21",
          "form": "혹시 코호트격리 될까봐",
          "original_form": "혹시 코호트격리 될까봐",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029148.1.22",
          "form": "바로 들고갈 수 있게 캐리어에 짐 싸두셨대요",
          "original_form": "바로 들고갈 수 있게 캐리어에 짐 싸두셨대요",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029148.1.23",
          "form": "제 주변에도",
          "original_form": "제 주변에도",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029148.1.24",
          "form": "요양병원에서 일하던 지인있는데",
          "original_form": "요양병원에서 일하던 지인있는데",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029148.1.25",
          "form": "매주 코로나검사받았었다고 하더라고요",
          "original_form": "매주 코로나검사받았었다고 하더라고요",
          "speaker_id": "1",
          "time": "20210903 00:01"
        }
      ]
