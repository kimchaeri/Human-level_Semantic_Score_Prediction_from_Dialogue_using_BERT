      "utterance": [
        {
          "id": "MDRW2100029290.1.1",
          "form": "저 시즌음료에",
          "original_form": "저 시즌음료에",
          "speaker_id": "1",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029290.1.2",
          "form": "도전하지않아요",
          "original_form": "도전하지않아요",
          "speaker_id": "1",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029290.1.3",
          "form": "ㅋㅋㅋ 역시 기본파시구나",
          "original_form": "ㅋㅋㅋ 역시 기본파시구나",
          "speaker_id": "2",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029290.1.4",
          "form": "변화를 싫어하는,,,",
          "original_form": "변화를 싫어하는,,,",
          "speaker_id": "1",
          "time": "20210903 01:07"
        },
        {
          "id": "MDRW2100029290.1.5",
          "form": "치킨은 좋아하시나요?",
          "original_form": "치킨은 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 01:07"
        },
        {
          "id": "MDRW2100029290.1.6",
          "form": "너무좋아합니다",
          "original_form": "너무좋아합니다",
          "speaker_id": "1",
          "time": "20210903 01:07"
        },
        {
          "id": "MDRW2100029290.1.7",
          "form": "어느 브랜드 치킨 좋아하세요??",
          "original_form": "어느 브랜드 치킨 좋아하세요??",
          "speaker_id": "2",
          "time": "20210903 01:07"
        },
        {
          "id": "MDRW2100029290.1.8",
          "form": "저요즘엔 교촌허니콤보ㅠ",
          "original_form": "저요즘엔 교촌허니콤보ㅠ",
          "speaker_id": "1",
          "time": "20210903 01:07"
        },
        {
          "id": "MDRW2100029290.1.9",
          "form": "원래는 비비큐 황금올리브 좋아했어요",
          "original_form": "원래는 비비큐 황금올리브 좋아했어요",
          "speaker_id": "1",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029290.1.10",
          "form": "저는 또래오래 갈릭반 핫양념반 좋아해요",
          "original_form": "저는 또래오래 갈릭반 핫양념반 좋아해요",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029290.1.11",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029290.1.12",
          "form": "요즘엔 고추바사삭도 맛있더라고요",
          "original_form": "요즘엔 고추바사삭도 맛있더라고요",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029290.1.13",
          "form": "그것도한동안엄청먹었어요",
          "original_form": "그것도한동안엄청먹었어요",
          "speaker_id": "1",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029290.1.14",
          "form": "고바삭은 안먹어봤어요",
          "original_form": "고바삭은 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029290.1.15",
          "form": "은근히 매운데 맛있어요 중독성있는 맛",
          "original_form": "은근히 매운데 맛있어요 중독성있는 맛",
          "speaker_id": "2",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029290.1.16",
          "form": "순살드셨어요?",
          "original_form": "순살드셨어요?",
          "speaker_id": "1",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029290.1.17",
          "form": "네 순살먹었어요",
          "original_form": "네 순살먹었어요",
          "speaker_id": "2",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029290.1.18",
          "form": "역시맛잘알",
          "original_form": "역시맛잘알",
          "speaker_id": "1",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029290.1.19",
          "form": "순살고바삭이 진리라더라고요",
          "original_form": "순살고바삭이 진리라더라고요",
          "speaker_id": "1",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029290.1.20",
          "form": "뼈를 좋아하는데 고추바사삭은 순살로 먹어야돼요 ㅋㅋ",
          "original_form": "뼈를 좋아하는데 고추바사삭은 순살로 먹어야돼요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029290.1.21",
          "form": "네넹ㅋㅋㅋ",
          "original_form": "네넹ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:10"
