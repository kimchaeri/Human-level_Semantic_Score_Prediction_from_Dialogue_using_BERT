      "utterance": [
        {
          "id": "MDRW2100032368.1.1",
          "form": "오, 샤브샤브도 좋죠",
          "original_form": "오, 샤브샤브도 좋죠",
          "speaker_id": "2",
          "time": "20210905 16:27"
        },
        {
          "id": "MDRW2100032368.1.2",
          "form": "그쵸 소고기 넣어먹음 맛있어요",
          "original_form": "그쵸 소고기 넣어먹음 맛있어요",
          "speaker_id": "1",
          "time": "20210905 16:27"
        },
        {
          "id": "MDRW2100032368.1.3",
          "form": "가끔 밀키트사서 먹어요",
          "original_form": "가끔 밀키트사서 먹어요",
          "speaker_id": "1",
          "time": "20210905 16:27"
        },
        {
          "id": "MDRW2100032368.1.4",
          "form": "오, 밀키트! 자주 시켜먹는 집 있으세요?",
          "original_form": "오, 밀키트! 자주 시켜먹는 집 있으세요?",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032368.1.5",
          "form": "아뇨 주로 마트에서 사다먹어요",
          "original_form": "아뇨 주로 마트에서 사다먹어요",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032368.1.6",
          "form": "애슐리 밀키트 괜찮더라구요",
          "original_form": "애슐리 밀키트 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032368.1.7",
          "form": "오, 그 뷔페 애슐리요??",
          "original_form": "오, 그 뷔페 애슐리요??",
          "speaker_id": "2",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100032368.1.8",
          "form": "네네 거기서 밀키트 나오더라구요",
          "original_form": "네네 거기서 밀키트 나오더라구요",
          "speaker_id": "1",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100032368.1.9",
          "form": "재료 조금 더 사서 해먹으면 은근 푸짐해요",
          "original_form": "재료 조금 더 사서 해먹으면 은근 푸짐해요",
          "speaker_id": "1",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100032368.1.10",
          "form": "오, 좋네요",
          "original_form": "오, 좋네요",
          "speaker_id": "2",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100032368.1.11",
          "form": "밀키트 드셔보신거 있으세요?",
          "original_form": "밀키트 드셔보신거 있으세요?",
          "speaker_id": "1",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100032368.1.12",
          "form": "고추잡채 먹어봤는데 양이 적었어요 ㅜ.ㅜ",
          "original_form": "고추잡채 먹어봤는데 양이 적었어요 ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100032368.1.13",
          "form": "아쉽네요 ᆢ 그런건 그냥 중국집이 나을거 같기도 하네요",
          "original_form": "아쉽네요 ᆢ 그런건 그냥 중국집이 나을거 같기도 하네요",
          "speaker_id": "1",
          "time": "20210905 16:33"
        },
        {
          "id": "MDRW2100032368.1.14",
          "form": "그러게 말이에요",
          "original_form": "그러게 말이에요",
          "speaker_id": "2",
          "time": "20210905 16:33"
        },
        {
          "id": "MDRW2100032368.1.15",
          "form": "집 근처에 중식 맛집 있으세요?",
          "original_form": "집 근처에 중식 맛집 있으세요?",
          "speaker_id": "1",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100032368.1.16",
          "form": "집 근처엔 없어요, 양식 집은 몇개 있는데",
          "original_form": "집 근처엔 없어요, 양식 집은 몇개 있는데",
          "speaker_id": "2",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100032368.1.17",
          "form": "저희집 주변도 없어서 좀 찾고싶네요ㅜㅜ",
          "original_form": "저희집 주변도 없어서 좀 찾고싶네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 16:35"
        }
