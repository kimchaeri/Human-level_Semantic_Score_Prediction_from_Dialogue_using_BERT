      "utterance": [
        {
          "id": "MDRW2100015471.1.1",
          "form": "재미님은",
          "original_form": "재미님은",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015471.1.2",
          "form": "어..지금 없다하셬ㅅ고",
          "original_form": "어..지금 없다하셬ㅅ고",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015471.1.3",
          "form": "다가가서 뭐라고 했나요",
          "original_form": "다가가서 뭐라고 했나요",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015471.1.4",
          "form": "있으셨나요",
          "original_form": "있으셨나요",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015471.1.5",
          "form": "아니요 없어요 하지만 개의치않아요",
          "original_form": "아니요 없어요 하지만 개의치않아요",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015471.1.6",
          "form": "오케이 앞으로 기회는 엄청나게 많으니꺼요",
          "original_form": "오케이 앞으로 기회는 엄청나게 많으니꺼요",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015471.1.7",
          "form": "곧 군대갈건데 무슨의미가있겠나요",
          "original_form": "곧 군대갈건데 무슨의미가있겠나요",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015471.1.8",
          "form": "군대도 안갔담서요",
          "original_form": "군대도 안갔담서요",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015471.1.9",
          "form": "곧..이요?",
          "original_form": "곧..이요?",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015471.1.10",
          "form": "쓰읍...눈물..한방울...",
          "original_form": "쓰읍...눈물..한방울...",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015471.1.11",
          "form": "ㅋㅋㅋ 그래서 보스베이비보다 다가가서 말걸었나요",
          "original_form": "ㅋㅋㅋ 그래서 보스베이비보다 다가가서 말걸었나요",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015471.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015471.1.13",
          "form": "남사친이었는뎈ㅋㅋㅋㅋㅋ",
          "original_form": "남사친이었는뎈ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015471.1.14",
          "form": "뭐라고요",
          "original_form": "뭐라고요",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015471.1.15",
          "form": "아 남사친이랑 보러갔구나",
          "original_form": "아 남사친이랑 보러갔구나",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015471.1.16",
          "form": "남인줄ㅋㅋㅋ",
          "original_form": "남인줄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015471.1.17",
          "form": "보고나서 급 떨리기 시작하길래",
          "original_form": "보고나서 급 떨리기 시작하길래",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015471.1.18",
          "form": "좀 좀..어",
          "original_form": "좀 좀..어",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015471.1.19",
          "form": "어 따로 만나는 횟수를 늘렸어요",
          "original_form": "어 따로 만나는 횟수를 늘렸어요",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015471.1.20",
          "form": "만약에 여자애가 따로 만나자 한다?",
          "original_form": "만약에 여자애가 따로 만나자 한다?",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015471.1.21",
          "form": "좋아하는거니까 알아두십쇼",
          "original_form": "좋아하는거니까 알아두십쇼",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015471.1.22",
          "form": "알겠습니다",
          "original_form": "알겠습니다",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015471.1.23",
          "form": "연락을 먼저 자주한다? 엠비티아이 먼저 물어보시고",
          "original_form": "연락을 먼저 자주한다? 엠비티아이 먼저 물어보시고",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015471.1.24",
          "form": "E형이면 그런갑다 하시고요",
          "original_form": "E형이면 그런갑다 하시고요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        }
      ]
