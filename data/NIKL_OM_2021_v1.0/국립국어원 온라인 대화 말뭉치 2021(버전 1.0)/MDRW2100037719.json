          "form": "워~~",
          "original_form": "워~~",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100037719.1.9",
          "form": "ㅋㅋㅋㅋ아 근데 가사만 봐도 잘 모르겠당",
          "original_form": "ㅋㅋㅋㅋ아 근데 가사만 봐도 잘 모르겠당",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100037719.1.10",
          "form": "그림대박 ㅋㅋㅋ",
          "original_form": "그림대박 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037719.1.11",
          "form": "ㅋㅋㅋ여기 내가 다 그려줬다",
          "original_form": "ㅋㅋㅋ여기 내가 다 그려줬다",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037719.1.12",
          "form": "대박 ㅋㅋㅋㅋ그림책내보면어때?",
          "original_form": "대박 ㅋㅋㅋㅋ그림책내보면어때?",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037719.1.13",
          "form": "ㅋㅋㅋㅋㅋ앜ㅋㅋㅋ내용이 음슴..",
          "original_form": "ㅋㅋㅋㅋㅋ앜ㅋㅋㅋ내용이 음슴..",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037719.1.14",
          "form": "혹시 김건모 모음인가 연속 3건모",
          "original_form": "혹시 김건모 모음인가 연속 3건모",
          "speaker_id": "2",
          "time": "20210901 17:13"
