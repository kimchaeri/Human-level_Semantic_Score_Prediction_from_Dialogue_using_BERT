      "utterance": [
        {
          "id": "MDRW2100022750.1.1",
          "form": "혹시 대학생이신가요",
          "original_form": "혹시 대학생이신가요",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022750.1.2",
          "form": "저는 고등학생입니다",
          "original_form": "저는 고등학생입니다",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022750.1.3",
          "form": "대학생이세요...?",
          "original_form": "대학생이세요...?",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022750.1.4",
          "form": "아 고등학생이셨구나",
          "original_form": "아 고등학생이셨구나",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022750.1.5",
          "form": "네 대학생이에요",
          "original_form": "네 대학생이에요",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022750.1.6",
          "form": "고등학교는 개학했나요?",
          "original_form": "고등학교는 개학했나요?",
          "speaker_id": "1",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022750.1.7",
          "form": "개학하고 일주일 넘었네요",
          "original_form": "개학하고 일주일 넘었네요",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022750.1.8",
          "form": "대학교는 비대면으로 수업하나요?",
          "original_form": "대학교는 비대면으로 수업하나요?",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022750.1.9",
          "form": "요즘은 대면수업이나 비대면 혼합으로 하고있는데",
          "original_form": "요즘은 대면수업이나 비대면 혼합으로 하고있는데",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022750.1.10",
          "form": "고등학교는 어떤식인가요?",
          "original_form": "고등학교는 어떤식인가요?",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022750.1.11",
          "form": "저희 학교는 1 3 , 2 3 학년 번갈아 등교하고 있어요",
          "original_form": "저희 학교는 1 3 , 2 3 학년 번갈아 등교하고 있어요",
          "speaker_id": "2",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022750.1.12",
          "form": "일주일 간격인가요?",
          "original_form": "일주일 간격인가요?",
          "speaker_id": "1",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022750.1.13",
          "form": "아뇨 ! 2주 정도요",
          "original_form": "아뇨 ! 2주 정도요",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022750.1.14",
          "form": "헉.. 2주에 한번씩 등교하는군요",
          "original_form": "헉.. 2주에 한번씩 등교하는군요",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022750.1.15",
          "form": "그럼 집에서 라이브로 수업듣는거에요?",
          "original_form": "그럼 집에서 라이브로 수업듣는거에요?",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022750.1.16",
          "form": "집에서는 줌 으로 수업하는데",
          "original_form": "집에서는 줌 으로 수업하는데",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022750.1.17",
          "form": "거의 자습 주시는 거 같아요",
          "original_form": "거의 자습 주시는 거 같아요",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022750.1.18",
          "form": "학교가서는",
          "original_form": "학교가서는",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022750.1.19",
          "form": "한칸띄워서 수업하고",
          "original_form": "한칸띄워서 수업하고",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022750.1.20",
          "form": "3학년들은 매일가는군요",
          "original_form": "3학년들은 매일가는군요",
          "speaker_id": "1",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022750.1.21",
          "form": "대학교에서는 친구 어떻게 사귀나요...?",
          "original_form": "대학교에서는 친구 어떻게 사귀나요...?",
          "speaker_id": "2",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022750.1.22",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022750.1.23",
          "form": "코로나 이전에 입학했었어서",
          "original_form": "코로나 이전에 입학했었어서",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022750.1.24",
          "form": "대학교 처음 입학하면",
          "original_form": "대학교 처음 입학하면",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022750.1.25",
          "form": "ot라고 1박2일로",
          "original_form": "ot라고 1박2일로",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022750.1.26",
          "form": "여행처럼 떠나거든요",
          "original_form": "여행처럼 떠나거든요",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022750.1.27",
          "form": "그때 선배들이랑 동기들 묶어서",
          "original_form": "그때 선배들이랑 동기들 묶어서",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022750.1.28",
          "form": "조를 만들고 얘기하다 친해지거나",
          "original_form": "조를 만들고 얘기하다 친해지거나",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022750.1.29",
          "form": "밤에 술마시는 시간이있어서",
          "original_form": "밤에 술마시는 시간이있어서",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022750.1.30",
          "form": "술퍼마시면서 두루두루 친해졌어요 ㅋㅋ",
          "original_form": "술퍼마시면서 두루두루 친해졌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022750.1.31",
          "form": "저는 살면서 수련회 한 번 가본 적이 없는지라",
          "original_form": "저는 살면서 수련회 한 번 가본 적이 없는지라",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022750.1.32",
          "form": "엄청 기대하고 있어요",
          "original_form": "엄청 기대하고 있어요",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022750.1.33",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022750.1.34",
          "form": "지금 혹시 고3이신가요",
          "original_form": "지금 혹시 고3이신가요",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022750.1.35",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022750.1.36",
          "form": "고3인 걸 들키고 싶지 않았는데",
          "original_form": "고3인 걸 들키고 싶지 않았는데",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022750.1.37",
          "form": "ㅋㅋㅋㅋ 너무 추리가 가능해서",
          "original_form": "ㅋㅋㅋㅋ 너무 추리가 가능해서",
          "speaker_id": "1",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022750.1.38",
          "form": "그런 얘기 들으면 지금 당장 공부 시작해야될 거 같아요",
          "original_form": "그런 얘기 들으면 지금 당장 공부 시작해야될 거 같아요",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022750.1.39",
          "form": "그래도 중학교 수학여행정도는",
          "original_form": "그래도 중학교 수학여행정도는",
          "speaker_id": "1",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022750.1.40",
          "form": "가보셨을거같은데",
          "original_form": "가보셨을거같은데",
          "speaker_id": "1",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022750.1.41",
          "form": "죄책감 들어요...ㅠㅠㅋㅋ",
          "original_form": "죄책감 들어요...ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022750.1.42",
          "form": "지금은 2학기라서",
          "original_form": "지금은 2학기라서",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022750.1.43",
          "form": "세월호 터지고 흐지부지 되서 ...",
          "original_form": "세월호 터지고 흐지부지 되서 ...",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022750.1.44",
          "form": "정시준비하는 사람들 말고는",
          "original_form": "정시준비하는 사람들 말고는",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022750.1.45",
          "form": "코로나까지 ...",
          "original_form": "코로나까지 ...",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022750.1.46",
          "form": "여유롭지않나요",
          "original_form": "여유롭지않나요",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022750.1.47",
          "form": "아 세월호도 겹쳤군여 ㄷㄷ",
          "original_form": "아 세월호도 겹쳤군여 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022750.1.48",
          "form": "고3 이시면 그럼",
          "original_form": "고3 이시면 그럼",
          "speaker_id": "1",
          "time": "20210829 17:49"
        }
      ]
    }
