      "utterance": [
        {
          "id": "MDRW2100025501.1.1",
          "form": "헛 마라탕 그래서 드셨나요?!",
          "original_form": "헛 마라탕 그래서 드셨나요?!",
          "speaker_id": "1",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100025501.1.2",
          "form": "갑자기 초밥 먹고싶어서 초밥으로 바꿨어요 ㅋㅋㅋㅋ",
          "original_form": "갑자기 초밥 먹고싶어서 초밥으로 바꿨어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100025501.1.3",
          "form": "이제 곧 먹으려구요",
          "original_form": "이제 곧 먹으려구요",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100025501.1.4",
          "form": "헐.. 저 초밥도 진짜 좋아하는데",
          "original_form": "헐.. 저 초밥도 진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100025501.1.5",
          "form": "name2님 최애 초밥 뭔가요",
          "original_form": "&name2&님 최애 초밥 뭔가요",
          "speaker_id": "1",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100025501.1.6",
          "form": "음 간장새우도 좋고 묵은지광어초밥도 좋아해요 ㅎㅎ",
          "original_form": "음 간장새우도 좋고 묵은지광어초밥도 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:09"
        },
        {
          "id": "MDRW2100025501.1.7",
          "form": "헐헐 저도 새우초밥 좋아하는데",
          "original_form": "헐헐 저도 새우초밥 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 16:30"
        },
        {
          "id": "MDRW2100025501.1.8",
          "form": "그 붉은 새우?",
          "original_form": "그 붉은 새우?",
          "speaker_id": "1",
          "time": "20210901 16:30"
        },
        {
          "id": "MDRW2100025501.1.9",
          "form": "륵 좋아해요!!",
          "original_form": "륵 좋아해요!!",
          "speaker_id": "1",
          "time": "20210901 16:30"
        },
        {
          "id": "MDRW2100025501.1.10",
          "form": "아하 ㅋㅋㅋㅋ 새우 맛있죠..",
          "original_form": "아하 ㅋㅋㅋㅋ 새우 맛있죠..",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025501.1.11",
          "form": "name1님 혹시 새우 꼬리 드시나요",
          "original_form": "&name1&님 혹시 새우 꼬리 드시나요",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025501.1.12",
          "form": "헉.. 아니요??!",
          "original_form": "헉.. 아니요??!",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025501.1.13",
          "form": "name2님은 드시나요?!?!",
          "original_form": "&name2&님은 드시나요?!?!",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025501.1.14",
          "form": "아하 저는 좋아해요 ㅋㅋㅌㅋㅋㅋ 제가 바삭한걸 좋아해서",
          "original_form": "아하 저는 좋아해요 ㅋㅋㅌㅋㅋㅋ 제가 바삭한걸 좋아해서",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100025501.1.15",
          "form": "생새우꼬리는 안먹을때도 있는데 새우튀김은 무조건 먹습니다 ㅎㅎ",
          "original_form": "생새우꼬리는 안먹을때도 있는데 새우튀김은 무조건 먹습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025501.1.16",
          "form": "헉!!!!!! 혹시 머리도 드시나요??? 종종 튀김으로 해서",
          "original_form": "헉!!!!!! 혹시 머리도 드시나요??? 종종 튀김으로 해서",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025501.1.17",
          "form": "드시는 분들 계시길래!",
          "original_form": "드시는 분들 계시길래!",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025501.1.18",
          "form": "아니요 머리는 안먹어요 ㅋㅋㅋㅋㅋ 뭔가 머리는 당연히 안먹는거라고 생각해서 먹어본적 없는데 새우 머리 드시는 분들도 있는 거 알고 놀랐어요 ㅋㅋㅋㅋ",
          "original_form": "아니요 머리는 안먹어요 ㅋㅋㅋㅋㅋ 뭔가 머리는 당연히 안먹는거라고 생각해서 먹어본적 없는데 새우 머리 드시는 분들도 있는 거 알고 놀랐어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100025501.1.19",
          "form": "그러니까요! 저도 친구가 먹는 거 보고",
          "original_form": "그러니까요! 저도 친구가 먹는 거 보고",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100025501.1.20",
          "form": "완전 깜짝 놀랐어요!!!!!!!",
          "original_form": "완전 깜짝 놀랐어요!!!!!!!",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100025501.1.21",
          "form": "그 혹시 번데기도 드세요?",
          "original_form": "그 혹시 번데기도 드세요?",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100025501.1.22",
          "form": "네네 먹어요 ㅎㅎ",
          "original_form": "네네 먹어요 ㅎㅎ",
