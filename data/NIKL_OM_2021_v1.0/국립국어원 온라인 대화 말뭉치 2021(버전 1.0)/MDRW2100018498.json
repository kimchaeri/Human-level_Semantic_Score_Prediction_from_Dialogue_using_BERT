      "utterance": [
        {
          "id": "MDRW2100018498.1.1",
          "form": "저는 킬러의 보디가드2 봤어요!",
          "original_form": "저는 킬러의 보디가드2 봤어요!",
          "speaker_id": "1",
          "time": "20210827 06:37"
        },
        {
          "id": "MDRW2100018498.1.2",
          "form": "아 그거 어때요? 저 안봤는데!",
          "original_form": "아 그거 어때요? 저 안봤는데!",
          "speaker_id": "2",
          "time": "20210827 06:37"
        },
        {
          "id": "MDRW2100018498.1.3",
          "form": "뭐가 젤 킬링포인트일까요!",
          "original_form": "뭐가 젤 킬링포인트일까요!",
          "speaker_id": "2",
          "time": "20210827 06:37"
        },
        {
          "id": "MDRW2100018498.1.4",
          "form": "1이 더 재밌긴한데 2도 나쁘지않아용",
          "original_form": "1이 더 재밌긴한데 2도 나쁘지않아용",
          "speaker_id": "1",
          "time": "20210827 06:38"
        },
        {
          "id": "MDRW2100018498.1.5",
          "form": "계속 라이언 레이놀즈가 입터는게 재밌어요ㅋㅋㅋㅋ",
          "original_form": "계속 라이언 레이놀즈가 입터는게 재밌어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:38"
        },
        {
          "id": "MDRW2100018498.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ앜ㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ앜ㄱㅋ",
          "speaker_id": "2",
          "time": "20210827 06:38"
        },
        {
          "id": "MDRW2100018498.1.7",
          "form": "뭔가 상상이 가요 그 느낌이",
          "original_form": "뭔가 상상이 가요 그 느낌이",
          "speaker_id": "2",
          "time": "20210827 06:38"
        },
        {
          "id": "MDRW2100018498.1.8",
          "form": "비슷한 계열로 스파이랑 나를 차버린 스파이도 꿀잼이에용",
          "original_form": "비슷한 계열로 스파이랑 나를 차버린 스파이도 꿀잼이에용",
          "speaker_id": "1",
          "time": "20210827 06:38"
        },
        {
          "id": "MDRW2100018498.1.9",
          "form": "아!그쪽 계열로 입터는거군요",
          "original_form": "아!그쪽 계열로 입터는거군요",
          "speaker_id": "2",
          "time": "20210827 06:38"
        },
        {
          "id": "MDRW2100018498.1.10",
          "form": "티키타가 잘되더라구요ㅋㅋ",
          "original_form": "티키타가 잘되더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:38"
        },
        {
          "id": "MDRW2100018498.1.11",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210827 06:39"
        },
        {
          "id": "MDRW2100018498.1.12",
          "form": "저 나를 차버린 스파이 보고",
          "original_form": "저 나를 차버린 스파이 보고",
          "speaker_id": "2",
          "time": "20210827 06:39"
        },
        {
          "id": "MDRW2100018498.1.13",
          "form": "근데 살짝 19금스러워서ㅠㅠㅋㅋ",
          "original_form": "근데 살짝 19금스러워서ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:39"
        },
        {
          "id": "MDRW2100018498.1.14",
          "form": "넘 웃겻어옄ㅋㅋㅋ아니 저런말은 어떻게 생각할 수 있지?",
          "original_form": "넘 웃겻어옄ㅋㅋㅋ아니 저런말은 어떻게 생각할 수 있지?",
          "speaker_id": "2",
          "time": "20210827 06:39"
        },
        {
          "id": "MDRW2100018498.1.15",
          "form": "싶어서요",
          "original_form": "싶어서요",
          "speaker_id": "2",
          "time": "20210827 06:39"
        },
        {
          "id": "MDRW2100018498.1.16",
          "form": "모건역할하신 케이트맥키넌",
          "original_form": "모건역할하신 케이트맥키넌",
          "speaker_id": "1",
          "time": "20210827 06:39"
        },
        {
          "id": "MDRW2100018498.1.17",
          "form": "진짜 웃겨요ㅋㅋㅋㅋ",
          "original_form": "진짜 웃겨요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:39"
        },
        {
          "id": "MDRW2100018498.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ진짜 너무 찰떡이예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ진짜 너무 찰떡이예요",
          "speaker_id": "2",
          "time": "20210827 06:39"
        },
        {
          "id": "MDRW2100018498.1.19",
          "form": "스파이나 나를 차버린 스파이 저도 그런류 좋아해요",
          "original_form": "스파이나 나를 차버린 스파이 저도 그런류 좋아해요",
          "speaker_id": "2",
          "time": "20210827 06:39"
        },
        {
          "id": "MDRW2100018498.1.20",
          "form": "저도 취향이 겹치시네용!ㅋㅋㅋ",
          "original_form": "저도 취향이 겹치시네용!ㅋㅋㅋ",
