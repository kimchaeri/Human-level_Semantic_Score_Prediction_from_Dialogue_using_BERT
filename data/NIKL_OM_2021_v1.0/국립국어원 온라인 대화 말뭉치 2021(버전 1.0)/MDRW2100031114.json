      "utterance": [
        {
          "id": "MDRW2100031114.1.1",
          "form": "싸이월드 하셨나요",
          "original_form": "싸이월드 하셨나요",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031114.1.2",
          "form": "네 예전에 헸어요",
          "original_form": "네 예전에 헸어요",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031114.1.3",
          "form": "부활하면 들어가보실건가요?",
          "original_form": "부활하면 들어가보실건가요?",
          "speaker_id": "1",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100031114.1.4",
          "form": "들어는 가볼거같은데 잘 안할것같아요",
          "original_form": "들어는 가볼거같은데 잘 안할것같아요",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031114.1.5",
          "form": "남친 염탐용",
          "original_form": "남친 염탐용",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031114.1.6",
          "form": "이었는데",
          "original_form": "이었는데",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031114.1.7",
          "form": "이제 남친이라서 ㅋㅋㅋㅋ",
          "original_form": "이제 남친이라서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031114.1.8",
          "form": "염탐 안해도 되어요 ㅋㅋㅋ",
          "original_form": "염탐 안해도 되어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031114.1.9",
          "form": "재밌네요 싸이월드 시절에 염탐이요?",
          "original_form": "재밌네요 싸이월드 시절에 염탐이요?",
          "speaker_id": "1",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100031114.1.10",
          "form": "네네 맞아요 게시판 가보고",
          "original_form": "네네 맞아요 게시판 가보고",
          "speaker_id": "2",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100031114.1.11",
          "form": "비지엠 들어보고 ㅋㅋㅋ",
          "original_form": "비지엠 들어보고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100031114.1.12",
          "form": "꽤 오래전일텐데 그 당시 염탐한 분과 계속 인연이 되셨나봅니다!",
          "original_form": "꽤 오래전일텐데 그 당시 염탐한 분과 계속 인연이 되셨나봅니다!",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031114.1.13",
          "form": "네네 맞아요 지금 남친입니다",
          "original_form": "네네 맞아요 지금 남친입니다",
          "speaker_id": "2",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031114.1.14",
          "form": "도토리 많으셨나요",
          "original_form": "도토리 많으셨나요",
          "speaker_id": "2",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031114.1.15",
          "form": "음 도토리는 돈주고 사거나 선물로 주고받았죠..덕분에 기억나네요",
          "original_form": "음 도토리는 돈주고 사거나 선물로 주고받았죠..덕분에 기억나네요",
          "speaker_id": "1",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031114.1.16",
          "form": "오 그랬군요 이제 기억도 잘 안나요",
          "original_form": "오 그랬군요 이제 기억도 잘 안나요",
          "speaker_id": "2",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100031114.1.17",
          "form": "일촌평도 기억나세요?",
          "original_form": "일촌평도 기억나세요?",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031114.1.18",
          "form": "일촌평은 기억안나요",
          "original_form": "일촌평은 기억안나요",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031114.1.19",
          "form": "그렇군요 일촌 맺을때 명칭 정하는것도 있었는데..",
          "original_form": "그렇군요 일촌 맺을때 명칭 정하는것도 있었는데..",
          "speaker_id": "1",
          "time": "20210904 18:43"
        }
