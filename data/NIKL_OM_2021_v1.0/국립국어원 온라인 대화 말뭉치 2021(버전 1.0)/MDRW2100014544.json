      "utterance": [
        {
          "id": "MDRW2100014544.1.1",
          "form": "동물 키우세요??",
          "original_form": "동물 키우세요??",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014544.1.2",
          "form": "네 강아지여",
          "original_form": "네 강아지여",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014544.1.3",
          "form": "오 강아지 키우시는군요!!",
          "original_form": "오 강아지 키우시는군요!!",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014544.1.4",
          "form": "어떤 종이에요?",
          "original_form": "어떤 종이에요?",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014544.1.5",
          "form": "비숑이여",
          "original_form": "비숑이여",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014544.1.6",
          "form": "엇? 저희가 이 대화를 했던가요ㅋㅋㅋㅋ",
          "original_form": "엇? 저희가 이 대화를 했던가요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014544.1.7",
          "form": "그런가요? 1살이에용ㅎ",
          "original_form": "그런가요? 1살이에용ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014544.1.8",
          "form": "ㅋㅋㅋ네네 했던 것 같아요",
          "original_form": "ㅋㅋㅋ네네 했던 것 같아요",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014544.1.9",
          "form": "키워보고 싶은 다른 동물 있으신가요?",
          "original_form": "키워보고 싶은 다른 동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014544.1.10",
          "form": "강아지말곤 없네요ㅎ",
          "original_form": "강아지말곤 없네요ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014544.1.11",
          "form": "전 애완동물은 키울 자신이 없어요.\r\n대단하셔요~",
          "original_form": "전 애완동물은 키울 자신이 없어요.\r\n대단하셔요~",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014544.1.12",
          "form": "아무리 키워도 2-3살짜리 애기 키우는 거라하더라고요,,",
          "original_form": "아무리 키워도 2-3살짜리 애기 키우는 거라하더라고요,,",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014544.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014544.1.14",
          "form": "귀여워서 참아요",
          "original_form": "귀여워서 참아요",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014544.1.15",
          "form": "ㅋㅋㅋ귀엽긴 진짜 귀여울 것 같아요.",
          "original_form": "ㅋㅋㅋ귀엽긴 진짜 귀여울 것 같아요.",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100014544.1.16",
          "form": "예방접종 같은 것도 다 했나요?",
          "original_form": "예방접종 같은 것도 다 했나요?",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100014544.1.17",
          "form": "네 코로나도 맞았어요",
          "original_form": "네 코로나도 맞았어요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        }
      ]
