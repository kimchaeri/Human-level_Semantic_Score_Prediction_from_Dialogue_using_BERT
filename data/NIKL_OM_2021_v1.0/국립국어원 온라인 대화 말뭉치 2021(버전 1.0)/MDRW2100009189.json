      "utterance": [
        {
          "id": "MDRW2100009189.1.1",
          "form": "오 만들어봤어? 오븐 필요할거 같은디?",
          "original_form": "오 만들어봤어? 오븐 필요할거 같은디?",
          "speaker_id": "2",
          "time": "20210823 09:23"
        },
        {
          "id": "MDRW2100009189.1.2",
          "form": "오븐없이 만드는 레시피 있더라구",
          "original_form": "오븐없이 만드는 레시피 있더라구",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100009189.1.3",
          "form": "전자레인지로!!!",
          "original_form": "전자레인지로!!!",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100009189.1.4",
          "form": "오 맛은 충분해? ㅋㅋㅋ왠지 맛이 떨어지지 않을까 걱정되는딩",
          "original_form": "오 맛은 충분해? ㅋㅋㅋ왠지 맛이 떨어지지 않을까 걱정되는딩",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100009189.1.5",
          "form": "웅 생각보다 괜찮더라구?",
          "original_form": "웅 생각보다 괜찮더라구?",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100009189.1.6",
          "form": "레시피도 간단한 편이고",
          "original_form": "레시피도 간단한 편이고",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100009189.1.7",
          "form": "ㅋㅋ나도 유툽 찾아봐야겠다ㅋㅋㅋ어우 난 가자탕 시켜먹었던게 아직도 배가 한아름이야",
          "original_form": "ㅋㅋ나도 유툽 찾아봐야겠다ㅋㅋㅋ어우 난 가자탕 시켜먹었던게 아직도 배가 한아름이야",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100009189.1.8",
          "form": "소자로 셋이 먹구 남기기까지 했는뎈ㅋㅋㅋ",
          "original_form": "소자로 셋이 먹구 남기기까지 했는뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100009189.1.9",
          "form": "가자탕은 뭐야???나 첨들어보ㅓ",
          "original_form": "가자탕은 뭐야???나 첨들어보ㅓ",
          "speaker_id": "1",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100009189.1.10",
          "form": "아ㅋㅋㅋ미안ㅋㅋㅋ오타야",
          "original_form": "아ㅋㅋㅋ미안ㅋㅋㅋ오타야",
          "speaker_id": "2",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100009189.1.11",
          "form": "감자탕ㅋㅋㅋ어제 감자탕 배민으로 시켜먹었거든",
          "original_form": "감자탕ㅋㅋㅋ어제 감자탕 배민으로 시켜먹었거든",
          "speaker_id": "2",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100009189.1.12",
          "form": "아ㅋㅋㅋㅋ뭐 특이한",
          "original_form": "아ㅋㅋㅋㅋ뭐 특이한",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100009189.1.13",
          "form": "메뉴인줄알았음",
          "original_form": "메뉴인줄알았음",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100009189.1.14",
          "form": "고기 살 다 발라서 한번에 먹는편이야?",
          "original_form": "고기 살 다 발라서 한번에 먹는편이야?",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100009189.1.15",
          "form": "아니ㅋㅋ나는 뜨거울 때먹어야해서 바르느라 시간을 쓸수 없어",
          "original_form": "아니ㅋㅋ나는 뜨거울 때먹어야해서 바르느라 시간을 쓸수 없어",
          "speaker_id": "2",
          "time": "20210827 01:02"
        },
        {
          "id": "MDRW2100009189.1.16",
          "form": "고기는 뼈 붙은채 뜯어먹어야지!!",
          "original_form": "고기는 뼈 붙은채 뜯어먹어야지!!",
          "speaker_id": "2",
          "time": "20210827 01:02"
        },
        {
          "id": "MDRW2100009189.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:56"
        },
        {
          "id": "MDRW2100009189.1.18",
          "form": "난 다 발라놓고 한입에 고기 가득 먹는 느낌이 좋아",
          "original_form": "난 다 발라놓고 한입에 고기 가득 먹는 느낌이 좋아",
          "speaker_id": "1",
          "time": "20210827 10:56"
        },
        {
          "id": "MDRW2100009189.1.19",
          "form": "수저로 왕창 떠서",
          "original_form": "수저로 왕창 떠서",
          "speaker_id": "1",
          "time": "20210827 10:57"
        },
        {
          "id": "MDRW2100009189.1.20",
          "form": "와아아ㅏㅇ",
          "original_form": "와아아ㅏㅇ",
          "speaker_id": "1",
          "time": "20210827 10:57"
        },
        {
          "id": "MDRW2100009189.1.21",
          "form": "아 진짜? ㅋㅋㅋ난 그렇게 왁! 하고 먹으면 속 안좋앜ㅋㅋㅋㅋ",
          "original_form": "아 진짜? ㅋㅋㅋ난 그렇게 왁! 하고 먹으면 속 안좋앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100009189.1.22",
          "form": "체할까봐 조심조심",
          "original_form": "체할까봐 조심조심",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100009189.1.23",
          "form": "아ㅋㅋㅋㅋ맞아...맛있게 먹고 체하면 속상해..",
          "original_form": "아ㅋㅋㅋㅋ맞아...맛있게 먹고 체하면 속상해..",
          "speaker_id": "1",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100009189.1.24",
          "form": "저녁은 뭐먹어ㅋㅋㅋ",
          "original_form": "저녁은 뭐먹어ㅋㅋㅋ",
