      "utterance": [
        {
          "id": "MDRW2100004660.1.1",
          "form": "비가 너무오네요",
          "original_form": "비가 너무오네요",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004660.1.2",
          "form": "지금도 오나요?",
          "original_form": "지금도 오나요?",
          "speaker_id": "2",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004660.1.3",
          "form": "네 저희동네는 잠깐 1시간 정도 갠줄알았는데 다시 비가 막오네요",
          "original_form": "네 저희동네는 잠깐 1시간 정도 갠줄알았는데 다시 비가 막오네요",
          "speaker_id": "1",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004660.1.4",
          "form": "저희는 멈춘 거 같아요",
          "original_form": "저희는 멈춘 거 같아요",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004660.1.5",
          "form": "아까는.진짜 엄청 오더라구요",
          "original_form": "아까는.진짜 엄청 오더라구요",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004660.1.6",
          "form": "아그렇군요 주말에 운동할겸 산에 가고싶었는데 비가와서 취소했어요",
          "original_form": "아그렇군요 주말에 운동할겸 산에 가고싶었는데 비가와서 취소했어요",
          "speaker_id": "1",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004660.1.7",
          "form": "운동으로 산 가는 거 괜찮은데",
          "original_form": "운동으로 산 가는 거 괜찮은데",
          "speaker_id": "2",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004660.1.8",
          "form": "너무 아쉽겠어요",
          "original_form": "너무 아쉽겠어요",
          "speaker_id": "2",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004660.1.9",
          "form": "요즘선선하니 좋은데 다음주에는 날씨좋았으면 해요",
          "original_form": "요즘선선하니 좋은데 다음주에는 날씨좋았으면 해요",
          "speaker_id": "1",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004660.1.10",
          "form": "맞아요 흑",
          "original_form": "맞아요 흑",
          "speaker_id": "2",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004660.1.11",
          "form": "근데 알기로는 다음주에도",
          "original_form": "근데 알기로는 다음주에도",
          "speaker_id": "2",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004660.1.12",
          "form": "비가온대요",
          "original_form": "비가온대요",
          "speaker_id": "2",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004660.1.13",
          "form": "그렇군요 다음주에도 집에서 할수있는활동들만 해야겠네요",
          "original_form": "그렇군요 다음주에도 집에서 할수있는활동들만 해야겠네요",
          "speaker_id": "1",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004660.1.14",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004660.1.15",
          "form": "날씨 때문에라도 집에 있는게 적응되어도 힘드네요 ㅎ",
          "original_form": "날씨 때문에라도 집에 있는게 적응되어도 힘드네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004660.1.16",
          "form": "그건진짜 적응이 되어도 나가고싶은 마음이 커요",
          "original_form": "그건진짜 적응이 되어도 나가고싶은 마음이 커요",
          "speaker_id": "1",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004660.1.17",
          "form": "맞아요 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004660.1.18",
          "form": "좋아하시는계절있으세요?",
          "original_form": "좋아하시는계절있으세요?",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004660.1.19",
          "form": "전 가을이요",
          "original_form": "전 가을이요",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004660.1.20",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004660.1.21",
          "form": "저는 봄을 좋아해요~~ 파릇파릇생명이 느껴지는게 좋아서",
          "original_form": "저는 봄을 좋아해요~~ 파릇파릇생명이 느껴지는게 좋아서",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004660.1.22",
          "form": "저도 봄 좋긴해요 !!!",
          "original_form": "저도 봄 좋긴해요 !!!",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004660.1.23",
          "form": "근데 꽃가루 때문에 살짝 하",
          "original_form": "근데 꽃가루 때문에 살짝 하",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004660.1.24",
          "form": "앗 알러지있으신가요?",
          "original_form": "앗 알러지있으신가요?",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004660.1.25",
          "form": "알러지는 없는데 뭔가 생기눈 느낌이에요 하하",
          "original_form": "알러지는 없는데 뭔가 생기눈 느낌이에요 하하",
