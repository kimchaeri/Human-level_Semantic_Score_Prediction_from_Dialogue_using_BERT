      "utterance": [
        {
          "id": "MDRW2100004809.1.1",
          "form": "반려동물 있으신가요??",
          "original_form": "반려동물 있으신가요??",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004809.1.2",
          "form": "아뇨ㅎㅎ",
          "original_form": "아뇨ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100004809.1.3",
          "form": "아하 동물은 좋아하세요? 요새 랜선집사도 많잖아요",
          "original_form": "아하 동물은 좋아하세요? 요새 랜선집사도 많잖아요",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100004809.1.4",
          "form": "네 오랫동안 키우던 애기를 보내고 못 키우고있어요",
          "original_form": "네 오랫동안 키우던 애기를 보내고 못 키우고있어요",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100004809.1.5",
          "form": "전 동물 넘 사랑해요 ㅎㅎ",
          "original_form": "전 동물 넘 사랑해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100004809.1.6",
          "form": "앗ㅠㅜ 듣기만해도 속상하네요",
          "original_form": "앗ㅠㅜ 듣기만해도 속상하네요",
          "speaker_id": "1",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100004809.1.7",
          "form": "저는 고양이랑 같이 지내요",
          "original_form": "저는 고양이랑 같이 지내요",
          "speaker_id": "1",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100004809.1.8",
          "form": "꺄",
          "original_form": "꺄",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100004809.1.9",
          "form": "넘 귀엽겠네요",
          "original_form": "넘 귀엽겠네요",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100004809.1.10",
          "form": "ㅋㅋㅋ너무 귀여운데 부르면 안와요ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋ너무 귀여운데 부르면 안와요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100004809.1.11",
          "form": "6살이구요",
          "original_form": "6살이구요",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100004809.1.12",
          "form": "세상에 ㅋㅋㅋㅋ",
          "original_form": "세상에 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100004809.1.13",
          "form": "저는 19년 키우고 18년에 보냈어요 ㅎㅎ",
          "original_form": "저는 19년 키우고 18년에 보냈어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100004809.1.14",
          "form": "돈으로 키운 내사랑 ㅋㅋㅋㅋㅋ",
          "original_form": "돈으로 키운 내사랑 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100004809.1.15",
          "form": "돈으로 키운다는말은 공감이네요ㅋㅋ",
          "original_form": "돈으로 키운다는말은 공감이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004809.1.16",
          "form": "오래 같이 가족이었을텐데 많이 속상하시고 못잊으시겠내요ㅠㅜㅜ",
          "original_form": "오래 같이 가족이었을텐데 많이 속상하시고 못잊으시겠내요ㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004809.1.17",
          "form": "저도 고양이가 어느날떠나면 쉽게 다른 아이 못데려올꺼같아요ㅠ",
          "original_form": "저도 고양이가 어느날떠나면 쉽게 다른 아이 못데려올꺼같아요ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100004809.1.18",
          "form": "맞아요 그게 쉽지않더라고요",
          "original_form": "맞아요 그게 쉽지않더라고요",
          "speaker_id": "2",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100004809.1.19",
          "form": "24시간 붙어서 케어하다보니 또 그만큼 희생할 자신이 없어요",
          "original_form": "24시간 붙어서 케어하다보니 또 그만큼 희생할 자신이 없어요",
          "speaker_id": "2",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100004809.1.20",
          "form": "네 책임감이 따른다고 생각해요",
          "original_form": "네 책임감이 따른다고 생각해요",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100004809.1.21",
          "form": "그래도 있는 동안 최선을 다하면 나중에 후회는 없어요",
          "original_form": "그래도 있는 동안 최선을 다하면 나중에 후회는 없어요",
          "speaker_id": "2",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100004809.1.22",
          "form": "네 조언감사합니다 저도 후회없게 최선을 다할께요",
          "original_form": "네 조언감사합니다 저도 후회없게 최선을 다할께요",
          "speaker_id": "1",
          "time": "20210822 10:42"
        }
      ]
    }
