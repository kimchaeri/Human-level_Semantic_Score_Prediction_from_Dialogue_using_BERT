      "utterance": [
        {
          "id": "MDRW2100022435.1.1",
          "form": "영양제는 드시나요?",
          "original_form": "영양제는 드시나요?",
          "speaker_id": "1",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100022435.1.2",
          "form": "영양제는 드시나요?",
          "original_form": "영양제는 드시나요?",
          "speaker_id": "1",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100022435.1.3",
          "form": "네 꾸준히 먹고 있어요",
          "original_form": "네 꾸준히 먹고 있어요",
          "speaker_id": "2",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100022435.1.4",
          "form": "혹시 어디서 구입하세요? 전 아이허브",
          "original_form": "혹시 어디서 구입하세요? 전 아이허브",
          "speaker_id": "1",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100022435.1.5",
          "form": "전 그냥 국내에서 구매해요",
          "original_form": "전 그냥 국내에서 구매해요",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100022435.1.6",
          "form": "영양제 뭐 드시고 있으신가요?",
          "original_form": "영양제 뭐 드시고 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100022435.1.7",
          "form": "전 자로우 락토페린이랑 오메가3 이렇게 먹어요",
          "original_form": "전 자로우 락토페린이랑 오메가3 이렇게 먹어요",
          "speaker_id": "1",
          "time": "20210829 14:00"
        },
        {
          "id": "MDRW2100022435.1.8",
          "form": "락토페린은 뭔가요?",
          "original_form": "락토페린은 뭔가요?",
          "speaker_id": "2",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100022435.1.9",
          "form": "면연력 좋아지는건데 초유로 만든거에요",
          "original_form": "면연력 좋아지는건데 초유로 만든거에요",
          "speaker_id": "1",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100022435.1.10",
          "form": "면역력 영양제군요. 전 유산균이랑 종합비타민 루테인지아잔틴 먹고 있어요",
          "original_form": "면역력 영양제군요. 전 유산균이랑 종합비타민 루테인지아잔틴 먹고 있어요",
          "speaker_id": "2",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022435.1.11",
          "form": "루테인 그거 눈 좋아지는거죠? 유산균은 자로우가 좋던데",
          "original_form": "루테인 그거 눈 좋아지는거죠? 유산균은 자로우가 좋던데",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100022435.1.12",
          "form": "눈 더 안 나빠지려고 먹고 있어요",
          "original_form": "눈 더 안 나빠지려고 먹고 있어요",
          "speaker_id": "2",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022435.1.13",
          "form": "아 저도 눈이 안좋아서 ㅋㅋ 저 난시예요. 0.1이고 시력은",
          "original_form": "아 저도 눈이 안좋아서 ㅋㅋ 저 난시예요. 0.1이고 시력은",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022435.1.14",
          "form": "저보단 좋으시네요",
          "original_form": "저보단 좋으시네요",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022435.1.15",
          "form": "전 많이 나빠서요",
          "original_form": "전 많이 나빠서요",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022435.1.16",
          "form": "와 ㄷㄷ 얼마나 안좋으시면",
          "original_form": "와 ㄷㄷ 얼마나 안좋으시면",
          "speaker_id": "1",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022435.1.17",
          "form": "근시가 심해요 ㅠ",
          "original_form": "근시가 심해요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
