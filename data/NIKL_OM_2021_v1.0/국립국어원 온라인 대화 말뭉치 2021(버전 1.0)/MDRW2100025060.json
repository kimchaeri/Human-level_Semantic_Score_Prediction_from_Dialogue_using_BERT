      "utterance": [
        {
          "id": "MDRW2100025060.1.1",
          "form": "응응 빨리오면 좋겠다!",
          "original_form": "응응 빨리오면 좋겠다!",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025060.1.2",
          "form": "십일번가에서샀음?",
          "original_form": "십일번가에서샀음?",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025060.1.3",
          "form": "6400원?",
          "original_form": "6400원?",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025060.1.4",
          "form": "응응! 6400원주고 샀어!",
          "original_form": "응응! 6400원주고 샀어!",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025060.1.5",
          "form": "나 칸쵸랑 웨하스 너뮤 잘먹고 있어!",
          "original_form": "나 칸쵸랑 웨하스 너뮤 잘먹고 있어!",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025060.1.6",
          "form": "칸쵸 뭐여 ㅋㅋㅋㅋ",
          "original_form": "칸쵸 뭐여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025060.1.7",
          "form": "눅눅하다며",
          "original_form": "눅눅하다며",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025060.1.8",
          "form": "그거 몇봉 먹으니까 꼭 원래 그런것처럼 적응됐어!ㅋㅋ",
          "original_form": "그거 몇봉 먹으니까 꼭 원래 그런것처럼 적응됐어!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025060.1.9",
          "form": "뭐야 ㅋㅋ원래그랬던거아니냐!",
          "original_form": "뭐야 ㅋㅋ원래그랬던거아니냐!",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025060.1.10",
          "form": "칸초맛이 변한거아닐까?!",
          "original_form": "칸초맛이 변한거아닐까?!",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025060.1.11",
          "form": "앗 설마... 이상한데그럼ㅜㅜ",
          "original_form": "앗 설마... 이상한데그럼ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025060.1.12",
          "form": "칸초가 원래 바삭바삭했는지 떠올려봐",
          "original_form": "칸초가 원래 바삭바삭했는지 떠올려봐",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025060.1.13",
          "form": "그래도 눅눅은 아니긴했다",
          "original_form": "그래도 눅눅은 아니긴했다",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025060.1.14",
          "form": "원래바삭이야ㅜㅜ 난나름자주사먹었엉!",
          "original_form": "원래바삭이야ㅜㅜ 난나름자주사먹었엉!",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025060.1.15",
          "form": "ㅋㅋㅋㅋㅋ 난 잘 안사먹었어서ㅋㅋㅋㅋ 올드유저네 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 난 잘 안사먹었어서ㅋㅋㅋㅋ 올드유저네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025060.1.16",
          "form": "난 에이스 초코실패함",
          "original_form": "난 에이스 초코실패함",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025060.1.17",
          "form": "왜ㅋㅋㅋ 맛이없어?",
          "original_form": "왜ㅋㅋㅋ 맛이없어?",
          "speaker_id": "2",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025060.1.18",
          "form": "어어어 ㅜㅜ 초코가 별로 초코답지못해..",
          "original_form": "어어어 ㅜㅜ 초코가 별로 초코답지못해..",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025060.1.19",
          "form": "헛 그런 안타까운...ㅜㅜ 사먹어봐야짓",
          "original_form": "헛 그런 안타까운...ㅜㅜ 사먹어봐야짓",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025060.1.20",
          "form": "궁금하다ㅋㅋㅋ",
          "original_form": "궁금하다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025060.1.21",
          "form": "굳이 추천하고싶지않은걸",
          "original_form": "굳이 추천하고싶지않은걸",
          "speaker_id": "1",
          "time": "20210831 23:13"
        }
      ]
