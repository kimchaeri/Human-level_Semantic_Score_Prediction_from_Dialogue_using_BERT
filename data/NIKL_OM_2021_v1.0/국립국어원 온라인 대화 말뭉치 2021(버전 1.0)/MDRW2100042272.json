      "utterance": [
        {
          "id": "MDRW2100042272.1.1",
          "form": "빵 좋아하세요?",
          "original_form": "빵 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042272.1.2",
          "form": "넹 ㅎㅎ",
          "original_form": "넹 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042272.1.3",
          "form": "저는 이성당 슈크림 매니아 입니당...!!",
          "original_form": "저는 이성당 슈크림 매니아 입니당...!!",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042272.1.4",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042272.1.5",
          "form": "이성당이면 군산인가요?",
          "original_form": "이성당이면 군산인가요?",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042272.1.6",
          "form": "이성당 라떼랑 슈크림빵 같이먹으면 힐링되여 ㅎㅎ",
          "original_form": "이성당 라떼랑 슈크림빵 같이먹으면 힐링되여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042272.1.7",
          "form": "요즘 체인있어서 서울에도 있어요!",
          "original_form": "요즘 체인있어서 서울에도 있어요!",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042272.1.8",
          "form": "드셔보셨어여?",
          "original_form": "드셔보셨어여?",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042272.1.9",
          "form": "아 그렇군요!!",
          "original_form": "아 그렇군요!!",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042272.1.10",
          "form": "이성당 야채빵 먹어봤어요",
          "original_form": "이성당 야채빵 먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042272.1.11",
          "form": "생과자랑",
          "original_form": "생과자랑",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100042272.1.12",
          "form": "야채고로케도 맛있어요 ㅎㅎ",
          "original_form": "야채고로케도 맛있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042272.1.13",
          "form": "야채빵 맛있었던 기억이 있어서",
          "original_form": "야채빵 맛있었던 기억이 있어서",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042272.1.14",
          "form": "야채고로케도 기대되네요",
          "original_form": "야채고로케도 기대되네요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042272.1.15",
          "form": "진짜옛날에 군산에서도 먹어봤는데 사람들이 줄서서 사가는 오픈된 느낌이라 어색했어요 ㅎㅎ",
          "original_form": "진짜옛날에 군산에서도 먹어봤는데 사람들이 줄서서 사가는 오픈된 느낌이라 어색했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042272.1.16",
          "form": "혹시 근처에 있는지 검색하고 가보세용",
          "original_form": "혹시 근처에 있는지 검색하고 가보세용",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042272.1.17",
          "form": "아 저는 직접 방문은 못해봤고",
          "original_form": "아 저는 직접 방문은 못해봤고",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042272.1.18",
          "form": "선물로 택배받아 먹어봤는데",
          "original_form": "선물로 택배받아 먹어봤는데",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042272.1.19",
          "form": "주변에 있나 찾아봐야겠네요",
          "original_form": "주변에 있나 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042272.1.20",
          "form": "네네~ 라떼도 맛있어서 같이 드셔보세용 ㅎ",
          "original_form": "네네~ 라떼도 맛있어서 같이 드셔보세용 ㅎ",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042272.1.21",
          "form": "네네 메뉴 추천 감사해용ㅎㅎ",
          "original_form": "네네 메뉴 추천 감사해용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042272.1.22",
          "form": " ",
          "original_form": "{emoji:☕} ",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042272.1.23",
          "form": "그거먹으니까 파바나 뚜쥬 못가겠더라구요 ㅜㅜ",
          "original_form": "그거먹으니까 파바나 뚜쥬 못가겠더라구요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042272.1.24",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042272.1.25",
          "form": "저도 파바나 뚜쥬는 잘 안가게 되더라고요",
          "original_form": "저도 파바나 뚜쥬는 잘 안가게 되더라고요",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042272.1.26",
          "form": "요즘은 동네 카페도 베이커리 특화된 곳들이 많아서",
          "original_form": "요즘은 동네 카페도 베이커리 특화된 곳들이 많아서",
          "speaker_id": "1",
