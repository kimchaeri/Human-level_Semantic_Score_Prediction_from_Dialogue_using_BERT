      "utterance": [
        {
          "id": "MDRW2100021214.1.1",
          "form": "조미료는 어떤걸 쓰세요?",
          "original_form": "조미료는 어떤걸 쓰세요?",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021214.1.2",
          "form": "미원? 연두?",
          "original_form": "미원? 연두?",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021214.1.3",
          "form": "참치액 자주써요.",
          "original_form": "참치액 자주써요.",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021214.1.4",
          "form": "아 전 보기만 하고 안사봤어요 어느 브랜드걸 쓰시나요?",
          "original_form": "아 전 보기만 하고 안사봤어요 어느 브랜드걸 쓰시나요?",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021214.1.5",
          "form": "다시다도살짝 써요",
          "original_form": "다시다도살짝 써요",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021214.1.6",
          "form": "한라 참치액 써요",
          "original_form": "한라 참치액 써요",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021214.1.7",
          "form": "모르는 브랜드네요 메모해둘께요 동네 마트엔 화미 참치액이 있었어요",
          "original_form": "모르는 브랜드네요 메모해둘께요 동네 마트엔 화미 참치액이 있었어요",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021214.1.8",
          "form": "인터넷에서도 팔아요",
          "original_form": "인터넷에서도 팔아요",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021214.1.9",
          "form": "ㅠ 인터넷은 배송비가 들어서 여러가지 살때 한꺼번에 사려구요",
          "original_form": "ㅠ 인터넷은 배송비가 들어서 여러가지 살때 한꺼번에 사려구요",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021214.1.10",
          "form": "그렇죠. 재료 준비하는것보다 때론 배달하는게 저렴하기도 한것 같아요",
          "original_form": "그렇죠. 재료 준비하는것보다 때론 배달하는게 저렴하기도 한것 같아요",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021214.1.11",
          "form": "네 그걸 비교하는게 되게  품이 들어요",
          "original_form": "네 그걸 비교하는게 되게  품이 들어요",
          "speaker_id": "1",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021214.1.12",
          "form": "그렇긴 하죠",
          "original_form": "그렇긴 하죠",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021214.1.13",
          "form": "네 그렇더라구요",
          "original_form": "네 그렇더라구요",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100021214.1.14",
          "form": "저녁은 뭐드시나요?",
          "original_form": "저녁은 뭐드시나요?",
          "speaker_id": "2",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100021214.1.15",
          "form": "전 그냥 있는 반찬에 김먹으려구요 ㅋㅋ name2님은요?",
          "original_form": "전 그냥 있는 반찬에 김먹으려구요 ㅋㅋ &name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100021214.1.16",
          "form": "아직 못정했어요.",
          "original_form": "아직 못정했어요.",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021214.1.17",
          "form": "남이 해준밥 시켜먹고 싶네요",
          "original_form": "남이 해준밥 시켜먹고 싶네요",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021214.1.18",
          "form": "그게 최고죠 넘 힘드시면 편의점 밥이라도 드시는건 어떠세요?",
          "original_form": "그게 최고죠 넘 힘드시면 편의점 밥이라도 드시는건 어떠세요?",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021214.1.19",
          "form": "편의점 도시락도 가끔 먹으면 맛있죠",
          "original_form": "편의점 도시락도 가끔 먹으면 맛있죠",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021214.1.20",
          "form": "그 가끔찬스를 지금 써보시는 것도 ㅋㅋ",
          "original_form": "그 가끔찬스를 지금 써보시는 것도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021214.1.21",
          "form": "주택가라 동네 편의점에 도시락이 많지 않아요.",
          "original_form": "주택가라 동네 편의점에 도시락이 많지 않아요.",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021214.1.22",
          "form": "아이구 편의점 가는게 일이겠네요",
          "original_form": "아이구 편의점 가는게 일이겠네요",
          "speaker_id": "1",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021214.1.23",
          "form": "간단하게 먹을 삼각김밥사러가요",
          "original_form": "간단하게 먹을 삼각김밥사러가요",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021214.1.24",
          "form": "삼김도 맛있죠 맛저하세요",
          "original_form": "삼김도 맛있죠 맛저하세요",
          "speaker_id": "1",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100021214.1.25",
          "form": "넹저녁 시간이 다가오네요",
          "original_form": "넹저녁 시간이 다가오네요",
          "speaker_id": "2",
          "time": "20210828 17:57"
        }
      ]
