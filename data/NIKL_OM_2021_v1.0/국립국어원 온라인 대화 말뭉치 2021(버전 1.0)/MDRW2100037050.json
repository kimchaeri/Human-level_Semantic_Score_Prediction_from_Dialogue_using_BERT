      "utterance": [
        {
          "id": "MDRW2100037050.1.1",
          "form": "고민있는데 얘기해도 되나요?",
          "original_form": "고민있는데 얘기해도 되나요?",
          "speaker_id": "1",
          "time": "20210831 18:03"
        },
        {
          "id": "MDRW2100037050.1.2",
          "form": "네네!! 들려주세요!",
          "original_form": "네네!! 들려주세요!",
          "speaker_id": "2",
          "time": "20210831 18:41"
        },
        {
          "id": "MDRW2100037050.1.3",
          "form": "저랑 나이차가 나는 남자애가 있는데",
          "original_form": "저랑 나이차가 나는 남자애가 있는데",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100037050.1.4",
          "form": "연하남10살",
          "original_form": "연하남10살",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100037050.1.5",
          "form": "차이",
          "original_form": "차이",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100037050.1.6",
          "form": "걔랑 실제로 사귀고있는건아닌데",
          "original_form": "걔랑 실제로 사귀고있는건아닌데",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100037050.1.7",
          "form": "분위기가좋다가도",
          "original_form": "분위기가좋다가도",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100037050.1.8",
          "form": "자주트러블일어나서 싸워서",
          "original_form": "자주트러블일어나서 싸워서",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100037050.1.9",
          "form": "걔가이제는 헤어지자고 해서 저를 차단한거에요",
          "original_form": "걔가이제는 헤어지자고 해서 저를 차단한거에요",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100037050.1.10",
          "form": "내가 안붙잡을꺼라고했거든요",
          "original_form": "내가 안붙잡을꺼라고했거든요",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100037050.1.11",
          "form": "그러더니 막 화내서 짜증내면서 토라졌는데",
          "original_form": "그러더니 막 화내서 짜증내면서 토라졌는데",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100037050.1.12",
          "form": "내가 다시 사과하니깐 되돌아오네요....",
          "original_form": "내가 다시 사과하니깐 되돌아오네요....",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100037050.1.13",
          "form": "헉!! name1님 능력자",
          "original_form": "헉!! &name1&님 능력자",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100037050.1.14",
          "form": "10살 연하라니!!!!",
          "original_form": "10살 연하라니!!!!",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100037050.1.15",
          "form": "네....저랑 10살차이가나요",
          "original_form": "네....저랑 10살차이가나요",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100037050.1.16",
          "form": "그럼 다시 화해하고 만나시는거군요!! ㅎㅎ",
          "original_form": "그럼 다시 화해하고 만나시는거군요!! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100037050.1.17",
          "form": "어제까지만해도...이별에서 다시 재결합됐어요",
          "original_form": "어제까지만해도...이별에서 다시 재결합됐어요",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100037050.1.18",
          "form": "제가 좋다고하면 걔도 좋다하고",
          "original_form": "제가 좋다고하면 걔도 좋다하고",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037050.1.19",
          "form": "ㅎㅎ 사랑하는 감정은 숨길 수 없잖아요 ㅎㅎ",
          "original_form": "ㅎㅎ 사랑하는 감정은 숨길 수 없잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037050.1.20",
          "form": "내가 싫다하면 걔도 싫다고해요",
          "original_form": "내가 싫다하면 걔도 싫다고해요",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037050.1.21",
          "form": "꼭 저따라하는것같아서",
          "original_form": "꼭 저따라하는것같아서",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037050.1.22",
          "form": "내가 울고싶다하면 걔도 울고싶데요",
          "original_form": "내가 울고싶다하면 걔도 울고싶데요",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037050.1.23",
          "form": "흠.!!!",
          "original_form": "흠.!!!",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037050.1.24",
          "form": "이게 머하자는건지....",
          "original_form": "이게 머하자는건지....",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037050.1.25",
          "form": "좀 유치하게 말을 주고받아요",
          "original_form": "좀 유치하게 말을 주고받아요",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037050.1.26",
          "form": "내가 존댓말하면 걔도 존댓말하고",
          "original_form": "내가 존댓말하면 걔도 존댓말하고",
          "speaker_id": "1",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100037050.1.27",
          "form": "역시 연하남은 귀여운 맛...!",
          "original_form": "역시 연하남은 귀여운 맛...!",
          "speaker_id": "2",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100037050.1.28",
          "form": "저랑 카톡프사 똑같아요ㅋㅋㅋㅋ",
          "original_form": "저랑 카톡프사 똑같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037050.1.29",
          "form": "제가 프사내리면",
          "original_form": "제가 프사내리면",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037050.1.30",
          "form": "걔도 내리고",
          "original_form": "걔도 내리고",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037050.1.31",
          "form": "ㅋㅋ혹시 20대 초반인가요?",
          "original_form": "ㅋㅋ혹시 20대 초반인가요?",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037050.1.32",
          "form": "걔 20대초반이에요",
          "original_form": "걔 20대초반이에요",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037050.1.33",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037050.1.34",
          "form": "아직 군대는 안갔다왔나요?!!",
          "original_form": "아직 군대는 안갔다왔나요?!!",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037050.1.35",
          "form": "20대초반  남자들이 혹 그런경향이 많나요?",
          "original_form": "20대초반  남자들이 혹 그런경향이 많나요?",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037050.1.36",
          "form": "걔 군면제라서 한달만하는 훈련만 남겨놓고 일하고 있어요",
          "original_form": "걔 군면제라서 한달만하는 훈련만 남겨놓고 일하고 있어요",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100037050.1.37",
          "form": "20대초반남자애들이 그런 경향이 있는가요",
          "original_form": "20대초반남자애들이 그런 경향이 있는가요",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100037050.1.38",
          "form": "제가 20대 초반 남자를 만날땐 저도 그 나이때여서 모르겠는데 사람 성향 차이가 있겠지만 군대를 가기 전엔 조금 아이같은 면이 있는 거 같아요!",
          "original_form": "제가 20대 초반 남자를 만날땐 저도 그 나이때여서 모르겠는데 사람 성향 차이가 있겠지만 군대를 가기 전엔 조금 아이같은 면이 있는 거 같아요!",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037050.1.39",
          "form": "걔는 철들없다고 자기 입으로 말하던데",
          "original_form": "걔는 철들없다고 자기 입으로 말하던데",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037050.1.40",
          "form": "가정적인남자라고도하고 집안일 돕기도하니깐",
          "original_form": "가정적인남자라고도하고 집안일 돕기도하니깐",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037050.1.41",
          "form": "저를 위해서 노래도열심히 불러줘요",
          "original_form": "저를 위해서 노래도열심히 불러줘요",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037050.1.42",
          "form": "name1님이 좋아하신다면 그런 유치한 면도 사랑으로 감싸주세요 ㅎㅎ",
          "original_form": "&name1&님이 좋아하신다면 그런 유치한 면도 사랑으로 감싸주세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037050.1.43",
          "form": "그런가요? 그러고보니 그런것같아요",
          "original_form": "그런가요? 그러고보니 그런것같아요",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100037050.1.44",
          "form": "연하남이랑 나이차가 많이나서 그런지",
          "original_form": "연하남이랑 나이차가 많이나서 그런지",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100037050.1.45",
          "form": "그것도 일리 있는것 같아요",
          "original_form": "그것도 일리 있는것 같아요",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100037050.1.46",
          "form": "일단 걔가 어린게티가나는데",
          "original_form": "일단 걔가 어린게티가나는데",
          "speaker_id": "1",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037050.1.47",
