      "utterance": [
        {
          "id": "MDRW2100026605.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026605.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026605.1.3",
          "form": "다이어트 하세요?",
          "original_form": "다이어트 하세요?",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026605.1.4",
          "form": "다이어트는 안하고 하늘의 뜻에 맡기고 있답니다...",
          "original_form": "다이어트는 안하고 하늘의 뜻에 맡기고 있답니다...",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026605.1.5",
          "form": "ㅋㅋ 저도 입다이어터예요",
          "original_form": "ㅋㅋ 저도 입다이어터예요",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026605.1.6",
          "form": "ㅋㅋ 제가 열심히 한다고 해서 달라질게 없더라고욬ㅋㅋ",
          "original_form": "ㅋㅋ 제가 열심히 한다고 해서 달라질게 없더라고욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026605.1.7",
          "form": "그럼 운동은하세요?ㅋ",
          "original_form": "그럼 운동은하세요?ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026605.1.8",
          "form": "안먹을순 없어요",
          "original_form": "안먹을순 없어요",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026605.1.9",
          "form": "운동은 귀찮아서 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "운동은 귀찮아서 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026605.1.10",
          "form": "운동 즐겨하시나요??",
          "original_form": "운동 즐겨하시나요??",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026605.1.11",
          "form": "아 ᆢ저도 운동 가볍게만해요",
          "original_form": "아 ᆢ저도 운동 가볍게만해요",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026605.1.12",
          "form": "걷기정도",
          "original_form": "걷기정도",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026605.1.13",
          "form": "뭐 거의 소화시키는 수준이죠ㅎㅎㅜ",
          "original_form": "뭐 거의 소화시키는 수준이죠ㅎㅎㅜ",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026605.1.14",
          "form": "저도 그정도... 그래도 우리 걷잖아요 ㅋㅋㅋ",
          "original_form": "저도 그정도... 그래도 우리 걷잖아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026605.1.15",
          "form": "그게 어딥니까",
          "original_form": "그게 어딥니까",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026605.1.16",
          "form": "ㅋㅋㅋ네 그걸로 몸무게",
          "original_form": "ㅋㅋㅋ네 그걸로 몸무게",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026605.1.17",
          "form": "현상유지중이예요",
          "original_form": "현상유지중이예요",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026605.1.18",
          "form": "혹시 하고 싶은 운동은 있으신가요??",
          "original_form": "혹시 하고 싶은 운동은 있으신가요??",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026605.1.19",
