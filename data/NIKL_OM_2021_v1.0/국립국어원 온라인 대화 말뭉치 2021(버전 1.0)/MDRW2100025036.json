      "utterance": [
        {
          "id": "MDRW2100025036.1.1",
          "form": "네이버에 송탄이나경부대찌개 치니까",
          "original_form": "네이버에 송탄이나경부대찌개 치니까",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100025036.1.2",
          "form": "수원에 있는데요...",
          "original_form": "수원에 있는데요...",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100025036.1.3",
          "form": "띠로리.......",
          "original_form": "띠로리.......",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100025036.1.4",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋㄱ맞아요",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋㄱ맞아요",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100025036.1.5",
          "form": "수원맛집....",
          "original_form": "수원맛집....",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100025036.1.6",
          "form": "어디서 이전했다고 들었어요",
          "original_form": "어디서 이전했다고 들었어요",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100025036.1.7",
          "form": "ㅋㅋㅋㅋㅋ 아 송탄이어야 된다구여~!",
          "original_form": "ㅋㅋㅋㅋㅋ 아 송탄이어야 된다구여~!",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100025036.1.8",
          "form": "아마그것이 송탄출신이 만든...그런...",
          "original_form": "아마그것이 송탄출신이 만든...그런...",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100025036.1.9",
          "form": "아 송탄가봐야겠네",
          "original_form": "아 송탄가봐야겠네",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100025036.1.10",
          "form": "김네집으로 가셔야 합니다",
          "original_form": "김네집으로 가셔야 합니다",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025036.1.11",
          "form": "저장해놓겠습니다",
          "original_form": "저장해놓겠습니다",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025036.1.12",
          "form": "좋은거알았네요",
          "original_form": "좋은거알았네요",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025036.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025036.1.14",
          "form": "짠거 좋아하세요?",
          "original_form": "짠거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025036.1.15",
          "form": "송탄 김네집 부대찌개",
          "original_form": "송탄 김네집 부대찌개",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025036.1.16",
          "form": "그럼 아주 딱일 거여요..",
          "original_form": "그럼 아주 딱일 거여요..",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025036.1.17",
          "form": "저 찌개라면",
          "original_form": "저 찌개라면",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025036.1.18",
          "form": "가리지않고먹는 국물러버",
          "original_form": "가리지않고먹는 국물러버",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025036.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025036.1.20",
          "form": "거기도 1인분에",
          "original_form": "거기도 1인분에",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025036.1.21",
          "form": "8천원인가 9천원인가 하는데",
          "original_form": "8천원인가 9천원인가 하는데",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025036.1.22",
          "form": "와 송탄 치니까 바로 김네집뜨네야",
          "original_form": "와 송탄 치니까 바로 김네집뜨네야",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025036.1.23",
          "form": "포장 2인분하면 네명은먹을수있어요",
          "original_form": "포장 2인분하면 네명은먹을수있어요",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025036.1.24",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025036.1.25",
          "form": "오오...",
          "original_form": "오오...",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025036.1.26",
          "form": "저기가 원래도",
          "original_form": "저기가 원래도",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025036.1.27",
          "form": "와 고기가 되게 실해요",
          "original_form": "와 고기가 되게 실해요",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025036.1.28",
          "form": "유명해서 기다려야 했는데",
          "original_form": "유명해서 기다려야 했는데",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025036.1.29",
          "form": "백종원삼대천왕나오고 더 잘돼서",
          "original_form": "백종원삼대천왕나오고 더 잘돼서",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025036.1.30",
          "form": "대기실을 아예 만들었더라구여",
          "original_form": "대기실을 아예 만들었더라구여",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025036.1.31",
          "form": "아하....",
          "original_form": "아하....",
          "speaker_id": "2",
          "time": "20210831 22:36"
        }
      ]
    }
