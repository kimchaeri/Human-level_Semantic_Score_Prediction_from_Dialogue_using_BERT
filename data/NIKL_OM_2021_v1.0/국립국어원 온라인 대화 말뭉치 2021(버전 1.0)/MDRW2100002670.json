      "utterance": [
        {
          "id": "MDRW2100002670.1.1",
          "form": "배달해요?",
          "original_form": "배달해요?",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002670.1.2",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002670.1.3",
          "form": "하지만 배달 음식은 좋아합니다 ㅎㅎ",
          "original_form": "하지만 배달 음식은 좋아합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002670.1.4",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002670.1.5",
          "form": "특히 치킨을 엄청 좋아해요!",
          "original_form": "특히 치킨을 엄청 좋아해요!",
          "speaker_id": "2",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002670.1.6",
          "form": "닭다리 뜯는것만큼 행복한게 없더라고요 ㅠ.ㅠ",
          "original_form": "닭다리 뜯는것만큼 행복한게 없더라고요 ㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002670.1.7",
          "form": "맛아요 다리가",
          "original_form": "맛아요 다리가",
          "speaker_id": "1",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002670.1.8",
          "form": "그리고 통닭이랑 피자, 떡볶이 등등",
          "original_form": "그리고 통닭이랑 피자, 떡볶이 등등",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002670.1.9",
          "form": "참 맛있는게 많은거같아요~",
          "original_form": "참 맛있는게 많은거같아요~",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002670.1.10",
          "form": "분식도 좋죠",
          "original_form": "분식도 좋죠",
          "speaker_id": "1",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002670.1.11",
          "form": "어묵국물하고 먹는 분식 진짜 최고에요 ㅠㅠ",
          "original_form": "어묵국물하고 먹는 분식 진짜 최고에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:47"
        },
        {
          "id": "MDRW2100002670.1.12",
          "form": "국물 최고죠",
          "original_form": "국물 최고죠",
          "speaker_id": "1",
          "time": "20210820 14:50"
        },
        {
          "id": "MDRW2100002670.1.13",
          "form": "맛집이나 카페는 가보셨나요?",
          "original_form": "맛집이나 카페는 가보셨나요?",
          "speaker_id": "2",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002670.1.14",
          "form": "맛집 좋아래요",
          "original_form": "맛집 좋아래요",
          "speaker_id": "1",
          "time": "20210820 14:54"
        },
        {
          "id": "MDRW2100002670.1.15",
          "form": "주변에 맛집있으면 꼭 한번 가게 되더라고요~",
          "original_form": "주변에 맛집있으면 꼭 한번 가게 되더라고요~",
          "speaker_id": "2",
          "time": "20210820 14:54"
        },
        {
          "id": "MDRW2100002670.1.16",
          "form": "카페도 그렇고요 ㅎㅎ",
          "original_form": "카페도 그렇고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:54"
        },
        {
          "id": "MDRW2100002670.1.17",
          "form": "더벤티라고 최근에 가성비 좋은 카페도 찾아서 거기위주로 갈까 싶기도 해요!",
          "original_form": "더벤티라고 최근에 가성비 좋은 카페도 찾아서 거기위주로 갈까 싶기도 해요!",
          "speaker_id": "2",
          "time": "20210820 14:55"
        },
        {
