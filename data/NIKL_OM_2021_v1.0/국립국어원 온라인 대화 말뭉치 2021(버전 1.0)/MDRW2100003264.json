      "utterance": [
        {
          "id": "MDRW2100003264.1.1",
          "form": "집안일은 많이 하세요?",
          "original_form": "집안일은 많이 하세요?",
          "speaker_id": "1",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003264.1.2",
          "form": "집안일 많이 하시나요?",
          "original_form": "집안일 많이 하시나요?",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003264.1.3",
          "form": "네 청소 빨래 요리 ㅋ 전부다요",
          "original_form": "네 청소 빨래 요리 ㅋ 전부다요",
          "speaker_id": "2",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003264.1.4",
          "form": "와 살림꾼이시네요",
          "original_form": "와 살림꾼이시네요",
          "speaker_id": "1",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003264.1.5",
          "form": "진짜 귀찮은데ㅠ",
          "original_form": "진짜 귀찮은데ㅠ",
          "speaker_id": "1",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003264.1.6",
          "form": "어쩔수 없이 하는거죠ㅎㅎ 해줄사람이 없어서",
          "original_form": "어쩔수 없이 하는거죠ㅎㅎ 해줄사람이 없어서",
          "speaker_id": "2",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003264.1.7",
          "form": "집안일 뭐하세요? 가족분들이랑 같이사세요?",
          "original_form": "집안일 뭐하세요? 가족분들이랑 같이사세요?",
          "speaker_id": "2",
          "time": "20210820 21:59"
        },
        {
          "id": "MDRW2100003264.1.8",
          "form": "네 같이 살아요.",
          "original_form": "네 같이 살아요.",
          "speaker_id": "1",
          "time": "20210820 21:59"
        },
        {
          "id": "MDRW2100003264.1.9",
          "form": "와 독립이시군요! 나혼자 산다!!",
          "original_form": "와 독립이시군요! 나혼자 산다!!",
          "speaker_id": "1",
          "time": "20210820 21:59"
        },
        {
          "id": "MDRW2100003264.1.10",
          "form": "네네 혼자ㅋ 자유로운건 좋은데 밥차리고 치우고 귀찮네요",
          "original_form": "네네 혼자ㅋ 자유로운건 좋은데 밥차리고 치우고 귀찮네요",
          "speaker_id": "2",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003264.1.11",
          "form": "ㅋㅋ 잘 때 안무서우신가요",
          "original_form": "ㅋㅋ 잘 때 안무서우신가요",
          "speaker_id": "1",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003264.1.12",
          "form": "옛날은 가끔 무서웠는데 요즘은 별느낌없어요ㅎ 대신 귀신나오는 그런건 안봐요ㅎ",
          "original_form": "옛날은 가끔 무서웠는데 요즘은 별느낌없어요ㅎ 대신 귀신나오는 그런건 안봐요ㅎ",
          "speaker_id": "2",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003264.1.13",
          "form": "맞아요 요즘세상에 ㅋㅋ",
          "original_form": "맞아요 요즘세상에 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:12"
        },
        {
          "id": "MDRW2100003264.1.14",
          "form": "귀신은 왠지 무서워서ㅋ 가위눌려보니 진짜 있을거같고그래요",
          "original_form": "귀신은 왠지 무서워서ㅋ 가위눌려보니 진짜 있을거같고그래요",
          "speaker_id": "2",
          "time": "20210820 22:13"
        },
        {
          "id": "MDRW2100003264.1.15",
          "form": "앗 가위 눌러보셨어요?",
          "original_form": "앗 가위 눌러보셨어요?",
          "speaker_id": "1",
          "time": "20210820 22:13"
        },
        {
          "id": "MDRW2100003264.1.16",
          "form": "혼자 있을때?",
          "original_form": "혼자 있을때?",
          "speaker_id": "1",
          "time": "20210820 22:13"
        },
        {
          "id": "MDRW2100003264.1.17",
          "form": "네 귀에다 애기가 장난감 가지고 놀더라구요",
          "original_form": "네 귀에다 애기가 장난감 가지고 놀더라구요",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003264.1.18",
          "form": "헉 무서워요ㅠㅠ",
          "original_form": "헉 무서워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 22:16"
        }
      ]
