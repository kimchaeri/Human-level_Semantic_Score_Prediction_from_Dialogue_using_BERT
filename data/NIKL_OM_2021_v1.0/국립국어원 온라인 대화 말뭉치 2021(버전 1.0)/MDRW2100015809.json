          "speaker_id": "2",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015809.1.3",
          "form": "게임 좋아해요?",
          "original_form": "게임 좋아해요?",
          "speaker_id": "2",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015809.1.4",
          "form": "예전에요ㅋㅋ",
          "original_form": "예전에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015809.1.5",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "1",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015809.1.6",
          "form": "저두 예전에요ㅋㅋㅋ",
          "original_form": "저두 예전에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015809.1.7",
          "form": "요즘은 안해요?",
          "original_form": "요즘은 안해요?",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015809.1.8",
          "form": "전 모바일게임 소소하게? 하고있어요ㅋㅋ",
          "original_form": "전 모바일게임 소소하게? 하고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015809.1.9",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015809.1.10",
          "form": "쿠키런이요",
          "original_form": "쿠키런이요",
          "speaker_id": "2",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015809.1.11",
          "form": "요즘에도 많이 하나요?",
          "original_form": "요즘에도 많이 하나요?",
          "speaker_id": "1",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015809.1.12",
          "form": "음 시간될때 조금씩 해요ㅋㅋ",
          "original_form": "음 시간될때 조금씩 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015809.1.13",
          "form": "메이플 해보셨나요?",
          "original_form": "메이플 해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015809.1.14",
          "form": "해봤죠ㅋㅋ",
          "original_form": "해봤죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015809.1.15",
          "form": "비숍 키웠었어요",
          "original_form": "비숍 키웠었어요",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015809.1.16",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015809.1.17",
          "form": "마법사랑 궁수요ㅋㅋ",
          "original_form": "마법사랑 궁수요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015809.1.18",
          "form": "저랑 같네요ㅋㅋ",
          "original_form": "저랑 같네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015809.1.19",
          "form": "언제까지 했었어요??",
          "original_form": "언제까지 했었어요??",
