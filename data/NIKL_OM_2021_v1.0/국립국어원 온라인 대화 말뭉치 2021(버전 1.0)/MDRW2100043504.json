      "utterance": [
        {
          "id": "MDRW2100043504.1.1",
          "form": "name1님 쇼핑 자주하시나요",
          "original_form": "&name1&님 쇼핑 자주하시나요",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043504.1.2",
          "form": "쇼핑 자주는 안해요 저 사고 싶은건 있어요!",
          "original_form": "쇼핑 자주는 안해요 저 사고 싶은건 있어요!",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100043504.1.3",
          "form": "어떤거 사고싶으세요?!",
          "original_form": "어떤거 사고싶으세요?!",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100043504.1.4",
          "form": "저 신발이요!!",
          "original_form": "저 신발이요!!",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100043504.1.5",
          "form": "오 저도 신발 사고싶어요",
          "original_form": "오 저도 신발 사고싶어요",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100043504.1.6",
          "form": "여름에 샌들 하나 사고싶었는데 ㅋㅋㅋㅋ",
          "original_form": "여름에 샌들 하나 사고싶었는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100043504.1.7",
          "form": "여름 다 지났네요 ㅎㅎ",
          "original_form": "여름 다 지났네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100043504.1.8",
          "form": "헐 그러네요 가을이 왔다고는 느꼈는데 여름이 지나갔다고는 생각 못했네요",
          "original_form": "헐 그러네요 가을이 왔다고는 느꼈는데 여름이 지나갔다고는 생각 못했네요",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100043504.1.9",
          "form": "저는 런닝화 사고 싶어요",
          "original_form": "저는 런닝화 사고 싶어요",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100043504.1.10",
          "form": "아하~~ 뭐 봐둔 제품 있으신가요",
          "original_form": "아하~~ 뭐 봐둔 제품 있으신가요",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100043504.1.11",
          "form": "아뇨 그런건 아니지만 그냥 걷기 좋은 신발을 갖고 싶어요",
          "original_form": "아뇨 그런건 아니지만 그냥 걷기 좋은 신발을 갖고 싶어요",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100043504.1.12",
          "form": "저는 이상하게 단화가 좋거든요",
          "original_form": "저는 이상하게 단화가 좋거든요",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100043504.1.13",
          "form": "근데 오래걷기엔 안좋다고 해서 하나 사볼까 하구요ㅠ",
          "original_form": "근데 오래걷기엔 안좋다고 해서 하나 사볼까 하구요ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100043504.1.14",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100043504.1.15",
          "form": "저는 발이 약간 평발이라",
          "original_form": "저는 발이 약간 평발이라",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100043504.1.16",
          "form": "오래걸으면 발아프거든요",
          "original_form": "오래걸으면 발아프거든요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100043504.1.17",
          "form": "그래서 무조건 운동화 신어요 ㅋㅋㅋ ㅜ",
          "original_form": "그래서 무조건 운동화 신어요 ㅋㅋㅋ ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100043504.1.18",
          "form": "컨버스는 꿈도 못꿔요 ㅎ",
          "original_form": "컨버스는 꿈도 못꿔요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100043504.1.19",
          "form": "아이고 그럼 오래 서계시면 엄청 피로하겠어요",
          "original_form": "아이고 그럼 오래 서계시면 엄청 피로하겠어요",
          "speaker_id": "1",
          "time": "20210907 23:05"
