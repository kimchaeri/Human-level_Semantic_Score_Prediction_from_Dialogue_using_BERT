      "utterance": [
        {
          "id": "MDRW2100019316.1.1",
          "form": "카페 좋아하시나요 ㅎㅎ",
          "original_form": "카페 좋아하시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100019316.1.2",
          "form": "저 디저트 좋아해서 카페 자주 다니는데",
          "original_form": "저 디저트 좋아해서 카페 자주 다니는데",
          "speaker_id": "2",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019316.1.3",
          "form": "요즘 코로나라 안간지 좀 됐어요 ㅠㅠ",
          "original_form": "요즘 코로나라 안간지 좀 됐어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019316.1.4",
          "form": "저도 디저트 엄청 좋아해요 ㅎㅎ !",
          "original_form": "저도 디저트 엄청 좋아해요 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019316.1.5",
          "form": "어떤 디저트 젤 좋아하세요?",
          "original_form": "어떤 디저트 젤 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019316.1.6",
          "form": "타르트요!",
          "original_form": "타르트요!",
          "speaker_id": "2",
          "time": "20210827 17:52"
        },
        {
          "id": "MDRW2100019316.1.7",
          "form": "에그타르트도 좋아하고",
          "original_form": "에그타르트도 좋아하고",
          "speaker_id": "2",
          "time": "20210827 17:52"
        },
        {
          "id": "MDRW2100019316.1.8",
          "form": "아니면 케이크에 가까운 과일 타르트들도 좋아해요!",
          "original_form": "아니면 케이크에 가까운 과일 타르트들도 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 17:52"
        },
        {
          "id": "MDRW2100019316.1.9",
          "form": "헐 저도 에그타르트 넘 좋아해요 !",
          "original_form": "헐 저도 에그타르트 넘 좋아해요 !",
          "speaker_id": "1",
          "time": "20210827 17:54"
        },
        {
          "id": "MDRW2100019316.1.10",
          "form": "과일타르트도 좋아하는데 비싼편이라서 자주 못먹죠 ..ㅎㅎ",
          "original_form": "과일타르트도 좋아하는데 비싼편이라서 자주 못먹죠 ..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:54"
        },
        {
          "id": "MDRW2100019316.1.11",
          "form": "그쵸 ㅋㅋㅋㅋ 엄청 작은데 가격이 너무 무시무시해요 ㅠ",
          "original_form": "그쵸 ㅋㅋㅋㅋ 엄청 작은데 가격이 너무 무시무시해요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019316.1.12",
          "form": "어떤 과일 올라간거 젤 좋아하시나요 ㅎㅎ",
          "original_form": "어떤 과일 올라간거 젤 좋아하시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019316.1.13",
          "form": "전 딸기요 ㅋㅋㅋㅋ",
          "original_form": "전 딸기요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019316.1.14",
          "form": "무화과도 당도 높으면 좋아해요!",
          "original_form": "무화과도 당도 높으면 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019316.1.15",
          "form": "저도 딸기 젤 좋아해요 !",
          "original_form": "저도 딸기 젤 좋아해요 !",
          "speaker_id": "1",
          "time": "20210827 18:02"
        },
        {
          "id": "MDRW2100019316.1.16",
          "form": "무화과타르트는 한번도 안먹어봤네요 ㅎㅎ",
          "original_form": "무화과타르트는 한번도 안먹어봤네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:02"
        },
        {
          "id": "MDRW2100019316.1.17",
          "form": "7~8월에 철이라고 한창 나오더라구요",
          "original_form": "7~8월에 철이라고 한창 나오더라구요",
          "speaker_id": "2",
          "time": "20210827 18:05"
        },
        {
          "id": "MDRW2100019316.1.18",
          "form": "복숭아 타르트도 맛있는거같아요",
          "original_form": "복숭아 타르트도 맛있는거같아요",
          "speaker_id": "2",
          "time": "20210827 18:06"
        },
        {
          "id": "MDRW2100019316.1.19",
          "form": "근데 딸기 제외하고는 과일 당도가 랜덤인 경우가 많아서",
          "original_form": "근데 딸기 제외하고는 과일 당도가 랜덤인 경우가 많아서",
          "speaker_id": "2",
          "time": "20210827 18:06"
        },
        {
          "id": "MDRW2100019316.1.20",
          "form": "젤 무난한건 딸기인거같아요 ㅋㅋㅋ",
          "original_form": "젤 무난한건 딸기인거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:06"
        },
        {
          "id": "MDRW2100019316.1.21",
          "form": "아 맞아요 ㅋㅋ 딸기랑 청포도도 ? 은근 무난한 느낌이에요",
          "original_form": "아 맞아요 ㅋㅋ 딸기랑 청포도도 ? 은근 무난한 느낌이에요",
          "speaker_id": "1",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019316.1.22",
          "form": "전 자몽타르트는 안맞더라구요 ㅠ",
          "original_form": "전 자몽타르트는 안맞더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 18:11"
        },
        {
          "id": "MDRW2100019316.1.23",
          "form": "요즘 샤인머스캣이 유행이라",
          "original_form": "요즘 샤인머스캣이 유행이라",
          "speaker_id": "2",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019316.1.24",
          "form": "샤인머스캣 타르트 많이나와요",
          "original_form": "샤인머스캣 타르트 많이나와요",
          "speaker_id": "2",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019316.1.25",
          "form": "자몽은 약간 쓴맛땜에 호불호가 갈리더라구요",
          "original_form": "자몽은 약간 쓴맛땜에 호불호가 갈리더라구요",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019316.1.26",
          "form": "그럼 자몽에이드도 안드세요?",
          "original_form": "그럼 자몽에이드도 안드세요?",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019316.1.27",
          "form": "음 에이드는 좀 달달해서 누가 주면 먹기는 해요  !",
          "original_form": "음 에이드는 좀 달달해서 누가 주면 먹기는 해요  !",
          "speaker_id": "1",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019316.1.28",
          "form": "근데 제 돈 주고는 안먹어요 ㅋㅋㅋ",
          "original_form": "근데 제 돈 주고는 안먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019316.1.29",
          "form": "오 샤인머스켓도 맛있겠어요 !",
          "original_form": "오 샤인머스켓도 맛있겠어요 !",
          "speaker_id": "1",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019316.1.30",
          "form": "네 근데 유행하면서 재배농가가 많아졌다더니",
          "original_form": "네 근데 유행하면서 재배농가가 많아졌다더니",
          "speaker_id": "2",
          "time": "20210827 18:19"
        },
        {
          "id": "MDRW2100019316.1.31",
          "form": "막 처음 먹었을 때처럼 충격적으로 맛있는 느낌은 아니구",
          "original_form": "막 처음 먹었을 때처럼 충격적으로 맛있는 느낌은 아니구",
          "speaker_id": "2",
          "time": "20210827 18:20"
        },
        {
          "id": "MDRW2100019316.1.32",
          "form": "이젠 걍 좀 달달한 청포도?같아요",
          "original_form": "이젠 걍 좀 달달한 청포도?같아요",
          "speaker_id": "2",
          "time": "20210827 18:20"
        },
        {
          "id": "MDRW2100019316.1.33",
          "form": "아 어쩐지 ! 처음 나왔을때보다 덜 달아진거같았는데",
          "original_form": "아 어쩐지 ! 처음 나왔을때보다 덜 달아진거같았는데",
          "speaker_id": "1",
          "time": "20210827 18:21"
        },
        {
          "id": "MDRW2100019316.1.34",
          "form": "그런 이유때문이군요 ㅠ",
          "original_form": "그런 이유때문이군요 ㅠ",
