      "utterance": [
        {
          "id": "MDRW2100042435.1.1",
          "form": "오늘 옷은 어떤거 입으셨어요?",
          "original_form": "오늘 옷은 어떤거 입으셨어요?",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042435.1.2",
          "form": "긴팔입었어요",
          "original_form": "긴팔입었어요",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042435.1.3",
          "form": "좀 서늘하죠?",
          "original_form": "좀 서늘하죠?",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042435.1.4",
          "form": "네 아무래도 비가와서 그런것같네요",
          "original_form": "네 아무래도 비가와서 그런것같네요",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042435.1.5",
          "form": "내일도서늘할것같아요",
          "original_form": "내일도서늘할것같아요",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042435.1.6",
          "form": "내일은 오늘보다 더 서늘할것 같네요 ㅎㅎ",
          "original_form": "내일은 오늘보다 더 서늘할것 같네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042435.1.7",
          "form": "일기예보가 그래도",
          "original_form": "일기예보가 그래도",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042435.1.8",
          "form": "좀 맞았어요",
          "original_form": "좀 맞았어요",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042435.1.9",
          "form": "ㅎㅎㅎ 요근래 잘 안맞았죠 ?",
          "original_form": "ㅎㅎㅎ 요근래 잘 안맞았죠 ?",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042435.1.10",
          "form": "ㅋㅋㄱㄱ네 오늘도 사실",
          "original_form": "ㅋㅋㄱㄱ네 오늘도 사실",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042435.1.11",
          "form": "비안올줄알았어요ㅋㅋ",
          "original_form": "비안올줄알았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042435.1.12",
          "form": "우산은 있었나요?",
          "original_form": "우산은 있었나요?",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042435.1.13",
          "form": "네네 우산은 항상",
          "original_form": "네네 우산은 항상",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042435.1.14",
          "form": "들고다닙니다",
          "original_form": "들고다닙니다",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042435.1.15",
          "form": "항상 들고다니는것 좋죠 ㅎㅎ",
          "original_form": "항상 들고다니는것 좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042435.1.16",
