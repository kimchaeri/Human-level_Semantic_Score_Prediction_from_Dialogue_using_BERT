      "utterance": [
        {
          "id": "MDRW2100040336.1.1",
          "form": "아뇨아뇨ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨아뇨ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040336.1.2",
          "form": "덕분에 오늘도 맛난걸 알아갑니다ㅋ",
          "original_form": "덕분에 오늘도 맛난걸 알아갑니다ㅋ",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040336.1.3",
          "form": "세상에는 맛있는게 많아서 오늘도 아름답군요ㅋㅋㅋ",
          "original_form": "세상에는 맛있는게 많아서 오늘도 아름답군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100040336.1.4",
          "form": "ㅎㅈㅎ 세상은 넓고 먹을건 많죠!",
          "original_form": "ㅎㅈㅎ 세상은 넓고 먹을건 많죠!",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100040336.1.5",
          "form": "그럼 배라 세가지맛 담으라 그러면 뭐뭐 담나요",
          "original_form": "그럼 배라 세가지맛 담으라 그러면 뭐뭐 담나요",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100040336.1.6",
          "form": "저는 뉴욕치즈랑 피스타치오 두개를 기본으로 돌아가며 먹어요",
          "original_form": "저는 뉴욕치즈랑 피스타치오 두개를 기본으로 돌아가며 먹어요",
          "speaker_id": "2",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100040336.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100040336.1.8",
          "form": "최애는 피스타치오구요ㅎㅎ",
          "original_form": "최애는 피스타치오구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100040336.1.9",
          "form": "두개만 꽉꽉 담는군요ㅎ",
          "original_form": "두개만 꽉꽉 담는군요ㅎ",
          "speaker_id": "1",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100040336.1.10",
          "form": "저는 굳이굳이 개수 채운다고 고민했는데",
          "original_form": "저는 굳이굳이 개수 채운다고 고민했는데",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100040336.1.11",
          "form": "저도 아몬드봉봉이랑 민초로만 채워야겠어요!",
          "original_form": "저도 아몬드봉봉이랑 민초로만 채워야겠어요!",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100040336.1.12",
          "form": "아몬드봉봉도 맛있죠?ㅋㅋㅋㄲ",
          "original_form": "아몬드봉봉도 맛있죠?ㅋㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100040336.1.13",
          "form": "네 저 견과류 들어간거 좋아해요ㅎ",
          "original_form": "네 저 견과류 들어간거 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100040336.1.14",
          "form": "피스타치오 아몬드도 그래서 제 멤 중 하나예요",
          "original_form": "피스타치오 아몬드도 그래서 제 멤 중 하나예요",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100040336.1.15",
          "form": "글구 베리베리스트로베리와 뉴치케?",
          "original_form": "글구 베리베리스트로베리와 뉴치케?",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100040336.1.16",
          "form": "맞아요! 베라가 견과류가 제법맛나요ㅋㅋ",
          "original_form": "맞아요! 베라가 견과류가 제법맛나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100040336.1.17",
          "form": "스트로베리도 엄청 좋아하는데 요새는 잠깐 거리두고 있어요ㅋㅋㅋㅋ",
          "original_form": "스트로베리도 엄청 좋아하는데 요새는 잠깐 거리두고 있어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100040336.1.18",
          "form": "너무먹어서ㅋㅋㅋㅋ",
          "original_form": "너무먹어서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100040336.1.19",
          "form": "ㅎㅎ 원래 체리쥬빌레 먹었는데",
          "original_form": "ㅎㅎ 원래 체리쥬빌레 먹었는데",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100040336.1.20",
          "form": "분홍은 스트로베리로 갈아탔죠ㅎ",
          "original_form": "분홍은 스트로베리로 갈아탔죠ㅎ",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100040336.1.21",
          "form": "체리쥬빌레도 은근 식감이 괜찮죠?",
          "original_form": "체리쥬빌레도 은근 식감이 괜찮죠?",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100040336.1.22",
          "form": "나름 흰 분 파 조합이네요ㅎ",
          "original_form": "나름 흰 분 파 조합이네요ㅎ",
          "speaker_id": "1",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100040336.1.23",
          "form": "네네ㅎ 매력있어요!",
          "original_form": "네네ㅎ 매력있어요!",
