      "utterance": [
        {
          "id": "MDRW2100013846.1.1",
          "form": "안녕하세요 공연 보는거 좋아하시나요?",
          "original_form": "안녕하세요 공연 보는거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013846.1.2",
          "form": "네 근데 요즘엔 거의 못봤어요ㅠㅠ",
          "original_form": "네 근데 요즘엔 거의 못봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013846.1.3",
          "form": "보신거 있으세요?",
          "original_form": "보신거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013846.1.4",
          "form": "저도 요즘에는 못봤는데 ㅠㅠ 원래 콘서트 가는거 좋아해요 ㅎㅎ",
          "original_form": "저도 요즘에는 못봤는데 ㅠㅠ 원래 콘서트 가는거 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013846.1.5",
          "form": "어떤 공연 좋아하세요 ??",
          "original_form": "어떤 공연 좋아하세요 ??",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013846.1.6",
          "form": "저도 콘서트도 좋아하구 뮤지컬도 가끔가요ㅎㅎ",
          "original_form": "저도 콘서트도 좋아하구 뮤지컬도 가끔가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013846.1.7",
          "form": "뮤지컬은 보러간적이 없어서 한번 가보고 싶네용",
          "original_form": "뮤지컬은 보러간적이 없어서 한번 가보고 싶네용",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013846.1.8",
          "form": "콘서트랑 느낌이 많이 다르죠 ?",
          "original_form": "콘서트랑 느낌이 많이 다르죠 ?",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013846.1.9",
          "form": "한번 보시면 재밌어요ㅎㅎ네 근데 안맞는 분들도 있어서 ㅜㅜ",
          "original_form": "한번 보시면 재밌어요ㅎㅎ네 근데 안맞는 분들도 있어서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013846.1.10",
          "form": "맞아요 저는 연극은 안좋아해요 ㅠㅠ",
          "original_form": "맞아요 저는 연극은 안좋아해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013846.1.11",
          "form": "약간 넓은 곳에서 하는 공연을 좋아해서 ㅎㅎ",
          "original_form": "약간 넓은 곳에서 하는 공연을 좋아해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013846.1.12",
          "form": "저도...안맞더라구요ㅎㅎ",
          "original_form": "저도...안맞더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013846.1.13",
          "form": "콘서트도 못간지 2년정도 된거같아요",
          "original_form": "콘서트도 못간지 2년정도 된거같아요",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013846.1.14",
          "form": "언제 마지막으로 가셨어요?",
          "original_form": "언제 마지막으로 가셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013846.1.15",
          "form": "비슷하네요ㅠㅠ작년 초에 가구 못갔어요ㅠㅠ",
          "original_form": "비슷하네요ㅠㅠ작년 초에 가구 못갔어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013846.1.16",
          "form": "아이돌 콘서트 가셨나용 ?",
          "original_form": "아이돌 콘서트 가셨나용 ?",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013846.1.17",
          "form": "저는 아이돌 콘서트 갔어요 ㅎㅎ",
          "original_form": "저는 아이돌 콘서트 갔어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013846.1.18",
          "form": "아 동생따라서 한번 가본적있어요ㅎㅎ \n누구 가보셨어요?",
          "original_form": "아 동생따라서 한번 가본적있어요ㅎㅎ \n누구 가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013846.1.19",
          "form": "저는 예전부터 방탄 좋아해서 콘서트 가봤어요 ㅎㅎ",
          "original_form": "저는 예전부터 방탄 좋아해서 콘서트 가봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013846.1.20",
          "form": "오 표구하기 힘들다던데ㅎㅎ 대단하세요",
          "original_form": "오 표구하기 힘들다던데ㅎㅎ 대단하세요",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013846.1.21",
          "form": "어떤 가수 콘서트 가셨어요 ??",
          "original_form": "어떤 가수 콘서트 가셨어요 ??",
