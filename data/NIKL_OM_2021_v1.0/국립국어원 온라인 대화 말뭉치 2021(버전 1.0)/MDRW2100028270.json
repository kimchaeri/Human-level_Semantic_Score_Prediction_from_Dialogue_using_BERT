      "utterance": [
        {
          "id": "MDRW2100028270.1.1",
          "form": "얼음 녹아서",
          "original_form": "얼음 녹아서",
          "speaker_id": "1",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028270.1.2",
          "form": "밍밍해졌어요ㅋㅋ",
          "original_form": "밍밍해졌어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028270.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그릭요거트 먹었어요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그릭요거트 먹었어요?",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028270.1.4",
          "form": "네네!!!",
          "original_form": "네네!!!",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028270.1.5",
          "form": "탄수화물 모자랄까봐 삼각김밥도 먹었어요",
          "original_form": "탄수화물 모자랄까봐 삼각김밥도 먹었어요",
          "speaker_id": "1",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028270.1.6",
          "form": "씨유 뭔가 더 꾸덕해진것같아요",
          "original_form": "씨유 뭔가 더 꾸덕해진것같아요",
          "speaker_id": "1",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028270.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 그런가요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그런가요??",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028270.1.8",
          "form": "저도 나중에 먹어야겠어요 ! 사실 볼비먹고시픙네",
          "original_form": "저도 나중에 먹어야겠어요 ! 사실 볼비먹고시픙네",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028270.1.9",
          "form": "거긴 너무 머네요",
          "original_form": "거긴 너무 머네요",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028270.1.10",
          "form": "볼비도 맛있었던거같아요!!",
          "original_form": "볼비도 맛있었던거같아요!!",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028270.1.11",
          "form": "간식느낌!!",
          "original_form": "간식느낌!!",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028270.1.12",
          "form": "맞아요 한끼로는 너무적죠!",
          "original_form": "맞아요 한끼로는 너무적죠!",
          "speaker_id": "2",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100028270.1.13",
          "form": "시리얼을 겁나 타서 먹어도 되긴 할거같아요",
          "original_form": "시리얼을 겁나 타서 먹어도 되긴 할거같아요",
          "speaker_id": "2",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100028270.1.14",
          "form": "언니 저 제정신이 아닌가봐요",
          "original_form": "언니 저 제정신이 아닌가봐요",
          "speaker_id": "1",
          "time": "20210903 09:09"
        },
        {
          "id": "MDRW2100028270.1.15",
          "form": "고분자 실강인데",
          "original_form": "고분자 실강인데",
          "speaker_id": "1",
          "time": "20210903 09:09"
        },
        {
          "id": "MDRW2100028270.1.16",
          "form": "오늘 교수님 면담시간이랑 겹쳐욬ㅋㅋ",
          "original_form": "오늘 교수님 면담시간이랑 겹쳐욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:10"
        },
        {
          "id": "MDRW2100028270.1.17",
          "form": "오늘 점심 어떤거드세요?",
          "original_form": "오늘 점심 어떤거드세요?",
          "speaker_id": "1",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100028270.1.18",
          "form": "할! 어쩌다가ㅜㅜ",
          "original_form": "할! 어쩌다가ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100028270.1.19",
          "form": "오티는... 출첵안하지 않을까툐...",
          "original_form": "오티는... 출첵안하지 않을까툐...",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100028270.1.20",
          "form": "ㅜㅜ 오늘은 고구마랑... 씨유가서 뭐 사려고요!",
          "original_form": "ㅜㅜ 오늘은 고구마랑... 씨유가서 뭐 사려고요!",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100028270.1.21",
          "form": "면담끝났어요ㅋㅋ",
          "original_form": "면담끝났어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100028270.1.22",
          "form": "언니 점심 같이드실래요....?",
          "original_form": "언니 점심 같이드실래요....?",
          "speaker_id": "1",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100028270.1.23",
          "form": "엇 그럴래요...? 저 점심먹고 바로 가긴 하느데 ㅋㅋㅋ",
          "original_form": "엇 그럴래요...? 저 점심먹고 바로 가긴 하느데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100028270.1.24",
          "form": "네네 좋아요!!!",
          "original_form": "네네 좋아요!!!",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100028270.1.25",
          "form": "언제 드시려고요??",
          "original_form": "언제 드시려고요??",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100028270.1.26",
          "form": "몇시에 어디서 만날까요?",
          "original_form": "몇시에 어디서 만날까요?",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100028270.1.27",
          "form": "언니드실때욪!",
          "original_form": "언니드실때욪!",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100028270.1.28",
          "form": "저 종과 1층이에요",
          "original_form": "저 종과 1층이에요",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100028270.1.29",
          "form": "생협에서 사실거에요??",
          "original_form": "생협에서 사실거에요??",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100028270.1.30",
          "form": "뭐드실건가요ㅠㅠ",
          "original_form": "뭐드실건가요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100028270.1.31",
          "form": "언니가는곳에서 사먹쥬ㅋㅋㅋㅋ",
          "original_form": "언니가는곳에서 사먹쥬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100028270.1.32",
          "form": "저는 고구마가 있어서 음 씨유 앞에서 먹을까요?",
          "original_form": "저는 고구마가 있어서 음 씨유 앞에서 먹을까요?",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100028270.1.33",
          "form": "씨유에서 뭐 사거나 옆에 오봉 가서 사와도 되고요!!",
          "original_form": "씨유에서 뭐 사거나 옆에 오봉 가서 사와도 되고요!!",
          "speaker_id": "2",
          "time": "20210903 11:18"
        }
      ]
