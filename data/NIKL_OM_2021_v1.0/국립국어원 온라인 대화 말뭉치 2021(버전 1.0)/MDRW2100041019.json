          "speaker_id": "1",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100041019.1.5",
          "form": "백신맞았다고",
          "original_form": "백신맞았다고",
          "speaker_id": "1",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100041019.1.6",
          "form": "안한다고하면",
