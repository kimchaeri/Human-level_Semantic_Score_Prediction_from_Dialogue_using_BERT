      "utterance": [
        {
          "id": "MDRW2100013329.1.1",
          "form": "저희는 해도 운동장ㅠ\r\n헬스장가기도 뭔가 찝찝",
          "original_form": "저희는 해도 운동장ㅠ\r\n헬스장가기도 뭔가 찝찝",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013329.1.2",
          "form": "응 애기도 있으니까",
          "original_form": "응 애기도 있으니까",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013329.1.3",
          "form": "운동장이 낫겠어요",
          "original_form": "운동장이 낫겠어요",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013329.1.4",
          "form": "창문 닫아놓고 에어컨 트니까 더 찝찝한거같아요",
          "original_form": "창문 닫아놓고 에어컨 트니까 더 찝찝한거같아요",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013329.1.5",
          "form": "아ㅎㅎㅎ",
          "original_form": "아ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013329.1.6",
          "form": "회사에서는 에어컨틀고 창문 다 열어놔요ㅋㅋ",
          "original_form": "회사에서는 에어컨틀고 창문 다 열어놔요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013329.1.7",
          "form": "요즘 선선해지지 않았어요?",
          "original_form": "요즘 선선해지지 않았어요?",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013329.1.8",
          "form": "전기세 천만원씩 나온데요",
          "original_form": "전기세 천만원씩 나온데요",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013329.1.9",
          "form": "비오고 습해서",
          "original_form": "비오고 습해서",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013329.1.10",
          "form": "창문열면 습기가 제거가 안되잖아요",
          "original_form": "창문열면 습기가 제거가 안되잖아요",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013329.1.11",
          "form": "네맞아요ㅋㅋ 근데 여기는 보건소니까.. 어쩔수없죠",
          "original_form": "네맞아요ㅋㅋ 근데 여기는 보건소니까.. 어쩔수없죠",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013329.1.12",
          "form": "1층 주차장에서 선별검사도 하고",
          "original_form": "1층 주차장에서 선별검사도 하고",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013329.1.13",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013329.1.14",
          "form": "와 한달에 전기세가 천만워?",
          "original_form": "와 한달에 전기세가 천만워?",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013329.1.15",
          "form": "네 뭐 다른 층까지겠죵ㅋㅋ",
          "original_form": "네 뭐 다른 층까지겠죵ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013329.1.16",
          "form": "저희가3층이고 건물이 3개인데 어디까지 책정한건지는 모르겠어용",
          "original_form": "저희가3층이고 건물이 3개인데 어디까지 책정한건지는 모르겠어용",
          "speaker_id": "2",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013329.1.17",
          "form": "name3 이제 머리묶여요~~",
          "original_form": "&name3& 이제 머리묶여요~~",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100013329.1.18",
          "form": "묶으니까 더 예뻐졌어요",
          "original_form": "묶으니까 더 예뻐졌어요",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100013329.1.19",
          "form": "추석때 볼 수 있어요?",
          "original_form": "추석때 볼 수 있어요?",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100013329.1.20",
          "form": "추석때 가야죠",
          "original_form": "추석때 가야죠",
