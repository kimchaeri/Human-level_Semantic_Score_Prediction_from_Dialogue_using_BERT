        },
        {
          "id": "MDRW2100000910.1.2",
          "form": "6천원",
          "original_form": "6천원",
          "speaker_id": "1",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000910.1.3",
          "form": "오 횡재했네",
          "original_form": "오 횡재했네",
          "speaker_id": "2",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000910.1.4",
          "form": "근데 안가지고 왔어",
          "original_form": "근데 안가지고 왔어",
          "speaker_id": "1",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000910.1.5",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000910.1.6",
          "form": "애들이랑 같이 타고 있었잖아. 남의 것을 함부로 가져가면 안된다고 애들한테 얘기해줬어 ㅎㅎ",
          "original_form": "애들이랑 같이 타고 있었잖아. 남의 것을 함부로 가져가면 안된다고 애들한테 얘기해줬어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000910.1.7",
          "form": "ㅎㅎ 애들 없었으면 가져갔고?",
          "original_form": "ㅎㅎ 애들 없었으면 가져갔고?",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000910.1.8",
          "form": "아니 그래도 안가져가",
          "original_form": "아니 그래도 안가져가",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000910.1.9",
          "form": "오오 양심적이네",
          "original_form": "오오 양심적이네",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000910.1.10",
          "form": "그게 아니라 엘리베이터 cctv가 있으니까 ㅋㅋㅋ",
          "original_form": "그게 아니라 엘리베이터 cctv가 있으니까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000910.1.11",
          "form": "악 ㅋㅋㅋㅋ 대박",
          "original_form": "악 ㅋㅋㅋㅋ 대박",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000910.1.12",
          "form": "ㅎㅎ 애들한테 모범을 보여야지",
          "original_form": "ㅎㅎ 애들한테 모범을 보여야지",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000910.1.13",
          "form": "잘했어. 애들이 그런거 당연하다고 생각하면 안돼~",
          "original_form": "잘했어. 애들이 그런거 당연하다고 생각하면 안돼~",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000910.1.14",
          "form": "맞아. 교육이 별 거 있나... 나부터 모범을 보여야징",
          "original_form": "맞아. 교육이 별 거 있나... 나부터 모범을 보여야징",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000910.1.15",
          "form": "아주 멋진 엄마임",
          "original_form": "아주 멋진 엄마임",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000910.1.16",
          "form": "ㅎㅎ 말이라도 고마워",
          "original_form": "ㅎㅎ 말이라도 고마워",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000910.1.17",
          "form": "아마 요즘 엘리베이터 배달하는 사람들이 많아서 금방 가져갔을껴",
          "original_form": "아마 요즘 엘리베이터 배달하는 사람들이 많아서 금방 가져갔을껴",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000910.1.18",
          "form": "안그래도 조금있다 다시보니 사라졌더라고",
          "original_form": "안그래도 조금있다 다시보니 사라졌더라고",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000910.1.19",
          "form": "글치 색시처럼 양심적인 사람 별로 없어",
          "original_form": "글치 색시처럼 양심적인 사람 별로 없어",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000910.1.20",
          "form": "나도 그렇게 생각해 ㅋㅋ",
          "original_form": "나도 그렇게 생각해 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000910.1.21",
          "form": "ㅎㅎ 누군가 횡재했군",
          "original_form": "ㅎㅎ 누군가 횡재했군",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000910.1.22",
          "form": "6천원에 무슨 횡재",
          "original_form": "6천원에 무슨 횡재",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000910.1.23",
          "form": "누군가에겐 하루 일당 ㅋ",
          "original_form": "누군가에겐 하루 일당 ㅋ",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000910.1.24",
          "form": "그냥 내 꺼 아닌건 손 안대는게 상책",
