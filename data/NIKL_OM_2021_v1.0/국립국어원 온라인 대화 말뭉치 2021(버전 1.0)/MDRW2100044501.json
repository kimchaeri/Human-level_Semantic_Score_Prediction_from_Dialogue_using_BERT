      "utterance": [
        {
          "id": "MDRW2100044501.1.1",
          "form": "요즘 드라마는 안보시나여!?",
          "original_form": "요즘 드라마는 안보시나여!?",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044501.1.2",
          "form": "요즘에는 보는거 없고 담달에 방영예정인거 기다리고있어요!",
          "original_form": "요즘에는 보는거 없고 담달에 방영예정인거 기다리고있어요!",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044501.1.3",
          "form": "보시는거있나요?",
          "original_form": "보시는거있나요?",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044501.1.4",
          "form": "요즘 옛날걸 보고있어요ㅋㅋ",
          "original_form": "요즘 옛날걸 보고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044501.1.5",
          "form": "악의꽃 보셨나요?",
          "original_form": "악의꽃 보셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044501.1.6",
          "form": "안봤어요ㅋㅋㅋㅋ!!",
          "original_form": "안봤어요ㅋㅋㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044501.1.7",
          "form": "저두 이제 2화 보는중요ㅋㅋㅋ",
          "original_form": "저두 이제 2화 보는중요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044501.1.8",
          "form": "기다리시는 드라마는 뭐에요??",
          "original_form": "기다리시는 드라마는 뭐에요??",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044501.1.9",
          "form": "연모 요!",
          "original_form": "연모 요!",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044501.1.10",
          "form": "사극이예요",
          "original_form": "사극이예요",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044501.1.11",
          "form": "오..어떤 배우가 나오나요..?",
          "original_form": "오..어떤 배우가 나오나요..?",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044501.1.12",
          "form": "로운x박은빈 이요!",
          "original_form": "로운x박은빈 이요!",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044501.1.13",
          "form": "남주여주!",
          "original_form": "남주여주!",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044501.1.14",
          "form": "오 로운 잘생겼던데",
          "original_form": "오 로운 잘생겼던데",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044501.1.15",
          "form": "저도 관심이가네요",
          "original_form": "저도 관심이가네요",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044501.1.16",
          "form": "많은 관심 부탁드립니다",
          "original_form": "많은 관심 부탁드립니다",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044501.1.17",
          "form": "로운님 팬이신가보네요!!",
          "original_form": "로운님 팬이신가보네요!!",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044501.1.18",
          "form": "로운 진짜 잘생겼죠",
          "original_form": "로운 진짜 잘생겼죠",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044501.1.19",
          "form": "아니면 그 그룹..?",
          "original_form": "아니면 그 그룹..?",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044501.1.20",
          "form": "그 그룹 좋아하는데",
          "original_form": "그 그룹 좋아하는데",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044501.1.21",
          "form": "최애는 찬희예요ㅋㅋㄱㅋㅋ",
          "original_form": "최애는 찬희예요ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044501.1.22",
          "form": "혹시 아시나욥?",
          "original_form": "혹시 아시나욥?",
