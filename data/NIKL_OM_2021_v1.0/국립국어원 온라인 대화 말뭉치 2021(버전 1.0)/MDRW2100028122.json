      "utterance": [
        {
          "id": "MDRW2100028122.1.1",
          "form": "동호회 하시는거 있어요?",
          "original_form": "동호회 하시는거 있어요?",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028122.1.2",
          "form": "동호회 하는 거 없어요 ㅋㅋㅋㅋ",
          "original_form": "동호회 하는 거 없어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028122.1.3",
          "form": "좋은 동호회 있을까요?",
          "original_form": "좋은 동호회 있을까요?",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028122.1.4",
          "form": "음 좋아하시는게 어떤거 있어요?",
          "original_form": "음 좋아하시는게 어떤거 있어요?",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.5",
          "form": "예를들면",
          "original_form": "예를들면",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.6",
          "form": "운동?",
          "original_form": "운동?",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.7",
          "form": "음악?",
          "original_form": "음악?",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.8",
          "form": "연주?",
          "original_form": "연주?",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.9",
          "form": "저는.....요새는 여행같아요",
          "original_form": "저는.....요새는 여행같아요",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.10",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.11",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.12",
          "form": "운전을 한지 얼마안되었더니",
          "original_form": "운전을 한지 얼마안되었더니",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.13",
          "form": "여행 지깁니다",
          "original_form": "여행 지깁니다",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.14",
          "form": "아무데나 차끌고 가는게 재밌더라구요",
          "original_form": "아무데나 차끌고 가는게 재밌더라구요",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.15",
          "form": "여행 너무좋죠",
          "original_form": "여행 너무좋죠",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.16",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.17",
          "form": "name1은요??",
          "original_form": "&name1&은요??",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028122.1.18",
          "form": "취미가 어떻게 되세요?",
          "original_form": "취미가 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028122.1.19",
          "form": "저는 아들보기?",
          "original_form": "저는 아들보기?",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028122.1.20",
          "form": "아들하고 목욕하기",
          "original_form": "아들하고 목욕하기",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028122.1.21",
          "form": "아들하고 산책하기",
          "original_form": "아들하고 산책하기",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028122.1.22",
          "form": "아들 책읽어주기",
          "original_form": "아들 책읽어주기",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028122.1.23",
          "form": "뭐 이런거 합니다 ㅋㅋ",
          "original_form": "뭐 이런거 합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028122.1.24",
          "form": "저는 없어졌어요",
          "original_form": "저는 없어졌어요",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028122.1.25",
          "form": "아기 태어나고",
          "original_form": "아기 태어나고",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028122.1.26",
          "form": "마지막 여행지는",
          "original_form": "마지막 여행지는",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028122.1.27",
          "form": "스페인이었네요",
          "original_form": "스페인이었네요",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028122.1.28",
          "form": "올라~",
          "original_form": "올라~",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028122.1.29",
          "form": "와 아기 데리고 가신거예요???",
          "original_form": "와 아기 데리고 가신거예요???",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028122.1.30",
          "form": "대박 잘 다녀오셨네요!",
          "original_form": "대박 잘 다녀오셨네요!",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028122.1.31",
          "form": "코로나 전엔 애기들 태어나면 기념 여행 많이들 가더라구요 ㅋㅋㅋ",
          "original_form": "코로나 전엔 애기들 태어나면 기념 여행 많이들 가더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028122.1.32",
          "form": "아기 지금 15개월입니다 ㅋㅋ",
          "original_form": "아기 지금 15개월입니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028122.1.33",
          "form": "애기 낳기 전에 다녀왔어용",
          "original_form": "애기 낳기 전에 다녀왔어용",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028122.1.34",
          "form": "와 완전 애기네요",
          "original_form": "와 완전 애기네요",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028122.1.35",
          "form": "네 이제 아빠",
          "original_form": "네 이제 아빠",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028122.1.36",
          "form": "합니다 ㅋㅋㅋㅋㅋ",
          "original_form": "합니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028122.1.37",
          "form": "우와 그럼 진짜 코로나 직전이네요 ㅋㅋㅋ",
          "original_form": "우와 그럼 진짜 코로나 직전이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028122.1.38",
          "form": "애기 벌써 걸어요??",
          "original_form": "애기 벌써 걸어요??",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028122.1.39",
          "form": "네 뛰어다닙니다 ㅋㅋㅋ",
          "original_form": "네 뛰어다닙니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028122.1.40",
          "form": "장난끼도 많고",
          "original_form": "장난끼도 많고",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028122.1.41",
          "form": "그래도 너무 귀엽고",
          "original_form": "그래도 너무 귀엽고",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028122.1.42",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028122.1.43",
          "form": "님은요 계획이 어떻게되나요?",
          "original_form": "님은요 계획이 어떻게되나요?",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028122.1.44",
          "form": "저는 없어요 ㅋㅋㅋ큐ㅠㅠ",
          "original_form": "저는 없어요 ㅋㅋㅋ큐ㅠㅠ",
