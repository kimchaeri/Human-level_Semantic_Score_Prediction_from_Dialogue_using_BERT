      "utterance": [
        {
          "id": "MDRW2100043139.1.1",
          "form": "된찌?",
          "original_form": "된찌?",
          "speaker_id": "2",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043139.1.2",
          "form": "오오 겉절이는 안먹어?",
          "original_form": "오오 겉절이는 안먹어?",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100043139.1.3",
          "form": "멊어여ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "멊어여ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100043139.1.4",
          "form": "샘표에 보니 겉절이 양념 팔던데",
          "original_form": "샘표에 보니 겉절이 양념 팔던데",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100043139.1.5",
          "form": "배추사서 해보고싶어요",
          "original_form": "배추사서 해보고싶어요",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100043139.1.6",
          "form": "맛없어여 하지마세여",
          "original_form": "맛없어여 하지마세여",
          "speaker_id": "2",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100043139.1.7",
          "form": "헐 해봤어요?",
          "original_form": "헐 해봤어요?",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043139.1.8",
          "form": "네ㅠㅠ 그게 절이고 하는게 아니라서",
          "original_form": "네ㅠㅠ 그게 절이고 하는게 아니라서",
          "speaker_id": "2",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043139.1.9",
          "form": "양념하고 따로 놀아여",
          "original_form": "양념하고 따로 놀아여",
          "speaker_id": "2",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043139.1.10",
          "form": "아아... 그렇군요",
          "original_form": "아아... 그렇군요",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043139.1.11",
          "form": "안사길 잘했다",
          "original_form": "안사길 잘했다",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043139.1.12",
          "form": "양념도 맛 그닥이고. 흰 줄기부분... 생거에 양념얹먹는기분?",
          "original_form": "양념도 맛 그닥이고. 흰 줄기부분... 생거에 양념얹먹는기분?",
          "speaker_id": "2",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043139.1.13",
          "form": "연듀 에센스는 써보셨나요",
          "original_form": "연듀 에센스는 써보셨나요",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043139.1.14",
          "form": "생줄기 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "생줄기 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043139.1.15",
          "form": "연두는 예전에 써봤는데",
          "original_form": "연두는 예전에 써봤는데",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043139.1.16",
          "form": "양념은 맛있어요? 감칠맛 나요?",
          "original_form": "양념은 맛있어요? 감칠맛 나요?",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043139.1.17",
          "form": "요즘은 연두안쓰고 액젓써요",
          "original_form": "요즘은 연두안쓰고 액젓써요",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043139.1.18",
          "form": "저는 요리 초보라 못써요ㅠㅠ",
          "original_form": "저는 요리 초보라 못써요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043139.1.19",
          "form": "줄기는 먹을만 한데 저는 좀 자극적인 맛 좋아해서 그런지 .. 별로였어여",
          "original_form": "줄기는 먹을만 한데 저는 좀 자극적인 맛 좋아해서 그런지 .. 별로였어여",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043139.1.20",
          "form": "비릴것만 같아서ㅠㅠ",
          "original_form": "비릴것만 같아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:55"
        }
      ]
