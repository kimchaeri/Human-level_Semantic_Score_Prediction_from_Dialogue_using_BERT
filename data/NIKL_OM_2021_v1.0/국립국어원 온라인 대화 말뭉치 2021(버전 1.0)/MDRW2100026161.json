        },
        {
          "id": "MDRW2100026161.1.2",
          "form": "와 name2",
          "original_form": "와 &name2&",
          "speaker_id": "2",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100026161.1.3",
          "form": "건강하신가요",
          "original_form": "건강하신가요",
          "speaker_id": "2",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100026161.1.4",
          "form": "건강 못하지만 노력하고있습니다",
          "original_form": "건강 못하지만 노력하고있습니다",
          "speaker_id": "1",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100026161.1.5",
          "form": "규칙적인 식사를 해야하는데",
          "original_form": "규칙적인 식사를 해야하는데",
          "speaker_id": "1",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100026161.1.6",
          "form": "집에 오면 아무래도 피곤하니까",
          "original_form": "집에 오면 아무래도 피곤하니까",
          "speaker_id": "1",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100026161.1.7",
          "form": "그게 잘 안되는 것 같아",
          "original_form": "그게 잘 안되는 것 같아",
          "speaker_id": "1",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100026161.1.8",
          "form": "name1는 제시간에 식사했어?",
          "original_form": "&name1&는 제시간에 식사했어?",
          "speaker_id": "1",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100026161.1.9",
          "form": "꼬박꼬박 3끼 챙겨먹고?",
          "original_form": "꼬박꼬박 3끼 챙겨먹고?",
          "speaker_id": "1",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100026161.1.10",
          "form": "name1 오늘은 나름 제 시간에 밥 먹은 거 같어",
          "original_form": "&name1& 오늘은 나름 제 시간에 밥 먹은 거 같어",
          "speaker_id": "2",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100026161.1.11",
          "form": "3끼는 무리야 아침까지 챙기기는 너무... 귀찮아",
          "original_form": "3끼는 무리야 아침까지 챙기기는 너무... 귀찮아",
          "speaker_id": "2",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100026161.1.12",
          "form": "나 최근에 의사선생님 만났잖아 위염으로",
          "original_form": "나 최근에 의사선생님 만났잖아 위염으로",
          "speaker_id": "1",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100026161.1.13",
          "form": "규칙적으로 먹는게 중요하대",
          "original_form": "규칙적으로 먹는게 중요하대",
          "speaker_id": "1",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100026161.1.14",
          "form": "굳이... 3끼가 아니더라도...",
          "original_form": "굳이... 3끼가 아니더라도...",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026161.1.15",
          "form": "아닌가?",
          "original_form": "아닌가?",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026161.1.16",
          "form": "아 맞어 나 식도염 때문에 갔을 때도",
          "original_form": "아 맞어 나 식도염 때문에 갔을 때도",
          "speaker_id": "2",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026161.1.17",
          "form": "밥 제때 먹으라 했는 거 같어",
          "original_form": "밥 제때 먹으라 했는 거 같어",
          "speaker_id": "2",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026161.1.18",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026161.1.19",
          "form": "많이 먹지 말고 규칙적이게 먹고 자기 전에 먹지 말라고",
          "original_form": "많이 먹지 말고 규칙적이게 먹고 자기 전에 먹지 말라고",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026161.1.20",
          "form": "의사쌤이 말해주셨어",
          "original_form": "의사쌤이 말해주셨어",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026161.1.21",
          "form": "나는... 다 하고있는데",
          "original_form": "나는... 다 하고있는데",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026161.1.22",
          "form": "맞어 야식도 먹지 말라 하고",
          "original_form": "맞어 야식도 먹지 말라 하고",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026161.1.23",
          "form": "그치만... 그게 가경이 최선을 다해서 사는 방식인것을...",
          "original_form": "그치만... 그게 가경이 최선을 다해서 사는 방식인것을...",
          "speaker_id": "2",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026161.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그래!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그래!",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026161.1.25",
          "form": "그리고 야식은... 야식은 행복하잖아",
          "original_form": "그리고 야식은... 야식은 행복하잖아",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026161.1.26",
          "form": "먹으면",
          "original_form": "먹으면",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026161.1.27",
          "form": "건강에는 안좋지만",
          "original_form": "건강에는 안좋지만",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026161.1.28",
          "form": "몸 건강을 버리는 대신",
          "original_form": "몸 건강을 버리는 대신",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026161.1.29",
          "form": "정신건강 챙기기",
          "original_form": "정신건강 챙기기",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026161.1.30",
          "form": "나쁘지 않지",
          "original_form": "나쁘지 않지",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026161.1.31",
          "form": "그치만 대체로... 정신의 건강은 몸에서 오는듯",
          "original_form": "그치만 대체로... 정신의 건강은 몸에서 오는듯",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026161.1.32",
          "form": "건강한 신체에 건강한 영혼이 깃든다...",
          "original_form": "건강한 신체에 건강한 영혼이 깃든다...",
          "speaker_id": "2",
          "time": "20210901 20:25"
        }
      ]
