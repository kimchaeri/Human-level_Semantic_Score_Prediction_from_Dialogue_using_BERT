      "utterance": [
        {
          "id": "MDRW2100039620.1.1",
          "form": "고양이 한마리 키우시나요?",
          "original_form": "고양이 한마리 키우시나요?",
          "speaker_id": "1",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039620.1.2",
          "form": "저는 2마리키워요",
          "original_form": "저는 2마리키워요",
          "speaker_id": "2",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039620.1.3",
          "form": "암수 짝으로요?",
          "original_form": "암수 짝으로요?",
          "speaker_id": "1",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039620.1.4",
          "form": "형제냥이에요 ㅎ",
          "original_form": "형제냥이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100039620.1.5",
          "form": "성별은 고르신건가요? 일부러?",
          "original_form": "성별은 고르신건가요? 일부러?",
          "speaker_id": "1",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100039620.1.6",
          "form": "길냥이가 집안에 몰래 낳았는데  두마리 수컷을 낳아버렸네요",
          "original_form": "길냥이가 집안에 몰래 낳았는데  두마리 수컷을 낳아버렸네요",
          "speaker_id": "2",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039620.1.7",
          "form": "아!! 그렇게 기르기게 된거군요~",
          "original_form": "아!! 그렇게 기르기게 된거군요~",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039620.1.8",
          "form": "맞아요 님은 키우시는거 있나요",
          "original_form": "맞아요 님은 키우시는거 있나요",
          "speaker_id": "2",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039620.1.9",
          "form": "전 애 키우기도 벅차서요 ㅋ",
          "original_form": "전 애 키우기도 벅차서요 ㅋ",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039620.1.10",
          "form": "고양이가 그렇게 깔끔을 떤다면서요?",
          "original_form": "고양이가 그렇게 깔끔을 떤다면서요?",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039620.1.11",
          "form": "ㅎㅎㅎㄹ 고양이도 영원히 3살이라 애기같다는요",
          "original_form": "ㅎㅎㅎㄹ 고양이도 영원히 3살이라 애기같다는요",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039620.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039620.1.13",
          "form": "냄새도 안나요",
          "original_form": "냄새도 안나요",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039620.1.14",
          "form": "그래서 저도 키운다면 고양이를 키워야겠구나 싶어요 ㅋ",
          "original_form": "그래서 저도 키운다면 고양이를 키워야겠구나 싶어요 ㅋ",
          "speaker_id": "1",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039620.1.15",
          "form": "목욕시키는것도 일이거든요",
          "original_form": "목욕시키는것도 일이거든요",
          "speaker_id": "1",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039620.1.16",
          "form": "강쥐는 1주일도 안돼서 냄새나요",
          "original_form": "강쥐는 1주일도 안돼서 냄새나요",
          "speaker_id": "2",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039620.1.17",
          "form": "고양이는 1년이 지나도 안나요 ㅋㅋ",
          "original_form": "고양이는 1년이 지나도 안나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039620.1.18",
          "form": "첨에 살균 ? 성분이 있데요",
          "original_form": "첨에 살균 ? 성분이 있데요",
          "speaker_id": "2",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039620.1.19",
          "form": "침",
          "original_form": "침",
          "speaker_id": "2",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039620.1.20",
