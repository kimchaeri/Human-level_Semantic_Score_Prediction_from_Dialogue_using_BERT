      "utterance": [
        {
          "id": "MDRW2100012430.1.1",
          "form": "한옥좋아하신다길래",
          "original_form": "한옥좋아하신다길래",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012430.1.2",
          "form": "안동이랑 전주도 추천드릴려고했는데",
          "original_form": "안동이랑 전주도 추천드릴려고했는데",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012430.1.3",
          "form": "말레이시아 이야기 너무 재밌어서 이제야 말씀드리네요",
          "original_form": "말레이시아 이야기 너무 재밌어서 이제야 말씀드리네요",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012430.1.4",
          "form": "안동, 전주 이야기 궁금해요!",
          "original_form": "안동, 전주 이야기 궁금해요!",
          "speaker_id": "2",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012430.1.5",
          "form": "안동 가보셨나요?",
          "original_form": "안동 가보셨나요?",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012430.1.6",
          "form": "아뇨 들어보기만 했어요",
          "original_form": "아뇨 들어보기만 했어요",
          "speaker_id": "2",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012430.1.7",
          "form": "안동 하회마을이 한옥이 본격적이라고 해야할까요 전통이 잘보존되어있어서",
          "original_form": "안동 하회마을이 한옥이 본격적이라고 해야할까요 전통이 잘보존되어있어서",
          "speaker_id": "1",
          "time": "20210824 22:05"
        },
        {
          "id": "MDRW2100012430.1.8",
          "form": "한옥으로된 마을 구경하기에 좋아요",
          "original_form": "한옥으로된 마을 구경하기에 좋아요",
          "speaker_id": "1",
          "time": "20210824 22:05"
        },
        {
          "id": "MDRW2100012430.1.9",
          "form": "시내랑은 조금 떨어져있지만 버스도 잘다니구요",
          "original_form": "시내랑은 조금 떨어져있지만 버스도 잘다니구요",
          "speaker_id": "1",
          "time": "20210824 22:05"
        },
        {
          "id": "MDRW2100012430.1.10",
          "form": "한옥마을안에서도 숙박도 가능해요",
          "original_form": "한옥마을안에서도 숙박도 가능해요",
          "speaker_id": "1",
          "time": "20210824 22:05"
        },
        {
          "id": "MDRW2100012430.1.11",
          "form": "우와 하회마을 유네스코 지정이네요",
          "original_form": "우와 하회마을 유네스코 지정이네요",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012430.1.12",
          "form": "그 안에서 여행하기도 편하겠어요",
          "original_form": "그 안에서 여행하기도 편하겠어요",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012430.1.13",
          "form": "맞아요 안에서 미니카...? 같은거 하나빌려서 다니시면 편하실거예요",
          "original_form": "맞아요 안에서 미니카...? 같은거 하나빌려서 다니시면 편하실거예요",
          "speaker_id": "1",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012430.1.14",
          "form": "생각보다 넓거든요",
          "original_form": "생각보다 넓거든요",
          "speaker_id": "1",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012430.1.15",
          "form": "그렇게 넓나요?",
          "original_form": "그렇게 넓나요?",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012430.1.16",
          "form": "상상보다 크네요",
          "original_form": "상상보다 크네요",
          "speaker_id": "2",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012430.1.17",
          "form": "겨울에 가도 멋질것 같고 여름에 가도 좋을것 같아요",
          "original_form": "겨울에 가도 멋질것 같고 여름에 가도 좋을것 같아요",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012430.1.18",
          "form": "눈 쌓인 풍경도 좋아하거든요",
          "original_form": "눈 쌓인 풍경도 좋아하거든요",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012430.1.19",
          "form": "눈 쌓인풍경 좋죠 세상이 조용하니 차분한 분위기 저도 좋아해요",
          "original_form": "눈 쌓인풍경 좋죠 세상이 조용하니 차분한 분위기 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012430.1.20",
          "form": "걸어다니기에 다리아플수있는 넓이 예요",
          "original_form": "걸어다니기에 다리아플수있는 넓이 예요",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012430.1.21",
          "form": "만만하게 봤는데 넓군요",
          "original_form": "만만하게 봤는데 넓군요",
          "speaker_id": "2",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012430.1.22",
          "form": "잠이 잘 오겠어요",
          "original_form": "잠이 잘 오겠어요",
          "speaker_id": "2",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012430.1.23",
          "form": "눈 때문에 겨울 좋아하는데 아닌 사람들도 많더라구요ㅋㅋㅋ",
          "original_form": "눈 때문에 겨울 좋아하는데 아닌 사람들도 많더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012430.1.24",
          "form": "저는 부산출신이라 눈 좋아했는데",
          "original_form": "저는 부산출신이라 눈 좋아했는데",
          "speaker_id": "1",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012430.1.25",
          "form": "서울에서 일년있어보니 왜싫어하는지 대충은 알겠어요",
          "original_form": "서울에서 일년있어보니 왜싫어하는지 대충은 알겠어요",
          "speaker_id": "1",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012430.1.26",
          "form": "그래도 아직은 눈이좋아요 정확히는 눈온 풍경이요",
          "original_form": "그래도 아직은 눈이좋아요 정확히는 눈온 풍경이요",
          "speaker_id": "1",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012430.1.27",
          "form": "눈이 오면 세상이 좀 고요하고 아늑하게 느껴지더라구요",
          "original_form": "눈이 오면 세상이 좀 고요하고 아늑하게 느껴지더라구요",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012430.1.28",
          "form": "시간 지나면 질철질척거리지만요",
          "original_form": "시간 지나면 질철질척거리지만요",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012430.1.29",
          "form": "질척거리는데 어니까 미끌미끌해서 위험했어요",
          "original_form": "질척거리는데 어니까 미끌미끌해서 위험했어요",
          "speaker_id": "1",
          "time": "20210824 22:21"
        }
      ]
    }
