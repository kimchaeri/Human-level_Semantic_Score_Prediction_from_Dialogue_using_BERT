          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003480.1.11",
          "form": "으헝",
          "original_form": "으헝",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003480.1.12",
          "form": "나는 수시러라서",
          "original_form": "나는 수시러라서",
          "speaker_id": "2",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003480.1.13",
          "form": "넘 다르네..",
          "original_form": "넘 다르네..",
          "speaker_id": "2",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003480.1.14",
          "form": "그렇지...",
          "original_form": "그렇지...",
          "speaker_id": "1",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003480.1.15",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003480.1.16",
