        },
        {
          "id": "MDRW2100003480.1.3",
          "form": "학교 어떻게정했어?",
          "original_form": "학교 어떻게정했어?",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003480.1.4",
          "form": "나는..",
          "original_form": "나는..",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003480.1.5",
          "form": "성적 되는데...",
          "original_form": "성적 되는데...",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003480.1.6",
          "form": "그리고 논술..",
          "original_form": "그리고 논술..",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003480.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003480.1.8",
          "form": "언니야는 정시러였재",
          "original_form": "언니야는 정시러였재",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003480.1.9",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003480.1.10",
