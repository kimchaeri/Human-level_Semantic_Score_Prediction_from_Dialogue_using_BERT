      "utterance": [
        {
          "id": "MDRW2100000417.1.1",
          "form": "플래닛테러도 최애영화 중에 하나고요.",
          "original_form": "플래닛테러도 최애영화 중에 하나고요.",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000417.1.2",
          "form": "아 들어봤어요. 신작이던가요?",
          "original_form": "아 들어봤어요. 신작이던가요?",
          "speaker_id": "1",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000417.1.3",
          "form": "아니요! 아주아주 오래된 영화예요",
          "original_form": "아니요! 아주아주 오래된 영화예요",
          "speaker_id": "2",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000417.1.4",
          "form": "쿠엔틴타란티노 감독",
          "original_form": "쿠엔틴타란티노 감독",
          "speaker_id": "2",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000417.1.5",
          "form": "오 봉준호에게 칸에서 박수쳐줄 때 감동!!!",
          "original_form": "오 봉준호에게 칸에서 박수쳐줄 때 감동!!!",
          "speaker_id": "1",
          "time": "20210819 16:03"
        },
        {
          "id": "MDRW2100000417.1.6",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000417.1.7",
          "form": "아까 그 단편 영화는 뭐였어요?",
          "original_form": "아까 그 단편 영화는 뭐였어요?",
          "speaker_id": "2",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000417.1.8",
          "form": "기억을 더듬고 있답니다. 그 좀비영화는 아빠가 좀비가 되어가면서도 딸을 구하는 영화인데..",
          "original_form": "기억을 더듬고 있답니다. 그 좀비영화는 아빠가 좀비가 되어가면서도 딸을 구하는 영화인데..",
          "speaker_id": "1",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000417.1.9",
          "form": "혹시 카고?",
          "original_form": "혹시 카고?",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000417.1.10",
          "form": "마틴 프리먼 아저씨 주연?",
          "original_form": "마틴 프리먼 아저씨 주연?",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000417.1.11",
          "form": "맞아요!! 카고!!!",
          "original_form": "맞아요!! 카고!!!",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000417.1.12",
          "form": "역시 대단하세요ㅎㅎ",
          "original_form": "역시 대단하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000417.1.13",
          "form": "셜록에 왓슨 아저씨!",
          "original_form": "셜록에 왓슨 아저씨!",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000417.1.14",
          "form": "으엉 그것도 울면서 봤어요",
          "original_form": "으엉 그것도 울면서 봤어요",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000417.1.15",
          "form": "슬픈 좀비영화..",
          "original_form": "슬픈 좀비영화..",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000417.1.16",
          "form": "좀비영화의 무슨 단계가 있다던데",
          "original_form": "좀비영화의 무슨 단계가 있다던데",
          "speaker_id": "1",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000417.1.17",
          "form": "지네인간? 그런 것도 들어보셨어요?",
          "original_form": "지네인간? 그런 것도 들어보셨어요?",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000417.1.18",
          "form": "어우 그건 너무 징그러운 고어물이라서",
          "original_form": "어우 그건 너무 징그러운 고어물이라서",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000417.1.19",
          "form": "거기까지는 아니에요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "거기까지는 아니에요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000417.1.20",
          "form": "아 그쵸? 고어랑 좀비는 또 다른가봐요ㅎㅎ",
          "original_form": "아 그쵸? 고어랑 좀비는 또 다른가봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000417.1.21",
          "form": "네 으으 상상했어요",
          "original_form": "네 으으 상상했어요",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000417.1.22",
          "form": "그거 잠깐 틀었다가",
          "original_form": "그거 잠깐 틀었다가",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000417.1.23",
          "form": "얼른 꺼 버렸거든요",
          "original_form": "얼른 꺼 버렸거든요",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000417.1.24",
          "form": "에구 죄송ㅠㅠ",
          "original_form": "에구 죄송ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000417.1.25",
          "form": "아유 괜찮아요!",
          "original_form": "아유 괜찮아요!",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000417.1.26",
          "form": "저 최근에 극장에서 수어사이드스쿼드2도 봤어요",
          "original_form": "저 최근에 극장에서 수어사이드스쿼드2도 봤어요",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000417.1.27",
          "form": "유치하지만 재밌었어요",
          "original_form": "유치하지만 재밌었어요",
