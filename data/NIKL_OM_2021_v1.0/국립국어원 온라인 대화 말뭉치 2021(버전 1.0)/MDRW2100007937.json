      "utterance": [
        {
          "id": "MDRW2100007937.1.1",
          "form": "좋아하는 프랜차이즈 있으세요?",
          "original_form": "좋아하는 프랜차이즈 있으세요?",
          "speaker_id": "2",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007937.1.2",
          "form": "스벗 제일 좋아하죠ㅎㅎ 어디 좋아하세요?",
          "original_form": "스벗 제일 좋아하죠ㅎㅎ 어디 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007937.1.3",
          "form": "저도 스타벅스 좋아해요",
          "original_form": "저도 스타벅스 좋아해요",
          "speaker_id": "2",
          "time": "20210822 12:05"
        },
        {
          "id": "MDRW2100007937.1.4",
          "form": "좋아하시는 메뉴 있으세요?",
          "original_form": "좋아하시는 메뉴 있으세요?",
          "speaker_id": "2",
          "time": "20210822 12:05"
        },
        {
          "id": "MDRW2100007937.1.5",
          "form": "저는 자바칩프라푸치노ㅎㅎ 당줄여야하는데 ㅠㅠ",
          "original_form": "저는 자바칩프라푸치노ㅎㅎ 당줄여야하는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:05"
        },
        {
          "id": "MDRW2100007937.1.6",
          "form": "액상과당이 치명적이죠",
          "original_form": "액상과당이 치명적이죠",
          "speaker_id": "2",
          "time": "20210822 12:06"
        },
        {
          "id": "MDRW2100007937.1.7",
          "form": "근데 커피를 안마시면 하루가 너무 힘들어서ㅠㅠ",
          "original_form": "근데 커피를 안마시면 하루가 너무 힘들어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007937.1.8",
          "form": "커피 없이 살기에는 일이 많죠",
          "original_form": "커피 없이 살기에는 일이 많죠",
          "speaker_id": "2",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007937.1.9",
          "form": "저도 하루에 한잔은 마셔요",
          "original_form": "저도 하루에 한잔은 마셔요",
          "speaker_id": "2",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007937.1.10",
          "form": "아메리카노로 바꿔야하는데 또 그건 싫구ㅎㅎ",
          "original_form": "아메리카노로 바꿔야하는데 또 그건 싫구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:10"
        },
        {
          "id": "MDRW2100007937.1.11",
          "form": "저도 아메리카노는 못 마시겠어요",
          "original_form": "저도 아메리카노는 못 마시겠어요",
          "speaker_id": "2",
          "time": "20210822 12:10"
        },
        {
          "id": "MDRW2100007937.1.12",
          "form": "그 쓴걸 어떻게 마시지",
          "original_form": "그 쓴걸 어떻게 마시지",
          "speaker_id": "2",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007937.1.13",
          "form": "그니까요ㅠㅠ",
          "original_form": "그니까요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007937.1.14",
          "form": "시럽 안 넣고는 안돼요",
          "original_form": "시럽 안 넣고는 안돼요",
          "speaker_id": "2",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100007937.1.15",
          "form": "저도 그래요ㅎㅎ",
          "original_form": "저도 그래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:14"
        }
