      "utterance": [
        {
          "id": "MDRW2100011480.1.1",
          "form": "콜라보레이션 제품 구매한 적 있나요",
          "original_form": "콜라보레이션 제품 구매한 적 있나요",
          "speaker_id": "1",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100011480.1.2",
          "form": "아니요 ㅋㅋ",
          "original_form": "아니요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011480.1.3",
          "form": "술 관심 없어서요",
          "original_form": "술 관심 없어서요",
          "speaker_id": "2",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011480.1.4",
          "form": "구매한 적 있으세요?",
          "original_form": "구매한 적 있으세요?",
          "speaker_id": "2",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011480.1.5",
          "form": "술 말구",
          "original_form": "술 말구",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011480.1.6",
          "form": "곰표팝콘이나 화장품 콜라보같은거",
          "original_form": "곰표팝콘이나 화장품 콜라보같은거",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011480.1.7",
          "form": "구매해봤어요",
          "original_form": "구매해봤어요",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011480.1.8",
          "form": "최근에는 배라 꼬북칩도 사봤구요",
          "original_form": "최근에는 배라 꼬북칩도 사봤구요",
          "speaker_id": "1",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011480.1.9",
          "form": "우와 전 안해봤어요ㅋㅋㅋㅋㅋ",
          "original_form": "우와 전 안해봤어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011480.1.10",
          "form": "배라 꼬북칩 맛이에요??",
          "original_form": "배라 꼬북칩 맛이에요??",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011480.1.11",
          "form": "어떤가요??",
          "original_form": "어떤가요??",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011480.1.12",
          "form": "음.. 맛없는 맛..ㅋㅋㅋㅋ",
          "original_form": "음.. 맛없는 맛..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011480.1.13",
          "form": "시나몬 맛이 너무 강해서 호불호가 확 갈리겠던데 제 주변은 다 별로라그랬어요",
          "original_form": "시나몬 맛이 너무 강해서 호불호가 확 갈리겠던데 제 주변은 다 별로라그랬어요",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011480.1.14",
          "form": "ㅋㅋㅋㅋㅋ아 아이스크림이 그런맛이면",
          "original_form": "ㅋㅋㅋㅋㅋ아 아이스크림이 그런맛이면",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011480.1.15",
          "form": "좀 별로일 거 같긴해요",
          "original_form": "좀 별로일 거 같긴해요",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011480.1.16",
          "form": "곰표팝콘같은건",
          "original_form": "곰표팝콘같은건",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011480.1.17",
          "form": "그냥 팝콘맛 같았구",
          "original_form": "그냥 팝콘맛 같았구",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011480.1.18",
          "form": "약간 콜라보라는건 귀여운 맛에 사는거같아요",
          "original_form": "약간 콜라보라는건 귀여운 맛에 사는거같아요",
          "speaker_id": "1",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100011480.1.19",
          "form": "ㅋㅋㅋㅋ 아 맞아요",
          "original_form": "ㅋㅋㅋㅋ 아 맞아요",
          "speaker_id": "2",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100011480.1.20",
          "form": "저는 산건 아닌데 진로 두꺼비 조그만한 잔을",
          "original_form": "저는 산건 아닌데 진로 두꺼비 조그만한 잔을",
          "speaker_id": "2",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100011480.1.21",
          "form": "가족이 받아왔더라구요 아기자기 귀여워요",
          "original_form": "가족이 받아왔더라구요 아기자기 귀여워요",
          "speaker_id": "2",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100011480.1.22",
          "form": "두꺼비 그 파란색두꺼비요?",
          "original_form": "두꺼비 그 파란색두꺼비요?",
          "speaker_id": "1",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011480.1.23",
          "form": "네 귀엽게 그려져있어용",
          "original_form": "네 귀엽게 그려져있어용",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011480.1.24",
          "form": "아하 역시 콜라보는 캐릭터가 다 하는거 같죠",
          "original_form": "아하 역시 콜라보는 캐릭터가 다 하는거 같죠",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011480.1.25",
          "form": "그게 시선을 사로잡으니까",
          "original_form": "그게 시선을 사로잡으니까",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011480.1.26",
          "form": "구매로 이어지고",
          "original_form": "구매로 이어지고",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011480.1.27",
          "form": "맞아요 !! 그래서 카카오가 잘되기도 하잖아요",
          "original_form": "맞아요 !! 그래서 카카오가 잘되기도 하잖아요",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011480.1.28",
          "form": "그러네요!!",
          "original_form": "그러네요!!",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011480.1.29",
          "form": "역시 귀여운게 최고인거같아요",
          "original_form": "역시 귀여운게 최고인거같아요",
          "speaker_id": "1",
          "time": "20210824 13:49"
        }
      ]
    }
