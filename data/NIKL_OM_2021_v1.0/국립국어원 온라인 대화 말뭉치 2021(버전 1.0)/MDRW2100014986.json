      "utterance": [
        {
          "id": "MDRW2100014986.1.1",
          "form": "중고거래 즐겨하시나요?",
          "original_form": "중고거래 즐겨하시나요?",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014986.1.2",
          "form": "당근은 가끔하고 있어요ㅎㅎ 중고거래 하시나요?",
          "original_form": "당근은 가끔하고 있어요ㅎㅎ 중고거래 하시나요?",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014986.1.3",
          "form": "저도 당근 가끔 해요 ㅋㅋㅋㅋ",
          "original_form": "저도 당근 가끔 해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014986.1.4",
          "form": "은근 재밌더라구요",
          "original_form": "은근 재밌더라구요",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014986.1.5",
          "form": "맞아요ㅋㅋ제가 돈주고 산건데 꽁돈생긴거같구ㅎㅎ",
          "original_form": "맞아요ㅋㅋ제가 돈주고 산건데 꽁돈생긴거같구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014986.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014986.1.7",
          "form": "이득본기분ㅋㅋㅋㅋ",
          "original_form": "이득본기분ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014986.1.8",
          "form": "더 막써버려요ㅋㅋ과자사고...현금이라ㅋㅋㅋ",
          "original_form": "더 막써버려요ㅋㅋ과자사고...현금이라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014986.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 ㅋㅋㅋㅋㅋㅋ꽁돈생겻으니 얼른써야한다며 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 ㅋㅋㅋㅋㅋㅋ꽁돈생겻으니 얼른써야한다며 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014986.1.10",
          "form": "사실 꽁돈 아닌데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사실 꽁돈 아닌데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014986.1.11",
          "form": "제말이요ㅋㅋㅋ금방 사라져요ㅠㅠ",
          "original_form": "제말이요ㅋㅋㅋ금방 사라져요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014986.1.12",
          "form": "싸게 올렸는데 사람들이 연락없으면 서운하고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "싸게 올렸는데 사람들이 연락없으면 서운하고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014986.1.13",
          "form": "네 계속 보고있고ㅋㅋㅋ매너없는사람만나면 하루가 짜증나고 ㅠㅠㅋㅋ",
          "original_form": "네 계속 보고있고ㅋㅋㅋ매너없는사람만나면 하루가 짜증나고 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014986.1.14",
          "form": "아 맞아요 하루종일기분나빠요 ㅋㅋㅋㅋ",
          "original_form": "아 맞아요 하루종일기분나빠요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014986.1.15",
          "form": "팔리는줄 알고 설렜는데ㅠㅠ",
          "original_form": "팔리는줄 알고 설렜는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014986.1.16",
          "form": "맞아요 세상 허탈함 ㅠㅠ",
