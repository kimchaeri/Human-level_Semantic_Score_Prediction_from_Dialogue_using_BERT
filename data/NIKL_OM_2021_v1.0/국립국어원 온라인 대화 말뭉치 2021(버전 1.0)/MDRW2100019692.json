        {
          "id": "MDRW2100019692.1.2",
          "form": "ㅋㅋㅋㅋ다른 하고싶은 색은 없어?",
          "original_form": "ㅋㅋㅋㅋ다른 하고싶은 색은 없어?",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019692.1.3",
          "form": "갈색은 넘 많이 해서 별로야",
          "original_form": "갈색은 넘 많이 해서 별로야",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100019692.1.4",
          "form": "아니면 빨강 초록 파랑 이런 것도 있잖아",
          "original_form": "아니면 빨강 초록 파랑 이런 것도 있잖아",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100019692.1.5",
          "form": "그건 안 어울릴거 같아서",
          "original_form": "그건 안 어울릴거 같아서",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019692.1.6",
          "form": "그럴 수 있지",
          "original_form": "그럴 수 있지",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019692.1.7",
          "form": "그니까 은색이 젤 나은듯",
          "original_form": "그니까 은색이 젤 나은듯",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019692.1.8",
          "form": "ㅋㅋㅋㅋ은색이 젤 튀는디",
          "original_form": "ㅋㅋㅋㅋ은색이 젤 튀는디",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019692.1.9",
          "form": "근데 빨강 파랑은 진심 안울릴거 같아서",
          "original_form": "근데 빨강 파랑은 진심 안울릴거 같아서",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019692.1.10",
          "form": "어울",
          "original_form": "어울",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019692.1.11",
          "form": "은색은 어울릴까?",
          "original_form": "은색은 어울릴까?",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019692.1.12",
          "form": "그나마 나은거 같은데",
          "original_form": "그나마 나은거 같은데",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019692.1.13",
          "form": "엄청 튀는 색인데 신기하네",
          "original_form": "엄청 튀는 색인데 신기하네",
          "speaker_id": "1",
