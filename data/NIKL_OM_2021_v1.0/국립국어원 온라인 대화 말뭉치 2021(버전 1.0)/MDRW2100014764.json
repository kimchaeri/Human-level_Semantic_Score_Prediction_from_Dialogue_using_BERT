      "utterance": [
        {
          "id": "MDRW2100014764.1.1",
          "form": "형제는 있으신가요??",
          "original_form": "형제는 있으신가요??",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014764.1.2",
          "form": "네 오빠 한놈 있어요!!",
          "original_form": "네 오빠 한놈 있어요!!",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014764.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014764.1.4",
          "form": "저는 언니랑 여동생있어요!",
          "original_form": "저는 언니랑 여동생있어요!",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014764.1.5",
          "form": "3자매랍니당",
          "original_form": "3자매랍니당",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014764.1.6",
          "form": "헐!!!! 가장 부러운 구성(?) 이에요",
          "original_form": "헐!!!! 가장 부러운 구성(?) 이에요",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014764.1.7",
          "form": "그런말 많이 들으시죠",
          "original_form": "그런말 많이 들으시죠",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014764.1.8",
          "form": "헉 자매여서 부럽다고는 많이 하시지만",
          "original_form": "헉 자매여서 부럽다고는 많이 하시지만",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014764.1.9",
          "form": "중간에 껴서 힘들겟단 소리도 많이 들어요 ㅎㅎ",
          "original_form": "중간에 껴서 힘들겟단 소리도 많이 들어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014764.1.10",
          "form": "앗 둘째셔서 그렇겠다",
          "original_form": "앗 둘째셔서 그렇겠다",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014764.1.11",
          "form": "혹시 덕선이 에피 보면서 우셨나요?",
          "original_form": "혹시 덕선이 에피 보면서 우셨나요?",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014764.1.12",
          "form": "아뇨 덕선이는 덕선이고",
          "original_form": "아뇨 덕선이는 덕선이고",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014764.1.13",
          "form": "저는 저거든요",
          "original_form": "저는 저거든요",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014764.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ제가 드라마보면서 공감을 잘 못해여!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ제가 드라마보면서 공감을 잘 못해여!",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014764.1.15",
