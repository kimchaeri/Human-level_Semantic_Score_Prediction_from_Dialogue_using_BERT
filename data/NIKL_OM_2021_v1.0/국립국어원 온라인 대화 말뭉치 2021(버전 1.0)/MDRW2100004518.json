      "utterance": [
        {
          "id": "MDRW2100004518.1.1",
          "form": "영화나 한편 볼까",
          "original_form": "영화나 한편 볼까",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004518.1.2",
          "form": "난 조승우 나온 후아유 볼라고 준비해둠ㅋㅋ",
          "original_form": "난 조승우 나온 후아유 볼라고 준비해둠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004518.1.3",
          "form": "나 그거 한국 영화중에 손 꼽히게 좋아함",
          "original_form": "나 그거 한국 영화중에 손 꼽히게 좋아함",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004518.1.4",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004518.1.5",
          "form": "너는 무슨영화 볼꺼니",
          "original_form": "너는 무슨영화 볼꺼니",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004518.1.6",
          "form": "뭐볼까나 뭔가 우중충하니 우중충한거 보고싶기도 하고 아예 밝은거 보고싶기도하고",
          "original_form": "뭐볼까나 뭔가 우중충하니 우중충한거 보고싶기도 하고 아예 밝은거 보고싶기도하고",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004518.1.7",
          "form": "우중충한데 우중충한거 보면 우울증 온다ㅋㅋㅋ",
          "original_form": "우중충한데 우중충한거 보면 우울증 온다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004518.1.8",
          "form": "그래봤자 분위기 우중충이다 중경삼림 정도",
          "original_form": "그래봤자 분위기 우중충이다 중경삼림 정도",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004518.1.9",
          "form": "내가 전에 추천해준 쇼퍼홀릭 봤니",
          "original_form": "내가 전에 추천해준 쇼퍼홀릭 봤니",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004518.1.10",
          "form": "안본듯 ㅋㅋ",
          "original_form": "안본듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004518.1.11",
          "form": "그거 귀엽고 잼난디ㅋㅋ 약간 프로포즈데이 비슷한 부류?",
          "original_form": "그거 귀엽고 잼난디ㅋㅋ 약간 프로포즈데이 비슷한 부류?",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004518.1.12",
          "form": "근데 아주 예전에 본것 같기도 하고 ㅋㅋ",
          "original_form": "근데 아주 예전에 본것 같기도 하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004518.1.13",
          "form": "뭐여ㅋㅋㅋㅋ",
          "original_form": "뭐여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004518.1.14",
          "form": "뭔가 익숙한 느낌이 있어",
          "original_form": "뭔가 익숙한 느낌이 있어",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004518.1.15",
          "form": "아 병맛 영화로 내이야기 라는 일본영화도 괜츈ㅋㅋㅋㅋㅋ",
          "original_form": "아 병맛 영화로 내이야기 라는 일본영화도 괜츈ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004518.1.16",
          "form": "병맛 영화는 잘 안봄 ㅋㅋ",
          "original_form": "병맛 영화는 잘 안봄 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004518.1.17",
          "form": "병맛이지만 유쾌해서 잼있게 봤다ㅋㅋ",
          "original_form": "병맛이지만 유쾌해서 잼있게 봤다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004518.1.18",
          "form": "영화관 가서 보는게 그립네",
          "original_form": "영화관 가서 보는게 그립네",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004518.1.19",
          "form": "올해 부국제 하나보더라",
          "original_form": "올해 부국제 하나보더라",
          "speaker_id": "1",
          "time": "20210821 17:06"
        }
