      "utterance": [
        {
          "id": "MDRW2100018079.1.1",
          "form": "강아지랑 고양이 어느쪽이세요?",
          "original_form": "강아지랑 고양이 어느쪽이세요?",
          "speaker_id": "1",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018079.1.2",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018079.1.3",
          "form": "근데 고양이도 좋아해요",
          "original_form": "근데 고양이도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018079.1.4",
          "form": "ㅋㅋㅋ 키워보신적은 엄서여ㅡ",
          "original_form": "ㅋㅋㅋ 키워보신적은 엄서여ㅡ",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018079.1.5",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018079.1.6",
          "form": "네 둘다 키워본적은 없어요",
          "original_form": "네 둘다 키워본적은 없어요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018079.1.7",
          "form": "옆에 토끼가 누워있어요",
          "original_form": "옆에 토끼가 누워있어요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018079.1.8",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018079.1.9",
          "form": "토깽이... 집토끼요?",
          "original_form": "토깽이... 집토끼요?",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018079.1.10",
          "form": "품종묘?",
          "original_form": "품종묘?",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018079.1.11",
          "form": "넹 라이언헤드예요",
          "original_form": "넹 라이언헤드예요",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018079.1.12",
          "form": "요즘 털날림이 솜사탕이예요",
          "original_form": "요즘 털날림이 솜사탕이예요",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018079.1.13",
          "form": "옹.. 옹.. 라이언헤드 엄청 뽀송이..",
          "original_form": "옹.. 옹.. 라이언헤드 엄청 뽀송이..",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018079.1.14",
          "form": "벌써 털갈이해요?",
          "original_form": "벌써 털갈이해요?",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018079.1.15",
          "form": "날씨 변화 귀신같이 알아서 엄청나요",
          "original_form": "날씨 변화 귀신같이 알아서 엄청나요",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018079.1.16",
          "form": "토끼치고는 귀가 작아요",
          "original_form": "토끼치고는 귀가 작아요",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018079.1.17",
          "form": "근데 쪼꼬미인데도 털이 많이날려여?",
          "original_form": "근데 쪼꼬미인데도 털이 많이날려여?",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018079.1.18",
          "form": "고양이 정도인가요?",
          "original_form": "고양이 정도인가요?",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018079.1.19",
          "form": "털뭉치가 날리니까요 고양이랑 비슷해요",
          "original_form": "털뭉치가 날리니까요 고양이랑 비슷해요",
          "speaker_id": "2",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018079.1.20",
          "form": "성토여서 강아지만해요",
          "original_form": "성토여서 강아지만해요",
          "speaker_id": "2",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018079.1.21",
          "form": "아ㅡ. 토끼 털날리는걸 이제.알았네..\n배변은요?",
          "original_form": "아ㅡ. 토끼 털날리는걸 이제.알았네..\n배변은요?",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018079.1.22",
          "form": "토끼화장실에시 볼일봐요",
          "original_form": "토끼화장실에시 볼일봐요",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018079.1.23",
          "form": "간혹 쿠션이나 이불에다 쉬야해서 혼나지만요",
          "original_form": "간혹 쿠션이나 이불에다 쉬야해서 혼나지만요",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018079.1.24",
          "form": "ㅋㅋㅋ 토끼는 걸어다니면서 초코볼 생성해서..",
          "original_form": "ㅋㅋㅋ 토끼는 걸어다니면서 초코볼 생성해서..",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018079.1.25",
          "form": "애기때만 그래요",
          "original_form": "애기때만 그래요",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018079.1.26",
          "form": "아... 배변 교육 따로 시키는 거에영?",
          "original_form": "아... 배변 교육 따로 시키는 거에영?",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018079.1.27",
          "form": "조기교육이 중요하죠",
          "original_form": "조기교육이 중요하죠",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018079.1.28",
          "form": "전 실패한 케이스지만요",
          "original_form": "전 실패한 케이스지만요",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018079.1.29",
          "form": "그래도 토깽이 사랑하시죠? ㅋㅋㅋㅋㅋ",
          "original_form": "그래도 토깽이 사랑하시죠? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018079.1.30",
          "form": "그럼요 울아가",
          "original_form": "그럼요 울아가",
          "speaker_id": "2",
          "time": "20210826 22:47"
        }
      ]
