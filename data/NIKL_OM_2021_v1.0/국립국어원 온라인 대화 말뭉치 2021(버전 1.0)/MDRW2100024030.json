      "utterance": [
        {
          "id": "MDRW2100024030.1.1",
          "form": "종교는 있으세요?",
          "original_form": "종교는 있으세요?",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024030.1.2",
          "form": "전 없어요ㅜ",
          "original_form": "전 없어요ㅜ",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024030.1.3",
          "form": "저도 없어요",
          "original_form": "저도 없어요",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100024030.1.4",
          "form": "절에서 하는 템플 스테이는 해보고싶어요",
          "original_form": "절에서 하는 템플 스테이는 해보고싶어요",
          "speaker_id": "2",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100024030.1.5",
          "form": "맞아요\r\n다른건 잘 모르겠는데",
          "original_form": "맞아요\r\n다른건 잘 모르겠는데",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100024030.1.6",
          "form": "템플스테이는 좀 도움이 될것 같더라구요",
          "original_form": "템플스테이는 좀 도움이 될것 같더라구요",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100024030.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024030.1.8",
          "form": "막상하면 지루해서 탈출하고 싶을것 같긴하지만",
          "original_form": "막상하면 지루해서 탈출하고 싶을것 같긴하지만",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024030.1.9",
          "form": "좋은 경험일것같아요",
          "original_form": "좋은 경험일것같아요",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024030.1.10",
          "form": "맞아요 그리고 혼자서 생각하는 종교라보니 스스로에 대해 더 고찰할 수 있을거 같구요",
          "original_form": "맞아요 그리고 혼자서 생각하는 종교라보니 스스로에 대해 더 고찰할 수 있을거 같구요",
          "speaker_id": "1",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024030.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100024030.1.12",
          "form": "인생을 되돌아보게되는",
          "original_form": "인생을 되돌아보게되는",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100024030.1.13",
          "form": "하루종일 나에 대해 생갇하는것도",
          "original_form": "하루종일 나에 대해 생갇하는것도",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100024030.1.14",
          "form": "좋은 고찰이죠",
          "original_form": "좋은 고찰이죠",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100024030.1.15",
          "form": "종교는 사실 별 생각없었는데",
          "original_form": "종교는 사실 별 생각없었는데",
          "speaker_id": "1",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100024030.1.16",
          "form": "절에 가는 길은 너무 예쁘더라구요",
          "original_form": "절에 가는 길은 너무 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100024030.1.17",
          "form": "산 속에 있어서 그런가봐요ㅎ",
          "original_form": "산 속에 있어서 그런가봐요ㅎ",
          "speaker_id": "1",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100024030.1.18",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024030.1.19",
          "form": "산이란 산엔 절이 있어서",
          "original_form": "산이란 산엔 절이 있어서",
          "speaker_id": "2",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024030.1.20",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024030.1.21",
          "form": "공기도 맑고 진짜 생각하긴 좋아요",
          "original_form": "공기도 맑고 진짜 생각하긴 좋아요",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024030.1.22",
          "form": "그쵸?ㅎㅎㅎ",
          "original_form": "그쵸?ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024030.1.23",
          "form": "경치가 너무 좋아서ㅎㅎ",
          "original_form": "경치가 너무 좋아서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024030.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024030.1.25",
          "form": "개인작으로 산을 좋아해서 한번 가보고싶어요",
          "original_form": "개인작으로 산을 좋아해서 한번 가보고싶어요",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024030.1.26",
          "form": "가을 단풍이랑 은행잎 어울어지면 더 힐링이죠",
          "original_form": "가을 단풍이랑 은행잎 어울어지면 더 힐링이죠",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024030.1.27",
          "form": "등산 좋아하시나요?",
          "original_form": "등산 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024030.1.28",
          "form": "크 멋지죠",
          "original_form": "크 멋지죠",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024030.1.29",
          "form": "네 물론 잘 하진못하지만",
          "original_form": "네 물론 잘 하진못하지만",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024030.1.30",
          "form": "한번 올라가면 기분 좋더라구요",
          "original_form": "한번 올라가면 기분 좋더라구요",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024030.1.31",
          "form": "근데 한번밖에 안가봤어요ㅋㅋㅋ",
          "original_form": "근데 한번밖에 안가봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024030.1.32",
          "form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024030.1.33",
          "form": "반전이네요ㅋㅋㅋㅋㅋ",
          "original_form": "반전이네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:48"
        }
