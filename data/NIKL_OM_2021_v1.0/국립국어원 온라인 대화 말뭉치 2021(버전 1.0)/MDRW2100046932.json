      "utterance": [
        {
          "id": "MDRW2100046932.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20211109 00:00"
        },
        {
          "id": "MDRW2100046932.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211109 00:51"
        },
        {
          "id": "MDRW2100046932.1.3",
          "form": "최근 즐겨하시는 취미가 있으신가요?",
          "original_form": "최근 즐겨하시는 취미가 있으신가요?",
          "speaker_id": "2",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.4",
          "form": "보석십자수라고 들어보셨나요",
          "original_form": "보석십자수라고 들어보셨나요",
          "speaker_id": "1",
          "time": "20211109 00:50"
        },
        {
          "id": "MDRW2100046932.1.5",
          "form": "엇 아니요?",
          "original_form": "엇 아니요?",
          "speaker_id": "2",
          "time": "20211109 00:51"
        },
        {
          "id": "MDRW2100046932.1.6",
          "form": "처음 들어봐요!",
          "original_form": "처음 들어봐요!",
          "speaker_id": "2",
          "time": "20211109 00:51"
        },
        {
          "id": "MDRW2100046932.1.7",
          "form": "제주변친구들이 많이 하더라구요",
          "original_form": "제주변친구들이 많이 하더라구요",
          "speaker_id": "1",
          "time": "20211109 00:51"
        },
        {
          "id": "MDRW2100046932.1.8",
          "form": "님도 하시나요?",
          "original_form": "님도 하시나요?",
          "speaker_id": "2",
          "time": "20211109 00:51"
        },
        {
          "id": "MDRW2100046932.1.9",
          "form": "저도 처음에 잠시했다가",
          "original_form": "저도 처음에 잠시했다가",
          "speaker_id": "1",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.10",
          "form": "힘들어서",
          "original_form": "힘들어서",
          "speaker_id": "1",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.11",
          "form": "서랍안에 고히",
          "original_form": "서랍안에 고히",
          "speaker_id": "1",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.12",
          "form": "봉인해뒀어요",
          "original_form": "봉인해뒀어요",
          "speaker_id": "1",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.15",
          "form": "뭐지 한 번 검색해보고",
          "original_form": "뭐지 한 번 검색해보고",
          "speaker_id": "2",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.16",
          "form": "올게요 ㅎㅎㅎ",
          "original_form": "올게요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.17",
          "form": "ㅇ..",
          "original_form": "ㅇ..",
          "speaker_id": "2",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.18",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.19",
          "form": "",
          "original_form": "{share:info}",
          "speaker_id": "1",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.20",
          "form": "엄청..",
          "original_form": "엄청..",
          "speaker_id": "2",
          "time": "20211109 00:52"
        },
        {
          "id": "MDRW2100046932.1.21",
          "form": "어려워보이는데요...?!",
          "original_form": "어려워보이는데요...?!",
          "speaker_id": "2",
          "time": "20211109 00:53"
        },
        {
          "id": "MDRW2100046932.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211109 00:53"
        },
        {
          "id": "MDRW2100046932.1.23",
          "form": "엄청난 인내심과 시간을 요구하는",
          "original_form": "엄청난 인내심과 시간을 요구하는",
          "speaker_id": "1",
          "time": "20211109 00:53"
        },
        {
          "id": "MDRW2100046932.1.24",
          "form": "와 근데",
          "original_form": "와 근데",
          "speaker_id": "2",
          "time": "20211109 00:54"
        },
        {
          "id": "MDRW2100046932.1.25",
          "form": "하나",
          "original_form": "하나",
          "speaker_id": "2",
          "time": "20211109 00:54"
        },
        {
          "id": "MDRW2100046932.1.26",
          "form": "완성하면",
          "original_form": "완성하면",
          "speaker_id": "2",
          "time": "20211109 00:54"
        },
        {
          "id": "MDRW2100046932.1.27",
          "form": "성취감이",
          "original_form": "성취감이",
          "speaker_id": "2",
          "time": "20211109 00:54"
        },
        {
          "id": "MDRW2100046932.1.28",
          "form": "장난 아닐 거 같아요!!",
          "original_form": "장난 아닐 거 같아요!!",
          "speaker_id": "2",
          "time": "20211109 00:54"
        },
        {
          "id": "MDRW2100046932.1.29",
          "form": "네 이쁘게 액자에 장식해두면 이쁠꺼같아요",
          "original_form": "네 이쁘게 액자에 장식해두면 이쁠꺼같아요",
          "speaker_id": "1",
          "time": "20211109 00:54"
        },
        {
          "id": "MDRW2100046932.1.30",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "2",
          "time": "20211109 00:57"
        },
        {
          "id": "MDRW2100046932.1.31",
          "form": "보석십자수는",
          "original_form": "보석십자수는",
          "speaker_id": "2",
          "time": "20211109 00:57"
        },
        {
          "id": "MDRW2100046932.1.32",
          "form": "비용이 많이드나요?",
          "original_form": "비용이 많이드나요?",
          "speaker_id": "2",
          "time": "20211109 00:57"
        },
        {
          "id": "MDRW2100046932.1.33",
          "form": "아니요 제품따라 틀린데",
          "original_form": "아니요 제품따라 틀린데",
          "speaker_id": "1",
          "time": "20211108 13:19"
        },
        {
          "id": "MDRW2100046932.1.34",
          "form": "싼건 2~3만원이면 살수있어요",
          "original_form": "싼건 2~3만원이면 살수있어요",
          "speaker_id": "1",
          "time": "20211108 13:19"
        },
        {
          "id": "MDRW2100046932.1.35",
          "form": "오호 좀 알아보고 하나 사봐야겠어요",
          "original_form": "오호 좀 알아보고 하나 사봐야겠어요",
          "speaker_id": "2",
          "time": "20211108 13:23"
        },
        {
          "id": "MDRW2100046932.1.36",
          "form": "사두면 언젠간 하지 않을까..",
          "original_form": "사두면 언젠간 하지 않을까..",
          "speaker_id": "2",
          "time": "20211108 13:23"
        },
        {
          "id": "MDRW2100046932.1.37",
          "form": "싶은 마음 ㅎㅎ",
          "original_form": "싶은 마음 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 13:23"
        },
        {
          "id": "MDRW2100046932.1.38",
          "form": "언젠가 ...",
          "original_form": "언젠가 ...",
          "speaker_id": "1",
          "time": "20211108 13:23"
        },
        {
          "id": "MDRW2100046932.1.39",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20211108 13:23"
        },
        {
          "id": "MDRW2100046932.1.40",
          "form": "님 덕에 새로운 걸 알아가네요!",
          "original_form": "님 덕에 새로운 걸 알아가네요!",
          "speaker_id": "2",
          "time": "20211108 13:23"
        },
        {
          "id": "MDRW2100046932.1.41",
          "form": "저도 그랬죠",
          "original_form": "저도 그랬죠",
          "speaker_id": "1",
          "time": "20211108 13:23"
        },
        {
          "id": "MDRW2100046932.1.42",
          "form": "하면 힐링될 거 같은 느낌 ㅎㅎ\r\n",
          "original_form": "하면 힐링될 거 같은 느낌 ㅎㅎ\r\n",
          "speaker_id": "2",
          "time": "20211108 13:23"
        }
      ]
