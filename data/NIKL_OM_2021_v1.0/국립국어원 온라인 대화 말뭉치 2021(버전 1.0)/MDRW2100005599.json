      "utterance": [
        {
          "id": "MDRW2100005599.1.1",
          "form": "택배산은 다 치웠어?ㅋㅋ",
          "original_form": "택배산은 다 치웠어?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005599.1.2",
          "form": "응ㅋㅋ냉동실 꽉꽉채우고 기저귀도 다 정리했어ㅋ",
          "original_form": "응ㅋㅋ냉동실 꽉꽉채우고 기저귀도 다 정리했어ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005599.1.3",
          "form": "ㅋㅋㅋ 잘했네",
          "original_form": "ㅋㅋㅋ 잘했네",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005599.1.4",
          "form": "기저귀 몇팩이라 그랬지?ㅋ",
          "original_form": "기저귀 몇팩이라 그랬지?ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005599.1.5",
          "form": "18팩ㅋㅋ",
          "original_form": "18팩ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005599.1.6",
          "form": "다샀다가 취소했어ㅋㅋ",
          "original_form": "다샀다가 취소했어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005599.1.7",
          "form": "왜?취소했어?",
          "original_form": "왜?취소했어?",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005599.1.8",
          "form": "더 샀다가",
          "original_form": "더 샀다가",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005599.1.9",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005599.1.10",
          "form": "더 샀다가ㅋㅋ",
          "original_form": "더 샀다가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005599.1.11",
          "form": "아직 바우처가 두달 정도 남았거든?",
          "original_form": "아직 바우처가 두달 정도 남았거든?",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005599.1.12",
          "form": "근데 그안에 안뜰거 같아서 다 샀는데",
          "original_form": "근데 그안에 안뜰거 같아서 다 샀는데",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005599.1.13",
          "form": "더 싼게 있는거야ㅋㅋ그래서취소를 했는데",
          "original_form": "더 싼게 있는거야ㅋㅋ그래서취소를 했는데",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005599.1.14",
          "form": "돈이ㅋㅋㅋ 바로 안돌아오는거야ㅠ",
          "original_form": "돈이ㅋㅋㅋ 바로 안돌아오는거야ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005599.1.15",
          "form": "그거는 잔고가 있어야 바우처에서 나가거든ㅠ",
          "original_form": "그거는 잔고가 있어야 바우처에서 나가거든ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005599.1.16",
          "form": "아아 ㅜ그렇구나",
          "original_form": "아아 ㅜ그렇구나",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005599.1.17",
          "form": "그럼 다음에",
          "original_form": "그럼 다음에",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005599.1.18",
          "form": "또 살수있겠네",
          "original_form": "또 살수있겠네",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005599.1.19",
          "form": "응 그래서 돈이 2일인가뒤에",
          "original_form": "응 그래서 돈이 2일인가뒤에",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005599.1.20",
          "form": "들어왔는데 더살려니 넣을곳도 없고해서",
          "original_form": "들어왔는데 더살려니 넣을곳도 없고해서",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005599.1.21",
          "form": "2단안에는 뜨겠지 싶어서",
          "original_form": "2단안에는 뜨겠지 싶어서",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005599.1.22",
          "form": "응응 또 뜨지싶다ㅋ",
          "original_form": "응응 또 뜨지싶다ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005599.1.23",
          "form": "바우처 너무 좋다ㅋ",
          "original_form": "바우처 너무 좋다ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005599.1.24",
          "form": "응 ㅋㅋ 근데 맥스드라이가",
          "original_form": "응 ㅋㅋ 근데 맥스드라이가",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005599.1.25",
          "form": "리뉴얼되고 나서는 핫딜도 없더라",
          "original_form": "리뉴얼되고 나서는 핫딜도 없더라",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005599.1.26",
          "form": "응응 맞아",
          "original_form": "응응 맞아",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005599.1.27",
          "form": "요즘 핫딜 잘 안뜨더라",
          "original_form": "요즘 핫딜 잘 안뜨더라",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005599.1.28",
          "form": "그니까 ㅠ 비싸6단계",
          "original_form": "그니까 ㅠ 비싸6단계",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005599.1.29",
          "form": "그렇지. 6단계ㅜㅜ비싸고 적고ㅜ",
