      "utterance": [
        {
          "id": "MDRW2100032876.1.1",
          "form": "신청해좠어?",
          "original_form": "신청해좠어?",
          "speaker_id": "1",
          "time": "20210906 03:10"
        },
        {
          "id": "MDRW2100032876.1.2",
          "form": "근로장려금은 그거아닌가",
          "original_form": "근로장려금은 그거아닌가",
          "speaker_id": "2",
          "time": "20210906 03:11"
        },
        {
          "id": "MDRW2100032876.1.3",
          "form": "이미 지급까지 다 된거",
          "original_form": "이미 지급까지 다 된거",
          "speaker_id": "2",
          "time": "20210906 03:11"
        },
        {
          "id": "MDRW2100032876.1.4",
          "form": "ㄴㄴ요건이안돼ㅠ",
          "original_form": "ㄴㄴ요건이안돼ㅠ",
          "speaker_id": "2",
          "time": "20210906 03:11"
        },
        {
          "id": "MDRW2100032876.1.5",
          "form": "웅 이미 끝났는데 어이없군 ㅎ",
          "original_form": "웅 이미 끝났는데 어이없군 ㅎ",
          "speaker_id": "1",
          "time": "20210906 03:12"
        },
        {
          "id": "MDRW2100032876.1.6",
          "form": "헉.... 글쿠나 소득분위?",
          "original_form": "헉.... 글쿠나 소득분위?",
          "speaker_id": "1",
          "time": "20210906 03:13"
        },
        {
          "id": "MDRW2100032876.1.7",
          "form": "난 안 해봐서 모르것다",
          "original_form": "난 안 해봐서 모르것다",
          "speaker_id": "1",
          "time": "20210906 03:13"
        },
        {
          "id": "MDRW2100032876.1.8",
          "form": "나 아직 부모님 집에서 살아서",
          "original_form": "나 아직 부모님 집에서 살아서",
          "speaker_id": "2",
          "time": "20210906 03:13"
        },
        {
          "id": "MDRW2100032876.1.9",
          "form": "부모님재산 2억인가 넘어도 안되는걸루앎...",
          "original_form": "부모님재산 2억인가 넘어도 안되는걸루앎...",
          "speaker_id": "2",
          "time": "20210906 03:13"
        },
        {
          "id": "MDRW2100032876.1.10",
          "form": "젠장",
          "original_form": "젠장",
          "speaker_id": "1",
          "time": "20210906 03:15"
        },
        {
          "id": "MDRW2100032876.1.11",
          "form": "고백하기도 전에 차인 기분이네",
          "original_form": "고백하기도 전에 차인 기분이네",
          "speaker_id": "1",
          "time": "20210906 03:15"
        },
        {
          "id": "MDRW2100032876.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 03:15"
        },
        {
          "id": "MDRW2100032876.1.13",
          "form": "기준 빡세다ㅠㅠㅠ이런게어딨어",
          "original_form": "기준 빡세다ㅠㅠㅠ이런게어딨어",
          "speaker_id": "1",
          "time": "20210906 03:15"
        },
        {
          "id": "MDRW2100032876.1.14",
          "form": "ㅋㅋㅋㅋㅋ고백하기도 전에 차엿대",
          "original_form": "ㅋㅋㅋㅋㅋ고백하기도 전에 차엿대",
          "speaker_id": "2",
          "time": "20210906 03:16"
        },
        {
          "id": "MDRW2100032876.1.15",
          "form": "그치 150만원씩이나 주더라",
          "original_form": "그치 150만원씩이나 주더라",
          "speaker_id": "2",
          "time": "20210906 03:16"
        },
        {
          "id": "MDRW2100032876.1.16",
          "form": "그래도 필요한 친구들한테 가야되니까ㅠ",
          "original_form": "그래도 필요한 친구들한테 가야되니까ㅠ",
          "speaker_id": "2",
          "time": "20210906 03:16"
        },
        {
          "id": "MDRW2100032876.1.17",
          "form": "근로장려금이랑 재난지원금 헷갈리는듯 ㅠ",
          "original_form": "근로장려금이랑 재난지원금 헷갈리는듯 ㅠ",
          "speaker_id": "2",
          "time": "20210906 03:16"
        },
        {
          "id": "MDRW2100032876.1.18",
          "form": "와 백오십?!??!?",
          "original_form": "와 백오십?!??!?",
          "speaker_id": "1",
          "time": "20210906 03:17"
        },
        {
          "id": "MDRW2100032876.1.19",
          "form": "내 전재산의 10%",
          "original_form": "내 전재산의 10%",
          "speaker_id": "1",
          "time": "20210906 03:17"
        },
        {
          "id": "MDRW2100032876.1.20",
          "form": "천오백? 부럽다ㆍㆍㆍ",
          "original_form": "천오백? 부럽다ㆍㆍㆍ",
          "speaker_id": "2",
          "time": "20210906 03:18"
        },
        {
          "id": "MDRW2100032876.1.21",
          "form": "아 맞다 재난지원금;",
          "original_form": "아 맞다 재난지원금;",
          "speaker_id": "1",
          "time": "20210906 03:18"
        },
        {
          "id": "MDRW2100032876.1.22",
          "form": "몇살이셔요ㅠ",
          "original_form": "몇살이셔요ㅠ",
          "speaker_id": "2",
          "time": "20210906 03:18"
        },
        {
          "id": "MDRW2100032876.1.23",
          "form": "아 그렇네 아니야 1100....",
          "original_form": "아 그렇네 아니야 1100....",
          "speaker_id": "1",
          "time": "20210906 03:18"
        },
        {
          "id": "MDRW2100032876.1.24",
          "form": "계산 미스 ><",
          "original_form": "계산 미스 ><",
          "speaker_id": "1",
          "time": "20210906 03:18"
        },
        {
          "id": "MDRW2100032876.1.25",
          "form": "난 부모님이랑 같이살아서 재난지원금도 탈락됨...",
          "original_form": "난 부모님이랑 같이살아서 재난지원금도 탈락됨...",
          "speaker_id": "2",
          "time": "20210906 03:18"
        },
        {
          "id": "MDRW2100032876.1.26",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210906 03:18"
        },
        {
          "id": "MDRW2100032876.1.27",
          "form": "20살이양",
          "original_form": "20살이양",
          "speaker_id": "1",
          "time": "20210906 03:18"
        },
        {
          "id": "MDRW2100032876.1.28",
          "form": "엥 재난지원금이???",
          "original_form": "엥 재난지원금이???",
          "speaker_id": "1",
          "time": "20210906 03:19"
        },
        {
          "id": "MDRW2100032876.1.29",
          "form": "나는 받는데.. 같이 살아서는 아닐골",
          "original_form": "나는 받는데.. 같이 살아서는 아닐골",
          "speaker_id": "1",
          "time": "20210906 03:19"
        },
        {
          "id": "MDRW2100032876.1.30",
          "form": "20살인데 천만원 넘어? 대단하네",
          "original_form": "20살인데 천만원 넘어? 대단하네",
          "speaker_id": "2",
          "time": "20210906 03:19"
        },
        {
          "id": "MDRW2100032876.1.31",
          "form": "같이 사는데 부모님 재산이 잡혀서",
          "original_form": "같이 사는데 부모님 재산이 잡혀서",
          "speaker_id": "2",
          "time": "20210906 03:19"
        },
        {
          "id": "MDRW2100032876.1.32",
          "form": "그렇게 되는걸거야ㅠ",
          "original_form": "그렇게 되는걸거야ㅠ",
          "speaker_id": "2",
          "time": "20210906 03:19"
        },
        {
          "id": "MDRW2100032876.1.33",
          "form": "유딩때부터 모은 적금이앜ㅋㅋㅋㅋ 근디 등록금으로 다 나갈듯ㅜㅜ",
          "original_form": "유딩때부터 모은 적금이앜ㅋㅋㅋㅋ 근디 등록금으로 다 나갈듯ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 03:20"
        },
        {
          "id": "MDRW2100032876.1.34",
          "form": "아하...세대주 분리를 해야겠구만",
          "original_form": "아하...세대주 분리를 해야겠구만",
          "speaker_id": "1",
          "time": "20210906 03:21"
        },
        {
          "id": "MDRW2100032876.1.35",
          "form": "아 올해 대학생인가? 풋풋하구먼... 부럽다",
          "original_form": "아 올해 대학생인가? 풋풋하구먼... 부럽다",
          "speaker_id": "2",
          "time": "20210906 03:21"
        },
        {
          "id": "MDRW2100032876.1.36",
