      "utterance": [
        {
          "id": "MDRW2100038108.1.1",
          "form": "아무래도 자극적이니까.",
          "original_form": "아무래도 자극적이니까.",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.2",
          "form": "위와 장에 안좋른가봐",
          "original_form": "위와 장에 안좋른가봐",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.3",
          "form": "하지만 너무 맛있지않아?",
          "original_form": "하지만 너무 맛있지않아?",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.4",
          "form": "하지만 스트레스 해소엔 직빵이야",
          "original_form": "하지만 스트레스 해소엔 직빵이야",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.5",
          "form": "나는 매운김치도 좋아해",
          "original_form": "나는 매운김치도 좋아해",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.6",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.7",
          "form": "하지만 먹고나면 항상 다음날 고통받아",
          "original_form": "하지만 먹고나면 항상 다음날 고통받아",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.8",
          "form": "도파민이 뿜어나와",
          "original_form": "도파민이 뿜어나와",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.9",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.10",
          "form": "나 사실 오늘 이정옥에서 김치를 먹었어",
          "original_form": "나 사실 오늘 이정옥에서 김치를 먹었어",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.11",
          "form": "너도 알지? 그 매운김치",
          "original_form": "너도 알지? 그 매운김치",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.12",
          "form": "세강에 어떻게 너혼자먹을수가",
          "original_form": "세강에 어떻게 너혼자먹을수가",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.13",
          "form": "아까워서 싹싹 긁어먹었어",
          "original_form": "아까워서 싹싹 긁어먹었어",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038108.1.14",
          "form": "그리고 너 이번주는 건강챙긴다고 하지 않았니?",
          "original_form": "그리고 너 이번주는 건강챙긴다고 하지 않았니?",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038108.1.15",
          "form": "잠시 잊었오",
          "original_form": "잠시 잊었오",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038108.1.16",
          "form": "칼국수와 먹으면 중화되지 않을꺄?",
          "original_form": "칼국수와 먹으면 중화되지 않을꺄?",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038108.1.17",
          "form": "헛소리말고 우리 조심해서 살자.",
          "original_form": "헛소리말고 우리 조심해서 살자.",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038108.1.18",
          "form": "그래서 배가 아팠던걸까?",
          "original_form": "그래서 배가 아팠던걸까?",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038108.1.19",
          "form": "응 ㅠ 고마워",
          "original_form": "응 ㅠ 고마워",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038108.1.20",
          "form": "위에 빵꾸나면 돌이킬수없어",
          "original_form": "위에 빵꾸나면 돌이킬수없어",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038108.1.21",
          "form": "너는 곧 가을인데",
          "original_form": "너는 곧 가을인데",
          "speaker_id": "1",
          "time": "20210901 23:19"
        }
      ]
    }
