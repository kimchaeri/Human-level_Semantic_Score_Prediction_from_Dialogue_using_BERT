      "utterance": [
        {
          "id": "MDRW2100008147.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008147.1.2",
          "form": "아점으로 스테이크요 ㅋㅋ",
          "original_form": "아점으로 스테이크요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008147.1.3",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008147.1.4",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008147.1.5",
          "form": "아니예요 ㅋㅋ고기 굽고 야채 샐러드 과일감자 해서  먹었어요",
          "original_form": "아니예요 ㅋㅋ고기 굽고 야채 샐러드 과일감자 해서  먹었어요",
          "speaker_id": "2",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008147.1.6",
          "form": "간단해요",
          "original_form": "간단해요",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008147.1.7",
          "form": "소스는 시판이라",
          "original_form": "소스는 시판이라",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008147.1.8",
          "form": "소스맛이죠",
          "original_form": "소스맛이죠",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008147.1.9",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:45"
        },
        {
          "id": "MDRW2100008147.1.10",
          "form": "소스가 300년전통 이라 써있네요",
          "original_form": "소스가 300년전통 이라 써있네요",
          "speaker_id": "2",
          "time": "20210822 13:45"
        },
        {
          "id": "MDRW2100008147.1.11",
          "form": "이야",
          "original_form": "이야",
          "speaker_id": "1",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008147.1.12",
          "form": "그정도면 걍 먹어도 맛나겠네녀",
          "original_form": "그정도면 걍 먹어도 맛나겠네녀",
          "speaker_id": "1",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008147.1.13",
          "form": "뭐 어찌 300년인건지 ㅋㅋ모르겠네요",
          "original_form": "뭐 어찌 300년인건지 ㅋㅋ모르겠네요",
          "speaker_id": "2",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008147.1.14",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008147.1.15",
          "form": "저녁은 뭐 드시세요?",
          "original_form": "저녁은 뭐 드시세요?",
          "speaker_id": "2",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008147.1.16",
          "form": "전 카페가서 샌드위치 먹고오게요",
          "original_form": "전 카페가서 샌드위치 먹고오게요",
          "speaker_id": "2",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008147.1.17",
          "form": "저녁 너무 간단히 아니에여?",
          "original_form": "저녁 너무 간단히 아니에여?",
          "speaker_id": "1",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008147.1.18",
          "form": "ㅋㅋ라면이라도 와서 먹을까요",
          "original_form": "ㅋㅋ라면이라도 와서 먹을까요",
          "speaker_id": "2",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008147.1.19",
          "form": "뭐 드시게요?",
          "original_form": "뭐 드시게요?",
          "speaker_id": "2",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008147.1.20",
          "form": "아직 생각이 ㅋ",
          "original_form": "아직 생각이 ㅋ",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008147.1.21",
          "form": "네 대충 뭐 먹음되죠",
          "original_form": "네 대충 뭐 먹음되죠",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008147.1.22",
          "form": "요리는 못하세요?",
          "original_form": "요리는 못하세요?",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008147.1.23",
          "form": "가끔 남편이 레시피 보고 해줄때 있어요",
          "original_form": "가끔 남편이 레시피 보고 해줄때 있어요",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008147.1.24",
          "form": "요샌 안하지만요 ㅋㅋ",
          "original_form": "요샌 안하지만요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008147.1.25",
          "form": "네 요똥이요",
          "original_form": "네 요똥이요",
          "speaker_id": "1",
          "time": "20210822 13:52"
        }
