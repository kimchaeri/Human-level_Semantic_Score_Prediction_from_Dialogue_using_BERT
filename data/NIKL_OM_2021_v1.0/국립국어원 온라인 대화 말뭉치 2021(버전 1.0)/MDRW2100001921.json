      "utterance": [
        {
          "id": "MDRW2100001921.1.1",
          "form": "오늘 날씨 시원하지 않았어요!?",
          "original_form": "오늘 날씨 시원하지 않았어요!?",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001921.1.2",
          "form": "맞아요 ~ 갑자기 시원해졌어요",
          "original_form": "맞아요 ~ 갑자기 시원해졌어요",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001921.1.3",
          "form": "그저께 비올땐 확 추워져서 드디어 가을인가!!했는데ㅋㅋ",
          "original_form": "그저께 비올땐 확 추워져서 드디어 가을인가!!했는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001921.1.4",
          "form": "입추 지나니까 날씨가 정말 변하네요 ㅋ",
          "original_form": "입추 지나니까 날씨가 정말 변하네요 ㅋ",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001921.1.5",
          "form": "그러니까요! 그래도 아직은 반팔 입어야하는 것 같아요ㅋㅋ",
          "original_form": "그러니까요! 그래도 아직은 반팔 입어야하는 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001921.1.6",
          "form": "맞아요 ㅎ 반팔은 계속입고 더 시원해지면 가디건을 걸쳐야겠어요",
          "original_form": "맞아요 ㅎ 반팔은 계속입고 더 시원해지면 가디건을 걸쳐야겠어요",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001921.1.7",
          "form": "사무실은 넘 추워서 가디건 필수ㅋㅋ",
          "original_form": "사무실은 넘 추워서 가디건 필수ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001921.1.8",
          "form": "ㅋㅋ 아 그렇죠",
          "original_form": "ㅋㅋ 아 그렇죠",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001921.1.9",
          "form": "낼도 더우려나 모르겠어용",
          "original_form": "낼도 더우려나 모르겠어용",
          "speaker_id": "1",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001921.1.10",
          "form": "낮에 조금은 덥겠지용",
          "original_form": "낮에 조금은 덥겠지용",
          "speaker_id": "2",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001921.1.11",
          "form": "얌전히 사무실에 있어야겠네요",
          "original_form": "얌전히 사무실에 있어야겠네요",
          "speaker_id": "1",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001921.1.12",
          "form": "올해는 생각보다 더운게 금방 지나간것같아요",
          "original_form": "올해는 생각보다 더운게 금방 지나간것같아요",
          "speaker_id": "1",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001921.1.13",
          "form": "그치만 진짜진짜 최고의 더위였어요 ㅋ",
          "original_form": "그치만 진짜진짜 최고의 더위였어요 ㅋ",
          "speaker_id": "2",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001921.1.14",
          "form": "맞아요 게다가 너무 습함...",
          "original_form": "맞아요 게다가 너무 습함...",
          "speaker_id": "1",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001921.1.15",
          "form": "습한거 진짜 싫어요ㅜㅜ",
          "original_form": "습한거 진짜 싫어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001921.1.16",
          "form": "다음주는 비가 계속 온다네요",
          "original_form": "다음주는 비가 계속 온다네요",
          "speaker_id": "2",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001921.1.17",
          "form": "담주 계속 비온대요????",
          "original_form": "담주 계속 비온대요????",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001921.1.18",
          "form": "우산들고다니기 넘 귀찮은데퓨",
          "original_form": "우산들고다니기 넘 귀찮은데퓨",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001921.1.19",
