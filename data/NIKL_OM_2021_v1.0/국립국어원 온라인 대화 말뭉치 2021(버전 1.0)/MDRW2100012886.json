      "utterance": [
        {
          "id": "MDRW2100012886.1.1",
          "form": "네 비오네요 ㅠㅠ",
          "original_form": "네 비오네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 10:08"
        },
        {
          "id": "MDRW2100012886.1.2",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100012886.1.3",
          "form": "가을 장마철인가봐요",
          "original_form": "가을 장마철인가봐요",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100012886.1.4",
          "form": "늦장마네요",
          "original_form": "늦장마네요",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100012886.1.5",
          "form": "그러게요 장마가 너무너무 늦게온거같은..",
          "original_form": "그러게요 장마가 너무너무 늦게온거같은..",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100012886.1.6",
          "form": "비가 너무 오네요",
          "original_form": "비가 너무 오네요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100012886.1.7",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100012886.1.8",
          "form": "보니까 다음주에도 오던데..",
          "original_form": "보니까 다음주에도 오던데..",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100012886.1.9",
          "form": "으휴",
          "original_form": "으휴",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100012886.1.10",
          "form": "시원해서 좋긴한데 그래도 너무 오는것도 싫네요",
          "original_form": "시원해서 좋긴한데 그래도 너무 오는것도 싫네요",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100012886.1.11",
          "form": "앞이 안보이더라구요",
          "original_form": "앞이 안보이더라구요",
          "speaker_id": "1",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100012886.1.12",
          "form": "엄청 쏟아지나보네요",
          "original_form": "엄청 쏟아지나보네요",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100012886.1.13",
          "form": "네 폭우요",
          "original_form": "네 폭우요",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100012886.1.14",
          "form": "아이구..",
          "original_form": "아이구..",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100012886.1.15",
          "form": "이 지역도 군데군데 갑자기 폭우오다가 그치고 조금오고 이렇게 반복이네요",
          "original_form": "이 지역도 군데군데 갑자기 폭우오다가 그치고 조금오고 이렇게 반복이네요",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100012886.1.16",
          "form": "오락가락해요",
          "original_form": "오락가락해요",
