      "utterance": [
        {
          "id": "MDRW2100040596.1.1",
          "form": "알러지 때문에 못먹거나 하는 음식 있나요?",
          "original_form": "알러지 때문에 못먹거나 하는 음식 있나요?",
          "speaker_id": "2",
          "time": "20210905 07:05"
        },
        {
          "id": "MDRW2100040596.1.2",
          "form": "알러지는 없구요~",
          "original_form": "알러지는 없구요~",
          "speaker_id": "1",
          "time": "20210905 10:16"
        },
        {
          "id": "MDRW2100040596.1.3",
          "form": "회랑 카레 못 먹어요:)",
          "original_form": "회랑 카레 못 먹어요:)",
          "speaker_id": "1",
          "time": "20210905 10:16"
        },
        {
          "id": "MDRW2100040596.1.4",
          "form": "아 회  못먹는군요",
          "original_form": "아 회  못먹는군요",
          "speaker_id": "2",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040596.1.5",
          "form": "그런 사람들 많더라구요",
          "original_form": "그런 사람들 많더라구요",
          "speaker_id": "2",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040596.1.6",
          "form": "저는 회는 없어서 못먹어요",
          "original_form": "저는 회는 없어서 못먹어요",
          "speaker_id": "2",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040596.1.7",
          "form": "어릴때 잘 먹었었는데\n한번 심하게 탈난 이후론\n못먹겠더라구요ㅠ",
          "original_form": "어릴때 잘 먹었었는데\n한번 심하게 탈난 이후론\n못먹겠더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100040596.1.8",
          "form": "무슨 회 젤 좋아하세요?",
          "original_form": "무슨 회 젤 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100040596.1.9",
          "form": "참치회 좋아하지만 비싸서 그냥 광어회 먹어요",
          "original_form": "참치회 좋아하지만 비싸서 그냥 광어회 먹어요",
          "speaker_id": "2",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100040596.1.10",
          "form": "참치회",
          "original_form": "참치회{emoji:♡}",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100040596.1.11",
          "form": "저 오징어회는 먹어요ㅋㅋㅋㅋㅋ",
          "original_form": "저 오징어회는 먹어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100040596.1.12",
          "form": "오징어는 괜찮군요",
          "original_form": "오징어는 괜찮군요",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100040596.1.13",
          "form": "제 딸도 어릴때 굴 먹고 체하더니 그후로 굴을 안먹어요",
          "original_form": "제 딸도 어릴때 굴 먹고 체하더니 그후로 굴을 안먹어요",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100040596.1.14",
          "form": "굴이 얼마나 맛있는데..ㅠ",
          "original_form": "굴이 얼마나 맛있는데..ㅠ",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100040596.1.15",
          "form": "아...  굴....",
          "original_form": "아...  굴....",
          "speaker_id": "1",
          "time": "20210905 19:05"
        },
        {
          "id": "MDRW2100040596.1.16",
          "form": "그러고보니\n저도 굴도 못먹네요 이제ㅜㅜ",
          "original_form": "그러고보니\n저도 굴도 못먹네요 이제ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 19:05"
        },
        {
          "id": "MDRW2100040596.1.17",
          "form": "예전엔 굴만\n골라먹고 그랬는데ㅜㅜ",
          "original_form": "예전엔 굴만\n골라먹고 그랬는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 19:05"
        },
        {
          "id": "MDRW2100040596.1.18",
          "form": "좋아하는데 못먹으면 참 괴로워요",
          "original_form": "좋아하는데 못먹으면 참 괴로워요",
          "speaker_id": "2",
          "time": "20210905 20:08"
        },
        {
          "id": "MDRW2100040596.1.19",
          "form": "그래도 굴전은 먹어요:)",
          "original_form": "그래도 굴전은 먹어요:)",
          "speaker_id": "1",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100040596.1.20",
          "form": "아 굴전 진짜 맛있죠",
          "original_form": "아 굴전 진짜 맛있죠",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100040596.1.21",
          "form": "굴이 제철일 때 엄청 사서 해먹었는데",
          "original_form": "굴이 제철일 때 엄청 사서 해먹었는데",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100040596.1.22",
          "form": "맞아요:)\n갓 해서 먹음 진짜 넘 맛있어요",
          "original_form": "맞아요:)\n갓 해서 먹음 진짜 넘 맛있어요",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100040596.1.23",
          "form": "저는 엄마가 해주신것만\n먹어봤네요^^",
          "original_form": "저는 엄마가 해주신것만\n먹어봤네요^^",
