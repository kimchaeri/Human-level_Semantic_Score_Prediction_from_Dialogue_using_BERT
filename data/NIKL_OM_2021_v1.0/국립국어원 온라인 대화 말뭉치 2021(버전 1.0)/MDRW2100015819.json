        {
          "id": "MDRW2100015819.1.1",
          "form": "스포츠 경기",
          "original_form": "스포츠 경기",
          "speaker_id": "1",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015819.1.2",
          "form": "보시나요?",
          "original_form": "보시나요?",
          "speaker_id": "1",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015819.1.3",
          "form": "name2님 달리기 못하잔아요",
          "original_form": "&name2&님 달리기 못하잔아요",
          "speaker_id": "1",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015819.1.4",
          "form": "네 달리기는 못하고 안 좋아해요",
          "original_form": "네 달리기는 못하고 안 좋아해요",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015819.1.5",
          "form": "하지만 가끔 신나면 달려요",
          "original_form": "하지만 가끔 신나면 달려요",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015819.1.6",
          "form": "스포츠경기는 챙겨보지 않지만 결과는 본답니다",
          "original_form": "스포츠경기는 챙겨보지 않지만 결과는 본답니다",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015819.1.7",
          "form": "결과만보다니",
          "original_form": "결과만보다니",
          "speaker_id": "1",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015819.1.8",
          "form": "결과주의자",
          "original_form": "결과주의자",
          "speaker_id": "1",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015819.1.9",
          "form": "우리집 앞에 야구장 있는데 한번을 안가봤어",
          "original_form": "우리집 앞에 야구장 있는데 한번을 안가봤어",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015819.1.10",
          "form": "와 야구장가서 치킨 먹으면서 놀고싶다",
          "original_form": "와 야구장가서 치킨 먹으면서 놀고싶다",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015819.1.11",
          "form": "후자가 중요한거지?",
          "original_form": "후자가 중요한거지?",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015819.1.12",
          "form": "아 나 경기 챙겨보는거 잇어",
          "original_form": "아 나 경기 챙겨보는거 잇어",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015819.1.13",
          "form": "강아지 장애물경기",
          "original_form": "강아지 장애물경기",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015819.1.14",
          "form": "와 대박 귀여움",
          "original_form": "와 대박 귀여움",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015819.1.15",
          "form": "진짜!!!!",
          "original_form": "진짜!!!!",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015819.1.16",
          "form": "성공해도귀엽고 못해도귀여워",
          "original_form": "성공해도귀엽고 못해도귀여워",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015819.1.17",
          "form": "못하는게 오히려 귀엽지",
          "original_form": "못하는게 오히려 귀엽지",
          "speaker_id": "2",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015819.1.18",
          "form": "어 나도",
          "original_form": "어 나도",
