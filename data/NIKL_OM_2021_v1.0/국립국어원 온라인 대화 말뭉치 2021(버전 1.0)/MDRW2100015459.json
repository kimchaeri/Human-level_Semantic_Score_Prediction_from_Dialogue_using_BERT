      "utterance": [
        {
          "id": "MDRW2100015459.1.1",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015459.1.2",
          "form": "좋아하지는 않는데 정신차리면 지르고 있네요",
          "original_form": "좋아하지는 않는데 정신차리면 지르고 있네요",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015459.1.3",
          "form": "최근에 뭐 쇼핑하셨어요?",
          "original_form": "최근에 뭐 쇼핑하셨어요?",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015459.1.4",
          "form": "얼굴필링기요",
          "original_form": "얼굴필링기요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015459.1.5",
          "form": "당근에서 샀어요",
          "original_form": "당근에서 샀어요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015459.1.6",
          "form": "당근 이용하시나요?",
          "original_form": "당근 이용하시나요?",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015459.1.7",
          "form": "필링기는 효과가 좋아요?",
          "original_form": "필링기는 효과가 좋아요?",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015459.1.8",
          "form": "깔아두긴 했는데 이용한 적이 없네요",
          "original_form": "깔아두긴 했는데 이용한 적이 없네요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015459.1.9",
          "form": "엄청난 효과는 모르겠는데",
          "original_form": "엄청난 효과는 모르겠는데",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015459.1.10",
          "form": "기름기는 좀 사라지는거 같아요",
          "original_form": "기름기는 좀 사라지는거 같아요",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015459.1.11",
          "form": "아ㅋㅋ 아주 좋네요",
          "original_form": "아ㅋㅋ 아주 좋네요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015459.1.12",
          "form": "지성피부라 저도 혹하는데요",
          "original_form": "지성피부라 저도 혹하는데요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015459.1.13",
          "form": "자극적이진 않던가요",
          "original_form": "자극적이진 않던가요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015459.1.14",
          "form": "처음에는 자극적인가 싶기도했는데",
          "original_form": "처음에는 자극적인가 싶기도했는데",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015459.1.15",
          "form": "제가 손힘 풀고 약하게 써서 괜찮더라구요",
          "original_form": "제가 손힘 풀고 약하게 써서 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015459.1.16",
          "form": "아 필링기도 요령이 필요하군요",
          "original_form": "아 필링기도 요령이 필요하군요",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015459.1.17",
          "form": "신품 사기는 너무 비싸서 당근에서 구입했어요 ㅎㅎ",
          "original_form": "신품 사기는 너무 비싸서 당근에서 구입했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015459.1.18",
          "form": "며칠에 한번꼴로 쓰세요?",
          "original_form": "며칠에 한번꼴로 쓰세요?",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015459.1.19",
          "form": "필링은 일이주에 한번씩요",
          "original_form": "필링은 일이주에 한번씩요",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015459.1.20",
          "form": "아 생각보다 띄엄이네요",
          "original_form": "아 생각보다 띄엄이네요",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015459.1.21",
          "form": "하긴 너무 자주할 제품은 아니죠",
          "original_form": "하긴 너무 자주할 제품은 아니죠",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015459.1.22",
          "form": "너무 자주하면 안좋다고하더라구요",
          "original_form": "너무 자주하면 안좋다고하더라구요",
          "speaker_id": "1",
          "time": "20210825 23:15"
        }
