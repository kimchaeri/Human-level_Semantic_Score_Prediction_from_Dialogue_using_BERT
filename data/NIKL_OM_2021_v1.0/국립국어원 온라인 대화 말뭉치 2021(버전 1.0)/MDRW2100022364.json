      "utterance": [
        {
          "id": "MDRW2100022364.1.1",
          "form": "다음 도서신청때 전자책 들여놔달라고 해야겠다",
          "original_form": "다음 도서신청때 전자책 들여놔달라고 해야겠다",
          "speaker_id": "2",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022364.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022364.1.3",
          "form": "타자가 안쳐진데",
          "original_form": "타자가 안쳐진데",
          "speaker_id": "1",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022364.1.4",
          "form": "난 폰이 멀쩡한데도 타자가 안 쳐져",
          "original_form": "난 폰이 멀쩡한데도 타자가 안 쳐져",
          "speaker_id": "2",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022364.1.5",
          "form": "이게 렉이 좀 걸리나봐",
          "original_form": "이게 렉이 좀 걸리나봐",
          "speaker_id": "2",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022364.1.6",
          "form": "컴으로 되면 좋은데",
          "original_form": "컴으로 되면 좋은데",
          "speaker_id": "2",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022364.1.7",
          "form": "폰이라 렉이 두 배!",
          "original_form": "폰이라 렉이 두 배!",
          "speaker_id": "2",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022364.1.8",
          "form": "검색해봤는데 아쉽게도",
          "original_form": "검색해봤는데 아쉽게도",
          "speaker_id": "2",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022364.1.9",
          "form": "피프티피플이란 책은 없다",
          "original_form": "피프티피플이란 책은 없다",
          "speaker_id": "2",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022364.1.10",
          "form": "안은영도 없네?",
          "original_form": "안은영도 없네?",
          "speaker_id": "2",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022364.1.11",
          "form": "뭐냐 도서관",
          "original_form": "뭐냐 도서관",
          "speaker_id": "2",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022364.1.12",
          "form": "아 저번 도서신청기간때 이갈리아 신청했는데",
          "original_form": "아 저번 도서신청기간때 이갈리아 신청했는데",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022364.1.13",
          "form": "아직도 응답 대기중이야",
          "original_form": "아직도 응답 대기중이야",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022364.1.14",
          "form": "나 말고도 이갈리아 책 신청한 사람 많은데",
          "original_form": "나 말고도 이갈리아 책 신청한 사람 많은데",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022364.1.15",
          "form": "다 응답 대기중인 거 이상하네",
          "original_form": "다 응답 대기중인 거 이상하네",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022364.1.16",
          "form": "작년부터 계속 신청하는데",
          "original_form": "작년부터 계속 신청하는데",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022364.1.17",
          "form": "왜 안 받아주지",
          "original_form": "왜 안 받아주지",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022364.1.18",
          "form": "낟 폰",
          "original_form": "낟 폰",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022364.1.19",
          "form": "맛이 상당히갔네",
          "original_form": "맛이 상당히갔네",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022364.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022364.1.21",
          "form": "테이스트 이즈 곤",
          "original_form": "테이스트 이즈 곤",
          "speaker_id": "2",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022364.1.22",
          "form": "진심 pc버전 있었으면 좋겠다",
          "original_form": "진심 pc버전 있었으면 좋겠다",
          "speaker_id": "2",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022364.1.23",
          "form": "너는 책 볼때 어떤 플랫폼으로 읽어?",
          "original_form": "너는 책 볼때 어떤 플랫폼으로 읽어?",
          "speaker_id": "2",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022364.1.24",
          "form": "나는 리디북스로 많이 봐",
          "original_form": "나는 리디북스로 많이 봐",
          "speaker_id": "1",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022364.1.25",
          "form": "그렇구나ㅎㅎ 알라딘서적도 써봤니?",
          "original_form": "그렇구나ㅎㅎ 알라딘서적도 써봤니?",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022364.1.26",
          "form": "아니 알라딘은 싫어서 안써",
          "original_form": "아니 알라딘은 싫어서 안써",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022364.1.27",
          "form": "그래? 그럼 예스 24도 안 써봤어?",
          "original_form": "그래? 그럼 예스 24도 안 써봤어?",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022364.1.28",
          "form": "알라딘이 책팔려고 쓰는 마케팅 방법이 싫더라",
          "original_form": "알라딘이 책팔려고 쓰는 마케팅 방법이 싫더라",
          "speaker_id": "1",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022364.1.29",
          "form": "응 안써봤어",
          "original_form": "응 안써봤어",
          "speaker_id": "1",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022364.1.30",
          "form": "아 정말? 알라딘 마케팅이 별로인가보구나",
          "original_form": "아 정말? 알라딘 마케팅이 별로인가보구나",
          "speaker_id": "2",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022364.1.31",
          "form": "최근에 뭐 읽었어?",
          "original_form": "최근에 뭐 읽었어?",
          "speaker_id": "2",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022364.1.32",
          "form": "나 최근에 보건교사 안은영",
          "original_form": "나 최근에 보건교사 안은영",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022364.1.33",
          "form": "넌 읽고싶은책 있니",
          "original_form": "넌 읽고싶은책 있니",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022364.1.34",
          "form": "나도 보건교사 읽어보고싶어",
          "original_form": "나도 보건교사 읽어보고싶어",
          "speaker_id": "2",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022364.1.35",
          "form": "이 책 많이 추천해",
          "original_form": "이 책 많이 추천해",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022364.1.36",
          "form": "얼른 전자도서관에 들어왔으면 좋겠다 ㅎㅎ",
          "original_form": "얼른 전자도서관에 들어왔으면 좋겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022364.1.37",
          "form": "추천해준 거니 얼른 읽어보고싶어",
          "original_form": "추천해준 거니 얼른 읽어보고싶어",
          "speaker_id": "2",
          "time": "20210829 19:14"
        }
      ]
