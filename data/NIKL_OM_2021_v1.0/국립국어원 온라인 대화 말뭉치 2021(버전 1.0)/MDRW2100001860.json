        },
        {
          "id": "MDRW2100001860.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001860.1.3",
          "form": "혹시 병이나 그런거 있어요?",
          "original_form": "혹시 병이나 그런거 있어요?",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001860.1.4",
          "form": "천식이랑 아토피 조금 있어요",
          "original_form": "천식이랑 아토피 조금 있어요",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001860.1.5",
          "form": "관리해서 그나마 괜찮아요",
          "original_form": "관리해서 그나마 괜찮아요",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001860.1.6",
          "form": "name1님은 지병 있나요? 아니면 어떤 병 예방으로 따로 관리하는 게 있나요?",
          "original_form": "&name1&님은 지병 있나요? 아니면 어떤 병 예방으로 따로 관리하는 게 있나요?",
          "speaker_id": "2",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001860.1.7",
          "form": "아 저도 어렸을때 천식 있었는데",
          "original_form": "아 저도 어렸을때 천식 있었는데",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001860.1.8",
          "form": "요즘에는 없어졌어요",
          "original_form": "요즘에는 없어졌어요",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001860.1.9",
          "form": "예방은 잘 안하고",
          "original_form": "예방은 잘 안하고",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001860.1.10",
          "form": "물만 많이 마셔요...ㅎㅎ",
          "original_form": "물만 많이 마셔요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001860.1.11",
          "form": "근데 겨울되면 콜록콜록 하는 게 남들보단 심하게 해요",
          "original_form": "근데 겨울되면 콜록콜록 하는 게 남들보단 심하게 해요",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001860.1.12",
          "form": "요즘 시국은 눈치보이지만.....",
          "original_form": "요즘 시국은 눈치보이지만.....",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001860.1.13",
          "form": "물 많이 마시는 게 좋대요",
          "original_form": "물 많이 마시는 게 좋대요",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001860.1.14",
          "form": "가급적 미지근한 물요",
          "original_form": "가급적 미지근한 물요",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001860.1.15",
          "form": "아... 저는 요로결석이나 그런게 되게 무서워서",
          "original_form": "아... 저는 요로결석이나 그런게 되게 무서워서",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001860.1.16",
          "form": "물 자주마셔요...",
          "original_form": "물 자주마셔요...",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001860.1.17",
          "form": "딴건 몰라도 그건 안걸렸으면 하는 바램에",
          "original_form": "딴건 몰라도 그건 안걸렸으면 하는 바램에",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001860.1.18",
          "form": "요로결석요?",
          "original_form": "요로결석요?",
          "speaker_id": "2",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001860.1.19",
          "form": "와 그거 되게 아프다던데ㅈ",
          "original_form": "와 그거 되게 아프다던데ㅈ",
          "speaker_id": "2",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001860.1.20",
          "form": "아프다던데 진짜",
          "original_form": "아프다던데 진짜",
          "speaker_id": "2",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001860.1.21",
          "form": "물론 전 걸리진 않았지만요",
          "original_form": "물론 전 걸리진 않았지만요",
          "speaker_id": "2",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001860.1.22",
          "form": "저도 다행히도 아직 안걸렸는데",
          "original_form": "저도 다행히도 아직 안걸렸는데",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001860.1.23",
          "form": "죽을때까지 걸리고 싶지 않네요...",
          "original_form": "죽을때까지 걸리고 싶지 않네요...",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001860.1.24",
          "form": "주변에 혹시 요로결석 걸리신 분이라도 있나요?",
          "original_form": "주변에 혹시 요로결석 걸리신 분이라도 있나요?",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100001860.1.25",
          "form": "왜 하필 요로결석이에요?",
          "original_form": "왜 하필 요로결석이에요?",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100001860.1.26",
          "form": "없긴한데 인터넷에 떠돌아다니는 고통순위 있길래 봤더니 요로결석이 되게 아파보이더라구요...",
          "original_form": "없긴한데 인터넷에 떠돌아다니는 고통순위 있길래 봤더니 요로결석이 되게 아파보이더라구요...",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001860.1.27",
          "form": "고통순위 ㅋㅋㅋㅋ 하긴 몸 어디든 아픈 건 곤란하죠",
          "original_form": "고통순위 ㅋㅋㅋㅋ 하긴 몸 어디든 아픈 건 곤란하죠",
          "speaker_id": "2",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001860.1.28",
          "form": "그건 적출(?)해야되고",
          "original_form": "그건 적출(?)해야되고",
          "speaker_id": "2",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001860.1.29",
          "form": "그냥 나이들때까지 안아팠으면 하는 바램이네요",
          "original_form": "그냥 나이들때까지 안아팠으면 하는 바램이네요",
          "speaker_id": "1",
          "time": "20210819 22:57"
        }
      ]
