      "utterance": [
        {
          "id": "MDRW2100044266.1.1",
          "form": "오늘 계속 비가 내리네요",
          "original_form": "오늘 계속 비가 내리네요",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044266.1.2",
          "form": "비내리는 날씨 좋아하시나요?",
          "original_form": "비내리는 날씨 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044266.1.3",
          "form": "저는맑은날이요",
          "original_form": "저는맑은날이요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044266.1.4",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044266.1.5",
          "form": "저도 맑은 날을 좋아하긴 하는데 비오는 날도 운치있어서 좋은 것 같아요",
          "original_form": "저도 맑은 날을 좋아하긴 하는데 비오는 날도 운치있어서 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044266.1.6",
          "form": "물론 제가 실내에 있다는 전제 하에요 ㅋㄱ",
          "original_form": "물론 제가 실내에 있다는 전제 하에요 ㅋㄱ",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044266.1.7",
          "form": "ㅎㅎ알죠알죠",
          "original_form": "ㅎㅎ알죠알죠",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044266.1.8",
          "form": "그리고 유독 비와 관련된 노래들이 좋은 게 많아서",
          "original_form": "그리고 유독 비와 관련된 노래들이 좋은 게 많아서",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044266.1.9",
          "form": "비만 오면 찾아듣게 되지 않나요?",
          "original_form": "비만 오면 찾아듣게 되지 않나요?",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044266.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044266.1.11",
          "form": "빗소리도좋아요",
          "original_form": "빗소리도좋아요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044266.1.12",
          "form": "맞아요 빗소리 듣고 있으면 뭔가 차분해지는 것 같고요",
          "original_form": "맞아요 빗소리 듣고 있으면 뭔가 차분해지는 것 같고요",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044266.1.13",
          "form": "센치해지죵ㅎㅎ",
          "original_form": "센치해지죵ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044266.1.14",
          "form": "그니깐요 ㅋㅋㅋ",
          "original_form": "그니깐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044266.1.15",
          "form": "눈도좋아하세요",
          "original_form": "눈도좋아하세요",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044266.1.16",
          "form": "네 눈 좋아해요",
          "original_form": "네 눈 좋아해요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044266.1.17",
          "form": "저 고향이 남부지방이라 눈이 잘 안왔거든요",
          "original_form": "저 고향이 남부지방이라 눈이 잘 안왔거든요",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044266.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044266.1.19",
          "form": "그래서 눈 내리는 거 보는게 희귀해서 좋아해요",
          "original_form": "그래서 눈 내리는 거 보는게 희귀해서 좋아해요",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044266.1.20",
          "form": "지금은위쪽이신가요",
          "original_form": "지금은위쪽이신가요",
          "speaker_id": "2",
          "time": "20210907 22:28"
