      "utterance": [
        {
          "id": "MDRW2100004772.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004772.1.2",
          "form": "네 ㅎㅎ 갈색 푸들 키우고있어요",
          "original_form": "네 ㅎㅎ 갈색 푸들 키우고있어요",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004772.1.3",
          "form": "우왕 귀엽겠다 ㅎㅎ 저도 두마리키우고 있어요",
          "original_form": "우왕 귀엽겠다 ㅎㅎ 저도 두마리키우고 있어요",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004772.1.4",
          "form": "헉 정말요? 무슨 종 키우세요?",
          "original_form": "헉 정말요? 무슨 종 키우세요?",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004772.1.5",
          "form": "말티즈랑 포메라니안 키우고있어요ㅎㅎ",
          "original_form": "말티즈랑 포메라니안 키우고있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004772.1.6",
          "form": "두마리면 귀엽지만 힘들기두 하시겠어요 ㅋㅋㅋ ㅎㅎ",
          "original_form": "두마리면 귀엽지만 힘들기두 하시겠어요 ㅋㅋㅋ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004772.1.7",
          "form": "와 말티즈 포메라니안 너무 귀엽겠다",
          "original_form": "와 말티즈 포메라니안 너무 귀엽겠다",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004772.1.8",
          "form": "둘이 잘 놀아요??",
          "original_form": "둘이 잘 놀아요??",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004772.1.9",
          "form": "아뇨ㅜㅜ사실 한마리는 언니가 키우다가 사정이생겨서 데려온거라 둘이 서먹해요",
          "original_form": "아뇨ㅜㅜ사실 한마리는 언니가 키우다가 사정이생겨서 데려온거라 둘이 서먹해요",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004772.1.10",
          "form": "근데 그상태로 3년이나 살았어요 ㅋㅋㅋ",
          "original_form": "근데 그상태로 3년이나 살았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004772.1.11",
          "form": "3년살아도 안친해지더라구요",
          "original_form": "3년살아도 안친해지더라구요",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004772.1.12",
          "form": "아고 ㅠㅠ  강아지들이 다 내성적인가봐요 ㅎㅎ",
          "original_form": "아고 ㅠㅠ  강아지들이 다 내성적인가봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004772.1.13",
          "form": "저희집 강아지도 사회성이 좀 결여된거같아서",
          "original_form": "저희집 강아지도 사회성이 좀 결여된거같아서",
          "speaker_id": "2",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004772.1.14",
          "form": "친구 좀 만들어주소 싶네요 ㅠㅜ",
          "original_form": "친구 좀 만들어주소 싶네요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004772.1.15",
          "form": "동네에 산책할때 만나지 않나요? 전 산책할때 친구강아지가 엄청 많아요ㅋㅋㅋ",
          "original_form": "동네에 산책할때 만나지 않나요? 전 산책할때 친구강아지가 엄청 많아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004772.1.16",
          "form": "아 정말요??",
          "original_form": "아 정말요??",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004772.1.17",
          "form": "저희집 강아지가 잘 짖어가지고",
          "original_form": "저희집 강아지가 잘 짖어가지고",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004772.1.18",
          "form": "매번 사람없을때만 지나가긴해요 ㅋㅋㅋ",
          "original_form": "매번 사람없을때만 지나가긴해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004772.1.19",
          "form": "이래선 언제 친구를 만든다고 ㅜㅜ",
          "original_form": "이래선 언제 친구를 만든다고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004772.1.20",
          "form": "나중에 되면 강아지카페도 강지 데리고 가보고싶네요 ㅎㅎ",
          "original_form": "나중에 되면 강아지카페도 강지 데리고 가보고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004772.1.21",
          "form": "강아지카페 가보셨어요?",
          "original_form": "강아지카페 가보셨어요?",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004772.1.22",
          "form": "아뇨 ㅜㅜ  저희강아지가 사회성이 부족해서 그런데는 무서워하더라구요",
          "original_form": "아뇨 ㅜㅜ  저희강아지가 사회성이 부족해서 그런데는 무서워하더라구요",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004772.1.23",
          "form": "가보신적 있으세요?ㅎㅎ",
          "original_form": "가보신적 있으세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004772.1.24",
          "form": "강아지는 안데려가고 저만은 가본 적 있네요 ㅋㅋㅋ",
          "original_form": "강아지는 안데려가고 저만은 가본 적 있네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004772.1.25",
          "form": "강아지카페 애들은 사회성이 정말 좋던데",
          "original_form": "강아지카페 애들은 사회성이 정말 좋던데",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004772.1.26",
          "form": "어떻게 사회성을 길러야할지 잘 모르겠더라구요",
          "original_form": "어떻게 사회성을 길러야할지 잘 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004772.1.27",
          "form": "하찮은 주인 ㅠㅠ...",
          "original_form": "하찮은 주인 ㅠㅠ...",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004772.1.28",
          "form": "저도ㅜㅜ 산책도 자주하는데 사회성은 안길러지더라구요ㅎㅎ",
          "original_form": "저도ㅜㅜ 산책도 자주하는데 사회성은 안길러지더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004772.1.29",
          "form": "나중에 유튜브로 찾아봐야겠어요 ㅋㅋㅋ",
          "original_form": "나중에 유튜브로 찾아봐야겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004772.1.30",
          "form": "사회성 좋은강아지 보면 진짜 신기해요ㅋㅋㅋ",
          "original_form": "사회성 좋은강아지 보면 진짜 신기해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004772.1.31",
          "form": "근데 그런거 봐도 어렵더라구요",
          "original_form": "근데 그런거 봐도 어렵더라구요",
          "speaker_id": "2",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004772.1.32",
          "form": "사람으로치면 엔프피느낌이에요ㅋㅋㅋ",
          "original_form": "사람으로치면 엔프피느낌이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004772.1.33",
          "form": "그니까요 ㅋㅋㅋ",
          "original_form": "그니까요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004772.1.34",
          "form": "주인닮아서 참 내성적인 강지..",
          "original_form": "주인닮아서 참 내성적인 강지..",
          "speaker_id": "2",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004772.1.35",
          "form": "유튜브 봐도 저희애한테 적용시키기가 힘들더라구요 ㅋㅋㅋ",
          "original_form": "유튜브 봐도 저희애한테 적용시키기가 힘들더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004772.1.36",
          "form": "강형욱이 괜히 개대통령이 된게 아닌거 같아요",
          "original_form": "강형욱이 괜히 개대통령이 된게 아닌거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004772.1.37",
          "form": "강아지육아 ㅠㅠ",
          "original_form": "강아지육아 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004772.1.38",
          "form": "진짜 그 분 신기해요ㅋㅋㅋ 문제견 코치해줄때 완전 존경스러워요",
          "original_form": "진짜 그 분 신기해요ㅋㅋㅋ 문제견 코치해줄때 완전 존경스러워요",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004772.1.39",
          "form": "어떻게 그렇게 개의 마음을 잘아는걸까요 ㅋㅋ",
          "original_form": "어떻게 그렇게 개의 마음을 잘아는걸까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004772.1.40",
          "form": "그니까요 ㅋㅋㅋ신기해요",
          "original_form": "그니까요 ㅋㅋㅋ신기해요",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004772.1.41",
          "form": "얘가 배고픈건지 심심한건지이제 그런건",
          "original_form": "얘가 배고픈건지 심심한건지이제 그런건",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004772.1.42",
          "form": "잘보이네요 ㅋㅋㅋ",
          "original_form": "잘보이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004772.1.43",
          "form": "맞아요 ㅋㅋㅋ 얘가 왜 이런행동을 한지 알게되니까 너무좋더라구요 ㅎㅎ",
          "original_form": "맞아요 ㅋㅋㅋ 얘가 왜 이런행동을 한지 알게되니까 너무좋더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004772.1.44",
          "form": "동물은 말을 못하니깐 가끔 왜그러나 싶잖아요ㅜㅜ",
          "original_form": "동물은 말을 못하니깐 가끔 왜그러나 싶잖아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004772.1.45",
          "form": "아 맞아요 ㅠㅠ",
          "original_form": "아 맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004772.1.46",
          "form": "그리고 강아지거 주인을 사랑할때 하는행동",
          "original_form": "그리고 강아지거 주인을 사랑할때 하는행동",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004772.1.47",
          "form": "이런거 찾아보면",
          "original_form": "이런거 찾아보면",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004772.1.48",
          "form": "거의 강지가 하는행동이라",
          "original_form": "거의 강지가 하는행동이라",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004772.1.49",
          "form": "그런거 맞을때도 되게 감동스럽고 귀엽더라구요 ㅠㅠ",
          "original_form": "그런거 맞을때도 되게 감동스럽고 귀엽더라구요 ㅠㅠ",
