      "utterance": [
        {
          "id": "MDRW2100017906.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017906.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017906.1.3",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017906.1.4",
          "form": "아뇨.. 제가 그 주변에 몇없다는 커피안마시는지인입니다",
          "original_form": "아뇨.. 제가 그 주변에 몇없다는 커피안마시는지인입니다",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017906.1.5",
          "form": "오 그러시구나 그럼 친구들 만나면 카페 안가시나요?",
          "original_form": "오 그러시구나 그럼 친구들 만나면 카페 안가시나요?",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017906.1.6",
          "form": "아 카페가도 커피안마시구 딴거먹어요~",
          "original_form": "아 카페가도 커피안마시구 딴거먹어요~",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017906.1.7",
          "form": "커피 안드시는 이유가 있나요?",
          "original_form": "커피 안드시는 이유가 있나요?",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017906.1.8",
          "form": "음 그냥 맛이없어요 ㅋㅋ 아직 저에겐 쓰기만할뿐입니다만",
          "original_form": "음 그냥 맛이없어요 ㅋㅋ 아직 저에겐 쓰기만할뿐입니다만",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017906.1.9",
          "form": "하긴 개인 취향이니까!!",
          "original_form": "하긴 개인 취향이니까!!",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017906.1.10",
          "form": "맞아요 ㅋㅋ 커피 좋아하시나봐요?",
          "original_form": "맞아요 ㅋㅋ 커피 좋아하시나봐요?",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017906.1.11",
          "form": "하루에 한 잔 정도 마시는 것 같아요",
          "original_form": "하루에 한 잔 정도 마시는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017906.1.12",
          "form": "안마셔도 특별히 지장없고요",
          "original_form": "안마셔도 특별히 지장없고요",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017906.1.13",
          "form": "그런데 와이프가 카페를 좋아해서",
          "original_form": "그런데 와이프가 카페를 좋아해서",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017906.1.14",
          "form": "따라다니면서 자주 마셨어요",
          "original_form": "따라다니면서 자주 마셨어요",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017906.1.15",
          "form": "음 술은 어때요? 즐겨 마시나요?",
          "original_form": "음 술은 어때요? 즐겨 마시나요?",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017906.1.16",
          "form": "아하! 그러시구나~",
          "original_form": "아하! 그러시구나~",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017906.1.17",
          "form": "아뇨 ㅋㅋ 술도 안마셔요 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ 술도 안마셔요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017906.1.18",
          "form": "되게 건강한라이프죠?ㅋㅋ",
          "original_form": "되게 건강한라이프죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017906.1.19",
          "form": "좋은거죠",
          "original_form": "좋은거죠",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017906.1.20",
          "form": "건강하게 사는게",
          "original_form": "건강하게 사는게",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017906.1.21",
          "form": "요리는 좋아하세요",
          "original_form": "요리는 좋아하세요",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017906.1.22",
          "form": "네~ 요리잘하진않지만 좋아해요!",
          "original_form": "네~ 요리잘하진않지만 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017906.1.23",
          "form": "가장 최근에 한 요리는?",
          "original_form": "가장 최근에 한 요리는?",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017906.1.24",
          "form": "파스타요~ 바질페스토파스타해먹었어요!",
          "original_form": "파스타요~ 바질페스토파스타해먹었어요!",
          "speaker_id": "1",
