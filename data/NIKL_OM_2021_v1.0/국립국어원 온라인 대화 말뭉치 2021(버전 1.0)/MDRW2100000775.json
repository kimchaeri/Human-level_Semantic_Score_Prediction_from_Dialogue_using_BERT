      "utterance": [
        {
          "id": "MDRW2100000775.1.1",
          "form": "군대는 어디 나오셨나요",
          "original_form": "군대는 어디 나오셨나요",
          "speaker_id": "1",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000775.1.2",
          "form": "저는 군대를 가지않았어요",
          "original_form": "저는 군대를 가지않았어요",
          "speaker_id": "2",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000775.1.3",
          "form": "혹시 군대 가셨나요?",
          "original_form": "혹시 군대 가셨나요?",
          "speaker_id": "2",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000775.1.4",
          "form": "네 군대 다녀왔습니다",
          "original_form": "네 군대 다녀왔습니다",
          "speaker_id": "1",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000775.1.5",
          "form": "육군으로 다녀왔어요",
          "original_form": "육군으로 다녀왔어요",
          "speaker_id": "1",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000775.1.6",
          "form": "어떤 보직을 맡으셨나요",
          "original_form": "어떤 보직을 맡으셨나요",
          "speaker_id": "2",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000775.1.7",
          "form": "작업병을 했습니다.",
          "original_form": "작업병을 했습니다.",
          "speaker_id": "1",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000775.1.8",
          "form": "작업병은 주로 무슨 일을 하나요?",
          "original_form": "작업병은 주로 무슨 일을 하나요?",
          "speaker_id": "2",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000775.1.9",
          "form": "부대 정비를 주로 합니다. 제초작업. 부대 보수.",
          "original_form": "부대 정비를 주로 합니다. 제초작업. 부대 보수.",
          "speaker_id": "1",
          "time": "20210819 19:19"
        },
        {
          "id": "MDRW2100000775.1.10",
          "form": "군대에 있었을 때 가장 힘들었던 점이 뭐에요?",
          "original_form": "군대에 있었을 때 가장 힘들었던 점이 뭐에요?",
          "speaker_id": "2",
          "time": "20210819 19:20"
        },
        {
          "id": "MDRW2100000775.1.11",
          "form": "친구한테 들었는데 삽질하는게 제일 힘들었다 그러던데.. name1님도 그러셨나요",
          "original_form": "친구한테 들었는데 삽질하는게 제일 힘들었다 그러던데.. &name1&님도 그러셨나요",
          "speaker_id": "2",
          "time": "20210819 19:20"
        },
        {
          "id": "MDRW2100000775.1.12",
          "form": "삽질은 재밌었죠 그나마",
          "original_form": "삽질은 재밌었죠 그나마",
          "speaker_id": "1",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000775.1.13",
          "form": "단체생활이라 선임이랑 있을때가 제일 힘들었습니다",
          "original_form": "단체생활이라 선임이랑 있을때가 제일 힘들었습니다",
          "speaker_id": "1",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000775.1.14",
          "form": "요즘엔 과거에 비해 군기잡는게 덜 하다던데 그래도 윗사람이 어려운건 어쩔 수 없나봐요ㅠ",
          "original_form": "요즘엔 과거에 비해 군기잡는게 덜 하다던데 그래도 윗사람이 어려운건 어쩔 수 없나봐요ㅠ",
          "speaker_id": "2",
          "time": "20210819 19:22"
        },
        {
          "id": "MDRW2100000775.1.15",
          "form": "혹시 군대에서 가장 기억에 남는 일이 있나요?",
          "original_form": "혹시 군대에서 가장 기억에 남는 일이 있나요?",
          "speaker_id": "2",
          "time": "20210819 19:22"
        },
        {
          "id": "MDRW2100000775.1.16",
          "form": "말년에 혹한기훈련때 라면먹던게 기억에 남네요",
          "original_form": "말년에 혹한기훈련때 라면먹던게 기억에 남네요",
          "speaker_id": "1",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000775.1.17",
          "form": "그나마 제일 최근 기억이라서 그런지",
          "original_form": "그나마 제일 최근 기억이라서 그런지",
          "speaker_id": "1",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000775.1.18",
          "form": "보통 배치는 강원도로 받나요?",
          "original_form": "보통 배치는 강원도로 받나요?",
          "speaker_id": "2",
          "time": "20210819 19:26"
        },
        {
          "id": "MDRW2100000775.1.19",
          "form": "저는 강원도에 받았습니다",
          "original_form": "저는 강원도에 받았습니다",
          "speaker_id": "1",
          "time": "20210819 19:28"
        },
        {
          "id": "MDRW2100000775.1.20",
          "form": "강원도쪽이 제일 많이 가지 않을까요?",
          "original_form": "강원도쪽이 제일 많이 가지 않을까요?",
