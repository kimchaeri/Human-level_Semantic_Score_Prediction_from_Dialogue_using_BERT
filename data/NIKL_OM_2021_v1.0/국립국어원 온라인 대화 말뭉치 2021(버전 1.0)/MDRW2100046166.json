          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046166.1.15",
          "form": "낙지 정말 좋아해요 ㅎㅎ",
          "original_form": "낙지 정말 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046166.1.16",
          "form": "초장에 먹고 싶어요 ㅠㅠ",
          "original_form": "초장에 먹고 싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046166.1.17",
          "form": "ㅎㅎㅎㅎㅎㅇ 낙지 초장에 찍어드세요??",
          "original_form": "ㅎㅎㅎㅎㅎㅇ 낙지 초장에 찍어드세요??",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046166.1.18",
          "form": "낙지는 참기름이요!!! ㅋㅋㅋ",
          "original_form": "낙지는 참기름이요!!! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046166.1.19",
          "form": "그렇죠 ㅎㅎ",
          "original_form": "그렇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046166.1.20",
          "form": "낙지는 참기름이죠ㅎㅎ",
          "original_form": "낙지는 참기름이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046166.1.21",
          "form": "저는 회를 간장에 찍어 먹는 편이예요",
          "original_form": "저는 회를 간장에 찍어 먹는 편이예요",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046166.1.22",
          "form": "와사비 가득해서 ㅎㅎ",
          "original_form": "와사비 가득해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046166.1.23",
          "form": "우아 저도요!!! 회 초장 다 좋아해욬ㅋㅋㅋㅋㅋ",
          "original_form": "우아 저도요!!! 회 초장 다 좋아해욬ㅋㅋㅋㅋㅋ",
