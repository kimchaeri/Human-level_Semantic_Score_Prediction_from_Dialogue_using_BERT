      "utterance": [
        {
          "id": "MDRW2100046166.1.1",
          "form": "늘 다른 거 같은데",
          "original_form": "늘 다른 거 같은데",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046166.1.2",
          "form": "고기 먹을 때도 있고",
          "original_form": "고기 먹을 때도 있고",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046166.1.3",
          "form": "회 먹을 때도 있구요",
          "original_form": "회 먹을 때도 있구요",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046166.1.4",
          "form": "간단하게 치킨을 먹기도 하죠ㅎㅎ",
          "original_form": "간단하게 치킨을 먹기도 하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046166.1.5",
          "form": "우아 회 ㅠㅠㅠ 너무 좋아요 ㅠㅠ",
          "original_form": "우아 회 ㅠㅠㅠ 너무 좋아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046166.1.6",
          "form": "연어 좋아하세요?",
          "original_form": "연어 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046166.1.7",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046166.1.8",
          "form": "며칠 전에 신랑이랑",
          "original_form": "며칠 전에 신랑이랑",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046166.1.9",
          "form": "오징어회를 시켜 먹었는데",
          "original_form": "오징어회를 시켜 먹었는데",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046166.1.10",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046166.1.11",
          "form": "우아 오징어회 !!",
          "original_form": "우아 오징어회 !!",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046166.1.12",
          "form": "산낙지도 좋아하세요?",
          "original_form": "산낙지도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046166.1.13",
          "form": "그럼요",
          "original_form": "그럼요",
