      "utterance": [
        {
          "id": "MDRW2100038686.1.1",
          "form": "여행가고싶죠",
          "original_form": "여행가고싶죠",
          "speaker_id": "1",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100038686.1.2",
          "form": "여행가신 적 있으신가요?",
          "original_form": "여행가신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100038686.1.3",
          "form": "국내 속초만 한번 다녀왔어요퓨",
          "original_form": "국내 속초만 한번 다녀왔어요퓨",
          "speaker_id": "1",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100038686.1.4",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100038686.1.5",
          "form": "저도 여행은 춘천이랑 경주밖에 못가봤어요ㅠㅠ",
          "original_form": "저도 여행은 춘천이랑 경주밖에 못가봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100038686.1.6",
          "form": "속초에서 뭘 보셨나요?",
          "original_form": "속초에서 뭘 보셨나요?",
          "speaker_id": "2",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100038686.1.7",
          "form": "바다보고~놀았어요~딱히 할거리는 많지는 않더라구요ㅠㅠ",
          "original_form": "바다보고~놀았어요~딱히 할거리는 많지는 않더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100038686.1.8",
          "form": "그렇군요\r\n속초에서 뭐 드셨나요?",
          "original_form": "그렇군요\r\n속초에서 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100038686.1.9",
          "form": "회냉면 먹었는데 맛있더라구요",
          "original_form": "회냉면 먹었는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100038686.1.10",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100038686.1.11",
          "form": "회냉면은 먹어봤는데 속초에서 회냉면은 더 맛있나요?",
          "original_form": "회냉면은 먹어봤는데 속초에서 회냉면은 더 맛있나요?",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100038686.1.12",
          "form": "ㅋㅋㅋ배고파서 그랬는지 맛있던데요?",
          "original_form": "ㅋㅋㅋ배고파서 그랬는지 맛있던데요?",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100038686.1.13",
          "form": "오 속초한번 가보고 싶네요",
          "original_form": "오 속초한번 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100038686.1.14",
          "form": "함 가보세요!!!",
          "original_form": "함 가보세요!!!",
          "speaker_id": "1",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100038686.1.15",
          "form": "어디 여행 다녀오신곳 있으세요?",
          "original_form": "어디 여행 다녀오신곳 있으세요?",
          "speaker_id": "1",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100038686.1.16",
          "form": "저는 경주랑 춘천이요",
          "original_form": "저는 경주랑 춘천이요",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100038686.1.17",
          "form": "어디가 제일 좋으셨어요?",
          "original_form": "어디가 제일 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210902 22:45"
