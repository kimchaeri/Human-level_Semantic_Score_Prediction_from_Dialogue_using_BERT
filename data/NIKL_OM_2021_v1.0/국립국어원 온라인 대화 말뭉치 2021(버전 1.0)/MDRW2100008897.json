      "utterance": [
        {
          "id": "MDRW2100008897.1.1",
          "form": "오늘 앱 하나 새로 가입해서 무료로 사과샀어 ㅎㅎ",
          "original_form": "오늘 앱 하나 새로 가입해서 무료로 사과샀어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008897.1.2",
          "form": "시장에 가보니 아오리 사과 많더만",
          "original_form": "시장에 가보니 아오리 사과 많더만",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008897.1.3",
          "form": "요즘 철이니까 ㅎㅎ",
          "original_form": "요즘 철이니까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008897.1.4",
          "form": "색시가 산 것도 아오리야?",
          "original_form": "색시가 산 것도 아오리야?",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008897.1.5",
          "form": "아니 세척사과라고 빨간 사과던데",
          "original_form": "아니 세척사과라고 빨간 사과던데",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008897.1.6",
          "form": "근데 그걸 어떻게 무료로 샀대? 비쌀텐데",
          "original_form": "근데 그걸 어떻게 무료로 샀대? 비쌀텐데",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008897.1.7",
          "form": "앱 가입 기념으로 주던데 ㅎㅎ 다음주에 도착할 것 같아",
          "original_form": "앱 가입 기념으로 주던데 ㅎㅎ 다음주에 도착할 것 같아",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008897.1.8",
          "form": "부디 맛있는 사과가 오길",
          "original_form": "부디 맛있는 사과가 오길",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008897.1.9",
          "form": "그래야지~ 상품평 보니 사람들이 다 맛있다고 했어 ㅎㅎ",
          "original_form": "그래야지~ 상품평 보니 사람들이 다 맛있다고 했어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008897.1.10",
          "form": "사과가 아침에 먹으면 건강해지는 기분이야",
          "original_form": "사과가 아침에 먹으면 건강해지는 기분이야",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008897.1.11",
          "form": "ㅇㅇ 건강을 위해서라도 잘 챙겨먹어야겠어 ㅎㅎ",
          "original_form": "ㅇㅇ 건강을 위해서라도 잘 챙겨먹어야겠어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008897.1.12",
          "form": "아침에 먹는 사과는 금사과",
          "original_form": "아침에 먹는 사과는 금사과",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008897.1.13",
          "form": "ㅎㅎ 소화도 잘되고",
          "original_form": "ㅎㅎ 소화도 잘되고",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008897.1.14",
          "form": "맛도 좋지",
          "original_form": "맛도 좋지",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008897.1.15",
          "form": "아까 시장 가니까 복숭아 가격 좋던데",
          "original_form": "아까 시장 가니까 복숭아 가격 좋던데",
