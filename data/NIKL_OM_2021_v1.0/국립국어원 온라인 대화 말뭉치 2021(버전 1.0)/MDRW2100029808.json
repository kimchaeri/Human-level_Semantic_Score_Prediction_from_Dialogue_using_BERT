        {
          "id": "MDRW2100029808.1.1",
          "form": "음료수좋아하세요?",
          "original_form": "음료수좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100029808.1.2",
          "form": "흑당밀크티 사랑합니다ㅎ",
          "original_form": "흑당밀크티 사랑합니다ㅎ",
          "speaker_id": "1",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100029808.1.3",
          "form": "님은욪?",
          "original_form": "님은욪?",
          "speaker_id": "1",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100029808.1.4",
          "form": "오 저도요ㅋ",
          "original_form": "오 저도요ㅋ",
          "speaker_id": "2",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100029808.1.5",
          "form": "평소엔뭐 거의 커피죠ㅜ",
          "original_form": "평소엔뭐 거의 커피죠ㅜ",
          "speaker_id": "2",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100029808.1.6",
          "form": "아메리카노 주로 드시나요?",
          "original_form": "아메리카노 주로 드시나요?",
          "speaker_id": "1",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100029808.1.7",
          "form": "아뇨ㅋ 캬라멜 마까아또요ㅋ",
          "original_form": "아뇨ㅋ 캬라멜 마까아또요ㅋ",
          "speaker_id": "2",
          "time": "20210903 15:16"
        },
        {
          "id": "MDRW2100029808.1.8",
          "form": "아ㅋㅋ 단거 잘 드시는군요ㅋㅋ",
          "original_form": "아ㅋㅋ 단거 잘 드시는군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:18"
        },
        {
          "id": "MDRW2100029808.1.9",
          "form": "좋아하죠ㄱ 스트레스땜시ㅋ",
          "original_form": "좋아하죠ㄱ 스트레스땜시ㅋ",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100029808.1.10",
          "form": "아ㅜㅇㅜ 맞아요",
          "original_form": "아ㅜㅇㅜ 맞아요",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029808.1.11",
          "form": "전 단거 잘 못 먹는데도 회사선 입맛이 변해요",
          "original_form": "전 단거 잘 못 먹는데도 회사선 입맛이 변해요",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029808.1.12",
          "form": "당 떨어져서",
          "original_form": "당 떨어져서",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029808.1.13",
          "form": "ㅋㅋㅋ졸리기도하고ㅜ",
          "original_form": "ㅋㅋㅋ졸리기도하고ㅜ",
          "speaker_id": "2",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029808.1.14",
          "form": "커피 마시면 실제 잠이 좀 가시나요?",
          "original_form": "커피 마시면 실제 잠이 좀 가시나요?",
          "speaker_id": "1",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029808.1.15",
          "form": "아ㅜ 퇴근하느라 이제 봤네요ㅜ",
          "original_form": "아ㅜ 퇴근하느라 이제 봤네요ㅜ",
          "speaker_id": "2",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100029808.1.16",
          "form": "잠이 안가셔요 저는ㅋㅋㅋㅋ",
          "original_form": "잠이 안가셔요 저는ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100029808.1.17",
          "form": "ㅎㅎ 실은 저도 그래요",
          "original_form": "ㅎㅎ 실은 저도 그래요",
          "speaker_id": "1",
          "time": "20210903 19:00"
        },
        {
          "id": "MDRW2100029808.1.18",
          "form": "카페인 효과는 어디 갔죠ㅜ",
          "original_form": "카페인 효과는 어디 갔죠ㅜ",
