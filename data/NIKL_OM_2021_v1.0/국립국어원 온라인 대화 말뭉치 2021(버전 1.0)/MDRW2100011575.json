      "utterance": [
        {
          "id": "MDRW2100011575.1.1",
          "form": "크게 아파본적있으신가요",
          "original_form": "크게 아파본적있으신가요",
          "speaker_id": "1",
          "time": "20210824 14:07"
        },
        {
          "id": "MDRW2100011575.1.2",
          "form": "전에 수두 걸렸을 때랑 장염 걸렸을 때가 제일 아파 봤던 것 같네요",
          "original_form": "전에 수두 걸렸을 때랑 장염 걸렸을 때가 제일 아파 봤던 것 같네요",
          "speaker_id": "2",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100011575.1.3",
          "form": "말만들어도 제가다 아프네요",
          "original_form": "말만들어도 제가다 아프네요",
          "speaker_id": "1",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011575.1.4",
          "form": "저는 한번있었는데",
          "original_form": "저는 한번있었는데",
          "speaker_id": "1",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011575.1.5",
          "form": "독감걸렸을때 정말 죽는줄알았어요",
          "original_form": "독감걸렸을때 정말 죽는줄알았어요",
          "speaker_id": "1",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011575.1.6",
          "form": "아 독감 정말 걸리면 힘들죠...",
          "original_form": "아 독감 정말 걸리면 힘들죠...",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011575.1.7",
          "form": "밥도 제대로 못 먹고...",
          "original_form": "밥도 제대로 못 먹고...",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011575.1.8",
          "form": "새벽에 증상이 나타나서 고생했네요",
          "original_form": "새벽에 증상이 나타나서 고생했네요",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011575.1.9",
          "form": "그때 혼자계셨나요?",
          "original_form": "그때 혼자계셨나요?",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011575.1.10",
          "form": "아뇨 어머니가 저 대리고 응급실 갔어요",
          "original_form": "아뇨 어머니가 저 대리고 응급실 갔어요",
          "speaker_id": "1",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011575.1.11",
          "form": "헐 두분 다 고생하셨네요... 며칠동안 앓던가요?",
          "original_form": "헐 두분 다 고생하셨네요... 며칠동안 앓던가요?",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011575.1.12",
          "form": "타미플루 먹으니까 한시간만에 안정되었어요ㅎㅎ",
          "original_form": "타미플루 먹으니까 한시간만에 안정되었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011575.1.13",
          "form": "아 다행이네요ㅎㅎ 전 수두 걸렸을 때 너무 간지러워서 장갑과 양말을 착용하고 잔 기억이 있어요ㅋㅋㅋㅋ 약도 무용지물 이었죠...",
          "original_form": "아 다행이네요ㅎㅎ 전 수두 걸렸을 때 너무 간지러워서 장갑과 양말을 착용하고 잔 기억이 있어요ㅋㅋㅋㅋ 약도 무용지물 이었죠...",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011575.1.14",
          "form": "엄청 힘드셨겠어요",
          "original_form": "엄청 힘드셨겠어요",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011575.1.15",
          "form": "아토피 앓은적이있는데",
          "original_form": "아토피 앓은적이있는데",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011575.1.16",
          "form": "가려운게 너무 고통스럽더라고요",
          "original_form": "가려운게 너무 고통스럽더라고요",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011575.1.17",
          "form": "어우 아토피는 만성 질환이니 더 힘드셨겠어요",
          "original_form": "어우 아토피는 만성 질환이니 더 힘드셨겠어요",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011575.1.18",
          "form": "어렸을때 잠깐 그래서 다행이었어요",
          "original_form": "어렸을때 잠깐 그래서 다행이었어요",
          "speaker_id": "1",
          "time": "20210824 14:26"
        }
