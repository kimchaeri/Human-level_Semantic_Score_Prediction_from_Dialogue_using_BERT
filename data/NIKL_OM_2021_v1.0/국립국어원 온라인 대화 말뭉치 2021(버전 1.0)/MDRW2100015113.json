      "utterance": [
        {
          "id": "MDRW2100015113.1.1",
          "form": "콘서트는 무슨 콘서트 가보셨나요?",
          "original_form": "콘서트는 무슨 콘서트 가보셨나요?",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015113.1.2",
          "form": "저 아이돌..ㅎ",
          "original_form": "저 아이돌..ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015113.1.3",
          "form": "엑소 태연 슈퍼주니어 동방신기",
          "original_form": "엑소 태연 슈퍼주니어 동방신기",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015113.1.4",
          "form": "에셈빠입니다",
          "original_form": "에셈빠입니다",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015113.1.5",
          "form": "와우.....",
          "original_form": "와우.....",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015113.1.6",
          "form": "전통있는 덕후셨잖아",
          "original_form": "전통있는 덕후셨잖아",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015113.1.7",
          "form": "티켓팅 짬이 상당하시겠어요",
          "original_form": "티켓팅 짬이 상당하시겠어요",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015113.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아닙니다 늘 변두리에서 봤러요ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아닙니다 늘 변두리에서 봤러요ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015113.1.9",
          "form": "정말 똥손이라 공연 갈 수 있게 된 것도 최근 2년?",
          "original_form": "정말 똥손이라 공연 갈 수 있게 된 것도 최근 2년?",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015113.1.10",
          "form": "어떤 콘서트 가보셨나요??",
          "original_form": "어떤 콘서트 가보셨나요??",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015113.1.11",
          "form": "앗 저는 박정현씨 연말콘서트 가봤어요",
          "original_form": "앗 저는 박정현씨 연말콘서트 가봤어요",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015113.1.12",
          "form": "대박 고막 괜찮으신가요??",
          "original_form": "대박 고막 괜찮으신가요??",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015113.1.13",
          "form": "음향시설이 조금 아쉬웠지만....진짜",
          "original_form": "음향시설이 조금 아쉬웠지만....진짜",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015113.1.14",
          "form": "성량도 어마무시하시구",
          "original_form": "성량도 어마무시하시구",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015113.1.15",
          "form": "눈물 줄줄 흘렸어요",
          "original_form": "눈물 줄줄 흘렸어요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015113.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015113.1.17",
          "form": "대박이었을 것 같아요",
          "original_form": "대박이었을 것 같아요",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015113.1.18",
          "form": "저는 가창력 제일 쩔었던 가수 공연이 케이윌?",
          "original_form": "저는 가창력 제일 쩔었던 가수 공연이 케이윌?",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015113.1.19",
          "form": "아 그분!",
          "original_form": "아 그분!",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015113.1.20",
          "form": "언제다녀오셨나요?",
          "original_form": "언제다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015113.1.21",
          "form": "2019년 연말이요!",
          "original_form": "2019년 연말이요!",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015113.1.22",
          "form": "크리스마스 이브였나 그랬던 것 같아요",
          "original_form": "크리스마스 이브였나 그랬던 것 같아요",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015113.1.23",
          "form": "분위기 좋았겠네요!",
          "original_form": "분위기 좋았겠네요!",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015113.1.24",
          "form": "네 좋더라구요 ㅎㅎ",
          "original_form": "네 좋더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015113.1.25",
          "form": "발라드를 좋아하시는 편이신가요??",
          "original_form": "발라드를 좋아하시는 편이신가요??",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015113.1.26",
          "form": "저는 좋아하는편은 아니예요",
          "original_form": "저는 좋아하는편은 아니예요",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015113.1.27",
          "form": "아는노래도 많이 없구요",
          "original_form": "아는노래도 많이 없구요",
          "speaker_id": "1",
