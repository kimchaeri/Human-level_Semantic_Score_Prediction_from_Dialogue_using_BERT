      "utterance": [
        {
          "id": "MDRW2100046282.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 13:11"
        },
        {
          "id": "MDRW2100046282.1.2",
          "form": "안녕하세요~점심식사하셨나요?",
          "original_form": "안녕하세요~점심식사하셨나요?",
          "speaker_id": "2",
          "time": "20211015 13:11"
        },
        {
          "id": "MDRW2100046282.1.3",
          "form": "네. 먹었습니다.",
          "original_form": "네. 먹었습니다.",
          "speaker_id": "1",
          "time": "20211015 13:11"
        },
        {
          "id": "MDRW2100046282.1.4",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20211015 13:12"
        },
        {
          "id": "MDRW2100046282.1.5",
          "form": "회사 급식 먹었습니다..",
          "original_form": "회사 급식 먹었습니다..",
          "speaker_id": "1",
          "time": "20211015 13:12"
        },
        {
          "id": "MDRW2100046282.1.6",
          "form": "점심 식사 하셨나요",
          "original_form": "점심 식사 하셨나요",
          "speaker_id": "1",
          "time": "20211015 13:12"
        },
        {
          "id": "MDRW2100046282.1.7",
          "form": "넵 저는 오늘 짬뽕먹었습니다!!",
          "original_form": "넵 저는 오늘 짬뽕먹었습니다!!",
          "speaker_id": "2",
          "time": "20211015 13:12"
        },
        {
          "id": "MDRW2100046282.1.8",
          "form": "짬뽕 좋아하시나요?",
          "original_form": "짬뽕 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211015 13:12"
        },
        {
          "id": "MDRW2100046282.1.9",
          "form": "짬뽕 좋아하지요",
          "original_form": "짬뽕 좋아하지요",
          "speaker_id": "1",
          "time": "20211015 13:13"
        },
        {
          "id": "MDRW2100046282.1.10",
          "form": "중국집가면 짜장면vs짬뽕 뭐고르시나요?",
          "original_form": "중국집가면 짜장면vs짬뽕 뭐고르시나요?",
          "speaker_id": "2",
          "time": "20211015 13:13"
        },
        {
          "id": "MDRW2100046282.1.11",
          "form": "날씨에 따라 다르겠지요..",
          "original_form": "날씨에 따라 다르겠지요..",
          "speaker_id": "1",
          "time": "20211015 13:14"
        },
        {
          "id": "MDRW2100046282.1.12",
          "form": "비가 오거나 추운 날에는 짬뽕입니다",
          "original_form": "비가 오거나 추운 날에는 짬뽕입니다",
          "speaker_id": "1",
          "time": "20211015 13:14"
        },
        {
          "id": "MDRW2100046282.1.13",
          "form": "무엇을 더 좋아하시나요",
          "original_form": "무엇을 더 좋아하시나요",
          "speaker_id": "1",
          "time": "20211015 13:14"
        },
        {
          "id": "MDRW2100046282.1.14",
          "form": "저도 비오는날에는 짬뽕 선택합니다!",
          "original_form": "저도 비오는날에는 짬뽕 선택합니다!",
          "speaker_id": "2",
          "time": "20211015 13:14"
        },
        {
          "id": "MDRW2100046282.1.15",
          "form": "결정을 영 못내릴때는 짬짜면 먹어요 ㅎㅎ",
          "original_form": "결정을 영 못내릴때는 짬짜면 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 13:14"
        },
        {
          "id": "MDRW2100046282.1.16",
          "form": "좋은 선택이십니다 ^^",
          "original_form": "좋은 선택이십니다 ^^",
          "speaker_id": "1",
          "time": "20211015 13:15"
        },
        {
          "id": "MDRW2100046282.1.17",
          "form": "오늘 금요일인데 오후에 약속 있으신가요?",
          "original_form": "오늘 금요일인데 오후에 약속 있으신가요?",
          "speaker_id": "2",
          "time": "20211015 13:15"
        },
        {
          "id": "MDRW2100046282.1.18",
          "form": "코로나 때문에 어디를 갈 수가 없네요...",
          "original_form": "코로나 때문에 어디를 갈 수가 없네요...",
          "speaker_id": "1",
          "time": "20211015 13:16"
        },
        {
          "id": "MDRW2100046282.1.19",
          "form": "가족들과 함께 하겠지요",
          "original_form": "가족들과 함께 하겠지요",
          "speaker_id": "1",
          "time": "20211015 13:16"
        },
        {
          "id": "MDRW2100046282.1.20",
          "form": "약속 있으신가요?",
          "original_form": "약속 있으신가요?",
          "speaker_id": "1",
          "time": "20211015 13:16"
        },
        {
          "id": "MDRW2100046282.1.21",
          "form": "저는 오늘은 약속은 없고 주말에 친구들 만나기로했어요! ",
          "original_form": "저는 오늘은 약속은 없고 주말에 친구들 만나기로했어요! ",
          "speaker_id": "2",
          "time": "20211015 13:17"
        },
        {
          "id": "MDRW2100046282.1.22",
          "form": "친구들 두명이 백신접종 2차까지 완료했거든요 ㅎㅎ",
          "original_form": "친구들 두명이 백신접종 2차까지 완료했거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 13:17"
        },
        {
          "id": "MDRW2100046282.1.23",
          "form": "좋네요 ^^ ㅎㅎ",
          "original_form": "좋네요 ^^ ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 13:17"
        },
        {
          "id": "MDRW2100046282.1.24",
          "form": "무엇을 하시나요",
          "original_form": "무엇을 하시나요",
          "speaker_id": "1",
          "time": "20211015 13:17"
        },
        {
          "id": "MDRW2100046282.1.25",
          "form": "그냥 친구들 오랜만에 만나서 술마시면서 근황토크 할 것 같아요",
          "original_form": "그냥 친구들 오랜만에 만나서 술마시면서 근황토크 할 것 같아요",
          "speaker_id": "2",
          "time": "20211015 13:18"
        },
        {
          "id": "MDRW2100046282.1.26",
          "form": "친구들 만나면 주로 뭐하시나요?",
          "original_form": "친구들 만나면 주로 뭐하시나요?",
          "speaker_id": "2",
          "time": "20211015 13:18"
        },
        {
          "id": "MDRW2100046282.1.27",
          "form": "나이를 먹으니 친구를 만날 기회가 많지가 않네요.....",
          "original_form": "나이를 먹으니 친구를 만날 기회가 많지가 않네요.....",
          "speaker_id": "1",
          "time": "20211015 13:19"
        },
        {
          "id": "MDRW2100046282.1.28",
          "form": "다들 가족이 우선이지요 ^^",
          "original_form": "다들 가족이 우선이지요 ^^",
          "speaker_id": "1",
          "time": "20211015 13:19"
        },
        {
          "id": "MDRW2100046282.1.29",
          "form": "그렇죠 가족이랑 함께 살다보면 가족이 우선시 되는거겠죠..?",
          "original_form": "그렇죠 가족이랑 함께 살다보면 가족이 우선시 되는거겠죠..?",
          "speaker_id": "2",
          "time": "20211015 13:21"
        },
        {
          "id": "MDRW2100046282.1.30",
          "form": "오늘 금요일인데 하루 잘 마무리하시고 좋은 주말 보내시길 바랄게요!",
          "original_form": "오늘 금요일인데 하루 잘 마무리하시고 좋은 주말 보내시길 바랄게요!",
