      "utterance": [
        {
          "id": "MDRW2100020338.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 03:06"
        },
        {
          "id": "MDRW2100020338.1.2",
          "form": "요즘 날이 많이 선선해졌죠",
          "original_form": "요즘 날이 많이 선선해졌죠",
          "speaker_id": "1",
          "time": "20210903 03:06"
        },
        {
          "id": "MDRW2100020338.1.3",
          "form": "안녕하세요\n네 요즘 날이 많이 시원해졌어요",
          "original_form": "안녕하세요\n네 요즘 날이 많이 시원해졌어요",
          "speaker_id": "2",
          "time": "20210903 03:06"
        },
        {
          "id": "MDRW2100020338.1.4",
          "form": "오늘외출하시나요??",
          "original_form": "오늘외출하시나요??",
          "speaker_id": "1",
          "time": "20210903 03:06"
        },
        {
          "id": "MDRW2100020338.1.5",
          "form": "반팔입을지 긴팔입을지 고민입니다",
          "original_form": "반팔입을지 긴팔입을지 고민입니다",
          "speaker_id": "1",
          "time": "20210903 03:06"
        },
        {
          "id": "MDRW2100020338.1.6",
          "form": "네 외출해요",
          "original_form": "네 외출해요",
          "speaker_id": "2",
          "time": "20210903 03:07"
        },
        {
          "id": "MDRW2100020338.1.7",
          "form": "어떤거 입으실거에요?!ㅎㅎ",
          "original_form": "어떤거 입으실거에요?!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 03:07"
        },
        {
          "id": "MDRW2100020338.1.8",
          "form": "저는 음 반팔과 긴팔 사이의 옷이요",
          "original_form": "저는 음 반팔과 긴팔 사이의 옷이요",
          "speaker_id": "2",
          "time": "20210903 03:07"
        },
        {
          "id": "MDRW2100020338.1.9",
          "form": "ㅋㅋㅋㅋ현명하시네요!",
          "original_form": "ㅋㅋㅋㅋ현명하시네요!",
          "speaker_id": "1",
          "time": "20210903 03:07"
        },
        {
          "id": "MDRW2100020338.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 요즘같은 날씨에는 그런 옷을 즐겨 입어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 요즘같은 날씨에는 그런 옷을 즐겨 입어요",
          "speaker_id": "2",
          "time": "20210903 03:08"
        },
        {
          "id": "MDRW2100020338.1.11",
          "form": "아니면 반팔에 가디건을 걸치구요",
          "original_form": "아니면 반팔에 가디건을 걸치구요",
          "speaker_id": "2",
          "time": "20210903 03:08"
        },
        {
          "id": "MDRW2100020338.1.12",
          "form": "오늘 비가올까요?!",
          "original_form": "오늘 비가올까요?!",
          "speaker_id": "1",
          "time": "20210903 03:08"
        },
        {
          "id": "MDRW2100020338.1.13",
          "form": "비는 모르겠네요 \n지역마다 다르겠죠?",
          "original_form": "비는 모르겠네요 \n지역마다 다르겠죠?",
          "speaker_id": "2",
          "time": "20210903 03:08"
        },
        {
          "id": "MDRW2100020338.1.14",
          "form": "일기예보는 요즘 거의 비표신데",
          "original_form": "일기예보는 요즘 거의 비표신데",
          "speaker_id": "1",
          "time": "20210903 03:09"
        },
        {
          "id": "MDRW2100020338.1.15",
          "form": "잘안맞아서 들고다니기 귀찮네요",
          "original_form": "잘안맞아서 들고다니기 귀찮네요",
          "speaker_id": "1",
          "time": "20210903 03:09"
        },
        {
          "id": "MDRW2100020338.1.16",
          "form": "일기예보를 믿을 수 없어서.....",
