      "utterance": [
        {
          "id": "MDRW2100033545.1.1",
          "form": "결혼에 대해 어떻게 생각하세요?",
          "original_form": "결혼에 대해 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210824 12:57"
        },
        {
          "id": "MDRW2100033545.1.2",
          "form": "결혼은 선택\r\n연애는 필수",
          "original_form": "결혼은 선택\r\n연애는 필수",
          "speaker_id": "2",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100033545.1.3",
          "form": "다시 돌아간다면\r\n평생 연애만ㅋㅋ",
          "original_form": "다시 돌아간다면\r\n평생 연애만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100033545.1.4",
          "form": "ㅋㅋㅋ 그정도인가요?",
          "original_form": "ㅋㅋㅋ 그정도인가요?",
          "speaker_id": "1",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100033545.1.5",
          "form": "결혼은 단점이 많나요?",
          "original_form": "결혼은 단점이 많나요?",
          "speaker_id": "1",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100033545.1.6",
          "form": "희생해야할것들이 많아지죠:)",
          "original_form": "희생해야할것들이 많아지죠:)",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100033545.1.7",
          "form": "name1님은 아직 미혼이신가요?",
          "original_form": "&name1&님은 아직 미혼이신가요?",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100033545.1.8",
          "form": "네네 맞아요ㅠㅠ",
          "original_form": "네네 맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100033545.1.9",
          "form": "결혼은 참 어려운것같네요",
          "original_form": "결혼은 참 어려운것같네요",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100033545.1.10",
          "form": "그럼 지금 연애중이세요?",
          "original_form": "그럼 지금 연애중이세요?",
          "speaker_id": "2",
          "time": "20210824 13:17"
        },
        {
          "id": "MDRW2100033545.1.11",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100033545.1.12",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100033545.1.13",
          "form": "요즘은 연애도힘든것같아요",
          "original_form": "요즘은 연애도힘든것같아요",
          "speaker_id": "1",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100033545.1.14",
          "form": "그럼 이상형은 어떻게 되세요?",
          "original_form": "그럼 이상형은 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033545.1.15",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100033545.1.16",
          "form": "내적인게 진짜중요한것같아요",
          "original_form": "내적인게 진짜중요한것같아요",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100033545.1.17",
          "form": "내적인 부분을 중시하시는군용",
          "original_form": "내적인 부분을 중시하시는군용",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033545.1.18",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033545.1.19",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033545.1.20",
          "form": "ㅋㅋ저도 그렇게 생각합니다",
          "original_form": "ㅋㅋ저도 그렇게 생각합니다",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033545.1.21",
          "form": "name1님은 자만추를 선호하세요?\r\n아님 소개팅을 선호하세요?",
          "original_form": "&name1&님은 자만추를 선호하세요?\r\n아님 소개팅을 선호하세요?",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033545.1.22",
          "form": "전 자만추요",
          "original_form": "전 자만추요",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100033545.1.23",
          "form": "근데 그게안되네요ㅋㅋㅋㅋ",
          "original_form": "근데 그게안되네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100033545.1.24",
          "form": "저도 자만추요ㅋㅋ",
          "original_form": "저도 자만추요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033545.1.25",
          "form": "코시국이라 만남 자체가\r\n쉽지 않을거 같아요ㅜ",
          "original_form": "코시국이라 만남 자체가\r\n쉽지 않을거 같아요ㅜ",
