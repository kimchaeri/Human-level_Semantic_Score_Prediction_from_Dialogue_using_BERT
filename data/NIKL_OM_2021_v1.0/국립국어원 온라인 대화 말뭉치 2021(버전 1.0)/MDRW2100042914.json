      "utterance": [
        {
          "id": "MDRW2100042914.1.1",
          "form": "아 그거 유명하죠!",
          "original_form": "아 그거 유명하죠!",
          "speaker_id": "2",
          "time": "20210907 11:20"
        },
        {
          "id": "MDRW2100042914.1.2",
          "form": "칼로리도 낮공",
          "original_form": "칼로리도 낮공",
          "speaker_id": "1",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100042914.1.3",
          "form": "그것도 돌체처럼 쾌변에 효과가 있다고?",
          "original_form": "그것도 돌체처럼 쾌변에 효과가 있다고?",
          "speaker_id": "2",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100042914.1.4",
          "form": "돌체만하진 않아요 ㅋㅋㅋ",
          "original_form": "돌체만하진 않아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:23"
        },
        {
          "id": "MDRW2100042914.1.5",
          "form": "근데 라떼류는 다 효과가 잇긴라더라구요.전 ㅋㅋ",
          "original_form": "근데 라떼류는 다 효과가 잇긴라더라구요.전 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:23"
        },
        {
          "id": "MDRW2100042914.1.6",
          "form": "ㅋㅋ스벅이 진짜 제일 만만하죠? 가까운 곳에 있으니...",
          "original_form": "ㅋㅋ스벅이 진짜 제일 만만하죠? 가까운 곳에 있으니...",
          "speaker_id": "2",
          "time": "20210907 11:24"
        },
        {
          "id": "MDRW2100042914.1.7",
          "form": "다른 카페도 좋아하시나요?",
          "original_form": "다른 카페도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 11:24"
        },
        {
          "id": "MDRW2100042914.1.8",
          "form": "할리스 바닐라도 조아하궁",
          "original_form": "할리스 바닐라도 조아하궁",
          "speaker_id": "1",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042914.1.9",
          "form": "투썸은 디저트용 ㅋㅋ",
          "original_form": "투썸은 디저트용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042914.1.10",
          "form": "커피빈도 바닐라나 헤이즐넛.",
          "original_form": "커피빈도 바닐라나 헤이즐넛.",
          "speaker_id": "1",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042914.1.11",
          "form": "폴바셋매장이 많지않은데 아이스크림라떼 맛있어요!!!",
          "original_form": "폴바셋매장이 많지않은데 아이스크림라떼 맛있어요!!!",
          "speaker_id": "2",
          "time": "20210907 11:39"
        },
        {
          "id": "MDRW2100042914.1.12",
          "form": "우왕우왕 맞아요 서울갈때 가끔머거요....",
          "original_form": "우왕우왕 맞아요 서울갈때 가끔머거요....",
          "speaker_id": "1",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100042914.1.13",
          "form": "아! 서울도 점점 지점이 사라지더라고요",
          "original_form": "아! 서울도 점점 지점이 사라지더라고요",
          "speaker_id": "2",
          "time": "20210907 12:04"
        },
        {
          "id": "MDRW2100042914.1.14",
          "form": "헐 왜죠!!!",
          "original_form": "헐 왜죠!!!",
          "speaker_id": "1",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042914.1.15",
          "form": "장사가 잘안되나봐요ㅜㅠ",
          "original_form": "장사가 잘안되나봐요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042914.1.16",
          "form": "공격적인 마케팅을 안하니까",
          "original_form": "공격적인 마케팅을 안하니까",
          "speaker_id": "2",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042914.1.17",
          "form": "컹... ㅜㅜ 의외네염..",
          "original_form": "컹... ㅜㅜ 의외네염..",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042914.1.18",
          "form": "아이스크림 마싯는데..",
          "original_form": "아이스크림 마싯는데..",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042914.1.19",
          "form": "ㅜㅜ스벅말곤 요즘 다 힘든거같아요",
          "original_form": "ㅜㅜ스벅말곤 요즘 다 힘든거같아요",
