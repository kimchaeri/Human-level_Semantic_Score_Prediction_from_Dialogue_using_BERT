      "utterance": [
        {
          "id": "MDRW2100030443.1.1",
          "form": "돼지고기요!",
          "original_form": "돼지고기요!",
          "speaker_id": "2",
          "time": "20210904 01:06"
        },
        {
          "id": "MDRW2100030443.1.2",
          "form": "수육용 고기 에프에 돌려서 구워먹었어요 홍홍",
          "original_form": "수육용 고기 에프에 돌려서 구워먹었어요 홍홍",
          "speaker_id": "2",
          "time": "20210904 01:06"
        },
        {
          "id": "MDRW2100030443.1.3",
          "form": "캬 맛있게 드셨네요",
          "original_form": "캬 맛있게 드셨네요",
          "speaker_id": "1",
          "time": "20210904 01:06"
        },
        {
          "id": "MDRW2100030443.1.4",
          "form": "수육 안먹은지도 쫌 됐네요",
          "original_form": "수육 안먹은지도 쫌 됐네요",
          "speaker_id": "1",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030443.1.5",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030443.1.6",
          "form": "김장철에 먹을수 있으려나",
          "original_form": "김장철에 먹을수 있으려나",
          "speaker_id": "1",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030443.1.7",
          "form": "이제 곧 김장청이니",
          "original_form": "이제 곧 김장청이니",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030443.1.8",
          "form": "앗찌찌뽕이에여",
          "original_form": "앗찌찌뽕이에여",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030443.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030443.1.10",
          "form": "곧 드실 수 있을 거예요!!",
          "original_form": "곧 드실 수 있을 거예요!!",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030443.1.11",
          "form": "넹 역시 수육 보쌈이져",
          "original_form": "넹 역시 수육 보쌈이져",
          "speaker_id": "1",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030443.1.12",
          "form": "크으...",
          "original_form": "크으...",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030443.1.13",
          "form": "보쌈용 김치는",
          "original_form": "보쌈용 김치는",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030443.1.14",
          "form": "쌈배추에 꺄",
          "original_form": "쌈배추에 꺄",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030443.1.15",
          "form": "좀 달달해야 해요",
          "original_form": "좀 달달해야 해요",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030443.1.16",
          "form": "맞아요 굴도 있었으면 좋겠는데",
          "original_form": "맞아요 굴도 있었으면 좋겠는데",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030443.1.17",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030443.1.18",
          "form": "노로",
          "original_form": "노로",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030443.1.19",
          "form": "굴은.. 노로",
