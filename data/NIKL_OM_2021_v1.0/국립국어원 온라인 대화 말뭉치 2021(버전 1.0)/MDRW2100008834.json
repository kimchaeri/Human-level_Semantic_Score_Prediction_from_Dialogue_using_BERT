      "utterance": [
        {
          "id": "MDRW2100008834.1.1",
          "form": "건강해지려고 헬스장은 못가지만",
          "original_form": "건강해지려고 헬스장은 못가지만",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008834.1.2",
          "form": "집에서 홈트하고있어요",
          "original_form": "집에서 홈트하고있어요",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008834.1.3",
          "form": "집에서 하는 홈트 쉽지 않더라구요",
          "original_form": "집에서 하는 홈트 쉽지 않더라구요",
          "speaker_id": "2",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008834.1.4",
          "form": "땅끄부부 유툽이나",
          "original_form": "땅끄부부 유툽이나",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008834.1.5",
          "form": "간단한체조도 좋더라구요",
          "original_form": "간단한체조도 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008834.1.6",
          "form": "오~ 저도 땅끄부부 알아요",
          "original_form": "오~ 저도 땅끄부부 알아요",
          "speaker_id": "2",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100008834.1.7",
          "form": "오 아시는군요!",
          "original_form": "오 아시는군요!",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008834.1.8",
          "form": "두분이 참 화목해보여서 좋더라구요",
          "original_form": "두분이 참 화목해보여서 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008834.1.9",
          "form": "운동하면서 몸도 좋아지고 유튭으로 수익도 올리고",
          "original_form": "운동하면서 몸도 좋아지고 유튭으로 수익도 올리고",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008834.1.10",
          "form": "유투버들 시장분석도 하궁 새 컨텐츠도 연구하구 은근스트레스많더래요ㅎㅎ",
          "original_form": "유투버들 시장분석도 하궁 새 컨텐츠도 연구하구 은근스트레스많더래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008834.1.11",
          "form": "경쟁자가 늘어나니 쉽지 않을꺼 같아요",
          "original_form": "경쟁자가 늘어나니 쉽지 않을꺼 같아요",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008834.1.12",
          "form": "많이들 하시더라구요",
          "original_form": "많이들 하시더라구요",
          "speaker_id": "1",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008834.1.13",
          "form": "역시.. 쉬운길은 없나봐요 또 집에서 건강을챙기는분들이 많아서",
          "original_form": "역시.. 쉬운길은 없나봐요 또 집에서 건강을챙기는분들이 많아서",
          "speaker_id": "1",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008834.1.14",
          "form": "홈트 많이 하시니까 구독자늘었을거같아요",
          "original_form": "홈트 많이 하시니까 구독자늘었을거같아요",
          "speaker_id": "1",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008834.1.15",
          "form": "코로나 때문에 홈트가 더 늘어나긴 한거 같아요",
          "original_form": "코로나 때문에 홈트가 더 늘어나긴 한거 같아요",
          "speaker_id": "2",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008834.1.16",
          "form": "헬스장을 못가잖아요ㅋㅋ 근손실온다구 집에서 하더래요",
          "original_form": "헬스장을 못가잖아요ㅋㅋ 근손실온다구 집에서 하더래요",
          "speaker_id": "1",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008834.1.17",
          "form": "전 근육없어서..모르는 고통ㅋㅋ",
          "original_form": "전 근육없어서..모르는 고통ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008834.1.18",
          "form": "나이드니 기초대사량도 떨어지고 확실히 근육 늘리는게 쉽지 않은거 같아요",
          "original_form": "나이드니 기초대사량도 떨어지고 확실히 근육 늘리는게 쉽지 않은거 같아요",
          "speaker_id": "2",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008834.1.19",
          "form": "건강지키는 정도만 딱 하면좋겠어요",
          "original_form": "건강지키는 정도만 딱 하면좋겠어요",
