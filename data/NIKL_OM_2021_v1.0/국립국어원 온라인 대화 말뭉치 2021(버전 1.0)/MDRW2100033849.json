      "utterance": [
        {
          "id": "MDRW2100033849.1.1",
          "form": "강아지 키우면",
          "original_form": "강아지 키우면",
          "speaker_id": "1",
          "time": "20210825 12:25"
        },
        {
          "id": "MDRW2100033849.1.2",
          "form": "뭐가좋을거 같으세요?",
          "original_form": "뭐가좋을거 같으세요?",
          "speaker_id": "1",
          "time": "20210825 12:25"
        },
        {
          "id": "MDRW2100033849.1.3",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "2",
          "time": "20210825 12:30"
        },
        {
          "id": "MDRW2100033849.1.4",
          "form": "강아지긴 좋아요ㅋㅋㅋ",
          "original_form": "강아지긴 좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:30"
        },
        {
          "id": "MDRW2100033849.1.5",
          "form": "좋아하시는 품종있으세요?",
          "original_form": "좋아하시는 품종있으세요?",
          "speaker_id": "1",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100033849.1.6",
          "form": "저",
          "original_form": "저",
          "speaker_id": "2",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100033849.1.7",
          "form": "포메요!",
          "original_form": "포메요!",
          "speaker_id": "2",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100033849.1.8",
          "form": "아하 포메귀엽죠 ㅎㅎ",
          "original_form": "아하 포메귀엽죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100033849.1.9",
          "form": "맞아요 언젠가는",
          "original_form": "맞아요 언젠가는",
          "speaker_id": "2",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100033849.1.10",
          "form": "꼭 키울거랍니다",
          "original_form": "꼭 키울거랍니다",
          "speaker_id": "2",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100033849.1.11",
          "form": "과외하는 집에",
          "original_form": "과외하는 집에",
          "speaker_id": "1",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100033849.1.12",
          "form": "포메가있었어요",
          "original_form": "포메가있었어요",
          "speaker_id": "1",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100033849.1.13",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 12:58"
        },
        {
          "id": "MDRW2100033849.1.14",
          "form": "너무 귀엽죠?",
          "original_form": "너무 귀엽죠?",
          "speaker_id": "2",
          "time": "20210825 12:58"
        },
        {
          "id": "MDRW2100033849.1.15",
          "form": "네 귀여웠어요",
          "original_form": "네 귀여웠어요",
          "speaker_id": "1",
          "time": "20210825 12:58"
        },
        {
          "id": "MDRW2100033849.1.16",
          "form": "집에간다고 하면 막 왈왈짖고요",
          "original_form": "집에간다고 하면 막 왈왈짖고요",
          "speaker_id": "1",
          "time": "20210825 12:59"
        },
        {
          "id": "MDRW2100033849.1.17",
          "form": "어우 넘귀엽네요",
          "original_form": "어우 넘귀엽네요",
          "speaker_id": "2",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100033849.1.18",
          "form": "저는 지나가는강아지봐도좋아요",
          "original_form": "저는 지나가는강아지봐도좋아요",
          "speaker_id": "2",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100033849.1.19",
          "form": "강아지 진짜 좋아하시나봐요",
          "original_form": "강아지 진짜 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100033849.1.20",
          "form": "저는 개 무서웠는데 저 과외학생집 강아지 이후로",
          "original_form": "저는 개 무서웠는데 저 과외학생집 강아지 이후로",
          "speaker_id": "1",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100033849.1.21",
          "form": "네네 전",
          "original_form": "네네 전",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100033849.1.22",
          "form": "진짜좋아해요",
          "original_form": "진짜좋아해요",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100033849.1.23",
          "form": "ㅠ 무서울수도있긴하죠",
          "original_form": "ㅠ 무서울수도있긴하죠",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100033849.1.24",
          "form": "네 어릴때 학교가는 길목에 개가 따라오는거 같고",
          "original_form": "네 어릴때 학교가는 길목에 개가 따라오는거 같고",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100033849.1.25",
          "form": "그런기억이 있어서요",
          "original_form": "그런기억이 있어서요",
          "speaker_id": "1",
          "time": "20210825 13:19"
        }
      ]
