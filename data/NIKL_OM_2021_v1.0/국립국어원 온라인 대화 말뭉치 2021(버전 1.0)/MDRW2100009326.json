      "utterance": [
        {
          "id": "MDRW2100009326.1.1",
          "form": "직접가서 본 스포츠 있나요?",
          "original_form": "직접가서 본 스포츠 있나요?",
          "speaker_id": "1",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009326.1.2",
          "form": "어릴때 축구 봤어요ㅎ",
          "original_form": "어릴때 축구 봤어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009326.1.3",
          "form": "혹시 축구 좋아해요?",
          "original_form": "혹시 축구 좋아해요?",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009326.1.4",
          "form": "축구장 가서요?",
          "original_form": "축구장 가서요?",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009326.1.5",
          "form": "네네ㅎㅎ 저희 쪽에 전남드래곤즈 있어요ㅎ",
          "original_form": "네네ㅎㅎ 저희 쪽에 전남드래곤즈 있어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009326.1.6",
          "form": "축구장 저도 딱 한번 가봤어요ㅎ",
          "original_form": "축구장 저도 딱 한번 가봤어요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009326.1.7",
          "form": "어릴때는 제 의견은 없었죠ㄹ",
          "original_form": "어릴때는 제 의견은 없었죠ㄹ",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009326.1.8",
          "form": "전남이면 이동국선수있던 팀이죠?",
          "original_form": "전남이면 이동국선수있던 팀이죠?",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009326.1.9",
          "form": "아버지가 가자고하면 좋아서 갔었어요ㅎ",
          "original_form": "아버지가 가자고하면 좋아서 갔었어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009326.1.10",
          "form": "전북일꺼에요ㅎㅎ",
          "original_form": "전북일꺼에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009326.1.11",
          "form": "아ㅎ 전북이구나",
          "original_form": "아ㅎ 전북이구나",
          "speaker_id": "1",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009326.1.12",
          "form": "ㅎㅎ 모를수도있져ㄹㄹ",
          "original_form": "ㅎㅎ 모를수도있져ㄹㄹ",
          "speaker_id": "2",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009326.1.13",
          "form": "축구장도 데려가시고 좋은아부지셨네요",
          "original_form": "축구장도 데려가시고 좋은아부지셨네요",
          "speaker_id": "1",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009326.1.14",
          "form": "축구선수중에 가장 좋아하는 선수는누구에요?",
          "original_form": "축구선수중에 가장 좋아하는 선수는누구에요?",
          "speaker_id": "2",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009326.1.15",
          "form": "손흥민?ㅋ",
          "original_form": "손흥민?ㅋ",
          "speaker_id": "1",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009326.1.16",
          "form": "아는선수가 몇명 없어서",
          "original_form": "아는선수가 몇명 없어서",
          "speaker_id": "1",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100009326.1.17",
          "form": "축구장가서 보면 선수들도 안보이는데",
          "original_form": "축구장가서 보면 선수들도 안보이는데",
          "speaker_id": "1",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009326.1.18",
          "form": "그래도 재밌나봐요ㄴㅅ",
          "original_form": "그래도 재밌나봐요ㄴㅅ",
          "speaker_id": "1",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009326.1.19",
          "form": "손흥민은 않좋아하는 사람이없죠ㄹㄹ",
          "original_form": "손흥민은 않좋아하는 사람이없죠ㄹㄹ",
          "speaker_id": "2",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009326.1.20",
          "form": "그럼 축구 말고 뭐좋아하세요?",
          "original_form": "그럼 축구 말고 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009326.1.21",
          "form": "글킨하죠ㅎ",
          "original_form": "글킨하죠ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009326.1.22",
          "form": "전  사실 국제경기 아니면 잘안봐요 ㅎㅎ",
          "original_form": "전  사실 국제경기 아니면 잘안봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009326.1.23",
          "form": "월드컵 올림픽 ㅎ",
          "original_form": "월드컵 올림픽 ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009326.1.24",
          "form": "요런거할때맍",
          "original_form": "요런거할때맍",
          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009326.1.25",
          "form": "ㅎㅎ 축구 좋아하시네요ㅎ",
          "original_form": "ㅎㅎ 축구 좋아하시네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009326.1.26",
          "form": "올림픽도 보시고ㅎ",
          "original_form": "올림픽도 보시고ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009326.1.27",
          "form": "야구장도 가서 보면 그렇게 잼나다던데",
          "original_form": "야구장도 가서 보면 그렇게 잼나다던데",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009326.1.28",
          "form": "아니 월드컵ㅎㅎ",
          "original_form": "아니 월드컵ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009326.1.29",
          "form": "야구장도 좋죠ㅎ",
          "original_form": "야구장도 좋죠ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009326.1.30",
          "form": "여럿이 가서 보면 응원하는 재미도 있고  치킨도먹고",
          "original_form": "여럿이 가서 보면 응원하는 재미도 있고  치킨도먹고",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009326.1.31",
          "form": "야구장은 티켓 가격이 얼마나해요?",
          "original_form": "야구장은 티켓 가격이 얼마나해요?",
          "speaker_id": "1",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100009326.1.32",
          "form": "음.. 전 야구장을 안가봤어요ㅍㅍ",
          "original_form": "음.. 전 야구장을 안가봤어요ㅍㅍ",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009326.1.33",
          "form": "아 그러시군요ㅎ",
          "original_form": "아 그러시군요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009326.1.34",
