      "utterance": [
        {
          "id": "MDRW2100016800.1.1",
          "form": "남친말고 동물은 안키우시나여?",
          "original_form": "남친말고 동물은 안키우시나여?",
          "speaker_id": "1",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016800.1.2",
          "form": "동물에 관심없어요",
          "original_form": "동물에 관심없어요",
          "speaker_id": "2",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016800.1.3",
          "form": "책임감이 무서워서",
          "original_form": "책임감이 무서워서",
          "speaker_id": "2",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016800.1.4",
          "form": "나이들고 시간 많이 보낼수있을때 키우고싶어요",
          "original_form": "나이들고 시간 많이 보낼수있을때 키우고싶어요",
          "speaker_id": "2",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016800.1.5",
          "form": "멍멍이 안귀여워요?",
          "original_form": "멍멍이 안귀여워요?",
          "speaker_id": "1",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016800.1.6",
          "form": "고양이 안귀여워요?",
          "original_form": "고양이 안귀여워요?",
          "speaker_id": "1",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016800.1.7",
          "form": "메마르셨네요..",
          "original_form": "메마르셨네요..",
          "speaker_id": "1",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016800.1.8",
          "form": "귀여워요",
          "original_form": "귀여워요",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016800.1.9",
          "form": "그래서 나이들면 책임질수잇을때 키우고싶어요",
          "original_form": "그래서 나이들면 책임질수잇을때 키우고싶어요",
          "speaker_id": "2",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100016800.1.10",
          "form": "강아지!!",
          "original_form": "강아지!!",
          "speaker_id": "2",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100016800.1.11",
          "form": "예전에 키워본적은 없어요?",
          "original_form": "예전에 키워본적은 없어요?",
          "speaker_id": "1",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100016800.1.12",
          "form": "없어요ㅎㅎ",
          "original_form": "없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016800.1.13",
          "form": "반려동물키우세요?",
          "original_form": "반려동물키우세요?",
          "speaker_id": "2",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016800.1.14",
          "form": "저도.. 저하나 키우기도 벅차서...",
          "original_form": "저도.. 저하나 키우기도 벅차서...",
          "speaker_id": "1",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016800.1.15",
          "form": "근데 동물 음청 좋아해용..",
          "original_form": "근데 동물 음청 좋아해용..",
          "speaker_id": "1",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016800.1.16",
          "form": "털 달린거 다 이뻐요",
          "original_form": "털 달린거 다 이뻐요",
          "speaker_id": "1",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016800.1.17",
          "form": "진짜 귀엽져ㅜㅜ",
          "original_form": "진짜 귀엽져ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100016800.1.18",
          "form": "에버랜드에서 팬더보고 심쿵해서",
          "original_form": "에버랜드에서 팬더보고 심쿵해서",
          "speaker_id": "2",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100016800.1.19",
          "form": "유툽찾아보고막 ㅠㅠ",
          "original_form": "유툽찾아보고막 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100016800.1.20",
          "form": "ㅋㅋ저도 제몸하나 건사하기 힘들어요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ저도 제몸하나 건사하기 힘들어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100016800.1.21",
          "form": "팬더 진짜 엄청 귀옂됴..",
          "original_form": "팬더 진짜 엄청 귀옂됴..",
          "speaker_id": "1",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100016800.1.22",
          "form": "랫서팬더는 100프로 취저",
          "original_form": "랫서팬더는 100프로 취저",
          "speaker_id": "1",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100016800.1.23",
          "form": "랫서팬더도 하 귀여움 찢엇어요",
          "original_form": "랫서팬더도 하 귀여움 찢엇어요",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016800.1.24",
          "form": "일본에선 랫서팬더 키울 수 있는거 같더라구요. 집에서..",
          "original_form": "일본에선 랫서팬더 키울 수 있는거 같더라구요. 집에서..",
          "speaker_id": "1",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016800.1.25",
          "form": "신기방기 ㅋㅋ",
          "original_form": "신기방기 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016800.1.26",
          "form": "어떻게 그렇게 귀엽게생긴건지",
          "original_form": "어떻게 그렇게 귀엽게생긴건지",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016800.1.27",
          "form": "쿼카도 귀엽던뎅",
          "original_form": "쿼카도 귀엽던뎅",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016800.1.28",
          "form": "그거 똥 네모난거요?",
          "original_form": "그거 똥 네모난거요?",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016800.1.29",
          "form": "쿼카똥은 모르겟는데ㅋㅋㅋㅋ",
          "original_form": "쿼카똥은 모르겟는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016800.1.30",
          "form": "귀엽더라구요",
          "original_form": "귀엽더라구요",
          "speaker_id": "2",
          "time": "20210826 15:36"
        }
      ]
    }
