      "utterance": [
        {
          "id": "MDRW2100012650.1.1",
          "form": "불금이네요~",
          "original_form": "불금이네요~",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100012650.1.2",
          "form": "오늘 맛있는 것 드셨나요?ㅎㅎ",
          "original_form": "오늘 맛있는 것 드셨나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100012650.1.3",
          "form": "그냥 반찬가게에서 사다 밥이랑 해서 먹었어요 ㅋㅋ name2은 뭐 드셨어요?",
          "original_form": "그냥 반찬가게에서 사다 밥이랑 해서 먹었어요 ㅋㅋ &name2&은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100012650.1.4",
          "form": "아 그러셨구나",
          "original_form": "아 그러셨구나",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100012650.1.5",
          "form": "저는 오늘 샐러디라는 곳에서 웜랩이라는 것 사먹었어요",
          "original_form": "저는 오늘 샐러디라는 곳에서 웜랩이라는 것 사먹었어요",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100012650.1.6",
          "form": "또띠아 안에 따뜻한 샐러드 넣어서 돌돌 싸먹는",
          "original_form": "또띠아 안에 따뜻한 샐러드 넣어서 돌돌 싸먹는",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100012650.1.7",
          "form": "오 맛있을 것 같아요",
          "original_form": "오 맛있을 것 같아요",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100012650.1.8",
          "form": "건강도 챙기고",
          "original_form": "건강도 챙기고",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100012650.1.9",
          "form": "네 맛있고 건강도 챙긴 기분ㅎㅎ",
          "original_form": "네 맛있고 건강도 챙긴 기분ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100012650.1.10",
          "form": "가격은 조금 나갔지만 기분 좋더라고요~",
          "original_form": "가격은 조금 나갔지만 기분 좋더라고요~",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100012650.1.11",
          "form": "가끔 새로운 거 먹으면 기분 좋죠 ㅎㅎ",
          "original_form": "가끔 새로운 거 먹으면 기분 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100012650.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100012650.1.13",
          "form": "새로운 메뉴 먹거나 맛집 가면 기분 좋아져요",
          "original_form": "새로운 메뉴 먹거나 맛집 가면 기분 좋아져요",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100012650.1.14",
          "form": "내일은 간만에 뭐라도 시켜 먹어야겠어요 ㅎㅎ",
          "original_form": "내일은 간만에 뭐라도 시켜 먹어야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100012650.1.15",
          "form": "배달음식 자주 드세요?",
          "original_form": "배달음식 자주 드세요?",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100012650.1.16",
          "form": "한동안 자주 먹었어요",
          "original_form": "한동안 자주 먹었어요",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100012650.1.17",
          "form": "더울땐 식사 차리기 싫어서",
          "original_form": "더울땐 식사 차리기 싫어서",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100012650.1.18",
          "form": "요즘은 좀 자제하고 있어요",
          "original_form": "요즘은 좀 자제하고 있어요",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100012650.1.19",
          "form": "배달 시키면 포장 쓰레기 분리수거가 좀 귀찮더라구요 ㅋㅋ",
          "original_form": "배달 시키면 포장 쓰레기 분리수거가 좀 귀찮더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100012650.1.20",
          "form": "맞아요 음식 배달하면 쓰레기가 너무 많이 나와요",
          "original_form": "맞아요 음식 배달하면 쓰레기가 너무 많이 나와요",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100012650.1.21",
          "form": "집밥이 제일 좋긴 하죠",
          "original_form": "집밥이 제일 좋긴 하죠",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100012650.1.22",
          "form": "근데 여름에는 진짜 불 쓰기 싫긴 해요ㅋㅋ",
          "original_form": "근데 여름에는 진짜 불 쓰기 싫긴 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100012650.1.23",
          "form": "맞아요 간단히 조리할 수 있는 음식으로 먹어야죠",
          "original_form": "맞아요 간단히 조리할 수 있는 음식으로 먹어야죠",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100012650.1.24",
          "form": "이제 좀 선선해지니 끓이거나 하는 음식도 해먹을만하더라고요",
          "original_form": "이제 좀 선선해지니 끓이거나 하는 음식도 해먹을만하더라고요",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100012650.1.25",
          "form": "잘 하시는 메뉴 있으신가요?",
          "original_form": "잘 하시는 메뉴 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 23:15"
        }
      ]
    }
