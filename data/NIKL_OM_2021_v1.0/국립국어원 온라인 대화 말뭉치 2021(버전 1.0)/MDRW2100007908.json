      "utterance": [
        {
          "id": "MDRW2100007908.1.1",
          "form": "종교있어요?",
          "original_form": "종교있어요?",
          "speaker_id": "1",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007908.1.2",
          "form": "기독교 모태신앙이었는데",
          "original_form": "기독교 모태신앙이었는데",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007908.1.3",
          "form": "지금은 무교에요",
          "original_form": "지금은 무교에요",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007908.1.4",
          "form": "어떤 계기가 있었나요?",
          "original_form": "어떤 계기가 있었나요?",
          "speaker_id": "1",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007908.1.5",
          "form": "기독교는 요즘 세상과 안 맞는거 같아요",
          "original_form": "기독교는 요즘 세상과 안 맞는거 같아요",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007908.1.6",
          "form": "개인의 자유일 수 있는 영역을",
          "original_form": "개인의 자유일 수 있는 영역을",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007908.1.7",
          "form": "신앙으로 막아버려서",
          "original_form": "신앙으로 막아버려서",
          "speaker_id": "2",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007908.1.8",
          "form": "억압이 문제내여",
          "original_form": "억압이 문제내여",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007908.1.9",
          "form": "님은 종교 있으세요?",
          "original_form": "님은 종교 있으세요?",
          "speaker_id": "2",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007908.1.10",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007908.1.11",
          "form": "부모님은불교요",
          "original_form": "부모님은불교요",
          "speaker_id": "1",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007908.1.12",
          "form": "불교를 안 믿으시는 이유가 있으세요?",
          "original_form": "불교를 안 믿으시는 이유가 있으세요?",
          "speaker_id": "2",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007908.1.13",
          "form": "욕하는스님을 몇몇 봐서 존경심이 없어짐",
          "original_form": "욕하는스님을 몇몇 봐서 존경심이 없어짐",
          "speaker_id": "1",
          "time": "20210822 11:51"
        },
        {
          "id": "MDRW2100007908.1.14",
          "form": "욕은 많이 심했네요",
          "original_form": "욕은 많이 심했네요",
          "speaker_id": "2",
          "time": "20210822 11:52"
        },
        {
          "id": "MDRW2100007908.1.15",
          "form": "불교자라고 전부 온화한게 아니었군요",
          "original_form": "불교자라고 전부 온화한게 아니었군요",
          "speaker_id": "2",
          "time": "20210822 11:52"
        },
        {
          "id": "MDRW2100007908.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 11:54"
        },
        {
          "id": "MDRW2100007908.1.17",
          "form": "완전 실망",
          "original_form": "완전 실망",
          "speaker_id": "1",
          "time": "20210822 11:54"
        },
        {
          "id": "MDRW2100007908.1.18",
          "form": "각 종교마다 이미지가 다른데",
          "original_form": "각 종교마다 이미지가 다른데",
          "speaker_id": "2",
          "time": "20210822 11:54"
        },
        {
          "id": "MDRW2100007908.1.19",
          "form": "그나마 불교가 제일 낫지 않나",
          "original_form": "그나마 불교가 제일 낫지 않나",
          "speaker_id": "2",
          "time": "20210822 11:54"
        },
        {
          "id": "MDRW2100007908.1.20",
          "form": "천주교도 괜찮아요",
          "original_form": "천주교도 괜찮아요",
          "speaker_id": "1",
          "time": "20210822 11:55"
        }
      ]
