      "utterance": [
        {
          "id": "MDRW2100026549.1.1",
          "form": "정말? 화도 내?",
          "original_form": "정말? 화도 내?",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026549.1.2",
          "form": "응ㅜㅜ 성격이 생각보다 나빠",
          "original_form": "응ㅜㅜ 성격이 생각보다 나빠",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026549.1.3",
          "form": "와 진짜 신기하다",
          "original_form": "와 진짜 신기하다",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026549.1.4",
          "form": "설마 화내면서 손도 물고 그러는거야?",
          "original_form": "설마 화내면서 손도 물고 그러는거야?",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026549.1.5",
          "form": "그건 그때마다 달라!",
          "original_form": "그건 그때마다 달라!",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026549.1.6",
          "form": "막 날갯짓하면서 화내기도 해!",
          "original_form": "막 날갯짓하면서 화내기도 해!",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026549.1.7",
          "form": "와 진짜 신기해ㅋㅋㅋ",
          "original_form": "와 진짜 신기해ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026549.1.8",
          "form": "그럼 집에 깃털도 날리겠다?!",
          "original_form": "그럼 집에 깃털도 날리겠다?!",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026549.1.9",
          "form": "ㅋㅋㅋㅋ호불호도 강하고 가끔 노래부르면서 춤도췃어",
          "original_form": "ㅋㅋㅋㅋ호불호도 강하고 가끔 노래부르면서 춤도췃어",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026549.1.10",
          "form": "키울땐 조금?",
          "original_form": "키울땐 조금?",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026549.1.11",
          "form": "우와!! 춤도춘다고??!!",
          "original_form": "우와!! 춤도춘다고??!!",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026549.1.12",
          "form": "응 춤은 춤인데 그냥...몸위아래로 ...움직이는??",
          "original_form": "응 춤은 춤인데 그냥...몸위아래로 ...움직이는??",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026549.1.13",
          "form": "귀엽네ㅋㅋㅋ 난 어릴때 동물원에서 말하는 앵무새 봤던거 기억나",
          "original_form": "귀엽네ㅋㅋㅋ 난 어릴때 동물원에서 말하는 앵무새 봤던거 기억나",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026549.1.14",
          "form": "와 난 못본것 같아ㅜㅜ 동물원에선 사자?",
          "original_form": "와 난 못본것 같아ㅜㅜ 동물원에선 사자?",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026549.1.15",
          "form": "동물원하면 코끼리도 기억나고ㅋㅋㅋ",
          "original_form": "동물원하면 코끼리도 기억나고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026549.1.16",
          "form": "맞아 ㅋㅋㅋ 난 기린이랑 북극곰!",
          "original_form": "맞아 ㅋㅋㅋ 난 기린이랑 북극곰!",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026549.1.17",
          "form": "그리고 동물원에서 나는 그 특유의 냄새ㅋㅋㅋ",
          "original_form": "그리고 동물원에서 나는 그 특유의 냄새ㅋㅋㅋ",
          "speaker_id": "2",
