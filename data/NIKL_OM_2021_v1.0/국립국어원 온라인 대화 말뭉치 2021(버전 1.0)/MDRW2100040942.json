      "utterance": [
        {
          "id": "MDRW2100040942.1.1",
          "form": "꼭 그래야겠어요!",
          "original_form": "꼭 그래야겠어요!",
          "speaker_id": "1",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040942.1.2",
          "form": "예전에 학교 다니실때 어떤 과목을 좋아하셨나요?",
          "original_form": "예전에 학교 다니실때 어떤 과목을 좋아하셨나요?",
          "speaker_id": "1",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040942.1.3",
          "form": "저는 음악을 좋아했어요!",
          "original_form": "저는 음악을 좋아했어요!",
          "speaker_id": "1",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040942.1.4",
          "form": "저는 체육이요",
          "original_form": "저는 체육이요",
          "speaker_id": "2",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100040942.1.5",
          "form": "운동을 잘하셨나봐요!",
          "original_form": "운동을 잘하셨나봐요!",
          "speaker_id": "1",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100040942.1.6",
          "form": "어떤 부분을 제일 잘하셨나요?",
          "original_form": "어떤 부분을 제일 잘하셨나요?",
          "speaker_id": "1",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100040942.1.7",
          "form": "그냥 몸을 움직이는 걸 좋아해서",
          "original_form": "그냥 몸을 움직이는 걸 좋아해서",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100040942.1.8",
          "form": "운동장에서 뛰놀 수 있는게 좋았어요",
          "original_form": "운동장에서 뛰놀 수 있는게 좋았어요",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100040942.1.9",
          "form": "달리기도 굉장히 잘 하셨겠어요!",
          "original_form": "달리기도 굉장히 잘 하셨겠어요!",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040942.1.10",
          "form": "막 계주선수도 하시고 그러셨던거 아니에요?",
          "original_form": "막 계주선수도 하시고 그러셨던거 아니에요?",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040942.1.11",
          "form": "시골출신이라 선수까진 아니고 학교에서 운동회같은 거 하면 계주 자신있었죠",
          "original_form": "시골출신이라 선수까진 아니고 학교에서 운동회같은 거 하면 계주 자신있었죠",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040942.1.12",
          "form": "진짜 부러워요!!",
          "original_form": "진짜 부러워요!!",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040942.1.13",
          "form": "저는 뛰고 있지만",
          "original_form": "저는 뛰고 있지만",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040942.1.14",
          "form": "장 애 물 달리기 하면 저는 매번",
          "original_form": "장 애 물 달리기 하면 저는 매번",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040942.1.15",
          "form": "다음 주자들이랑 같이 들어갈 정도로 달리기를 못해요",
          "original_form": "다음 주자들이랑 같이 들어갈 정도로 달리기를 못해요",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040942.1.16",
          "form": "저런.. 그래도 음악 좋아했다는 걸 보면 음악은 잘했을 거 같네요",
          "original_form": "저런.. 그래도 음악 좋아했다는 걸 보면 음악은 잘했을 거 같네요",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100040942.1.17",
          "form": "노래쪽인가요 악기연주 쪽인가요?",
          "original_form": "노래쪽인가요 악기연주 쪽인가요?",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100040942.1.18",
          "form": "저는 악기 연주쪽을 좋아했어요!",
          "original_form": "저는 악기 연주쪽을 좋아했어요!",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100040942.1.19",
          "form": "앞에나가서 노래 부르는건 그렇게 쑥쓰럽더라구요",
          "original_form": "앞에나가서 노래 부르는건 그렇게 쑥쓰럽더라구요",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100040942.1.20",
          "form": "그럼 연주할 수 있는 악기 있나요?",
          "original_form": "그럼 연주할 수 있는 악기 있나요?",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100040942.1.21",
          "form": "지금 당장 엄청 잘 다룰 수 있는 악기는 굉장히 적고",
          "original_form": "지금 당장 엄청 잘 다룰 수 있는 악기는 굉장히 적고",
          "speaker_id": "1",
          "time": "20210905 18:11"
        },
        {
          "id": "MDRW2100040942.1.22",
          "form": "배워본 악기가 많은쪽이에요 ㅎㅎ",
          "original_form": "배워본 악기가 많은쪽이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:12"
        },
        {
          "id": "MDRW2100040942.1.23",
          "form": "피아노랑 바이올린 오카리나 기타 가야금  정도 간단하게 바로 연주 할 수 있을 것 같아요 ㅎㅎ",
          "original_form": "피아노랑 바이올린 오카리나 기타 가야금  정도 간단하게 바로 연주 할 수 있을 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:13"
        },
        {
          "id": "MDRW2100040942.1.24",
          "form": "와 대단하네요",
          "original_form": "와 대단하네요",
          "speaker_id": "2",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100040942.1.25",
          "form": "정말 악기 연주에 흥미가 있는 분이셨군요",
          "original_form": "정말 악기 연주에 흥미가 있는 분이셨군요",
          "speaker_id": "2",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100040942.1.26",
          "form": "특히 가야금쪽이 부럽네요",
          "original_form": "특히 가야금쪽이 부럽네요",
          "speaker_id": "2",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100040942.1.27",
          "form": "국악기를 다루다니",
          "original_form": "국악기를 다루다니",
          "speaker_id": "2",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100040942.1.28",
          "form": "오른손 일부 손가락의 지문이 사라지지만",
          "original_form": "오른손 일부 손가락의 지문이 사라지지만",
          "speaker_id": "1",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100040942.1.29",
          "form": "정말 재미있어요",
          "original_form": "정말 재미있어요",
          "speaker_id": "1",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100040942.1.30",
          "form": "한번 해보시면 정말 재미있는거 아실텐데 ㅠ",
          "original_form": "한번 해보시면 정말 재미있는거 아실텐데 ㅠ",
          "speaker_id": "1",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100040942.1.31",
          "form": "음악은 혹시 좋아하셨나요??",
          "original_form": "음악은 혹시 좋아하셨나요??",
          "speaker_id": "1",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100040942.1.32",
          "form": "저는 음악감상하는 건 참 좋아하는데",
          "original_form": "저는 음악감상하는 건 참 좋아하는데",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100040942.1.33",
          "form": "악기는 다룰 줄 아는게 한개도 없네요",
          "original_form": "악기는 다룰 줄 아는게 한개도 없네요",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100040942.1.34",
          "form": "그래서 악기 잘 다루는 분들 부러워요",
          "original_form": "그래서 악기 잘 다루는 분들 부러워요",
          "speaker_id": "2",
          "time": "20210905 20:20"
        }
      ]
