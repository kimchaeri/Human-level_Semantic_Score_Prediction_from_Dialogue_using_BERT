        },
        {
          "id": "MDRW2100040771.1.4",
          "form": "은근히 저력이 있나봅니다.",
          "original_form": "은근히 저력이 있나봅니다.",
          "speaker_id": "1",
          "time": "20210905 04:20"
        },
        {
          "id": "MDRW2100040771.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 04:20"
        },
        {
          "id": "MDRW2100040771.1.6",
          "form": "숨쉬는법도",
          "original_form": "숨쉬는법도",
          "speaker_id": "2",
          "time": "20210905 04:20"
        },
        {
          "id": "MDRW2100040771.1.7",
          "form": "배운다면서요",
          "original_form": "배운다면서요",
          "speaker_id": "2",
          "time": "20210905 04:20"
        },
        {
          "id": "MDRW2100040771.1.8",
          "form": "갈비뼈를 조이는?!",
          "original_form": "갈비뼈를 조이는?!",
          "speaker_id": "2",
          "time": "20210905 04:20"
        },
        {
          "id": "MDRW2100040771.1.9",
          "form": "막그런?!",
          "original_form": "막그런?!",
          "speaker_id": "2",
          "time": "20210905 04:20"
        },
        {
          "id": "MDRW2100040771.1.10",
          "form": "아 헐 헐",
          "original_form": "아 헐 헐",
          "speaker_id": "1",
          "time": "20210905 04:20"
        },
        {
          "id": "MDRW2100040771.1.11",
          "form": "전 그런건 안하던데!",
          "original_form": "전 그런건 안하던데!",
          "speaker_id": "1",
          "time": "20210905 04:20"
        },
        {
          "id": "MDRW2100040771.1.12",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210905 04:20"
        },
        {
          "id": "MDRW2100040771.1.13",
          "form": "아마 기구 필라테스라 그런걸까요?",
          "original_form": "아마 기구 필라테스라 그런걸까요?",
          "speaker_id": "1",
          "time": "20210905 04:21"
        },
        {
          "id": "MDRW2100040771.1.14",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210905 04:21"
        },
        {
          "id": "MDRW2100040771.1.15",
          "form": "아니면 1대 1수업이나",
          "original_form": "아니면 1대 1수업이나",
          "speaker_id": "1",
          "time": "20210905 04:21"
        },
        {
          "id": "MDRW2100040771.1.16",
          "form": "2대 1 수업에서는",
          "original_form": "2대 1 수업에서는",
          "speaker_id": "1",
          "time": "20210905 04:21"
        },
        {
          "id": "MDRW2100040771.1.17",
          "form": "오옹오그런거같네요",
          "original_form": "오옹오그런거같네요",
          "speaker_id": "2",
          "time": "20210905 04:21"
        },
        {
          "id": "MDRW2100040771.1.18",
          "form": "배울지도 모르겠네요,,,",
          "original_form": "배울지도 모르겠네요,,,",
          "speaker_id": "1",
          "time": "20210905 04:21"
        },
        {
          "id": "MDRW2100040771.1.19",
          "form": "크...",
          "original_form": "크...",
          "speaker_id": "2",
          "time": "20210905 04:21"
        },
        {
          "id": "MDRW2100040771.1.20",
          "form": "근데 필라테스 비싸서....",
          "original_form": "근데 필라테스 비싸서....",
          "speaker_id": "1",
          "time": "20210905 04:21"
        },
        {
          "id": "MDRW2100040771.1.21",
          "form": "무섭네요",
          "original_form": "무섭네요",
          "speaker_id": "2",
          "time": "20210905 04:21"
        },
        {
          "id": "MDRW2100040771.1.22",
          "form": "차마 그런 개인수업은... ",
          "original_form": "차마 그런 개인수업은... {emoji:☆}",
          "speaker_id": "1",
          "time": "20210905 04:21"
        },
        {
          "id": "MDRW2100040771.1.23",
