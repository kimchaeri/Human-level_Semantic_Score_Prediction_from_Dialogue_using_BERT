      "utterance": [
        {
          "id": "MDRW2100007209.1.1",
          "form": "좋아요 좋아요",
          "original_form": "좋아요 좋아요",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007209.1.2",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007209.1.3",
          "form": "저는 집에 있는 걸 좋아하긴 하는데",
          "original_form": "저는 집에 있는 걸 좋아하긴 하는데",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007209.1.4",
          "form": "여름 겨울에는 꼭 어디 한번씩은 다녀오는 것 같아요",
          "original_form": "여름 겨울에는 꼭 어디 한번씩은 다녀오는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007209.1.5",
          "form": "해외로? 국내로?",
          "original_form": "해외로? 국내로?",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007209.1.6",
          "form": "코에 바람 넣어주는거 좋죠!",
          "original_form": "코에 바람 넣어주는거 좋죠!",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007209.1.7",
          "form": "국내로!",
          "original_form": "국내로!",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007209.1.8",
          "form": "운전 시작하고 나니 다니기가 너무 좋네요",
          "original_form": "운전 시작하고 나니 다니기가 너무 좋네요",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007209.1.9",
          "form": "맞아요 차가 있으면 국내여행 정말 좋아요",
          "original_form": "맞아요 차가 있으면 국내여행 정말 좋아요",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007209.1.10",
          "form": "도로도 정말 잘해놓고!",
          "original_form": "도로도 정말 잘해놓고!",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007209.1.11",
          "form": "맛집 투어파이신가요",
          "original_form": "맛집 투어파이신가요",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007209.1.12",
          "form": "저는 맛집보다는 물구경 집착파예요",
          "original_form": "저는 맛집보다는 물구경 집착파예요",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007209.1.13",
          "form": "물이 있는 곳을 좋아합니다",
          "original_form": "물이 있는 곳을 좋아합니다",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007209.1.14",
          "form": "ㅋㅋ 그래서 별명이 연가시입니다",
          "original_form": "ㅋㅋ 그래서 별명이 연가시입니다",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007209.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007209.1.16",
          "form": "바다나 강가 진짜 좋아해요 리버뷰 오션뷰 최고...",
          "original_form": "바다나 강가 진짜 좋아해요 리버뷰 오션뷰 최고...",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007209.1.17",
          "form": "추천해주신다면??",
          "original_form": "추천해주신다면??",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007209.1.18",
          "form": "저 올해 서핑 배우기 시작했거든요",
          "original_form": "저 올해 서핑 배우기 시작했거든요",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007209.1.19",
          "form": "질리도록 물 구경하고 물에서 구르고 물 먹기 가능해서 너무 좋아요",
          "original_form": "질리도록 물 구경하고 물에서 구르고 물 먹기 가능해서 너무 좋아요",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007209.1.20",
          "form": "오오!!! 어디로 가세요? 양양?",
          "original_form": "오오!!! 어디로 가세요? 양양?",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007209.1.21",
          "form": "이번에 양양 갔고 몇 달 전에는 강릉 갔었어요",
          "original_form": "이번에 양양 갔고 몇 달 전에는 강릉 갔었어요",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007209.1.22",
          "form": "여름에 혹시 어디 다녀온 곳 있으세요?",
          "original_form": "여름에 혹시 어디 다녀온 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007209.1.23",
          "form": "양양이 그렇~~~게 핫하다는데 한번 꼭 가봐야겠어요",
          "original_form": "양양이 그렇~~~게 핫하다는데 한번 꼭 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007209.1.24",
          "form": "전 이번 여름은 패스했어요ㅠㅠ 요양을 택했달까",
          "original_form": "전 이번 여름은 패스했어요ㅠㅠ 요양을 택했달까",
