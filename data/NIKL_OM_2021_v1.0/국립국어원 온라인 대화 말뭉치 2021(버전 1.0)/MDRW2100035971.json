      "utterance": [
        {
          "id": "MDRW2100035971.1.1",
          "form": "식사는 하셨나요 !",
          "original_form": "식사는 하셨나요 !",
          "speaker_id": "2",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100035971.1.2",
          "form": "네! 방금 먹교 치웠어요",
          "original_form": "네! 방금 먹교 치웠어요",
          "speaker_id": "1",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100035971.1.3",
          "form": "드셨나용?",
          "original_form": "드셨나용?",
          "speaker_id": "1",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100035971.1.4",
          "form": "이제 먹으려구요! 어떤거 드셨어요?",
          "original_form": "이제 먹으려구요! 어떤거 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 13:06"
        },
        {
          "id": "MDRW2100035971.1.5",
          "form": "저는 바베큐 치킨이요!",
          "original_form": "저는 바베큐 치킨이요!",
          "speaker_id": "1",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100035971.1.6",
          "form": "어떤거 드셨어요?",
          "original_form": "어떤거 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100035971.1.7",
          "form": "전 고등어구이요 !",
          "original_form": "전 고등어구이요 !",
          "speaker_id": "2",
          "time": "20210830 15:14"
        },
        {
          "id": "MDRW2100035971.1.8",
          "form": "치킨 맛있겠네요 ㅎㅎ",
          "original_form": "치킨 맛있겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 15:14"
        },
        {
          "id": "MDRW2100035971.1.9",
          "form": "오 고등어도 맛있죠!!",
          "original_form": "오 고등어도 맛있죠!!",
          "speaker_id": "1",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100035971.1.10",
          "form": "후식도 드셨어용?",
          "original_form": "후식도 드셨어용?",
          "speaker_id": "1",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100035971.1.11",
          "form": "네 먹었어요 ㅎㅎ !!",
          "original_form": "네 먹었어요 ㅎㅎ !!",
          "speaker_id": "2",
          "time": "20210830 18:19"
        },
        {
          "id": "MDRW2100035971.1.12",
          "form": "역시이~",
          "original_form": "역시이~",
          "speaker_id": "1",
          "time": "20210830 18:28"
        },
        {
          "id": "MDRW2100035971.1.13",
          "form": "벌써 저녁먹을때네요ㅠ",
          "original_form": "벌써 저녁먹을때네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 18:28"
        },
        {
          "id": "MDRW2100035971.1.14",
          "form": "그러게요!",
          "original_form": "그러게요!",
          "speaker_id": "2",
          "time": "20210830 18:31"
        },
        {
          "id": "MDRW2100035971.1.15",
          "form": "저녁은 어떤거드세요 ㅎㅎ",
          "original_form": "저녁은 어떤거드세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:31"
        },
