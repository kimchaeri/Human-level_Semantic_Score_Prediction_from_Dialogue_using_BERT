      "utterance": [
        {
          "id": "MDRW2100026488.1.1",
          "form": "공연 좋아해?",
          "original_form": "공연 좋아해?",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026488.1.2",
          "form": "좋아하는데 많이 가보진 못했어요 ㅠㅠ",
          "original_form": "좋아하는데 많이 가보진 못했어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026488.1.3",
          "form": "어떤 종류의 공연을 좋아해요?",
          "original_form": "어떤 종류의 공연을 좋아해요?",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026488.1.4",
          "form": "음 아무래도 콘서트가 제일 좋은 것 같아요",
          "original_form": "음 아무래도 콘서트가 제일 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026488.1.5",
          "form": "님은요??",
          "original_form": "님은요??",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026488.1.6",
          "form": "저는 뮤지컬이 좀 흥미가 있어요!",
          "original_form": "저는 뮤지컬이 좀 흥미가 있어요!",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026488.1.7",
          "form": "최근엔 시국이 이래서 갈 엄두가 안나지만요ㅠㅠ",
          "original_form": "최근엔 시국이 이래서 갈 엄두가 안나지만요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026488.1.8",
          "form": "그쵸 ㅠㅠ",
          "original_form": "그쵸 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026488.1.9",
          "form": "직접 보러가신 뮤지컬이 있나요??",
          "original_form": "직접 보러가신 뮤지컬이 있나요??",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026488.1.10",
          "form": "저 노트르담 드 파리요! 그거말곤 전에 온라인으로 방송?...했던 팬레터와 여신님이 보고계셔....있습니다ㅋㅋㅋ",
          "original_form": "저 노트르담 드 파리요! 그거말곤 전에 온라인으로 방송?...했던 팬레터와 여신님이 보고계셔....있습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026488.1.11",
          "form": "오 멋있어요!",
          "original_form": "오 멋있어요!",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026488.1.12",
          "form": "저도 뮤지컬 직접 보러 가보고싶네용 ㅎㅎ",
          "original_form": "저도 뮤지컬 직접 보러 가보고싶네용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026488.1.13",
          "form": "ㅎㅎ 저두요ㅜㅜ 벌써 몇년 전 일이라 넘 슬퍼요...",
          "original_form": "ㅎㅎ 저두요ㅜㅜ 벌써 몇년 전 일이라 넘 슬퍼요...",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026488.1.14",
          "form": "콘서트는 누구 다녀오셨어요?",
          "original_form": "콘서트는 누구 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026488.1.15",
          "form": "생각해보니 한번도 가본 적이 없네요,,,",
          "original_form": "생각해보니 한번도 가본 적이 없네요,,,",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026488.1.16",
          "form": "아이유 콘서트 꼭 한번 가보고싶어요",
          "original_form": "아이유 콘서트 꼭 한번 가보고싶어요",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026488.1.17",
          "form": "와 저도요.... 좋을것 같아요....ㅜ",
          "original_form": "와 저도요.... 좋을것 같아요....ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026488.1.18",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026488.1.19",
          "form": "제일 최근에 본 영화가 뭐예요??",
          "original_form": "제일 최근에 본 영화가 뭐예요??",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026488.1.20",
          "form": "저 ㅋㅋㅋㅋ명탐정 코난이여 ㅎ...",
          "original_form": "저 ㅋㅋㅋㅋ명탐정 코난이여 ㅎ...",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026488.1.21",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026488.1.22",
          "form": "극장판인가요???",
          "original_form": "극장판인가요???",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026488.1.23",
          "form": "네 ㅋㅋㅋㅋㅋㅋ 몇달 전에 봤는데 제목은 기억이 안나네요 ㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋ 몇달 전에 봤는데 제목은 기억이 안나네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026488.1.24",
          "form": "요새 모가디슈? 재밌대서 볼까 생각중이에요",
          "original_form": "요새 모가디슈? 재밌대서 볼까 생각중이에요",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026488.1.25",
          "form": "저 모가디슈 봤는데 재밌었어요 추천합니당",
          "original_form": "저 모가디슈 봤는데 재밌었어요 추천합니당",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026488.1.26",
          "form": "요즘 아프간 상황이랑 쪼금 비슷해서 와닿을 거예요",
          "original_form": "요즘 아프간 상황이랑 쪼금 비슷해서 와닿을 거예요",
          "speaker_id": "2",
          "time": "20210901 23:40"
        }
      ]
