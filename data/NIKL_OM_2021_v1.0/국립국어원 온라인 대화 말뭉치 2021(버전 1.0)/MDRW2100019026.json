          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019026.1.7",
          "form": "그쵸 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019026.1.8",
          "form": "저는 양궁이랑 배구요!",
          "original_form": "저는 양궁이랑 배구요!",
          "speaker_id": "1",
          "time": "20210827 14:55"
