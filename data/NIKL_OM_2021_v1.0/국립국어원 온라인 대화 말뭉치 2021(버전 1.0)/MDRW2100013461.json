      "utterance": [
        {
          "id": "MDRW2100013461.1.1",
          "form": "안녕하세요~식사하셨나요?",
          "original_form": "안녕하세요~식사하셨나요?",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013461.1.2",
          "form": "아르바이트 해야해서 일찍먹어요!",
          "original_form": "아르바이트 해야해서 일찍먹어요!",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100013461.1.3",
          "form": "아 아르바이트하세요? 어떤일하세요?",
          "original_form": "아 아르바이트하세요? 어떤일하세요?",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100013461.1.4",
          "form": "전 카페요! 아르바이트하세요?",
          "original_form": "전 카페요! 아르바이트하세요?",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100013461.1.5",
          "form": "아 카페~ 저도 카페에서 1년넘게 했었는데 ㅋㅋㅋ",
          "original_form": "아 카페~ 저도 카페에서 1년넘게 했었는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100013461.1.6",
          "form": "아뇨 저는 직장다니고있습니다",
          "original_form": "아뇨 저는 직장다니고있습니다",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100013461.1.7",
          "form": "카페아르바이트 하시면 커피 좋아하시나요?",
          "original_form": "카페아르바이트 하시면 커피 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100013461.1.8",
          "form": "그때는 안좋아해서 잘 안먹었는데 지금은 돈주고 사먹는경지에 이르렀네요",
          "original_form": "그때는 안좋아해서 잘 안먹었는데 지금은 돈주고 사먹는경지에 이르렀네요",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100013461.1.9",
          "form": "업무 스트레스 많으세요?",
          "original_form": "업무 스트레스 많으세요?",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013461.1.10",
          "form": "네,,,아무래도,,,",
          "original_form": "네,,,아무래도,,,",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013461.1.11",
          "form": "그만두고싶은마음은 큰데 월급들어오면 또 못그만두겟드라구요",
          "original_form": "그만두고싶은마음은 큰데 월급들어오면 또 못그만두겟드라구요",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100013461.1.12",
          "form": "이직 생각은 안해보셨어요?",
          "original_form": "이직 생각은 안해보셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100013461.1.13",
          "form": "해봣는데 다 거기서거기같아요 ㅋㅋㅋㅋ 그래서 그냥버텨요",
          "original_form": "해봣는데 다 거기서거기같아요 ㅋㅋㅋㅋ 그래서 그냥버텨요",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100013461.1.14",
          "form": "인턴은 해보셨나요?",
          "original_form": "인턴은 해보셨나요?",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100013461.1.15",
          "form": "아뇨 저인턴안해봣어요",
          "original_form": "아뇨 저인턴안해봣어요",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100013461.1.16",
          "form": "5인미만사업장에서 일해가지고,,",
          "original_form": "5인미만사업장에서 일해가지고,,",
          "speaker_id": "1",
