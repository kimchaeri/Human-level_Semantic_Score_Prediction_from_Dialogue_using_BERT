      "utterance": [
        {
          "id": "MDRW2100047211.1.1",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100047211.1.2",
          "form": "저는 프라푸치노나 에이드 종류 자주 마셔요!",
          "original_form": "저는 프라푸치노나 에이드 종류 자주 마셔요!",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100047211.1.3",
          "form": "에이드 좋아하시나요?",
          "original_form": "에이드 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100047211.1.4",
          "form": "네 가끔 달달한거 질릴땐 또 에이드로 ㅋㅋㅋ 상콤하게",
          "original_form": "네 가끔 달달한거 질릴땐 또 에이드로 ㅋㅋㅋ 상콤하게",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100047211.1.5",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100047211.1.6",
          "form": "가끔 스무디도 마시는데 맛있어요!",
          "original_form": "가끔 스무디도 마시는데 맛있어요!",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100047211.1.7",
          "form": "커피는 안드시나요?",
          "original_form": "커피는 안드시나요?",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100047211.1.8",
          "form": "커피도 매일 마시죵",
          "original_form": "커피도 매일 마시죵",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100047211.1.9",
          "form": "오 아메리카노 자주 드시나요?",
          "original_form": "오 아메리카노 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100047211.1.10",
          "form": "저는 아직 아메리카노는 잘못먹겠더라구요ㅎㅎ",
          "original_form": "저는 아직 아메리카노는 잘못먹겠더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100047211.1.11",
          "form": "저도 그랬는데 늙으니까 ㅋㅋㅋ 그게 더 낮더라구요",
          "original_form": "저도 그랬는데 늙으니까 ㅋㅋㅋ 그게 더 낮더라구요",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100047211.1.12",
          "form": "오 저도 좀 더 나이들면....",
          "original_form": "오 저도 좀 더 나이들면....",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100047211.1.13",
          "form": "아메리카노 마시게 될런지...",
          "original_form": "아메리카노 마시게 될런지...",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100047211.1.14",
          "form": "저는 자몽에이드 좋아하는데 무슨 에이드 좋아하시나요?",
          "original_form": "저는 자몽에이드 좋아하는데 무슨 에이드 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100047211.1.15",
          "form": "저도 자몽에이드ㅎ",
          "original_form": "저도 자몽에이드ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100047211.1.16",
          "form": "좋아해욤",
          "original_form": "좋아해욤",
          "speaker_id": "2",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100047211.1.17",
          "form": "상큼하니 맛있더라구요",
          "original_form": "상큼하니 맛있더라구요",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100047211.1.18",
          "form": "카페 자주 가시나요?",
          "original_form": "카페 자주 가시나요?",
          "speaker_id": "1",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100047211.1.19",
          "form": "주말에 한번씩 가고ㅠ나머진 집이나 편의점 컵커피로ㅠ",
          "original_form": "주말에 한번씩 가고ㅠ나머진 집이나 편의점 컵커피로ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100047211.1.20",
          "form": "오 집에서 드실 정도면 많이 좋아하시나봐요!",
          "original_form": "오 집에서 드실 정도면 많이 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100047211.1.21",
          "form": "저는 카페 갈때만 마시고 있어요ㅠㅠ",
          "original_form": "저는 카페 갈때만 마시고 있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100047211.1.22",
          "form": "네 집에서는 카누로ㅋㅋ",
          "original_form": "네 집에서는 카누로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:02"
        }
      ]
    }
