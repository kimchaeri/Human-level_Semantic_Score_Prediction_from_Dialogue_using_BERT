      "utterance": [
        {
          "id": "MDRW2100035695.1.1",
          "form": "name1님은 못 드시는 음식 있나요?",
          "original_form": "&name1&님은 못 드시는 음식 있나요?",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100035695.1.2",
          "form": "저는 못먹는건 없어요 ㅋㅋㅋ",
          "original_form": "저는 못먹는건 없어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100035695.1.3",
          "form": "오이 좀 싫어하기는 했는데",
          "original_form": "오이 좀 싫어하기는 했는데",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100035695.1.4",
          "form": "안 먹진 않아요 먹으려고 노력했더니 먹어지더라구요",
          "original_form": "안 먹진 않아요 먹으려고 노력했더니 먹어지더라구요",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100035695.1.5",
          "form": "와 어딜가든 그럼 고민은 없겠네요",
          "original_form": "와 어딜가든 그럼 고민은 없겠네요",
          "speaker_id": "2",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100035695.1.6",
          "form": "닭발 개불 이런 것도 드시나요",
          "original_form": "닭발 개불 이런 것도 드시나요",
          "speaker_id": "2",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100035695.1.7",
          "form": "네네 ㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100035695.1.8",
          "form": "호기심에 다 먹어보는 편인데",
          "original_form": "호기심에 다 먹어보는 편인데",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100035695.1.9",
          "form": "와!!! 그럼 천엽도?",
          "original_form": "와!!! 그럼 천엽도?",
          "speaker_id": "2",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100035695.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100035695.1.11",
          "form": "다 시러하시나옷",
          "original_form": "다 시러하시나옷",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100035695.1.12",
          "form": "진짜 못 먹는 게 거의 없는거나 마찬가지네요!!!",
          "original_form": "진짜 못 먹는 게 거의 없는거나 마찬가지네요!!!",
          "speaker_id": "2",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100035695.1.13",
          "form": "네 저 위에 쓴건 다 못 먹어요 ㅋㅋㅋ",
          "original_form": "네 저 위에 쓴건 다 못 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100035695.1.14",
          "form": "마자요ㅋㅋㅋㅋ앜ㅋ",
          "original_form": "마자요ㅋㅋㅋㅋ앜ㅋ",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100035695.1.15",
          "form": "향신료 이런 것도 괜찮으세요? 고수 같은",
          "original_form": "향신료 이런 것도 괜찮으세요? 고수 같은",
          "speaker_id": "2",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100035695.1.16",
          "form": "수박 오이 멜론 참외 이런 류를 별로 안좋아하긴 해요",
          "original_form": "수박 오이 멜론 참외 이런 류를 별로 안좋아하긴 해요",
          "speaker_id": "1",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100035695.1.17",
          "form": "있으면 먹는데 없으면 굳이 찾진않는",
          "original_form": "있으면 먹는데 없으면 굳이 찾진않는",
          "speaker_id": "1",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100035695.1.18",
          "form": "오 신기해요 무난한 과일 같은데",
          "original_form": "오 신기해요 무난한 과일 같은데",
          "speaker_id": "2",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100035695.1.19",
          "form": "물비린내 나는 걸 안 좋아하시나보다",
          "original_form": "물비린내 나는 걸 안 좋아하시나보다",
          "speaker_id": "2",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100035695.1.20",
          "form": "네 좀 향이 강하달까 ㅋㅋㅋ",
          "original_form": "네 좀 향이 강하달까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100035695.1.21",
          "form": "어릴때 그럼 반찬투정 이런거 안하셨나요? ㅋㅋ",
