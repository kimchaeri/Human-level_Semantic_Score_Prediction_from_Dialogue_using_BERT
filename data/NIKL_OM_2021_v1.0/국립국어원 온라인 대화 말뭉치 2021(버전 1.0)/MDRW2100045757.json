      "utterance": [
        {
          "id": "MDRW2100045757.1.1",
          "form": "만두좋죠ㅠㅠㅠㅠㅠ",
          "original_form": "만두좋죠ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045757.1.2",
          "form": "저 냉동만두 완전 좋아해서",
          "original_form": "저 냉동만두 완전 좋아해서",
          "speaker_id": "2",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045757.1.3",
          "form": "한때 주식이 냉동만두였어요",
          "original_form": "한때 주식이 냉동만두였어요",
          "speaker_id": "2",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045757.1.4",
          "form": "얇은피만두 사다가 쟁여놓고",
          "original_form": "얇은피만두 사다가 쟁여놓고",
          "speaker_id": "2",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045757.1.5",
          "form": "만두전골도 너무 맛있죠",
          "original_form": "만두전골도 너무 맛있죠",
          "speaker_id": "1",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045757.1.6",
          "form": "만두는 보관이 편해서 좋아요",
          "original_form": "만두는 보관이 편해서 좋아요",
          "speaker_id": "1",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045757.1.7",
          "form": "오 만두전골",
          "original_form": "오 만두전골",
          "speaker_id": "2",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045757.1.8",
          "form": "너무 좋죠ㅠㅠ",
          "original_form": "너무 좋죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045757.1.9",
          "form": "칼국수 넣고 먹으면",
          "original_form": "칼국수 넣고 먹으면",
          "speaker_id": "1",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045757.1.10",
          "form": "만두국은 많이 먹어봤는데 전골은 조금 생소한 것 같아요?",
          "original_form": "만두국은 많이 먹어봤는데 전골은 조금 생소한 것 같아요?",
          "speaker_id": "2",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045757.1.11",
          "form": "한끼 뚝딱",
          "original_form": "한끼 뚝딱",
          "speaker_id": "1",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045757.1.12",
          "form": "보통 밖에서 많이 드시나요 전골?",
          "original_form": "보통 밖에서 많이 드시나요 전골?",
          "speaker_id": "2",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045757.1.13",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045757.1.14",
          "form": "저는 식당에서 여러 팩 사다가",
          "original_form": "저는 식당에서 여러 팩 사다가",
          "speaker_id": "1",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045757.1.15",
          "form": "칼국수 세상에ㅠㅠㅠ 너무 아름다운 조합이에요",
          "original_form": "칼국수 세상에ㅠㅠㅠ 너무 아름다운 조합이에요",
          "speaker_id": "2",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045757.1.16",
          "form": "냉장고에 넣어두고 한 이틀 먹어요ㅋㅋㅋ",
          "original_form": "냉장고에 넣어두고 한 이틀 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045757.1.17",
          "form": "요리가 귀찮어서ㅋㅋㅋ",
          "original_form": "요리가 귀찮어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045757.1.18",
          "form": "경제적이고 좋은 방법이죠!!!",
          "original_form": "경제적이고 좋은 방법이죠!!!",
          "speaker_id": "2",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045757.1.19",
          "form": "식당에서 여러팩으로 나눠서 포장해주나요?",
          "original_form": "식당에서 여러팩으로 나눠서 포장해주나요?",
          "speaker_id": "2",
          "time": "20210908 06:45"
        },
        {
          "id": "MDRW2100045757.1.20",
          "form": "네네! 너무 편해요",
          "original_form": "네네! 너무 편해요",
          "speaker_id": "1",
          "time": "20210908 06:46"
        },
        {
          "id": "MDRW2100045757.1.21",
          "form": "데워먹기만 하면 될 것 같아서 너무 편하겠어요ㅠㅠㅠㅠ",
          "original_form": "데워먹기만 하면 될 것 같아서 너무 편하겠어요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045757.1.22",
          "form": "요즘 식당에서도 이런거 잘해줘서 너무 좋아요",
          "original_form": "요즘 식당에서도 이런거 잘해줘서 너무 좋아요",
          "speaker_id": "2",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045757.1.23",
          "form": "먹다 남은 것도 바로 진공포장해주고",
          "original_form": "먹다 남은 것도 바로 진공포장해주고",
          "speaker_id": "2",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045757.1.24",
          "form": "맞아요ㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045757.1.25",
          "form": "확실히 1인가구가 늘어서 그런가봐요",
          "original_form": "확실히 1인가구가 늘어서 그런가봐요",
