        },
        {
          "id": "MDRW2100005752.1.2",
          "form": "추석때 가족들 모이시나용?",
          "original_form": "추석때 가족들 모이시나용?",
          "speaker_id": "1",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100005752.1.3",
          "form": "아 안녕하세요",
          "original_form": "아 안녕하세요",
          "speaker_id": "2",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100005752.1.4",
          "form": "저흰 추석때 어른들만 모여서 차례지낼 것 같아요",
          "original_form": "저흰 추석때 어른들만 모여서 차례지낼 것 같아요",
          "speaker_id": "2",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100005752.1.5",
          "form": "오 저희도 그렇게해요",
          "original_form": "오 저희도 그렇게해요",
          "speaker_id": "1",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100005752.1.6",
          "form": "코시국되고서 계속 그랫어요",
          "original_form": "코시국되고서 계속 그랫어요",
          "speaker_id": "1",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100005752.1.7",
          "form": "네 아무래도 인원제한도 있고",
          "original_form": "네 아무래도 인원제한도 있고",
          "speaker_id": "2",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100005752.1.8",
          "form": "지역 옮겨다니는 것도 걱정되고요",
          "original_form": "지역 옮겨다니는 것도 걱정되고요",
          "speaker_id": "2",
          "time": "20210907 10:52"
        },
        {
          "id": "MDRW2100005752.1.9",
          "form": "ㅜㅜ맞아요ㅜㅜ",
          "original_form": "ㅜㅜ맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100005752.1.10",
          "form": "백신은 다 맞으셧나용?",
          "original_form": "백신은 다 맞으셧나용?",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100005752.1.11",
          "form": "네 저 2차접종까지 끝났어요",
          "original_form": "네 저 2차접종까지 끝났어요",
          "speaker_id": "2",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100005752.1.12",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210907 11:07"
        },
        {
          "id": "MDRW2100005752.1.13",
          "form": "의료인이라서 진작...ㅋㅋㅋ",
          "original_form": "의료인이라서 진작...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:07"
        },
        {
          "id": "MDRW2100005752.1.14",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100005752.1.15",
          "form": "근데 접종 완료자 포함해도 가족 수가 많음 모이기 힘들죠ㅜ",
          "original_form": "근데 접종 완료자 포함해도 가족 수가 많음 모이기 힘들죠ㅜ",
          "speaker_id": "2",
          "time": "20210907 11:20"
        },
        {
          "id": "MDRW2100005752.1.16",
          "form": "네 애기들도잇고하니까",
          "original_form": "네 애기들도잇고하니까",
          "speaker_id": "1",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100005752.1.17",
          "form": "어른들만 제사지내고 바로 헤어져용 ㅋㅋ",
          "original_form": "어른들만 제사지내고 바로 헤어져용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100005752.1.18",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100005752.1.19",
          "form": "얼른 끝나구",
          "original_form": "얼른 끝나구",
          "speaker_id": "1",
          "time": "20210907 12:42"
        },
        {
          "id": "MDRW2100005752.1.20",
          "form": "여행가고싶네요",
          "original_form": "여행가고싶네요",
