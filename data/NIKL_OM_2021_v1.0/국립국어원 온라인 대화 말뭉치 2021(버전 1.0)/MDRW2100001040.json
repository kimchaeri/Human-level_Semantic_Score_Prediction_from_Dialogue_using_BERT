        },
        {
          "id": "MDRW2100001040.1.4",
          "form": "응 좋아해",
          "original_form": "응 좋아해",
          "speaker_id": "2",
          "time": "20210722 16:16"
        },
        {
          "id": "MDRW2100001040.1.5",
          "form": "좋아하는 장르있어?",
          "original_form": "좋아하는 장르있어?",
          "speaker_id": "2",
          "time": "20210722 16:16"
        },
        {
          "id": "MDRW2100001040.1.6",
          "form": "나는 좀 슬픈거랑 액션..??",
          "original_form": "나는 좀 슬픈거랑 액션..??",
          "speaker_id": "1",
          "time": "20210722 16:17"
        },
        {
          "id": "MDRW2100001040.1.7",
          "form": "근데 다 좋아해",
          "original_form": "근데 다 좋아해",
          "speaker_id": "1",
          "time": "20210722 16:17"
        },
        {
          "id": "MDRW2100001040.1.8",
          "form": "가장 재밌게 본게 머야?",
          "original_form": "가장 재밌게 본게 머야?",
          "speaker_id": "2",
          "time": "20210722 16:18"
        },
        {
          "id": "MDRW2100001040.1.9",
          "form": "난",
          "original_form": "난",
          "speaker_id": "1",
          "time": "20210722 16:18"
        },
        {
          "id": "MDRW2100001040.1.10",
          "form": "위대한 쇼맨 이라고",
          "original_form": "위대한 쇼맨 이라고",
          "speaker_id": "1",
          "time": "20210722 16:18"
        },
        {
          "id": "MDRW2100001040.1.11",
          "form": "알아??",
          "original_form": "알아??",
          "speaker_id": "1",
          "time": "20210722 16:18"
        },
        {
          "id": "MDRW2100001040.1.12",
          "form": "뮤지컬 영화인데",
          "original_form": "뮤지컬 영화인데",
          "speaker_id": "1",
          "time": "20210722 16:18"
        },
        {
          "id": "MDRW2100001040.1.13",
          "form": "응! 보진않았는데",
          "original_form": "응! 보진않았는데",
          "speaker_id": "2",
          "time": "20210722 16:19"
        },
        {
          "id": "MDRW2100001040.1.14",
          "form": "재밌어?",
          "original_form": "재밌어?",
          "speaker_id": "2",
          "time": "20210722 16:19"
        },
        {
          "id": "MDRW2100001040.1.15",
          "form": "엄청 재밌어",
          "original_form": "엄청 재밌어",
          "speaker_id": "1",
          "time": "20210722 16:20"
        },
        {
          "id": "MDRW2100001040.1.16",
          "form": "내 인생영화야!!",
          "original_form": "내 인생영화야!!",
          "speaker_id": "1",
          "time": "20210722 16:20"
        },
        {
          "id": "MDRW2100001040.1.17",
          "form": "나는 애니메이션 좋아해",
          "original_form": "나는 애니메이션 좋아해",
          "speaker_id": "2",
          "time": "20210722 16:20"
        },
        {
          "id": "MDRW2100001040.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210722 16:21"
        },
        {
          "id": "MDRW2100001040.1.19",
          "form": "어떤걸 가장 재밌게봤어??",
          "original_form": "어떤걸 가장 재밌게봤어??",
          "speaker_id": "1",
          "time": "20210722 16:21"
        },
        {
          "id": "MDRW2100001040.1.20",
          "form": "디즈니꺼는 다 재밌게 봤어",
          "original_form": "디즈니꺼는 다 재밌게 봤어",
          "speaker_id": "2",
          "time": "20210722 16:22"
        },
        {
          "id": "MDRW2100001040.1.21",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210722 16:22"
        },
        {
          "id": "MDRW2100001040.1.22",
