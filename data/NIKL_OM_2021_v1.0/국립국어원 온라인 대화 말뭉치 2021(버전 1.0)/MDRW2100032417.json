        {
          "id": "MDRW2100032417.1.1",
          "form": "만득이 핫도그라고 하구나",
          "original_form": "만득이 핫도그라고 하구나",
          "speaker_id": "2",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032417.1.2",
          "form": "어릴 때 ㅋㅋㅋ",
          "original_form": "어릴 때 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032417.1.3",
          "form": "만득이처럼 생겼다고해서",
          "original_form": "만득이처럼 생겼다고해서",
          "speaker_id": "1",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032417.1.4",
          "form": "ㅋㅋㅋㅋ 감자 핫도그 맛있지",
          "original_form": "ㅋㅋㅋㅋ 감자 핫도그 맛있지",
          "speaker_id": "2",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032417.1.5",
          "form": "명랑핫도그 생겨서 너무 좋아",
          "original_form": "명랑핫도그 생겨서 너무 좋아",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032417.1.6",
          "form": "저렴하고 맛있긴 하더라",
          "original_form": "저렴하고 맛있긴 하더라",
          "speaker_id": "2",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032417.1.7",
          "form": "뭔가 포장마차에서만 먹다가",
          "original_form": "뭔가 포장마차에서만 먹다가",
          "speaker_id": "1",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100032417.1.8",
          "form": "접근성이랑 위생이 좋아진 느낌",
          "original_form": "접근성이랑 위생이 좋아진 느낌",
          "speaker_id": "1",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100032417.1.9",
          "form": "그렇네",
          "original_form": "그렇네",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100032417.1.10",
          "form": "핫도그 종류도 많아서 너무 좋아",
          "original_form": "핫도그 종류도 많아서 너무 좋아",
          "speaker_id": "1",
          "time": "20210905 17:14"
        },
        {
          "id": "MDRW2100032417.1.11",
          "form": "메뉴 진짜 다양하더라 치즈랑 소스랑",
          "original_form": "메뉴 진짜 다양하더라 치즈랑 소스랑",
          "speaker_id": "2",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100032417.1.12",
          "form": "골라 먹는 재미가 있다!",
          "original_form": "골라 먹는 재미가 있다!",
          "speaker_id": "1",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100032417.1.13",
          "form": "아무말 ㅋㅋㅋ",
          "original_form": "아무말 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100032417.1.14",
          "form": "진짜 골라 먹을 수 있으니까 좋지",
          "original_form": "진짜 골라 먹을 수 있으니까 좋지",
          "speaker_id": "2",
          "time": "20210905 17:15"
        },
