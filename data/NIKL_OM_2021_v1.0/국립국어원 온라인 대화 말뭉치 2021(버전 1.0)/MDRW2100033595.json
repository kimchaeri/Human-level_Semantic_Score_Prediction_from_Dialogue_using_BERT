      "utterance": [
        {
          "id": "MDRW2100033595.1.1",
          "form": "좋아하는 노래 있으세요?",
          "original_form": "좋아하는 노래 있으세요?",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033595.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033595.1.3",
          "form": "너의모드순간 좋아합니다ㅋㅋ",
          "original_form": "너의모드순간 좋아합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033595.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100033595.1.5",
          "form": "그 노래가 성시경 노래죠??!",
          "original_form": "그 노래가 성시경 노래죠??!",
          "speaker_id": "1",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100033595.1.6",
          "form": "그 유명한 이윽고...",
          "original_form": "그 유명한 이윽고...",
          "speaker_id": "1",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100033595.1.7",
          "form": "ㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100033595.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100033595.1.9",
          "form": "저는 다 듣는데 요즘은",
          "original_form": "저는 다 듣는데 요즘은",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100033595.1.10",
          "form": "알콜프리에 빠져있어요",
          "original_form": "알콜프리에 빠져있어요",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100033595.1.11",
          "form": "ㅋㅋㅋㅋ저는 그거 영상을 하도봐서 그런가",
          "original_form": "ㅋㅋㅋㅋ저는 그거 영상을 하도봐서 그런가",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100033595.1.12",
          "form": "그 노래 나올때마다\r\n저도 자꾸 몸을 흔들고 있어요",
          "original_form": "그 노래 나올때마다\r\n저도 자꾸 몸을 흔들고 있어요",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100033595.1.13",
          "form": "트와이스 좋아하세요?",
          "original_form": "트와이스 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100033595.1.14",
          "form": "별 관심 없었는데 요새 좋아졌어요",
          "original_form": "별 관심 없었는데 요새 좋아졌어요",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100033595.1.15",
          "form": "좋아하는 연예인 있으세요?",
          "original_form": "좋아하는 연예인 있으세요?",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100033595.1.16",
          "form": "조인성님 좋아합니당ㅋㅋ",
          "original_form": "조인성님 좋아합니당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100033595.1.17",
          "form": "조인성 잘생겼죠",
          "original_form": "조인성 잘생겼죠",
          "speaker_id": "1",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100033595.1.18",
          "form": "요샌 알고있지만 보면서\r\n송강님도 좋더라구요ㅋㅋ",
          "original_form": "요샌 알고있지만 보면서\r\n송강님도 좋더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100033595.1.19",
          "form": "나오는 드라마 다 보셨나요ㅋㅋ",
          "original_form": "나오는 드라마 다 보셨나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100033595.1.20",
          "form": "조인성님 나오는건 다 본 거 같아요ㅋ",
          "original_form": "조인성님 나오는건 다 본 거 같아요ㅋ",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100033595.1.21",
          "form": "이번에 영화도 개봉했는데\r\n보러가지 못하는게 넘나 아쉽네요",
          "original_form": "이번에 영화도 개봉했는데\r\n보러가지 못하는게 넘나 아쉽네요",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100033595.1.22",
          "form": "코로나 때문에요?",
          "original_form": "코로나 때문에요?",
          "speaker_id": "1",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100033595.1.23",
          "form": "좀 진정이 되어야 볼텐데요 ㅠㅠ",
          "original_form": "좀 진정이 되어야 볼텐데요 ㅠㅠ",
