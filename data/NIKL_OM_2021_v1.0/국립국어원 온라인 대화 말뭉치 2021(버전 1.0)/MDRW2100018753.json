        },
        {
          "id": "MDRW2100018753.1.3",
          "form": "검진 몇시에요",
          "original_form": "검진 몇시에요",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018753.1.4",
          "form": "먹고만다..!",
          "original_form": "먹고만다..!",
          "speaker_id": "2",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018753.1.5",
          "form": "앗 예약도 안햇어요",
          "original_form": "앗 예약도 안햇어요",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018753.1.6",
          "form": "걍..걍 갈려고",
          "original_form": "걍..걍 갈려고",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018753.1.7",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018753.1.8",
          "form": "예약도 안했다구요?",
          "original_form": "예약도 안했다구요?",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018753.1.9",
          "form": "예약없이 검진이 가능한가",
          "original_form": "예약없이 검진이 가능한가",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018753.1.10",
          "form": "1예약....해야했나요",
          "original_form": "1예약....해야했나요",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018753.1.11",
          "form": "보통 예약하지않아요?",
          "original_form": "보통 예약하지않아요?",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018753.1.12",
          "form": "검진은 보통 2차병원에서 하니까",
          "original_form": "검진은 보통 2차병원에서 하니까",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018753.1.13",
          "form": "?!!!",
          "original_form": "?!!!",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018753.1.14",
          "form": "작은 1차에서 안해주지않나",
          "original_form": "작은 1차에서 안해주지않나",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018753.1.15",
          "form": "어 그래요? 피검사만하려고 해서 그냥 내과 갈라했엉ㅎ",
          "original_form": "어 그래요? 피검사만하려고 해서 그냥 내과 갈라했엉ㅎ",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
