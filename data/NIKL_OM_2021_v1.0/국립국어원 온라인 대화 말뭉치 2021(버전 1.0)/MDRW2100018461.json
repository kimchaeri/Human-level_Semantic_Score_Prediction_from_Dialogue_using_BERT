      "utterance": [
        {
          "id": "MDRW2100018461.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 02:16"
        },
        {
          "id": "MDRW2100018461.1.2",
          "form": "안녕하세요? 게임좋아하죠ㅋ\r\n보드게임도 좋아하고 컴퓨터게임도 좋아하고ㅋㅋ",
          "original_form": "안녕하세요? 게임좋아하죠ㅋ\r\n보드게임도 좋아하고 컴퓨터게임도 좋아하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018461.1.3",
          "form": "오올 컴겜은 뭐하세요?",
          "original_form": "오올 컴겜은 뭐하세요?",
          "speaker_id": "1",
          "time": "20210827 02:18"
        },
        {
          "id": "MDRW2100018461.1.4",
          "form": "출시된지 오래되었지만 스타2를 즐겨해요. 님은요??",
          "original_form": "출시된지 오래되었지만 스타2를 즐겨해요. 님은요??",
          "speaker_id": "2",
          "time": "20210827 02:18"
        },
        {
          "id": "MDRW2100018461.1.5",
          "form": "앜ㅋㅋ 전 가끔 피시방가서 오버워치해요",
          "original_form": "앜ㅋㅋ 전 가끔 피시방가서 오버워치해요",
          "speaker_id": "1",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018461.1.6",
          "form": "롤은 안하시나봐요",
          "original_form": "롤은 안하시나봐요",
          "speaker_id": "1",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018461.1.7",
          "form": "오버워치 해봤는데 정신없어서 못하겠더군요ㅋㅋ",
          "original_form": "오버워치 해봤는데 정신없어서 못하겠더군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018461.1.8",
          "form": "롤은 안 해요.",
          "original_form": "롤은 안 해요.",
          "speaker_id": "2",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018461.1.9",
          "form": "롤도 적성에 안 맞는지 못하겠어요. 적성보다는 재능의 문제인가...ㅋㅋㅋ",
          "original_form": "롤도 적성에 안 맞는지 못하겠어요. 적성보다는 재능의 문제인가...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018461.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 롤은 딱 10렙까지만 참고하면 그뒤론 중독된대요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 롤은 딱 10렙까지만 참고하면 그뒤론 중독된대요",
          "speaker_id": "1",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018461.1.11",
          "form": "근데 저도 못하겠더라구욬ㅋㅋ",
          "original_form": "근데 저도 못하겠더라구욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018461.1.12",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018461.1.13",
          "form": "히오스는 조금 해봤는데 재미있더라고요. 워낙 스타를 좋아하니까 캐릭터도 친숙하고",
          "original_form": "히오스는 조금 해봤는데 재미있더라고요. 워낙 스타를 좋아하니까 캐릭터도 친숙하고",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018461.1.14",
          "form": "근데 역시나 실력이 처참함...",
          "original_form": "근데 역시나 실력이 처참함...",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018461.1.15",
          "form": "아핰ㅋㅋㅋㅋㅋㅋㅋ 히오스는 밈으로 넘 많이 쓰여서 친숙하네요",
          "original_form": "아핰ㅋㅋㅋㅋㅋㅋㅋ 히오스는 밈으로 넘 많이 쓰여서 친숙하네요",
          "speaker_id": "1",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018461.1.16",
          "form": "모바일게임은 안하새요?",
          "original_form": "모바일게임은 안하새요?",
          "speaker_id": "1",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018461.1.17",
          "form": "네, 휴대폰이 후져서...ㅋ",
          "original_form": "네, 휴대폰이 후져서...ㅋ",
          "speaker_id": "2",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018461.1.18",
          "form": "그것도 그렇고 해보고 싶고 관심가는 게임들이 있어도",
          "original_form": "그것도 그렇고 해보고 싶고 관심가는 게임들이 있어도",
          "speaker_id": "2",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018461.1.19",
          "form": "휴대폰 자체적으로 조작이 너무 불편해서...",
          "original_form": "휴대폰 자체적으로 조작이 너무 불편해서...",
          "speaker_id": "2",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018461.1.20",
          "form": "턴제 게임이나 단순 아케이드는 몰라도",
          "original_form": "턴제 게임이나 단순 아케이드는 몰라도",
          "speaker_id": "2",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100018461.1.21",
          "form": "앜ㅋㅋㅋ 맞아요 발열도 심하고 속터지긴 하죠",
          "original_form": "앜ㅋㅋㅋ 맞아요 발열도 심하고 속터지긴 하죠",
          "speaker_id": "1",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100018461.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100018461.1.23",
          "form": "자동전투없는겜들은 폰으로 돌리기는쫌 짜증나요",
          "original_form": "자동전투없는겜들은 폰으로 돌리기는쫌 짜증나요",
          "speaker_id": "1",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100018461.1.24",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100018461.1.25",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100018461.1.26",
          "form": "그거 자동전투 같은 거 무슨 재미로 해요? 친구도 항상 그거 돌리는 친구있던듸",
          "original_form": "그거 자동전투 같은 거 무슨 재미로 해요? 친구도 항상 그거 돌리는 친구있던듸",
          "speaker_id": "2",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100018461.1.27",
          "form": "데",
          "original_form": "데",
          "speaker_id": "2",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100018461.1.28",
          "form": "본인이 게임을 하는 기분이 없지 않나요??",
          "original_form": "본인이 게임을 하는 기분이 없지 않나요??",
          "speaker_id": "2",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018461.1.29",
          "form": "그냥... 아이템얻는재미로?",
          "original_form": "그냥... 아이템얻는재미로?",
          "speaker_id": "1",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018461.1.30",
          "form": "맞아욬ㅋㅋ 전 그래서 잘 안해요",
          "original_form": "맞아욬ㅋㅋ 전 그래서 잘 안해요",
          "speaker_id": "1",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018461.1.31",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018461.1.32",
          "form": "저는 다음달에 플스5 지를까 진지하게 고민중...ㅋ",
          "original_form": "저는 다음달에 플스5 지를까 진지하게 고민중...ㅋ",
          "speaker_id": "2",
          "time": "20210827 02:27"
        },
        {
          "id": "MDRW2100018461.1.33",
          "form": "플스2 이후로 콘솔을 써본 일이 없긴하지만",
          "original_form": "플스2 이후로 콘솔을 써본 일이 없긴하지만",
          "speaker_id": "2",
          "time": "20210827 02:27"
        },
        {
          "id": "MDRW2100018461.1.34",
          "form": "왘ㅋㅋ 플스 5얼마예요?",
          "original_form": "왘ㅋㅋ 플스 5얼마예요?",
          "speaker_id": "1",
          "time": "20210827 02:27"
        },
        {
          "id": "MDRW2100018461.1.35",
          "form": "중고로 60중반에서 70초중반?",
          "original_form": "중고로 60중반에서 70초중반?",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018461.1.36",
          "form": "70만원선인것 같아요.",
          "original_form": "70만원선인것 같아요.",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018461.1.37",
          "form": "플스5 신작겜 많나요??",
          "original_form": "플스5 신작겜 많나요??",
          "speaker_id": "1",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018461.1.38",
          "form": "몰라요. 그냥 막연하게 해보고싶은 거라ㅋㅋㅋ",
          "original_form": "몰라요. 그냥 막연하게 해보고싶은 거라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:29"
        },
        {
          "id": "MDRW2100018461.1.39",
          "form": "그 무슨 호라이즌 제로?",
          "original_form": "그 무슨 호라이즌 제로?",
          "speaker_id": "2",
          "time": "20210827 02:29"
        },
        {
          "id": "MDRW2100018461.1.40",
          "form": "그거 해보고싶던데",
          "original_form": "그거 해보고싶던데",
          "speaker_id": "2",
          "time": "20210827 02:29"
        },
        {
          "id": "MDRW2100018461.1.41",
          "form": "재밋어보이네요",
          "original_form": "재밋어보이네요",
          "speaker_id": "1",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018461.1.42",
          "form": "약간 야숨느낌도 나고..",
          "original_form": "약간 야숨느낌도 나고..",
          "speaker_id": "1",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018461.1.43",
          "form": "찾아보셨어요?",
          "original_form": "찾아보셨어요?",
          "speaker_id": "2",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018461.1.44",
          "form": "야숨?? 그건 뭐죠?",
          "original_form": "야숨?? 그건 뭐죠?",
          "speaker_id": "2",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018461.1.45",
          "form": "젤다??(지금 찾아봄)",
          "original_form": "젤다??(지금 찾아봄)",
          "speaker_id": "2",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018461.1.46",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 그냥 자유도가 높은 rpg같아서요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 그냥 자유도가 높은 rpg같아서요",
          "speaker_id": "1",
