      "utterance": [
        {
          "id": "MDRW2100003783.1.1",
          "form": "앗 죄송한데 블로그 이름 못봤어요ㅠ",
          "original_form": "앗 죄송한데 블로그 이름 못봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003783.1.2",
          "form": "재테크의여왕 슈앤슈",
          "original_form": "재테크의여왕 슈앤슈",
          "speaker_id": "1",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003783.1.3",
          "form": "소유보다 경험을",
          "original_form": "소유보다 경험을",
          "speaker_id": "1",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003783.1.4",
          "form": "아~감사합니다!!^^",
          "original_form": "아~감사합니다!!^^",
          "speaker_id": "2",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003783.1.5",
          "form": "항상 블로그 참조해서 재택에서 소소히 공모주로 돈벌어요 ㅎㅎ",
          "original_form": "항상 블로그 참조해서 재택에서 소소히 공모주로 돈벌어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003783.1.6",
          "form": "근데 다른 일거리 찾고파요 ㅜㅜ",
          "original_form": "근데 다른 일거리 찾고파요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003783.1.7",
          "form": "멋지세요~~!!",
          "original_form": "멋지세요~~!!",
          "speaker_id": "2",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003783.1.8",
          "form": "소소해요 ㅋㅋ",
          "original_form": "소소해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003783.1.9",
          "form": "집에서도 돈 벌수 있으면 좋죠",
          "original_form": "집에서도 돈 벌수 있으면 좋죠",
          "speaker_id": "2",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003783.1.10",
          "form": "네 집에서 하는일 있으세요?",
          "original_form": "네 집에서 하는일 있으세요?",
          "speaker_id": "1",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003783.1.11",
          "form": "저는 없어요ㅠㅠ",
          "original_form": "저는 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 12:05"
        },
        {
          "id": "MDRW2100003783.1.12",
          "form": "아 네 공모주 해보세요",
          "original_form": "아 네 공모주 해보세요",
          "speaker_id": "1",
          "time": "20210821 12:05"
        },
        {
          "id": "MDRW2100003783.1.13",
          "form": "상장주식은 마이너스예요",
          "original_form": "상장주식은 마이너스예요",
          "speaker_id": "1",
          "time": "20210821 12:05"
        },
        {
          "id": "MDRW2100003783.1.14",
          "form": "몇십은 벌어요 ㅎㅎ",
          "original_form": "몇십은 벌어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003783.1.15",
          "form": "오~~좋네요~!!",
          "original_form": "오~~좋네요~!!",
          "speaker_id": "2",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003783.1.16",
          "form": "카뱅 짱이였어요 ㅋㅋ",
          "original_form": "카뱅 짱이였어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003783.1.17",
          "form": "소소하게 소감도 남기고 그래요",
          "original_form": "소소하게 소감도 남기고 그래요",
          "speaker_id": "1",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003783.1.18",
          "form": "뭔가 일을 하고 돈벌면 활력이 솟아요",
          "original_form": "뭔가 일을 하고 돈벌면 활력이 솟아요",
          "speaker_id": "1",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003783.1.19",
          "form": "집에선 무기력해요",
          "original_form": "집에선 무기력해요",
          "speaker_id": "1",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003783.1.20",
          "form": "근데 좀 더 벌고 싶어요 집에서",
          "original_form": "근데 좀 더 벌고 싶어요 집에서",
          "speaker_id": "1",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003783.1.21",
          "form": "맞아요~~집에서도 돈벌면 최고죠",
          "original_form": "맞아요~~집에서도 돈벌면 최고죠",
          "speaker_id": "2",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003783.1.22",
          "form": "전 이상한건싫고요",
          "original_form": "전 이상한건싫고요",
          "speaker_id": "1",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003783.1.23",
          "form": "이상한건 어떤거요?",
          "original_form": "이상한건 어떤거요?",
          "speaker_id": "2",
          "time": "20210821 12:08"
        },
        {
          "id": "MDRW2100003783.1.24",
          "form": "미얀마에서 광석사다 온라인에서 팔았는데 망했어요 ㅜㅜ",
          "original_form": "미얀마에서 광석사다 온라인에서 팔았는데 망했어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 12:12"
        },
        {
          "id": "MDRW2100003783.1.25",
          "form": "재택에서 돈벌기 힘들어요",
          "original_form": "재택에서 돈벌기 힘들어요",
          "speaker_id": "1",
          "time": "20210821 12:12"
        },
        {
          "id": "MDRW2100003783.1.26",
          "form": "요새 그 사람들 댓글알바랑 그런거요",
          "original_form": "요새 그 사람들 댓글알바랑 그런거요",
          "speaker_id": "1",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003783.1.27",
          "form": "자기가 노력하고 하는일아님 사기가 많은거 같아요",
          "original_form": "자기가 노력하고 하는일아님 사기가 많은거 같아요",
          "speaker_id": "1",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003783.1.28",
          "form": "내가 힘들고 내돈으로 노력한게 돈을 버는거 같아요 ㅎㅎ",
          "original_form": "내가 힘들고 내돈으로 노력한게 돈을 버는거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 12:15"
        }
      ]
    }
