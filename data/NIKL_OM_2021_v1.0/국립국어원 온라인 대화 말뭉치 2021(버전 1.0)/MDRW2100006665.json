      "utterance": [
        {
          "id": "MDRW2100006665.1.1",
          "form": "이번 신제품 보셨나요",
          "original_form": "이번 신제품 보셨나요",
          "speaker_id": "1",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100006665.1.2",
          "form": "넵!!",
          "original_form": "넵!!",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100006665.1.3",
          "form": "플립 너무 예쁘네요",
          "original_form": "플립 너무 예쁘네요",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100006665.1.4",
          "form": "되게 잘될거같지않나요?",
          "original_form": "되게 잘될거같지않나요?",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100006665.1.5",
          "form": "네 사전예약때부터 반응이 뜨겁더라구요",
          "original_form": "네 사전예약때부터 반응이 뜨겁더라구요",
          "speaker_id": "1",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100006665.1.6",
          "form": "역시 디자인을 잘뽑아야돼요",
          "original_form": "역시 디자인을 잘뽑아야돼요",
          "speaker_id": "2",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100006665.1.7",
          "form": "실물도 보신건가요",
          "original_form": "실물도 보신건가요",
          "speaker_id": "1",
          "time": "20210908 05:55"
        },
        {
          "id": "MDRW2100006665.1.8",
          "form": "실물 아직 안봤어요..",
          "original_form": "실물 아직 안봤어요..",
          "speaker_id": "2",
          "time": "20210908 05:55"
        },
        {
          "id": "MDRW2100006665.1.9",
          "form": "아 아니다 봤어요",
          "original_form": "아 아니다 봤어요",
          "speaker_id": "2",
          "time": "20210908 05:55"
        },
        {
          "id": "MDRW2100006665.1.10",
          "form": "알바하면서 봤는데 예쁘더라구요",
          "original_form": "알바하면서 봤는데 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100006665.1.11",
          "form": "실물보셨나요?",
          "original_form": "실물보셨나요?",
          "speaker_id": "2",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100006665.1.12",
          "form": "어떤 색이 최고던가요?",
          "original_form": "어떤 색이 최고던가요?",
          "speaker_id": "1",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100006665.1.13",
          "form": "크림이요!",
          "original_form": "크림이요!",
          "speaker_id": "2",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100006665.1.14",
          "form": "선생님은 어떤색 좋으세요?",
          "original_form": "선생님은 어떤색 좋으세요?",
          "speaker_id": "2",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100006665.1.15",
          "form": "아 지금껏 대화나눈바로는 라벤더가 가장 핫했는데 크림도 인기 많죠",
          "original_form": "아 지금껏 대화나눈바로는 라벤더가 가장 핫했는데 크림도 인기 많죠",
          "speaker_id": "1",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100006665.1.16",
          "form": "그쵸 라벤더가 온라인에서 인기더라구요",
          "original_form": "그쵸 라벤더가 온라인에서 인기더라구요",
          "speaker_id": "2",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100006665.1.17",
          "form": "근데 실물은 크림이 예뻐요",
          "original_form": "근데 실물은 크림이 예뻐요",
          "speaker_id": "2",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100006665.1.18",
          "form": "실물을.못봤네요 폴드도 보셨나요",
          "original_form": "실물을.못봤네요 폴드도 보셨나요",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100006665.1.19",
          "form": "폴드 봤어요!! 폴드도 예뻐요",
          "original_form": "폴드 봤어요!! 폴드도 예뻐요",
          "speaker_id": "2",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100006665.1.20",
          "form": "폴드는 색상이 하나인가요?",
          "original_form": "폴드는 색상이 하나인가요?",
