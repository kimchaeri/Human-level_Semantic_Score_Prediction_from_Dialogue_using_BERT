      "utterance": [
        {
          "id": "MDRW2100016758.1.1",
          "form": "맛집 좋아하시나요",
          "original_form": "맛집 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016758.1.2",
          "form": "네 좋아하는데 코로나라 못가요",
          "original_form": "네 좋아하는데 코로나라 못가요",
          "speaker_id": "2",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016758.1.3",
          "form": "최근간덴 힙지로?",
          "original_form": "최근간덴 힙지로?",
          "speaker_id": "2",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016758.1.4",
          "form": "오 어떤거드셨나요",
          "original_form": "오 어떤거드셨나요",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016758.1.5",
          "form": "우육면이요~",
          "original_form": "우육면이요~",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016758.1.6",
          "form": "오 우육면 저도 좋아해요",
          "original_form": "오 우육면 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016758.1.7",
          "form": "어디서 드셨나요? 전 을지로 줄리아요",
          "original_form": "어디서 드셨나요? 전 을지로 줄리아요",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016758.1.8",
          "form": "저는 대만에서 먹어본 맛 잊지못해요ㅜ",
          "original_form": "저는 대만에서 먹어본 맛 잊지못해요ㅜ",
          "speaker_id": "1",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016758.1.9",
          "form": "ㅠ제가 먹은 줄리아는 한국식 퓨전이었어요",
          "original_form": "ㅠ제가 먹은 줄리아는 한국식 퓨전이었어요",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016758.1.10",
          "form": "소고기 무국 맛이에요",
          "original_form": "소고기 무국 맛이에요",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016758.1.11",
          "form": "오 줄리아 함 검색해바야겠네요",
          "original_form": "오 줄리아 함 검색해바야겠네요",
          "speaker_id": "1",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016758.1.12",
          "form": "한국화된 맛의 중식집이에요.",
          "original_form": "한국화된 맛의 중식집이에요.",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016758.1.13",
          "form": "최근 또 추천 받은 집은 을지로 촙촙인데",
          "original_form": "최근 또 추천 받은 집은 을지로 촙촙인데",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016758.1.14",
          "form": "chop chop",
          "original_form": "chop chop",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016758.1.15",
          "form": "역시 힙지로 맛집많네요",
          "original_form": "역시 힙지로 맛집많네요",
          "speaker_id": "1",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016758.1.16",
          "form": "네 많은데 사람도 많아서..",
          "original_form": "네 많은데 사람도 많아서..",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016758.1.17",
          "form": "코로나여도 점심시간을 틈타갔는데",
          "original_form": "코로나여도 점심시간을 틈타갔는데",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016758.1.18",
          "form": "촙촙은 대기가 3팀이어서 못먹었어요",
          "original_form": "촙촙은 대기가 3팀이어서 못먹었어요",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016758.1.19",
          "form": "코로나라 찝찝하긴하네요",
          "original_form": "코로나라 찝찝하긴하네요",
          "speaker_id": "1",
          "time": "20210826 16:39"
        }
      ]
