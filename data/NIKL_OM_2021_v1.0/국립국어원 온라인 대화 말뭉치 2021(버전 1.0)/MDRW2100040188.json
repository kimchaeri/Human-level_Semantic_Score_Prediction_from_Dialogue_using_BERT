      "utterance": [
        {
          "id": "MDRW2100040188.1.1",
          "form": "코로나 이후 여행 계획 있으세요?",
          "original_form": "코로나 이후 여행 계획 있으세요?",
          "speaker_id": "2",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100040188.1.2",
          "form": "코로나 이후엔 국내여행을 좀 활발히 다녀볼까 해요",
          "original_form": "코로나 이후엔 국내여행을 좀 활발히 다녀볼까 해요",
          "speaker_id": "1",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100040188.1.3",
          "form": "그동안 휴가때마다 해외 자주 다녔는데",
          "original_form": "그동안 휴가때마다 해외 자주 다녔는데",
          "speaker_id": "1",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100040188.1.4",
          "form": "한동안 해외 나가는 건 좀 걱정될 것 같아요",
          "original_form": "한동안 해외 나가는 건 좀 걱정될 것 같아요",
          "speaker_id": "1",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100040188.1.5",
          "form": "다들 해외는 걱정 많이 하더라구요",
          "original_form": "다들 해외는 걱정 많이 하더라구요",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100040188.1.6",
          "form": "네 아무래도 해외여행은 당분간...",
          "original_form": "네 아무래도 해외여행은 당분간...",
          "speaker_id": "1",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100040188.1.7",
          "form": "국내여행이 인기가 많아질 듯해요 ㅎㅎ",
          "original_form": "국내여행이 인기가 많아질 듯해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100040188.1.8",
          "form": "네 name2님은 어디 여행지가 좋으셨어요?",
          "original_form": "네 &name2&님은 어디 여행지가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210904 13:48"
        },
        {
          "id": "MDRW2100040188.1.9",
          "form": "좀 오래되긴 했지만 부여요",
          "original_form": "좀 오래되긴 했지만 부여요",
          "speaker_id": "2",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100040188.1.10",
          "form": "요즘은 많이 개발되서 더 좋을 것 같아요",
          "original_form": "요즘은 많이 개발되서 더 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100040188.1.11",
          "form": "오 부여요?",
          "original_form": "오 부여요?",
          "speaker_id": "1",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100040188.1.12",
          "form": "부여는 초등학생 때 가보고 안가봤는데",
          "original_form": "부여는 초등학생 때 가보고 안가봤는데",
          "speaker_id": "1",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100040188.1.13",
          "form": "부여 어떤 점이 좋으셨어요?",
          "original_form": "부여 어떤 점이 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100040188.1.14",
          "form": "전 낙화암이랑 그 아래 강이 좋았어요",
          "original_form": "전 낙화암이랑 그 아래 강이 좋았어요",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100040188.1.15",
          "form": "저도 어릴 때 간거라 잘 생각이 안나네요",
          "original_form": "저도 어릴 때 간거라 잘 생각이 안나네요",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100040188.1.16",
          "form": "아 경치랑 분위기가 좋았다는 얘기시군요",
          "original_form": "아 경치랑 분위기가 좋았다는 얘기시군요",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100040188.1.17",
          "form": "저는 몇년 전에 순천 가봤는데 좋았어요",
          "original_form": "저는 몇년 전에 순천 가봤는데 좋았어요",
          "speaker_id": "1",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100040188.1.18",
          "form": "순천하면 고추장밖에 생각이 안 나네요",
          "original_form": "순천하면 고추장밖에 생각이 안 나네요",
          "speaker_id": "2",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100040188.1.19",
          "form": "어떤 점이 좋으셨나요?",
          "original_form": "어떤 점이 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100040188.1.20",
          "form": "아 고추장은 순창 아닌가요ㅎㅎ",
          "original_form": "아 고추장은 순창 아닌가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100040188.1.21",
          "form": "앗 잘못봤네요 ㅋㅋ",
          "original_form": "앗 잘못봤네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100040188.1.22",
          "form": "순천만 갈대숲이 좋더라고요 저는",
          "original_form": "순천만 갈대숲이 좋더라고요 저는",
          "speaker_id": "1",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100040188.1.23",
          "form": "갈대숲 가을에 가면 참 좋겠네요",
          "original_form": "갈대숲 가을에 가면 참 좋겠네요",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100040188.1.24",
          "form": "네 특히 가을에 가면 좋죠",
          "original_form": "네 특히 가을에 가면 좋죠",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100040188.1.25",
          "form": "일몰시간이 진짜 멋있어요",
          "original_form": "일몰시간이 진짜 멋있어요",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100040188.1.26",
          "form": "와 경치며 분위기까지 좋겠어요 보고 싶네요",
          "original_form": "와 경치며 분위기까지 좋겠어요 보고 싶네요",
          "speaker_id": "2",
          "time": "20210904 14:22"
        }
      ]
