      "utterance": [
        {
          "id": "MDRW2100040366.1.1",
          "form": "그아악 못 봤어요 갈비집에 양념게장 나와요?",
          "original_form": "그아악 못 봤어요 갈비집에 양념게장 나와요?",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100040366.1.2",
          "form": "악ㅋㅋㅋㅋㅋㅋㅋㅋ가봐야 알 것 같아요",
          "original_form": "악ㅋㅋㅋㅋㅋㅋㅋㅋ가봐야 알 것 같아요",
          "speaker_id": "1",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100040366.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ양념게장 중요하죠...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ양념게장 중요하죠...",
          "speaker_id": "1",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100040366.1.4",
          "form": "진짜 맛있는데 요즘 나오는 곳이 드물어요 흐흑",
          "original_form": "진짜 맛있는데 요즘 나오는 곳이 드물어요 흐흑",
          "speaker_id": "2",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100040366.1.5",
          "form": "갈비먹고 주로 냉면 드시나요?",
          "original_form": "갈비먹고 주로 냉면 드시나요?",
          "speaker_id": "2",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100040366.1.6",
          "form": "당빠죠 같이먹어요 저는",
          "original_form": "당빠죠 같이먹어요 저는",
          "speaker_id": "1",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100040366.1.7",
          "form": "ㅋㅋㅋㅋㅋ같이 먹는 거 너무 맛있어요",
          "original_form": "ㅋㅋㅋㅋㅋ같이 먹는 거 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100040366.1.8",
          "form": "물냉면파신가요 비빔냉면파신가요!",
          "original_form": "물냉면파신가요 비빔냉면파신가요!",
          "speaker_id": "2",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100040366.1.9",
          "form": "저는 비냉파!",
          "original_form": "저는 비냉파!",
          "speaker_id": "1",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040366.1.10",
          "form": "하지만 주변에 한명쯤 물냉먹으면 좋겠어요",
          "original_form": "하지만 주변에 한명쯤 물냉먹으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040366.1.11",
          "form": "뺏어먹게",
          "original_form": "뺏어먹게",
          "speaker_id": "1",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040366.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040366.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 공감이예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 공감이예요",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040366.1.14",
          "form": "전 물냉판데",
          "original_form": "전 물냉판데",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040366.1.15",
          "form": "다 물냉먹으면 좀",
          "original_form": "다 물냉먹으면 좀",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040366.1.16",
          "form": "비냉도 좋아하고 암튼 둘 중 하나만 먹다보면 약간 질려서",
          "original_form": "비냉도 좋아하고 암튼 둘 중 하나만 먹다보면 약간 질려서",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040366.1.17",
          "form": "섭섭하시죠",
          "original_form": "섭섭하시죠",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040366.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040366.1.19",
          "form": "ㅋㅋㅋㅋㅋ남의 거 좀 뺏어먹어야 해요",
          "original_form": "ㅋㅋㅋㅋㅋ남의 거 좀 뺏어먹어야 해요",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040366.1.20",
          "form": "짬짜면처럼 물비냉도 나왔으면 하는 마음",
          "original_form": "짬짜면처럼 물비냉도 나왔으면 하는 마음",
          "speaker_id": "2",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100040366.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 저도 물냉 한입쯤 먹는거 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 저도 물냉 한입쯤 먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100040366.1.22",
          "form": "물비냉은 좀 과해요 저는 딱 한두젓가락만",
          "original_form": "물비냉은 좀 과해요 저는 딱 한두젓가락만",
          "speaker_id": "1",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040366.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040366.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저랑 같이 가서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저랑 같이 가서",
          "speaker_id": "2",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040366.1.25",
          "form": "한 젓가락만 바꿔먹읍시다",
          "original_form": "한 젓가락만 바꿔먹읍시다",
          "speaker_id": "2",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040366.1.26",
          "form": "딱 좋네요",
          "original_form": "딱 좋네요",
