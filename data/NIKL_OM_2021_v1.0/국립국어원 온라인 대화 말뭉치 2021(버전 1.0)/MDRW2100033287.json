      "utterance": [
        {
          "id": "MDRW2100033287.1.1",
          "form": "앙뇽",
          "original_form": "앙뇽",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100033287.1.2",
          "form": "안녕~",
          "original_form": "안녕~",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100033287.1.3",
          "form": "나는 10월에 뮤지컬을 보기로 햇서",
          "original_form": "나는 10월에 뮤지컬을 보기로 햇서",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100033287.1.4",
          "form": "무슨 뮤지컬인뎅",
          "original_form": "무슨 뮤지컬인뎅",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100033287.1.5",
          "form": "하데스타운~ 한국에서 처음으로 개막하는 공연이야",
          "original_form": "하데스타운~ 한국에서 처음으로 개막하는 공연이야",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100033287.1.6",
          "form": "대박~",
          "original_form": "대박~",
          "speaker_id": "1",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100033287.1.7",
          "form": "어쩐지 첨 들어보더라!",
          "original_form": "어쩐지 첨 들어보더라!",
          "speaker_id": "1",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100033287.1.8",
          "form": "오늘 조교님이 혜화에 자주 놀러오냐고 해서",
          "original_form": "오늘 조교님이 혜화에 자주 놀러오냐고 해서",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100033287.1.9",
          "form": "그렇다고 하니까 연극을 보러 오는 거녜",
          "original_form": "그렇다고 하니까 연극을 보러 오는 거녜",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100033287.1.10",
          "form": "그래서 연극은 본 적 없고 뮤지컬은 몇 번 봣다고 햇더니",
          "original_form": "그래서 연극은 본 적 없고 뮤지컬은 몇 번 봣다고 햇더니",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100033287.1.11",
          "form": "무슨 뮤지컬을 봣녜",
          "original_form": "무슨 뮤지컬을 봣녜",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100033287.1.12",
          "form": "말해도 모를 것 같앗는데...",
          "original_form": "말해도 모를 것 같앗는데...",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100033287.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100033287.1.14",
          "form": "그래서 레드북을 봤다고 하니깐",
          "original_form": "그래서 레드북을 봤다고 하니깐",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100033287.1.15",
          "form": "코로나인데 뮤지컬 하냐는 거야",
          "original_form": "코로나인데 뮤지컬 하냐는 거야",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100033287.1.16",
          "form": "그래서 내가 봣던 뮤지컬 회차가 취소됏다가",
          "original_form": "그래서 내가 봣던 뮤지컬 회차가 취소됏다가",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100033287.1.17",
          "form": "다시 티켓 예매햇다구 설명햇지",
          "original_form": "다시 티켓 예매햇다구 설명햇지",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100033287.1.18",
          "form": "잘햇옹",
          "original_form": "잘햇옹",
          "speaker_id": "1",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100033287.1.19",
          "form": "근데 연극은 하나봐??",
          "original_form": "근데 연극은 하나봐??",
          "speaker_id": "1",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100033287.1.20",
          "form": "연극도 많이는 안할걸",
          "original_form": "연극도 많이는 안할걸",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100033287.1.21",
          "form": "전에 보니까 연극 안해서 대학로가 텅텅 비엇더라",
          "original_form": "전에 보니까 연극 안해서 대학로가 텅텅 비엇더라",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100033287.1.22",
          "form": "그나마 요즘? 올해 들어서는",
          "original_form": "그나마 요즘? 올해 들어서는",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100033287.1.23",
          "form": "관객 자리 그머냐 비우기 해가지고",
          "original_form": "관객 자리 그머냐 비우기 해가지고",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100033287.1.24",
          "form": "하긴 하는데",
          "original_form": "하긴 하는데",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100033287.1.25",
          "form": "근데 혜화하면 연극이 유명하긴 하넹\r\n바로 연극보러 오냐고 묻는거 보면!",
          "original_form": "근데 혜화하면 연극이 유명하긴 하넹\r\n바로 연극보러 오냐고 묻는거 보면!",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100033287.1.26",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100033287.1.27",
          "form": "자리가 가변석이랑 고정석? 이러케 나뉘어",
          "original_form": "자리가 가변석이랑 고정석? 이러케 나뉘어",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100033287.1.28",
          "form": "가변석은 취소될 수도 잇어",
          "original_form": "가변석은 취소될 수도 잇어",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100033287.1.29",
          "form": "웅 헤화는 연극 유명하지",
          "original_form": "웅 헤화는 연극 유명하지",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100033287.1.30",
          "form": "아하 신기하다",
          "original_form": "아하 신기하다",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100033287.1.31",
          "form": "너두 봣자나 길거리에서 사람들이",
          "original_form": "너두 봣자나 길거리에서 사람들이",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100033287.1.32",
          "form": "연극 예매하셧어요~?? 하고",
          "original_form": "연극 예매하셧어요~?? 하고",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100033287.1.33",
          "form": "엄청 표 판다니깐",
          "original_form": "엄청 표 판다니깐",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100033287.1.34",
          "form": "가변석이면 계속 긴장하고 있어야겠네",
          "original_form": "가변석이면 계속 긴장하고 있어야겠네",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100033287.1.35",
          "form": "웅 봤지렁~",
          "original_form": "웅 봤지렁~",
          "speaker_id": "1",
          "time": "20210906 21:18"
        }
      ]
    }
