      "utterance": [
        {
          "id": "MDRW2100033550.1.1",
          "form": "네네 일찍영접하시길 권합니다",
          "original_form": "네네 일찍영접하시길 권합니다",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100033550.1.2",
          "form": "손목이 망가지기전에요",
          "original_form": "손목이 망가지기전에요",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100033550.1.3",
          "form": "피씨를",
          "original_form": "피씨를",
          "speaker_id": "1",
          "time": "20210824 13:32"
        },
        {
          "id": "MDRW2100033550.1.4",
          "form": "자주쓰시나보네요?",
          "original_form": "자주쓰시나보네요?",
          "speaker_id": "1",
          "time": "20210824 13:32"
        },
        {
          "id": "MDRW2100033550.1.5",
          "form": "아 네 그런일을자주하네요",
          "original_form": "아 네 그런일을자주하네요",
          "speaker_id": "2",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100033550.1.6",
          "form": "요새 재택근무많으니 피씨로 많이들하시더라구요",
          "original_form": "요새 재택근무많으니 피씨로 많이들하시더라구요",
          "speaker_id": "2",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100033550.1.7",
          "form": "와우 아예재택인가요? 그래서마우스를 사셨군요",
          "original_form": "와우 아예재택인가요? 그래서마우스를 사셨군요",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100033550.1.8",
          "form": "카페가더라도 노트북써야하면 마우스챙겨가야죵",
          "original_form": "카페가더라도 노트북써야하면 마우스챙겨가야죵",
          "speaker_id": "2",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100033550.1.9",
          "form": "ㅋㅋㅋ 본격적이네요",
          "original_form": "ㅋㅋㅋ 본격적이네요",
          "speaker_id": "1",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100033550.1.10",
          "form": "저는 패드주로쓰는데 마우스를 안씁니다",
          "original_form": "저는 패드주로쓰는데 마우스를 안씁니다",
          "speaker_id": "1",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100033550.1.11",
          "form": "오홍 패드 편하신가요?",
          "original_form": "오홍 패드 편하신가요?",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100033550.1.12",
          "form": "ㅋㅋㅋ넵",
          "original_form": "ㅋㅋㅋ넵",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033550.1.13",
          "form": "펜슬을 주로 쓴답니다",
          "original_form": "펜슬을 주로 쓴답니다",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033550.1.14",
          "form": "오오 아이패드 부럽당",
          "original_form": "오오 아이패드 부럽당",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033550.1.15",
          "form": "저 안써봤어요ㅜㅜㅜ",
          "original_form": "저 안써봤어요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033550.1.16",
          "form": "ㅋㅋㅋ저는요즘 가습기도",
          "original_form": "ㅋㅋㅋ저는요즘 가습기도",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033550.1.17",
          "form": "사고싶어요",
          "original_form": "사고싶어요",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033550.1.18",
          "form": "가습기요? 제습기가아니구요?",
          "original_form": "가습기요? 제습기가아니구요?",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033550.1.19",
          "form": "네네 가습기요ㅠㅠ",
          "original_form": "네네 가습기요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033550.1.20",
          "form": "건조해서요",
          "original_form": "건조해서요",
          "speaker_id": "1",
          "time": "20210824 23:53"
        }
      ]
