        {
          "id": "MDRW2100029041.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100029041.1.2",
          "form": "오랜만이네요",
          "original_form": "오랜만이네요",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100029041.1.3",
          "form": "올만에 올리브영 털고왔어요",
          "original_form": "올만에 올리브영 털고왔어요",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100029041.1.4",
          "form": "전 장보고왔어요",
          "original_form": "전 장보고왔어요",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100029041.1.5",
          "form": "크 좋은소비하셨나요",
          "original_form": "크 좋은소비하셨나요",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100029041.1.6",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100029041.1.7",
          "form": "요증 소비독올라서ㅜㅜ",
          "original_form": "요증 소비독올라서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100029041.1.8",
          "form": "소비독이라니ㅋㅋ",
          "original_form": "소비독이라니ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100029041.1.9",
          "form": "짠테크잘안하게돼요",
          "original_form": "짠테크잘안하게돼요",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100029041.1.10",
          "form": "소비는 관성이라잖아요ㅋㅋ",
          "original_form": "소비는 관성이라잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100029041.1.11",
          "form": "그런거아닐까여",
          "original_form": "그런거아닐까여",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100029041.1.12",
          "form": "그러게요....",
          "original_form": "그러게요....",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100029041.1.13",
          "form": "또 한주 절약하고 살면 괜찮을거에요",
          "original_form": "또 한주 절약하고 살면 괜찮을거에요",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100029041.1.14",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100029041.1.15",
          "form": "가을옷 이쁜 거 진짜 많죠",
          "original_form": "가을옷 이쁜 거 진짜 많죠",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100029041.1.16",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100029041.1.17",
          "form": "쇼핑갈까봐요",
          "original_form": "쇼핑갈까봐요",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100029041.1.18",
          "form": "좋은걸루사세요ㅋㅋ",
          "original_form": "좋은걸루사세요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100029041.1.19",
          "form": "그럴게요",
          "original_form": "그럴게요",
          "speaker_id": "2",
