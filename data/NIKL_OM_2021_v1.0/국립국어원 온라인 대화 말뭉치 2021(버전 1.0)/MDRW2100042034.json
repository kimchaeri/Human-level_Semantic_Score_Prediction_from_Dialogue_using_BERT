      "utterance": [
        {
          "id": "MDRW2100042034.1.1",
          "form": "예능 보시는거 있으세요?",
          "original_form": "예능 보시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100042034.1.2",
          "form": "저는 아스트로 플레이랑",
          "original_form": "저는 아스트로 플레이랑",
          "speaker_id": "2",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100042034.1.3",
          "form": "해방타운 좋아해요 나혼산이랑",
          "original_form": "해방타운 좋아해요 나혼산이랑",
          "speaker_id": "2",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100042034.1.4",
          "form": "우아 해방타운 ㅋㅋㅋㅋㅋ허재",
          "original_form": "우아 해방타운 ㅋㅋㅋㅋㅋ허재",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100042034.1.5",
          "form": "허재 완전 웃겨요",
          "original_form": "허재 완전 웃겨요",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100042034.1.6",
          "form": "네네 허재 웃겨요",
          "original_form": "네네 허재 웃겨요",
          "speaker_id": "2",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100042034.1.7",
          "form": "나혼산엔 허훈 나오자나요 ㅋㅋㅋ",
          "original_form": "나혼산엔 허훈 나오자나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100042034.1.8",
          "form": "코부자 삼인방",
          "original_form": "코부자 삼인방",
          "speaker_id": "2",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100042034.1.9",
          "form": "아맞다 나왓죠 ㅋㅋㅋ 두번 나온거죠??",
          "original_form": "아맞다 나왓죠 ㅋㅋㅋ 두번 나온거죠??",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100042034.1.10",
          "form": "네네그런거같아요",
          "original_form": "네네그런거같아요",
          "speaker_id": "2",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100042034.1.11",
          "form": "다들 위트있어요",
          "original_form": "다들 위트있어요",
          "speaker_id": "2",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100042034.1.12",
          "form": "연애 프로그램도 보세요??",
          "original_form": "연애 프로그램도 보세요??",
          "speaker_id": "1",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100042034.1.13",
          "form": "하트시그널 좋아해요",
          "original_form": "하트시그널 좋아해요",
          "speaker_id": "2",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042034.1.14",
          "form": "오오 ㅠㅠ",
          "original_form": "오오 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042034.1.15",
          "form": "시즌 3까지 나왓죠",
          "original_form": "시즌 3까지 나왓죠",
          "speaker_id": "1",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042034.1.16",
          "form": "아마도요? 누구좋아하세요",
          "original_form": "아마도요? 누구좋아하세요",
          "speaker_id": "2",
          "time": "20210906 18:59"
        },
        {
          "id": "MDRW2100042034.1.17",
          "form": "저는 한의사가 기억에 남네요 !!",
          "original_form": "저는 한의사가 기억에 남네요 !!",
          "speaker_id": "1",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100042034.1.18",
          "form": "아하 김도균씨",
          "original_form": "아하 김도균씨",
          "speaker_id": "2",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100042034.1.19",
          "form": "저는 이규빈이요ㅠㅠ",
          "original_form": "저는 이규빈이요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100042034.1.20",
          "form": "여자는 임현주",
          "original_form": "여자는 임현주",
