      "utterance": [
        {
          "id": "MDRW2100008400.1.1",
          "form": "개인전에서도 좋은결과였으면 했는데 아쉬웠어요",
          "original_form": "개인전에서도 좋은결과였으면 했는데 아쉬웠어요",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008400.1.2",
          "form": "배구도 보셨다고 하셨죠?",
          "original_form": "배구도 보셨다고 하셨죠?",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008400.1.3",
          "form": "예전엔 선수들이 가운데",
          "original_form": "예전엔 선수들이 가운데",
          "speaker_id": "2",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008400.1.4",
          "form": "10점에 렌즈 막깼어요",
          "original_form": "10점에 렌즈 막깼어요",
          "speaker_id": "2",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008400.1.5",
          "form": "우리 선수들 신이였어요",
          "original_form": "우리 선수들 신이였어요",
          "speaker_id": "2",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008400.1.6",
          "form": "우리나라 양궁이 올림픽 먹여 살려요 ㅋㅋ",
          "original_form": "우리나라 양궁이 올림픽 먹여 살려요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008400.1.7",
          "form": "와 소름이네요",
          "original_form": "와 소름이네요",
          "speaker_id": "1",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008400.1.8",
          "form": "네 배구 응원 이벤트 하며 봤어료",
          "original_form": "네 배구 응원 이벤트 하며 봤어료",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008400.1.9",
          "form": "네 지금이나 이러지 그땐 진짜 원래그랬어요",
          "original_form": "네 지금이나 이러지 그땐 진짜 원래그랬어요",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008400.1.10",
          "form": "배구는 브라질인가랑 하는데 답답해서",
          "original_form": "배구는 브라질인가랑 하는데 답답해서",
          "speaker_id": "1",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008400.1.11",
          "form": "티비껐어요",
          "original_form": "티비껐어요",
          "speaker_id": "1",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008400.1.12",
          "form": "편파판정에 체력적으로도 남자선수들같더라구요",
          "original_form": "편파판정에 체력적으로도 남자선수들같더라구요",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008400.1.13",
          "form": "아 그렇죠 저도 거기선 좀",
          "original_form": "아 그렇죠 저도 거기선 좀",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008400.1.14",
          "form": "결승전은 진짜 너무 안타까웠어요",
          "original_form": "결승전은 진짜 너무 안타까웠어요",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008400.1.15",
          "form": "그래도 열심히 해주고 김연경 예능만 봐서 몰랐는데",
          "original_form": "그래도 열심히 해주고 김연경 예능만 봐서 몰랐는데",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008400.1.16",
          "form": "넘 멋졌어요",
          "original_form": "넘 멋졌어요",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008400.1.17",
          "form": "도핑도 왠지 한명만 걸릴것같지않은 느킴적인 느낌이였어요",
          "original_form": "도핑도 왠지 한명만 걸릴것같지않은 느킴적인 느낌이였어요",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008400.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008400.1.19",
          "form": "저도 김연경 너무 반했어요 머쩌부러요",
          "original_form": "저도 김연경 너무 반했어요 머쩌부러요",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008400.1.20",
          "form": "다그럴걸요",
          "original_form": "다그럴걸요",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008400.1.21",
          "form": "그죠 ㅎㅎ",
          "original_form": "그죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008400.1.22",
          "form": "의심하기시작하니 끝도 없지만요",
          "original_form": "의심하기시작하니 끝도 없지만요",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008400.1.23",
          "form": "기분이 너무 안좋아지더라구요 ㅎ",
          "original_form": "기분이 너무 안좋아지더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008400.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008400.1.25",
          "form": "솔직히 일본에서 해서 믿을수가 없어요",
          "original_form": "솔직히 일본에서 해서 믿을수가 없어요",
          "speaker_id": "2",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008400.1.26",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008400.1.27",
          "form": "진짜 일본하고는 멀해도 안맞는거같아요 ㅎ",
          "original_form": "진짜 일본하고는 멀해도 안맞는거같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008400.1.28",
          "form": "멎아요 무조건 이기고 봐야해요",
          "original_form": "멎아요 무조건 이기고 봐야해요",
          "speaker_id": "2",
