        },
        {
          "id": "MDRW2100007636.1.2",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210822 08:53"
        },
        {
          "id": "MDRW2100007636.1.3",
          "form": "방갑구먼",
          "original_form": "방갑구먼",
          "speaker_id": "1",
          "time": "20210822 08:53"
        },
        {
          "id": "MDRW2100007636.1.4",
          "form": "ㅋㅋㅋㅋ 그러게.",
          "original_form": "ㅋㅋㅋㅋ 그러게.",
          "speaker_id": "2",
          "time": "20210822 08:53"
        },
        {
          "id": "MDRW2100007636.1.5",
          "form": "너도 맛집 자주가?",
          "original_form": "너도 맛집 자주가?",
          "speaker_id": "2",
          "time": "20210822 08:54"
        },
        {
          "id": "MDRW2100007636.1.6",
          "form": "그러게말이다 그래 자주가",
          "original_form": "그러게말이다 그래 자주가",
          "speaker_id": "1",
          "time": "20210822 08:54"
        },
        {
          "id": "MDRW2100007636.1.7",
          "form": "음식 뭐 좋아하는 데",
          "original_form": "음식 뭐 좋아하는 데",
          "speaker_id": "2",
          "time": "20210822 08:54"
        },
        {
          "id": "MDRW2100007636.1.8",
          "form": "수비드",
          "original_form": "수비드",
          "speaker_id": "1",
          "time": "20210822 08:54"
        },
        {
          "id": "MDRW2100007636.1.9",
          "form": "헐 입 고급이네",
          "original_form": "헐 입 고급이네",
          "speaker_id": "2",
          "time": "20210822 08:55"
        },
        {
          "id": "MDRW2100007636.1.10",
          "form": "난 날것들 좋아함",
          "original_form": "난 날것들 좋아함",
          "speaker_id": "2",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007636.1.11",
          "form": "어떤거",
          "original_form": "어떤거",
          "speaker_id": "1",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007636.1.12",
          "form": "초밥이나 회같은거",
          "original_form": "초밥이나 회같은거",
          "speaker_id": "2",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007636.1.13",
          "form": "어젠 오징어회 먹었다 팔딱팔딱",
          "original_form": "어젠 오징어회 먹었다 팔딱팔딱",
          "speaker_id": "2",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007636.1.14",
          "form": "뛰었군",
          "original_form": "뛰었군",
          "speaker_id": "1",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007636.1.15",
          "form": "맞음",
          "original_form": "맞음",
          "speaker_id": "2",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007636.1.16",
          "form": "수비드면 소고기 먹냐?",
          "original_form": "수비드면 소고기 먹냐?",
          "speaker_id": "2",
