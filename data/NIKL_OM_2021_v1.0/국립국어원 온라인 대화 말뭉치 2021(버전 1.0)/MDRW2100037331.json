      "utterance": [
        {
          "id": "MDRW2100037331.1.1",
          "form": "세상에 벌레는 너무 싫어",
          "original_form": "세상에 벌레는 너무 싫어",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037331.1.2",
          "form": "친구가 산책나갔다가 모기뮬렸는데 손이 퉁퉁 부었더라",
          "original_form": "친구가 산책나갔다가 모기뮬렸는데 손이 퉁퉁 부었더라",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037331.1.3",
          "form": "너 집에서도 모기 물려??",
          "original_form": "너 집에서도 모기 물려??",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037331.1.4",
          "form": "산모기수준이야",
          "original_form": "산모기수준이야",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037331.1.5",
          "form": "난 아니. 친구얘기야",
          "original_form": "난 아니. 친구얘기야",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037331.1.6",
          "form": "근데 요즘 귀뚜라미같은것들도ㅠ많아",
          "original_form": "근데 요즘 귀뚜라미같은것들도ㅠ많아",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037331.1.7",
          "form": "아아 산모기 진짜 무섭지",
          "original_form": "아아 산모기 진짜 무섭지",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037331.1.8",
          "form": "길을 가면 걔네들이 통통튀어서 무서워",
          "original_form": "길을 가면 걔네들이 통통튀어서 무서워",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037331.1.9",
          "form": "귀뚜라미.라니 ㅠㅠ 끔찍해",
          "original_form": "귀뚜라미.라니 ㅠㅠ 끔찍해",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037331.1.10",
          "form": "그래서 난 한동안 청소를 삭 하고 모기장텐트를 쳐뒀어",
          "original_form": "그래서 난 한동안 청소를 삭 하고 모기장텐트를 쳐뒀어",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037331.1.11",
          "form": "그러니까 청소한 기뷴이 하나도 안나더라",
          "original_form": "그러니까 청소한 기뷴이 하나도 안나더라",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037331.1.12",
          "form": "너는 모기 물리는걸 많이 싫어하는구나",
          "original_form": "너는 모기 물리는걸 많이 싫어하는구나",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037331.1.13",
          "form": "방이 훨 좁아보여",
          "original_form": "방이 훨 좁아보여",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037331.1.14",
          "form": "걔네가 귀에서 지저귀는게 싫어",
          "original_form": "걔네가 귀에서 지저귀는게 싫어",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037331.1.15",
          "form": "맞아 그런게있으면 미관상은 별로지",
          "original_form": "맞아 그런게있으면 미관상은 별로지",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037331.1.16",
          "form": "넌 좋아해?",
          "original_form": "넌 좋아해?",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037331.1.17",
          "form": "그것도 집안일일까",
          "original_form": "그것도 집안일일까",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037331.1.18",
          "form": "어제 모기장을 거두고 청소를 했어",
          "original_form": "어제 모기장을 거두고 청소를 했어",
          "speaker_id": "1",
          "time": "20210831 22:36"
        }
      ]
    }
