      "utterance": [
        {
          "id": "MDRW2100039984.1.1",
          "form": "집이 거제라고 햇는데",
          "original_form": "집이 거제라고 햇는데",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039984.1.2",
          "form": "은행이 광주은행이더라구요 ㅎ",
          "original_form": "은행이 광주은행이더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039984.1.3",
          "form": "헐 무섭네요",
          "original_form": "헐 무섭네요",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039984.1.4",
          "form": "먼가 경상도가.....광주은행???",
          "original_form": "먼가 경상도가.....광주은행???",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039984.1.5",
          "form": "그래도 눈썰미가 있으셨나봐요",
          "original_form": "그래도 눈썰미가 있으셨나봐요",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039984.1.6",
          "form": "여기서부터 조금 이런 의심이 들었는데",
          "original_form": "여기서부터 조금 이런 의심이 들었는데",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039984.1.7",
          "form": "안심결제하는건데 수수료는 자동 차감되야 하는거 아닌가요?",
          "original_form": "안심결제하는건데 수수료는 자동 차감되야 하는거 아닌가요?",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100039984.1.8",
          "form": "근데 수수료를 내가 계산해서 보내라는거예요",
          "original_form": "근데 수수료를 내가 계산해서 보내라는거예요",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100039984.1.9",
          "form": "여기서부터 의심이 들기 시작해서",
          "original_form": "여기서부터 의심이 들기 시작해서",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039984.1.10",
          "form": "수상한건 말이 많아요",
          "original_form": "수상한건 말이 많아요",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039984.1.11",
          "form": "책 사진 좀 찍어서 달라니까",
          "original_form": "책 사진 좀 찍어서 달라니까",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039984.1.12",
          "form": "게시판에 있는 사진들을 보내더라구요",
          "original_form": "게시판에 있는 사진들을 보내더라구요",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039984.1.13",
          "form": "와 심하네요",
          "original_form": "와 심하네요",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039984.1.14",
          "form": "그리고 때마침 제가 이걸로 머리굴리는동안",
          "original_form": "그리고 때마침 제가 이걸로 머리굴리는동안",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039984.1.15",
          "form": "아이들이 목욕탕에 넣어놨는데 욕실은 난장판을 만들고 있더라구요 ㅎ",
          "original_form": "아이들이 목욕탕에 넣어놨는데 욕실은 난장판을 만들고 있더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039984.1.16",
          "form": "힘드셨겠어요ㅋㅋㅋ",
          "original_form": "힘드셨겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039984.1.17",
          "form": "그래서 그냥 에잇~ 그쪽한테는 안하겠다고하고",
          "original_form": "그래서 그냥 에잇~ 그쪽한테는 안하겠다고하고",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039984.1.18",
          "form": "욕실 수습했죠 ㅋㅋㅋ",
          "original_form": "욕실 수습했죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039984.1.19",
          "form": "당하려면 다알아도 코베이잖아요",
          "original_form": "당하려면 다알아도 코베이잖아요",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039984.1.20",
          "form": "타이밍이 좋으셨네요",
          "original_form": "타이밍이 좋으셨네요",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039984.1.21",
          "form": "그러고나서 중나가보니 그 사기꾼이 올렸던 글이 삭제되어 있더라구요",
          "original_form": "그러고나서 중나가보니 그 사기꾼이 올렸던 글이 삭제되어 있더라구요",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039984.1.22",
          "form": "그러고는 담날 또 올라오고 같은 사진들이 ㅎ",
          "original_form": "그러고는 담날 또 올라오고 같은 사진들이 ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039984.1.23",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039984.1.24",
          "form": "댓글 달고 싶었어요 ㅎㅎ 사기꾼이라고...",
          "original_form": "댓글 달고 싶었어요 ㅎㅎ 사기꾼이라고...",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039984.1.25",
          "form": "진짜 저라도 열받을거같아여",
          "original_form": "진짜 저라도 열받을거같아여",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039984.1.26",
          "form": "근데 그랬다간 그사람은 저희집주소 전화번호 넘겨줬던 상태라...",
          "original_form": "근데 그랬다간 그사람은 저희집주소 전화번호 넘겨줬던 상태라...",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039984.1.27",
          "form": "아아 그럼좀 찜찜하져",
          "original_form": "아아 그럼좀 찜찜하져",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039984.1.28",
          "form": "신랑이 하지마라고 무슨 헷꼬지 할지 모른다고 참으라고 해서 ㅎㅎ",
          "original_form": "신랑이 하지마라고 무슨 헷꼬지 할지 모른다고 참으라고 해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:59"
        }
      ]
    }
