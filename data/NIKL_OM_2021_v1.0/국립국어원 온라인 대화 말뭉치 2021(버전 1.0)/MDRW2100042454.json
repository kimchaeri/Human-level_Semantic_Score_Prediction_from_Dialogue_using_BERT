      "utterance": [
        {
          "id": "MDRW2100042454.1.1",
          "form": "교촌 허콤이랑 노랑통닭의 알마치여",
          "original_form": "교촌 허콤이랑 노랑통닭의 알마치여",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042454.1.2",
          "form": "name2님은 어디브랜드 자주드세요?",
          "original_form": "&name2&님은 어디브랜드 자주드세요?",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042454.1.3",
          "form": "저도 노랑통닭이요.",
          "original_form": "저도 노랑통닭이요.",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042454.1.4",
          "form": "알마치도 자주 먹는데 깐풍치킨이 취향이에요",
          "original_form": "알마치도 자주 먹는데 깐풍치킨이 취향이에요",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042454.1.5",
          "form": "오 전 알마치도 약간 깐풍스타일같았는데",
          "original_form": "오 전 알마치도 약간 깐풍스타일같았는데",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042454.1.6",
          "form": "어때요? 좀다른가여?",
          "original_form": "어때요? 좀다른가여?",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042454.1.7",
          "form": "깐풍기 소스라 마늘소스랑은 맛이 다르긴 해요.",
          "original_form": "깐풍기 소스라 마늘소스랑은 맛이 다르긴 해요.",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042454.1.8",
          "form": "더 달콤한 거 같아요",
          "original_form": "더 달콤한 거 같아요",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042454.1.9",
          "form": "오오 마늘소는 그 다진마늘엄청들어가서 쏘는맛이있긴하던데",
          "original_form": "오오 마늘소는 그 다진마늘엄청들어가서 쏘는맛이있긴하던데",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042454.1.10",
          "form": "다음번에 깐풍먹어봐야겠네요",
          "original_form": "다음번에 깐풍먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042454.1.11",
          "form": "노랑통닭은 튀김옷도 맛있어서 좋더라구요",
          "original_form": "노랑통닭은 튀김옷도 맛있어서 좋더라구요",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042454.1.12",
          "form": "맞아요 바삭하고 ㅎㅎ간이 잘되어있는듯여",
          "original_form": "맞아요 바삭하고 ㅎㅎ간이 잘되어있는듯여",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042454.1.13",
          "form": "얘기하니까 또 먹고싶네요ㅋㅋ",
          "original_form": "얘기하니까 또 먹고싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:37"
        },
        {
          "id": "MDRW2100042454.1.14",
          "form": "ㅋㅋㅋㅋㅋ저두여 ㅋㅋㅋㅋ며칠전에 고바삭먹었는데",
          "original_form": "ㅋㅋㅋㅋㅋ저두여 ㅋㅋㅋㅋ며칠전에 고바삭먹었는데",
          "speaker_id": "1",
          "time": "20210906 23:37"
        },
        {
          "id": "MDRW2100042454.1.15",
          "form": "깐풍기 땡기네여",
          "original_form": "깐풍기 땡기네여",
          "speaker_id": "1",
          "time": "20210906 23:38"
        },
        {
          "id": "MDRW2100042454.1.16",
          "form": "깐풍기ㅠㅠ 예전에 친구들이랑 술마시면서 꼭 먹었어요ㅋㅋ",
          "original_form": "깐풍기ㅠㅠ 예전에 친구들이랑 술마시면서 꼭 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:44"
        },
        {
          "id": "MDRW2100042454.1.17",
          "form": "크 술주종은 어떤걸로 드셨었나요",
          "original_form": "크 술주종은 어떤걸로 드셨었나요",
          "speaker_id": "1",
          "time": "20210907 00:06"
        },
        {
          "id": "MDRW2100042454.1.18",
          "form": "맥주요! 과일소주도 많이 마셨던거 같네요ㅋㅋ",
          "original_form": "맥주요! 과일소주도 많이 마셨던거 같네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:23"
        },
        {
          "id": "MDRW2100042454.1.19",
          "form": "전 알쓰라 ㅋㅋㅋ술잘먹고싶어요 암만먹어도 쓴맛나는..",
          "original_form": "전 알쓰라 ㅋㅋㅋ술잘먹고싶어요 암만먹어도 쓴맛나는..",
          "speaker_id": "1",
          "time": "20210907 15:17"
        }
      ]
