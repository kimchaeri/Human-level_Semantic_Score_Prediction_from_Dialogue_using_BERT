      "utterance": [
        {
          "id": "MDRW2100038220.1.1",
          "form": "그 요거트맛인가 그것도 맛있던데",
          "original_form": "그 요거트맛인가 그것도 맛있던데",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100038220.1.2",
          "form": "인절미였나",
          "original_form": "인절미였나",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100038220.1.3",
          "form": "그 아몬드 인절미맛에서 요거트맛 났던거 같아요",
          "original_form": "그 아몬드 인절미맛에서 요거트맛 났던거 같아요",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100038220.1.4",
          "form": "오 그런맛도 있어요?",
          "original_form": "오 그런맛도 있어요?",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100038220.1.5",
          "form": "맛 종류 진짜 많네요 ㅎ",
          "original_form": "맛 종류 진짜 많네요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038220.1.6",
          "form": "인절미였던가 제 기억이 왔다갔다라 ㅋㅋㅋㅋㅋ",
          "original_form": "인절미였던가 제 기억이 왔다갔다라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038220.1.7",
          "form": "하여튼 웬만하면 다 맛있더라고요",
          "original_form": "하여튼 웬만하면 다 맛있더라고요",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038220.1.8",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038220.1.9",
          "form": "진짜 잘 만들었네요 ㅎ",
          "original_form": "진짜 잘 만들었네요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038220.1.10",
          "form": "녹차맛도 약간 요거트맛 났던거 같아요",
          "original_form": "녹차맛도 약간 요거트맛 났던거 같아요",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038220.1.11",
          "form": "이상한 맛도 있긴 했어요 망고맛이었나",
          "original_form": "이상한 맛도 있긴 했어요 망고맛이었나",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038220.1.12",
          "form": "앗 그건 아닐것같아오",
          "original_form": "앗 그건 아닐것같아오",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038220.1.13",
          "form": "그거 최악이었어욬ㅋㅋㅋ 사은품으로 왔는데 악...",
          "original_form": "그거 최악이었어욬ㅋㅋㅋ 사은품으로 왔는데 악...",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038220.1.14",
          "form": "사은품으로도 보내지마 싶은 맛ㅋㅋㅋㅋ",
          "original_form": "사은품으로도 보내지마 싶은 맛ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038220.1.15",
          "form": "프레첼인가 같이 들어있는건",
          "original_form": "프레첼인가 같이 들어있는건",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038220.1.16",
          "form": "그 이후로 안사먹었어욬ㅋㅋㅋㅋ",
          "original_form": "그 이후로 안사먹었어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038220.1.17",
          "form": "맛나던데",
          "original_form": "맛나던데",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038220.1.18",
          "form": "아 그거 맛있었어요",
          "original_form": "아 그거 맛있었어요",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038220.1.19",
          "form": "안팔려서 사은품으로 소진 ㅋ",
          "original_form": "안팔려서 사은품으로 소진 ㅋ",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038220.1.20",
          "form": "그랬던거 같아욬ㅋㅋㅋ",
          "original_form": "그랬던거 같아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038220.1.21",
          "form": "군옥수수맛도 맛있었어요",
          "original_form": "군옥수수맛도 맛있었어요",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038220.1.22",
          "form": "옥수수 같이 들어있는거요",
          "original_form": "옥수수 같이 들어있는거요",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038220.1.23",
          "form": "네 그거 맛있더라고요",
          "original_form": "네 그거 맛있더라고요",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038220.1.24",
          "form": "먹어봤어요",
          "original_form": "먹어봤어요",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038220.1.25",
          "form": "근데 씹을수록 턱 깨지는",
          "original_form": "근데 씹을수록 턱 깨지는",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038220.1.26",
          "form": "고소하더라구요",
          "original_form": "고소하더라구요",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038220.1.27",
          "form": "하리보 먹는 기분이었어요",
          "original_form": "하리보 먹는 기분이었어요",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038220.1.28",
          "form": "턱 깨지는 ㅋㅋㅋ",
          "original_form": "턱 깨지는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038220.1.29",
          "form": "넹 ㅋㅋㅋㅋㅋ",
          "original_form": "넹 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038220.1.30",
          "form": "넘 딱딱해서 ㅎㅎ",
          "original_form": "넘 딱딱해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038220.1.31",
          "form": "맛있긴 하더라고욬ㅋㅋ",
          "original_form": "맛있긴 하더라고욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:07"
        }
      ]
