      "utterance": [
        {
          "id": "MDRW2100020638.1.1",
          "form": "저 김재환 좋아해요!",
          "original_form": "저 김재환 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020638.1.2",
          "form": "혹시 누군지 아세요 ?",
          "original_form": "혹시 누군지 아세요 ?",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020638.1.3",
          "form": "프듀나오신분 맞죵?",
          "original_form": "프듀나오신분 맞죵?",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020638.1.4",
          "form": "네 맞아요 !!",
          "original_form": "네 맞아요 !!",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020638.1.5",
          "form": "알아봐주시다니 감격입니다",
          "original_form": "알아봐주시다니 감격입니다",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020638.1.6",
          "form": "네버 메보였던 !ㅋㅋ",
          "original_form": "네버 메보였던 !ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020638.1.7",
          "form": "덕님음요 ??",
          "original_form": "덕님음요 ??",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020638.1.8",
          "form": "네네 맞아요 !!!",
          "original_form": "네네 맞아요 !!!",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020638.1.9",
          "form": "저는 데이식스 좋아해용",
          "original_form": "저는 데이식스 좋아해용",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020638.1.10",
          "form": "데이식스",
          "original_form": "데이식스",
          "speaker_id": "2",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020638.1.11",
          "form": "저 노래 좋아해요",
          "original_form": "저 노래 좋아해요",
          "speaker_id": "2",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020638.1.12",
          "form": "프듀때 진짜 엄청 났었는데 피디가...ㅠㅠㅠ",
          "original_form": "프듀때 진짜 엄청 났었는데 피디가...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020638.1.13",
          "form": "흐흐 감사해뇽",
          "original_form": "흐흐 감사해뇽",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020638.1.14",
          "form": "맞어요 진짜  .....",
          "original_form": "맞어요 진짜  .....",
          "speaker_id": "2",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020638.1.15",
          "form": "노래 진짜 너무 잘해요",
          "original_form": "노래 진짜 너무 잘해요",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020638.1.16",
          "form": "그냥 타고난것같아요",
          "original_form": "그냥 타고난것같아요",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020638.1.17",
          "form": "너무 안타까워요 다들",
          "original_form": "너무 안타까워요 다들",
          "speaker_id": "2",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020638.1.18",
          "form": "김재환 노래부르는거 보면",
          "original_form": "김재환 노래부르는거 보면",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020638.1.19",
          "form": "그런 생각이 들어용",
          "original_form": "그런 생각이 들어용",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020638.1.20",
          "form": "흐헉 ㅠ ㅠ ㅠ ㅠ 제가 다 감사한걸요 ......",
          "original_form": "흐헉 ㅠ ㅠ ㅠ ㅠ 제가 다 감사한걸요 ......",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020638.1.21",
          "form": "아니에옄ㅋㅋㅋ",
          "original_form": "아니에옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020638.1.22",
          "form": "데이식스도 노래 진짜 잘하던데요 !!",
          "original_form": "데이식스도 노래 진짜 잘하던데요 !!",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020638.1.23",
          "form": "밴든데 뭔가",
          "original_form": "밴든데 뭔가",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020638.1.24",
          "form": "예전의 밴드들과는 다른 느낌이랄까요",
          "original_form": "예전의 밴드들과는 다른 느낌이랄까요",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020638.1.25",
          "form": "보컬이 진짜 시원한거 같아요",
          "original_form": "보컬이 진짜 시원한거 같아요",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020638.1.26",
          "form": "ㅎㅎ 제일 좋아하는 멤버가 있긴한데",
          "original_form": "ㅎㅎ 제일 좋아하는 멤버가 있긴한데",
          "speaker_id": "1",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020638.1.27",
          "form": "자꾸 병크를 터트려서",
          "original_form": "자꾸 병크를 터트려서",
          "speaker_id": "1",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020638.1.28",
          "form": "그 한페이지가 될 수 있게 ? 맞나요 그거 되게 좋아했어요",
          "original_form": "그 한페이지가 될 수 있게 ? 맞나요 그거 되게 좋아했어요",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020638.1.29",
          "form": "아 이런 ㅠ ㅠ",
          "original_form": "아 이런 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020638.1.30",
          "form": "헐 맞아여ㅋㅋㅋ 감사합니당?ㅋㅋ",
          "original_form": "헐 맞아여ㅋㅋㅋ 감사합니당?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020638.1.31",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020638.1.32",
          "form": "발 반쯤 담그고 있어요ㅠㅠ",
