      "utterance": [
        {
          "id": "MDRW2100016331.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016331.1.2",
          "form": "최근에 사신 물건있으신가요",
          "original_form": "최근에 사신 물건있으신가요",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016331.1.3",
          "form": "브리타정수기요",
          "original_form": "브리타정수기요",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016331.1.4",
          "form": "브리타정수기관심있는데.. 좋나요?ㅎ",
          "original_form": "브리타정수기관심있는데.. 좋나요?ㅎ",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016331.1.5",
          "form": "1년째 쓰는데",
          "original_form": "1년째 쓰는데",
          "speaker_id": "1",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016331.1.6",
          "form": "만족해요",
          "original_form": "만족해요",
          "speaker_id": "1",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016331.1.7",
          "form": "필터따로계속 사야되서 고민했거든요",
          "original_form": "필터따로계속 사야되서 고민했거든요",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016331.1.8",
          "form": "생각보다 자주 안갈아요",
          "original_form": "생각보다 자주 안갈아요",
          "speaker_id": "1",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016331.1.9",
          "form": "조금더 비교해보고 사야겠어요ㅋ",
          "original_form": "조금더 비교해보고 사야겠어요ㅋ",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016331.1.10",
          "form": "중고거래 해보셨나요",
          "original_form": "중고거래 해보셨나요",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100016331.1.11",
          "form": "당근은 2번해봤어요ㅎ",
          "original_form": "당근은 2번해봤어요ㅎ",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016331.1.12",
          "form": "자주하세요?",
          "original_form": "자주하세요?",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016331.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016331.1.14",
          "form": "중고거래하면 한번씩 예의안지키는 사람도 있더라구요",
          "original_form": "중고거래하면 한번씩 예의안지키는 사람도 있더라구요",
          "speaker_id": "2",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100016331.1.15",
          "form": "맞아요 진상 많아요ㅋㅋㅋ",
