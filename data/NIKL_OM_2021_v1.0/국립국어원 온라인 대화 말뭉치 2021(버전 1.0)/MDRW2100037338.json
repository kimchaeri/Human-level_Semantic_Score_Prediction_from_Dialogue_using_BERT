      "utterance": [
        {
          "id": "MDRW2100037338.1.1",
          "form": "너 유튜브 써?",
          "original_form": "너 유튜브 써?",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.2",
          "form": "아 쓰는구나",
          "original_form": "아 쓰는구나",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.3",
          "form": "응응 쓰는데 요즘 볼게없어",
          "original_form": "응응 쓰는데 요즘 볼게없어",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.4",
          "form": "유튜브로 영상을 보거나 노래를 들을 수 있우니까 난 자주 써",
          "original_form": "유튜브로 영상을 보거나 노래를 들을 수 있우니까 난 자주 써",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.5",
          "form": "추천부탁해!!",
          "original_form": "추천부탁해!!",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.6",
          "form": "햄튜브 알아?",
          "original_form": "햄튜브 알아?",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.7",
          "form": "햄튜브 친구 해쭈라고 있는데 쿼카 닮은 유튜버야",
          "original_form": "햄튜브 친구 해쭈라고 있는데 쿼카 닮은 유튜버야",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.8",
          "form": "세상에 그럼 너무 귀엽겠다",
          "original_form": "세상에 그럼 너무 귀엽겠다",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.9",
          "form": "다이어트를 한다던데 잘됐을까?",
          "original_form": "다이어트를 한다던데 잘됐을까?",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.10",
          "form": "보통 어떤 컨텐츠를해?",
          "original_form": "보통 어떤 컨텐츠를해?",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.11",
          "form": "어제 영상이 올라왔는데 잠와서 못봤어",
          "original_form": "어제 영상이 올라왔는데 잠와서 못봤어",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.12",
          "form": "나는 해쭈가 먹는게 제일 좋아",
          "original_form": "나는 해쭈가 먹는게 제일 좋아",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.13",
          "form": "다이어트 유투버야??",
          "original_form": "다이어트 유투버야??",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.14",
          "form": "해쭈는 참 맛있게 먹어",
          "original_form": "해쭈는 참 맛있게 먹어",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.15",
          "form": "아아 먹는 유투버구나",
          "original_form": "아아 먹는 유투버구나",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037338.1.16",
          "form": "아니 그냥 별걸다해",
          "original_form": "아니 그냥 별걸다해",
          "speaker_id": "2",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037338.1.17",
          "form": "상황극도 해",
          "original_form": "상황극도 해",
          "speaker_id": "2",
          "time": "20210831 22:41"
        }
