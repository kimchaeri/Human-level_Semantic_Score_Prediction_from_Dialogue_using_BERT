      "utterance": [
        {
          "id": "MDRW2100019250.1.1",
          "form": "자취하는중?",
          "original_form": "자취하는중?",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019250.1.2",
          "form": "자취??",
          "original_form": "자취??",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019250.1.3",
          "form": "갑자기?",
          "original_form": "갑자기?",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019250.1.4",
          "form": "가족은 몇명이서 살아?",
          "original_form": "가족은 몇명이서 살아?",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019250.1.5",
          "form": "세.세명이요 ㅋㅋ",
          "original_form": "세.세명이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019250.1.6",
          "form": "가사는 어캐 분담했어?",
          "original_form": "가사는 어캐 분담했어?",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019250.1.7",
          "form": "난 혼자 살아ㅜㅜ",
          "original_form": "난 혼자 살아ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019250.1.8",
          "form": "가족들과 사는거 좋은거같아",
          "original_form": "가족들과 사는거 좋은거같아",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019250.1.9",
          "form": "가족가 사는게 좋지 혼자는 외로워",
          "original_form": "가족가 사는게 좋지 혼자는 외로워",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019250.1.10",
          "form": "외로운거보단 과일이나 식품 살 때 소량구매 불편해",
          "original_form": "외로운거보단 과일이나 식품 살 때 소량구매 불편해",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019250.1.11",
          "form": "오 그렇지 혼자사면 쇼핑하기도 힘들지",
          "original_form": "오 그렇지 혼자사면 쇼핑하기도 힘들지",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019250.1.12",
          "form": "맞어 특히 과일이 어려워ㅜㅜ",
          "original_form": "맞어 특히 과일이 어려워ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100019250.1.13",
          "form": "그렇겠다 과일.. 보통 파는 단위가 크니끼ㅣ",
          "original_form": "그렇겠다 과일.. 보통 파는 단위가 크니끼ㅣ",
          "speaker_id": "2",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100019250.1.14",
          "form": "크니까",
          "original_form": "크니까",
          "speaker_id": "2",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100019250.1.15",
          "form": "응 글고ㅋ집안일 혼자 해야함ㅋㅋ",
          "original_form": "응 글고ㅋ집안일 혼자 해야함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100019250.1.16",
          "form": "아 맞네 ㅋㅋ 가족끼리 있음 서로 나눠서 하긴하지",
          "original_form": "아 맞네 ㅋㅋ 가족끼리 있음 서로 나눠서 하긴하지",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019250.1.17",
          "form": "맞져ㅎㅎ그게 달라",
          "original_form": "맞져ㅎㅎ그게 달라",
          "speaker_id": "1",
          "time": "20210827 17:58"
        },
        {
          "id": "MDRW2100019250.1.18",
          "form": "하기싫을땐 미룰수 있어 좋긴한데",
          "original_form": "하기싫을땐 미룰수 있어 좋긴한데",
          "speaker_id": "1",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100019250.1.19",
          "form": "ㅇㅇ 그렇지 ㅎㅎ",
          "original_form": "ㅇㅇ 그렇지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100019250.1.20",
          "form": "그리고 퇴근하면 누가 있거나\r\n나 있다가 누가 오고하는 사람온기가 있어\r\n가족들과 사는거 좋은거 같어",
          "original_form": "그리고 퇴근하면 누가 있거나\r\n나 있다가 누가 오고하는 사람온기가 있어\r\n가족들과 사는거 좋은거 같어",
          "speaker_id": "1",
          "time": "20210827 18:04"
        },
        {
          "id": "MDRW2100019250.1.21",
          "form": "맞아 집에 갔을때 누가 있으면 좋지 ㅎㅎ",
          "original_form": "맞아 집에 갔을때 누가 있으면 좋지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:12"
        }
      ]
