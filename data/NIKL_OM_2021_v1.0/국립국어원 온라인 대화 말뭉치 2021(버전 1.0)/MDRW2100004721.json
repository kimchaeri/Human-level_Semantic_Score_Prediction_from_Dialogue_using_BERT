      "utterance": [
        {
          "id": "MDRW2100004721.1.1",
          "form": "지금 학교 다니고 있으신가요?",
          "original_form": "지금 학교 다니고 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004721.1.2",
          "form": "직장인이에요",
          "original_form": "직장인이에요",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004721.1.3",
          "form": "졸업한지 5년이 넘어가네요ㅠ",
          "original_form": "졸업한지 5년이 넘어가네요ㅠ",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004721.1.4",
          "form": "name1님은 학생이신가요?",
          "original_form": "&name1&님은 학생이신가요?",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004721.1.5",
          "form": "넵 지금 대학교 재학중이에요 ㅎㅎ",
          "original_form": "넵 지금 대학교 재학중이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004721.1.6",
          "form": "name2님 무슨 과 나오셨는지 물어봐도 될까요?",
          "original_form": "&name2&님 무슨 과 나오셨는지 물어봐도 될까요?",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004721.1.7",
          "form": "저는 지질학과 나왔어요ㅎㅎ",
          "original_form": "저는 지질학과 나왔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004721.1.8",
          "form": "name1님은 어디과세요?",
          "original_form": "&name1&님은 어디과세요?",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004721.1.9",
          "form": "저는 회계세무학과에요 ㅎㅎ",
          "original_form": "저는 회계세무학과에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004721.1.10",
          "form": "그럼 전공살려서 취업 하셨나요?",
          "original_form": "그럼 전공살려서 취업 하셨나요?",
          "speaker_id": "1",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004721.1.11",
          "form": "슬프게도 아니요ㅠㅠ",
          "original_form": "슬프게도 아니요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:59"
        },
        {
          "id": "MDRW2100004721.1.12",
          "form": "따지고보면 지금 하고있는 일은 환경공학과랑 맞는 일이에요",
          "original_form": "따지고보면 지금 하고있는 일은 환경공학과랑 맞는 일이에요",
          "speaker_id": "2",
          "time": "20210821 18:59"
        },
        {
          "id": "MDRW2100004721.1.13",
          "form": "그래도 전공수업은 재밋게 들었었어요ㅎㅎ",
          "original_form": "그래도 전공수업은 재밋게 들었었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004721.1.14",
          "form": "name1님은 전공이 적성에 맞으신가요?",
          "original_form": "&name1&님은 전공이 적성에 맞으신가요?",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004721.1.15",
          "form": "아뇨ㅜㅜ 지금 너무 안맞아서 수능 다시볼까 고민중이에요..",
          "original_form": "아뇨ㅜㅜ 지금 너무 안맞아서 수능 다시볼까 고민중이에요..",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004721.1.16",
          "form": "배우면 배울수록 저랑 안맞는게 느껴져요 ㅋㅋ",
          "original_form": "배우면 배울수록 저랑 안맞는게 느껴져요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004721.1.17",
          "form": "헉.. ㅠㅠ 그정도세요ㅜㅜ",
          "original_form": "헉.. ㅠㅠ 그정도세요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004721.1.18",
          "form": "그래도 세무회계면 갈 곳 많으실텐데ㅠ",
          "original_form": "그래도 세무회계면 갈 곳 많으실텐데ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004721.1.19",
          "form": "다른 과 간다면 어느 과로 가시게요?",
          "original_form": "다른 과 간다면 어느 과로 가시게요?",
          "speaker_id": "2",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004721.1.20",
          "form": "간호학과 생각 중 이에요 ㅎㅎ 사실 고등학생때 꿈이 간호사였는데 수능때 실수해서 못 갔어요ㅜ",
          "original_form": "간호학과 생각 중 이에요 ㅎㅎ 사실 고등학생때 꿈이 간호사였는데 수능때 실수해서 못 갔어요ㅜ",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004721.1.21",
          "form": "아아ㅠㅠ",
          "original_form": "아아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004721.1.22",
          "form": "너무 안타깝네요ㅠ",
          "original_form": "너무 안타깝네요ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004721.1.23",
          "form": "그럼 진짜 후회 많이 될 것 같아요",
          "original_form": "그럼 진짜 후회 많이 될 것 같아요",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004721.1.24",
          "form": "재수능말고 전과나 편입으론 생각안해보셨나요?",
          "original_form": "재수능말고 전과나 편입으론 생각안해보셨나요?",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004721.1.25",
          "form": "그것도 생각해봤는데 제가 1학년이라 편입하려면 1년을 더 다녀야 되서요ㅜㅜ 전과는 간호학과라 불가능해요..",
          "original_form": "그것도 생각해봤는데 제가 1학년이라 편입하려면 1년을 더 다녀야 되서요ㅜㅜ 전과는 간호학과라 불가능해요..",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004721.1.26",
          "form": "아..  그런게 있구나ㅠ",
          "original_form": "아..  그런게 있구나ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004721.1.27",
          "form": "아니 세상에 이제 1학년이면 아직 제대로 대학생활 못 즐기셨겠네요",
          "original_form": "아니 세상에 이제 1학년이면 아직 제대로 대학생활 못 즐기셨겠네요",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004721.1.28",
          "form": "코로나도 있고해서ㅠ",
          "original_form": "코로나도 있고해서ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004721.1.29",
          "form": "너무 안타깝네요ㅠ",
          "original_form": "너무 안타깝네요ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004721.1.30",
          "form": "넵ㅜㅜ 거의 비대면수업이라 아직 동기들하고도 서먹해요ㅎㅎ",
          "original_form": "넵ㅜㅜ 거의 비대면수업이라 아직 동기들하고도 서먹해요ㅎㅎ",
          "speaker_id": "1",
