        {
          "id": "MDRW2100027591.1.1",
          "form": "일반인 브이로그 보세요?",
          "original_form": "일반인 브이로그 보세요?",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027591.1.2",
          "form": "네네 봐요!!",
          "original_form": "네네 봐요!!",
          "speaker_id": "2",
          "time": "20210904 01:12"
        },
        {
          "id": "MDRW2100027591.1.3",
          "form": "name1은요??",
          "original_form": "&name1&은요??",
          "speaker_id": "2",
          "time": "20210904 01:12"
        },
        {
          "id": "MDRW2100027591.1.4",
          "form": "저는 브이로그는 거의안봐용 ㅠ",
          "original_form": "저는 브이로그는 거의안봐용 ㅠ",
          "speaker_id": "1",
          "time": "20210905 12:01"
        },
        {
          "id": "MDRW2100027591.1.5",
          "form": "아하 그럼 주로 뭐 보세요?",
          "original_form": "아하 그럼 주로 뭐 보세요?",
          "speaker_id": "2",
          "time": "20210907 17:09"
        },
        {
          "id": "MDRW2100027591.1.6",
          "form": "흠 편집예능이나 춤노래 이런거용",
          "original_form": "흠 편집예능이나 춤노래 이런거용",
          "speaker_id": "1",
          "time": "20210907 17:10"
        },
        {
          "id": "MDRW2100027591.1.7",
          "form": "아하 저도 티비로 예능은 잘 안보는데",
          "original_form": "아하 저도 티비로 예능은 잘 안보는데",
          "speaker_id": "2",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100027591.1.8",
          "form": "유튜브로 짧은건 좀 봐요",
          "original_form": "유튜브로 짧은건 좀 봐요",
          "speaker_id": "2",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100027591.1.9",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100027591.1.10",
          "form": "그쵸 ㅋㅋ 잼나요",
          "original_form": "그쵸 ㅋㅋ 잼나요",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100027591.1.11",
          "form": "다보긴 재미없는부분도 많은데",
          "original_form": "다보긴 재미없는부분도 많은데",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100027591.1.12",
          "form": "하이라이트만 잇어서 좋아요 ㅋㅋㅋ",
          "original_form": "하이라이트만 잇어서 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100027591.1.13",
          "form": "먹방은 안보세용?",
          "original_form": "먹방은 안보세용?",
          "speaker_id": "1",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100027591.1.14",
          "form": "오 많이 봐요!",
          "original_form": "오 많이 봐요!",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100027591.1.15",
          "form": "주로 많이먹는거 아님 맛있는거먹는거",
          "original_form": "주로 많이먹는거 아님 맛있는거먹는거",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100027591.1.16",
          "form": "어떤거 보세요",
          "original_form": "어떤거 보세요",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100027591.1.17",
          "form": "음... 다보긴 하는데 주로 많이 먹는거요!",
          "original_form": "음... 다보긴 하는데 주로 많이 먹는거요!",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100027591.1.18",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100027591.1.19",
