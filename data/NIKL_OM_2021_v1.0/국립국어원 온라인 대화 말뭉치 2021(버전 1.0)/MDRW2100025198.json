      "utterance": [
        {
          "id": "MDRW2100025198.1.1",
          "form": "요즘 관심가는 운동 있니?",
          "original_form": "요즘 관심가는 운동 있니?",
          "speaker_id": "1",
          "time": "20210901 00:42"
        },
        {
          "id": "MDRW2100025198.1.2",
          "form": "난 무게치는거 좋아해",
          "original_form": "난 무게치는거 좋아해",
          "speaker_id": "2",
          "time": "20210901 00:42"
        },
        {
          "id": "MDRW2100025198.1.3",
          "form": "삼대오백이 꿈이야",
          "original_form": "삼대오백이 꿈이야",
          "speaker_id": "2",
          "time": "20210901 00:42"
        },
        {
          "id": "MDRW2100025198.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210901 00:42"
        },
        {
          "id": "MDRW2100025198.1.5",
          "form": "헬스장 꿈나무구나",
          "original_form": "헬스장 꿈나무구나",
          "speaker_id": "1",
          "time": "20210901 00:42"
        },
        {
          "id": "MDRW2100025198.1.6",
          "form": "나는 필라테스 하고 있는데 실내 클라이밍 관심 생기더라",
          "original_form": "나는 필라테스 하고 있는데 실내 클라이밍 관심 생기더라",
          "speaker_id": "1",
          "time": "20210901 00:43"
        },
        {
          "id": "MDRW2100025198.1.7",
          "form": "그거말고 나랑 등산하자",
          "original_form": "그거말고 나랑 등산하자",
          "speaker_id": "2",
          "time": "20210901 00:43"
        },
        {
          "id": "MDRW2100025198.1.8",
          "form": "왜이래",
          "original_form": "왜이래",
          "speaker_id": "1",
          "time": "20210901 00:43"
        },
        {
          "id": "MDRW2100025198.1.9",
          "form": "등산이랑 다른게없잖아!",
          "original_form": "등산이랑 다른게없잖아!",
          "speaker_id": "2",
          "time": "20210901 00:43"
        },
        {
          "id": "MDRW2100025198.1.10",
          "form": "싫어",
          "original_form": "싫어",
          "speaker_id": "1",
          "time": "20210901 00:43"
        },
        {
          "id": "MDRW2100025198.1.11",
          "form": "완전 달라",
          "original_form": "완전 달라",
          "speaker_id": "1",
          "time": "20210901 00:43"
        },
        {
          "id": "MDRW2100025198.1.12",
          "form": "어떤점이?",
          "original_form": "어떤점이?",
          "speaker_id": "2",
          "time": "20210901 00:43"
        },
        {
          "id": "MDRW2100025198.1.13",
          "form": "나는 무릎 건강이 좋지 않아서 등산은 힘들어",
          "original_form": "나는 무릎 건강이 좋지 않아서 등산은 힘들어",
          "speaker_id": "1",
          "time": "20210901 00:43"
        },
        {
          "id": "MDRW2100025198.1.14",
          "form": "등산의 어떤 점이 좋니?",
          "original_form": "등산의 어떤 점이 좋니?",
          "speaker_id": "1",
          "time": "20210901 00:44"
        },
        {
          "id": "MDRW2100025198.1.15",
          "form": "경치구경이지",
          "original_form": "경치구경이지",
          "speaker_id": "2",
          "time": "20210901 00:44"
        },
        {
          "id": "MDRW2100025198.1.16",
          "form": "그리고 산에서 내려와서 먹는",
          "original_form": "그리고 산에서 내려와서 먹는",
          "speaker_id": "2",
          "time": "20210901 00:45"
        },
        {
          "id": "MDRW2100025198.1.17",
          "form": "라면이 그렇게 맛있대",
          "original_form": "라면이 그렇게 맛있대",
          "speaker_id": "2",
          "time": "20210901 00:45"
        },
        {
          "id": "MDRW2100025198.1.18",
          "form": "운동하고 먹으면 다 맛있을듯",
          "original_form": "운동하고 먹으면 다 맛있을듯",
          "speaker_id": "1",
          "time": "20210901 00:45"
        },
        {
          "id": "MDRW2100025198.1.19",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "2",
          "time": "20210901 00:45"
        },
        {
          "id": "MDRW2100025198.1.20",
          "form": "수영은 어때?",
          "original_form": "수영은 어때?",
          "speaker_id": "2",
          "time": "20210901 00:45"
        },
        {
          "id": "MDRW2100025198.1.21",
          "form": "나 수영 못하는데",
          "original_form": "나 수영 못하는데",
          "speaker_id": "1",
          "time": "20210901 00:45"
        },
        {
          "id": "MDRW2100025198.1.22",
          "form": "너 수영할 줄 알아??",
          "original_form": "너 수영할 줄 알아??",
          "speaker_id": "1",
          "time": "20210901 00:45"
        },
        {
          "id": "MDRW2100025198.1.23",
          "form": "나 잠깐 배웠었어",
          "original_form": "나 잠깐 배웠었어",
          "speaker_id": "2",
          "time": "20210901 00:45"
        },
        {
          "id": "MDRW2100025198.1.24",
          "form": "근데 안하니까 다까먹음",
          "original_form": "근데 안하니까 다까먹음",
          "speaker_id": "2",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025198.1.25",
          "form": "같이 배울래?",
          "original_form": "같이 배울래?",
          "speaker_id": "2",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025198.1.26",
          "form": "어... 나 옛날에 한 3번은 빠졌더니 엄두가 안난다",
          "original_form": "어... 나 옛날에 한 3번은 빠졌더니 엄두가 안난다",
          "speaker_id": "1",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025198.1.27",
          "form": "어쩔수없지",
          "original_form": "어쩔수없지",
          "speaker_id": "2",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025198.1.28",
          "form": "복싱은 괜찮니?",
          "original_form": "복싱은 괜찮니?",
          "speaker_id": "2",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025198.1.29",
          "form": "나 옛날에 복싱 했었어",
          "original_form": "나 옛날에 복싱 했었어",
          "speaker_id": "1",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025198.1.30",
          "form": "엄청 좋아",
          "original_form": "엄청 좋아",
          "speaker_id": "1",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025198.1.31",
          "form": "온몸이 후들거려서 운동한다는 느낌이 제대로더라",
          "original_form": "온몸이 후들거려서 운동한다는 느낌이 제대로더라",
          "speaker_id": "1",
          "time": "20210901 00:47"
        },
        {
          "id": "MDRW2100025198.1.32",
          "form": "굿이다 복싱장 앞에서 보자",
          "original_form": "굿이다 복싱장 앞에서 보자",
          "speaker_id": "2",
          "time": "20210901 00:47"
        }
      ]
    }
  ]
