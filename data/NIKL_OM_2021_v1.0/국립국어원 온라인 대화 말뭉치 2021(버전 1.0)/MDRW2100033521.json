      "utterance": [
        {
          "id": "MDRW2100033521.1.1",
          "form": "영양제 드시나요?",
          "original_form": "영양제 드시나요?",
          "speaker_id": "1",
          "time": "20210824 11:07"
        },
        {
          "id": "MDRW2100033521.1.2",
          "form": "전 귀찮아서 안먹게되더라구요",
          "original_form": "전 귀찮아서 안먹게되더라구요",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100033521.1.3",
          "form": "맨날 사서 날짜지나서 버려요",
          "original_form": "맨날 사서 날짜지나서 버려요",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100033521.1.4",
          "form": "저는 요즘",
          "original_form": "저는 요즘",
          "speaker_id": "1",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100033521.1.5",
          "form": "영양제 자꾸 이거저거사고싶더라고요ㅋㅋㅋ",
          "original_form": "영양제 자꾸 이거저거사고싶더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100033521.1.6",
          "form": "ㅎㅎ 유산균도 먹고 오메가3",
          "original_form": "ㅎㅎ 유산균도 먹고 오메가3",
          "speaker_id": "2",
          "time": "20210824 11:16"
        },
        {
          "id": "MDRW2100033521.1.7",
          "form": "다들 엄청 챙겨먹던데요",
          "original_form": "다들 엄청 챙겨먹던데요",
          "speaker_id": "2",
          "time": "20210824 11:16"
        },
        {
          "id": "MDRW2100033521.1.8",
          "form": "맞아요ㅋㅋㅋ 요즘은",
          "original_form": "맞아요ㅋㅋㅋ 요즘은",
          "speaker_id": "1",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100033521.1.9",
          "form": "안먹는애들이 거의 없더라고요",
          "original_form": "안먹는애들이 거의 없더라고요",
          "speaker_id": "1",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100033521.1.10",
          "form": "먹음효과가 있나요?ㅎ",
          "original_form": "먹음효과가 있나요?ㅎ",
          "speaker_id": "2",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100033521.1.11",
          "form": "피로회복좀 되려나요",
          "original_form": "피로회복좀 되려나요",
          "speaker_id": "2",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100033521.1.12",
          "form": "점심은  뭐드실건가요?",
          "original_form": "점심은  뭐드실건가요?",
          "speaker_id": "2",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100033521.1.13",
          "form": "플라시보효과라해야하나 좋더라구요ㅋㅋㅋ",
          "original_form": "플라시보효과라해야하나 좋더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:29"
        },
        {
          "id": "MDRW2100033521.1.14",
          "form": "저는 간단하게 라면생각중이예요!",
          "original_form": "저는 간단하게 라면생각중이예요!",
          "speaker_id": "1",
          "time": "20210824 11:29"
        },
        {
          "id": "MDRW2100033521.1.15",
          "form": "그런것도 있긴할듯요ㅎ 마음의 안정",
          "original_form": "그런것도 있긴할듯요ㅎ 마음의 안정",
          "speaker_id": "2",
          "time": "20210824 11:29"
        },
        {
          "id": "MDRW2100033521.1.16",
          "form": "프로폴리스 사보려구요ㅋㅋㅋ",
          "original_form": "프로폴리스 사보려구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:36"
        },
        {
          "id": "MDRW2100033521.1.17",
          "form": "목감기에좋다는데요",
          "original_form": "목감기에좋다는데요",
          "speaker_id": "2",
          "time": "20210824 11:37"
        },
        {
          "id": "MDRW2100033521.1.18",
          "form": "면역에도 조하ㅡ",
          "original_form": "면역에도 조하ㅡ",
          "speaker_id": "2",
          "time": "20210824 11:37"
        },
        {
          "id": "MDRW2100033521.1.19",
          "form": "좋고",
          "original_form": "좋고",
          "speaker_id": "2",
          "time": "20210824 11:37"
        },
        {
          "id": "MDRW2100033521.1.20",
          "form": "점심메뉴 추천좀 해주세요  ㅋ",
          "original_form": "점심메뉴 추천좀 해주세요  ㅋ",
          "speaker_id": "2",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100033521.1.21",
          "form": "맨날 메뉴고민ㅠ",
          "original_form": "맨날 메뉴고민ㅠ",
          "speaker_id": "2",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100033521.1.22",
          "form": "짬뽕 어떤가요????!!!!",
          "original_form": "짬뽕 어떤가요????!!!!",
          "speaker_id": "1",
          "time": "20210824 11:46"
        },
        {
