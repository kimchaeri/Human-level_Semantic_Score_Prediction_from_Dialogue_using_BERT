      "utterance": [
        {
          "id": "MDRW2100009406.1.1",
          "form": "배달음식자주드시나요?",
          "original_form": "배달음식자주드시나요?",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009406.1.2",
          "form": "자주는 아니고 한달에 한두번은 먹는 것 같아요ㅎㅎ",
          "original_form": "자주는 아니고 한달에 한두번은 먹는 것 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009406.1.3",
          "form": "주로 어떤음식드세요.",
          "original_form": "주로 어떤음식드세요.",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009406.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009406.1.5",
          "form": "저 치킨이나 초밥 좋아해서 보통 그렇게 시켜먹어요ㅎㅎ",
          "original_form": "저 치킨이나 초밥 좋아해서 보통 그렇게 시켜먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009406.1.6",
          "form": "초밥어떤거좋아하세요?",
          "original_form": "초밥어떤거좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009406.1.7",
          "form": "저는 생새우랑 연어 초밥 좋아해요!",
          "original_form": "저는 생새우랑 연어 초밥 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009406.1.8",
          "form": "저도요! 광어초밥도 좋아합니다",
          "original_form": "저도요! 광어초밥도 좋아합니다",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009406.1.9",
          "form": "방금 당치땡에서 치킨시켰어요 ㅎㅎ",
          "original_form": "방금 당치땡에서 치킨시켰어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009406.1.10",
          "form": "오 광어초밥도 맛있죠ㅎㅎㅎ",
          "original_form": "오 광어초밥도 맛있죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009406.1.11",
          "form": "당치땡은 처음 듣는 브랜드인데 맛있나요??",
          "original_form": "당치땡은 처음 듣는 브랜드인데 맛있나요??",
          "speaker_id": "2",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009406.1.12",
          "form": "당싣은치킨이땡긴다였나",
          "original_form": "당싣은치킨이땡긴다였나",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009406.1.13",
          "form": "로제소스인데맛나요",
          "original_form": "로제소스인데맛나요",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009406.1.14",
          "form": "로제좋아하세요?",
          "original_form": "로제좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009406.1.15",
          "form": "아아 풀네임은 들어본 것 같네요",
          "original_form": "아아 풀네임은 들어본 것 같네요",
          "speaker_id": "2",
          "time": "20210823 21:11"
        },
        {
          "id": "MDRW2100009406.1.16",
          "form": "로제를 사실 아직 제대로 먹어본 적이 없어요..ㅋㅋㅋ",
          "original_form": "로제를 사실 아직 제대로 먹어본 적이 없어요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:11"
        },
        {
          "id": "MDRW2100009406.1.17",
          "form": "파스타드시면 뭐드세요?",
          "original_form": "파스타드시면 뭐드세요?",
          "speaker_id": "1",
          "time": "20210823 21:13"
        },
        {
          "id": "MDRW2100009406.1.18",
          "form": "저는 보통 크림이나 까르보나라 먹습니다!",
          "original_form": "저는 보통 크림이나 까르보나라 먹습니다!",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100009406.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100009406.1.20",
          "form": "저는 물려서 로제로 먹어요",
          "original_form": "저는 물려서 로제로 먹어요",
          "speaker_id": "1",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100009406.1.21",
          "form": "보쌈좋라하세요?",
          "original_form": "보쌈좋라하세요?",
          "speaker_id": "1",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100009406.1.22",
          "form": "넵 보쌈도 좋아합니다! 고기는 맛있으니까요",
          "original_form": "넵 보쌈도 좋아합니다! 고기는 맛있으니까요",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100009406.1.23",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210828 22:05"
        }
      ]
