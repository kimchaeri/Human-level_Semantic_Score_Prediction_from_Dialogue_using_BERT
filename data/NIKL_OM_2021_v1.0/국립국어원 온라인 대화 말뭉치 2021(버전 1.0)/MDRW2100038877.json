      "utterance": [
        {
          "id": "MDRW2100038877.1.1",
          "form": "아무튼 내일 먹을 생각을 하며",
          "original_form": "아무튼 내일 먹을 생각을 하며",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100038877.1.2",
          "form": "오늘은 주린배를 쥐고 자야갰어요",
          "original_form": "오늘은 주린배를 쥐고 자야갰어요",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100038877.1.3",
          "form": "샤브샤브 맛있게 드세요 ㅋㅋ",
          "original_form": "샤브샤브 맛있게 드세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038877.1.4",
          "form": "이런 날시에 딱이죠",
          "original_form": "이런 날시에 딱이죠",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038877.1.5",
          "form": "내일 조금만 더 선성해졌으면 좋겠네요",
          "original_form": "내일 조금만 더 선성해졌으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038877.1.6",
          "form": "이런말을 들으니",
          "original_form": "이런말을 들으니",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038877.1.7",
          "form": "아님 가게가 에어컨을 팍팍 틀면 되죠 ㅋㅋㅋ",
          "original_form": "아님 가게가 에어컨을 팍팍 틀면 되죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100038877.1.8",
          "form": "그런 가게를 찾아버ㅏ야겠어요",
          "original_form": "그런 가게를 찾아버ㅏ야겠어요",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100038877.1.9",
          "form": "갑자기 넘 배거파요!~!~!",
          "original_form": "갑자기 넘 배거파요!~!~!",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100038877.1.10",
          "form": "ㅠㅠ 위장이 내일을 벌써부터 준비하나봐요",
          "original_form": "ㅠㅠ 위장이 내일을 벌써부터 준비하나봐요",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038877.1.11",
          "form": "저보다 부지런한걸요?",
          "original_form": "저보다 부지런한걸요?",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038877.1.12",
          "form": "ㅋㅋㅋㅋㅋ그러게요.. 왜 위장만 부지런한건지..?ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ그러게요.. 왜 위장만 부지런한건지..?ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100038877.1.13",
          "form": "뇌도 부지런 한거죠! ㅋㅋㅋ 뇌가 샤브샤브를 상상하고 위장이 답하구,,",
          "original_form": "뇌도 부지런 한거죠! ㅋㅋㅋ 뇌가 샤브샤브를 상상하고 위장이 답하구,,",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100038877.1.14",
          "form": "저도 샤브를 먹고 싶지만 약속으 잡아서 먹어야 하니 좀 뒤로 미뤄야겠어요",
          "original_form": "저도 샤브를 먹고 싶지만 약속으 잡아서 먹어야 하니 좀 뒤로 미뤄야겠어요",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100038877.1.15",
          "form": "맞아요 ㅠㅠ 아니면 1인샤브는",
          "original_form": "맞아요 ㅠㅠ 아니면 1인샤브는",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100038877.1.16",
          "form": "어떠새요?",
          "original_form": "어떠새요?",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100038877.1.17",
          "form": "ㅠㅠ 그것도 좋지만 전 훠궈 부페에가서 새우랑 고기를 막 퍼먹고 싶어서 나중을 기약할래요",
          "original_form": "ㅠㅠ 그것도 좋지만 전 훠궈 부페에가서 새우랑 고기를 막 퍼먹고 싶어서 나중을 기약할래요",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100038877.1.18",
          "form": "1인샤브는 대개 한접시에 야채 주로 고기조금 나와서요 ㅋㅋ",
          "original_form": "1인샤브는 대개 한접시에 야채 주로 고기조금 나와서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100038877.1.19",
          "form": "아하..~!",
          "original_form": "아하..~!",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100038877.1.20",
          "form": "아 갑자기 새우도 너무 먹고파요",
          "original_form": "아 갑자기 새우도 너무 먹고파요",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100038877.1.21",
          "form": "감바스.. 주셍ㅅ",
          "original_form": "감바스.. 주셍ㅅ",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100038877.1.22",
          "form": "요즘 타이거 새우가인기던데 전 아직 못먹어봤어요 드셔보셨나요?",
          "original_form": "요즘 타이거 새우가인기던데 전 아직 못먹어봤어요 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100038877.1.23",
          "form": "타이거새우요???",
          "original_form": "타이거새우요???",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038877.1.24",
          "form": "처음 들어봤어요 한번 찾아봐야갰내요!",
          "original_form": "처음 들어봤어요 한번 찾아봐야갰내요!",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038877.1.25",
          "form": "엄청 큰 새우인데 못먹어봤어요,, 언젠간 먹어볼거에요",
          "original_form": "엄청 큰 새우인데 못먹어봤어요,, 언젠간 먹어볼거에요",
          "speaker_id": "1",
          "time": "20210902 23:20"
        }
      ]
    }
