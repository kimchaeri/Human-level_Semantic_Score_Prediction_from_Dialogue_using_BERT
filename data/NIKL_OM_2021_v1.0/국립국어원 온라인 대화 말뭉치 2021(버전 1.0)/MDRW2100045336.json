        },
        {
          "id": "MDRW2100045336.1.2",
          "form": "난 어제 남은 고기 데워 먹을려고",
          "original_form": "난 어제 남은 고기 데워 먹을려고",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045336.1.3",
          "form": "맛없을낀데 데워멱으면",
          "original_form": "맛없을낀데 데워멱으면",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045336.1.4",
          "form": "버릴수가 없으니",
          "original_form": "버릴수가 없으니",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045336.1.5",
          "form": "맞다 어떻게든 먹어야지",
          "original_form": "맞다 어떻게든 먹어야지",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045336.1.6",
          "form": "엉 파김치 사온거랑 같이 먹을려고",
          "original_form": "엉 파김치 사온거랑 같이 먹을려고",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045336.1.7",
          "form": "파김치 맛나겠다",
          "original_form": "파김치 맛나겠다",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045336.1.8",
          "form": "조금사와서 낼 먹으면 없을듯",
          "original_form": "조금사와서 낼 먹으면 없을듯",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045336.1.9",
          "form": "좀더사오지",
          "original_form": "좀더사오지",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045336.1.10",
          "form": "맛없을까봐서",
          "original_form": "맛없을까봐서",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045336.1.11",
          "form": "잘했네",
          "original_form": "잘했네",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045336.1.12",
          "form": "엉 난중에 가면 또 사올려고",
          "original_form": "엉 난중에 가면 또 사올려고",
