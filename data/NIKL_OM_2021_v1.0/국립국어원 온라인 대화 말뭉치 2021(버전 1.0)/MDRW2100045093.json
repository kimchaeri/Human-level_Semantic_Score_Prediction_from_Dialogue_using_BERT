      "utterance": [
        {
          "id": "MDRW2100045093.1.1",
          "form": "추억에 남는 여행지가 있을까요?",
          "original_form": "추억에 남는 여행지가 있을까요?",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045093.1.2",
          "form": "추억이라 코로나전에 고성 갔던 게 좋았어요",
          "original_form": "추억이라 코로나전에 고성 갔던 게 좋았어요",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045093.1.3",
          "form": "name2님은 혹시 추억에 남는 여행지 있으세요??",
          "original_form": "&name2&님은 혹시 추억에 남는 여행지 있으세요??",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045093.1.4",
          "form": "전 여수요!!!",
          "original_form": "전 여수요!!!",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045093.1.5",
          "form": "여수 예술랜드가 엄청 좋았어요",
          "original_form": "여수 예술랜드가 엄청 좋았어요",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045093.1.6",
          "form": "오 여수 예술랜드라는 곳이 있나보네요!!",
          "original_form": "오 여수 예술랜드라는 곳이 있나보네요!!",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045093.1.7",
          "form": "네네 리조트인데",
          "original_form": "네네 리조트인데",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045093.1.8",
          "form": "오 좋겠어요",
          "original_form": "오 좋겠어요",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045093.1.9",
          "form": "리조트라니!!",
          "original_form": "리조트라니!!",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045093.1.10",
          "form": "익스트림공원 같으곳이 있는데요",
          "original_form": "익스트림공원 같으곳이 있는데요",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045093.1.11",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045093.1.12",
          "form": "리조트안묵어도 이용권구매가능해요",
          "original_form": "리조트안묵어도 이용권구매가능해요",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045093.1.13",
          "form": "하늘걷기 하늘그네.. 있거드요",
          "original_form": "하늘걷기 하늘그네.. 있거드요",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045093.1.14",
          "form": "거기가 좋군요!!",
          "original_form": "거기가 좋군요!!",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045093.1.15",
          "form": "오 처음 들었어요",
          "original_form": "오 처음 들었어요",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045093.1.16",
          "form": "완전 좋아요",
          "original_form": "완전 좋아요",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045093.1.17",
          "form": "찾아보고 싶은데",
          "original_form": "찾아보고 싶은데",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045093.1.18",
          "form": "적어는 놔야겠어욬ㅋㅋㅋ",
          "original_form": "적어는 놔야겠어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045093.1.19",
          "form": "거인의 손에서 사진찍고 바다도 보고요 ㅎㅎ",
          "original_form": "거인의 손에서 사진찍고 바다도 보고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045093.1.20",
          "form": "재미있게 놀다 오셨네요",
          "original_form": "재미있게 놀다 오셨네요",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045093.1.21",
          "form": "네네 진짜 괜찮아요",
          "original_form": "네네 진짜 괜찮아요",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045093.1.22",
          "form": "저는 고성에서 스퀘어루트라는 카페를 갔는데",
          "original_form": "저는 고성에서 스퀘어루트라는 카페를 갔는데",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045093.1.23",
          "form": "1박이였지만",
          "original_form": "1박이였지만",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045093.1.24",
          "form": "좋았어용",
          "original_form": "좋았어용",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045093.1.25",
          "form": "그 카페앞 바다가 좋았어요!!",
          "original_form": "그 카페앞 바다가 좋았어요!!",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045093.1.26",
          "form": "그럴거 같아요!!",
          "original_form": "그럴거 같아요!!",
