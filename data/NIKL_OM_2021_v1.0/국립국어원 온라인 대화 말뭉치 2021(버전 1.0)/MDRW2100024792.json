      "utterance": [
        {
          "id": "MDRW2100024792.1.1",
          "form": "설...설마요!!!",
          "original_form": "설...설마요!!!",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024792.1.2",
          "form": "실시간인가요???",
          "original_form": "실시간인가요???",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024792.1.3",
          "form": "저 녹강으로 알고있었는데요ㅠㅠ",
          "original_form": "저 녹강으로 알고있었는데요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024792.1.4",
          "form": "아 맞네요!!!!",
          "original_form": "아 맞네요!!!!",
          "speaker_id": "1",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024792.1.5",
          "form": "녹강 맞아요!!!!",
          "original_form": "녹강 맞아요!!!!",
          "speaker_id": "1",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024792.1.6",
          "form": "휴 다행이에요",
          "original_form": "휴 다행이에요",
          "speaker_id": "1",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024792.1.7",
          "form": "코로나 시국에 학교를 다니면 그래도 좋은 점은...",
          "original_form": "코로나 시국에 학교를 다니면 그래도 좋은 점은...",
          "speaker_id": "1",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024792.1.8",
          "form": "2교시 수업에 맞춰서 오지 않아도 된다는 것...",
          "original_form": "2교시 수업에 맞춰서 오지 않아도 된다는 것...{emoji:☆}",
          "speaker_id": "1",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024792.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아니 누가 대학원 강의를 아침부터 하나뇨ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아니 누가 대학원 강의를 아침부터 하나뇨ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:16"
        },
        {
          "id": "MDRW2100024792.1.10",
          "form": "정말 너무하네요",
          "original_form": "정말 너무하네요",
          "speaker_id": "2",
          "time": "20210831 20:16"
        },
        {
          "id": "MDRW2100024792.1.11",
          "form": "자비없는 대학원 같으니라고...",
          "original_form": "자비없는 대학원 같으니라고...",
          "speaker_id": "2",
          "time": "20210831 20:16"
        },
        {
          "id": "MDRW2100024792.1.12",
          "form": "그러게 말이에요...",
          "original_form": "그러게 말이에요...",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100024792.1.13",
          "form": "도비들은 아무 일을 하지 않아도 항상 지쳐있는데!!!",
          "original_form": "도비들은 아무 일을 하지 않아도 항상 지쳐있는데!!!",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100024792.1.14",
          "form": "딱히 뭐 안해도 왜이렇게 피곤하나 생각해봤는데",
          "original_form": "딱히 뭐 안해도 왜이렇게 피곤하나 생각해봤는데",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100024792.1.15",
          "form": "언제 교수님이 올라오실지 모른다는 불안감 때문인 것 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "언제 교수님이 올라오실지 모른다는 불안감 때문인 것 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100024792.1.16",
          "form": "그렇죠! 저희 2년만 잘버텨요ㅜㅜ",
          "original_form": "그렇죠! 저희 2년만 잘버텨요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 20:34"
        },
        {
          "id": "MDRW2100024792.1.17",
          "form": "언니는 이제 1년 반...",
          "original_form": "언니는 이제 1년 반...",
          "speaker_id": "2",
          "time": "20210831 20:34"
        },
        {
          "id": "MDRW2100024792.1.18",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100024792.1.19",
          "form": "그래도 저는",
          "original_form": "그래도 저는",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100024792.1.20",
          "form": "인턴을 한 학기를 해서",
          "original_form": "인턴을 한 학기를 해서",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100024792.1.21",
          "form": "2년 6개월 다닌거다보니",
          "original_form": "2년 6개월 다닌거다보니",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100024792.1.22",
          "form": "이제 두 학기 지낸 기분이에요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이제 두 학기 지낸 기분이에요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100024792.1.23",
          "form": "사실 인턴할 때가 제일 바빴던 것 같아요...",
          "original_form": "사실 인턴할 때가 제일 바빴던 것 같아요...",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100024792.1.24",
          "form": "그러네요 진짜!",
          "original_form": "그러네요 진짜!",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024792.1.25",
          "form": "언니 인턴 때 바빠보이셨는데...!!",
          "original_form": "언니 인턴 때 바빠보이셨는데...!!",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024792.1.26",
          "form": "그때 제일 바빠보이셨어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그때 제일 바빠보이셨어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024792.1.27",
          "form": "언니는 근데 왜 그렇게 일찍 컨택하신거에요? ㅜㅜ 물론 일찍와서 열심히 하셨지만 좀 쉬고싶지 않으셨어요??",
          "original_form": "언니는 근데 왜 그렇게 일찍 컨택하신거에요? ㅜㅜ 물론 일찍와서 열심히 하셨지만 좀 쉬고싶지 않으셨어요??",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024792.1.28",
          "form": "저는 왜그랬냐면요",
          "original_form": "저는 왜그랬냐면요",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024792.1.29",
          "form": "8월에 피트 끝나고 채점하자마자",
          "original_form": "8월에 피트 끝나고 채점하자마자",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024792.1.30",
          "form": "아 이건 내 길이 아니구나 싶어서",
          "original_form": "아 이건 내 길이 아니구나 싶어서",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024792.1.31",
          "form": "근데 나이는 또 많고....",
          "original_form": "근데 나이는 또 많고....",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024792.1.32",
          "form": "근데 엄마가 당장 대학원 들어가라그래서",
          "original_form": "근데 엄마가 당장 대학원 들어가라그래서",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024792.1.33",
          "form": "2학기 원서접수는 4월인가 그런데 그것도 모르고",
          "original_form": "2학기 원서접수는 4월인가 그런데 그것도 모르고",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024792.1.34",
          "form": "8월에 교수님한테 다짜고짜 메일 보냈어욬ㅋㅋㅋㅋ",
          "original_form": "8월에 교수님한테 다짜고짜 메일 보냈어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024792.1.35",
          "form": "아 원서접수일을 모르셨구나ㅠㅠ",
          "original_form": "아 원서접수일을 모르셨구나ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024792.1.36",
          "form": "그래도 어떻게보면 행운이죠!!",
          "original_form": "그래도 어떻게보면 행운이죠!!",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024792.1.37",
          "form": "저도 언니랑 1년차이났으면 1년은 언니 없이 지내야 할텐데ㅜㅜ",
          "original_form": "저도 언니랑 1년차이났으면 1년은 언니 없이 지내야 할텐데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024792.1.38",
          "form": "그러게요!!!",
          "original_form": "그러게요!!!",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024792.1.39",
          "form": "결국 잘됐어요 ",
          "original_form": "결국 잘됐어요 ",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
