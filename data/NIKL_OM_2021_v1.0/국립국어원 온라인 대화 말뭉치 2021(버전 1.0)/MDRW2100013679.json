      "utterance": [
        {
          "id": "MDRW2100013679.1.1",
          "form": "집안일은 대부분 name1님이 하시나요?",
          "original_form": "집안일은 대부분 &name1&님이 하시나요?",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013679.1.2",
          "form": "아뇨 분담해서 하는데",
          "original_form": "아뇨 분담해서 하는데",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013679.1.3",
          "form": "딱딱 나누지는 않았고",
          "original_form": "딱딱 나누지는 않았고",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013679.1.4",
          "form": "와이프랑 저랑 시간나는 사람이",
          "original_form": "와이프랑 저랑 시간나는 사람이",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013679.1.5",
          "form": "알아서 해요",
          "original_form": "알아서 해요",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013679.1.6",
          "form": "자취하세요?",
          "original_form": "자취하세요?",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013679.1.7",
          "form": "아뇨 저는 부모님한테 덤으로 붙어있어요..ㅎㅎ",
          "original_form": "아뇨 저는 부모님한테 덤으로 붙어있어요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013679.1.8",
          "form": "그래서 거의 모든 집안일은 제가 해요",
          "original_form": "그래서 거의 모든 집안일은 제가 해요",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013679.1.9",
          "form": "하기 싫은 집안일 있으세요?",
          "original_form": "하기 싫은 집안일 있으세요?",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013679.1.10",
          "form": "저는 설거지가 너무 귀찮아요..",
          "original_form": "저는 설거지가 너무 귀찮아요..",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013679.1.11",
          "form": "저도 설거지 너무 싫어요",
          "original_form": "저도 설거지 너무 싫어요",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013679.1.12",
          "form": "식세기 꼭 필요해요",
          "original_form": "식세기 꼭 필요해요",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013679.1.13",
          "form": "사실 좋아하는 집안일은 없슴니다",
          "original_form": "사실 좋아하는 집안일은 없슴니다",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013679.1.14",
          "form": "해야하기 때문에 할 뿐",
          "original_form": "해야하기 때문에 할 뿐",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013679.1.15",
          "form": "저는 월세 사는데 돈이 안모여요",
          "original_form": "저는 월세 사는데 돈이 안모여요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013679.1.16",
          "form": "그쵸 집안일 좋아하기 쉽지않죠 ㅠ",
          "original_form": "그쵸 집안일 좋아하기 쉽지않죠 ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013679.1.17",
          "form": "돈 모으는게 진짜... 저도 언젠가 독립해야한다고 생각하면 너무 착잡해요",
          "original_form": "돈 모으는게 진짜... 저도 언젠가 독립해야한다고 생각하면 너무 착잡해요",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013679.1.18",
          "form": "죽기전에 제 집 마련하는게 가능할지 모르겠어요",
          "original_form": "죽기전에 제 집 마련하는게 가능할지 모르겠어요",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013679.1.19",
          "form": "집값ㅠㅠ",
          "original_form": "집값ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013679.1.20",
          "form": "수도권사세요?",
          "original_form": "수도권사세요?",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013679.1.21",
          "form": "네 서울 살아요 name1님은요?",
          "original_form": "네 서울 살아요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013679.1.22",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013679.1.23",
          "form": "저는 울산이에요",
          "original_form": "저는 울산이에요",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013679.1.24",
          "form": "서울이면 진짜 집값 답없다",
          "original_form": "서울이면 진짜 집값 답없다",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013679.1.25",
          "form": "울산에 비 많이 왔다고 들었는데 괜찮으신가 모르겠어요 건조기 있으세요?",
          "original_form": "울산에 비 많이 왔다고 들었는데 괜찮으신가 모르겠어요 건조기 있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013679.1.26",
          "form": "서울에도 지금 비가 계속 왔다갔다해서 빨래가 안말라요",
          "original_form": "서울에도 지금 비가 계속 왔다갔다해서 빨래가 안말라요",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013679.1.27",
          "form": "비 생각보다 조용히 지나갔어요",
          "original_form": "비 생각보다 조용히 지나갔어요",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013679.1.28",
          "form": "건조기 있는데 너무 편리해요",
          "original_form": "건조기 있는데 너무 편리해요",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013679.1.29",
          "form": "수건 같은거 건조할때",
          "original_form": "수건 같은거 건조할때",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013679.1.30",
          "form": "사람은 기계가 필요해요 그쵸 건조기는 없지만 그나마 제습기로 빨래를 말려요 ㅠ",
          "original_form": "사람은 기계가 필요해요 그쵸 건조기는 없지만 그나마 제습기로 빨래를 말려요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013679.1.31",
          "form": "빨래 고민이 많으시군여ㅎㅎ",
          "original_form": "빨래 고민이 많으시군여ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013679.1.32",
          "form": "나가서 살아본적은 없나요?",
          "original_form": "나가서 살아본적은 없나요?",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013679.1.33",
          "form": "네 아마 나가 살겠다고 하면 머리를 밀릴거에요",
          "original_form": "네 아마 나가 살겠다고 하면 머리를 밀릴거에요",
          "speaker_id": "2",
          "time": "20210825 16:23"
        }
      ]
