      "utterance": [
        {
          "id": "MDRW2100032451.1.1",
          "form": "몸에지장은없나",
          "original_form": "몸에지장은없나",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100032451.1.2",
          "form": "즐겨드시는 거 추천해주세요",
          "original_form": "즐겨드시는 거 추천해주세요",
          "speaker_id": "2",
          "time": "20210907 08:26"
        },
        {
          "id": "MDRW2100032451.1.3",
          "form": "스벅은 자주 가시죠?",
          "original_form": "스벅은 자주 가시죠?",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100032451.1.4",
          "form": "스벅들고 야구장 가고싶네요",
          "original_form": "스벅들고 야구장 가고싶네요",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100032451.1.5",
          "form": "야구 좋아하세요??",
          "original_form": "야구 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100032451.1.6",
          "form": "ㅋㅋㅋ 직관은 잼더라구용",
          "original_form": "ㅋㅋㅋ 직관은 잼더라구용",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100032451.1.7",
          "form": "맞아요!!! 직관은 달라요!",
          "original_form": "맞아요!!! 직관은 달라요!",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100032451.1.8",
          "form": "ㅋㅋㅋ 사실 잘 몰라서요 야구",
          "original_form": "ㅋㅋㅋ 사실 잘 몰라서요 야구",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100032451.1.9",
          "form": "근대 가는건 조아함 ㅋㅋㅋ",
          "original_form": "근대 가는건 조아함 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100032451.1.10",
          "form": "치킨사들고요",
          "original_form": "치킨사들고요",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100032451.1.11",
          "form": "ㅋㅋ보통 먹으러가죠ㅋㅋ",
          "original_form": "ㅋㅋ보통 먹으러가죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100032451.1.12",
          "form": "응원몰라도 가서 따라하다보면 존잼 ㅋㅋ",
          "original_form": "응원몰라도 가서 따라하다보면 존잼 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100032451.1.13",
          "form": "그렇다고하더라고요!",
          "original_form": "그렇다고하더라고요!",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100032451.1.14",
          "form": "응원하는 팀 있어요?",
          "original_form": "응원하는 팀 있어요?",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100032451.1.15",
          "form": "이젠 없어요",
          "original_form": "이젠 없어요",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100032451.1.16",
          "form": "옛날에 두산이옄 ㅋㅋㅋ",
          "original_form": "옛날에 두산이옄 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100032451.1.17",
          "form": "ㅋㅋ왜요!!!",
          "original_form": "ㅋㅋ왜요!!!",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100032451.1.18",
          "form": "걍 야구 탈덕 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "걍 야구 탈덕 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100032451.1.19",
          "form": "최근엔 한화경기 응원가고 그랫네용 대전이라ㅋㅋㅋ",
          "original_form": "최근엔 한화경기 응원가고 그랫네용 대전이라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100032451.1.20",
          "form": "탈덕은 진행중?",
          "original_form": "탈덕은 진행중?",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100032451.1.21",
          "form": "ㅋㅋㅋ 10년됏어요 야구 안챙겨본지 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 10년됏어요 야구 안챙겨본지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100032451.1.22",
          "form": "그냥 1년에 한번씩 이벤트로 직관가고 ㅋㅋㅋ",
          "original_form": "그냥 1년에 한번씩 이벤트로 직관가고 ㅋㅋㅋ",
