      "utterance": [
        {
          "id": "MDRW2100034529.1.1",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100034529.1.2",
          "form": "네! 닭갈비요",
          "original_form": "네! 닭갈비요",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100034529.1.3",
          "form": "드셨나요",
          "original_form": "드셨나요",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100034529.1.4",
          "form": "네 저는 삼치 구워 먹었어요",
          "original_form": "네 저는 삼치 구워 먹었어요",
          "speaker_id": "2",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100034529.1.5",
          "form": "맛있겠어요!",
          "original_form": "맛있겠어요!",
          "speaker_id": "1",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100034529.1.6",
          "form": "집밥이 최고죠",
          "original_form": "집밥이 최고죠",
          "speaker_id": "1",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100034529.1.7",
          "form": "네 주말에 집밥 먹어줘야죠ㅎㅎ",
          "original_form": "네 주말에 집밥 먹어줘야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100034529.1.8",
          "form": "맞아요 요새 너무 사먹어서요 저두 ㅜㅜ",
          "original_form": "맞아요 요새 너무 사먹어서요 저두 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100034529.1.9",
          "form": "네 저도 한동안 배달음식 많이 먹어서",
          "original_form": "네 저도 한동안 배달음식 많이 먹어서",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100034529.1.10",
          "form": "자제하려는 중이예요",
          "original_form": "자제하려는 중이예요",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100034529.1.11",
          "form": "다 똑같네요 요즘에는 ㅋㅋㅋ",
          "original_form": "다 똑같네요 요즘에는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100034529.1.12",
          "form": "살도찌구 ㅜㅜ",
          "original_form": "살도찌구 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100034529.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100034529.1.14",
          "form": "배달음식은 아무래도 양도 많고 간도 세니까",
          "original_form": "배달음식은 아무래도 양도 많고 간도 세니까",
          "speaker_id": "2",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100034529.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100034529.1.16",
          "form": "건강에 별로죠",
          "original_form": "건강에 별로죠",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100034529.1.17",
          "form": "근데 여름엔 넘 더워서 요리할 엄두가 나지 않아 자주 시켜먹었어요",
          "original_form": "근데 여름엔 넘 더워서 요리할 엄두가 나지 않아 자주 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100034529.1.18",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100034529.1.19",
          "form": "너무 덥죠 땀나고..",
          "original_form": "너무 덥죠 땀나고..",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100034529.1.20",
          "form": "네 더운 날 입맛도 떨어지니까",
          "original_form": "네 더운 날 입맛도 떨어지니까",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100034529.1.21",
          "form": "자극적이거나 시원한 음식 자주 시켜 먹었어요",
          "original_form": "자극적이거나 시원한 음식 자주 시켜 먹었어요",
          "speaker_id": "2",
          "time": "20210828 15:14"
        }
