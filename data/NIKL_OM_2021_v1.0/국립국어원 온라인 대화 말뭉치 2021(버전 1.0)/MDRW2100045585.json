        {
          "id": "MDRW2100045585.1.2",
          "form": "오늘 저녁 뭐먹었어",
          "original_form": "오늘 저녁 뭐먹었어",
          "speaker_id": "1",
          "time": "20210908 03:50"
        },
        {
          "id": "MDRW2100045585.1.3",
          "form": "나 오늘 순대국밥",
          "original_form": "나 오늘 순대국밥",
          "speaker_id": "2",
          "time": "20210908 03:50"
        },
        {
          "id": "MDRW2100045585.1.4",
          "form": "헐 내사랑 순대국밥",
          "original_form": "헐 내사랑 순대국밥",
          "speaker_id": "1",
          "time": "20210908 03:50"
        },
        {
          "id": "MDRW2100045585.1.5",
          "form": "ㅋㅋㅋ너도 국밥 좋아하구나",
          "original_form": "ㅋㅋㅋ너도 국밥 좋아하구나",
          "speaker_id": "2",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100045585.1.6",
          "form": "응 난 머리국밥도 좋아해",
          "original_form": "응 난 머리국밥도 좋아해",
          "speaker_id": "1",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100045585.1.7",
          "form": "ㅋㅋㅋ나는 순대만 좋더라",
          "original_form": "ㅋㅋㅋ나는 순대만 좋더라",
          "speaker_id": "2",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100045585.1.8",
          "form": "어디가서 먹었어",
          "original_form": "어디가서 먹었어",
          "speaker_id": "1",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100045585.1.9",
          "form": "신용산역까지 걸어갔어",
          "original_form": "신용산역까지 걸어갔어",
          "speaker_id": "2",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100045585.1.10",
          "form": "그쪽에 맛집많지",
          "original_form": "그쪽에 맛집많지",
          "speaker_id": "1",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100045585.1.11",
          "form": "맞아 먹을곳 진짜 많아",
          "original_form": "맞아 먹을곳 진짜 많아",
          "speaker_id": "2",
          "time": "20210908 03:52"
        },
        {
          "id": "MDRW2100045585.1.12",
          "form": "국밥에 부추넣는거 너무좋아",
          "original_form": "국밥에 부추넣는거 너무좋아",
          "speaker_id": "1",
          "time": "20210908 03:52"
        },
        {
          "id": "MDRW2100045585.1.13",
          "form": "나도 부추 꼭 넣어먹어",
          "original_form": "나도 부추 꼭 넣어먹어",
          "speaker_id": "2",
          "time": "20210908 03:52"
        },
        {
          "id": "MDRW2100045585.1.14",
          "form": "비오니까 따뜻한 거 사먹었구나 ㅋㅋㅋ",
          "original_form": "비오니까 따뜻한 거 사먹었구나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:53"
        },
        {
          "id": "MDRW2100045585.1.15",
          "form": "응 비와서 국밥이 땡기더라 ㅋㅋ",
          "original_form": "응 비와서 국밥이 땡기더라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:53"
        },
        {
          "id": "MDRW2100045585.1.16",
          "form": "거기 김치도 맛있어?",
          "original_form": "거기 김치도 맛있어?",
          "speaker_id": "1",
          "time": "20210908 03:53"
        },
        {
          "id": "MDRW2100045585.1.17",
          "form": "어 체인점인데도 맛있더라",
          "original_form": "어 체인점인데도 맛있더라",
          "speaker_id": "2",
