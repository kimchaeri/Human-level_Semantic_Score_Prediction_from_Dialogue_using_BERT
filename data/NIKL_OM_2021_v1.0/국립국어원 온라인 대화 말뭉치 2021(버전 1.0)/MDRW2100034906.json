      "utterance": [
        {
          "id": "MDRW2100034906.1.1",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100034906.1.2",
          "form": "네네 ㅎㅎ 방송도 쉬고 ,,",
          "original_form": "네네 ㅎㅎ 방송도 쉬고 ,,",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100034906.1.3",
          "form": "먹방은 주로 누구 보세요?",
          "original_form": "먹방은 주로 누구 보세요?",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100034906.1.4",
          "form": "입짧은햇님 님이랑 나도 님 영상 많이 봐욤",
          "original_form": "입짧은햇님 님이랑 나도 님 영상 많이 봐욤",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100034906.1.5",
          "form": "어 저두요 !",
          "original_form": "어 저두요 !",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100034906.1.6",
          "form": "두분 같이 채널하시잖아요 name3님이랑 ㅎㅎ",
          "original_form": "두분 같이 채널하시잖아요 &name3&님이랑 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100034906.1.7",
          "form": "맞아요! 햇도시락!",
          "original_form": "맞아요! 햇도시락!",
          "speaker_id": "2",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100034906.1.8",
          "form": "맞아요 ㅋㅋ ! 아시는군요 !",
          "original_form": "맞아요 ㅋㅋ ! 아시는군요 !",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100034906.1.9",
          "form": "name3님도 좋아해요 전 ㅎㅎ",
          "original_form": "&name3&님도 좋아해요 전 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100034906.1.10",
          "form": "저도 이번에 알게 돼서 영상 몇 개 찾아봤어요! name3 님 너무 밝으시고 영상도 편하고 친근한 느낌이라 너무 좋았어요",
          "original_form": "저도 이번에 알게 돼서 영상 몇 개 찾아봤어요! &name3& 님 너무 밝으시고 영상도 편하고 친근한 느낌이라 너무 좋았어요",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100034906.1.11",
          "form": "저는 원래 name3님 좋아했거든요 ! 잘되서 너무 좋아요 ㅠㅠ",
          "original_form": "저는 원래 &name3&님 좋아했거든요 ! 잘되서 너무 좋아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100034906.1.12",
          "form": "세 분 다 뭔가 성격이 밝아요 ㅋㅋㅋ",
          "original_form": "세 분 다 뭔가 성격이 밝아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100034906.1.13",
          "form": "그래서 케미가 넘치는 거 같아요!",
          "original_form": "그래서 케미가 넘치는 거 같아요!",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100034906.1.14",
          "form": "오늘 라이브방송 하셨는데",
          "original_form": "오늘 라이브방송 하셨는데",
          "speaker_id": "1",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100034906.1.15",
          "form": "보셨어요?!",
          "original_form": "보셨어요?!",
          "speaker_id": "1",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100034906.1.16",
          "form": "햇님 채널에서 한다는 공지는 봤는데 아까 일이 있어서 못 봤어요ㅠㅠ",
          "original_form": "햇님 채널에서 한다는 공지는 봤는데 아까 일이 있어서 못 봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034906.1.17",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034906.1.18",
          "form": "저도 못봤네요 ㅠㅠ 실시간 보려했는데 !",
          "original_form": "저도 못봤네요 ㅠㅠ 실시간 보려했는데 !",
          "speaker_id": "1",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100034906.1.19",
          "form": "편집본 기다리고있어요 ㅋㅋㅋ",
          "original_form": "편집본 기다리고있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100034906.1.20",
          "form": "저도 편집본으로 보려고요",
          "original_form": "저도 편집본으로 보려고요",
