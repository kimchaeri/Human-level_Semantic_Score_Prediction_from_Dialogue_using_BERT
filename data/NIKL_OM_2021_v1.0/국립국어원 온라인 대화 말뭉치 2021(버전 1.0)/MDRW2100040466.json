      "utterance": [
        {
          "id": "MDRW2100040466.1.1",
          "form": "언니랑은 옷도 나눠입고 그러시나요?",
          "original_form": "언니랑은 옷도 나눠입고 그러시나요?",
          "speaker_id": "1",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100040466.1.2",
          "form": "(언니없는 사람의 로망) ㅋㅋㅋ ㅠㅠ",
          "original_form": "(언니없는 사람의 로망) ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100040466.1.3",
          "form": "어릴때 많이 싸워서... 거의 분리...",
          "original_form": "어릴때 많이 싸워서... 거의 분리...",
          "speaker_id": "2",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100040466.1.4",
          "form": "매번 옷으로 싸우고...ㅜ",
          "original_form": "매번 옷으로 싸우고...ㅜ",
          "speaker_id": "2",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100040466.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 근데진짜 어릴땐 그랬을것같아요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 근데진짜 어릴땐 그랬을것같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100040466.1.6",
          "form": "맞아요 그땐 철이 없고 그래서... ㅜㅋㅋ 반성합니다",
          "original_form": "맞아요 그땐 철이 없고 그래서... ㅜㅋㅋ 반성합니다",
          "speaker_id": "2",
          "time": "20210904 20:58"
        },
        {
          "id": "MDRW2100040466.1.7",
          "form": "ㅋㅋ언니랑은 나이차이 얼마나나요?",
          "original_form": "ㅋㅋ언니랑은 나이차이 얼마나나요?",
          "speaker_id": "1",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100040466.1.8",
          "form": "2살이요ㅋㅋ",
          "original_form": "2살이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100040466.1.9",
          "form": "우왕ㅋㅋ남동생이랑은요?",
          "original_form": "우왕ㅋㅋ남동생이랑은요?",
          "speaker_id": "1",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100040466.1.10",
          "form": "세살",
          "original_form": "세살",
          "speaker_id": "2",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100040466.1.11",
          "form": "몇살 차이나세요.",
          "original_form": "몇살 차이나세요.",
          "speaker_id": "2",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100040466.1.12",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100040466.1.13",
          "form": "저도 세살차인데 남동생이랑 진짜",
          "original_form": "저도 세살차인데 남동생이랑 진짜",
          "speaker_id": "1",
          "time": "20210904 21:06"
        },
        {
          "id": "MDRW2100040466.1.14",
          "form": "미친듯이 싸웠었어요 ㅋㅋㅋㅋㅋ",
          "original_form": "미친듯이 싸웠었어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:06"
        },
        {
          "id": "MDRW2100040466.1.15",
          "form": "ㅋㅋ아 어렸을땐 육탄전했죠",
          "original_form": "ㅋㅋ아 어렸을땐 육탄전했죠",
          "speaker_id": "2",
          "time": "20210904 21:07"
        },
        {
          "id": "MDRW2100040466.1.16",
          "form": "서로 컴퓨터 더 하겠다고",
          "original_form": "서로 컴퓨터 더 하겠다고",
          "speaker_id": "2",
          "time": "20210904 21:08"
        },
        {
          "id": "MDRW2100040466.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 공감ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 공감ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:08"
        },
        {
          "id": "MDRW2100040466.1.18",
          "form": "크래이지아캐이드 2인되는게 정말 최고였죠...",
          "original_form": "크래이지아캐이드 2인되는게 정말 최고였죠...",
          "speaker_id": "1",
          "time": "20210904 21:08"
        },
        {
          "id": "MDRW2100040466.1.19",
          "form": "아... 진짜 한시간이 어찌나 빨리 가는지요ㅜㅠ",
          "original_form": "아... 진짜 한시간이 어찌나 빨리 가는지요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100040466.1.20",
          "form": "남이 할땐 진짜 긴데ㅋㅋ",
          "original_form": "남이 할땐 진짜 긴데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100040466.1.21",
          "form": "맞아요 진짜 ㅋㅋㅋ ㅋ저희는 싸울때 상대방ㅇㅣ 컴터하고 있는데 코드뽑고 막그랬어요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 진짜 ㅋㅋㅋ ㅋ저희는 싸울때 상대방ㅇㅣ 컴터하고 있는데 코드뽑고 막그랬어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100040466.1.22",
          "form": "아 저 막 허벅지 꼬집고",
          "original_form": "아 저 막 허벅지 꼬집고",
          "speaker_id": "2",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100040466.1.23",
          "form": "피봤죠",
          "original_form": "피봤죠",
          "speaker_id": "2",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100040466.1.24",
          "form": "아앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ꼬집기 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ꼬집기 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:14"
        },
        {
