      "utterance": [
        {
          "id": "MDRW2100002447.1.1",
          "form": "너 요즘 하는 일있어?",
          "original_form": "너 요즘 하는 일있어?",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002447.1.2",
          "form": "나는 과외하고 있어",
          "original_form": "나는 과외하고 있어",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002447.1.3",
          "form": "과외로 돈 열심히 버는중",
          "original_form": "과외로 돈 열심히 버는중",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002447.1.4",
          "form": "오오~ 배운거 써먹기네",
          "original_form": "오오~ 배운거 써먹기네",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002447.1.5",
          "form": "무슨 과외해?",
          "original_form": "무슨 과외해?",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002447.1.6",
          "form": "중학생 국어 과외랑",
          "original_form": "중학생 국어 과외랑",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002447.1.7",
          "form": "고등학생 국어 과외",
          "original_form": "고등학생 국어 과외",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002447.1.8",
          "form": "초등학생 수학 과학 과외",
          "original_form": "초등학생 수학 과학 과외",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002447.1.9",
          "form": "초중고 다양하게 하네",
          "original_form": "초중고 다양하게 하네",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002447.1.10",
          "form": "안힘들어?",
          "original_form": "안힘들어?",
          "speaker_id": "1",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002447.1.11",
          "form": "힘들긴 한데",
          "original_form": "힘들긴 한데",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002447.1.12",
          "form": "사는데 너무 돈이 많이 들어서 해야하네",
          "original_form": "사는데 너무 돈이 많이 들어서 해야하네",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002447.1.13",
          "form": "너는 일 하는거 있어?",
          "original_form": "너는 일 하는거 있어?",
          "speaker_id": "2",
          "time": "20210820 12:29"
        },
        {
          "id": "MDRW2100002447.1.14",
          "form": "난 집에서 소설 쓰고 주식하고 있어",
          "original_form": "난 집에서 소설 쓰고 주식하고 있어",
          "speaker_id": "1",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002447.1.15",
          "form": "다양하게 일하고 있네",
          "original_form": "다양하게 일하고 있네",
          "speaker_id": "2",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002447.1.16",
          "form": "난 소설이 그렇게 안써지더라",
          "original_form": "난 소설이 그렇게 안써지더라",
          "speaker_id": "2",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002447.1.17",
          "form": "나도 처음쓸때는 힘들더라고",
          "original_form": "나도 처음쓸때는 힘들더라고",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002447.1.18",
          "form": "소설 전개도 안되고",
          "original_form": "소설 전개도 안되고",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002447.1.19",
          "form": "엉덩이 붙이고 앉아서 쓰기 힘들어",
          "original_form": "엉덩이 붙이고 앉아서 쓰기 힘들어",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002447.1.20",
          "form": "나도 앉아서 쓰기 힘들어서 누워서 폰으로 쓰기도 해",
          "original_form": "나도 앉아서 쓰기 힘들어서 누워서 폰으로 쓰기도 해",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002447.1.21",
          "form": "그거 좋네",
          "original_form": "그거 좋네",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002447.1.22",
          "form": "누워서 일하기",
          "original_form": "누워서 일하기",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002447.1.23",
          "form": "근데 잠도 잘오더라~ 자주 할게 안돼",
          "original_form": "근데 잠도 잘오더라~ 자주 할게 안돼",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002447.1.24",
          "form": "ㅋㅋㅋㅋ 누워서 일하기 아니고 누워서 자기네",
          "original_form": "ㅋㅋㅋㅋ 누워서 일하기 아니고 누워서 자기네",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002447.1.25",
          "form": "요즘 일하느라 힘들다...",
          "original_form": "요즘 일하느라 힘들다...",
          "speaker_id": "2",
          "time": "20210823 10:51"
        }
      ]
    }
  ]
