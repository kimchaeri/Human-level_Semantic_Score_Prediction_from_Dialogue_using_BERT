      "utterance": [
        {
          "id": "MDRW2100034551.1.1",
          "form": "그니까요ㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100034551.1.2",
          "form": "궁금하네요 ㅋㅋㅋ",
          "original_form": "궁금하네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100034551.1.3",
          "form": "입은 참 몸에 안좋은걸 좋아하는거같아요",
          "original_form": "입은 참 몸에 안좋은걸 좋아하는거같아요",
          "speaker_id": "2",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100034551.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100034551.1.5",
          "form": "맛있는게 몸에도 좋으면 좋겠어요",
          "original_form": "맛있는게 몸에도 좋으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100034551.1.6",
          "form": "그니까요ㅠ 맛있는게 살도 안찌면 좋을텐데ㅠ",
          "original_form": "그니까요ㅠ 맛있는게 살도 안찌면 좋을텐데ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100034551.1.7",
          "form": "그이까요 ㅜㅜ",
          "original_form": "그이까요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100034551.1.8",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100034551.1.9",
          "form": "먹을수록 살빠지면 좋겠어요 ㅌㅋㅋ",
          "original_form": "먹을수록 살빠지면 좋겠어요 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100034551.1.10",
          "form": "아ㅋㅋㄱ진짜 그럼 지금 엄청 말랐을텐데ㅠ",
          "original_form": "아ㅋㅋㄱ진짜 그럼 지금 엄청 말랐을텐데ㅠ",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100034551.1.11",
          "form": "상상만해도 좋네요ㅋㅋ",
          "original_form": "상상만해도 좋네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100034551.1.12",
          "form": "맞아옄ㅋㅋㅋ",
          "original_form": "맞아옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100034551.1.13",
          "form": "그럼전 너무 말라서 탈일거에요  ..",
          "original_form": "그럼전 너무 말라서 탈일거에요  ..",
          "speaker_id": "1",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100034551.1.14",
          "form": "저두요  이러나저러나 그만 먹으라고 잔소리 듣겠네요ㅋㅋㅋ",
          "original_form": "저두요  이러나저러나 그만 먹으라고 잔소리 듣겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100034551.1.15",
          "form": "앜ㅋㅋㅋㅋㅋ 그러네요 ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 그러네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:03"
        },
        {
          "id": "MDRW2100034551.1.16",
          "form": "진짜 전 먹는 낙으루 살아여 ㅜ",
          "original_form": "진짜 전 먹는 낙으루 살아여 ㅜ",
          "speaker_id": "1",
          "time": "20210828 15:03"
        },
        {
          "id": "MDRW2100034551.1.17",
          "form": "저도요ㅠㅋㅋㅋ\n맛있는거 먹는게 제일  행복해요ㅋㄱㅋ",
          "original_form": "저도요ㅠㅋㅋㅋ\n맛있는거 먹는게 제일  행복해요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100034551.1.18",
          "form": "맞아요 세상행복",
          "original_form": "맞아요 세상행복",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100034551.1.19",
