      "utterance": [
        {
          "id": "MDRW2100009116.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 21:23"
        },
        {
          "id": "MDRW2100009116.1.2",
          "form": "운동 좋아하시나요?",
          "original_form": "운동 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 21:23"
        },
        {
          "id": "MDRW2100009116.1.3",
          "form": "안녕하세요!\r\n네! 가끔 귀찮기도 하지만 운동하는거좋아해요",
          "original_form": "안녕하세요!\r\n네! 가끔 귀찮기도 하지만 운동하는거좋아해요",
          "speaker_id": "2",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009116.1.4",
          "form": "오옹!저는 요가하고 있어요!",
          "original_form": "오옹!저는 요가하고 있어요!",
          "speaker_id": "1",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009116.1.5",
          "form": "어떤 운동하세요!!",
          "original_form": "어떤 운동하세요!!",
          "speaker_id": "1",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009116.1.6",
          "form": "오~진짜요? 오래하셨나요?",
          "original_form": "오~진짜요? 오래하셨나요?",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009116.1.7",
          "form": "저는 그냥 걷기나 근력운동하고 있어요",
          "original_form": "저는 그냥 걷기나 근력운동하고 있어요",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009116.1.8",
          "form": "아직 3개월밖에 안됐습니당ㅎㅎ",
          "original_form": "아직 3개월밖에 안됐습니당ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009116.1.9",
          "form": "근데 유연성이 조금 좋아진거 같아요!",
          "original_form": "근데 유연성이 조금 좋아진거 같아요!",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009116.1.10",
          "form": "배드민턴이나 야구같은거 배워보고싶은데 좀 멀기도하고 많이  없어서 못하고있어요ㅠ",
          "original_form": "배드민턴이나 야구같은거 배워보고싶은데 좀 멀기도하고 많이  없어서 못하고있어요ㅠ",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009116.1.11",
          "form": "오호! 운동효과가 있네요!",
          "original_form": "오호! 운동효과가 있네요!",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009116.1.12",
          "form": "야구!저도 야구 진짜 좋아해요!",
          "original_form": "야구!저도 야구 진짜 좋아해요!",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009116.1.13",
          "form": "요가 기회되면 한번해보세요!",
          "original_form": "요가 기회되면 한번해보세요!",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009116.1.14",
          "form": "근육도 예쁘게 잡힙니다!",
          "original_form": "근육도 예쁘게 잡힙니다!",
          "speaker_id": "1",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009116.1.15",
          "form": "아 요가 조금 해보긴했는데 너무 잠깐해서",
          "original_form": "아 요가 조금 해보긴했는데 너무 잠깐해서",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009116.1.16",
          "form": "마자요 속근육?생겨서 좋을거같아요",
          "original_form": "마자요 속근육?생겨서 좋을거같아요",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009116.1.17",
          "form": "야구해보셨어요?",
          "original_form": "야구해보셨어요?",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009116.1.18",
          "form": "고등학교때 소프트볼로 했었어요!",
          "original_form": "고등학교때 소프트볼로 했었어요!",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009116.1.19",
          "form": "배워보지는 못하고 그냥 수행평가용?",
          "original_form": "배워보지는 못하고 그냥 수행평가용?",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009116.1.20",
          "form": "오!소프트볼도 되게 매력있더라구요",
          "original_form": "오!소프트볼도 되게 매력있더라구요",
          "speaker_id": "2",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009116.1.21",
          "form": "야구 해보고싶은데 근처에 동호회가 없더라구요ㅠ",
          "original_form": "야구 해보고싶은데 근처에 동호회가 없더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009116.1.22",
          "form": "경기하면 재밌을거같은데ㅠ",
          "original_form": "경기하면 재밌을거같은데ㅠ",
          "speaker_id": "2",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009116.1.23",
          "form": "아ㅠㅠ",
          "original_form": "아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009116.1.24",
          "form": "동호회는 한번도 가입을 안해봐서요",
          "original_form": "동호회는 한번도 가입을 안해봐서요",
          "speaker_id": "1",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009116.1.25",
          "form": "근데 가입하면 진짜 열심히 재밌게 할수 있을거 같아요",
          "original_form": "근데 가입하면 진짜 열심히 재밌게 할수 있을거 같아요",
          "speaker_id": "1",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009116.1.26",
          "form": "그니까요 한팀으로 경기하면 재밌을거같은데",
          "original_form": "그니까요 한팀으로 경기하면 재밌을거같은데",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100009116.1.27",
          "form": "또 배워보신거 뭐 있으세요?",
          "original_form": "또 배워보신거 뭐 있으세요?",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100009116.1.28",
          "form": "저 폴댄스도 해봤습니다!",
          "original_form": "저 폴댄스도 해봤습니다!",
          "speaker_id": "1",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009116.1.29",
          "form": "와! 멋있으시네요\r\n폴댄스 엄청 어려워보이던데 멍도 많이 든다그러더라구요",
          "original_form": "와! 멋있으시네요\r\n폴댄스 엄청 어려워보이던데 멍도 많이 든다그러더라구요",
          "speaker_id": "2",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100009116.1.30",
          "form": "멍 진짜 많이 생겨요ㅠ",
          "original_form": "멍 진짜 많이 생겨요ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009116.1.31",
          "form": "근데 동작을 해냈다는 성취감때문인지 중독성 있더라고요",
          "original_form": "근데 동작을 해냈다는 성취감때문인지 중독성 있더라고요",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009116.1.32",
          "form": "수업비가 비싼게 흠이지만요ㅠ",
          "original_form": "수업비가 비싼게 흠이지만요ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:13"
        }
      ]
