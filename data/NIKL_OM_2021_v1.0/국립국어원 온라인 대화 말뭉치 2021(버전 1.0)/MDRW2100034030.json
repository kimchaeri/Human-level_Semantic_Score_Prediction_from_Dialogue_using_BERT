      "utterance": [
        {
          "id": "MDRW2100034030.1.1",
          "form": "식사 하셔나요!",
          "original_form": "식사 하셔나요!",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100034030.1.2",
          "form": "곧 하려고요!",
          "original_form": "곧 하려고요!",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100034030.1.3",
          "form": "name2 님은 드셨나요?",
          "original_form": "&name2& 님은 드셨나요?",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100034030.1.4",
          "form": "약속이 있어 답이 늦었내요",
          "original_form": "약속이 있어 답이 늦었내요",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034030.1.5",
          "form": "간단하게 챙겨먹었어요",
          "original_form": "간단하게 챙겨먹었어요",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034030.1.6",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034030.1.7",
          "form": "닭볶음탕이랑 같이 먹었어요",
          "original_form": "닭볶음탕이랑 같이 먹었어요",
          "speaker_id": "1",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034030.1.8",
          "form": "name2 님은 닭요리 좋아하시나요?",
          "original_form": "&name2& 님은 닭요리 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034030.1.9",
          "form": "너무 좋아하죠 ㅠㅠ",
          "original_form": "너무 좋아하죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100034030.1.10",
          "form": "특히 닭날개 진짜 사랑해요",
          "original_form": "특히 닭날개 진짜 사랑해요",
          "speaker_id": "2",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100034030.1.11",
          "form": "닭날개 진짜 맛있죠! 저도 치킨 시킬 때 윙봉으로 자주 시켜요ㅎㅎ",
          "original_form": "닭날개 진짜 맛있죠! 저도 치킨 시킬 때 윙봉으로 자주 시켜요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100034030.1.12",
          "form": "치즈볼은 좋아하세여?",
          "original_form": "치즈볼은 좋아하세여?",
          "speaker_id": "2",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100034030.1.13",
          "form": "네! 치즈를 좋아해서",
          "original_form": "네! 치즈를 좋아해서",
          "speaker_id": "1",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100034030.1.14",
          "form": "name2 님은 좋아하시나요?",
          "original_form": "&name2& 님은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100034030.1.15",
          "form": "저도 치즈 엄청 좋아해요",
          "original_form": "저도 치즈 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100034030.1.16",
          "form": "칼로리랑 가격만 착했음..",
          "original_form": "칼로리랑 가격만 착했음..",
          "speaker_id": "2",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100034030.1.17",
          "form": "맨날 먹는건대..",
          "original_form": "맨날 먹는건대..",
          "speaker_id": "2",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100034030.1.18",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100034030.1.19",
          "form": "치즈볼 너무 비싸요ㅠㅠ",
          "original_form": "치즈볼 너무 비싸요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100034030.1.20",
          "form": "진짜..한개에 천원 ㅠㅠ",
          "original_form": "진짜..한개에 천원 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100034030.1.21",
          "form": "가격 무서워요.ㅡ",
          "original_form": "가격 무서워요.ㅡ",
          "speaker_id": "2",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100034030.1.22",
          "form": "금발라놧나..",
          "original_form": "금발라놧나..",
          "speaker_id": "2",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100034030.1.23",
          "form": "정말요... 맛있긴 한데 그래도 가격이 너무 양심없는 거 같아요",
          "original_form": "정말요... 맛있긴 한데 그래도 가격이 너무 양심없는 거 같아요",
          "speaker_id": "1",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100034030.1.24",
          "form": "어유 배고프내요",
          "original_form": "어유 배고프내요",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034030.1.25",
          "form": "저녁은 드셨어요?",
          "original_form": "저녁은 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034030.1.26",
          "form": "좀 전에 먹었어요!",
          "original_form": "좀 전에 먹었어요!",
          "speaker_id": "1",
          "time": "20210827 19:16"
        }
      ]
    }
