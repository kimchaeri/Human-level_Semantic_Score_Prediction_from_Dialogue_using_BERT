      "utterance": [
        {
          "id": "MDRW2100033143.1.1",
          "form": "날씨가 너무 애매하지?",
          "original_form": "날씨가 너무 애매하지?",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100033143.1.2",
          "form": "ㅇㅇ 반팔 입자니 추운 것 같고 긴팔은 덥고 ㅎㅎ",
          "original_form": "ㅇㅇ 반팔 입자니 추운 것 같고 긴팔은 덥고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100033143.1.3",
          "form": "낮엔 꽤 쌀쌀한데 밤엔 더우니",
          "original_form": "낮엔 꽤 쌀쌀한데 밤엔 더우니",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100033143.1.4",
          "form": "이제 하의는 긴바지 입어야 하나",
          "original_form": "이제 하의는 긴바지 입어야 하나",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033143.1.5",
          "form": "바람이 차니 아무래도 긴 게 낫겠지",
          "original_form": "바람이 차니 아무래도 긴 게 낫겠지",
          "speaker_id": "2",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033143.1.6",
          "form": "위는 반팔 입고 아래는 긴팔 입고 해야긋다 ㅎㅎ",
          "original_form": "위는 반팔 입고 아래는 긴팔 입고 해야긋다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033143.1.7",
          "form": "아니면 둘다 반팔 입고 가디건이나 바람막이 챙기던지",
          "original_form": "아니면 둘다 반팔 입고 가디건이나 바람막이 챙기던지",
          "speaker_id": "2",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033143.1.8",
          "form": "그게 나을라나?",
          "original_form": "그게 나을라나?",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033143.1.9",
          "form": "ㅇㅇ 추우면 더 껴입으면 되는데 더우면 어떻게 할 수가 없잖아",
          "original_form": "ㅇㅇ 추우면 더 껴입으면 되는데 더우면 어떻게 할 수가 없잖아",
          "speaker_id": "2",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100033143.1.10",
          "form": "그건 그래.. 얇은 겉옷 항상 챙겨야겠다",
          "original_form": "그건 그래.. 얇은 겉옷 항상 챙겨야겠다",
          "speaker_id": "1",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100033143.1.11",
          "form": "나도 아직까진 반팔 입는데 아침엔 좀 쌀쌀 하더라고",
          "original_form": "나도 아직까진 반팔 입는데 아침엔 좀 쌀쌀 하더라고",
          "speaker_id": "2",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100033143.1.12",
          "form": "신랑도 가디건 같은 거 챙겨",
          "original_form": "신랑도 가디건 같은 거 챙겨",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100033143.1.13",
