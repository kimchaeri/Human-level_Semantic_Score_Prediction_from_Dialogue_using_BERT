      "utterance": [
        {
          "id": "MDRW2100004644.1.1",
          "form": "즐겨보는 프로그램 있어요?",
          "original_form": "즐겨보는 프로그램 있어요?",
          "speaker_id": "1",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004644.1.2",
          "form": "요즘 드라마 알고있지만 봐용",
          "original_form": "요즘 드라마 알고있지만 봐용",
          "speaker_id": "2",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004644.1.3",
          "form": "오 저도 가끔 재방으로 보는데",
          "original_form": "오 저도 가끔 재방으로 보는데",
          "speaker_id": "1",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004644.1.4",
          "form": "잼있던데요",
          "original_form": "잼있던데요",
          "speaker_id": "1",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004644.1.5",
          "form": "그쵸 송강 너뮤 잘생겼어요",
          "original_form": "그쵸 송강 너뮤 잘생겼어요",
          "speaker_id": "2",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004644.1.6",
          "form": "그니까요 매력이 줄줄",
          "original_form": "그니까요 매력이 줄줄",
          "speaker_id": "1",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004644.1.7",
          "form": "피부도 엄청 좋던데요",
          "original_form": "피부도 엄청 좋던데요",
          "speaker_id": "1",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004644.1.8",
          "form": "맞아요 한소희도 예쁘고",
          "original_form": "맞아요 한소희도 예쁘고",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004644.1.9",
          "form": "잘어울려여",
          "original_form": "잘어울려여",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004644.1.10",
          "form": "풋풋하고 이쁘더라구요",
          "original_form": "풋풋하고 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004644.1.11",
          "form": "괜히 설레이고",
          "original_form": "괜히 설레이고",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004644.1.12",
          "form": "맞아요ㅠㅠ 송강 캐릭터가 쓰레기긴하지만..",
          "original_form": "맞아요ㅠㅠ 송강 캐릭터가 쓰레기긴하지만..",
          "speaker_id": "2",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004644.1.13",
          "form": "왜그런 남자한테 끌리는건지ㅋ",
          "original_form": "왜그런 남자한테 끌리는건지ㅋ",
          "speaker_id": "1",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004644.1.14",
          "form": "그런외모면 근데 다 끌릴듯요",
          "original_form": "그런외모면 근데 다 끌릴듯요",
          "speaker_id": "1",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004644.1.15",
          "form": "왜 주변엔 없는지 ㅡㅡ",
          "original_form": "왜 주변엔 없는지 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004644.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요.. 잘생긴 남자좀 보고싶다ㅠㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요.. 잘생긴 남자좀 보고싶다ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004644.1.17",
          "form": "알고있지만은 언제 본방이예요?",
          "original_form": "알고있지만은 언제 본방이예요?",
          "speaker_id": "1",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004644.1.18",
          "form": "오늘 11시요!!",
          "original_form": "오늘 11시요!!",
          "speaker_id": "2",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004644.1.19",
          "form": "마지막화에요ㅠㅠ 오늘ㅠㅜ",
          "original_form": "마지막화에요ㅠㅠ 오늘ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004644.1.20",
          "form": "아 오늘 막방이예요?",
          "original_form": "아 오늘 막방이예요?",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004644.1.21",
          "form": "둘이 어떻게 돼요?",
          "original_form": "둘이 어떻게 돼요?",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004644.1.22",
          "form": "그건 모르겠어용 오늘 봐야알거가타요!",
          "original_form": "그건 모르겠어용 오늘 봐야알거가타요!",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004644.1.23",
          "form": "저도 챙겨봐야겠네요ㅎ 잘되면 좋겠는데",
          "original_form": "저도 챙겨봐야겠네요ㅎ 잘되면 좋겠는데",
