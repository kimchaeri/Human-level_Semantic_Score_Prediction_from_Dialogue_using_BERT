      "utterance": [
        {
          "id": "MDRW2100008351.1.1",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 15:20"
        },
        {
          "id": "MDRW2100008351.1.2",
          "form": "돈 낸거 아까워서라도 가게 돼 있어요",
          "original_form": "돈 낸거 아까워서라도 가게 돼 있어요",
          "speaker_id": "2",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008351.1.3",
          "form": "근데 필테 할 때는 좀...",
          "original_form": "근데 필테 할 때는 좀...",
          "speaker_id": "2",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008351.1.4",
          "form": "왜 내가 돈을 이렇게 많이 내고 힘든걸 하고 있는가 하는 생각이...",
          "original_form": "왜 내가 돈을 이렇게 많이 내고 힘든걸 하고 있는가 하는 생각이...",
          "speaker_id": "2",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008351.1.5",
          "form": "요가..도 그랬어요",
          "original_form": "요가..도 그랬어요",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008351.1.6",
          "form": "요가보다 딱 백배 힘들어요",
          "original_form": "요가보다 딱 백배 힘들어요",
          "speaker_id": "2",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008351.1.7",
          "form": "온 몸이 후들거리는 걍험을 할 수 있어요",
          "original_form": "온 몸이 후들거리는 걍험을 할 수 있어요",
          "speaker_id": "2",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008351.1.8",
          "form": "와 저 허리도 장난없나봐요 요가할때 허리스트레칭하는거 하는데 편히 누울 수가 없어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 저 허리도 장난없나봐요 요가할때 허리스트레칭하는거 하는데 편히 누울 수가 없어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008351.1.9",
          "form": "폼롤러같은? 반원 나무베개에 허리 굴리는 건데 굴리기는 무슨 놓을 수도 없어요",
          "original_form": "폼롤러같은? 반원 나무베개에 허리 굴리는 건데 굴리기는 무슨 놓을 수도 없어요",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008351.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008351.1.11",
          "form": "힘내요...",
          "original_form": "힘내요...",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008351.1.12",
          "form": "그래도 누워있다보니까 괜찮아 지네요 살빼고 처음하는데 살을 뺀건지 건강을 뺀건지 모르겠네요",
          "original_form": "그래도 누워있다보니까 괜찮아 지네요 살빼고 처음하는데 살을 뺀건지 건강을 뺀건지 모르겠네요",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008351.1.13",
          "form": "둘 다 뺀 건 가요",
          "original_form": "둘 다 뺀 건 가요",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008351.1.14",
          "form": "안돼요",
          "original_form": "안돼요",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008351.1.15",
          "form": "스트레칭부터 천천히 시작해서 덜아파지면 동작같은거 해야겠어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "스트레칭부터 천천히 시작해서 덜아파지면 동작같은거 해야겠어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008351.1.16",
          "form": "모래위에 집지으면 무너지니까요..ㅎ",
          "original_form": "모래위에 집지으면 무너지니까요..ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008351.1.17",
          "form": "그래요ㅋㅋㅋㅋ",
          "original_form": "그래요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008351.1.18",
          "form": "무너질 정도로 하지도 못하겠지만...",
          "original_form": "무너질 정도로 하지도 못하겠지만...",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008351.1.19",
          "form": "아야",
          "original_form": "아야",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008351.1.20",
          "form": "제가 때리고 저도 맞았어요",
          "original_form": "제가 때리고 저도 맞았어요",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008351.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008351.1.22",
          "form": "저 플라잉요가 해보고싶었는데 그냥 요가도 안되는데 플라잉은 무슨 싶네요",
          "original_form": "저 플라잉요가 해보고싶었는데 그냥 요가도 안되는데 플라잉은 무슨 싶네요",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008351.1.23",
          "form": "근데 진짜 저 고딩때 너무 안좋아서 보조기 차야된다고 했는데",
          "original_form": "근데 진짜 저 고딩때 너무 안좋아서 보조기 차야된다고 했는데",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008351.1.24",
          "form": "그때부터 운동도 하고 물리치료도 받고 도수치료도 받고 이것저거슈다 해봤는데",
          "original_form": "그때부터 운동도 하고 물리치료도 받고 도수치료도 받고 이것저거슈다 해봤는데",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008351.1.25",
          "form": "헐 진짜 심각하셨네요",
          "original_form": "헐 진짜 심각하셨네요",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008351.1.26",
          "form": "운동만큼 하기싫고 효과좋은게 없어요",
          "original_form": "운동만큼 하기싫고 효과좋은게 없어요",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008351.1.27",
          "form": "너무 공감되네요",
          "original_form": "너무 공감되네요",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008351.1.28",
          "form": "정말 하길 싫고 피곤하고 힘든데",
          "original_form": "정말 하길 싫고 피곤하고 힘든데",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008351.1.29",
          "form": "효과가 좋아요",
          "original_form": "효과가 좋아요",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008351.1.30",
          "form": "화남",
          "original_form": "화남",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008351.1.31",
          "form": "도수치료 = 단기성 효과",
          "original_form": "도수치료 = 단기성 효과",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008351.1.32",
          "form": "물리치료 = no 효과",
          "original_form": "물리치료 = no 효과",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008351.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008351.1.34",
          "form": "물리치료 no 효과인데 안하면 두고두고 아픈게 화나요",
          "original_form": "물리치료 no 효과인데 안하면 두고두고 아픈게 화나요",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008351.1.35",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008351.1.36",
          "form": "물리치료 = 심신의 안정",
          "original_form": "물리치료 = 심신의 안정",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008351.1.37",
          "form": "진짜 심신의 안정ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 심신의 안정ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008351.1.38",
          "form": "효과는 없지만 심신의 안정을 가져다 줍니다",
          "original_form": "효과는 없지만 심신의 안정을 가져다 줍니다",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008351.1.39",
          "form": "해도 티 안나는데 안하면 티나버리는 전형적인 치료죠",
          "original_form": "해도 티 안나는데 안하면 티나버리는 전형적인 치료죠",
