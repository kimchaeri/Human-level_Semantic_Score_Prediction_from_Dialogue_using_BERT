        {
          "id": "MDRW2100003287.1.1",
          "form": "동물키우시나요?",
          "original_form": "동물키우시나요?",
          "speaker_id": "1",
          "time": "20210820 21:25"
        },
        {
          "id": "MDRW2100003287.1.2",
          "form": "동물은 안키우는데 좋아해요 동물키우시나요",
          "original_form": "동물은 안키우는데 좋아해요 동물키우시나요",
          "speaker_id": "2",
          "time": "20210820 21:29"
        },
        {
          "id": "MDRW2100003287.1.3",
          "form": "어떤 동물좋아하세요?",
          "original_form": "어떤 동물좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 21:29"
        },
        {
          "id": "MDRW2100003287.1.4",
          "form": "강아지도 좋아하구 고양이도 좋아해요",
          "original_form": "강아지도 좋아하구 고양이도 좋아해요",
          "speaker_id": "2",
          "time": "20210820 21:29"
        },
        {
          "id": "MDRW2100003287.1.5",
          "form": "강아지 좋아하는 종 있어요?",
          "original_form": "강아지 좋아하는 종 있어요?",
          "speaker_id": "1",
          "time": "20210820 21:30"
        },
        {
          "id": "MDRW2100003287.1.6",
          "form": "딱히 종 가리진않는데 비숑 너무  귀엽더라구요",
          "original_form": "딱히 종 가리진않는데 비숑 너무  귀엽더라구요",
          "speaker_id": "2",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003287.1.7",
          "form": "전 닥스훈트",
          "original_form": "전 닥스훈트",
          "speaker_id": "1",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003287.1.8",
          "form": "오 닥스훈트 한때 다들 많이 키우셨었는데 혹시 키우세용?",
          "original_form": "오 닥스훈트 한때 다들 많이 키우셨었는데 혹시 키우세용?",
          "speaker_id": "2",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003287.1.9",
          "form": "아뇨 지금은 열대어만",
          "original_form": "아뇨 지금은 열대어만",
          "speaker_id": "1",
          "time": "20210820 22:00"
        },
        {
          "id": "MDRW2100003287.1.10",
          "form": "오 열대어 키우셔요?",
          "original_form": "오 열대어 키우셔요?",
          "speaker_id": "2",
          "time": "20210820 22:00"
        },
        {
          "id": "MDRW2100003287.1.11",
          "form": "네 플래티요",
          "original_form": "네 플래티요",
          "speaker_id": "1",
          "time": "20210820 22:02"
        },
        {
          "id": "MDRW2100003287.1.12",
          "form": "플래티 찾아보고올게요ㅋㅋ",
          "original_form": "플래티 찾아보고올게요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:06"
        },
        {
          "id": "MDRW2100003287.1.13",
          "form": "번식력 강해요",
          "original_form": "번식력 강해요",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003287.1.14",
          "form": "오 그럼 몇마리 키우셔용?",
          "original_form": "오 그럼 몇마리 키우셔용?",
