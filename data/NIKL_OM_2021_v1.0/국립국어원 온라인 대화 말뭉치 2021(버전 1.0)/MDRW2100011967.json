      "utterance": [
        {
          "id": "MDRW2100011967.1.1",
          "form": "하비 운동하는거 까페에 있길래 어제 폰보면서 따라했거든",
          "original_form": "하비 운동하는거 까페에 있길래 어제 폰보면서 따라했거든",
          "speaker_id": "2",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011967.1.2",
          "form": "하체비막",
          "original_form": "하체비막",
          "speaker_id": "2",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011967.1.3",
          "form": "만",
          "original_form": "만",
          "speaker_id": "2",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011967.1.4",
          "form": "운동하면 갑자기 근육이 팍 살잖아",
          "original_form": "운동하면 갑자기 근육이 팍 살잖아",
          "speaker_id": "2",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011967.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011967.1.6",
          "form": "언니 하체비만도아니구만",
          "original_form": "언니 하체비만도아니구만",
          "speaker_id": "1",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011967.1.7",
          "form": "애들 재우고 나오더니 갑자기 신랑이",
          "original_form": "애들 재우고 나오더니 갑자기 신랑이",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011967.1.8",
          "form": "응 그렇지ㅋㅋ",
          "original_form": "응 그렇지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011967.1.9",
          "form": "종아이 알이 오늘따라 왜 도드라져 보이냐고 ㅋㅋㅋㄱㅋㅋ",
          "original_form": "종아이 알이 오늘따라 왜 도드라져 보이냐고 ㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011967.1.10",
          "form": "깜짝 놀랐다고",
          "original_form": "깜짝 놀랐다고",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011967.1.11",
          "form": "아 웃겨ㅋㅋ",
          "original_form": "아 웃겨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011967.1.12",
          "form": "왜 튀어나올라하노 카면서 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "왜 튀어나올라하노 카면서 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011967.1.13",
          "form": "아 진짜 너무웃긴다ㅋㅋ",
          "original_form": "아 진짜 너무웃긴다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011967.1.14",
          "form": "신랑은 언니 놀리는재미가 있나보다ㅋㅋ",
          "original_form": "신랑은 언니 놀리는재미가 있나보다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011967.1.15",
          "form": "가",
          "original_form": "가",
          "speaker_id": "2",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011967.1.16",
          "form": "아휴 영 그렇네ㅋㅋㅋ울신랑은 저번에 내보고 살찌라고 막 하더니",
          "original_form": "아휴 영 그렇네ㅋㅋㅋ울신랑은 저번에 내보고 살찌라고 막 하더니",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011967.1.17",
          "form": "막상 살찌니깐ㅋㅋ",
          "original_form": "막상 살찌니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011967.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아",
          "speaker_id": "2",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011967.1.19",
          "form": "언제였더라? 내가 막 화가나서",
          "original_form": "언제였더라? 내가 막 화가나서",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011967.1.20",
          "form": "누워있었거든?",
          "original_form": "누워있었거든?",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011967.1.21",
          "form": "옆에와서 막 안는거야. 근데",
          "original_form": "옆에와서 막 안는거야. 근데",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011967.1.22",
          "form": "하는말이",
          "original_form": "하는말이",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011967.1.23",
          "form": "응 ㅋㅋㅋ",
          "original_form": "응 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.24",
          "form": "아구 옛날엔 다 잡히고 막 힘없어서 들리고했는데",
          "original_form": "아구 옛날엔 다 잡히고 막 힘없어서 들리고했는데",
          "speaker_id": "1",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.25",
          "form": "지금은 버티면",
          "original_form": "지금은 버티면",
          "speaker_id": "1",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.26",
          "form": "못들겠다구",
          "original_form": "못들겠다구",
          "speaker_id": "1",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.29",
          "form": "화난사람한테 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "화난사람한테 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.30",
          "form": "빵터졌음ㅋㅋ",
          "original_form": "빵터졌음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.31",
          "form": "발로 차라",
          "original_form": "발로 차라",
          "speaker_id": "2",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.32",
          "form": "요즘 살만하나?",
          "original_form": "요즘 살만하나?",
          "speaker_id": "1",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.33",
          "form": "이럼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이럼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.34",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.35",
          "form": "여유가 있네",
          "original_form": "여유가 있네",
          "speaker_id": "2",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011967.1.36",
          "form": "그러이  근데 나도 웃겨서",
          "original_form": "그러이  근데 나도 웃겨서",
          "speaker_id": "1",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011967.1.37",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㄱㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011967.1.38",
          "form": "막 웃었지뭐ㅋㅋ",
          "original_form": "막 웃었지뭐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011967.1.39",
          "form": "살찌라매? 캄써 ㅋㅋ",
          "original_form": "살찌라매? 캄써 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011967.1.40",
          "form": "둘다 막 웃음ㅋㅋ",
          "original_form": "둘다 막 웃음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:57"
