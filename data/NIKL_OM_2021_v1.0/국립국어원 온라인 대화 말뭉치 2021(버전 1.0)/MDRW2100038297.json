      "utterance": [
        {
          "id": "MDRW2100038297.1.1",
          "form": "점심 급식 단단히 챙겨드세요",
          "original_form": "점심 급식 단단히 챙겨드세요",
          "speaker_id": "2",
          "time": "20210902 08:57"
        },
        {
          "id": "MDRW2100038297.1.2",
          "form": "ㅋㅋㅋㅋㅋ 그것만 바라보고 있습니다",
          "original_form": "ㅋㅋㅋㅋㅋ 그것만 바라보고 있습니다",
          "speaker_id": "1",
          "time": "20210902 08:58"
        },
        {
          "id": "MDRW2100038297.1.3",
          "form": "어제 짜장면 나왔는데 오늘은 뭐나올지 기대중이에요",
          "original_form": "어제 짜장면 나왔는데 오늘은 뭐나올지 기대중이에요",
          "speaker_id": "1",
          "time": "20210902 08:58"
        },
        {
          "id": "MDRW2100038297.1.4",
          "form": "꼭 수요일 잘나오면",
          "original_form": "꼭 수요일 잘나오면",
          "speaker_id": "2",
          "time": "20210902 09:00"
        },
        {
          "id": "MDRW2100038297.1.5",
          "form": "목요일은 이상하던데",
          "original_form": "목요일은 이상하던데",
          "speaker_id": "2",
          "time": "20210902 09:00"
        },
        {
          "id": "MDRW2100038297.1.6",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210902 09:00"
        },
        {
          "id": "MDRW2100038297.1.7",
          "form": "이틀 연속 맛있을순 없는건짘ㅋㅋㅋ",
          "original_form": "이틀 연속 맛있을순 없는건짘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:00"
        },
        {
          "id": "MDRW2100038297.1.8",
          "form": "수요일에 예산 몰빵하고 목요일엔 가난한ㄴ건지..",
          "original_form": "수요일에 예산 몰빵하고 목요일엔 가난한ㄴ건지..",
          "speaker_id": "1",
          "time": "20210902 09:00"
        },
        {
          "id": "MDRW2100038297.1.9",
          "form": "가난해도 맛있게 만들 수는 없는걸까요ㅠ",
          "original_form": "가난해도 맛있게 만들 수는 없는걸까요ㅠ",
          "speaker_id": "2",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100038297.1.10",
          "form": "ㅋㅋㅋㅋ 예산과 맛이 비례해야만하는지 ㅜ",
          "original_form": "ㅋㅋㅋㅋ 예산과 맛이 비례해야만하는지 ㅜ",
          "speaker_id": "1",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100038297.1.11",
          "form": "그래도 요즘은 친환경무상급식이라 퀄이 높아진거같아요",
          "original_form": "그래도 요즘은 친환경무상급식이라 퀄이 높아진거같아요",
          "speaker_id": "1",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100038297.1.12",
          "form": "끼니마다 과일이나오더라구요",
          "original_form": "끼니마다 과일이나오더라구요",
          "speaker_id": "1",
          "time": "20210902 09:02"
        },
        {
          "id": "MDRW2100038297.1.13",
          "form": "오 그거 엄청 좋네요",
          "original_form": "오 그거 엄청 좋네요",
          "speaker_id": "2",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100038297.1.14",
          "form": "과일로 식사 끝내면 기분 좋은데",
          "original_form": "과일로 식사 끝내면 기분 좋은데",
          "speaker_id": "2",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100038297.1.15",
          "form": "맞아요 영양소도 좀 더 좋을거같구",
          "original_form": "맞아요 영양소도 좀 더 좋을거같구",
          "speaker_id": "1",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100038297.1.16",
          "form": "든든하기도 하죠",
          "original_form": "든든하기도 하죠",
          "speaker_id": "2",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100038297.1.17",
          "form": "오늘은 포도가 나왔어요 ㅋㅋㅋ",
          "original_form": "오늘은 포도가 나왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100038297.1.18",
          "form": "점심 드셨나유",
          "original_form": "점심 드셨나유",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100038297.1.19",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100038297.1.20",
          "form": "곧 먹으려구요",
          "original_form": "곧 먹으려구요",
          "speaker_id": "2",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100038297.1.21",
          "form": "날이 추워져서 슬슬 뜨끈한 음식 땡겨요",
          "original_form": "날이 추워져서 슬슬 뜨끈한 음식 땡겨요",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100038297.1.22",
