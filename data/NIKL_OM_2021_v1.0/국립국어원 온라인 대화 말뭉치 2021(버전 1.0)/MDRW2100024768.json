      "utterance": [
        {
          "id": "MDRW2100024768.1.1",
          "form": "헉 학교에서 코로나가 터졌어?!?!",
          "original_form": "헉 학교에서 코로나가 터졌어?!?!",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024768.1.2",
          "form": "응응 ..",
          "original_form": "응응 ..",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024768.1.3",
          "form": "세상에 선생님들은 괜찮아?!",
          "original_form": "세상에 선생님들은 괜찮아?!",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024768.1.4",
          "form": "응다행히도!!",
          "original_form": "응다행히도!!",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024768.1.5",
          "form": "헐 학생들 사이에서 옮은거야???",
          "original_form": "헐 학생들 사이에서 옮은거야???",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024768.1.6",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024768.1.7",
          "form": "으아아아",
          "original_form": "으아아아",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024768.1.8",
          "form": "다행히 딱 한명이고 동선 겹치거나 하는건 전혀없어",
          "original_form": "다행히 딱 한명이고 동선 겹치거나 하는건 전혀없어",
          "speaker_id": "2",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024768.1.9",
          "form": "계속 자가격리로 미등교하던 학생이라",
          "original_form": "계속 자가격리로 미등교하던 학생이라",
          "speaker_id": "2",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024768.1.10",
          "form": "그나마 다행 ㅜ",
          "original_form": "그나마 다행 ㅜ",
          "speaker_id": "2",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024768.1.11",
          "form": "아 그럼 그 학생도",
          "original_form": "아 그럼 그 학생도",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024768.1.12",
          "form": "가족한테서 옮은건가..?",
          "original_form": "가족한테서 옮은건가..?",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024768.1.13",
          "form": "그러면 같은 반 학생들이",
          "original_form": "그러면 같은 반 학생들이",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024768.1.14",
          "form": "불안해했겠다 8ㅅ8",
          "original_form": "불안해했겠다 8ㅅ8",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024768.1.15",
          "form": "응응맞아 근데 그 학생이 학교안나왔어서",
          "original_form": "응응맞아 근데 그 학생이 학교안나왔어서",
          "speaker_id": "2",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100024768.1.16",
          "form": "학교구성원은 괜찮아 다행히도",
          "original_form": "학교구성원은 괜찮아 다행히도",
          "speaker_id": "2",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100024768.1.17",
          "form": "그냥 선생님들이 바빠지고 아이가 안아파야될텐데 걱정될뿐...",
          "original_form": "그냥 선생님들이 바빠지고 아이가 안아파야될텐데 걱정될뿐...",
          "speaker_id": "2",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100024768.1.18",
          "form": "근데 우리 (대)학교 같은거 신기하지않아? ㅎㅎ",
          "original_form": "근데 우리 (대)학교 같은거 신기하지않아? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024768.1.19",
          "form": "맞아~! 우리학교 학생들 사이에서",
          "original_form": "맞아~! 우리학교 학생들 사이에서",
          "speaker_id": "1",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024768.1.20",
          "form": "취미가 공유되고 있나봐~~~",
          "original_form": "취미가 공유되고 있나봐~~~",
          "speaker_id": "1",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024768.1.21",
          "form": "그러게 선생님들이 바쁜건 정말 안좋다ㅠ",
          "original_form": "그러게 선생님들이 바쁜건 정말 안좋다ㅠ",
          "speaker_id": "1",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024768.1.22",
          "form": "ㅎㅎ 새삼 넘 좋고 신기해",
          "original_form": "ㅎㅎ 새삼 넘 좋고 신기해",
          "speaker_id": "2",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024768.1.23",
          "form": "우리 학교 아니였음 여성학을 훨씬 늦게 접했을지도 몰라",
          "original_form": "우리 학교 아니였음 여성학을 훨씬 늦게 접했을지도 몰라",
          "speaker_id": "2",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024768.1.24",
          "form": "맞아맞아!!!!",
          "original_form": "맞아맞아!!!!",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024768.1.25",
          "form": "나는 그 전에 다니던 학교는 공학이었어서",
          "original_form": "나는 그 전에 다니던 학교는 공학이었어서",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024768.1.26",
          "form": "불편함은 항상 느꼈지만",
          "original_form": "불편함은 항상 느꼈지만",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024768.1.27",
          "form": "그걸 나의 말로 풀어서 설명하진 못했어",
          "original_form": "그걸 나의 말로 풀어서 설명하진 못했어",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024768.1.28",
          "form": "ㅎㅎ 멋지게 살아가는 여성들과 함께한다는건 큰 영감인것 같아",
          "original_form": "ㅎㅎ 멋지게 살아가는 여성들과 함께한다는건 큰 영감인것 같아",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024768.1.29",
          "form": "힘도 되고 ㅎㅎ",
          "original_form": "힘도 되고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024768.1.30",
          "form": "(학교서 지쳐서 코비드 얘길 안하고싶어서..급 대화 주제를 바꿨오 미안해)",
          "original_form": "(학교서 지쳐서 코비드 얘길 안하고싶어서..급 대화 주제를 바꿨오 미안해)",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024768.1.31",
          "form": "응응!!!!!!!",
          "original_form": "응응!!!!!!!",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024768.1.32",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024768.1.33",
          "form": "나는 지금 대학원생이라 이 나이까지 학교를 다니지만",
          "original_form": "나는 지금 대학원생이라 이 나이까지 학교를 다니지만",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024768.1.34",
          "form": "이게 싫을 때도 많은데,,, 그래도 학교 품에 있는 기분이라",
          "original_form": "이게 싫을 때도 많은데,,, 그래도 학교 품에 있는 기분이라",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024768.1.35",
          "form": "좋을 때도 많아 ",
          "original_form": "좋을 때도 많아 ",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024768.1.36",
          "form": "들디는 훌륭한 여성이야",
          "original_form": "들디는 훌륭한 여성이야",
          "speaker_id": "2",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100024768.1.37",
          "form": "우리나라 역사를 바꾸는!",
          "original_form": "우리나라 역사를 바꾸는!",
          "speaker_id": "2",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100024768.1.38",
          "form": "정말 고마워!",
          "original_form": "정말 고마워!",
          "speaker_id": "1",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024768.1.39",
          "form": "name2이도 마찬가지야 ",
          "original_form": "&name2&이도 마찬가지야 ",
