      "utterance": [
        {
          "id": "MDRW2100026473.1.1",
          "form": "예전엔 막 찾아서 다녔는데",
          "original_form": "예전엔 막 찾아서 다녔는데",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026473.1.2",
          "form": "요즘은 유툽 보고 집 가까운덴 가봤어여",
          "original_form": "요즘은 유툽 보고 집 가까운덴 가봤어여",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026473.1.3",
          "form": "카페 더줌이라고",
          "original_form": "카페 더줌이라고",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026473.1.4",
          "form": "오 맛잇엇나용",
          "original_form": "오 맛잇엇나용",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026473.1.5",
          "form": "모드셧어요",
          "original_form": "모드셧어요",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026473.1.6",
          "form": "유튜브 햇도시락에 나온곳있데",
          "original_form": "유튜브 햇도시락에 나온곳있데",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026473.1.7",
          "form": "케익 맛집이에요",
          "original_form": "케익 맛집이에요",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026473.1.8",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026473.1.9",
          "form": "그  입짧은 햇님?이랑 누구랑 합방한 채널이에요",
          "original_form": "그  입짧은 햇님?이랑 누구랑 합방한 채널이에요",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026473.1.10",
          "form": "아아아! ㅋㅋㅋ",
          "original_form": "아아아! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026473.1.11",
          "form": "어떠셧어용",
          "original_form": "어떠셧어용",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026473.1.12",
          "form": "맛있었어요",
          "original_form": "맛있었어요",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026473.1.13",
          "form": "웨이팅도 없고ㅋ",
          "original_form": "웨이팅도 없고ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026473.1.14",
          "form": "맛난건 왜 다 살찌는건지",
          "original_form": "맛난건 왜 다 살찌는건지",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026473.1.15",
          "form": "ㅜㅜㅜㅋㅋㅋ 그죠....",
          "original_form": "ㅜㅜㅜㅋㅋㅋ 그죠....",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026473.1.16",
          "form": "닭가슴살 맛집은 듣도보도 못했어요ㅋ",
          "original_form": "닭가슴살 맛집은 듣도보도 못했어요ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026473.1.17",
          "form": "앜ㅋㅋㅋㅋㅋ 그러네욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 그러네욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026473.1.18",
          "form": "있는데 우리가 모르는걸까요?ㅋㅋ",
          "original_form": "있는데 우리가 모르는걸까요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026473.1.19",
          "form": "지금 드시고싶은거있나요",
          "original_form": "지금 드시고싶은거있나요",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026473.1.20",
          "form": "꾸덕한 브라우니요",
          "original_form": "꾸덕한 브라우니요",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026473.1.21",
          "form": "크....ㅜㅜㅜㅜㅜ",
          "original_form": "크....ㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026473.1.22",
          "form": "넘마싯겟다...차갑게해서먹어도 따뜻하게 해서 먹어도 다 마싯",
          "original_form": "넘마싯겟다...차갑게해서먹어도 따뜻하게 해서 먹어도 다 마싯",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026473.1.23",
          "form": "차게먹고 뜨아로 입가심 하고싶어요",
          "original_form": "차게먹고 뜨아로 입가심 하고싶어요",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026473.1.24",
          "form": "당충전 하고 기분조아지는 느낌",
          "original_form": "당충전 하고 기분조아지는 느낌",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026473.1.25",
          "form": "따뜻한브라우니에 흰우유도 좋은데",
          "original_form": "따뜻한브라우니에 흰우유도 좋은데",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026473.1.26",
          "form": "배부를거 같아서ㅎㅎ",
          "original_form": "배부를거 같아서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:37"
        }
      ]
    }
  ]
