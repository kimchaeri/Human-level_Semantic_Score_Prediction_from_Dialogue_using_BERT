      "utterance": [
        {
          "id": "MDRW2100018915.1.1",
          "form": "그럼 좋겠다",
          "original_form": "그럼 좋겠다",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018915.1.2",
          "form": "그러나 그러기 위해선 졸업을 하거나 방학이어야 할 듯",
          "original_form": "그러나 그러기 위해선 졸업을 하거나 방학이어야 할 듯",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018915.1.3",
          "form": "학기중에 하기엔 너무 너덜너덜해서",
          "original_form": "학기중에 하기엔 너무 너덜너덜해서",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018915.1.4",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018915.1.5",
          "form": "그건그럼",
          "original_form": "그건그럼",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018915.1.6",
          "form": "학기중에는못배울듯",
          "original_form": "학기중에는못배울듯",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018915.1.7",
          "form": "운동을 하고 싶어도 못하지 않아?",
          "original_form": "운동을 하고 싶어도 못하지 않아?",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018915.1.8",
          "form": "밀려오는 과제와 스트레스",
          "original_form": "밀려오는 과제와 스트레스",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018915.1.9",
          "form": "ㅇㅇㅇ그니까",
          "original_form": "ㅇㅇㅇ그니까",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018915.1.10",
          "form": "너네과는밤새서과제하기도하잖아",
          "original_form": "너네과는밤새서과제하기도하잖아",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018915.1.11",
          "form": "일찍 일어나서 하는 것도 못할듯",
          "original_form": "일찍 일어나서 하는 것도 못할듯",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018915.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018915.1.13",
          "form": "큰일났네 아니야 괜찮을걸",
          "original_form": "큰일났네 아니야 괜찮을걸",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018915.1.14",
          "form": "일찍일어나게 되어있어....",
          "original_form": "일찍일어나게 되어있어....",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018915.1.15",
          "form": "1교시 잡았거든.....",
          "original_form": "1교시 잡았거든.....",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018915.1.16",
          "form": "ㅋㅋㅋㅋㅋ밤새고",
          "original_form": "ㅋㅋㅋㅋㅋ밤새고",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
