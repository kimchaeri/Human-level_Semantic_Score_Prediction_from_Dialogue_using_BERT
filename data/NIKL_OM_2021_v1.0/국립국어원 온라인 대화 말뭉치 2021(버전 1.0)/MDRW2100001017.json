      "utterance": [
        {
          "id": "MDRW2100001017.1.1",
          "form": "아르바이트 한 것 중에 젤 힘든게 뭐였어요?",
          "original_form": "아르바이트 한 것 중에 젤 힘든게 뭐였어요?",
          "speaker_id": "2",
          "time": "20210820 12:06"
        },
        {
          "id": "MDRW2100001017.1.2",
          "form": "피씨방알바요..",
          "original_form": "피씨방알바요..",
          "speaker_id": "1",
          "time": "20210820 12:06"
        },
        {
          "id": "MDRW2100001017.1.3",
          "form": "끔찍했어요",
          "original_form": "끔찍했어요",
          "speaker_id": "1",
          "time": "20210820 12:06"
        },
        {
          "id": "MDRW2100001017.1.4",
          "form": "피시방 알바는 괜찮다는 소문도 있는데",
          "original_form": "피시방 알바는 괜찮다는 소문도 있는데",
          "speaker_id": "2",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100001017.1.5",
          "form": "아니요..",
          "original_form": "아니요..",
          "speaker_id": "1",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100001017.1.6",
          "form": "정말 소문인가요... 어떤점이 힘드셨어요!!",
          "original_form": "정말 소문인가요... 어떤점이 힘드셨어요!!",
          "speaker_id": "2",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100001017.1.7",
          "form": "소문은.. 믿지마세요",
          "original_form": "소문은.. 믿지마세요",
          "speaker_id": "1",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100001017.1.8",
          "form": "이게 예전에는 음식이 한정적이었는데",
          "original_form": "이게 예전에는 음식이 한정적이었는데",
          "speaker_id": "1",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100001017.1.9",
          "form": "음식만드는거랑.. 방학되면 급식이 몰려오고..",
          "original_form": "음식만드는거랑.. 방학되면 급식이 몰려오고..",
          "speaker_id": "1",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100001017.1.10",
          "form": "헉ㅋㅋㅋㅋㅋ 맞아요 요즘은 피시방에서",
          "original_form": "헉ㅋㅋㅋㅋㅋ 맞아요 요즘은 피시방에서",
          "speaker_id": "2",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100001017.1.11",
          "form": "카페랑 밥 둘다 다한다고 하잖아요",
          "original_form": "카페랑 밥 둘다 다한다고 하잖아요",
          "speaker_id": "2",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100001017.1.12",
          "form": "음식점이 따로 없더라구요..",
          "original_form": "음식점이 따로 없더라구요..",
          "speaker_id": "1",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100001017.1.13",
          "form": "그리고 안나가는 인간들은..",
          "original_form": "그리고 안나가는 인간들은..",
          "speaker_id": "1",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100001017.1.14",
          "form": "거기서살아요",
          "original_form": "거기서살아요",
          "speaker_id": "1",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100001017.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100001017.1.16",
          "form": "ㅋㅋㅋㅋ 뭔지 알 거 같아요",
          "original_form": "ㅋㅋㅋㅋ 뭔지 알 거 같아요",
          "speaker_id": "2",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100001017.1.17",
          "form": "방학에 하니까 더 힘드셨겠어요",
          "original_form": "방학에 하니까 더 힘드셨겠어요",
          "speaker_id": "2",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100001017.1.18",
          "form": "그럼 쉬운 알바는 뭐 하셨어요?",
          "original_form": "그럼 쉬운 알바는 뭐 하셨어요?",
          "speaker_id": "2",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100001017.1.19",
          "form": "맞아요 진짜 헬.. \r\n음..제가 알바경험은 많지않아서",
          "original_form": "맞아요 진짜 헬.. \r\n음..제가 알바경험은 많지않아서",
          "speaker_id": "1",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100001017.1.20",
          "form": "영화관 괜찮았어요",
          "original_form": "영화관 괜찮았어요",
          "speaker_id": "1",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100001017.1.21",
          "form": "표받는 일 했거든요",
          "original_form": "표받는 일 했거든요",
          "speaker_id": "1",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100001017.1.22",
          "form": "오 영화관.. 재밌었을 거 같아요",
          "original_form": "오 영화관.. 재밌었을 거 같아요",
          "speaker_id": "2",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100001017.1.23",
          "form": "경험은 적어도 굵은 것만 했네요ㅋㅋㅋㅋㅋ",
          "original_form": "경험은 적어도 굵은 것만 했네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100001017.1.24",
          "form": "영화관은",
          "original_form": "영화관은",
          "speaker_id": "1",
          "time": "20210820 12:13"
        },
        {
          "id": "MDRW2100001017.1.25",
          "form": "메표소가 제일 기피였어요",
          "original_form": "메표소가 제일 기피였어요",
          "speaker_id": "1",
          "time": "20210820 12:13"
        },
        {
          "id": "MDRW2100001017.1.26",
          "form": "시재관리 어우 틀리면 눈치 겁나보여요",
          "original_form": "시재관리 어우 틀리면 눈치 겁나보여요",
          "speaker_id": "1",
          "time": "20210820 12:13"
        },
        {
          "id": "MDRW2100001017.1.27",
          "form": "아 그렇죠 모든 알바에서 돈관리가 어려웠어요",
          "original_form": "아 그렇죠 모든 알바에서 돈관리가 어려웠어요",
          "speaker_id": "2",
          "time": "20210820 12:15"
        }
      ]
    }
