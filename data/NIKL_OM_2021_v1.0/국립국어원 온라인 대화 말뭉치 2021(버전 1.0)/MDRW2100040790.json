      "utterance": [
        {
          "id": "MDRW2100040790.1.1",
          "form": "친정에 방문하셨다고 했죠?",
          "original_form": "친정에 방문하셨다고 했죠?",
          "speaker_id": "1",
          "time": "20210905 07:11"
        },
        {
          "id": "MDRW2100040790.1.2",
          "form": "친정에 방문하셨으니 좋으시겠어요~",
          "original_form": "친정에 방문하셨으니 좋으시겠어요~",
          "speaker_id": "1",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100040790.1.3",
          "form": "네네~저 저번주에 백신 맞았는데..",
          "original_form": "네네~저 저번주에 백신 맞았는데..",
          "speaker_id": "2",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100040790.1.4",
          "form": "맞벌이에 애들케어에 너무 지쳐서",
          "original_form": "맞벌이에 애들케어에 너무 지쳐서",
          "speaker_id": "2",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100040790.1.5",
          "form": "피난왔아요ㅠ",
          "original_form": "피난왔아요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100040790.1.6",
          "form": "아 그러셨군요",
          "original_form": "아 그러셨군요",
          "speaker_id": "1",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100040790.1.7",
          "form": "저도 엊그제 2차백신 맞아서",
          "original_form": "저도 엊그제 2차백신 맞아서",
          "speaker_id": "1",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100040790.1.8",
          "form": "뭐 맞으셨어요??",
          "original_form": "뭐 맞으셨어요??",
          "speaker_id": "2",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100040790.1.9",
          "form": "이번 주말엔 집안일 완전 손놓고 지내려고요",
          "original_form": "이번 주말엔 집안일 완전 손놓고 지내려고요",
          "speaker_id": "1",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100040790.1.10",
          "form": "화이자요",
          "original_form": "화이자요",
          "speaker_id": "1",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100040790.1.11",
          "form": "저 화이자 맞았는데 피로감이ㅠㅠ",
          "original_form": "저 화이자 맞았는데 피로감이ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100040790.1.12",
          "form": "전 1차거든요ㅠ",
          "original_form": "전 1차거든요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100040790.1.13",
          "form": "아 1차인데도 증상 느끼셨구나",
          "original_form": "아 1차인데도 증상 느끼셨구나",
          "speaker_id": "1",
          "time": "20210905 07:16"
        },
        {
          "id": "MDRW2100040790.1.14",
          "form": "아이가 여럿이신가요?",
          "original_form": "아이가 여럿이신가요?",
          "speaker_id": "1",
          "time": "20210905 07:16"
        },
        {
          "id": "MDRW2100040790.1.15",
          "form": "둘이예요~둘째가 4살이라 손이 많이가요ㅠ",
          "original_form": "둘이예요~둘째가 4살이라 손이 많이가요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:16"
        },
        {
          "id": "MDRW2100040790.1.16",
          "form": "2차가 겁나요ㅠ",
          "original_form": "2차가 겁나요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:16"
        },
        {
          "id": "MDRW2100040790.1.17",
          "form": "아이고 백신맞고 아이들 육아까진 힘들죠ㅜ",
          "original_form": "아이고 백신맞고 아이들 육아까진 힘들죠ㅜ",
          "speaker_id": "1",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100040790.1.18",
          "form": "이번주엔 친정찬스 꼭 필요했네요",
          "original_form": "이번주엔 친정찬스 꼭 필요했네요",
          "speaker_id": "1",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100040790.1.19",
          "form": "저는 2차가 1차보다 좀 더 증상이 있었는데, 짧게 지나가긴 했어요",
          "original_form": "저는 2차가 1차보다 좀 더 증상이 있었는데, 짧게 지나가긴 했어요",
          "speaker_id": "1",
          "time": "20210905 07:20"
        },
        {
          "id": "MDRW2100040790.1.20",
          "form": "2차맞고도 피난와야겠네요 ㅋㅋㅋㅋㅋ",
          "original_form": "2차맞고도 피난와야겠네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100040790.1.21",
          "form": "네네 2차때도 친정으로 피난 추천합니다!!",
          "original_form": "네네 2차때도 친정으로 피난 추천합니다!!",
          "speaker_id": "1",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100040790.1.22",
          "form": "정말 접종도 일이예요ㅠㅠ",
          "original_form": "정말 접종도 일이예요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100040790.1.23",
          "form": "코로나 정말..ㅡㅡ",
          "original_form": "코로나 정말..ㅡㅡ",
          "speaker_id": "2",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100040790.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
