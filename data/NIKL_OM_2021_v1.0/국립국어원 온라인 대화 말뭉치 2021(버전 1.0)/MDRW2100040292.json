      "utterance": [
        {
          "id": "MDRW2100040292.1.1",
          "form": "로제떡볶이 드셔 보셨나요",
          "original_form": "로제떡볶이 드셔 보셨나요",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040292.1.2",
          "form": "내 근데 엄청 맛있진 않았어요",
          "original_form": "내 근데 엄청 맛있진 않았어요",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040292.1.3",
          "form": "저는 엽떡에서 로제떡볶이 입어 가봤거든요",
          "original_form": "저는 엽떡에서 로제떡볶이 입어 가봤거든요",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040292.1.4",
          "form": "어디 거 시켜서 얻으셨나요",
          "original_form": "어디 거 시켜서 얻으셨나요",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040292.1.5",
          "form": "시던가요",
          "original_form": "시던가요",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040292.1.6",
          "form": "맛있었어요",
          "original_form": "맛있었어요",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040292.1.7",
          "form": "맛있었어요 엽떡 로제떡볶이",
          "original_form": "맛있었어요 엽떡 로제떡볶이",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040292.1.8",
          "form": "시켜 먹지 않았었고",
          "original_form": "시켜 먹지 않았었고",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040292.1.9",
          "form": "기억나지는 않는데 먹은 기억은 있어요",
          "original_form": "기억나지는 않는데 먹은 기억은 있어요",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040292.1.10",
          "form": "요리 하셨나요",
          "original_form": "요리 하셨나요",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040292.1.11",
          "form": "배달 떡볶이인가 거기가 업계 1위인데",
          "original_form": "배달 떡볶이인가 거기가 업계 1위인데",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.12",
          "form": "삼아 떡 먹은 거 같아요 삼",
          "original_form": "삼아 떡 먹은 거 같아요 삼",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.13",
          "form": "사먹은 거 같아요",
          "original_form": "사먹은 거 같아요",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.14",
          "form": "밖에 나가서",
          "original_form": "밖에 나가서",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.15",
          "form": "거기 보다는 신전떡볶이 나 엽기떡볶이 로제 떡볶이가 더 맛있다 더라고요",
          "original_form": "거기 보다는 신전떡볶이 나 엽기떡볶이 로제 떡볶이가 더 맛있다 더라고요",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.16",
          "form": "남포동에서",
          "original_form": "남포동에서",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.17",
          "form": "신전 떡볶이에서도 다음에 사 먹어 보려고요",
          "original_form": "신전 떡볶이에서도 다음에 사 먹어 보려고요",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.18",
          "form": "그런데 살이 많이 질 것 같아요 맛은 좋았어요",
          "original_form": "그런데 살이 많이 질 것 같아요 맛은 좋았어요",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.19",
          "form": "저희 동네 신전 떡볶이",
          "original_form": "저희 동네 신전 떡볶이",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.20",
          "form": "원래 맛있는게 칼로리가 좀 높잖아요",
          "original_form": "원래 맛있는게 칼로리가 좀 높잖아요",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.21",
          "form": "예전 진상 회원 엄마라서",
          "original_form": "예전 진상 회원 엄마라서",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.22",
          "form": "절대 안 시켜 먹으려구요",
          "original_form": "절대 안 시켜 먹으려구요",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.23",
          "form": "신전 떡볶이 치즈 떡볶이 맛있는데",
          "original_form": "신전 떡볶이 치즈 떡볶이 맛있는데",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.24",
          "form": "저 대신 맛있게 드셔 주세요",
          "original_form": "저 대신 맛있게 드셔 주세요",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.25",
          "form": "그럼 시켜 먹지 못 하겠네요",
          "original_form": "그럼 시켜 먹지 못 하겠네요",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040292.1.26",
          "form": "음성 임형 신세계에요",
          "original_form": "음성 임형 신세계에요",
          "speaker_id": "2",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100040292.1.27",
          "form": "제 친구는 감자탕을 일주일 째 먹고 있더라고요",
          "original_form": "제 친구는 감자탕을 일주일 째 먹고 있더라고요",
          "speaker_id": "2",
          "time": "20210904 15:37"
        }
      ]
    }
