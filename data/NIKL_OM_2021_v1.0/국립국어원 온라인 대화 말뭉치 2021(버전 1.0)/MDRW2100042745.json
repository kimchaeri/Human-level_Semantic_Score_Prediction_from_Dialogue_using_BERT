      "utterance": [
        {
          "id": "MDRW2100042745.1.1",
          "form": "했습니다..",
          "original_form": "했습니다..",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042745.1.2",
          "form": "업무 많아요?",
          "original_form": "업무 많아요?",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042745.1.3",
          "form": "항가해요",
          "original_form": "항가해요",
          "speaker_id": "1",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042745.1.4",
          "form": "추석상여 받았나요",
          "original_form": "추석상여 받았나요",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042745.1.5",
          "form": "저희 그런 거 안줘요",
          "original_form": "저희 그런 거 안줘요",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042745.1.6",
          "form": "멸치줌",
          "original_form": "멸치줌",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042745.1.7",
          "form": "저희는 멸치도안줘요",
          "original_form": "저희는 멸치도안줘요",
          "speaker_id": "2",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042745.1.8",
          "form": "무슨 샴푸세트 주던데여",
          "original_form": "무슨 샴푸세트 주던데여",
          "speaker_id": "2",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042745.1.9",
          "form": "설때도아니고 추석에 웬..",
          "original_form": "설때도아니고 추석에 웬..",
          "speaker_id": "2",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042745.1.10",
          "form": "추석에 샴푸?.",
          "original_form": "추석에 샴푸?.",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042745.1.11",
          "form": "넘하네",
          "original_form": "넘하네",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042745.1.12",
          "form": "뻥이요줘라",
          "original_form": "뻥이요줘라",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042745.1.13",
          "form": "진짜 차라리 그거나줘라",
          "original_form": "진짜 차라리 그거나줘라",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042745.1.14",
          "form": "님 사실 전 일있읍",
          "original_form": "님 사실 전 일있읍",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042745.1.15",
          "form": "운영서류해야대",
          "original_form": "운영서류해야대",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042745.1.16",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042745.1.17",
          "form": "아침업무있러",
          "original_form": "아침업무있러",
          "speaker_id": "1",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042745.1.18",
          "form": "모야ㅠ 바뻐요?",
          "original_form": "모야ㅠ 바뻐요?",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042745.1.19",
          "form": "아침에는 일있제",
          "original_form": "아침에는 일있제",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042745.1.20",
          "form": "그치만 월루 가능",
          "original_form": "그치만 월루 가능",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042745.1.21",
          "form": "ㅋㅋㅋㅋㅋ저두..근대 그런말하면",
          "original_form": "ㅋㅋㅋㅋㅋ저두..근대 그런말하면",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042745.1.22",
          "form": "귀신같이바빠짐",
          "original_form": "귀신같이바빠짐",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042745.1.23",
          "form": "알지",
          "original_form": "알지",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042745.1.24",
          "form": "근데 나는 바쁠일이없야",
          "original_form": "근데 나는 바쁠일이없야",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042745.1.25",
          "form": "월초끝나서??",
          "original_form": "월초끝나서??",
          "speaker_id": "2",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042745.1.26",
          "form": "퇴근원합니다",
          "original_form": "퇴근원합니다",
          "speaker_id": "1",
          "time": "20210907 13:17"
        }
      ]
