      "utterance": [
        {
          "id": "MDRW2100015029.1.1",
          "form": "안녕하세요~\r\n저녁 드셨나요?",
          "original_form": "안녕하세요~\r\n저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015029.1.2",
          "form": "안녕하세요 맛저하셨나요?",
          "original_form": "안녕하세요 맛저하셨나요?",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100015029.1.3",
          "form": "전 친구랑 돈까스 먹었어요",
          "original_form": "전 친구랑 돈까스 먹었어요",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015029.1.4",
          "form": "와 돈까스 맛있겠네요 ㅠㅠㅋㅋㅋ",
          "original_form": "와 돈까스 맛있겠네요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015029.1.5",
          "form": "전 샌드위치 먹었어요!",
          "original_form": "전 샌드위치 먹었어요!",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015029.1.6",
          "form": "오 맛있겠네요 든든해요?",
          "original_form": "오 맛있겠네요 든든해요?",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015029.1.7",
          "form": "네 ㅋㅋㅋㅋㅋ 우유도 같이 먹었어요 ㅎㅎ",
          "original_form": "네 ㅋㅋㅋㅋㅋ 우유도 같이 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015029.1.8",
          "form": "앙꼬는 머들어있었나요",
          "original_form": "앙꼬는 머들어있었나요",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015029.1.9",
          "form": "감자갈은거에 + 옥수수랑 야채들 잘게 썰은것들? 그렇게 들어있었어요 ㅋㅋㅋ",
          "original_form": "감자갈은거에 + 옥수수랑 야채들 잘게 썰은것들? 그렇게 들어있었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015029.1.10",
          "form": "오 든든하겠네요 서양식",
          "original_form": "오 든든하겠네요 서양식",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015029.1.11",
          "form": "넹 ㅋㅋㅋㅋ 좋았어요!",
          "original_form": "넹 ㅋㅋㅋㅋ 좋았어요!",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015029.1.12",
          "form": "요즘 꽂힌(?) 음식 같은거 있으신가요??",
          "original_form": "요즘 꽂힌(?) 음식 같은거 있으신가요??",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015029.1.13",
          "form": "오 저는 병이 있는데요.",
          "original_form": "오 저는 병이 있는데요.",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015029.1.14",
          "form": "배고프면 뭐든 잘먹는 병이에요",
          "original_form": "배고프면 뭐든 잘먹는 병이에요",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015029.1.15",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015029.1.16",
          "form": "누구나 다 앓고있는 병 아닌가요 ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "누구나 다 앓고있는 병 아닌가요 ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015029.1.17",
          "form": "메뉴고를때 힘들어요",
          "original_form": "메뉴고를때 힘들어요",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015029.1.18",
          "form": "ㅋㅋㅋㅋㅋ뭔지 알 것 같아요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ뭔지 알 것 같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015029.1.19",
          "form": "저도 식당에서 메뉴고를 때 항상 오래걸려요...",
          "original_form": "저도 식당에서 메뉴고를 때 항상 오래걸려요...",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015029.1.20",
          "form": "친구가 추천한거 따라먹어요",
          "original_form": "친구가 추천한거 따라먹어요",
