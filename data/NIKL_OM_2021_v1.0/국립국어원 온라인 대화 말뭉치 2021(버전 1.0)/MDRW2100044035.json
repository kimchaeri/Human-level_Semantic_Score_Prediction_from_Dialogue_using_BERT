      "utterance": [
        {
          "id": "MDRW2100044035.1.1",
          "form": "저두요!!!!",
          "original_form": "저두요!!!!",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044035.1.2",
          "form": "그리고 볶다가 타서 까매지고그랬거든요ㅜㅜ",
          "original_form": "그리고 볶다가 타서 까매지고그랬거든요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044035.1.3",
          "form": "앗 너무 슬프네여ㅠㅠㅠㅠㅠㅠ",
          "original_form": "앗 너무 슬프네여ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044035.1.4",
          "form": "그리고 볶는 건 뭔가..",
          "original_form": "그리고 볶는 건 뭔가..",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044035.1.5",
          "form": "잘못 볶으면 찌개에서 쓴맛이 나는 것 같아요ㅠㅠ",
          "original_form": "잘못 볶으면 찌개에서 쓴맛이 나는 것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044035.1.6",
          "form": "맞아요ㅜㅜㅜ",
          "original_form": "맞아요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044035.1.7",
          "form": "쓴맛나고 탄맛나고...ㅜㅜ",
          "original_form": "쓴맛나고 탄맛나고...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044035.1.8",
          "form": "참치김치찌개는 별로신가요?",
          "original_form": "참치김치찌개는 별로신가요?",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044035.1.9",
          "form": "헐 너무좋아요!!!!",
          "original_form": "헐 너무좋아요!!!!",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044035.1.10",
          "form": "참치김치찌개도 항상 김치볶고하는데 맛이복불복이라서ㅜㅜ",
          "original_form": "참치김치찌개도 항상 김치볶고하는데 맛이복불복이라서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044035.1.11",
          "form": "돼지고기 김치찌개로 따라해봤어요ㅠㅠ",
          "original_form": "돼지고기 김치찌개로 따라해봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044035.1.12",
          "form": "ㅠㅠㅠㅠㅠ 둘다넣어서는 안 드셔보셨나요?",
          "original_form": "ㅠㅠㅠㅠㅠ 둘다넣어서는 안 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044035.1.13",
          "form": "헐 네",
          "original_form": "헐 네",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044035.1.14",
          "form": "어때요? 더맛있어요?",
          "original_form": "어때요? 더맛있어요?",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044035.1.15",
          "form": "앗 저도",
          "original_form": "앗 저도",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044035.1.16",
          "form": "안 먹어봐서 여쭤본 거였어욬ㅋㅋㅋㅋ",
          "original_form": "안 먹어봐서 여쭤본 거였어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044035.1.17",
          "form": "맛있을것같지않나요ㅠㅠㅠㅠ",
          "original_form": "맛있을것같지않나요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044035.1.18",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044035.1.19",
          "form": "아직 시도는안했지만 진짜 더맛있을거같아요",
          "original_form": "아직 시도는안했지만 진짜 더맛있을거같아요",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044035.1.20",
          "form": "부대찌개도 재료많을수록맛있으니까요!",
          "original_form": "부대찌개도 재료많을수록맛있으니까요!",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044035.1.21",
          "form": "갑자기 부대찌개 먹고싶...ㅋㅋㅋㅋ",
          "original_form": "갑자기 부대찌개 먹고싶...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044035.1.22",
          "form": "후후 저는",
          "original_form": "후후 저는",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044035.1.23",
          "form": "부대찌개의 고장에서 자라났죠",
          "original_form": "부대찌개의 고장에서 자라났죠",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044035.1.24",
          "form": "와....!!!!!",
          "original_form": "와....!!!!!",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044035.1.25",
          "form": "서울쏴람이신가요",
          "original_form": "서울쏴람이신가요",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044035.1.26",
          "form": "에에 아니요!",
          "original_form": "에에 아니요!",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044035.1.27",
          "form": "저 부대찌개 처음만드신 할머니댁 가게가서 먹어봤어요",
          "original_form": "저 부대찌개 처음만드신 할머니댁 가게가서 먹어봤어요",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044035.1.28",
          "form": "앗 서울이아니었나봐요ㅜㅜ",
          "original_form": "앗 서울이아니었나봐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044035.1.29",
          "form": "경기도 남부입니다 캬캬캬",
          "original_form": "경기도 남부입니다 캬캬캬",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044035.1.30",
          "form": "앗!!!!!그럼 제가갔던가게가 경기도인가봐요",
          "original_form": "앗!!!!!그럼 제가갔던가게가 경기도인가봐요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044035.1.31",
          "form": "느에?!",
          "original_form": "느에?!",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044035.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 서울일 수도 있어요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 서울일 수도 있어요!!",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044035.1.33",
          "form": "부대찌개는",
          "original_form": "부대찌개는",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044035.1.34",
          "form": "미군부대 있는 지역이면 다들 자기가 처음이라고 하더라구요",
          "original_form": "미군부대 있는 지역이면 다들 자기가 처음이라고 하더라구요",
          "speaker_id": "1",
          "time": "20210907 21:32"
        }
      ]
