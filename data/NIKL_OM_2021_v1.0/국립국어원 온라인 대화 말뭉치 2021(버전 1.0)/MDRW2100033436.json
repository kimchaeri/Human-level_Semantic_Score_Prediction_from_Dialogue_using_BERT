      "utterance": [
        {
          "id": "MDRW2100033436.1.1",
          "form": "최근에 구매하신 물품 있으세요?",
          "original_form": "최근에 구매하신 물품 있으세요?",
          "speaker_id": "2",
          "time": "20210823 22:10"
        },
        {
          "id": "MDRW2100033436.1.2",
          "form": "저 폰케 샀어요!!",
          "original_form": "저 폰케 샀어요!!",
          "speaker_id": "1",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100033436.1.3",
          "form": "name2님은요?!",
          "original_form": "&name2&님은요?!",
          "speaker_id": "1",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100033436.1.4",
          "form": "전 방금 전에 치킨을 사먹었어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 방금 전에 치킨을 사먹었어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100033436.1.5",
          "form": "근데 맛이 없네요",
          "original_form": "근데 맛이 없네요",
          "speaker_id": "2",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100033436.1.6",
          "form": "폰케이스 뭐 사셨어요?",
          "original_form": "폰케이스 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100033436.1.7",
          "form": "헙 치킨이라니 멋진 소비였다",
          "original_form": "헙 치킨이라니 멋진 소비였다",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100033436.1.8",
          "form": "맛은 멋지지 않았군요",
          "original_form": "맛은 멋지지 않았군요",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100033436.1.9",
          "form": "저 스트랩달린 폰케요",
          "original_form": "저 스트랩달린 폰케요",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100033436.1.10",
          "form": "근데 하 요즘 플립을 너무 사고싶어요",
          "original_form": "근데 하 요즘 플립을 너무 사고싶어요",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100033436.1.11",
          "form": "플립산다면 무슨 색 사실거같나요?!",
          "original_form": "플립산다면 무슨 색 사실거같나요?!",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100033436.1.12",
          "form": "무승 색 사고싶으세요???!",
          "original_form": "무승 색 사고싶으세요???!",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100033436.1.13",
          "form": "저는 크림이랑 라벤더중에 고민할 것 같아요",
          "original_form": "저는 크림이랑 라벤더중에 고민할 것 같아요",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100033436.1.14",
          "form": "근데 크림.",
          "original_form": "근데 크림.",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100033436.1.15",
          "form": "name2님은요?!",
          "original_form": "&name2&님은요?!",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100033436.1.16",
          "form": "ㅋㅋㅋㅋㅋ 저도 크림하고 그린이요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 저도 크림하고 그린이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100033436.1.17",
          "form": "크림 실물도 예쁜지 궁금해요",
          "original_form": "크림 실물도 예쁜지 궁금해요",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100033436.1.18",
          "form": "크림 근데 매장에서 보면 좀 별로예요",
          "original_form": "크림 근데 매장에서 보면 좀 별로예요",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100033436.1.19",
          "form": "근데 친구가 산걸 밖에서 본다? 이쁩니다",
          "original_form": "근데 친구가 산걸 밖에서 본다? 이쁩니다",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100033436.1.20",
          "form": "그린이 고급스럽더라고요",
          "original_form": "그린이 고급스럽더라고요",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100033436.1.21",
          "form": "그린 실물로 보면 예쁠 거 같아요ㅋㅋㅋㅋㅋ",
          "original_form": "그린 실물로 보면 예쁠 거 같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100033436.1.22",
          "form": "맞아요 ㄹㅇ 이뻐욬ㅋㅋㅋ",
          "original_form": "맞아요 ㄹㅇ 이뻐욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100033436.1.23",
          "form": "사고싶은거 있으신가요?",
          "original_form": "사고싶은거 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100033436.1.24",
          "form": "전 요즘 가을옷이요",
          "original_form": "전 요즘 가을옷이요",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100033436.1.25",
          "form": "아무거나 다 사고싶어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아무거나 다 사고싶어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100033436.1.26",
          "form": "헉 가을옷 진짜 이쁘죠",
          "original_form": "헉 가을옷 진짜 이쁘죠",
          "speaker_id": "1",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100033436.1.27",
          "form": "가디건 좋아하세여?!",
          "original_form": "가디건 좋아하세여?!",
          "speaker_id": "1",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100033436.1.28",
          "form": "네 좋죠 가디건 !!!!",
