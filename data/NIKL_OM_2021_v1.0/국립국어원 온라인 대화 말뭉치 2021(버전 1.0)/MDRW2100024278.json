      "utterance": [
        {
          "id": "MDRW2100024278.1.1",
          "form": "우리신랑이",
          "original_form": "우리신랑이",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024278.1.2",
          "form": "브이알게임기를샀는데",
          "original_form": "브이알게임기를샀는데",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024278.1.3",
          "form": "어머머ㅋㅋ너그러우시다ㅋㅋ그런것도사주시네여",
          "original_form": "어머머ㅋㅋ너그러우시다ㅋㅋ그런것도사주시네여",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024278.1.4",
          "form": "사진궁금해용",
          "original_form": "사진궁금해용",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024278.1.5",
          "form": "이건사진안되어서아쉬움",
          "original_form": "이건사진안되어서아쉬움",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024278.1.6",
          "form": "우리는 집안일을 서로 나누어해요",
          "original_form": "우리는 집안일을 서로 나누어해요",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024278.1.7",
          "form": "육아도 그렇고",
          "original_form": "육아도 그렇고",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024278.1.8",
          "form": "저는...주말부부예요",
          "original_form": "저는...주말부부예요",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024278.1.9",
          "form": "제가 밥하면 설거지는 남편이",
          "original_form": "제가 밥하면 설거지는 남편이",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024278.1.10",
          "form": "어머 축복받으셨네요",
          "original_form": "어머 축복받으셨네요",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024278.1.11",
          "form": "그렇지만친정에 살아서 집안일은 해방",
          "original_form": "그렇지만친정에 살아서 집안일은 해방",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024278.1.12",
          "form": "친정이 최고죠",
          "original_form": "친정이 최고죠",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024278.1.13",
          "form": "남편오는 주말이 제일 싫어요ㅎㅎ",
          "original_form": "남편오는 주말이 제일 싫어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024278.1.14",
          "form": "가까우면가까울수록",
          "original_form": "가까우면가까울수록",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024278.1.15",
          "form": "친정은사랑이죠",
          "original_form": "친정은사랑이죠",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024278.1.16",
          "form": "남편이 집안일 육아를 함께해야되는데",
          "original_form": "남편이 집안일 육아를 함께해야되는데",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024278.1.17",
          "form": "그래도 불편한것도 많아요ㅎㅎㅎ",
          "original_form": "그래도 불편한것도 많아요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024278.1.18",
          "form": "많이아쉬워하시겠어요",
          "original_form": "많이아쉬워하시겠어요",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024278.1.19",
          "form": "맞아요특히 육아..",
          "original_form": "맞아요특히 육아..",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024278.1.20",
          "form": "좋아해여ㅋㅋㅋ아무것도 안해서^^",
          "original_form": "좋아해여ㅋㅋㅋ아무것도 안해서^^",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024278.1.21",
          "form": "ㅋㅋㅋㅋㅋ앞으로 잘하실듯",
          "original_form": "ㅋㅋㅋㅋㅋ앞으로 잘하실듯",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100024278.1.22",
          "form": "집사주면ㅋㅋ",
          "original_form": "집사주면ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100024278.1.23",
          "form": "휴...남동생고민입니다",
          "original_form": "휴...남동생고민입니다",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100024278.1.24",
          "form": "백수거든요ㅠ",
          "original_form": "백수거든요ㅠ",
          "speaker_id": "1",
