      "utterance": [
        {
          "id": "MDRW2100020655.1.1",
          "form": "진짜 강의 느낌이에요",
          "original_form": "진짜 강의 느낌이에요",
          "speaker_id": "2",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020655.1.2",
          "form": "좀 있으면 점심시간이네요!",
          "original_form": "좀 있으면 점심시간이네요!",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020655.1.3",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020655.1.4",
          "form": "점심 뭐 드실지 정하셨나요?",
          "original_form": "점심 뭐 드실지 정하셨나요?",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020655.1.5",
          "form": "저 어제 먹고 남은 치킨이요 ㅋㅋㅋㅋ",
          "original_form": "저 어제 먹고 남은 치킨이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020655.1.6",
          "form": "맛있겠죠?",
          "original_form": "맛있겠죠?",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020655.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020655.1.8",
          "form": "불금에 치킨 먹으셨군요",
          "original_form": "불금에 치킨 먹으셨군요",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020655.1.9",
          "form": "치킨은 남은치킨도 맛있죠",
          "original_form": "치킨은 남은치킨도 맛있죠",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020655.1.10",
          "form": "전 아직 고민중입니다",
          "original_form": "전 아직 고민중입니다",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020655.1.11",
          "form": "생각해둔 건 있어요?",
          "original_form": "생각해둔 건 있어요?",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020655.1.12",
          "form": "그냥 식빵에다가 잼 발라 먹을까했는데",
          "original_form": "그냥 식빵에다가 잼 발라 먹을까했는데",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020655.1.13",
          "form": "그것도 맛있겠는데요?",
          "original_form": "그것도 맛있겠는데요?",
          "speaker_id": "1",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020655.1.14",
          "form": "무슨잼 있어요?",
          "original_form": "무슨잼 있어요?",
          "speaker_id": "1",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020655.1.15",
          "form": "달걀이라도 구울까 생각중이에요",
          "original_form": "달걀이라도 구울까 생각중이에요",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020655.1.16",
          "form": "사과잼이랑 딸기잼이요",
          "original_form": "사과잼이랑 딸기잼이요",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020655.1.17",
          "form": "사과잼 맛있더라구요",
          "original_form": "사과잼 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020655.1.18",
          "form": "오 사과잼이라는 것도 있어요?",
          "original_form": "오 사과잼이라는 것도 있어요?",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020655.1.19",
          "form": "전 딸기잼이랑 땅콩잼 밖에 안먹어봤어요 ㅎㅎ",
          "original_form": "전 딸기잼이랑 땅콩잼 밖에 안먹어봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020655.1.20",
          "form": "땅콩잼도 맛있죠",
          "original_form": "땅콩잼도 맛있죠",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020655.1.21",
          "form": "딸기잼 질려서 골라봤는데 꽤 괜찮더라구요",
          "original_form": "딸기잼 질려서 골라봤는데 꽤 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020655.1.22",
          "form": "상큼하니 너무 찐득거리지않아서 좋아요",
          "original_form": "상큼하니 너무 찐득거리지않아서 좋아요",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020655.1.23",
          "form": "나중에 사과잼 사먹어봐야겠어요 ㅎㅎ",
          "original_form": "나중에 사과잼 사먹어봐야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020655.1.24",
          "form": "식빵은 무슨 식빵이에요?",
          "original_form": "식빵은 무슨 식빵이에요?",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020655.1.25",
          "form": "그냥 우유식빵이요",
          "original_form": "그냥 우유식빵이요",
          "speaker_id": "2",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020655.1.26",
          "form": "식빵이 냉동실에 얼려놨다 먹으면 편하더라구요",
          "original_form": "식빵이 냉동실에 얼려놨다 먹으면 편하더라구요",
          "speaker_id": "2",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020655.1.27",
          "form": "뭔가 숨겨둔 양식 하나씩 꺼내먹는 다람쥐가 된 느낌이에요",
          "original_form": "뭔가 숨겨둔 양식 하나씩 꺼내먹는 다람쥐가 된 느낌이에요",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020655.1.28",
          "form": "식빵 얼려놓으면 딱딱해지던데 ㅜㅜ",
          "original_form": "식빵 얼려놓으면 딱딱해지던데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 11:21"
        }
      ]
