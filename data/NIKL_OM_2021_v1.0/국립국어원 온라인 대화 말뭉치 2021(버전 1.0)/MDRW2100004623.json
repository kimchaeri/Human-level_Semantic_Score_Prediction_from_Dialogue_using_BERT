      "utterance": [
        {
          "id": "MDRW2100004623.1.1",
          "form": "바비 애 아빠 되는거 봤어? ㅋㅋ",
          "original_form": "바비 애 아빠 되는거 봤어? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004623.1.2",
          "form": "으응?",
          "original_form": "으응?",
          "speaker_id": "2",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004623.1.3",
          "form": "그 아이콘 바비?",
          "original_form": "그 아이콘 바비?",
          "speaker_id": "2",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004623.1.4",
          "form": "응 아직 몰랐나 무려 담달 애아빠 됨 ㅋㅋ",
          "original_form": "응 아직 몰랐나 무려 담달 애아빠 됨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004623.1.5",
          "form": "헐...담달...충격이네ㅋㅋㅋ",
          "original_form": "헐...담달...충격이네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004623.1.6",
          "form": "그것도 충격이었는데 자필로 썼는데 글씨체 때문에 넘 놀람 ㅋㅋㅋㅋ",
          "original_form": "그것도 충격이었는데 자필로 썼는데 글씨체 때문에 넘 놀람 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004623.1.7",
          "form": "글씨체가 어떤데?ㅋㅋㅋ",
          "original_form": "글씨체가 어떤데?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004623.1.8",
          "form": "읽다가 못 알아본 정도? ㅋㅋㅋ",
          "original_form": "읽다가 못 알아본 정도? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004623.1.9",
          "form": "ㅋㅋㅋㅋ 근데 아무리 충격적이어도 김용건 기사보다는ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 근데 아무리 충격적이어도 김용건 기사보다는ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004623.1.10",
          "form": "바비랑 김용건이랑 같이 키카가래",
          "original_form": "바비랑 김용건이랑 같이 키카가래",
          "speaker_id": "1",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004623.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ 너무 웃기다ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 너무 웃기다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004623.1.12",
          "form": "아니 현역 아이돌이 그것도 출산 임박해서 .. 팬들 많이 놀랐을듯",
          "original_form": "아니 현역 아이돌이 그것도 출산 임박해서 .. 팬들 많이 놀랐을듯",
          "speaker_id": "1",
          "time": "20210821 17:42"
        },
        {
          "id": "MDRW2100004623.1.13",
          "form": "9개월동안 어케 숨겼냐ㅋㅋ",
          "original_form": "9개월동안 어케 숨겼냐ㅋㅋ",
