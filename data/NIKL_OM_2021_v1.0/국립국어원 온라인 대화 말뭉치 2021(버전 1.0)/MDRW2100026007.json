      "utterance": [
        {
          "id": "MDRW2100026007.1.1",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100026007.1.2",
          "form": "저녁 뭐드셨어요?",
          "original_form": "저녁 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100026007.1.3",
          "form": "아 벌써 저녁때네요",
          "original_form": "아 벌써 저녁때네요",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100026007.1.4",
          "form": "전 스터디카페에서 공부하다 쉬는중이라",
          "original_form": "전 스터디카페에서 공부하다 쉬는중이라",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100026007.1.5",
          "form": "아직 뭘 못 먹었어요",
          "original_form": "아직 뭘 못 먹었어요",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100026007.1.6",
          "form": "집에 가면 빵 먹으려고요ㅎㅎ",
          "original_form": "집에 가면 빵 먹으려고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100026007.1.7",
          "form": "저녁식사 하셨어요?",
          "original_form": "저녁식사 하셨어요?",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100026007.1.8",
          "form": "저희는 저녁 시간도 늦어서 8시나 9시쯤 돼야 식사하네요ㅎㅎㅎ",
          "original_form": "저희는 저녁 시간도 늦어서 8시나 9시쯤 돼야 식사하네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100026007.1.9",
          "form": "아 스터디하시나보네요",
          "original_form": "아 스터디하시나보네요",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100026007.1.10",
          "form": "아구 빵ㅜㅜㅋㅋ",
          "original_form": "아구 빵ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100026007.1.11",
          "form": "늦게 저녁드시네요.",
          "original_form": "늦게 저녁드시네요.",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100026007.1.12",
          "form": "저는 애들이랑 김밥먹었어요.ㅋㅋ",
          "original_form": "저는 애들이랑 김밥먹었어요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100026007.1.13",
          "form": "김밥 맛있었겠네여",
          "original_form": "김밥 맛있었겠네여",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.14",
          "form": "무슨 김밥 드셨어요?",
          "original_form": "무슨 김밥 드셨어요?",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.15",
          "form": "집에서",
          "original_form": "집에서",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.16",
          "form": "재료로 간단하게",
          "original_form": "재료로 간단하게",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.17",
          "form": "전 바른김밥에 있는 크림치즈호두김밥을 좋아해요ㅋㅋㅋ",
          "original_form": "전 바른김밥에 있는 크림치즈호두김밥을 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.18",
          "form": "아하 그런것도 좋죠",
          "original_form": "아하 그런것도 좋죠",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.19",
          "form": "진미넣고 진미김밥이요ㅋㅋ",
          "original_form": "진미넣고 진미김밥이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.20",
          "form": "앗 진미 좋아하는데",
          "original_form": "앗 진미 좋아하는데",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.21",
          "form": "맛있었겠어요",
          "original_form": "맛있었겠어요",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.22",
          "form": "크림치즈호두김밥 ㅋㅋ",
          "original_form": "크림치즈호두김밥 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.23",
          "form": "아이들도 잘 먹었겠어요",
          "original_form": "아이들도 잘 먹었겠어요",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.24",
          "form": "처음들어보는데 맛있겟어요",
          "original_form": "처음들어보는데 맛있겟어요",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026007.1.25",
          "form": "우와ㅋㅋㅋㅋ",
          "original_form": "우와ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026007.1.26",
          "form": "치즈 싫어하시는 거 아니면",
          "original_form": "치즈 싫어하시는 거 아니면",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026007.1.27",
          "form": "좋아하실 맛이예요",
          "original_form": "좋아하실 맛이예요",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026007.1.28",
          "form": "느끼하지도 않고ㅎㅎ",
          "original_form": "느끼하지도 않고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026007.1.29",
          "form": "저녁메뉴는",
          "original_form": "저녁메뉴는",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026007.1.30",
          "form": "늘 고민인 것 같아요",
          "original_form": "늘 고민인 것 같아요",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026007.1.31",
          "form": "돌아서면 끼니때니까",
          "original_form": "돌아서면 끼니때니까",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026007.1.32",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026007.1.33",
          "form": "밥 챙기기도 쉽지 않아요",
          "original_form": "밥 챙기기도 쉽지 않아요",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026007.1.34",
          "form": "늘 고민이에요ㅋㅋ",
          "original_form": "늘 고민이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026007.1.35",
          "form": "메뉴 고민하기도 지치고ㅎㅎ",
          "original_form": "메뉴 고민하기도 지치고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:48"
        }
      ]
