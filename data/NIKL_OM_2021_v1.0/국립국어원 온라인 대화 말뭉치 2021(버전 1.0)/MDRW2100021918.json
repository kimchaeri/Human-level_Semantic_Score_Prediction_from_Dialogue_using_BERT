      "utterance": [
        {
          "id": "MDRW2100021918.1.1",
          "form": "데이트는 일주일에 하루밖에 안해요",
          "original_form": "데이트는 일주일에 하루밖에 안해요",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021918.1.2",
          "form": "평일에 하기가 힘들어요ㅜㅜ",
          "original_form": "평일에 하기가 힘들어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021918.1.3",
          "form": "평일에 할거까지 주말 하루에 다한다구 보시면 될듯",
          "original_form": "평일에 할거까지 주말 하루에 다한다구 보시면 될듯",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021918.1.4",
          "form": "그렇게 몰아하듯이 하면\r\n것도 에너지 소모가\r\n어마무시할거 같아요",
          "original_form": "그렇게 몰아하듯이 하면\r\n것도 에너지 소모가\r\n어마무시할거 같아요",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021918.1.5",
          "form": "네 어마무시한데",
          "original_form": "네 어마무시한데",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021918.1.6",
          "form": "그래도 제대로해야하니....",
          "original_form": "그래도 제대로해야하니....",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021918.1.7",
          "form": "ㅋㅋ그래서 오늘은\r\n제대로 무얼 하셨나요",
          "original_form": "ㅋㅋ그래서 오늘은\r\n제대로 무얼 하셨나요",
          "speaker_id": "1",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021918.1.8",
          "form": "궁금하세요?ㅋㅋㅋ",
          "original_form": "궁금하세요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021918.1.9",
          "form": "우선 점심에 만나서 밥을 먹었구요",
          "original_form": "우선 점심에 만나서 밥을 먹었구요",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021918.1.10",
          "form": "그리고 여친이 옷 사는걸 옆에서 지켜보다가",
          "original_form": "그리고 여친이 옷 사는걸 옆에서 지켜보다가",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021918.1.11",
          "form": "커피마시며 좀 쉬다가",
          "original_form": "커피마시며 좀 쉬다가",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021918.1.12",
          "form": "교외드라이브좀 하구",
          "original_form": "교외드라이브좀 하구",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021918.1.13",
          "form": "좀 걷다가",
          "original_form": "좀 걷다가",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021918.1.14",
          "form": "저녁에 고기를 먹었어요",
          "original_form": "저녁에 고기를 먹었어요",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021918.1.15",
          "form": "특별한 데이트는 아닌데",
          "original_form": "특별한 데이트는 아닌데",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021918.1.16",
          "form": "부럽네요 ㅋㅋㅋㅋㅋ",
          "original_form": "부럽네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021918.1.17",
          "form": "ㅋㅋㅋㅋ 왜요",
          "original_form": "ㅋㅋㅋㅋ 왜요",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021918.1.18",
          "form": "부러울건 딱히 없어보이는데",
          "original_form": "부러울건 딱히 없어보이는데",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021918.1.19",
          "form": "데이트 자체가 부러운거죵ㅋㅋ",
          "original_form": "데이트 자체가 부러운거죵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021918.1.20",
          "form": "아이가 생기면 불가능인가요?",
          "original_form": "아이가 생기면 불가능인가요?",
          "speaker_id": "2",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021918.1.21",
          "form": "아이 잠시 맡기고 오붓하게 데이트하면 안되나여?",
          "original_form": "아이 잠시 맡기고 오붓하게 데이트하면 안되나여?",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021918.1.22",
          "form": "손잡고 걸어본지가 언제인지ㅋㅋ\r\n기억도 안나네여 ㅋㅋ",
          "original_form": "손잡고 걸어본지가 언제인지ㅋㅋ\r\n기억도 안나네여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021918.1.23",
          "form": "다시 예전처럼은 아니더라도....",
          "original_form": "다시 예전처럼은 아니더라도....",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021918.1.24",
          "form": "그때 기분을 느껴보시면 좋을텐데",
          "original_form": "그때 기분을 느껴보시면 좋을텐데",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021918.1.25",
          "form": "결혼해서도\r\n둘만의 시간을\r\n갖는게 중요한 거 같아요:)",
          "original_form": "결혼해서도\r\n둘만의 시간을\r\n갖는게 중요한 거 같아요:)",
          "speaker_id": "1",
          "time": "20210829 08:50"
        },
        {
          "id": "MDRW2100021918.1.26",
          "form": "그러니까 얼른 가지세요",
          "original_form": "그러니까 얼른 가지세요",
          "speaker_id": "2",
          "time": "20210829 10:26"
        },
        {
          "id": "MDRW2100021918.1.27",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:26"
        },
        {
          "id": "MDRW2100021918.1.28",
          "form": "주말이 제일 힘드시죠?",
          "original_form": "주말이 제일 힘드시죠?",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100021918.1.29",
          "form": "밥차려줘야하고",
          "original_form": "밥차려줘야하고",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100021918.1.30",
          "form": "맞아요:)\r\n삼시세.끼가 제일 싫어요ㅋㅋ",
          "original_form": "맞아요:)\r\n삼시세.끼가 제일 싫어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:16"
        }
      ]
