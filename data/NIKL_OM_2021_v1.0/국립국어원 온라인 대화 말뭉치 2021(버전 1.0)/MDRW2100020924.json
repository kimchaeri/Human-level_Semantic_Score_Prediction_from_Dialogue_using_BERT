      "utterance": [
        {
          "id": "MDRW2100020924.1.1",
          "form": "다이어트하세요?",
          "original_form": "다이어트하세요?",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020924.1.2",
          "form": "네 지금 하는 중이에요",
          "original_form": "네 지금 하는 중이에요",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020924.1.3",
          "form": "이미 성공했지만 꾸준히 하려구요",
          "original_form": "이미 성공했지만 꾸준히 하려구요",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020924.1.4",
          "form": "와 성공하신분 첨봐요",
          "original_form": "와 성공하신분 첨봐요",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020924.1.5",
          "form": "얼마나 빼셨어요?",
          "original_form": "얼마나 빼셨어요?",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020924.1.6",
          "form": "원래 거의 70kg까지 쪘다가 지금은 47.5 정도요 ㅎㅎ",
          "original_form": "원래 거의 70kg까지 쪘다가 지금은 47.5 정도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020924.1.7",
          "form": "와 진짜 대박 성공하셨네요",
          "original_form": "와 진짜 대박 성공하셨네요",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020924.1.8",
          "form": "의지가 멋있어요",
          "original_form": "의지가 멋있어요",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020924.1.9",
          "form": "체지방률도 엄청 많았는데 지금 1/3으로 줄었어요",
          "original_form": "체지방률도 엄청 많았는데 지금 1/3으로 줄었어요",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020924.1.10",
          "form": "저도 지방 줄이고싶은데",
          "original_form": "저도 지방 줄이고싶은데",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020924.1.11",
          "form": "맨날 입다이어터예요",
          "original_form": "맨날 입다이어터예요",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020924.1.12",
          "form": "무슨 운동하셨어요?",
          "original_form": "무슨 운동하셨어요?",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020924.1.13",
          "form": "체지방 줄이려면 유산소 운동만 하셔도 돼요 ㅎㅎ",
          "original_form": "체지방 줄이려면 유산소 운동만 하셔도 돼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020924.1.14",
          "form": "유산소 운동은 큰 힘 안들이고 할 수 있어요",
          "original_form": "유산소 운동은 큰 힘 안들이고 할 수 있어요",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020924.1.15",
          "form": "추천해주실만한 유산소운동 있나요?",
          "original_form": "추천해주실만한 유산소운동 있나요?",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020924.1.16",
          "form": "걷기, 달리기, 자전거 타기 이거 3가지요",
          "original_form": "걷기, 달리기, 자전거 타기 이거 3가지요",
          "speaker_id": "2",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020924.1.17",
          "form": "단기간에 효과 보고 싶으시면 인터벌 트레이닝 하세요!",
          "original_form": "단기간에 효과 보고 싶으시면 인터벌 트레이닝 하세요!",
          "speaker_id": "2",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020924.1.18",
          "form": "오 찾아봐야겠어요",
          "original_form": "오 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020924.1.19",
          "form": "ㅋㅋㅋ근데 진짜 꾸준히 하는게 중요한 것 같아요...",
          "original_form": "ㅋㅋㅋ근데 진짜 꾸준히 하는게 중요한 것 같아요...",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020924.1.20",
          "form": "이제 날씨도 풀렸으니 저도 다시 꾸준히 해볼까해요",
          "original_form": "이제 날씨도 풀렸으니 저도 다시 꾸준히 해볼까해요",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020924.1.21",
          "form": "아 진짜 꾸준히가 너무 어렵죠",
          "original_form": "아 진짜 꾸준히가 너무 어렵죠",
          "speaker_id": "1",
          "time": "20210828 14:30"
        }
      ]
