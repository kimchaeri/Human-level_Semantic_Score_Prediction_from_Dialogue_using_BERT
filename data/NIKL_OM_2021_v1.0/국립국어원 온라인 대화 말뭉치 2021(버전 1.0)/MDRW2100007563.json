      "utterance": [
        {
          "id": "MDRW2100007563.1.1",
          "form": "언제 결혼하고싶어요?",
          "original_form": "언제 결혼하고싶어요?",
          "speaker_id": "1",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007563.1.2",
          "form": "전 음...결혼 안해도 되고",
          "original_form": "전 음...결혼 안해도 되고",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007563.1.3",
          "form": "일찍은 안하고 싶어요",
          "original_form": "일찍은 안하고 싶어요",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007563.1.4",
          "form": "언니랑똑같네요",
          "original_form": "언니랑똑같네요",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007563.1.5",
          "form": "곧 30중반이지만 혼자 다할수있는데 왜 하내고ㅋㅋ",
          "original_form": "곧 30중반이지만 혼자 다할수있는데 왜 하내고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007563.1.6",
          "form": "혼자 여행갈거 다 가고 싱글라이프 신나게 즐겨요",
          "original_form": "혼자 여행갈거 다 가고 싱글라이프 신나게 즐겨요",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007563.1.7",
          "form": "저도 그렇게 했어야됬나 싶은 부러움도있고",
          "original_form": "저도 그렇게 했어야됬나 싶은 부러움도있고",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007563.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그것도 삶을 즐기는 방식 중 하나인 것같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그것도 삶을 즐기는 방식 중 하나인 것같아요",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007563.1.9",
          "form": "부모님도 결혼 안하고 싶으면 안해도 된다고 하셨어용",
          "original_form": "부모님도 결혼 안하고 싶으면 안해도 된다고 하셨어용",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007563.1.10",
          "form": "멋있어요 부모님이 젊으셔서 마인드가 신세대예요",
          "original_form": "멋있어요 부모님이 젊으셔서 마인드가 신세대예요",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007563.1.11",
          "form": "언니도결혼생각없데요?",
          "original_form": "언니도결혼생각없데요?",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007563.1.12",
          "form": "언니는 아직 아무 생각 없는 것같아요ㅋㅋㅋㅋ",
          "original_form": "언니는 아직 아무 생각 없는 것같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007563.1.13",
          "form": "친언니라 그런지 결혼 못할 것...ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "친언니라 그런지 결혼 못할 것...ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007563.1.14",
          "form": "그날되면 눈물날걸요",
          "original_form": "그날되면 눈물날걸요",
          "speaker_id": "1",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007563.1.15",
          "form": "저도 안날거같아요ㅋㅋ",
          "original_form": "저도 안날거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007563.1.16",
          "form": "왠지 신날것같은",
          "original_form": "왠지 신날것같은",
          "speaker_id": "1",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007563.1.17",
          "form": "ㅋㅋㅋㅋㅋ왠지 저두 신날 것같은",
          "original_form": "ㅋㅋㅋㅋㅋ왠지 저두 신날 것같은",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007563.1.18",
          "form": "그래도 기분은 이상할 것같아요",
          "original_form": "그래도 기분은 이상할 것같아요",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007563.1.19",
          "form": "남자친구있어요?",
          "original_form": "남자친구있어요?",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007563.1.20",
          "form": "제일예쁠때만나야되는데",
          "original_form": "제일예쁠때만나야되는데",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007563.1.21",
          "form": "언니요?",
          "original_form": "언니요?",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007563.1.22",
          "form": "안냥님이요~~",
          "original_form": "안냥님이요~~",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007563.1.23",
          "form": "언니는...없을걸요 아마도",
          "original_form": "언니는...없을걸요 아마도",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007563.1.24",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007563.1.25",
          "form": "저도 없어요 또르륵",
          "original_form": "저도 없어요 또르륵",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007563.1.26",
          "form": "결혼은 자상한사람\r\n연애는 잘생긴사람",
          "original_form": "결혼은 자상한사람\r\n연애는 잘생긴사람",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007563.1.27",
          "form": "친구들이 다 그래요ㅋㅋ",
          "original_form": "친구들이 다 그래요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007563.1.28",
          "form": "결혼한사람만이 아는ㅋㅋ",
          "original_form": "결혼한사람만이 아는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007563.1.29",
          "form": "결혼도 잘생긴 사람이 좋은거 아니에요??",
          "original_form": "결혼도 잘생긴 사람이 좋은거 아니에요??",
          "speaker_id": "2",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007563.1.30",
          "form": "얼굴믿고 깝치는애들많은데",
          "original_form": "얼굴믿고 깝치는애들많은데",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007563.1.31",
          "form": "결혼은 보통 30년이상 같이살거라",
          "original_form": "결혼은 보통 30년이상 같이살거라",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007563.1.32",
          "form": "나랑 우리애기한테 잘해줄남자\r\n내친정까지도 잘해주면좋고",
          "original_form": "나랑 우리애기한테 잘해줄남자\r\n내친정까지도 잘해주면좋고",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007563.1.33",
          "form": "이런거죠~~",
          "original_form": "이런거죠~~",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007563.1.34",
          "form": "아하 그렇구나",
          "original_form": "아하 그렇구나",
          "speaker_id": "2",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007563.1.35",
          "form": "제 이상형도 자상한 남자긴해요ㅋㅋㅋ",
          "original_form": "제 이상형도 자상한 남자긴해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007563.1.36",
          "form": "연애때는 사겨보고 아닌거같으면 바로버리멘되니까",
          "original_form": "연애때는 사겨보고 아닌거같으면 바로버리멘되니까",
