      "utterance": [
        {
          "id": "MDRW2100013895.1.1",
          "form": "비오는 날씨 좋아하시나요?",
          "original_form": "비오는 날씨 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013895.1.2",
          "form": "네 빗소리 좋아요",
          "original_form": "네 빗소리 좋아요",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013895.1.3",
          "form": "아 정말요? 저도 빗소린좋은데 비오는날 걷는건 싫어해요",
          "original_form": "아 정말요? 저도 빗소린좋은데 비오는날 걷는건 싫어해요",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013895.1.4",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013895.1.5",
          "form": "그럼 무슨 날씨 좋아해요?",
          "original_form": "그럼 무슨 날씨 좋아해요?",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013895.1.6",
          "form": "저는 선선한 날씨를 좋아해요 ㅎㅎㅎ",
          "original_form": "저는 선선한 날씨를 좋아해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013895.1.7",
          "form": "가을이네요",
          "original_form": "가을이네요",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013895.1.8",
          "form": "좋으시겠다",
          "original_form": "좋으시겠다",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013895.1.9",
          "form": "ㅎㅎㅎ장마기간 끝나면 딱 시원할것같아요",
          "original_form": "ㅎㅎㅎ장마기간 끝나면 딱 시원할것같아요",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013895.1.10",
          "form": "지금만돼도",
          "original_form": "지금만돼도",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013895.1.11",
          "form": "살겠어요",
          "original_form": "살겠어요",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013895.1.12",
          "form": "너무 더웠어요",
          "original_form": "너무 더웠어요",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013895.1.13",
          "form": "맞아요 ㅎㅎㅎ에어컨 엄청 튼거같아요 이번에",
          "original_form": "맞아요 ㅎㅎㅎ에어컨 엄청 튼거같아요 이번에",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013895.1.14",
          "form": "전기세 무섭네요",
          "original_form": "전기세 무섭네요",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013895.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 ㅋㅋㅋㅋㅋ그래도 여름이 거의 끝나서좋아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 ㅋㅋㅋㅋㅋ그래도 여름이 거의 끝나서좋아요",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013895.1.16",
          "form": "가을에는 여행가고 싶어요",
          "original_form": "가을에는 여행가고 싶어요",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013895.1.17",
          "form": "저두요 ㅠㅠ 미세먼지없고 선선한 날씨엔 소풍이라도 가야죠",
          "original_form": "저두요 ㅠㅠ 미세먼지없고 선선한 날씨엔 소풍이라도 가야죠",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013895.1.18",
          "form": "소풍 좋죠 ㅠ 제발",
          "original_form": "소풍 좋죠 ㅠ 제발",
          "speaker_id": "2",
          "time": "20210825 16:26"
        }
