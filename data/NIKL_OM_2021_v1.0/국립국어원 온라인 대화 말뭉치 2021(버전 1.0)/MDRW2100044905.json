        {
          "id": "MDRW2100044905.1.1",
          "form": "힐링제대로 일 것 같네요ㅠㅠㅠ",
          "original_form": "힐링제대로 일 것 같네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044905.1.2",
          "form": "그러게요!",
          "original_form": "그러게요!",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044905.1.3",
          "form": "name1님은 어디가보고 싶으세요?",
          "original_form": "&name1&님은 어디가보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044905.1.4",
          "form": "저는 그 그리스??가보고싶어요",
          "original_form": "저는 그 그리스??가보고싶어요",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044905.1.5",
          "form": "그 막 하얀색 집에 파란색 지붕인 거기가 그리스인지 모르겠는데 거기요!!",
          "original_form": "그 막 하얀색 집에 파란색 지붕인 거기가 그리스인지 모르겠는데 거기요!!",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044905.1.6",
          "form": "아 거기 산토리니 아닌가요?",
          "original_form": "아 거기 산토리니 아닌가요?",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044905.1.7",
          "form": "저도 거기 꼭한번 가보고싶어요",
          "original_form": "저도 거기 꼭한번 가보고싶어요",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044905.1.8",
          "form": "음식도 맛있을 것 같고ㅜ",
          "original_form": "음식도 맛있을 것 같고ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044905.1.9",
          "form": "오 맞아요 산토리니!",
          "original_form": "오 맞아요 산토리니!",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044905.1.10",
          "form": "그니꺼요ㅠㅠ 너무 먹어보고싶어요",
          "original_form": "그니꺼요ㅠㅠ 너무 먹어보고싶어요",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044905.1.11",
          "form": "보고만있어도 좋을것같아요",
          "original_form": "보고만있어도 좋을것같아요",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044905.1.12",
          "form": "맞아요 나중에 시간많으면 꼭 가보고 싶은 곳이에요",
          "original_form": "맞아요 나중에 시간많으면 꼭 가보고 싶은 곳이에요",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044905.1.13",
          "form": "갔던 곳 중에 좋았던 곳은 있나요???",
          "original_form": "갔던 곳 중에 좋았던 곳은 있나요???",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044905.1.14",
          "form": "저는 아무래도 제주도요! 엄청 자연도 좋고 먹을것도 많고!!",
          "original_form": "저는 아무래도 제주도요! 엄청 자연도 좋고 먹을것도 많고!!",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044905.1.15",
          "form": "제주도 너무좋죠~~",
          "original_form": "제주도 너무좋죠~~",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100044905.1.16",
          "form": "어릴 때 제주도 가서 말탔는데 또 타고싶어요",
          "original_form": "어릴 때 제주도 가서 말탔는데 또 타고싶어요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100044905.1.17",
          "form": "오오 말은 못타본것같아요 대박대박",
          "original_form": "오오 말은 못타본것같아요 대박대박",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100044905.1.18",
          "form": "나중에 제주도 가면 타보세요 ㅎㅎ",
          "original_form": "나중에 제주도 가면 타보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100044905.1.19",
          "form": "무서운데 나름 재밌어요",
          "original_form": "무서운데 나름 재밌어요",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100044905.1.20",
          "form": "오오 꼭 타볼게요ㅋㅋㅋ 조오오금 무섭지만!!",
