      "utterance": [
        {
          "id": "MDRW2100021418.1.1",
          "form": "쇼핑 자주 하세요?",
          "original_form": "쇼핑 자주 하세요?",
          "speaker_id": "2",
          "time": "20210828 18:45"
        },
        {
          "id": "MDRW2100021418.1.2",
          "form": "저는 인터넷 쇼핑을 즐겨요 ㅎㅎ 마느리님은요?",
          "original_form": "저는 인터넷 쇼핑을 즐겨요 ㅎㅎ 마느리님은요?",
          "speaker_id": "1",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100021418.1.3",
          "form": "저두여! 요즘은 앱에서만 할인되고 하는 것두 많아서요",
          "original_form": "저두여! 요즘은 앱에서만 할인되고 하는 것두 많아서요",
          "speaker_id": "2",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100021418.1.4",
          "form": "나가긴 귀찮고 코로나 걱정도 되구...",
          "original_form": "나가긴 귀찮고 코로나 걱정도 되구...",
          "speaker_id": "2",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100021418.1.5",
          "form": "최근엔 뭐 사셨어요?",
          "original_form": "최근엔 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100021418.1.6",
          "form": "할인하는 제품 사는거 너무 재밌어요ㅋㅋ",
          "original_form": "할인하는 제품 사는거 너무 재밌어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100021418.1.7",
          "form": "최근엔 옷을 많이샀네요ㅜㅜ",
          "original_form": "최근엔 옷을 많이샀네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100021418.1.8",
          "form": "ㅋㅋㅋ 저는 나갈일이 적어져서 올해는 그러고보니 옷을 안 샀네요",
          "original_form": "ㅋㅋㅋ 저는 나갈일이 적어져서 올해는 그러고보니 옷을 안 샀네요",
          "speaker_id": "2",
          "time": "20210828 18:52"
        },
        {
          "id": "MDRW2100021418.1.9",
          "form": "저는 먹을거... 요즘 많이 산 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "저는 먹을거... 요즘 많이 산 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:53"
        },
        {
          "id": "MDRW2100021418.1.10",
          "form": "할인딜 너무 짜릿",
          "original_form": "할인딜 너무 짜릿",
          "speaker_id": "2",
          "time": "20210828 18:53"
        },
        {
          "id": "MDRW2100021418.1.11",
          "form": "마자요 이렇게까지 할인해주면 남는게있나 싶을정도에요ㅋㅋㅋㅋ",
          "original_form": "마자요 이렇게까지 할인해주면 남는게있나 싶을정도에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021418.1.12",
          "form": "그쵸그쵸 요즘은 또 무슨 라이브 방송중 할인 그런것도 하더라구요 홈쇼핑처럼",
          "original_form": "그쵸그쵸 요즘은 또 무슨 라이브 방송중 할인 그런것도 하더라구요 홈쇼핑처럼",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021418.1.13",
          "form": "중고거래두 하세요? 당근!",
          "original_form": "중고거래두 하세요? 당근!",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021418.1.14",
          "form": "네네 많이는 아니규 몇번 해봤어요!!",
          "original_form": "네네 많이는 아니규 몇번 해봤어요!!",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021418.1.15",
          "form": "그것두 은근 일이더라구요",
          "original_form": "그것두 은근 일이더라구요",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021418.1.16",
          "form": "ㅋㅋ 저는 물건이 안 팔려서 몇번 시도만 해보다가 지웠어요ㅠ",
          "original_form": "ㅋㅋ 저는 물건이 안 팔려서 몇번 시도만 해보다가 지웠어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021418.1.17",
          "form": "중고거래도 기술이 필요하더라구요",
          "original_form": "중고거래도 기술이 필요하더라구요",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021418.1.18",
          "form": "사진찍어올리고 하는게 생각보다 귀찮아서 저도 지금은 안하고있어요ㅜㅜ",
          "original_form": "사진찍어올리고 하는게 생각보다 귀찮아서 저도 지금은 안하고있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021418.1.19",
          "form": "그쵸그쵸 ㅋㅋ 상품 설명이나 사진찍는게 은근 귀찮더라구요",
          "original_form": "그쵸그쵸 ㅋㅋ 상품 설명이나 사진찍는게 은근 귀찮더라구요",
          "speaker_id": "2",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021418.1.20",
          "form": "가격 네고도 엄청 귀찮구ㅜㅜ",
          "original_form": "가격 네고도 엄청 귀찮구ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021418.1.21",
          "form": "당근마켓은 일단 무조건 깎아달라하고 시작하더라구요ㅠㅠ",
          "original_form": "당근마켓은 일단 무조건 깎아달라하고 시작하더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021418.1.22",
          "form": "맞아요 가격 완전 후려쳐야 돼서 힘들더라구요",
          "original_form": "맞아요 가격 완전 후려쳐야 돼서 힘들더라구요",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021418.1.23",
          "form": "그냥 정가주고 새 물건 사는게 속편하구...",
          "original_form": "그냥 정가주고 새 물건 사는게 속편하구...",
          "speaker_id": "2",
          "time": "20210828 19:53"
        }
      ]
