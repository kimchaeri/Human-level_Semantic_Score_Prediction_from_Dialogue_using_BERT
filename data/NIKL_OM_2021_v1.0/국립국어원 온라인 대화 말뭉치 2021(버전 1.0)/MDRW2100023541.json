          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100023541.1.8",
          "form": "우와우와",
          "original_form": "우와우와",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023541.1.9",
          "form": "교육봉사라니...!!!",
          "original_form": "교육봉사라니...!!!",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023541.1.10",
          "form": "혹시 전공을 살리신건가요?!",
          "original_form": "혹시 전공을 살리신건가요?!",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023541.1.11",
          "form": "네 어떻게보면 그런 셈이죠...? 과학이긴 하니까요ㅋㅋㅋㅋㅋ",
          "original_form": "네 어떻게보면 그런 셈이죠...? 과학이긴 하니까요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023541.1.12",
          "form": "주로 오는 친구들이 애기들이라서 설명하기 어려웠지만요ㅋㅋㅋ",
          "original_form": "주로 오는 친구들이 애기들이라서 설명하기 어려웠지만요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023541.1.13",
          "form": "오 저도 전공이 과학이에요!!!",
          "original_form": "오 저도 전공이 과학이에요!!!",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023541.1.14",
          "form": "그래도 아이들은",
          "original_form": "그래도 아이들은",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023541.1.15",
          "form": "name1이 설명해주셔서 좋아했을거예요",
          "original_form": "&name1&이 설명해주셔서 좋아했을거예요",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023541.1.16",
          "form": "앗ㅋㅋㅋㅋ 그런가요ㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋ 그런가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023541.1.17",
          "form": "그럼요 그럼요",
          "original_form": "그럼요 그럼요",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023541.1.18",
          "form": "저는 과학관에서 봉사할 때",
          "original_form": "저는 과학관에서 봉사할 때",
          "speaker_id": "1",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023541.1.19",
          "form": "제일 좋았던 점이",
          "original_form": "제일 좋았던 점이",
          "speaker_id": "1",
          "time": "20210830 11:47"
        },
        {
          "id": "MDRW2100023541.1.20",
          "form": "식대가 없고 아무거나 먹을 수 있다는 거...!!",
          "original_form": "식대가 없고 아무거나 먹을 수 있다는 거...!!",
          "speaker_id": "1",
          "time": "20210830 11:47"
        },
        {
          "id": "MDRW2100023541.1.21",
          "form": "아주 최고였죠...",
          "original_form": "아주 최고였죠...",
