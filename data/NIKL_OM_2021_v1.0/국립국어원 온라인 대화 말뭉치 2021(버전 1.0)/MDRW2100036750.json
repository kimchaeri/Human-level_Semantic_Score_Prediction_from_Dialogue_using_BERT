      "utterance": [
        {
          "id": "MDRW2100036750.1.1",
          "form": "음식에 이물질나오면",
          "original_form": "음식에 이물질나오면",
          "speaker_id": "1",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100036750.1.2",
          "form": "신고할 수 있대요",
          "original_form": "신고할 수 있대요",
          "speaker_id": "1",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100036750.1.3",
          "form": "아 신고가능하군요ㅇㅁㅇ",
          "original_form": "아 신고가능하군요ㅇㅁㅇ",
          "speaker_id": "2",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100036750.1.4",
          "form": "한번도해보진않았는데",
          "original_form": "한번도해보진않았는데",
          "speaker_id": "2",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100036750.1.5",
          "form": "네 식약청 인가",
          "original_form": "네 식약청 인가",
          "speaker_id": "1",
          "time": "20210831 11:27"
        },
        {
          "id": "MDRW2100036750.1.6",
          "form": "신고하는데있대요",
          "original_form": "신고하는데있대요",
          "speaker_id": "1",
          "time": "20210831 11:27"
        },
        {
          "id": "MDRW2100036750.1.7",
          "form": "아 너무 심하면 신고해야할거같아요",
          "original_form": "아 너무 심하면 신고해야할거같아요",
          "speaker_id": "2",
          "time": "20210831 12:47"
        },
        {
          "id": "MDRW2100036750.1.8",
          "form": "네 신고하려구요 ㅜ 금욜에 그랬는데 늦어졌네요",
          "original_form": "네 신고하려구요 ㅜ 금욜에 그랬는데 늦어졌네요",
          "speaker_id": "1",
          "time": "20210831 12:48"
        },
        {
          "id": "MDRW2100036750.1.9",
          "form": "ㅠㅡㅠ 네 특히 미안한기색도없으면 전바로신고할듯",
          "original_form": "ㅠㅡㅠ 네 특히 미안한기색도없으면 전바로신고할듯",
          "speaker_id": "2",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100036750.1.10",
          "form": "그니까요 근데 증거사진이 딱 음식안에 있는 휴지가 아니라",
          "original_form": "그니까요 근데 증거사진이 딱 음식안에 있는 휴지가 아니라",
          "speaker_id": "1",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100036750.1.11",
          "form": "주인이 잡아뗄까 걱정이에요",
          "original_form": "주인이 잡아뗄까 걱정이에요",
          "speaker_id": "1",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100036750.1.12",
          "form": "아 그럴 수도 있겠네요 잡아떼기 -_-)",
          "original_form": "아 그럴 수도 있겠네요 잡아떼기 -_-)",
          "speaker_id": "2",
          "time": "20210831 13:02"
        },
        {
          "id": "MDRW2100036750.1.13",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 13:05"
        },
        {
          "id": "MDRW2100036750.1.14",
          "form": "컵라면하나먹을까하구요  ㅎㅎㅅ드셨나용?",
          "original_form": "컵라면하나먹을까하구요  ㅎㅎㅅ드셨나용?",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036750.1.15",
          "form": "네 간단히 먹었어요 ㅎㅎ",
          "original_form": "네 간단히 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036750.1.16",
          "form": "뭔가맛있는거먹고싶은데 그게뭔지모르겠어요..ㅋㅋ",
          "original_form": "뭔가맛있는거먹고싶은데 그게뭔지모르겠어요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100036750.1.17",
          "form": "흠 뭐가좋을까요 ㅎㅎㅎ",
          "original_form": "흠 뭐가좋을까요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 13:14"
        },
        {
