      "utterance": [
        {
          "id": "MDRW2100047630.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100047630.1.2",
          "form": "저녁 뭐 드셨나요",
          "original_form": "저녁 뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100047630.1.3",
          "form": "저는 짜글이요 name2님은요?",
          "original_form": "저는 짜글이요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100047630.1.4",
          "form": "저는 새우찜 먹었어요",
          "original_form": "저는 새우찜 먹었어요",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100047630.1.5",
          "form": "짜글이! 맛있었겠네요",
          "original_form": "짜글이! 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100047630.1.6",
          "form": "소스 찍어드셨나요?",
          "original_form": "소스 찍어드셨나요?",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100047630.1.7",
          "form": "어 새우를 찐거말구요 해물찜다른버전 새우찜을 먹었어요",
          "original_form": "어 새우를 찐거말구요 해물찜다른버전 새우찜을 먹었어요",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100047630.1.8",
          "form": "아~~ 고춧가루 넣어서 만든거였나요?",
          "original_form": "아~~ 고춧가루 넣어서 만든거였나요?",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100047630.1.9",
          "form": "시켜먹었어요 아마 고추가루가 들어갔겠죠...?",
          "original_form": "시켜먹었어요 아마 고추가루가 들어갔겠죠...?",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100047630.1.10",
          "form": "프랜차이즈였나요",
          "original_form": "프랜차이즈였나요",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100047630.1.11",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100047630.1.12",
          "form": "잘모르겠어요. 개인식당같아요",
          "original_form": "잘모르겠어요. 개인식당같아요",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100047630.1.13",
          "form": "새우찜에도 오만둥이랑 콩나물 들어가나요?",
          "original_form": "새우찜에도 오만둥이랑 콩나물 들어가나요?",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100047630.1.14",
          "form": "네 들어가요 오징어랑 고니도 들어갑니다",
          "original_form": "네 들어가요 오징어랑 고니도 들어갑니다",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100047630.1.15",
          "form": "정말 맛있을 것 같네요",
          "original_form": "정말 맛있을 것 같네요",
          "speaker_id": "1",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100047630.1.16",
          "form": "짜글이는 무엇이 들어가요?",
          "original_form": "짜글이는 무엇이 들어가요?",
          "speaker_id": "2",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100047630.1.17",
          "form": "고기랑 양파랑 고춧가루가 들어가요 맛있더라고요",
          "original_form": "고기랑 양파랑 고춧가루가 들어가요 맛있더라고요",
          "speaker_id": "1",
          "time": "20210826 22:55"
        }
