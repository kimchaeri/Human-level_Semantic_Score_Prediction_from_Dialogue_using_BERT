      "utterance": [
        {
          "id": "MDRW2100023287.1.1",
          "form": "독서 좋아하시나요?",
          "original_form": "독서 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100023287.1.2",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "2",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100023287.1.3",
          "form": "좋아하세요:?",
          "original_form": "좋아하세요:?",
          "speaker_id": "2",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100023287.1.4",
          "form": "저도 좋아하는데 요즘은 책을 잘 못읽고있어요ㅠㅠ",
          "original_form": "저도 좋아하는데 요즘은 책을 잘 못읽고있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023287.1.5",
          "form": "최근에 읽은 책 소개해주세요!",
          "original_form": "최근에 읽은 책 소개해주세요!",
          "speaker_id": "1",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023287.1.6",
          "form": "가장최근에 읽은책은 비혼수업이에요!",
          "original_form": "가장최근에 읽은책은 비혼수업이에요!",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023287.1.7",
          "form": "오 처음들어보는 챗이예요",
          "original_form": "오 처음들어보는 챗이예요",
          "speaker_id": "1",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023287.1.8",
          "form": "책",
          "original_form": "책",
          "speaker_id": "1",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023287.1.9",
          "form": "요즘 책에 관심이 없어져수..ㅠ  재밌으셨나요?!!",
          "original_form": "요즘 책에 관심이 없어져수..ㅠ  재밌으셨나요?!!",
          "speaker_id": "1",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023287.1.10",
          "form": "네 제가 비혼주의자라서",
          "original_form": "네 제가 비혼주의자라서",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023287.1.11",
          "form": "비혼으로 살기위해,,,!",
          "original_form": "비혼으로 살기위해,,,!",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023287.1.12",
          "form": "이것저것챙길이야기들이 많았어요",
          "original_form": "이것저것챙길이야기들이 많았어요",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023287.1.13",
          "form": "저도 비혼주의자예요!",
          "original_form": "저도 비혼주의자예요!",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023287.1.14",
          "form": "화요일에 도서관가서 그 책",
          "original_form": "화요일에 도서관가서 그 책",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023287.1.15",
          "form": "찾아봐야겠네요!",
          "original_form": "찾아봐야겠네요!",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023287.1.16",
          "form": "오 반가워요 ㅎㅎ 책추천합니다!",
          "original_form": "오 반가워요 ㅎㅎ 책추천합니다!",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023287.1.17",
          "form": "감사합니다~~! ㅎㅎㅎ",
          "original_form": "감사합니다~~! ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023287.1.18",
          "form": "전 올해 목표가 토지 완독하기였는데",
          "original_form": "전 올해 목표가 토지 완독하기였는데",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023287.1.19",
          "form": "헉토지",
          "original_form": "헉토지",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023287.1.20",
          "form": "너무길죠",
          "original_form": "너무길죠",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023287.1.21",
          "form": "절반밖에 못읽엇어요ㅠㅠ 9월부터 다시 독서하려구요!",
          "original_form": "절반밖에 못읽엇어요ㅠㅠ 9월부터 다시 독서하려구요!",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023287.1.22",
          "form": "서문보고 읽고싶었는데",
          "original_form": "서문보고 읽고싶었는데",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023287.1.23",
          "form": "아직도전을못한",
          "original_form": "아직도전을못한",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023287.1.24",
          "form": "막상 읽기시작하먄..!! 흡입력이 나름 좋아요ㅠㅠ 괜히 유명한 소설이 아니죠",
          "original_form": "막상 읽기시작하먄..!! 흡입력이 나름 좋아요ㅠㅠ 괜히 유명한 소설이 아니죠",
          "speaker_id": "1",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100023287.1.25",
          "form": "박경리작가님 넘 멋진거같아요",
          "original_form": "박경리작가님 넘 멋진거같아요",
          "speaker_id": "1",
          "time": "20210829 23:58"
        },
        {
          "id": "MDRW2100023287.1.26",
          "form": "역시 유명한덴 이유가 있다고 생각해요",
          "original_form": "역시 유명한덴 이유가 있다고 생각해요",
          "speaker_id": "2",
          "time": "20210829 23:58"
        },
        {
          "id": "MDRW2100023287.1.27",
          "form": "진짜멋지시죠",
          "original_form": "진짜멋지시죠",
