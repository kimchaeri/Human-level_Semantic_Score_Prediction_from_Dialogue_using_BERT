      "utterance": [
        {
          "id": "MDRW2100016700.1.1",
          "form": "name2 님은 기억나는 여행지 있으세요?",
          "original_form": "&name2& 님은 기억나는 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016700.1.2",
          "form": "음 파리랑 네덜란드요!",
          "original_form": "음 파리랑 네덜란드요!",
          "speaker_id": "2",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016700.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016700.1.4",
          "form": "와 해외 많이 가보셨군요",
          "original_form": "와 해외 많이 가보셨군요",
          "speaker_id": "1",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100016700.1.5",
          "form": "저는 수학여행으로 갔던 제주도요",
          "original_form": "저는 수학여행으로 갔던 제주도요",
          "speaker_id": "1",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100016700.1.6",
          "form": "제주도도 좋죠ㅎㅎ",
          "original_form": "제주도도 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016700.1.7",
          "form": "우리나라면서도 여행 분위기를 쉽게 느낄 수 있는 곳인 것같아요",
          "original_form": "우리나라면서도 여행 분위기를 쉽게 느낄 수 있는 곳인 것같아요",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016700.1.8",
          "form": "맞아요 어딜 찍어도 여행 분위기예요",
          "original_form": "맞아요 어딜 찍어도 여행 분위기예요",
          "speaker_id": "1",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100016700.1.9",
          "form": "그쵸 인생샷 건지러 가시는 분들도 있잖아요ㅋㅋㅋ",
          "original_form": "그쵸 인생샷 건지러 가시는 분들도 있잖아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100016700.1.10",
          "form": "맞아요ㅋㅋ 제가 갔을 때도 햇볕 엄청 쎄서 사진이 다 쨍하게 잘 나왔어요",
          "original_form": "맞아요ㅋㅋ 제가 갔을 때도 햇볕 엄청 쎄서 사진이 다 쨍하게 잘 나왔어요",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100016700.1.11",
          "form": "바람도 자주 불어서 잘 나오는 것같기도 해요ㅋㅋ",
          "original_form": "바람도 자주 불어서 잘 나오는 것같기도 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100016700.1.12",
          "form": "감귤 초콜릿 드셔보셨나요 그럼?ㅋㅋㅋ--",
          "original_form": "감귤 초콜릿 드셔보셨나요 그럼?ㅋㅋㅋ--",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100016700.1.13",
          "form": "네 저 그거 좋아해요",
          "original_form": "네 저 그거 좋아해요",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100016700.1.14",
          "form": "고급진 초콜릿은 아닌데 중독성 있는 것 같아요ㅋㅋ",
          "original_form": "고급진 초콜릿은 아닌데 중독성 있는 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100016700.1.15",
          "form": "제주도가면 한번씩은 먹어봐야한다는 초콜릿이잖아요ㅎㅎ",
          "original_form": "제주도가면 한번씩은 먹어봐야한다는 초콜릿이잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100016700.1.16",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100016700.1.17",
          "form": "오메기떡은 드셔보셨어요?",
          "original_form": "오메기떡은 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100016700.1.18",
          "form": "앗 네! 근데 너무 오래전에 먹어서 기억이 잘 안나요ㅠ",
          "original_form": "앗 네! 근데 너무 오래전에 먹어서 기억이 잘 안나요ㅠ",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100016700.1.19",
          "form": "무슨 맛이었죠??",
          "original_form": "무슨 맛이었죠??",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100016700.1.20",
          "form": "저도 오래전에 먹었는데 찹쌀떡 안에 콩고물이 있었어요",
          "original_form": "저도 오래전에 먹었는데 찹쌀떡 안에 콩고물이 있었어요",
          "speaker_id": "1",
          "time": "20210827 18:11"
        },
        {
          "id": "MDRW2100016700.1.21",
          "form": "아  팥이었어요!",
          "original_form": "아  팥이었어요!",
          "speaker_id": "1",
          "time": "20210827 18:11"
        },
        {
          "id": "MDRW2100016700.1.22",
          "form": "맛은 잘 기억 안나는데 맛있었던 기억만 있어요ㅋㅋ",
          "original_form": "맛은 잘 기억 안나는데 맛있었던 기억만 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100016700.1.23",
          "form": "아ㅋㅋㅋㅋ언젠가 다시 한번 먹어봐야겠어요",
          "original_form": "아ㅋㅋㅋㅋ언젠가 다시 한번 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 18:27"
        }
      ]
    }
