      "utterance": [
        {
          "id": "MDRW2100016046.1.1",
          "form": "연애중이신가요",
          "original_form": "연애중이신가요",
          "speaker_id": "2",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100016046.1.2",
          "form": "전 결혼했어요",
          "original_form": "전 결혼했어요",
          "speaker_id": "1",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100016046.1.3",
          "form": "남자친구 있으세요?",
          "original_form": "남자친구 있으세요?",
          "speaker_id": "1",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100016046.1.4",
          "form": "아 네네 있어요",
          "original_form": "아 네네 있어요",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100016046.1.5",
          "form": "하지만 해외 롱디입니다ㅠㅠ유학갔어요",
          "original_form": "하지만 해외 롱디입니다ㅠㅠ유학갔어요",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100016046.1.6",
          "form": "결혼 생활  어떤가요 너무 궁금해요",
          "original_form": "결혼 생활  어떤가요 너무 궁금해요",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100016046.1.7",
          "form": "아 ㅜ 언제돌아오나요?",
          "original_form": "아 ㅜ 언제돌아오나요?",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100016046.1.8",
          "form": "아이가 4학년이에요..책임감있는 남편 만나서 결혼은 잘한거같아요",
          "original_form": "아이가 4학년이에요..책임감있는 남편 만나서 결혼은 잘한거같아요",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100016046.1.9",
          "form": "우와 벌써 4학년인가요",
          "original_form": "우와 벌써 4학년인가요",
          "speaker_id": "2",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100016046.1.10",
          "form": "저는 3년 유학갔는데 이제 1년 반 남았어요",
          "original_form": "저는 3년 유학갔는데 이제 1년 반 남았어요",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100016046.1.11",
          "form": "아 너무부러워요... 어디계세요?",
          "original_form": "아 너무부러워요... 어디계세요?",
          "speaker_id": "1",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100016046.1.12",
          "form": "아...남친이 유학가셨단말이군요..죄송해요",
          "original_form": "아...남친이 유학가셨단말이군요..죄송해요",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100016046.1.13",
          "form": "아아 맞아요",
          "original_form": "아아 맞아요",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100016046.1.14",
          "form": "북유럽쪽으로 가있습니다ㅜㅜ",
          "original_form": "북유럽쪽으로 가있습니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100016046.1.15",
          "form": "저는 한국 ㅠㅠ또르리",
          "original_form": "저는 한국 ㅠㅠ또르리",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100016046.1.16",
          "form": "아주 먼곳에 계시네요 보고싶죠",
          "original_form": "아주 먼곳에 계시네요 보고싶죠",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100016046.1.17",
          "form": "남친분이 어떤쪽으로 공부하러갔나요?",
          "original_form": "남친분이 어떤쪽으로 공부하러갔나요?",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100016046.1.18",
          "form": "네 맞아요 ㅠ 공부는 빅데이터랑 자율운항선박 쪽이에요",
          "original_form": "네 맞아요 ㅠ 공부는 빅데이터랑 자율운항선박 쪽이에요",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100016046.1.19",
          "form": "멋지시네요~ \r\n코로나라서 걱정도 되시겠어요",
          "original_form": "멋지시네요~ \r\n코로나라서 걱정도 되시겠어요",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100016046.1.20",
          "form": "맞아요 근데 유럽은 마스크도 안끼더라구요",
          "original_form": "맞아요 근데 유럽은 마스크도 안끼더라구요",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100016046.1.21",
          "form": "제남친도 헐렐레 팔렐레 그냥 다녀서 걱정이에요",
          "original_form": "제남친도 헐렐레 팔렐레 그냥 다녀서 걱정이에요",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100016046.1.22",
          "form": "아 신기하네요 안껴도...감염괜찮아요?",
          "original_form": "아 신기하네요 안껴도...감염괜찮아요?",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100016046.1.23",
          "form": "그니까요 근데 1년반째 괜찮네요",
          "original_form": "그니까요 근데 1년반째 괜찮네요",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100016046.1.24",
          "form": "워낙 사람 없는 곳이라 가능한가봐요",
          "original_form": "워낙 사람 없는 곳이라 가능한가봐요",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100016046.1.25",
          "form": "땅도넓고ㅠ",
          "original_form": "땅도넓고ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:14"
        }
      ]
    }
