      "utterance": [
        {
          "id": "MDRW2100035110.1.1",
          "form": "날이 추워졌는데 이불은 바꾸셨나용?",
          "original_form": "날이 추워졌는데 이불은 바꾸셨나용?",
          "speaker_id": "1",
          "time": "20210829 02:55"
        },
        {
          "id": "MDRW2100035110.1.2",
          "form": "아뇨 아직 ㅠ ㅈ",
          "original_form": "아뇨 아직 ㅠ ㅈ",
          "speaker_id": "2",
          "time": "20210829 02:55"
        },
        {
          "id": "MDRW2100035110.1.3",
          "form": "전오늘도 좀더워용 ㅠㅠ",
          "original_form": "전오늘도 좀더워용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:56"
        },
        {
          "id": "MDRW2100035110.1.4",
          "form": "이러다가 갑자기",
          "original_form": "이러다가 갑자기",
          "speaker_id": "1",
          "time": "20210829 02:56"
        },
        {
          "id": "MDRW2100035110.1.5",
          "form": "더워질까봐 저도 고민중이에요ㅋㅋ",
          "original_form": "더워질까봐 저도 고민중이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:56"
        },
        {
          "id": "MDRW2100035110.1.6",
          "form": "며칠전까진 포근이불덥고싶다 생각햇는데",
          "original_form": "며칠전까진 포근이불덥고싶다 생각햇는데",
          "speaker_id": "2",
          "time": "20210829 02:56"
        },
        {
          "id": "MDRW2100035110.1.7",
          "form": "다음주부터 내내 비온대요",
          "original_form": "다음주부터 내내 비온대요",
          "speaker_id": "1",
          "time": "20210829 02:56"
        },
        {
          "id": "MDRW2100035110.1.8",
          "form": "또비인가요ㅠ",
          "original_form": "또비인가요ㅠ",
          "speaker_id": "2",
          "time": "20210829 02:56"
        },
        {
          "id": "MDRW2100035110.1.9",
          "form": "빨래 주말에 다하세용!ㅋㅋㅋ",
          "original_form": "빨래 주말에 다하세용!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:56"
        },
        {
          "id": "MDRW2100035110.1.10",
          "form": "전 주변에서 날씨말해주니깐 그냥 ㅋㅋㅋ 날씨안찾아봐요",
          "original_form": "전 주변에서 날씨말해주니깐 그냥 ㅋㅋㅋ 날씨안찾아봐요",
          "speaker_id": "2",
          "time": "20210829 02:56"
        },
        {
          "id": "MDRW2100035110.1.11",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:56"
        },
        {
          "id": "MDRW2100035110.1.12",
          "form": "주변에 든든하시겠네용",
          "original_form": "주변에 든든하시겠네용",
          "speaker_id": "1",
          "time": "20210829 02:57"
        },
        {
          "id": "MDRW2100035110.1.13",
          "form": "제가 혼자사는게 아니라섴ㅋㅋㅋ 내일 누가 댜할꺼에요 ㅋㅋㅋ",
          "original_form": "제가 혼자사는게 아니라섴ㅋㅋㅋ 내일 누가 댜할꺼에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:57"
        },
        {
          "id": "MDRW2100035110.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 저도 엄마가 주로 하세용...ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저도 엄마가 주로 하세용...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:57"
        },
        {
          "id": "MDRW2100035110.1.15",
          "form": "오늘 날좋으니깐",
          "original_form": "오늘 날좋으니깐",
          "speaker_id": "2",
          "time": "20210829 02:57"
        },
        {
          "id": "MDRW2100035110.1.16",
          "form": "날씨가 맑아졌음 좋겠어요ㅠ",
          "original_form": "날씨가 맑아졌음 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 02:57"
        },
        {
          "id": "MDRW2100035110.1.17",
          "form": "오늘도 좋아용?",
          "original_form": "오늘도 좋아용?",
          "speaker_id": "1",
          "time": "20210829 02:57"
        },
        {
          "id": "MDRW2100035110.1.18",
          "form": "이불이랑 세탁해야하는데",
          "original_form": "이불이랑 세탁해야하는데",
          "speaker_id": "2",
          "time": "20210829 02:57"
        },
        {
          "id": "MDRW2100035110.1.19",
          "form": "하고 ㅋㅋㅋ지굼 집에없어요 ㅋㅋ",
          "original_form": "하고 ㅋㅋㅋ지굼 집에없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:57"
        },
        {
          "id": "MDRW2100035110.1.20",
          "form": "맞아요... 저도 빨래 일어나서 돌리고...",
          "original_form": "맞아요... 저도 빨래 일어나서 돌리고...",
          "speaker_id": "1",
          "time": "20210829 02:57"
        },
        {
          "id": "MDRW2100035110.1.21",
          "form": "내일아마싹할듯 ㅋㅋㅋ",
          "original_form": "내일아마싹할듯 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:58"
        }
      ]
