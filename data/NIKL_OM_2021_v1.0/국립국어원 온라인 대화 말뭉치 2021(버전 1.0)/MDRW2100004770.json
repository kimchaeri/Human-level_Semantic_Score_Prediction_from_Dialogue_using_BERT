      "utterance": [
        {
          "id": "MDRW2100004770.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004770.1.2",
          "form": "아뇨 ㅠ 안키워요",
          "original_form": "아뇨 ㅠ 안키워요",
          "speaker_id": "2",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004770.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004770.1.4",
          "form": "키울생각은요?",
          "original_form": "키울생각은요?",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004770.1.5",
          "form": "키우고싶은데 키울자신이없네여",
          "original_form": "키우고싶은데 키울자신이없네여",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004770.1.6",
          "form": "돈도그렇고 보살펴줄자신이없어요ㅜ",
          "original_form": "돈도그렇고 보살펴줄자신이없어요ㅜ",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004770.1.7",
          "form": "케어가 어렵죠",
          "original_form": "케어가 어렵죠",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100004770.1.8",
          "form": "보면 병원비가 진짜 부담이더라고요",
          "original_form": "보면 병원비가 진짜 부담이더라고요",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100004770.1.9",
          "form": "사람보다 더 들죠?",
          "original_form": "사람보다 더 들죠?",
          "speaker_id": "1",
          "time": "20210822 08:33"
        },
        {
          "id": "MDRW2100004770.1.10",
          "form": "그쵸 보험이안되어가지고..",
          "original_form": "그쵸 보험이안되어가지고..",
          "speaker_id": "2",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100004770.1.11",
          "form": "사람보다 더 들아요",
          "original_form": "사람보다 더 들아요",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100004770.1.12",
          "form": "동물도 늙으면 수발들어야하고..책임질자신이ㅠ아직없네요",
          "original_form": "동물도 늙으면 수발들어야하고..책임질자신이ㅠ아직없네요",
          "speaker_id": "2",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100004770.1.13",
          "form": "케어가어렵죠",
          "original_form": "케어가어렵죠",
          "speaker_id": "1",
          "time": "20210823 07:19"
        },
        {
          "id": "MDRW2100004770.1.14",
          "form": "맞아요 진짜 평생 아기키우는거랑똑같대여",
          "original_form": "맞아요 진짜 평생 아기키우는거랑똑같대여",
          "speaker_id": "2",
          "time": "20210823 21:36"
        },
        {
          "id": "MDRW2100004770.1.15",
          "form": "사람보다 더ㅜ힘들듯",
          "original_form": "사람보다 더ㅜ힘들듯",
          "speaker_id": "1",
