        {
          "id": "MDRW2100019206.1.1",
          "form": "네 근대 또 사고싶어요",
          "original_form": "네 근대 또 사고싶어요",
          "speaker_id": "2",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019206.1.2",
          "form": "궁금해요 name2님 가디건 콜렉션",
          "original_form": "궁금해요 &name2&님 가디건 콜렉션",
          "speaker_id": "1",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019206.1.3",
          "form": "열심히 입고 나가볼게요 name1님만날때",
          "original_form": "열심히 입고 나가볼게요 &name1&님만날때",
          "speaker_id": "2",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019206.1.4",
          "form": "트렌치도 사고싶어요",
          "original_form": "트렌치도 사고싶어요",
          "speaker_id": "2",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019206.1.5",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019206.1.6",
          "form": "저도트렌차",
          "original_form": "저도트렌차",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019206.1.7",
          "form": "버버리사구파요",
