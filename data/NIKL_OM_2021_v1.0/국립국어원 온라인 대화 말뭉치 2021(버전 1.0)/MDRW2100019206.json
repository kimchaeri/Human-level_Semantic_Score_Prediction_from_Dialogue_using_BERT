          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019206.1.8",
          "form": "맞아요 찐 버버리 사고싶어요",
          "original_form": "맞아요 찐 버버리 사고싶어요",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019206.1.9",
          "form": "버버리 가방도 하나 사고싶고요",
          "original_form": "버버리 가방도 하나 사고싶고요",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019206.1.10",
          "form": "샤넬 셀린도 사고싶은데",
          "original_form": "샤넬 셀린도 사고싶은데",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019206.1.11",
          "form": "돈이없네요",
          "original_form": "돈이없네요",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019206.1.12",
          "form": "ㄹㅇㄹㅇㄹㅇ ㅠㅠㅠㅠ",
          "original_form": "ㄹㅇㄹㅇㄹㅇ ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100019206.1.13",
          "form": "하 너무사고싶다",
          "original_form": "하 너무사고싶다",
          "speaker_id": "1",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100019206.1.14",
          "form": "name1님 지금 백하나 살수있다면",
          "original_form": "&name1&님 지금 백하나 살수있다면",
          "speaker_id": "2",
          "time": "20210827 16:24"
        },
        {
          "id": "MDRW2100019206.1.15",
          "form": "뭐 고르실?",
          "original_form": "뭐 고르실?",
          "speaker_id": "2",
          "time": "20210827 16:24"
        },
        {
          "id": "MDRW2100019206.1.16",
          "form": "ㅋㅋㅋㅋ 아 ...암 거나 살수있음 젤 비싼거 고르죠 샤넬 클래식이나 레이디디올 ㅋㅋㅋㅋㅋㅋㅋ 내가못살거 에르메스 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 아 ...암 거나 살수있음 젤 비싼거 고르죠 샤넬 클래식이나 레이디디올 ㅋㅋㅋㅋㅋㅋㅋ 내가못살거 에르메스 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019206.1.17",
          "form": "현답이시네요",
          "original_form": "현답이시네요",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019206.1.18",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019206.1.19",
          "form": "저도 그럼 에르메스 켈리백으로 할게요",
          "original_form": "저도 그럼 에르메스 켈리백으로 할게요",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019206.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ 크~~",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 크~~",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019206.1.21",
          "form": "걍 턱턱사고싶다",
          "original_form": "걍 턱턱사고싶다",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019206.1.22",
          "form": "저는 요즘 러그도 사고싶고",
          "original_form": "저는 요즘 러그도 사고싶고",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019206.1.23",
          "form": "파라솔도 사고싶은데요",
          "original_form": "파라솔도 사고싶은데요",
          "speaker_id": "2",
          "time": "20210827 16:34"
