      "utterance": [
        {
          "id": "MDRW2100036854.1.1",
          "form": "그래 정말 쉬운게없구나",
          "original_form": "그래 정말 쉬운게없구나",
          "speaker_id": "2",
          "time": "20210831 13:47"
        },
        {
          "id": "MDRW2100036854.1.2",
          "form": "이래서 돈은 꽁으로 버는게 없네 ㅋㅋㅋㅋ",
          "original_form": "이래서 돈은 꽁으로 버는게 없네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:47"
        },
        {
          "id": "MDRW2100036854.1.3",
          "form": "맞어 맞어 ㅋㅋ",
          "original_form": "맞어 맞어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:47"
        },
        {
          "id": "MDRW2100036854.1.4",
          "form": "언니 내 이제 좀 티비보면서 쉴게 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "언니 내 이제 좀 티비보면서 쉴게 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100036854.1.5",
          "form": "좀따가 또 톡합세 ㅋ 폰과 멀어질 시간이 되었다 ㅋㅋ",
          "original_form": "좀따가 또 톡합세 ㅋ 폰과 멀어질 시간이 되었다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100036854.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 그래 고생했다 ㅋㅋㅋㅋ 쉬어",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 그래 고생했다 ㅋㅋㅋㅋ 쉬어",
          "speaker_id": "2",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100036854.1.7",
          "form": "얍얍 ㅋㅋㅋ 좀따 하자 ㅋㅋ",
          "original_form": "얍얍 ㅋㅋㅋ 좀따 하자 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100036854.1.8",
          "form": "언니뭐하노ㅋ애들보나ㅋ",
          "original_form": "언니뭐하노ㅋ애들보나ㅋ",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100036854.1.9",
          "form": "엉 놀이터갔다가",
          "original_form": "엉 놀이터갔다가",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100036854.1.10",
          "form": "이제 집에와서",
          "original_form": "이제 집에와서",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100036854.1.11",
          "form": "주스하나 마시고 센터가려고",
          "original_form": "주스하나 마시고 센터가려고",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036854.1.12",
          "form": "좀 쉬었낭??",
          "original_form": "좀 쉬었낭??",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036854.1.13",
          "form": "응응 오늘도 센터가네ㅋ",
          "original_form": "응응 오늘도 센터가네ㅋ",
          "speaker_id": "1",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100036854.1.14",
          "form": "응 좀 쉬었엉 좋다ㅋ",
          "original_form": "응 좀 쉬었엉 좋다ㅋ",
          "speaker_id": "1",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100036854.1.15",
          "form": "애들데리고 나는 케이스마트가려고",
          "original_form": "애들데리고 나는 케이스마트가려고",
          "speaker_id": "1",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100036854.1.16",
          "form": "거기서 놀고싶대서ㅋ",
          "original_form": "거기서 놀고싶대서ㅋ",
          "speaker_id": "1",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100036854.1.17",
          "form": "아하 놀라고 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "아하 놀라고 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100036854.1.18",
          "form": "좋구먼 장도보고 놀고 ㅎㅎ",
          "original_form": "좋구먼 장도보고 놀고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036854.1.19",
          "form": "응 그러니깐",
          "original_form": "응 그러니깐",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036854.1.20",
          "form": "어제 우유사서왔는데",
          "original_form": "어제 우유사서왔는데",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036854.1.21",
          "form": "놀고싶다고 막해가ㅡㅡㅋㅋ",
          "original_form": "놀고싶다고 막해가ㅡㅡㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036854.1.22",
          "form": "오늘도 데리고간다",
          "original_form": "오늘도 데리고간다",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036854.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㄱ 좋다이",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㄱ 좋다이",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100036854.1.24",
          "form": "담엔 거기 놀러를 한번 가봐야겠노\nㅋㅋㅋㅋㅋㅋ",
          "original_form": "담엔 거기 놀러를 한번 가봐야겠노\nㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100036854.1.25",
          "form": "응응 ㅋㅋㅋㅋ 좋지좋지 ㅋㅋ지오재우니 이시간ㅜㅜㅋㅋ",
          "original_form": "응응 ㅋㅋㅋㅋ 좋지좋지 ㅋㅋ지오재우니 이시간ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:49"
        },
        {
          "id": "MDRW2100036854.1.26",
          "form": "웁스 태권도라도 보내야 되나",
          "original_form": "웁스 태권도라도 보내야 되나",
          "speaker_id": "2",
          "time": "20210901 08:27"
        },
        {
          "id": "MDRW2100036854.1.27",
          "form": "비오네 또",
          "original_form": "비오네 또",
          "speaker_id": "2",
          "time": "20210901 08:27"
        },
        {
          "id": "MDRW2100036854.1.28",
          "form": "ㅋㅋㅋㅋㅋ진짜 뭐라도 보내야되는지ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ진짜 뭐라도 보내야되는지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 08:31"
        },
        {
          "id": "MDRW2100036854.1.29",
          "form": "ㅋㅋㅋㅋㄱ 그래 얼집으로는 부족하다",
          "original_form": "ㅋㅋㅋㅋㄱ 그래 얼집으로는 부족하다",
          "speaker_id": "2",
          "time": "20210901 09:18"
        },
        {
          "id": "MDRW2100036854.1.30",
          "form": "트램폴린을 사라 그냥 ㄹㅋ",
          "original_form": "트램폴린을 사라 그냥 ㄹㅋ",
          "speaker_id": "2",
          "time": "20210901 09:18"
        },
        {
          "id": "MDRW2100036854.1.31",
          "form": "ㅋㅋ 그래야되나 고민이다 아놔ㅋㅋ",
          "original_form": "ㅋㅋ 그래야되나 고민이다 아놔ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 09:36"
        },
        {
          "id": "MDRW2100036854.1.32",
          "form": "쉽지않다 에너지빼기ㅋ",
          "original_form": "쉽지않다 에너지빼기ㅋ",
          "speaker_id": "1",
          "time": "20210901 09:37"
        },
        {
          "id": "MDRW2100036854.1.33",
          "form": "ㅋㅋㅋㅋㄱ 그래 그때 경미언니집에 트램폴린 작은거 있드만",
          "original_form": "ㅋㅋㅋㅋㄱ 그래 그때 경미언니집에 트램폴린 작은거 있드만",
          "speaker_id": "2",
          "time": "20210901 09:50"
        },
        {
          "id": "MDRW2100036854.1.34",
          "form": "지오는 뭐라도 해야겠다",
          "original_form": "지오는 뭐라도 해야겠다",
          "speaker_id": "2",
          "time": "20210901 09:50"
        },
        {
          "id": "MDRW2100036854.1.35",
          "form": "얼집에서 낮잠은 자나",
          "original_form": "얼집에서 낮잠은 자나",
          "speaker_id": "2",
          "time": "20210901 09:51"
        },
        {
          "id": "MDRW2100036854.1.36",
          "form": "ㅋㅋㅋㅋ응낮잠은잔대ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ응낮잠은잔대ㅋㅋ",
          "speaker_id": "1",
