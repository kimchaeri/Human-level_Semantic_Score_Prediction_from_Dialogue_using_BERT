        {
          "id": "MDRW2100018934.1.1",
          "form": "전 비행기 탈때 힘들던데",
          "original_form": "전 비행기 탈때 힘들던데",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018934.1.2",
          "form": "그래서 운동 등도 권유했는데",
          "original_form": "그래서 운동 등도 권유했는데",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018934.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018934.1.4",
          "form": "공황이 무섭다더라고요",
          "original_form": "공황이 무섭다더라고요",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018934.1.5",
          "form": "운동은좀",
          "original_form": "운동은좀",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018934.1.6",
          "form": "역시 치료가ㅠ",
          "original_form": "역시 치료가ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018934.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018934.1.8",
          "form": "전 심하지 않아서 약먹고 괜찮아요",
          "original_form": "전 심하지 않아서 약먹고 괜찮아요",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018934.1.9",
          "form": "일시적인 거라",
          "original_form": "일시적인 거라",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018934.1.10",
          "form": "오히려",
          "original_form": "오히려",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018934.1.11",
          "form": "수집하는 취미가 도움됐는데",
          "original_form": "수집하는 취미가 도움됐는데",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018934.1.12",
          "form": "오 뭐수집하세요?",
          "original_form": "오 뭐수집하세요?",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018934.1.13",
          "form": "이쁜 장난감들도 모으고",
          "original_form": "이쁜 장난감들도 모으고",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018934.1.14",
          "form": "근데 다팔았지만요 ㅋㅋ",
          "original_form": "근데 다팔았지만요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018934.1.15",
          "form": "와 좋다 피규어요?",
          "original_form": "와 좋다 피규어요?",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018934.1.16",
          "form": "팔때도 쏠쏠해요 ㅋㅋ",
          "original_form": "팔때도 쏠쏠해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018934.1.17",
          "form": "어쩌다 파셨어요ㅠ",
          "original_form": "어쩌다 파셨어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
