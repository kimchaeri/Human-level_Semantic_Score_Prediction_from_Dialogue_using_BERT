      "utterance": [
        {
          "id": "MDRW2100000522.1.1",
          "form": "요즘도 대학교가면 예뻐진다는 말이 있나요?",
          "original_form": "요즘도 대학교가면 예뻐진다는 말이 있나요?",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000522.1.2",
          "form": "앜ㅎㅋㅎㅋㅎ 저희 부모님이 매일 하시는 말씀이에요ㅠ",
          "original_form": "앜ㅎㅋㅎㅋㅎ 저희 부모님이 매일 하시는 말씀이에요ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000522.1.3",
          "form": "크흐 그렇구나",
          "original_form": "크흐 그렇구나",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000522.1.4",
          "form": "역시 고3부모님들이 늘 하시는",
          "original_form": "역시 고3부모님들이 늘 하시는",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000522.1.5",
          "form": "저 때도 그랬거든요",
          "original_form": "저 때도 그랬거든요",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000522.1.6",
          "form": "근데..예..여기까지",
          "original_form": "근데..예..여기까지",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000522.1.7",
          "form": "앟ㅋㅎㅋㅎㅋㅋㅋ 그렇군요",
          "original_form": "앟ㅋㅎㅋㅎㅋㅋㅋ 그렇군요",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000522.1.8",
          "form": "그냥 마음이 편해서 얼굴이 피는 건가..",
          "original_form": "그냥 마음이 편해서 얼굴이 피는 건가..",
          "speaker_id": "1",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000522.1.9",
          "form": "요즘은 학생들이 꾸미는 건 더 잘하는 것 같아요",
          "original_form": "요즘은 학생들이 꾸미는 건 더 잘하는 것 같아요",
          "speaker_id": "1",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000522.1.10",
          "form": "아 맞아요.. 그건 진짜 맞는 것 같아요",
          "original_form": "아 맞아요.. 그건 진짜 맞는 것 같아요",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000522.1.11",
          "form": "요즘 유튜브도 막 뷰티 유튜버도 많고 패션 유튜버도 있고 그러니까 그런거 보고 애들이 따라하는거 보면 너무 잘 따라해요ㅋㅋㅋㅋ",
          "original_form": "요즘 유튜브도 막 뷰티 유튜버도 많고 패션 유튜버도 있고 그러니까 그런거 보고 애들이 따라하는거 보면 너무 잘 따라해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000522.1.12",
          "form": "그래서 요즘에 사복입으면",
          "original_form": "그래서 요즘에 사복입으면",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000522.1.13",
          "form": "학생인지 성인인지 구분도 잘 안되더라고요",
          "original_form": "학생인지 성인인지 구분도 잘 안되더라고요",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000522.1.14",
          "form": "다들 키도 크도 !",
          "original_form": "다들 키도 크도 !",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000522.1.15",
          "form": "맞아요.. 패션도 20대랑 다를게 없어요...",
          "original_form": "맞아요.. 패션도 20대랑 다를게 없어요...",
          "speaker_id": "2",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000522.1.16",
          "form": "라떼는 똑단발이었는네",
          "original_form": "라떼는 똑단발이었는네",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000522.1.17",
          "form": "요즘은 두발자유예요?",
          "original_form": "요즘은 두발자유예요?",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000522.1.18",
          "form": "앟ㅋㅋㅋㅋ 저희 학교를 기준으로는 머리기장은 상관 없고 염색만 안하면 되는 정도에요",
          "original_form": "앟ㅋㅋㅋㅋ 저희 학교를 기준으로는 머리기장은 상관 없고 염색만 안하면 되는 정도에요",
          "speaker_id": "2",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000522.1.19",
          "form": "거의 두발 자유죠!",
          "original_form": "거의 두발 자유죠!",
          "speaker_id": "2",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000522.1.20",
          "form": "오호 펌도 괜찮은 거예요?",
          "original_form": "오호 펌도 괜찮은 거예요?",
          "speaker_id": "1",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000522.1.21",
          "form": "네! 약간 구준표 헤어스타일만 아니면 괜찮은정도?ㅎㅋㅋㅋㅋ",
          "original_form": "네! 약간 구준표 헤어스타일만 아니면 괜찮은정도?ㅎㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000522.1.22",
          "form": "구준표를 알아요?",
          "original_form": "구준표를 알아요?",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000522.1.23",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000522.1.24",
          "form": "앜ㅎㅋㅎㅋㅋㅋ 모를리가 있나요!",
          "original_form": "앜ㅎㅋㅎㅋㅋㅋ 모를리가 있나요!",
          "speaker_id": "2",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000522.1.25",
          "form": "그 드라마 거의 10년 넘지 않았나요?",
          "original_form": "그 드라마 거의 10년 넘지 않았나요?",
          "speaker_id": "1",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000522.1.26",
          "form": "근데 확실히 지금 꽃보다 남자 보면 스타일이 참 옛날같다라고 느껴요..",
          "original_form": "근데 확실히 지금 꽃보다 남자 보면 스타일이 참 옛날같다라고 느껴요..",
          "speaker_id": "2",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000522.1.27",
          "form": "10년도 더 넘었을걸요?ㅋㅋㅋ",
          "original_form": "10년도 더 넘었을걸요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000522.1.28",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000522.1.29",
          "form": "옛날 스타일",
          "original_form": "옛날 스타일",
          "speaker_id": "1",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000522.1.30",
          "form": "추억의 드라마",
          "original_form": "추억의 드라마",
          "speaker_id": "1",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000522.1.31",
          "form": "그땐 미용실가면 남저들이 다 구준표 머리 해주세요를 외쳤다고ㅋㅋㅋㅋㅋ",
          "original_form": "그땐 미용실가면 남저들이 다 구준표 머리 해주세요를 외쳤다고ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000522.1.32",
          "form": "으으 그래서 너무 별로였어요",
          "original_form": "으으 그래서 너무 별로였어요",
          "speaker_id": "1",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000522.1.33",
          "form": "구준표라서 괜찮았던 머리인데..",
          "original_form": "구준표라서 괜찮았던 머리인데..",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000522.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요 그 예전에 또 이태원 클라스 거기에 박서준이 했던 머리도 또 유행했잖아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요 그 예전에 또 이태원 클라스 거기에 박서준이 했던 머리도 또 유행했잖아요",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000522.1.35",
          "form": "흡...",
          "original_form": "흡...",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000522.1.36",
          "form": "그또한 박서준이라서 괜찮은 밤톨머리였는데",
          "original_form": "그또한 박서준이라서 괜찮은 밤톨머리였는데",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000522.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 웃겨라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 웃겨라",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000522.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋ 확실히 유명한 드라마가 있으면 그게 유행이 되고 스타일이 되는 것 같아요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 확실히 유명한 드라마가 있으면 그게 유행이 되고 스타일이 되는 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000522.1.39",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000522.1.40",
          "form": "그래도 자기관리하는 사람이 멋지요 안 하는 것보다는!",
          "original_form": "그래도 자기관리하는 사람이 멋지요 안 하는 것보다는!",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000522.1.41",
          "form": "맞아요! 자기관리가 중요하죠!",
          "original_form": "맞아요! 자기관리가 중요하죠!",
