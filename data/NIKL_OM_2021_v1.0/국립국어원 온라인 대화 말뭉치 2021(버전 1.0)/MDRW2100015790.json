      "utterance": [
        {
          "id": "MDRW2100015790.1.1",
          "form": "난 오늘 고등어구이를 했지",
          "original_form": "난 오늘 고등어구이를 했지",
          "speaker_id": "2",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015790.1.2",
          "form": "너가고등어구웟다고?",
          "original_form": "너가고등어구웟다고?",
          "speaker_id": "1",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015790.1.3",
          "form": "엄마가 구웠징ㅋㅋㅋㅋ",
          "original_form": "엄마가 구웠징ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015790.1.4",
          "form": "나도 구울수있엉",
          "original_form": "나도 구울수있엉",
          "speaker_id": "2",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015790.1.5",
          "form": "야 대단한데 난 생선 한번도 안구워봄 ㅋㅋㅋ익은지어떻게알아 ㅋㅋㅋ",
          "original_form": "야 대단한데 난 생선 한번도 안구워봄 ㅋㅋㅋ익은지어떻게알아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100015790.1.6",
          "form": "적당히 타이밍이 있디",
          "original_form": "적당히 타이밍이 있디",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015790.1.7",
          "form": "훗",
          "original_form": "훗",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015790.1.8",
          "form": "아직 어리구나",
          "original_form": "아직 어리구나",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015790.1.9",
          "form": "난 이번에 부추무침도 했다고",
          "original_form": "난 이번에 부추무침도 했다고",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015790.1.10",
          "form": "야 니 대단한데? 진짜가? 무침이 진짜 어려운거라던데",
          "original_form": "야 니 대단한데? 진짜가? 무침이 진짜 어려운거라던데",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015790.1.11",
          "form": "하하하핳",
          "original_form": "하하하핳",
          "speaker_id": "2",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015790.1.12",
          "form": "나나 된장국도 잘하고",
          "original_form": "나나 된장국도 잘하고",
          "speaker_id": "2",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015790.1.13",
          "form": "김치찌개도 하고",
          "original_form": "김치찌개도 하고",
          "speaker_id": "2",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015790.1.14",
          "form": "미역국이랑",
          "original_form": "미역국이랑",
          "speaker_id": "2",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015790.1.15",
          "form": "너 미래가 밝구나",
          "original_form": "너 미래가 밝구나",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015790.1.16",
          "form": "또또 북어국!",
          "original_form": "또또 북어국!",
          "speaker_id": "2",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015790.1.17",
          "form": "배추겉절이도 해봄ㅋㅋ",
          "original_form": "배추겉절이도 해봄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015790.1.18",
          "form": "난 엄빠 생일때 미역국도 아직 못끓여드렸는데...",
          "original_form": "난 엄빠 생일때 미역국도 아직 못끓여드렸는데...",
          "speaker_id": "1",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015790.1.19",
          "form": "뭐꼭할필요는.....",
          "original_form": "뭐꼭할필요는.....",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015790.1.20",
          "form": "현금좀 빠방하게 드리먄...",
          "original_form": "현금좀 빠방하게 드리먄...",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015790.1.21",
          "form": "그래도 약간 손수끓인 미역국이 주는 느낌이 있자넠ㅋ",
          "original_form": "그래도 약간 손수끓인 미역국이 주는 느낌이 있자넠ㅋ",
          "speaker_id": "1",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015790.1.22",
          "form": "한번 도전해봐ㅋㅋㅋ",
          "original_form": "한번 도전해봐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015790.1.23",
          "form": "한상차림",
          "original_form": "한상차림",
          "speaker_id": "2",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015790.1.24",
          "form": "정말 큰도전인데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "정말 큰도전인데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015790.1.25",
          "form": "막 생일에 미역국 끓이다가 엄마아빠 다 나와볼듯... 불안해서..",
          "original_form": "막 생일에 미역국 끓이다가 엄마아빠 다 나와볼듯... 불안해서..",
          "speaker_id": "1",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015790.1.26",
          "form": "ㅋㅋㅋㅋ미역국 갈비 잡채...ㅡ",
          "original_form": "ㅋㅋㅋㅋ미역국 갈비 잡채...ㅡ",
          "speaker_id": "2",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015790.1.27",
          "form": "너 다할줄알아??",
          "original_form": "너 다할줄알아??",
          "speaker_id": "1",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015790.1.28",
          "form": "아니지ㅋㅋㅋㅋㅌ",
          "original_form": "아니지ㅋㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015790.1.29",
          "form": "안해봄ㅋㅋㅋ",
          "original_form": "안해봄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015790.1.30",
          "form": "먹을일이 걱정이라ㅋㅌ",
          "original_form": "먹을일이 걱정이라ㅋㅌ",
          "speaker_id": "2",
          "time": "20210826 01:18"
        }
      ]
    }
  ]
