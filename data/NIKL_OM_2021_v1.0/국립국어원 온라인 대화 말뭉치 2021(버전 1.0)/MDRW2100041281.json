      "utterance": [
        {
          "id": "MDRW2100041281.1.1",
          "form": "오늘 제가 루프탑으로만",
          "original_form": "오늘 제가 루프탑으로만",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100041281.1.2",
          "form": "카페랑 수제버거집을 다녀왔답니다",
          "original_form": "카페랑 수제버거집을 다녀왔답니다",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100041281.1.3",
          "form": "이야 시원하셨겠는데요?",
          "original_form": "이야 시원하셨겠는데요?",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041281.1.4",
          "form": "수제버거집은 어땠어요?",
          "original_form": "수제버거집은 어땠어요?",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041281.1.5",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041281.1.6",
          "form": "더백푸드트럭 <- 여기였는데",
          "original_form": "더백푸드트럭 <- 여기였는데",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041281.1.7",
          "form": "그 앞쪽에 사람들이",
          "original_form": "그 앞쪽에 사람들이",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041281.1.8",
          "form": "다 서서 사진을 엄청 찍는거예요",
          "original_form": "다 서서 사진을 엄청 찍는거예요",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041281.1.9",
          "form": "오 왜요?",
          "original_form": "오 왜요?",
          "speaker_id": "2",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100041281.1.10",
          "form": "처음엔 연예인이 온 줄 알았는데 그게 아니고",
          "original_form": "처음엔 연예인이 온 줄 알았는데 그게 아니고",
          "speaker_id": "1",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100041281.1.11",
          "form": "일몰이 너무 예뻐서",
          "original_form": "일몰이 너무 예뻐서",
          "speaker_id": "1",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100041281.1.12",
          "form": "다들 하늘 사진 찍으려고 그런거더라구요!!",
          "original_form": "다들 하늘 사진 찍으려고 그런거더라구요!!",
          "speaker_id": "1",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100041281.1.13",
          "form": "우와 ㅠ name1님도 찍으셨어요?",
          "original_form": "우와 ㅠ &name1&님도 찍으셨어요?",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100041281.1.14",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041281.1.15",
          "form": "하 여기에 사진을 보낼 수 있다면 좋을텐데",
          "original_form": "하 여기에 사진을 보낼 수 있다면 좋을텐데",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041281.1.16",
          "form": "하여튼,, 제 앞에 웨이팅 20팀이 있었는데",
          "original_form": "하여튼,, 제 앞에 웨이팅 20팀이 있었는데",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041281.1.17",
          "form": "1시간 정도 돌아다니면서 기다려서",
          "original_form": "1시간 정도 돌아다니면서 기다려서",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041281.1.18",
          "form": "들어가서 먹었답니다",
          "original_form": "들어가서 먹었답니다",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041281.1.19",
          "form": "사진 ㅠ 아쉽네요 1시간이라니 으아 맛이없을수가 없다ㅋㅋㅋㅋㅋ 기다리는거 힘들지 않으셨어요?",
          "original_form": "사진 ㅠ 아쉽네요 1시간이라니 으아 맛이없을수가 없다ㅋㅋㅋㅋㅋ 기다리는거 힘들지 않으셨어요?",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100041281.1.20",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100041281.1.21",
          "form": "사실 힘들었어요ㅠ",
          "original_form": "사실 힘들었어요ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100041281.1.22",
          "form": "핸드폰 배터리도 없고...",
          "original_form": "핸드폰 배터리도 없고...",
          "speaker_id": "1",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100041281.1.23",
          "form": "ㅠㅠㅠ 식사는 만족스러우셨어요?",
          "original_form": "ㅠㅠㅠ 식사는 만족스러우셨어요?",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100041281.1.24",
          "form": "네네! 그리고 친절하셔서",
          "original_form": "네네! 그리고 친절하셔서",
          "speaker_id": "1",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041281.1.25",
          "form": "핸드폰도 충전해주시고",
          "original_form": "핸드폰도 충전해주시고",
          "speaker_id": "1",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041281.1.26",
          "form": "버거도 진짜 맛있었어요!!!",
          "original_form": "버거도 진짜 맛있었어요!!!",
          "speaker_id": "1",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041281.1.27",
          "form": "치킨버거 였어요?",
          "original_form": "치킨버거 였어요?",
          "speaker_id": "2",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100041281.1.28",
          "form": "원래 처음엔 치킨 버거 먹으려다가",
          "original_form": "원래 처음엔 치킨 버거 먹으려다가",
          "speaker_id": "1",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100041281.1.29",
          "form": "아무래도 처음에 왔을 땐",
          "original_form": "아무래도 처음에 왔을 땐",
          "speaker_id": "1",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100041281.1.30",
          "form": "시그니처를 먹어야한다는 생각에",
          "original_form": "시그니처를 먹어야한다는 생각에",
          "speaker_id": "1",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100041281.1.31",
          "form": "The 100 버거를 먹었어요!!!",
          "original_form": "The 100 버거를 먹었어요!!!",
          "speaker_id": "1",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100041281.1.32",
          "form": "진짜 맛있었어요",
          "original_form": "진짜 맛있었어요",
          "speaker_id": "1",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100041281.1.33",
          "form": "오호 수제버거들은 엄청 크잖아요",
          "original_form": "오호 수제버거들은 엄청 크잖아요",
          "speaker_id": "2",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100041281.1.34",
          "form": "다 드셨나요!",
          "original_form": "다 드셨나요!",
          "speaker_id": "2",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100041281.1.35",
          "form": "그럼요 그럼요",
          "original_form": "그럼요 그럼요",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100041281.1.36",
          "form": "버거도 먹고",
          "original_form": "버거도 먹고",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100041281.1.37",
          "form": "제로콜라도 마시고",
          "original_form": "제로콜라도 마시고",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100041281.1.38",
          "form": "그리고...!!!",
          "original_form": "그리고...!!!",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100041281.1.39",
          "form": "감튀도 먹고",
          "original_form": "감튀도 먹고",
          "speaker_id": "1",
          "time": "20210905 21:43"
        }
      ]
    }
  ]
