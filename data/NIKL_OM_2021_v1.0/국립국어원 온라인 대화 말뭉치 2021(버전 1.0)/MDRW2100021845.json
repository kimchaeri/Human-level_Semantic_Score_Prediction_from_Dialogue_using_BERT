      "utterance": [
        {
          "id": "MDRW2100021845.1.1",
          "form": "제주에 공연있남?",
          "original_form": "제주에 공연있남?",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100021845.1.2",
          "form": "부산은 해변에서 모래조각 축제하는디",
          "original_form": "부산은 해변에서 모래조각 축제하는디",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100021845.1.3",
          "form": "몰라 그냥 보고싶은거를 쓰면됌",
          "original_form": "몰라 그냥 보고싶은거를 쓰면됌",
          "speaker_id": "1",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021845.1.4",
          "form": "가장 마지막에 본 전시가 머지?",
          "original_form": "가장 마지막에 본 전시가 머지?",
          "speaker_id": "1",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021845.1.5",
          "form": "나는..응..음...피카소전",
          "original_form": "나는..응..음...피카소전",
          "speaker_id": "2",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021845.1.6",
          "form": "아 피카소전이구나",
          "original_form": "아 피카소전이구나",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021845.1.7",
          "form": "그건 안갔나",
          "original_form": "그건 안갔나",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021845.1.8",
          "form": "불가리 보석전인가",
          "original_form": "불가리 보석전인가",
          "speaker_id": "1",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100021845.1.9",
          "form": "노웅ㅜㅜ",
          "original_form": "노웅ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100021845.1.10",
          "form": "표 안삼ㅜㅜ",
          "original_form": "표 안삼ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100021845.1.11",
          "form": "그 전시는 끝났남",
          "original_form": "그 전시는 끝났남",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021845.1.12",
          "form": "아직 하고있능감 몰겠네 나는 장신구안차자넝",
          "original_form": "아직 하고있능감 몰겠네 나는 장신구안차자넝",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021845.1.13",
          "form": "존잘님전시 가고싶어용",
          "original_form": "존잘님전시 가고싶어용",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021845.1.14",
          "form": "이제 2차까지 맞았으니 가면되겠다",
          "original_form": "이제 2차까지 맞았으니 가면되겠다",
          "speaker_id": "1",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100021845.1.15",
          "form": "오 나는 건강맨인데 옮기지않으려나ㅜ",
          "original_form": "오 나는 건강맨인데 옮기지않으려나ㅜ",
          "speaker_id": "2",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021845.1.16",
          "form": "난 다음주 김택화미술관에 가보려고",
          "original_form": "난 다음주 김택화미술관에 가보려고",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100021845.1.17",
          "form": "옮기는게 또 문제구만",
          "original_form": "옮기는게 또 문제구만",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100021845.1.18",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100021845.1.19",
          "form": "부러우시",
          "original_form": "부러우시",
          "speaker_id": "2",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100021845.1.20",
          "form": "제주풍경만그리셨다네",
          "original_form": "제주풍경만그리셨다네",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021845.1.21",
          "form": "근데 이해가 가는게",
          "original_form": "근데 이해가 가는게",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021845.1.22",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021845.1.23",
          "form": "나도 진짜 사직안찍는데",
          "original_form": "나도 진짜 사직안찍는데",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021845.1.24",
          "form": "찍고있고",
          "original_form": "찍고있고",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021845.1.25",
          "form": "감탄하게됨",
          "original_form": "감탄하게됨",
