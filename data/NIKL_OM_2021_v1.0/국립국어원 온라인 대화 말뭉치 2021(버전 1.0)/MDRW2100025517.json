      "utterance": [
        {
          "id": "MDRW2100025517.1.1",
          "form": "점심 뭐 먹었어요??",
          "original_form": "점심 뭐 먹었어요??",
          "speaker_id": "1",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100025517.1.2",
          "form": "난 짬뽕 먹고 왔음",
          "original_form": "난 짬뽕 먹고 왔음",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100025517.1.3",
          "form": "떡볶이",
          "original_form": "떡볶이",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100025517.1.4",
          "form": "오늘은 분식ㅋ",
          "original_form": "오늘은 분식ㅋ",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100025517.1.5",
          "form": "오호 맛있겠군",
          "original_form": "오호 맛있겠군",
          "speaker_id": "1",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025517.1.6",
          "form": "언니 떡볶이\r\n먹고싶다ㄸ만 ㅋㅋㅋㅋ",
          "original_form": "언니 떡볶이\r\n먹고싶다ㄸ만 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025517.1.7",
          "form": "언니떡볶이다ㅋㅋ",
          "original_form": "언니떡볶이다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025517.1.8",
          "form": "오랜만에 먹는다 여기",
          "original_form": "오랜만에 먹는다 여기",
          "speaker_id": "2",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100025517.1.9",
          "form": "돌핑이나 대박 주로먹었는듸ㅣ",
          "original_form": "돌핑이나 대박 주로먹었는듸ㅣ",
          "speaker_id": "2",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100025517.1.10",
          "form": "아 오늘 이름이\r\n언니떡볶이가\r\nㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 웃기노 ㅋㅋㅋㅋㄱ",
          "original_form": "아 오늘 이름이\r\n언니떡볶이가\r\nㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 웃기노 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025517.1.11",
          "form": "난 언니를 얘기한건데 ㅋㅋㅋㅋ",
          "original_form": "난 언니를 얘기한건데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025517.1.12",
          "form": "그언니가 아니고 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "그언니가 아니고 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025517.1.13",
          "form": "오케이마트안에 언니 떡볶이잖아",
          "original_form": "오케이마트안에 언니 떡볶이잖아",
          "speaker_id": "2",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025517.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025517.1.15",
          "form": "내가한거먹고싶다고?",
          "original_form": "내가한거먹고싶다고?",
          "speaker_id": "2",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025517.1.16",
          "form": "아 거기 ㅋㅋㅋㅋㅋ",
          "original_form": "아 거기 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025517.1.17",
          "form": "언니야떡볶이잖아",
          "original_form": "언니야떡볶이잖아",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025517.1.18",
          "form": "왜 한글자 빼는데\r\nㅋㅋㅋㄱㄱㅋㄱㄱㄱㅋㅋ",
          "original_form": "왜 한글자 빼는데\r\nㅋㅋㅋㄱㄱㅋㄱㄱㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025517.1.19",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025517.1.20",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025517.1.21",
          "form": "언니가 며칠전에 떡볶이 먹고 싶다했잖아",
          "original_form": "언니가 며칠전에 떡볶이 먹고 싶다했잖아",
          "speaker_id": "1",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100025517.1.22",
          "form": "그래서 오늘 먹었다길래",
          "original_form": "그래서 오늘 먹었다길래",
          "speaker_id": "1",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100025517.1.23",
          "form": "(name2)언니 떡볶이 먹고 싶다더니",
          "original_form": "(&name2&)언니 떡볶이 먹고 싶다더니",
          "speaker_id": "1",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100025517.1.24",
          "form": "이말 이라고 ㅋㅋㅋㅋ",
          "original_form": "이말 이라고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100025517.1.25",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100025517.1.26",
          "form": "그래 오늘 먹었다 드디어ㅋ",
          "original_form": "그래 오늘 먹었다 드디어ㅋ",
          "speaker_id": "2",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100025517.1.27",
          "form": "이래의사소통안되가 우짜노",
          "original_form": "이래의사소통안되가 우짜노",
          "speaker_id": "2",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100025517.1.28",
          "form": "푸하하",
          "original_form": "푸하하",
          "speaker_id": "2",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100025517.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래 언니 ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래 언니 ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100025517.1.30",
          "form": "대화좀 잘해보자 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "대화좀 잘해보자 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100025517.1.31",
          "form": "오늘 저녁은 뭐먹슈??",
          "original_form": "오늘 저녁은 뭐먹슈??",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100025517.1.32",
          "form": "난 어제\r\n먹던거 다시 먹어야겠다",
          "original_form": "난 어제\r\n먹던거 다시 먹어야겠다",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100025517.1.33",
          "form": "삼계탕",
          "original_form": "삼계탕",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100025517.1.34",
          "form": "난 고민이다 진짜",
          "original_form": "난 고민이다 진짜",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025517.1.35",
          "form": "미역국생각하고있는듸ㅣ",
          "original_form": "미역국생각하고있는듸ㅣ",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025517.1.36",
          "form": "이것도 질린다",
          "original_form": "이것도 질린다",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025517.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025517.1.38",
          "form": "미역국",
          "original_form": "미역국",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025517.1.39",
          "form": "끓여 놓고",
          "original_form": "끓여 놓고",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025517.1.40",
          "form": "냉장고 안 넣어 놔서",
          "original_form": "냉장고 안 넣어 놔서",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025517.1.41",
          "form": "다음날 바로 상했음",
          "original_form": "다음날 바로 상했음",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025517.1.42",
          "form": "오늘 소고기 사 가서 내일 다시 끓이려고",
          "original_form": "오늘 소고기 사 가서 내일 다시 끓이려고",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025517.1.43",
          "form": "하루만에?",
          "original_form": "하루만에?",
          "speaker_id": "2",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100025517.1.44",
          "form": "설마",
          "original_form": "설마",
          "speaker_id": "2",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100025517.1.45",
          "form": "난 하루는 가던데",
          "original_form": "난 하루는 가던데",
          "speaker_id": "2",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100025517.1.46",
          "form": "엉 엄마가 오전에 끓여놓고 내가 오후에 안데우고 투 밥말아준다고 뒤적거려놔서 그런듯",
          "original_form": "엉 엄마가 오전에 끓여놓고 내가 오후에 안데우고 투 밥말아준다고 뒤적거려놔서 그런듯",
          "speaker_id": "1",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025517.1.47",
          "form": "아까비 ㅜㅠ",
          "original_form": "아까비 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025517.1.48",
          "form": "ㅜㅜ 아깝다 증말",
          "original_form": "ㅜㅜ 아깝다 증말",
          "speaker_id": "2",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100025517.1.49",
          "form": "나가볼까",
          "original_form": "나가볼까",
          "speaker_id": "2",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025517.1.50",
          "form": "집에 뭐시 아무것도없닺",
          "original_form": "집에 뭐시 아무것도없닺",
          "speaker_id": "2",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025517.1.51",
          "form": "커피사러가고싶노",
          "original_form": "커피사러가고싶노",
          "speaker_id": "2",
          "time": "20210901 15:59"
        }
      ]
