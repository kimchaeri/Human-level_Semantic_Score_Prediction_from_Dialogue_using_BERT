      "utterance": [
        {
          "id": "MDRW2100017942.1.1",
          "form": "미세먼지가 별로 없네요",
          "original_form": "미세먼지가 별로 없네요",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017942.1.2",
          "form": "일기예보 보시는 편이신가요?",
          "original_form": "일기예보 보시는 편이신가요?",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017942.1.3",
          "form": "네 거의 매일 봐요",
          "original_form": "네 거의 매일 봐요",
          "speaker_id": "1",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100017942.1.4",
          "form": "오늘은 날이 좋았어요",
          "original_form": "오늘은 날이 좋았어요",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100017942.1.5",
          "form": "내일도 좋겠지요?",
          "original_form": "내일도 좋겠지요?",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100017942.1.6",
          "form": "그랬으면 좋겠어요!",
          "original_form": "그랬으면 좋겠어요!",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100017942.1.7",
          "form": "다만 여기는 비소식이 있더라구요ㅜㅠ",
          "original_form": "다만 여기는 비소식이 있더라구요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100017942.1.8",
          "form": "여기도 내내 비소식이ㅠ",
          "original_form": "여기도 내내 비소식이ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100017942.1.9",
          "form": "ㅜㅜ우산 잘 챙겨가세요",
          "original_form": "ㅜㅜ우산 잘 챙겨가세요",
          "speaker_id": "2",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100017942.1.10",
          "form": "비 너무귀찮네요",
          "original_form": "비 너무귀찮네요",
          "speaker_id": "1",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100017942.1.11",
          "form": "그러게요ㅠ 오늘 날 좋으니 좋았었는데말이죠",
          "original_form": "그러게요ㅠ 오늘 날 좋으니 좋았었는데말이죠",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100017942.1.12",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100017942.1.13",
          "form": "그래도 비 그치면 더 가을다워질거같아요",
          "original_form": "그래도 비 그치면 더 가을다워질거같아요",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100017942.1.14",
          "form": "맞아요",
