      "utterance": [
        {
          "id": "MDRW2100013151.1.1",
          "form": "안녕하세요 다이어트? 하시나요",
          "original_form": "안녕하세요 다이어트? 하시나요",
          "speaker_id": "1",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100013151.1.2",
          "form": "네 다이어트해요!",
          "original_form": "네 다이어트해요!",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013151.1.3",
          "form": "요즘에는 홈트를 하죠ㅎ",
          "original_form": "요즘에는 홈트를 하죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013151.1.4",
          "form": "저도 홈트해요ㅋㅋ",
          "original_form": "저도 홈트해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013151.1.5",
          "form": "영상보고 하시나요?",
          "original_form": "영상보고 하시나요?",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013151.1.6",
          "form": "네ㅋㅋ땅끄부부라고 아시나요?",
          "original_form": "네ㅋㅋ땅끄부부라고 아시나요?",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013151.1.7",
          "form": "아 들어만 봤어요",
          "original_form": "아 들어만 봤어요",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013151.1.8",
          "form": "난이도가 있는편인가요?",
          "original_form": "난이도가 있는편인가요?",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013151.1.9",
          "form": "난이도가 쉬운 것도 있구 어려운 것도 있어요ㅋㅋ",
          "original_form": "난이도가 쉬운 것도 있구 어려운 것도 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013151.1.10",
          "form": "햇별님은 영상보고하세요?",
          "original_form": "햇별님은 영상보고하세요?",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013151.1.11",
          "form": "아하 저도 보면서 해요 혼자서는 안해서ㅋㅋ",
          "original_form": "아하 저도 보면서 해요 혼자서는 안해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013151.1.12",
          "form": "저는 somifit님꺼 보고 해요!",
          "original_form": "저는 somifit님꺼 보고 해요!",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013151.1.13",
          "form": "오...찾아보니까 갑자기 운동하고싶어지네요ㅋㅋㅋㅋ",
          "original_form": "오...찾아보니까 갑자기 운동하고싶어지네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013151.1.14",
          "form": "얼마나 하셨어요?",
          "original_form": "얼마나 하셨어요?",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013151.1.15",
          "form": "저는 한 몇달 했어요! 어렵지않아서 자주하게되더라구요",
          "original_form": "저는 한 몇달 했어요! 어렵지않아서 자주하게되더라구요",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013151.1.16",
          "form": "효과 많이 보셨겠네요...!",
          "original_form": "효과 많이 보셨겠네요...!",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013151.1.17",
          "form": "많이는 아니에요ㅋㅋ 해봐야 10분 20분?밖에 안해서요",
          "original_form": "많이는 아니에요ㅋㅋ 해봐야 10분 20분?밖에 안해서요",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013151.1.18",
          "form": "매일하시는거죠?",
          "original_form": "매일하시는거죠?",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013151.1.19",
          "form": "하루걸러서 해요 ㅎ",
          "original_form": "하루걸러서 해요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 15:44"
