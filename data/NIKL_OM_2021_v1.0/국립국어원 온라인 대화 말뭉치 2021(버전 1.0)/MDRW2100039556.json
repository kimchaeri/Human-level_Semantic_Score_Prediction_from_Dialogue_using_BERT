      "utterance": [
        {
          "id": "MDRW2100039556.1.1",
          "form": "팔도 비빔면이요 ㅎ",
          "original_form": "팔도 비빔면이요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 19:01"
        },
        {
          "id": "MDRW2100039556.1.2",
          "form": "아 맛있었겠다",
          "original_form": "아 맛있었겠다",
          "speaker_id": "1",
          "time": "20210903 19:01"
        },
        {
          "id": "MDRW2100039556.1.3",
          "form": "맛 없어요",
          "original_form": "맛 없어요",
          "speaker_id": "2",
          "time": "20210903 19:01"
        },
        {
          "id": "MDRW2100039556.1.4",
          "form": "헉 왜요?,",
          "original_form": "헉 왜요?,",
          "speaker_id": "1",
          "time": "20210903 19:01"
        },
        {
          "id": "MDRW2100039556.1.5",
          "form": "입맛에 안 맞더라고요",
          "original_form": "입맛에 안 맞더라고요",
          "speaker_id": "2",
          "time": "20210903 19:01"
        },
        {
          "id": "MDRW2100039556.1.6",
          "form": "요즘 좀 심심해지긴 했죠",
          "original_form": "요즘 좀 심심해지긴 했죠",
          "speaker_id": "1",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039556.1.7",
          "form": "네 밍밍하고",
          "original_form": "네 밍밍하고",
          "speaker_id": "2",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039556.1.8",
          "form": "느끼해요",
          "original_form": "느끼해요",
          "speaker_id": "2",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039556.1.9",
          "form": "라면이 좀 느끼하죠",
          "original_form": "라면이 좀 느끼하죠",
          "speaker_id": "1",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039556.1.10",
          "form": "네 후회돼요",
          "original_form": "네 후회돼요",
          "speaker_id": "2",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039556.1.11",
          "form": "김치랑 같이 먹어야 좀 나은데",
          "original_form": "김치랑 같이 먹어야 좀 나은데",
          "speaker_id": "1",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039556.1.12",
          "form": "김치도 다 먹어서 ㅠㅠ",
          "original_form": "김치도 다 먹어서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039556.1.13",
          "form": "헉 ㅠ  아쉽네요",
          "original_form": "헉 ㅠ  아쉽네요",
          "speaker_id": "1",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039556.1.14",
          "form": "남겼다가 버렸어요..",
          "original_form": "남겼다가 버렸어요..",
          "speaker_id": "2",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039556.1.15",
          "form": "아깝지만 ㅜ",
          "original_form": "아깝지만 ㅜ",
          "speaker_id": "1",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039556.1.16",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
