      "utterance": [
        {
          "id": "MDRW2100040805.1.1",
          "form": "괜찮다고만 하고..",
          "original_form": "괜찮다고만 하고..",
          "speaker_id": "1",
          "time": "20210905 07:57"
        },
        {
          "id": "MDRW2100040805.1.2",
          "form": "그쳐ㅠㅠ 막 아픈건 아니니",
          "original_form": "그쳐ㅠㅠ 막 아픈건 아니니",
          "speaker_id": "2",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100040805.1.3",
          "form": "참 애매해요",
          "original_form": "참 애매해요",
          "speaker_id": "2",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100040805.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100040805.1.5",
          "form": "딱히 많이아픈게아니고 낮엔 괜찮고",
          "original_form": "딱히 많이아픈게아니고 낮엔 괜찮고",
          "speaker_id": "1",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100040805.1.6",
          "form": "저도 학생때 제가 아침저녁으로 휴지달고 ㅋㅋ 괴로워하니 엄마가",
          "original_form": "저도 학생때 제가 아침저녁으로 휴지달고 ㅋㅋ 괴로워하니 엄마가",
          "speaker_id": "2",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100040805.1.7",
          "form": "뭐라고",
          "original_form": "뭐라고",
          "speaker_id": "1",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100040805.1.8",
          "form": "혼자 부실하다 놀렷으요ㅠㅠ ㅋㅋ",
          "original_form": "혼자 부실하다 놀렷으요ㅠㅠ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100040805.1.9",
          "form": "하셨나요?",
          "original_form": "하셨나요?",
          "speaker_id": "1",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100040805.1.10",
          "form": "저희집에 저만 이래서",
          "original_form": "저희집에 저만 이래서",
          "speaker_id": "2",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100040805.1.11",
          "form": "병원가라하셧죠ㅠㅠ",
          "original_form": "병원가라하셧죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100040805.1.12",
          "form": "아~~저희집도 아들 혼자서만",
          "original_form": "아~~저희집도 아들 혼자서만",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100040805.1.13",
          "form": "전 첨에 몰랏어여ㅠㅠ",
          "original_form": "전 첨에 몰랏어여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100040805.1.14",
          "form": "병원도 몇군데 가봣어요",
          "original_form": "병원도 몇군데 가봣어요",
          "speaker_id": "2",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100040805.1.15",
          "form": "계속 킁킁거리고 재채기하고",
          "original_form": "계속 킁킁거리고 재채기하고",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100040805.1.16",
          "form": "계절성 비염이네요 진짜 아침저녁에 괴롭고 이쯤에 증상이오면 ㅠㅠ",
          "original_form": "계절성 비염이네요 진짜 아침저녁에 괴롭고 이쯤에 증상이오면 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100040805.1.17",
          "form": "전 봄도 별로에요 꽃알러지도 잇어서",
          "original_form": "전 봄도 별로에요 꽃알러지도 잇어서",
          "speaker_id": "2",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100040805.1.18",
          "form": "진짜 심할땐 꽃집지날때도 난리낫엇엇여ㅠㅠ",
          "original_form": "진짜 심할땐 꽃집지날때도 난리낫엇엇여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100040805.1.19",
          "form": "오늘 약보다는 병원데려가서 일주일치 약 져와야겠네요",
          "original_form": "오늘 약보다는 병원데려가서 일주일치 약 져와야겠네요",
          "speaker_id": "1",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100040805.1.20",
          "form": "많이 힘든거였어요",
          "original_form": "많이 힘든거였어요",
          "speaker_id": "1",
          "time": "20210905 08:00"
        }
