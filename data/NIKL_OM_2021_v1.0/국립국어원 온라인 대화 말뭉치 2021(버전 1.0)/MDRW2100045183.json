      "utterance": [
        {
          "id": "MDRW2100045183.1.1",
          "form": "넷플릭스 같은것도 보시나요?",
          "original_form": "넷플릭스 같은것도 보시나요?",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045183.1.2",
          "form": "햄님 많이 추천하시니까 한번 물어봐요 ㅎㅎ",
          "original_form": "햄님 많이 추천하시니까 한번 물어봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045183.1.3",
          "form": "저 지금은안보는데 넷플 보긴봤었어요",
          "original_form": "저 지금은안보는데 넷플 보긴봤었어요",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045183.1.4",
          "form": "name2님은여",
          "original_form": "&name2&님은여",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045183.1.5",
          "form": "저는 지금도 넷플 많이봐요",
          "original_form": "저는 지금도 넷플 많이봐요",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045183.1.6",
          "form": "어떤게 재밌었어요?",
          "original_form": "어떤게 재밌었어요?",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045183.1.7",
          "form": "친구들이랑 나눠내서 한 3500정도라 안봐도 유지가 편해서요",
          "original_form": "친구들이랑 나눠내서 한 3500정도라 안봐도 유지가 편해서요",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045183.1.8",
          "form": "러시아 인형처럼이요",
          "original_form": "러시아 인형처럼이요",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045183.1.9",
          "form": "ㅋㅋㅋ그쳐 저도 그렇게했었는데 패드가고장나서 ㅎㅎ",
          "original_form": "ㅋㅋㅋ그쳐 저도 그렇게했었는데 패드가고장나서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045183.1.10",
          "form": "뭔가 무서운장르인가여?",
          "original_form": "뭔가 무서운장르인가여?",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045183.1.11",
          "form": "약간 시간루프물이요",
          "original_form": "약간 시간루프물이요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045183.1.12",
          "form": "주인공이 30살 생일에 계속 죽고 돌아와서 원인을 알아내는 내용이예요",
          "original_form": "주인공이 30살 생일에 계속 죽고 돌아와서 원인을 알아내는 내용이예요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045183.1.13",
          "form": "오 재밌을것같아요",
          "original_form": "오 재밌을것같아요",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045183.1.14",
          "form": "그 해피데스데이인가",
          "original_form": "그 해피데스데이인가",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045183.1.15",
          "form": "그것도 생각나네여",
          "original_form": "그것도 생각나네여",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045183.1.16",
          "form": "그 영화도 살아 돌아왔던가요?",
          "original_form": "그 영화도 살아 돌아왔던가요?",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045183.1.17",
          "form": "죽음채로 있는 공포영화인줄 알았어요 그거..",
          "original_form": "죽음채로 있는 공포영화인줄 알았어요 그거..",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045183.1.18",
          "form": "계속 살해당하는데 생일날로돌아오는스토리였어요",
          "original_form": "계속 살해당하는데 생일날로돌아오는스토리였어요",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045183.1.19",
          "form": "와 비슷하네요 미국인들이 즐기는 소재인가봐요 ㅋㅋ",
          "original_form": "와 비슷하네요 미국인들이 즐기는 소재인가봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045183.1.20",
          "form": "ㅋㅋㅋㅋㅋ그런가봐여 미국건가보네여?",
          "original_form": "ㅋㅋㅋㅋㅋ그런가봐여 미국건가보네여?",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045183.1.21",
          "form": "러시안인형은 주인공이 허무주의적인 태도를 버리고 다시 살려고 노력하는 엔딩이어서 특별히 무섭지는 안았던거같아요",
          "original_form": "러시안인형은 주인공이 허무주의적인 태도를 버리고 다시 살려고 노력하는 엔딩이어서 특별히 무섭지는 안았던거같아요",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045183.1.22",
          "form": "오 뭔가교훈적인내용이네여",
          "original_form": "오 뭔가교훈적인내용이네여",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045183.1.23",
          "form": "미국인들은 왜 생일에 인생의 위기를 맞는걸 좋아하는걸까요 ㅋㅋㅋ",
          "original_form": "미국인들은 왜 생일에 인생의 위기를 맞는걸 좋아하는걸까요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045183.1.24",
          "form": "저 해피데스데이는 비급영화였어요 ㅋㅋㅋㅋ",
          "original_form": "저 해피데스데이는 비급영화였어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:16"
        }
      ]
    }
