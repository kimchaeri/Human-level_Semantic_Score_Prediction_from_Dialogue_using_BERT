      "utterance": [
        {
          "id": "MDRW2100045384.1.1",
          "form": "우유는 어디꺼드세용?",
          "original_form": "우유는 어디꺼드세용?",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045384.1.2",
          "form": "서울우유 먹어요",
          "original_form": "서울우유 먹어요",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045384.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045384.1.4",
          "form": "저두요!ㅋㅋ 여러우유 먹었는데 서울우유가 제일 맛잇어요",
          "original_form": "저두요!ㅋㅋ 여러우유 먹었는데 서울우유가 제일 맛잇어요",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045384.1.5",
          "form": "원래 남양 마셧으나...",
          "original_form": "원래 남양 마셧으나...",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045384.1.6",
          "form": "ㅋㅋㅋ......",
          "original_form": "ㅋㅋㅋ......",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045384.1.7",
          "form": "아.....그쵸....ㅜㅜ 초코에몽 맛있엇는데..",
          "original_form": "아.....그쵸....ㅜㅜ 초코에몽 맛있엇는데..",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045384.1.8",
          "form": "원래 남양 맛잇는우유 마셧엇는데 ㅋㅋㅋㅋㅋ",
          "original_form": "원래 남양 맛잇는우유 마셧엇는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045384.1.9",
          "form": "갈아탓어요 ㅋㅋㅋㅋㅋ",
          "original_form": "갈아탓어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045384.1.10",
          "form": "저도 그거 가끔먹었었는데..ㅜㅜ",
          "original_form": "저도 그거 가끔먹었었는데..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045384.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ음료는 우유만 주로 드세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ음료는 우유만 주로 드세요?",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045384.1.12",
          "form": "아니죵 요구르트 나랑드사이다 등등",
          "original_form": "아니죵 요구르트 나랑드사이다 등등",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045384.1.13",
          "form": "아 생각해보니 주스는 잘 안먹었네용?!",
          "original_form": "아 생각해보니 주스는 잘 안먹었네용?!",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045384.1.14",
          "form": "커피나 차는요?",
          "original_form": "커피나 차는요?",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045384.1.15",
          "form": "커피는 잘안즐겨요 커피숍가면 스무디나 에이드ㅋㅋ",
          "original_form": "커피는 잘안즐겨요 커피숍가면 스무디나 에이드ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045384.1.16",
          "form": "커피좋아하세용??",
          "original_form": "커피좋아하세용??",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045384.1.17",
          "form": "저는 커피 좋아하는데 카페인에 예민해요 ㅋㅋㅋㅋ",
          "original_form": "저는 커피 좋아하는데 카페인에 예민해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045384.1.18",
          "form": "앗 이런 안타까운ㅜㅜㅜ",
          "original_form": "앗 이런 안타까운ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045384.1.19",
          "form": "심장이 두그두근하고 그런거에요?",
          "original_form": "심장이 두그두근하고 그런거에요?",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045384.1.20",
          "form": "네 그리고 밤에 잠을 못자요",
          "original_form": "네 그리고 밤에 잠을 못자요",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045384.1.21",
          "form": "저 저번에 큰텀블러에 라떼 네개 타먹고 죽을뻔했어요ㅋㅋ",
          "original_form": "저 저번에 큰텀블러에 라떼 네개 타먹고 죽을뻔했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045384.1.22",
          "form": "울렁거리고 두근거리고ㅋㅋ",
          "original_form": "울렁거리고 두근거리고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045384.1.23",
          "form": "조심하세요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "조심하세요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045384.1.24",
          "form": "라떼 네개나... ㄷㄷ",
          "original_form": "라떼 네개나... ㄷㄷ",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045384.1.25",
          "form": "텀블러가 커서 채우고 싶어서ㅋㅋㅋ카페인에 혼이 안나봐가지고 무서운줄몰랐어요",
