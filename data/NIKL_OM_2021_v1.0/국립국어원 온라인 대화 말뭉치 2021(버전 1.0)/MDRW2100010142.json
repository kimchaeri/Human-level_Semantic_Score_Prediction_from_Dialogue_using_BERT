      "utterance": [
        {
          "id": "MDRW2100010142.1.1",
          "form": "이상형 있어?",
          "original_form": "이상형 있어?",
          "speaker_id": "1",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010142.1.2",
          "form": "일단 말이 잘 통하는 사람!",
          "original_form": "일단 말이 잘 통하는 사람!",
          "speaker_id": "2",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100010142.1.3",
          "form": "맞아 말 통하기가 쉽지 않아",
          "original_form": "맞아 말 통하기가 쉽지 않아",
          "speaker_id": "1",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100010142.1.4",
          "form": "맞아...그리고 잘 경청해주는 사람",
          "original_form": "맞아...그리고 잘 경청해주는 사람",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010142.1.5",
          "form": "나는 자기할일 잘하는 사람이 좋아",
          "original_form": "나는 자기할일 잘하는 사람이 좋아",
          "speaker_id": "1",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010142.1.6",
          "form": "그 점도 중요하지, 앞가림 혼자 하는 것도 능력이니까",
          "original_form": "그 점도 중요하지, 앞가림 혼자 하는 것도 능력이니까",
          "speaker_id": "2",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010142.1.7",
          "form": "그것도 그렇고 열중하는 모습들에 반할 때가 있거든",
          "original_form": "그것도 그렇고 열중하는 모습들에 반할 때가 있거든",
          "speaker_id": "1",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010142.1.8",
          "form": "너는 순간 순간 깨닫게 되는 거야?",
          "original_form": "너는 순간 순간 깨닫게 되는 거야?",
          "speaker_id": "1",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010142.1.9",
          "form": "아 이 사람이랑 잘 통하네 하고 좋아하게 되는?",
          "original_form": "아 이 사람이랑 잘 통하네 하고 좋아하게 되는?",
          "speaker_id": "1",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010142.1.10",
          "form": "확실히 처음 부터 알긴 힘들고 지내면서 알게 되는 것 같아",
          "original_form": "확실히 처음 부터 알긴 힘들고 지내면서 알게 되는 것 같아",
          "speaker_id": "2",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010142.1.11",
          "form": "근데 점점 호감이 쌓였다가 말 몇 마디에 정이 떨어지기도 하고 그래ㅋㅋ",
          "original_form": "근데 점점 호감이 쌓였다가 말 몇 마디에 정이 떨어지기도 하고 그래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010142.1.12",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100010142.1.13",
          "form": "지금 연애 하고 있어?",
          "original_form": "지금 연애 하고 있어?",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100010142.1.14",
          "form": "지금은 쉬어가는 중, 너는?",
          "original_form": "지금은 쉬어가는 중, 너는?",
          "speaker_id": "2",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100010142.1.15",
          "form": "나도 연애 안 하고 있어",
          "original_form": "나도 연애 안 하고 있어",
          "speaker_id": "1",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100010142.1.16",
          "form": "요즘은 새로운 사람을 만들기도 애매하지ㅠ",
          "original_form": "요즘은 새로운 사람을 만들기도 애매하지ㅠ",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100010142.1.17",
          "form": "왜왜 어떤 점에서?",
          "original_form": "왜왜 어떤 점에서?",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100010142.1.18",
          "form": "미팅이나 소개팅을 나가기도 그러니까 요즘은",
          "original_form": "미팅이나 소개팅을 나가기도 그러니까 요즘은",
          "speaker_id": "2",
          "time": "20210825 19:10"
        }
      ]
