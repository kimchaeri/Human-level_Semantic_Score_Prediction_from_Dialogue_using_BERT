      "utterance": [
        {
          "id": "MDRW2100045516.1.1",
          "form": "백화점이 목표인가 바요",
          "original_form": "백화점이 목표인가 바요",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045516.1.2",
          "form": "마트에 좀비가 너무",
          "original_form": "마트에 좀비가 너무",
          "speaker_id": "1",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045516.1.3",
          "form": "차 세우지도 못할 듯 요",
          "original_form": "차 세우지도 못할 듯 요",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045516.1.4",
          "form": "어그로 끄는 중",
          "original_form": "어그로 끄는 중",
          "speaker_id": "1",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045516.1.5",
          "form": "귀여우심 ㅋㅋㅋㅋ",
          "original_form": "귀여우심 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045516.1.6",
          "form": "좀비가 달려들진 않네요",
          "original_form": "좀비가 달려들진 않네요",
          "speaker_id": "1",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045516.1.7",
          "form": "모이면 꿀 잼 인데",
          "original_form": "모이면 꿀 잼 인데",
          "speaker_id": "2",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045516.1.8",
          "form": "일이는 순간 죽을 듯",
          "original_form": "일이는 순간 죽을 듯",
          "speaker_id": "1",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045516.1.9",
          "form": "내리는",
          "original_form": "내리는",
          "speaker_id": "1",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100045516.1.10",
          "form": "와 개미때같아요",
          "original_form": "와 개미때같아요",
          "speaker_id": "1",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100045516.1.11",
          "form": "너무 무서운데요 ㄷㄷ",
          "original_form": "너무 무서운데요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045516.1.12",
          "form": "저래서 살아남겠는지",
          "original_form": "저래서 살아남겠는지",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045516.1.13",
          "form": "생존겜 그 자체 ㄷㄷ",
          "original_form": "생존겜 그 자체 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045516.1.14",
          "form": "어그로 효과가 있는가봐요",
          "original_form": "어그로 효과가 있는가봐요",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045516.1.15",
          "form": "긔가 아프긴한 ㅜㅜㅋㅋ",
          "original_form": "긔가 아프긴한 ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045516.1.16",
          "form": "많으니까 렉이걸리네요",
          "original_form": "많으니까 렉이걸리네요",
          "speaker_id": "1",
          "time": "20210908 03:09"
        }
