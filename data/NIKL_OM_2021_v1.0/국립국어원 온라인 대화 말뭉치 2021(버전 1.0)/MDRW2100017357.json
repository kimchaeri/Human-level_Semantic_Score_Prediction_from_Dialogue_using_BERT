      "utterance": [
        {
          "id": "MDRW2100017357.1.1",
          "form": "요새 슬의생 보고 있어요!",
          "original_form": "요새 슬의생 보고 있어요!",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017357.1.2",
          "form": "저도 보고 있어요",
          "original_form": "저도 보고 있어요",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017357.1.3",
          "form": "누굴 제일 좋아하세요?",
          "original_form": "누굴 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017357.1.4",
          "form": "정원이죠 제 원픽은 정원입니다",
          "original_form": "정원이죠 제 원픽은 정원입니다",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017357.1.5",
          "form": "저는 준완이요 ㅋㅋㅋㅋ",
          "original_form": "저는 준완이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017357.1.6",
          "form": "제 친구도 원픽이 츤데레 준완이라더라구요",
          "original_form": "제 친구도 원픽이 츤데레 준완이라더라구요",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017357.1.7",
          "form": "오늘도 하는데 기대가 됩니다!",
          "original_form": "오늘도 하는데 기대가 됩니다!",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017357.1.8",
          "form": "밖에서는 츤츤거려도 여자친구에겐 세상 다정한 모습이 너무 좋아요",
          "original_form": "밖에서는 츤츤거려도 여자친구에겐 세상 다정한 모습이 너무 좋아요",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017357.1.9",
          "form": "그리고 정경호 배우가 호감이라 더 좋은것도 잇는것같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "그리고 정경호 배우가 호감이라 더 좋은것도 잇는것같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017357.1.10",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017357.1.11",
          "form": "정경호 배우님이 호감이긴 하죠",
          "original_form": "정경호 배우님이 호감이긴 하죠",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017357.1.12",
          "form": "라이프온마스를 너무 재밌게봐서 호감이 됐어요 ㅋㅋㅋ",
          "original_form": "라이프온마스를 너무 재밌게봐서 호감이 됐어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017357.1.13",
          "form": "막 챙겨본건 아닌제",
          "original_form": "막 챙겨본건 아닌제",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017357.1.14",
          "form": "가끔 본적있어요!",
          "original_form": "가끔 본적있어요!",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017357.1.15",
          "form": "나중에 시간날때 한번 각잡고 보세요 재밌어요",
          "original_form": "나중에 시간날때 한번 각잡고 보세요 재밌어요",
          "speaker_id": "1",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017357.1.16",
          "form": "그래야겠네요!",
          "original_form": "그래야겠네요!",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017357.1.17",
          "form": "슬의생말고 다른드라마도 보세요?",
          "original_form": "슬의생말고 다른드라마도 보세요?",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017357.1.18",
          "form": "요즘에는 안봐요!",
          "original_form": "요즘에는 안봐요!",
          "speaker_id": "2",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017357.1.19",
          "form": "저도 요즘엔 안보는것같아요 ㅋㅋㅋㅋ",
          "original_form": "저도 요즘엔 안보는것같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017357.1.20",
          "form": "한때는 드라마 엄청 열심히 봣는뎈ㅋㅋㅋ",
          "original_form": "한때는 드라마 엄청 열심히 봣는뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017357.1.21",
          "form": "요새 딱히 볼 드라마가 없는것같아서.. 안보게 되더라구요",
          "original_form": "요새 딱히 볼 드라마가 없는것같아서.. 안보게 되더라구요",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017357.1.22",
          "form": "인생드라마 있으세요?",
          "original_form": "인생드라마 있으세요?",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017357.1.23",
          "form": "저 시그널이욤",
          "original_form": "저 시그널이욤",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017357.1.24",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017357.1.25",
          "form": "시그널 저도 엄청 좋아햇어요 ㅠㅠ",
          "original_form": "시그널 저도 엄청 좋아햇어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017357.1.26",
          "form": "진짜 재밌었는데..",
          "original_form": "진짜 재밌었는데..",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017357.1.27",
          "form": "시그널2 금방 나올 줄 알았는데 ㅠㅠ",
          "original_form": "시그널2 금방 나올 줄 알았는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017357.1.28",
          "form": "마자요...",
          "original_form": "마자요...",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017357.1.29",
          "form": "언제쯤 나올까요...",
          "original_form": "언제쯤 나올까요...",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017357.1.30",
          "form": "언제 나올까요 진짜 ㅠㅠ",
          "original_form": "언제 나올까요 진짜 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017357.1.31",
          "form": "그니까요...",
          "original_form": "그니까요...",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017357.1.32",
          "form": "마지막을 그렇게 끝내놓고 ㅠㅠ!!",
          "original_form": "마지막을 그렇게 끝내놓고 ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017357.1.33",
          "form": "시즌 2 내놔요!",
          "original_form": "시즌 2 내놔요!",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017357.1.34",
          "form": "너무 궁금하단 말이에요!",
          "original_form": "너무 궁금하단 말이에요!",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017357.1.35",
          "form": "너무 궁금해서 현기증 ㅠㅠ",
          "original_form": "너무 궁금해서 현기증 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017357.1.36",
          "form": "현기증나서 쓰러질것 같으니 얼른 시즌2...",
