        },
        {
          "id": "MDRW2100045362.1.2",
          "form": "그렇게 노선 다른 캐 하나쯤 있는것도",
          "original_form": "그렇게 노선 다른 캐 하나쯤 있는것도",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.3",
          "form": "실버크로스",
          "original_form": "실버크로스",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.4",
          "form": "재미지",
          "original_form": "재미지",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.5",
          "form": "민기는",
          "original_form": "민기는",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.6",
          "form": "엇 그거",
          "original_form": "엇 그거",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.7",
          "form": "안지우더라고여???",
          "original_form": "안지우더라고여???",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.9",
          "form": "없어?",
          "original_form": "없어?",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.10",
          "form": "조는 어케지우는거야",
          "original_form": "조는 어케지우는거야",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.12",
          "form": "그 짧은 순간에",
          "original_form": "그 짧은 순간에",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.13",
          "form": "언제지우고나왔담",
          "original_form": "언제지우고나왔담",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.14",
          "form": "ㅁ민기는 안지우더라고?",
          "original_form": "ㅁ민기는 안지우더라고?",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045362.1.15",
          "form": "뀨는 기억이안나고...",
          "original_form": "뀨는 기억이안나고...",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045362.1.16",
          "form": "지웠던거같은데",
          "original_form": "지웠던거같은데",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045362.1.17",
          "form": "넘오 신기할뿐",
          "original_form": "넘오 신기할뿐",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045362.1.18",
          "form": "아 안지우는거구나",
          "original_form": "아 안지우는거구나",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045362.1.19",
          "form": "아니 대체",
          "original_form": "아니 대체",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045362.1.20",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045362.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045362.1.22",
          "form": "어케 지우는거지",
          "original_form": "어케 지우는거지",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045362.1.23",
          "form": "아니 그게 토미라서",
          "original_form": "아니 그게 토미라서",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045362.1.24",
          "form": "하고있는건데",
          "original_form": "하고있는건데",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045362.1.25",
          "form": "헤드윅이갑자기하고있으니까",
          "original_form": "헤드윅이갑자기하고있으니까",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045362.1.26",
          "form": "당황스럽긴하더라고",
