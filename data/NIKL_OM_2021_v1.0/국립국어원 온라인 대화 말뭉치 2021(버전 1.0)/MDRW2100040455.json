        {
          "id": "MDRW2100040455.1.1",
          "form": "네 언니 남동생",
          "original_form": "네 언니 남동생",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100040455.1.2",
          "form": "여동생이에요???",
          "original_form": "여동생이에요???",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100040455.1.3",
          "form": "아니요 저는 여자고 동생은 남동생이에요ㅠㅠ",
          "original_form": "아니요 저는 여자고 동생은 남동생이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100040455.1.4",
          "form": "저도 언니갖고 싶어요ㅠㅠㅠㅠㅠ",
          "original_form": "저도 언니갖고 싶어요ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100040455.1.5",
          "form": "아... 남동생....진짜 쓸데없죠...",
          "original_form": "아... 남동생....진짜 쓸데없죠...",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100040455.1.6",
          "form": "결혼하면 더더욱 남이 된다고ㅜ",
          "original_form": "결혼하면 더더욱 남이 된다고ㅜ",
          "speaker_id": "2",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100040455.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아웃곀ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요ㅋㅋㅋㅋㅋ쓸데없죠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아웃곀ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요ㅋㅋㅋㅋㅋ쓸데없죠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100040455.1.8",
          "form": "헐 ㅠㅠ 그럴것같아요 결혼하면 ㅠㅠㅠ",
          "original_form": "헐 ㅠㅠ 그럴것같아요 결혼하면 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100040455.1.9",
          "form": "아닌 경우도 간혹있지만...",
          "original_form": "아닌 경우도 간혹있지만...",
          "speaker_id": "2",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100040455.1.10",
          "form": "언니나 여동생이 진짜 어릴때나 커서나 최고 좋은것같아요ㅠㅠㅠ",
          "original_form": "언니나 여동생이 진짜 어릴때나 커서나 최고 좋은것같아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100040455.1.11",
          "form": "저희집은 남자우대라ㅋㅋ 부모님은 아니라고하시지만요",
          "original_form": "저희집은 남자우대라ㅋㅋ 부모님은 아니라고하시지만요",
          "speaker_id": "2",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100040455.1.12",
          "form": "클수록 그런거같아요",
          "original_form": "클수록 그런거같아요",
          "speaker_id": "2",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100040455.1.13",
          "form": "부모님세대까지는 아직 남아선호가 조금은 남아있는것같아요ㅠ",
          "original_form": "부모님세대까지는 아직 남아선호가 조금은 남아있는것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040455.1.14",
          "form": "그놈의 제사가 뭔지 ㅋㅋㅋㅋ",
          "original_form": "그놈의 제사가 뭔지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040455.1.15",
          "form": "그러니까요...ㅜㅠ 근데 언니랑 저는 비혼에 가까워서 남동생이 빨리 장가갔음 좋겠어요",
          "original_form": "그러니까요...ㅜㅠ 근데 언니랑 저는 비혼에 가까워서 남동생이 빨리 장가갔음 좋겠어요",
          "speaker_id": "2",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100040455.1.16",
          "form": "ㅋㅋㅋ맞아요 그냥 동생이 대를(?) 잇고 ㅋㅋㅋ 부모님 수금(?)도 동생이 맡아주고 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ맞아요 그냥 동생이 대를(?) 잇고 ㅋㅋㅋ 부모님 수금(?)도 동생이 맡아주고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:51"
        },
        {
          "id": "MDRW2100040455.1.17",
          "form": "ㅋㅋ 조카 다섯명 낳았으면!!! ㅋㅋ",
          "original_form": "ㅋㅋ 조카 다섯명 낳았으면!!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100040455.1.18",
          "form": "용돈은 많이 줄테니",
          "original_form": "용돈은 많이 줄테니",
          "speaker_id": "2",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100040455.1.19",
          "form": "ㅋㅋㅋ정말요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ정말요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100040455.1.20",
          "form": "저도 비혼인데 나이먹고가 조금 걱정인데",
          "original_form": "저도 비혼인데 나이먹고가 조금 걱정인데",
          "speaker_id": "1",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100040455.1.21",
          "form": "언니가 있으면 든든하시겠어요 ㅠㅠ",
          "original_form": "언니가 있으면 든든하시겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100040455.1.22",
          "form": "ㅜㅠ의지가 되긴해요",
          "original_form": "ㅜㅠ의지가 되긴해요",
