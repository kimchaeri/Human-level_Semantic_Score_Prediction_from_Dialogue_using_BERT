      "utterance": [
        {
          "id": "MDRW2100037583.1.1",
          "form": "점심 맛있게 먹었어요?",
          "original_form": "점심 맛있게 먹었어요?",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100037583.1.2",
          "form": "배부르다ㅋ",
          "original_form": "배부르다ㅋ",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100037583.1.3",
          "form": "아아 드링킹중",
          "original_form": "아아 드링킹중",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100037583.1.4",
          "form": "다이어트 하는가 보네요ㅋㅋㅋ",
          "original_form": "다이어트 하는가 보네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:17"
        },
        {
          "id": "MDRW2100037583.1.5",
          "form": "아아 잡수시는 거 보니",
          "original_form": "아아 잡수시는 거 보니",
          "speaker_id": "1",
          "time": "20210901 13:18"
        },
        {
          "id": "MDRW2100037583.1.6",
          "form": "라떼가업네 집에ㅋㅋ",
          "original_form": "라떼가업네 집에ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100037583.1.7",
          "form": "오늘은 카누 아아",
          "original_form": "오늘은 카누 아아",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100037583.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ없는거 였어 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ없는거 였어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037583.1.9",
          "form": "오늘 1일인데 ㅋㅋㅋ",
          "original_form": "오늘 1일인데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037583.1.10",
          "form": "다요트 시작합니꺼??",
          "original_form": "다요트 시작합니꺼??",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100037583.1.11",
          "form": "그래야되는데",
          "original_form": "그래야되는데",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037583.1.12",
          "form": "현실은 지금 라떼드링킹증",
          "original_form": "현실은 지금 라떼드링킹증",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100037583.1.13",
          "form": "난 오늘 땀을 너무 많이 흘렸더니 입맛도 없네요",
          "original_form": "난 오늘 땀을 너무 많이 흘렸더니 입맛도 없네요",
          "speaker_id": "1",
          "time": "20210901 17:41"
        },
        {
          "id": "MDRW2100037583.1.14",
          "form": "이 기세를 몰아 저녁을 먹지 않겠어요",
          "original_form": "이 기세를 몰아 저녁을 먹지 않겠어요",
          "speaker_id": "1",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100037583.1.15",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037583.1.16",
          "form": "정말?",
          "original_form": "정말?",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037583.1.17",
          "form": "나 라면먹을꺼야ㅎㅎ",
          "original_form": "나 라면먹을꺼야ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037583.1.18",
          "form": "밥도말거야ㅋㅋ",
          "original_form": "밥도말거야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037583.1.19",
          "form": "언니 시급 폭발했네요ㅋㅋㅋㅋ",
          "original_form": "언니 시급 폭발했네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037583.1.20",
          "form": "식욕 ㅋㅋㅋㅋㅋ",
          "original_form": "식욕 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037583.1.21",
          "form": "ㅋㅇㅋㅋ 그러게 울신랑있으니 더 먹게되노ㅋㅋ",
          "original_form": "ㅋㅇㅋㅋ 그러게 울신랑있으니 더 먹게되노ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037583.1.22",
          "form": "백신맞고와서 계속 징징",
          "original_form": "백신맞고와서 계속 징징",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037583.1.23",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037583.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ오늘은 애가 셋이구만요~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ오늘은 애가 셋이구만요~",
          "speaker_id": "1",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037583.1.25",
          "form": "맛있게 먹고 일찍자요 ㅋㅋㅋㅋㅋ",
          "original_form": "맛있게 먹고 일찍자요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037583.1.26",
          "form": "더 징징하기 전에 ㅋㅋㅋㅋ",
          "original_form": "더 징징하기 전에 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037583.1.27",
          "form": "어ㅋ 그래야지ㅋㅋ",
          "original_form": "어ㅋ 그래야지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100037583.1.28",
          "form": "애들오면 밥먹고 오늘 얼른 쫑!",
          "original_form": "애들오면 밥먹고 오늘 얼른 쫑!",
          "speaker_id": "2",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100037583.1.29",
          "form": "역시 사람마음 똑같음 ㅋㅋㅋㅋ",
          "original_form": "역시 사람마음 똑같음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100037583.1.30",
          "form": "애들 아직 안왔어여?",
          "original_form": "애들 아직 안왔어여?",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100037583.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100037583.1.32",
          "form": "땅콩먹고싶다하더니 사와서 삶고있다 울신랑 직접",
          "original_form": "땅콩먹고싶다하더니 사와서 삶고있다 울신랑 직접",
          "speaker_id": "2",
          "time": "20210901 18:15"
        }
      ]
    }
