      "utterance": [
        {
          "id": "MDRW2100006548.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100006548.1.2",
          "form": "네 날씨가 추워요",
          "original_form": "네 날씨가 추워요",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100006548.1.3",
          "form": "맞아요 거기도 비가 오나요?",
          "original_form": "맞아요 거기도 비가 오나요?",
          "speaker_id": "1",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100006548.1.4",
          "form": "비는 안오는데 바람이 차요",
          "original_form": "비는 안오는데 바람이 차요",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100006548.1.5",
          "form": "여기도 비는 그쳤는데 바람이 많이 불어요",
          "original_form": "여기도 비는 그쳤는데 바람이 많이 불어요",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100006548.1.6",
          "form": "요즘 옷차림은 어떠세요",
          "original_form": "요즘 옷차림은 어떠세요",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100006548.1.7",
          "form": "반팔로는 아침저녁엔 춥네요",
          "original_form": "반팔로는 아침저녁엔 춥네요",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100006548.1.8",
          "form": "헉 아직 반팔만 입으세요?",
          "original_form": "헉 아직 반팔만 입으세요?",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100006548.1.9",
          "form": "네...^^;",
          "original_form": "네...^^;",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100006548.1.10",
          "form": "ㅋㅋㅋㅋㅋ 일교차가 커서 감기 조심하셔야 겠어요",
          "original_form": "ㅋㅋㅋㅋㅋ 일교차가 커서 감기 조심하셔야 겠어요",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100006548.1.11",
          "form": "저는 긴팔 긴바지 입고 이써요",
          "original_form": "저는 긴팔 긴바지 입고 이써요",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100006548.1.12",
          "form": "오늘은 뭐라도 더 걸쳤어야 했나 생각했답니다",
          "original_form": "오늘은 뭐라도 더 걸쳤어야 했나 생각했답니다",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100006548.1.13",
          "form": "그렇군요ㅠ아이들 외투도 꼭 입혀야겠어요",
          "original_form": "그렇군요ㅠ아이들 외투도 꼭 입혀야겠어요",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100006548.1.14",
          "form": "맞아요 아이들은 특히나 체온조절을 잘 못하니까 어른들이 신경써야죠",
          "original_form": "맞아요 아이들은 특히나 체온조절을 잘 못하니까 어른들이 신경써야죠",
          "speaker_id": "1",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100006548.1.15",
          "form": "네.환절기에 감기에 쉽게 걸리더라구요",
          "original_form": "네.환절기에 감기에 쉽게 걸리더라구요",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006548.1.16",
          "form": "감기라고 쉽게 볼게 아니에요ㅠ",
          "original_form": "감기라고 쉽게 볼게 아니에요ㅠ",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006548.1.17",
          "form": "진짜 조심해야해요",
          "original_form": "진짜 조심해야해요",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006548.1.18",
