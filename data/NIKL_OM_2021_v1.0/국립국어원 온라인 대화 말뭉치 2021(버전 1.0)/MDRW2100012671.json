      "utterance": [
        {
          "id": "MDRW2100012671.1.1",
          "form": "즐겨보는 프로그램이 있나요",
          "original_form": "즐겨보는 프로그램이 있나요",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100012671.1.2",
          "form": "펜트 하우스 전참시 놀면 뭐하니요",
          "original_form": "펜트 하우스 전참시 놀면 뭐하니요",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100012671.1.3",
          "form": "나혼자산다\n뭉쳐야찬다  슬기로운 의사생활 을 봅니다",
          "original_form": "나혼자산다\n뭉쳐야찬다  슬기로운 의사생활 을 봅니다",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100012671.1.4",
          "form": "아 슬기로운 1 봤어요",
          "original_form": "아 슬기로운 1 봤어요",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100012671.1.5",
          "form": "2는 놓쳐서 못봤어요",
          "original_form": "2는 놓쳐서 못봤어요",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100012671.1.6",
          "form": "1 , 2탄 모두 보고 있어요",
          "original_form": "1 , 2탄 모두 보고 있어요",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100012671.1.7",
          "form": "잔잔하니 잼나고",
          "original_form": "잔잔하니 잼나고",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100012671.1.8",
          "form": "ost 넘좋구요",
          "original_form": "ost 넘좋구요",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100012671.1.9",
          "form": "맨날 들었어요 1때",
          "original_form": "맨날 들었어요 1때",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100012671.1.10",
          "form": "내용이 흥미롭고 노래도 좋아요",
          "original_form": "내용이 흥미롭고 노래도 좋아요",
          "speaker_id": "2",
          "time": "20210827 06:46"
        },
        {
          "id": "MDRW2100012671.1.11",
          "form": "네 다들 노래 실력도 너무 좋아요",
          "original_form": "네 다들 노래 실력도 너무 좋아요",
          "speaker_id": "1",
          "time": "20210827 06:49"
        },
        {
          "id": "MDRW2100012671.1.12",
          "form": "그 전미도랑 조정석 어떻게 사귀나요?",
          "original_form": "그 전미도랑 조정석 어떻게 사귀나요?",
          "speaker_id": "1",
          "time": "20210827 06:51"
        },
        {
          "id": "MDRW2100012671.1.13",
          "form": "저 앞부분 보니 전미도가 거절하던데",
          "original_form": "저 앞부분 보니 전미도가 거절하던데",
          "speaker_id": "1",
          "time": "20210827 06:51"
        },
        {
          "id": "MDRW2100012671.1.14",
          "form": "아니요 그냥 좋은 감정있는 친구로 지내요",
          "original_form": "아니요 그냥 좋은 감정있는 친구로 지내요",
          "speaker_id": "2",
          "time": "20210827 06:53"
        },
        {
          "id": "MDRW2100012671.1.15",
          "form": "아 그렇군요 오히려 쿨하니 좋네요 ㅎㅎ",
          "original_form": "아 그렇군요 오히려 쿨하니 좋네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:03"
        },
        {
          "id": "MDRW2100012671.1.16",
          "form": "날잡아 정주행 해봐야 겠어요",
          "original_form": "날잡아 정주행 해봐야 겠어요",
          "speaker_id": "1",
          "time": "20210827 07:03"
        },
        {
          "id": "MDRW2100012671.1.17",
          "form": "전 전미도가 노래 엄청 잘하는데 음치연기 너무 재밌어요",
          "original_form": "전 전미도가 노래 엄청 잘하는데 음치연기 너무 재밌어요",
          "speaker_id": "1",
          "time": "20210827 07:03"
        },
        {
          "id": "MDRW2100012671.1.18",
          "form": "여전히 음치컨셉이네요",
          "original_form": "여전히 음치컨셉이네요",
          "speaker_id": "2",
          "time": "20210827 07:11"
        },
        {
          "id": "MDRW2100012671.1.19",
          "form": "ㅋㅋ진짜 근질근질 하겠어요",
          "original_form": "ㅋㅋ진짜 근질근질 하겠어요",
          "speaker_id": "1",
          "time": "20210827 07:11"
        },
        {
          "id": "MDRW2100012671.1.20",
          "form": "노래 잘하고 싶을텐데",
          "original_form": "노래 잘하고 싶을텐데",
          "speaker_id": "1",
          "time": "20210827 07:11"
        },
        {
          "id": "MDRW2100012671.1.21",
          "form": "응치 노래하는것 방송 나왔는데 그래도 완전음치는 아닌것 같이 잘하더라고요",
          "original_form": "응치 노래하는것 방송 나왔는데 그래도 완전음치는 아닌것 같이 잘하더라고요",
          "speaker_id": "2",
          "time": "20210827 07:30"
        },
        {
          "id": "MDRW2100012671.1.22",
          "form": "삑사리하고 고음불가만 ?",
          "original_form": "삑사리하고 고음불가만 ?",
          "speaker_id": "2",
          "time": "20210827 07:30"
        },
        {
          "id": "MDRW2100012671.1.23",
          "form": "오 제가 볼때는 완전 음치였는데",
          "original_form": "오 제가 볼때는 완전 음치였는데",
          "speaker_id": "1",
          "time": "20210827 07:30"
        },
        {
          "id": "MDRW2100012671.1.24",
          "form": "좀 변화가 있었나요",
          "original_form": "좀 변화가 있었나요",
          "speaker_id": "1",
          "time": "20210827 07:31"
        },
        {
          "id": "MDRW2100012671.1.25",
          "form": "아 그렇긴해요 ㅎㅎ",
          "original_form": "아 그렇긴해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:31"
        },
        {
          "id": "MDRW2100012671.1.26",
          "form": "진짜 연기자는 연기자예요 반대연기 ㅎㅎ",
