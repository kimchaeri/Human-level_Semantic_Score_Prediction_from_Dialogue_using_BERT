      "utterance": [
        {
          "id": "MDRW2100019807.1.1",
          "form": "오븐 없이 프라이팬으로 만드는 뜯어먹는 빵 이라는 책이 있어요",
          "original_form": "오븐 없이 프라이팬으로 만드는 뜯어먹는 빵 이라는 책이 있어요",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019807.1.2",
          "form": "헐 그런 책이 있어요??",
          "original_form": "헐 그런 책이 있어요??",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019807.1.3",
          "form": "책만봤어요 언젠간하려고,,",
          "original_form": "책만봤어요 언젠간하려고,,",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019807.1.4",
          "form": "다른 가족들이 해주길 기다리며 책을 거실에 뒀는데",
          "original_form": "다른 가족들이 해주길 기다리며 책을 거실에 뒀는데",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019807.1.5",
          "form": "아무도 해주지 않았어요",
          "original_form": "아무도 해주지 않았어요",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019807.1.6",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019807.1.7",
          "form": "다른 가족들도 같은 마음이었을거예요..",
          "original_form": "다른 가족들도 같은 마음이었을거예요..",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019807.1.8",
          "form": "맞아요 저랑 비슷한 성격이라다들,, 하면 먹어는 줄께,,라고,,ㅋㅋㅋㅋ",
          "original_form": "맞아요 저랑 비슷한 성격이라다들,, 하면 먹어는 줄께,,라고,,ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019807.1.9",
          "form": "19년도에 나온 책인데 레시피는 괜찮아보였어요",
          "original_form": "19년도에 나온 책인데 레시피는 괜찮아보였어요",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019807.1.10",
          "form": "좋은... 건가요 ㅋㅋㅋㅋㅋ",
          "original_form": "좋은... 건가요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019807.1.11",
          "form": "노오븐 베이킹이라니 혹하긴 하네요",
          "original_form": "노오븐 베이킹이라니 혹하긴 하네요",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019807.1.12",
          "form": "ㅠㅠ 콩심은데 콩나는건 어쩔수없죠 ㅋㅋㅋㅋ",
          "original_form": "ㅠㅠ 콩심은데 콩나는건 어쩔수없죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019807.1.13",
          "form": "책 보고 해보셨어요?",
          "original_form": "책 보고 해보셨어요?",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019807.1.14",
          "form": "한 번 해봤는데 빵 같인 나왔어요",
          "original_form": "한 번 해봤는데 빵 같인 나왔어요",
          "speaker_id": "2",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019807.1.15",
          "form": "앗 ㅋㅋㅋㅋㅋ 그럼 성공이네요",
          "original_form": "앗 ㅋㅋㅋㅋㅋ 그럼 성공이네요",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019807.1.16",
          "form": "전 오븐 없어서 베이킹 생각 못하고 있엇는데 프라이팬이라니 함 찾아봐야겠어요",
          "original_form": "전 오븐 없어서 베이킹 생각 못하고 있엇는데 프라이팬이라니 함 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019807.1.17",
          "form": "하기 쉬워요 식사빵은 들어가는 재료도 적구요",
          "original_form": "하기 쉬워요 식사빵은 들어가는 재료도 적구요",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100019807.1.18",
          "form": "그치만 밥하기보단 귀찮단 점..",
          "original_form": "그치만 밥하기보단 귀찮단 점..",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100019807.1.19",
          "form": "아 ㅋㅋㅋㅋㅋ 그쵸 밥보다는 귀찮죠 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ 그쵸 밥보다는 귀찮죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100019807.1.20",
          "form": "이스트 같은것도 마트에서 사세요?",
          "original_form": "이스트 같은것도 마트에서 사세요?",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100019807.1.21",
          "form": "그래서 한 번 해보고 와..귀찮자 하고 끝..",
          "original_form": "그래서 한 번 해보고 와..귀찮자 하고 끝..",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100019807.1.22",
          "form": "이스트. 밀가루. 소금 바닐리향 정도는 큰 마트먄 팔고요 바닐라빈 이런걸 쓸라믄 온라인 쇼핑을 해야져",
          "original_form": "이스트. 밀가루. 소금 바닐리향 정도는 큰 마트먄 팔고요 바닐라빈 이런걸 쓸라믄 온라인 쇼핑을 해야져",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100019807.1.23",
          "form": "바닐라빈 들어갈 정도의 고급 베이킹은",
          "original_form": "바닐라빈 들어갈 정도의 고급 베이킹은",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019807.1.24",
          "form": "아직 먼 얘기인거같아요 ㅋㅋㅋㅋ",
          "original_form": "아직 먼 얘기인거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019807.1.25",
          "form": "푸딩은 한번 해보고 싶긴 해요",
          "original_form": "푸딩은 한번 해보고 싶긴 해요",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019807.1.26",
          "form": "푸딩이 의외로 어려웠어요",
          "original_form": "푸딩이 의외로 어려웠어요",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019807.1.27",
          "form": "계란찜이냐 푸딩이냐가 한끗차이더라고요",
          "original_form": "계란찜이냐 푸딩이냐가 한끗차이더라고요",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019807.1.28",
          "form": "그쵸 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019807.1.29",
          "form": "저도 달달한 계란찜 됐다는 후기 보고 선뜻 도전을 못하고 있어요",
          "original_form": "저도 달달한 계란찜 됐다는 후기 보고 선뜻 도전을 못하고 있어요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019807.1.30",
          "form": "이게 질감이 퍼석해도 계란찜이 되고",
          "original_form": "이게 질감이 퍼석해도 계란찜이 되고",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100019807.1.31",
          "form": "계란 비린내가 나도 계란찜이 되고..",
          "original_form": "계란 비린내가 나도 계란찜이 되고..",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100019807.1.32",
          "form": "계란이 쌀 때 함 도전해보세요",
          "original_form": "계란이 쌀 때 함 도전해보세요",
