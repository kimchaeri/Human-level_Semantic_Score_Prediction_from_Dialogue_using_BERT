      "utterance": [
        {
          "id": "MDRW2100011306.1.1",
          "form": "요즘 재밌는 프로나 드라마 있나요?",
          "original_form": "요즘 재밌는 프로나 드라마 있나요?",
          "speaker_id": "2",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100011306.1.2",
          "form": "저 요즘 드라마 즐겨보고있어요",
          "original_form": "저 요즘 드라마 즐겨보고있어요",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100011306.1.3",
          "form": "임성한 작가꺼 아시나요",
          "original_form": "임성한 작가꺼 아시나요",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100011306.1.4",
          "form": "아!! 그 무슨 이혼작곡인지 어쩌구,,ㅠ 제목이 기억안나네요",
          "original_form": "아!! 그 무슨 이혼작곡인지 어쩌구,,ㅠ 제목이 기억안나네요",
          "speaker_id": "2",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100011306.1.5",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011306.1.6",
          "form": "보시나요 혹시",
          "original_form": "보시나요 혹시",
          "speaker_id": "1",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011306.1.7",
          "form": "아뇨아뇨ㅜㅜ 요즘 드라마 잘 안봐서 보진 못했네용",
          "original_form": "아뇨아뇨ㅜㅜ 요즘 드라마 잘 안봐서 보진 못했네용",
          "speaker_id": "2",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011306.1.8",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100011306.1.9",
          "form": "앗 그러시구나",
          "original_form": "앗 그러시구나",
          "speaker_id": "1",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100011306.1.10",
          "form": "막장 드라마라 약간 욕하면서 보게 되는 뭔지 아시죠",
          "original_form": "막장 드라마라 약간 욕하면서 보게 되는 뭔지 아시죠",
          "speaker_id": "1",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100011306.1.11",
          "form": "ㅋㅋㅋㅋㄲ맞아요",
          "original_form": "ㅋㅋㅋㅋㄲ맞아요",
          "speaker_id": "2",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100011306.1.12",
          "form": "드라마 내용 매일 거지같다고 그래도",
          "original_form": "드라마 내용 매일 거지같다고 그래도",
          "speaker_id": "2",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100011306.1.13",
          "form": "욕하는재미에 보게되는 드라마 있죠ㅎㅎ",
          "original_form": "욕하는재미에 보게되는 드라마 있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100011306.1.14",
          "form": "그게 약간 막장에  맛인 것 같아요",
          "original_form": "그게 약간 막장에  맛인 것 같아요",
          "speaker_id": "1",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100011306.1.15",
          "form": "리틀 송 님은 즐겨 보는 거 있으세요",
          "original_form": "리틀 송 님은 즐겨 보는 거 있으세요",
          "speaker_id": "1",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100011306.1.16",
          "form": "요즘 즐겨보는 드라마는 슬기로운 의사생활이요!!",
          "original_form": "요즘 즐겨보는 드라마는 슬기로운 의사생활이요!!",
          "speaker_id": "2",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100011306.1.17",
          "form": "시즌1부터 꼭꼭 챙겨봤네용ㅎㅎ",
          "original_form": "시즌1부터 꼭꼭 챙겨봤네용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100011306.1.18",
          "form": "그거 재밌다고 하더라구요",
          "original_form": "그거 재밌다고 하더라구요",
          "speaker_id": "1",
          "time": "20210824 11:02"
        },
        {
          "id": "MDRW2100011306.1.19",
          "form": "네네 맞아요ㅎㅎ 혹시 시간되시면 한번 보시는거 추천드려요~",
          "original_form": "네네 맞아요ㅎㅎ 혹시 시간되시면 한번 보시는거 추천드려요~",
          "speaker_id": "2",
          "time": "20210824 11:02"
        },
        {
          "id": "MDRW2100011306.1.20",
          "form": "어떤 점이 매력 있었나요",
          "original_form": "어떤 점이 매력 있었나요",
          "speaker_id": "1",
          "time": "20210824 11:02"
        },
        {
          "id": "MDRW2100011306.1.21",
          "form": "심심풀이로 아무생각 없이 볼수 있고 주인공들도 매력있는 캐릭터인데 조연으로 나오는 인물들도 다 너무 매력있어서",
          "original_form": "심심풀이로 아무생각 없이 볼수 있고 주인공들도 매력있는 캐릭터인데 조연으로 나오는 인물들도 다 너무 매력있어서",
          "speaker_id": "2",
          "time": "20210824 11:03"
        },
        {
          "id": "MDRW2100011306.1.22",
          "form": "누구하나 안좋아할수없는것 같아요~",
          "original_form": "누구하나 안좋아할수없는것 같아요~",
          "speaker_id": "2",
          "time": "20210824 11:03"
        }
      ]
    }
