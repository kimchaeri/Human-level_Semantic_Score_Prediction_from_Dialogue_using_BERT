        {
          "id": "MDRW2100015888.1.1",
          "form": "두산베어스 좋아하시나봐요.",
          "original_form": "두산베어스 좋아하시나봐요.",
          "speaker_id": "2",
          "time": "20210826 02:24"
        },
        {
          "id": "MDRW2100015888.1.2",
          "form": "맞아요.",
          "original_form": "맞아요.",
          "speaker_id": "1",
          "time": "20210826 02:24"
        },
        {
          "id": "MDRW2100015888.1.3",
          "form": "좋아하는 야구팀 있으세요?",
          "original_form": "좋아하는 야구팀 있으세요?",
          "speaker_id": "1",
          "time": "20210826 02:24"
        },
        {
          "id": "MDRW2100015888.1.4",
          "form": "저는 원래 기아 타이거즈 팬이었어요",
          "original_form": "저는 원래 기아 타이거즈 팬이었어요",
          "speaker_id": "2",
          "time": "20210826 02:25"
        },
        {
          "id": "MDRW2100015888.1.5",
          "form": "지금은 야구를 잘 안 봐요",
          "original_form": "지금은 야구를 잘 안 봐요",
          "speaker_id": "2",
          "time": "20210826 02:25"
        },
        {
          "id": "MDRW2100015888.1.6",
          "form": "기아 예전에 해태였잖아요.",
          "original_form": "기아 예전에 해태였잖아요.",
          "speaker_id": "1",
          "time": "20210826 02:25"
        },
        {
          "id": "MDRW2100015888.1.7",
          "form": "성적도 좋았고...",
          "original_form": "성적도 좋았고...",
          "speaker_id": "1",
          "time": "20210826 02:25"
        },
        {
          "id": "MDRW2100015888.1.8",
          "form": "2017년인가 우승 했었는데",
          "original_form": "2017년인가 우승 했었는데",
          "speaker_id": "2",
          "time": "20210826 02:25"
        },
        {
          "id": "MDRW2100015888.1.9",
          "form": "그 이후로는 그닥이네요.",
          "original_form": "그 이후로는 그닥이네요.",
          "speaker_id": "2",
          "time": "20210826 02:25"
        },
        {
          "id": "MDRW2100015888.1.10",
          "form": "두산도 이제는 힘든가봐요ㅠㅠ",
          "original_form": "두산도 이제는 힘든가봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 02:26"
        },
        {
          "id": "MDRW2100015888.1.11",
          "form": "순위도 떨어지고",
          "original_form": "순위도 떨어지고",
          "speaker_id": "1",
          "time": "20210826 02:26"
        },
        {
          "id": "MDRW2100015888.1.12",
          "form": "선수들도 떠나고...",
          "original_form": "선수들도 떠나고...",
          "speaker_id": "1",
          "time": "20210826 02:26"
        },
        {
          "id": "MDRW2100015888.1.13",
          "form": "그동안 오래 1등해서 그런가봐요",
          "original_form": "그동안 오래 1등해서 그런가봐요",
          "speaker_id": "2",
          "time": "20210826 02:26"
        },
        {
          "id": "MDRW2100015888.1.14",
          "form": "맞아요ㅜㅜㅜ",
          "original_form": "맞아요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 02:26"
        },
        {
          "id": "MDRW2100015888.1.15",
          "form": "그래도 주변에 두산 팬들 많은 것 같아요.",
          "original_form": "그래도 주변에 두산 팬들 많은 것 같아요.",
          "speaker_id": "2",
          "time": "20210826 02:26"
        },
        {
          "id": "MDRW2100015888.1.16",
          "form": "제 주변에도 그래요!!",
          "original_form": "제 주변에도 그래요!!",
          "speaker_id": "1",
          "time": "20210826 02:27"
        },
        {
          "id": "MDRW2100015888.1.17",
          "form": "수도권 친구들 대부분 두산 아니면 엘지 팬이더라구요",
          "original_form": "수도권 친구들 대부분 두산 아니면 엘지 팬이더라구요",
          "speaker_id": "1",
          "time": "20210826 02:27"
        },
        {
          "id": "MDRW2100015888.1.18",
          "form": "키움은 팬 수가 적죠?",
          "original_form": "키움은 팬 수가 적죠?",
