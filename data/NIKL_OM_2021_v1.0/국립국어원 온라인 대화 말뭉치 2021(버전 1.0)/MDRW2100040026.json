      "utterance": [
        {
          "id": "MDRW2100040026.1.1",
          "form": "야식의시간이네요",
          "original_form": "야식의시간이네요",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100040026.1.2",
          "form": "야식 드셨나요?",
          "original_form": "야식 드셨나요?",
          "speaker_id": "1",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100040026.1.3",
          "form": "아뇨...그냥 생각만 ㅋㅋㅋㅋ",
          "original_form": "아뇨...그냥 생각만 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100040026.1.4",
          "form": "치킨 조아하세요?",
          "original_form": "치킨 조아하세요?",
          "speaker_id": "2",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100040026.1.5",
          "form": "네네 치맥도 좋아요 ㅎㅎ 좋아하시나요?",
          "original_form": "네네 치맥도 좋아요 ㅎㅎ 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100040026.1.6",
          "form": "그럼요 어디꺼조아하세요",
          "original_form": "그럼요 어디꺼조아하세요",
          "speaker_id": "2",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100040026.1.7",
          "form": "전 네네치킨이랑 치킨매니아요 ㅎㅎ",
          "original_form": "전 네네치킨이랑 치킨매니아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 01:19"
        },
        {
          "id": "MDRW2100040026.1.8",
          "form": "어디걸 주로 시키시나요?",
          "original_form": "어디걸 주로 시키시나요?",
          "speaker_id": "1",
          "time": "20210904 01:19"
        },
        {
          "id": "MDRW2100040026.1.9",
          "form": "굽네 비비큐요",
          "original_form": "굽네 비비큐요",
          "speaker_id": "2",
          "time": "20210904 01:21"
        },
        {
          "id": "MDRW2100040026.1.10",
          "form": "치킨매니아 맛잇어요? 메뉴머잇어요",
          "original_form": "치킨매니아 맛잇어요? 메뉴머잇어요",
          "speaker_id": "2",
          "time": "20210904 01:21"
        },
        {
          "id": "MDRW2100040026.1.11",
          "form": "마늘간장양념이랑 빠사칸치킨이라고 현미가루를 살짝 묻힌 치킨이 있는데 맛있어요 ㅋㅋㄱ",
          "original_form": "마늘간장양념이랑 빠사칸치킨이라고 현미가루를 살짝 묻힌 치킨이 있는데 맛있어요 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100040026.1.12",
          "form": "굽네에서는 주로 무얼 드시나요?",
          "original_form": "굽네에서는 주로 무얼 드시나요?",
          "speaker_id": "1",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100040026.1.13",
          "form": "오허ㅎㅎ...  무바야겟다",
          "original_form": "오허ㅎㅎ...  무바야겟다",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040026.1.14",
          "form": "오리지멀",
          "original_form": "오리지멀",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040026.1.15",
          "form": "고 추 바사삭이요",
          "original_form": "고 추 바사삭이요",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040026.1.16",
          "form": "고-추-바사삭.. 저도 먹어보고 싶어요",
          "original_form": "고-추-바사삭.. 저도 먹어보고 싶어요",
          "speaker_id": "1",
          "time": "20210904 01:29"
        },
        {
          "id": "MDRW2100040026.1.17",
          "form": "맛잇다는 이야기 많이 들었어요ㅋㅋㅋㅋ",
          "original_form": "맛잇다는 이야기 많이 들었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:29"
        },
        {
          "id": "MDRW2100040026.1.18",
          "form": "ㅋㅋㅋ 안먹어보셧나요",
          "original_form": "ㅋㅋㅋ 안먹어보셧나요",
          "speaker_id": "2",
          "time": "20210904 01:29"
        },
        {
          "id": "MDRW2100040026.1.19",
          "form": "맛있다는 이야기는 많이 들었어요 ㅋㅋㅋㅋ",
          "original_form": "맛있다는 이야기는 많이 들었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100040026.1.20",
          "form": "새벽에 오류가 있었는지 메시지가 안갔네요 ㅠ",
          "original_form": "새벽에 오류가 있었는지 메시지가 안갔네요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100040026.1.21",
          "form": "가끔 그러더라구요ㅋㅋㅋ 드셔보새용",
          "original_form": "가끔 그러더라구요ㅋㅋㅋ 드셔보새용",
          "speaker_id": "2",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100040026.1.22",
          "form": "네네 기회가 되면 먹어봐야겠어요.. 친구중에 치킨을 안 좋아하는 친구가 있다보니ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네네 기회가 되면 먹어봐야겠어요.. 친구중에 치킨을 안 좋아하는 친구가 있다보니ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100040026.1.23",
          "form": "저녁드셧나요?",
          "original_form": "저녁드셧나요?",
          "speaker_id": "1",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100040026.1.24",
          "form": "네 마라탕먹엇어요 ㅋㅋㅋ",
          "original_form": "네 마라탕먹엇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100040026.1.25",
          "form": "전 삼겹살구이요..",
          "original_form": "전 삼겹살구이요..",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100040026.1.26",
          "form": "주말이니까ㅋㅋㅋㅋ",
          "original_form": "주말이니까ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100040026.1.27",
          "form": "오 저는 토욜에! ㅋㅋㅋ",
          "original_form": "오 저는 토욜에! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100040026.1.28",
          "form": "삼겹살 또먹고싶네요",
          "original_form": "삼겹살 또먹고싶네요",
          "speaker_id": "2",
