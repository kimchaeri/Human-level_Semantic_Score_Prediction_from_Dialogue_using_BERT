      "utterance": [
        {
          "id": "MDRW2100002389.1.1",
          "form": "전 요번에 휴가 패쓰했어요",
          "original_form": "전 요번에 휴가 패쓰했어요",
          "speaker_id": "2",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002389.1.2",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002389.1.3",
          "form": "그냥 코로나도 있고 같이갈 남편이 아팠어요",
          "original_form": "그냥 코로나도 있고 같이갈 남편이 아팠어요",
          "speaker_id": "2",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002389.1.4",
          "form": "아 이런 ㅎㅎㅎ",
          "original_form": "아 이런 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002389.1.5",
          "form": "전 제가 휴가가 없어서 남편이 포기했어요",
          "original_form": "전 제가 휴가가 없어서 남편이 포기했어요",
          "speaker_id": "1",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002389.1.6",
          "form": "요번엔 마땅히 간곳은 없고 가까운곳 구경정도",
          "original_form": "요번엔 마땅히 간곳은 없고 가까운곳 구경정도",
          "speaker_id": "2",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002389.1.7",
          "form": "근교라도 갈까 생각했지만 코로나가 무서워요",
          "original_form": "근교라도 갈까 생각했지만 코로나가 무서워요",
          "speaker_id": "1",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002389.1.8",
          "form": "아쉽네요 ㅜㅜ맞아요 무서워요",
          "original_form": "아쉽네요 ㅜㅜ맞아요 무서워요",
          "speaker_id": "2",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002389.1.9",
          "form": "근데 뉴스보면 저빼고 다 여행가는 느낌",
          "original_form": "근데 뉴스보면 저빼고 다 여행가는 느낌",
          "speaker_id": "1",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002389.1.10",
          "form": "sns를 봐도 그렇고요 ㅎㅎ",
          "original_form": "sns를 봐도 그렇고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002389.1.11",
          "form": "코로나 직전에 대만 갔었는데 신의 한수였어요 ㅜㅜ",
          "original_form": "코로나 직전에 대만 갔었는데 신의 한수였어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002389.1.12",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "1",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002389.1.13",
          "form": "맞아요 저빼고 다갔어요",
          "original_form": "맞아요 저빼고 다갔어요",
          "speaker_id": "2",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002389.1.14",
          "form": "언제 가셨어요?",
          "original_form": "언제 가셨어요?",
          "speaker_id": "1",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002389.1.15",
          "form": "전 일본 ㅋㅋ",
          "original_form": "전 일본 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002389.1.16",
          "form": "2018년 겨울",
          "original_form": "2018년 겨울",
          "speaker_id": "2",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002389.1.17",
          "form": "가물가물하네요",
          "original_form": "가물가물하네요",
          "speaker_id": "2",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002389.1.18",
          "form": "겨울 대만 좋죠",
          "original_form": "겨울 대만 좋죠",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002389.1.19",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "2",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002389.1.20",
          "form": "취두부 냄새만 빼면 ㅋㅋ",
          "original_form": "취두부 냄새만 빼면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002389.1.21",
          "form": "저도 대만전에 일본 갔었어요",
          "original_form": "저도 대만전에 일본 갔었어요",
          "speaker_id": "2",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002389.1.22",
          "form": "ㅋㅋ맞아요 지날때마다",
          "original_form": "ㅋㅋ맞아요 지날때마다",
          "speaker_id": "2",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002389.1.23",
          "form": "자유여행가기 넘좋아요",
          "original_form": "자유여행가기 넘좋아요",
          "speaker_id": "2",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002389.1.24",
          "form": "ㅋㅋ 그쵸?",
          "original_form": "ㅋㅋ 그쵸?",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002389.1.25",
          "form": "진짜 코막고 다녔는데",
          "original_form": "진짜 코막고 다녔는데",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002389.1.26",
          "form": "중간에 버스투어 끼워 넣어서 했어요",
          "original_form": "중간에 버스투어 끼워 넣어서 했어요",
          "speaker_id": "2",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002389.1.27",
          "form": "ㅋㅋ코 맞아요 전 못먹어요",
          "original_form": "ㅋㅋ코 맞아요 전 못먹어요",
          "speaker_id": "2",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002389.1.28",
          "form": "버스 가이드 아저씨 너무 한국말을 잘해서 잼났어요",
          "original_form": "버스 가이드 아저씨 너무 한국말을 잘해서 잼났어요",
          "speaker_id": "2",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002389.1.29",
          "form": "ㅎㅎ 저도 취두부는 자신 없더라고요",
          "original_form": "ㅎㅎ 저도 취두부는 자신 없더라고요",
          "speaker_id": "1",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002389.1.30",
          "form": "네 먹는 사람이 많진 않을듯해요",
          "original_form": "네 먹는 사람이 많진 않을듯해요",
          "speaker_id": "2",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002389.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002389.1.32",
          "form": "화덕만두는 맛있었어요",
          "original_form": "화덕만두는 맛있었어요",
          "speaker_id": "1",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002389.1.33",
          "form": "내 전 곱창인가 들어있는 시장국수요",
          "original_form": "내 전 곱창인가 들어있는 시장국수요",
          "speaker_id": "2",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002389.1.34",
          "form": "와우 맛있겠다",
          "original_form": "와우 맛있겠다",
          "speaker_id": "1",
          "time": "20210820 11:06"
        },
        {
          "id": "MDRW2100002389.1.35",
          "form": "대만 야시장도 먹을게 은근 많죠",
          "original_form": "대만 야시장도 먹을게 은근 많죠",
          "speaker_id": "1",
          "time": "20210820 11:06"
        },
        {
          "id": "MDRW2100002389.1.36",
          "form": "네 근데 전 그날 잘못먹어서 아쉬웠어요",
          "original_form": "네 근데 전 그날 잘못먹어서 아쉬웠어요",
          "speaker_id": "2",
          "time": "20210820 11:06"
        },
        {
          "id": "MDRW2100002389.1.37",
          "form": "아프셨어요?",
          "original_form": "아프셨어요?",
          "speaker_id": "1",
          "time": "20210820 11:06"
        },
        {
          "id": "MDRW2100002389.1.38",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 11:07"
        },
        {
          "id": "MDRW2100002389.1.39",
          "form": "배가 좀 아팠어요 진짜 기대했는데",
          "original_form": "배가 좀 아팠어요 진짜 기대했는데",
          "speaker_id": "2",
          "time": "20210820 11:07"
        },
        {
          "id": "MDRW2100002389.1.40",
          "form": "이런 ㅜㅜ",
          "original_form": "이런 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 11:07"
        },
        {
          "id": "MDRW2100002389.1.41",
          "form": "여행가서 아픈게 젤 서러워요",
          "original_form": "여행가서 아픈게 젤 서러워요",
          "speaker_id": "1",
          "time": "20210820 11:07"
        },
        {
          "id": "MDRW2100002389.1.42",
          "form": "약사기도 힘들고",
          "original_form": "약사기도 힘들고",
          "speaker_id": "1",
          "time": "20210820 11:07"
        },
        {
          "id": "MDRW2100002389.1.43",
          "form": "너무 습해서 사실 놀랐어요 그래서 탈났나싶어요",
          "original_form": "너무 습해서 사실 놀랐어요 그래서 탈났나싶어요",
          "speaker_id": "2",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002389.1.44",
          "form": "저 약을 왕창 가져 갔어요",
          "original_form": "저 약을 왕창 가져 갔어요",
          "speaker_id": "2",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002389.1.45",
          "form": "맞아요 진짜 습하죠",
          "original_form": "맞아요 진짜 습하죠",
          "speaker_id": "1",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002389.1.46",
          "form": "아직도 체감 42도래요",
          "original_form": "아직도 체감 42도래요",
          "speaker_id": "1",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002389.1.47",
          "form": "벽에 곰팡이가 까매서 벽이 회색 ㅋㅋ",
          "original_form": "벽에 곰팡이가 까매서 벽이 회색 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002389.1.48",
          "form": "으악 ㅋㅋ",
          "original_form": "으악 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002389.1.49",
          "form": "겨울에 가니 좀낫더라고요",
          "original_form": "겨울에 가니 좀낫더라고요",
          "speaker_id": "2",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002389.1.50",
          "form": "또 가고싶어요",
          "original_form": "또 가고싶어요",
