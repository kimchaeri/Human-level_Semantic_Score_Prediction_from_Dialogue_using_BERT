      "utterance": [
        {
          "id": "MDRW2100006575.1.1",
          "form": "내동생 롤 진짜 열심히 하더라",
          "original_form": "내동생 롤 진짜 열심히 하더라",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006575.1.2",
          "form": "중독성 엄청난가봐",
          "original_form": "중독성 엄청난가봐",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006575.1.3",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006575.1.4",
          "form": "우리 형도 엄청 해",
          "original_form": "우리 형도 엄청 해",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006575.1.5",
          "form": "난 그래서 최대한 제대로 안 하려고 하는 중이고",
          "original_form": "난 그래서 최대한 제대로 안 하려고 하는 중이고",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006575.1.6",
          "form": "와 진짜!? 겜을 어떻게 만든거야",
          "original_form": "와 진짜!? 겜을 어떻게 만든거야",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100006575.1.7",
          "form": "대박이네",
          "original_form": "대박이네",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100006575.1.8",
          "form": "잠도안자고하더라 휴가때보니까",
          "original_form": "잠도안자고하더라 휴가때보니까",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100006575.1.9",
          "form": "ㅋㅋㅋㅋㅋ휴가면 그럴만하지",
          "original_form": "ㅋㅋㅋㅋㅋ휴가면 그럴만하지",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100006575.1.10",
          "form": "게임 관리를 잘하더라",
          "original_form": "게임 관리를 잘하더라",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100006575.1.11",
          "form": "아까 사람들이 강한 것들 몇개 중에서 들고나온다고 했잖아",
          "original_form": "아까 사람들이 강한 것들 몇개 중에서 들고나온다고 했잖아",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100006575.1.12",
          "form": "응응 게임 개발자가 대단하다",
          "original_form": "응응 게임 개발자가 대단하다",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100006575.1.13",
          "form": "계속 똑같으면 질리고 재미 없으니까 업데이트해서 약한 영웅은 좀 강하게",
          "original_form": "계속 똑같으면 질리고 재미 없으니까 업데이트해서 약한 영웅은 좀 강하게",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100006575.1.14",
          "form": "강한 영웅은 좀 약하게 만들어서",
          "original_form": "강한 영웅은 좀 약하게 만들어서",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100006575.1.15",
          "form": "사람들이 새로운 거 들고나올 수 밖에 없도록 하더라고",
          "original_form": "사람들이 새로운 거 들고나올 수 밖에 없도록 하더라고",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100006575.1.16",
          "form": "오ㅋㅋㅋ 신기하닼ㅋㅋㅋ",
          "original_form": "오ㅋㅋㅋ 신기하닼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100006575.1.17",
          "form": "그러면 또 안 질리고 새로운 재미니까",
          "original_form": "그러면 또 안 질리고 새로운 재미니까",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100006575.1.18",
          "form": "그럼 현질도 많이하겠지?",
          "original_form": "그럼 현질도 많이하겠지?",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100006575.1.19",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100006575.1.20",
          "form": "음 하는 사람들은 많이 하더라",
          "original_form": "음 하는 사람들은 많이 하더라",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100006575.1.21",
          "form": "나도 아이디어 생각좀 해볼걸",
          "original_form": "나도 아이디어 생각좀 해볼걸",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100006575.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ부럽다 창설자",
          "original_form": "ㅋㅋㅋㅋㅋㅋ부럽다 창설자",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100006575.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ다 그런거지 뭐",
          "original_form": "ㅋㅋㅋㅋㅋㅋ다 그런거지 뭐",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100006575.1.24",
          "form": "나도 부럽다 창설자",
          "original_form": "나도 부럽다 창설자",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100006575.1.25",
          "form": "ㅠㅠ너가 먼저생각하지",
          "original_form": "ㅠㅠ너가 먼저생각하지",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100006575.1.26",
          "form": "아쉽네",
          "original_form": "아쉽네",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100006575.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그러게 말이야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그러게 말이야",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006575.1.28",
          "form": "근데 난이도가 높긴하더라",
          "original_form": "근데 난이도가 높긴하더라",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006575.1.29",
          "form": "유일한 단점이지 뭐",
          "original_form": "유일한 단점이지 뭐",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006575.1.30",
          "form": "아그래? 안해봐서 몰라",
          "original_form": "아그래? 안해봐서 몰라",
          "speaker_id": "1",
          "time": "20210908 02:50"
        }
      ]
    }
