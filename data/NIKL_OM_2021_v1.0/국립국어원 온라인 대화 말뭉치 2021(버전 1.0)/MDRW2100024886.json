          "time": "20210831 20:13"
        },
        {
          "id": "MDRW2100024886.1.4",
          "form": "네 다녀요",
          "original_form": "네 다녀요",
          "speaker_id": "2",
          "time": "20210831 20:13"
        },
        {
          "id": "MDRW2100024886.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100024886.1.6",
          "form": "저도 다녀용",
          "original_form": "저도 다녀용",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100024886.1.7",
          "form": "2개월따리 신입입니당",
          "original_form": "2개월따리 신입입니당",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100024886.1.8",
          "form": "삐약삐약",
          "original_form": "삐약삐약",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100024886.1.9",
          "form": "와 신입 ㅜㅜㅜ",
          "original_form": "와 신입 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100024886.1.10",
          "form": "다니시는곳은",
          "original_form": "다니시는곳은",
          "speaker_id": "2",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100024886.1.11",
          "form": "괜찮아요?",
          "original_form": "괜찮아요?",
          "speaker_id": "2",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100024886.1.12",
          "form": "음.... 처음에 너무 힘들어서 퇴사하려고 했어요",
          "original_form": "음.... 처음에 너무 힘들어서 퇴사하려고 했어요",
          "speaker_id": "1",
          "time": "20210831 20:15"
        },
        {
          "id": "MDRW2100024886.1.13",
          "form": "먼산......",
          "original_form": "먼산......",
          "speaker_id": "1",
          "time": "20210831 20:15"
        },
        {
          "id": "MDRW2100024886.1.14",
          "form": "헉 이개월밖에 안됐는데 ᆢ",
          "original_form": "헉 이개월밖에 안됐는데 ᆢ",
