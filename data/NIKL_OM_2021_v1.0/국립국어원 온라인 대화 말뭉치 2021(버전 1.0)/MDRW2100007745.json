      "utterance": [
        {
          "id": "MDRW2100007745.1.1",
          "form": "저는 영드에 빠졌어요",
          "original_form": "저는 영드에 빠졌어요",
          "speaker_id": "2",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100007745.1.2",
          "form": "웨이브에서 브라운신부 보는데 너무 재미있어요",
          "original_form": "웨이브에서 브라운신부 보는데 너무 재미있어요",
          "speaker_id": "2",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007745.1.3",
          "form": "영드 뭐보세요? 저도 한창 셜록 좋아했는데",
          "original_form": "영드 뭐보세요? 저도 한창 셜록 좋아했는데",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007745.1.4",
          "form": "아아 저도 웨이브써서 한번 봐야겠네요",
          "original_form": "아아 저도 웨이브써서 한번 봐야겠네요",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007745.1.5",
          "form": "이상하게 영드 매력있어요",
          "original_form": "이상하게 영드 매력있어요",
          "speaker_id": "2",
          "time": "20210822 10:13"
        },
        {
          "id": "MDRW2100007745.1.6",
          "form": "저는 수사물덕후인데",
          "original_form": "저는 수사물덕후인데",
          "speaker_id": "2",
          "time": "20210822 10:13"
        },
        {
          "id": "MDRW2100007745.1.7",
          "form": "브라운신부 짱추천이요",
          "original_form": "브라운신부 짱추천이요",
          "speaker_id": "2",
          "time": "20210822 10:13"
        },
        {
          "id": "MDRW2100007745.1.8",
          "form": "저도 추리수사 엄청 좋아해요ㅋㅋ브라운신부도 그런내용이에요?",
          "original_form": "저도 추리수사 엄청 좋아해요ㅋㅋ브라운신부도 그런내용이에요?",
          "speaker_id": "1",
          "time": "20210822 10:14"
        },
        {
          "id": "MDRW2100007745.1.9",
          "form": "네 신부님이 수사합니다 너무 우울하거나 진지하지않아서 딱좋아요 ㅋㅋ",
          "original_form": "네 신부님이 수사합니다 너무 우울하거나 진지하지않아서 딱좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007745.1.10",
          "form": "우울한거 아니라니 끌리네요",
          "original_form": "우울한거 아니라니 끌리네요",
          "speaker_id": "1",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007745.1.11",
          "form": "인데버는 너무 우울해서 보다 말았거든요ㅠㅜ",
          "original_form": "인데버는 너무 우울해서 보다 말았거든요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007745.1.12",
          "form": "아 저 그거보고있어요",
          "original_form": "아 저 그거보고있어요",
          "speaker_id": "2",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007745.1.13",
          "form": "지금 시즌5",
          "original_form": "지금 시즌5",
          "speaker_id": "2",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007745.1.14",
          "form": "와 진짜 반가워요",
          "original_form": "와 진짜 반가워요",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007745.1.15",
          "form": "아 그러시구나ㅎㅎ 근데 재밌긴해요ㅎㅎ",
          "original_form": "아 그러시구나ㅎㅎ 근데 재밌긴해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007745.1.16",
          "form": "아 근데 진짜 캐공감요",
          "original_form": "아 근데 진짜 캐공감요",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007745.1.17",
          "form": "보고나면 우울해요",
          "original_form": "보고나면 우울해요",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007745.1.18",
          "form": "영드 특유분위기가ㅜ",
          "original_form": "영드 특유분위기가ㅜ",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007745.1.19",
          "form": "파더 브라운은 그런거없어서 좋아요 ㅋㅋ",
          "original_form": "파더 브라운은 그런거없어서 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007745.1.20",
          "form": "꼭봐야겠네요ㅋㅋ 저는 아가사크리스티시리즈도 다봤거든요ㅋㅋ",
          "original_form": "꼭봐야겠네요ㅋㅋ 저는 아가사크리스티시리즈도 다봤거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007745.1.21",
          "form": "아가사 크리스티 시리즈 좋죠",
          "original_form": "아가사 크리스티 시리즈 좋죠",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007745.1.22",
          "form": "포와르랑 마플중 뭐보셨어요",
          "original_form": "포와르랑 마플중 뭐보셨어요",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007745.1.23",
          "form": "가볍게 보기좋더라구요ㅎㅎ",
          "original_form": "가볍게 보기좋더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007745.1.24",
          "form": "저는 둘다 봤어요ㅋㅋㅋ",
          "original_form": "저는 둘다 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:22"
        }
      ]
