      "utterance": [
        {
          "id": "MDRW2100020589.1.1",
          "form": "혹시 콘서트 가보셨나요?",
          "original_form": "혹시 콘서트 가보셨나요?",
          "speaker_id": "1",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020589.1.2",
          "form": "예전에 아주 예전에",
          "original_form": "예전에 아주 예전에",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020589.1.3",
          "form": "마룬5 콘서트 갔었어요",
          "original_form": "마룬5 콘서트 갔었어요",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020589.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020589.1.5",
          "form": "와 마룬5라니! 신기하네요",
          "original_form": "와 마룬5라니! 신기하네요",
          "speaker_id": "1",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020589.1.6",
          "form": "저는 아이돌 좋아해서",
          "original_form": "저는 아이돌 좋아해서",
          "speaker_id": "1",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020589.1.7",
          "form": "네 ㅎㅎ 근데 가사를 안외우고가서",
          "original_form": "네 ㅎㅎ 근데 가사를 안외우고가서",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020589.1.8",
          "form": "재미없었어요",
          "original_form": "재미없었어요",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020589.1.9",
          "form": "오 무슨 아이돌이요?",
          "original_form": "오 무슨 아이돌이요?",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020589.1.10",
          "form": "빅스랑 마마무랑 데이식스 콘서트 가봤어요",
          "original_form": "빅스랑 마마무랑 데이식스 콘서트 가봤어요",
          "speaker_id": "1",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020589.1.11",
          "form": "오 마마무 좋아해요 저",
          "original_form": "오 마마무 좋아해요 저",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020589.1.12",
          "form": "마자요 따라부르는 재미도 있어서 가사모르면그렇죠",
          "original_form": "마자요 따라부르는 재미도 있어서 가사모르면그렇죠",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020589.1.13",
          "form": "데이식스도 멤버는 모르지만 노래 알아요",
          "original_form": "데이식스도 멤버는 모르지만 노래 알아요",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020589.1.14",
          "form": "오호! 진짜요?",
          "original_form": "오호! 진짜요?",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020589.1.15",
          "form": "근데 데이식스 하니까 급 병크 생각나네요 ㅠㅠ",
          "original_form": "근데 데이식스 하니까 급 병크 생각나네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020589.1.16",
          "form": "죄송 ㅠㅠ",
          "original_form": "죄송 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020589.1.17",
          "form": "무슨 병크요?",
          "original_form": "무슨 병크요?",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020589.1.18",
          "form": "하지만 노래는 좋아요!",
          "original_form": "하지만 노래는 좋아요!",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020589.1.19",
          "form": "마자요 노래좋죠!",
          "original_form": "마자요 노래좋죠!",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020589.1.20",
          "form": "그 멤버 탈퇴설이었나?",
          "original_form": "그 멤버 탈퇴설이었나?",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020589.1.21",
          "form": "데이식스 아닐 수도 있어요",
          "original_form": "데이식스 아닐 수도 있어요",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020589.1.22",
          "form": "탈퇴한 분이요?",
          "original_form": "탈퇴한 분이요?",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020589.1.23",
          "form": "아 맞아요?",
          "original_form": "아 맞아요?",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020589.1.24",
          "form": "빅스도 들어본 것 같아요",
          "original_form": "빅스도 들어본 것 같아요",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020589.1.25",
          "form": "그전에 팬이랑 연애? 한다고해서",
          "original_form": "그전에 팬이랑 연애? 한다고해서",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020589.1.26",
          "form": "공연 어떠셨어요?",
          "original_form": "공연 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020589.1.27",
          "form": "탈퇴한 멤버있긴했어요",
          "original_form": "탈퇴한 멤버있긴했어요",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020589.1.28",
          "form": "아 그거였나봐요",
          "original_form": "아 그거였나봐요",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020589.1.29",
          "form": "빅스콘서트때는 스탠딩한번 가고 좌석 한번 갔었는데",
          "original_form": "빅스콘서트때는 스탠딩한번 가고 좌석 한번 갔었는데",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020589.1.30",
          "form": "좌석은 너무 좋았었는데 스탠딩에서 진짜 힘들었어요ㅠ",
          "original_form": "좌석은 너무 좋았었는데 스탠딩에서 진짜 힘들었어요ㅠ",
          "speaker_id": "1",
          "time": "20210828 10:17"
        }
      ]
    }
  ]
