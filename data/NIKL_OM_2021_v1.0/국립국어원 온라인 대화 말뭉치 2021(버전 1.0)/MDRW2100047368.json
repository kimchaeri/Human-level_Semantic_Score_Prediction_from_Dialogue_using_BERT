      "utterance": [
        {
          "id": "MDRW2100047368.1.1",
          "form": "그리고 맛있는 음식이",
          "original_form": "그리고 맛있는 음식이",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100047368.1.2",
          "form": "너무 많죠!",
          "original_form": "너무 많죠!",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100047368.1.3",
          "form": "맞아요 ㅎㅎㅎㅎ예쁜 카페도 많더라구요",
          "original_form": "맞아요 ㅎㅎㅎㅎ예쁜 카페도 많더라구요",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100047368.1.4",
          "form": "맞아요ㅋㅋㅋ요즘은 진짜많죠",
          "original_form": "맞아요ㅋㅋㅋ요즘은 진짜많죠",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100047368.1.5",
          "form": "귤카페 이런거 귀엽더라구요",
          "original_form": "귤카페 이런거 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100047368.1.6",
          "form": "ㅋㄱㅋ 제주도에있나요?",
          "original_form": "ㅋㄱㅋ 제주도에있나요?",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047368.1.7",
          "form": "네 ㅎㅎ 제주도랑 어울린다 생각했어요",
          "original_form": "네 ㅎㅎ 제주도랑 어울린다 생각했어요",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100047368.1.8",
          "form": "ㅋㅋㅋ 진짜좋네요",
          "original_form": "ㅋㅋㅋ 진짜좋네요",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100047368.1.9",
          "form": "제주도한달살기 하고파오",
          "original_form": "제주도한달살기 하고파오",
          "speaker_id": "1",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100047368.1.10",
          "form": "친구가 하는 것 봤는데 무척 부럽더라구요",
          "original_form": "친구가 하는 것 봤는데 무척 부럽더라구요",
          "speaker_id": "2",
          "time": "20210827 00:07"
        },
        {
          "id": "MDRW2100047368.1.11",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210827 00:08"
        },
        {
          "id": "MDRW2100047368.1.12",
          "form": "실천한분이있다니",
          "original_form": "실천한분이있다니",
          "speaker_id": "1",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100047368.1.13",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100047368.1.14",
          "form": "그러게요 ㅎㅎㅎ저는 딱히 한달씩 여행가본적은 없어서",
          "original_form": "그러게요 ㅎㅎㅎ저는 딱히 한달씩 여행가본적은 없어서",
          "speaker_id": "2",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100047368.1.15",
          "form": "저도요ㅠ",
          "original_form": "저도요ㅠ",
          "speaker_id": "1",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100047368.1.16",
          "form": "길어야4일?",
          "original_form": "길어야4일?",
          "speaker_id": "1",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100047368.1.17",
          "form": "ㅎㅎㅎ휴가를 오래가는건 정말 맘먹어야 가능한 것같아요",
