      "utterance": [
        {
          "id": "MDRW2100016755.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016755.1.2",
          "form": "반려동물 키우시나요?ㅎ",
          "original_form": "반려동물 키우시나요?ㅎ",
          "speaker_id": "1",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016755.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016755.1.4",
          "form": "전에 부모님과같이살땐 집에 강아지있었어요 !",
          "original_form": "전에 부모님과같이살땐 집에 강아지있었어요 !",
          "speaker_id": "2",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016755.1.5",
          "form": "오 강아지",
          "original_form": "오 강아지",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016755.1.6",
          "form": "저는 집에 알러지 있는 가족이 있어서 강아지를 들이지 못했는데",
          "original_form": "저는 집에 알러지 있는 가족이 있어서 강아지를 들이지 못했는데",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016755.1.7",
          "form": "점점 반려동물이 있음 좋겠다 하는 맘이 들더라고요",
          "original_form": "점점 반려동물이 있음 좋겠다 하는 맘이 들더라고요",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016755.1.8",
          "form": "어떤 종이었나요?",
          "original_form": "어떤 종이었나요?",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016755.1.9",
          "form": "말티푸에요!",
          "original_form": "말티푸에요!",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016755.1.10",
          "form": "지금은 동생이 결혼하고 데려가서키워요 ㅋㅋ",
          "original_form": "지금은 동생이 결혼하고 데려가서키워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016755.1.11",
          "form": "아아 그러시구나",
          "original_form": "아아 그러시구나",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016755.1.12",
          "form": "반려동물이 있으면 확실히 좋죠?",
          "original_form": "반려동물이 있으면 확실히 좋죠?",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016755.1.13",
          "form": "엄마네있을땐 엄마가 엄마고 저희랑같이 형제개념이였는데 ㅋㅋ 지금은 엄마가 할머니 제가 이모개념로 서열이 바꼈어요 ㅎ",
          "original_form": "엄마네있을땐 엄마가 엄마고 저희랑같이 형제개념이였는데 ㅋㅋ 지금은 엄마가 할머니 제가 이모개념로 서열이 바꼈어요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016755.1.14",
          "form": "그쵸 그만큼 책임감이필요하지만",
          "original_form": "그쵸 그만큼 책임감이필요하지만",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016755.1.15",
          "form": "집에있고없고 차이가나요 !",
          "original_form": "집에있고없고 차이가나요 !",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016755.1.16",
          "form": "애교가얼마나많은지",
          "original_form": "애교가얼마나많은지",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016755.1.17",
          "form": "반려동물이 있으면 정서적으로 확실히 좋은 듯해요",
          "original_form": "반려동물이 있으면 정서적으로 확실히 좋은 듯해요",
          "speaker_id": "1",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016755.1.18",
          "form": "맞아요 외로움이 덜하고",
          "original_form": "맞아요 외로움이 덜하고",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016755.1.19",
          "form": "분위기도 차이가낮ㅅ",
          "original_form": "분위기도 차이가낮ㅅ",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016755.1.20",
          "form": "나죠",
          "original_form": "나죠",
          "speaker_id": "2",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016755.1.21",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016755.1.22",
          "form": "저도 나중엔 강아지 한마리 키우고 싶어요",
          "original_form": "저도 나중엔 강아지 한마리 키우고 싶어요",
          "speaker_id": "1",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016755.1.23",
          "form": "나중에 독립하시면 괜찮을꺼같아요",
          "original_form": "나중에 독립하시면 괜찮을꺼같아요",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016755.1.24",
          "form": "저녁에 같이 산책하면서 뱌람도쐬고~",
          "original_form": "저녁에 같이 산책하면서 뱌람도쐬고~",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016755.1.25",
          "form": "네네 그런데 또 다 출근하면",
          "original_form": "네네 그런데 또 다 출근하면",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016755.1.26",
          "form": "강아지가 낮동안 너무 외로울까봐",
          "original_form": "강아지가 낮동안 너무 외로울까봐",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016755.1.27",
          "form": "그건또 구래요ㅠㅠ",
          "original_form": "그건또 구래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100016755.1.28",
          "form": "그래서 두마리키우게되나봐요ㅠㅠ",
          "original_form": "그래서 두마리키우게되나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016755.1.29",
          "form": "저 좋자고 강아지 외롭게 할 수는 없으니까ㅠ",
          "original_form": "저 좋자고 강아지 외롭게 할 수는 없으니까ㅠ",
          "speaker_id": "1",
          "time": "20210826 15:21"
        }
      ]
    }
