      "utterance": [
        {
          "id": "MDRW2100031154.1.1",
          "form": "스타벅스에 이어",
          "original_form": "스타벅스에 이어",
          "speaker_id": "1",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100031154.1.2",
          "form": "백신 날짜 언제랬죠",
          "original_form": "백신 날짜 언제랬죠",
          "speaker_id": "1",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100031154.1.3",
          "form": "우린데스티니예요 같은날짜조",
          "original_form": "우린데스티니예요 같은날짜조",
          "speaker_id": "2",
          "time": "20210904 17:37"
        },
        {
          "id": "MDRW2100031154.1.4",
          "form": "헐 그랬네 맞네요",
          "original_form": "헐 그랬네 맞네요",
          "speaker_id": "1",
          "time": "20210904 17:37"
        },
        {
          "id": "MDRW2100031154.1.5",
          "form": "우리 1분마다 이야기하기로 했잔항요",
          "original_form": "우리 1분마다 이야기하기로 했잔항요",
          "speaker_id": "1",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100031154.1.6",
          "form": "10시맞나요?",
          "original_form": "10시맞나요?",
          "speaker_id": "1",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100031154.1.7",
          "form": "1분마닥ㅈㄱㄷㄱㄷㄱ",
          "original_form": "1분마닥ㅈㄱㄷㄱㄷㄱ",
          "speaker_id": "2",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100031154.1.8",
          "form": "저5시에맞습니당",
          "original_form": "저5시에맞습니당",
          "speaker_id": "2",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031154.1.9",
          "form": "몇시에맞아용?아침?",
          "original_form": "몇시에맞아용?아침?",
          "speaker_id": "2",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031154.1.10",
          "form": "아 저녁 이시군요ㅠ",
          "original_form": "아 저녁 이시군요ㅠ",
          "speaker_id": "1",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031154.1.11",
          "form": "네네 저는 아침 10시 맞고 쉬려고요",
          "original_form": "네네 저는 아침 10시 맞고 쉬려고요",
          "speaker_id": "1",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031154.1.12",
          "form": "저번엔 양궁 패션이었는데",
          "original_form": "저번엔 양궁 패션이었는데",
          "speaker_id": "1",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031154.1.13",
          "form": "이번엔 베레모 패션으로 가보겠습니다",
          "original_form": "이번엔 베레모 패션으로 가보겠습니다",
          "speaker_id": "1",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031154.1.14",
          "form": "ㅋㅈㅋㅈㄱㅈㄱㅈㄱ",
          "original_form": "ㅋㅈㅋㅈㄱㅈㄱㅈㄱ",
          "speaker_id": "2",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031154.1.15",
          "form": "패셔니스타의 백신맞는날",
          "original_form": "패셔니스타의 백신맞는날{emoji:☆}",
          "speaker_id": "2",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031154.1.16",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031154.1.17",
          "form": "백신 듀렵네요",
          "original_form": "백신 듀렵네요",
          "speaker_id": "1",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031154.1.18",
          "form": "1차는 그래도 잘 지나간것같은데",
          "original_form": "1차는 그래도 잘 지나간것같은데",
          "speaker_id": "1",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031154.1.19",
          "form": "전진짜무서워요",
          "original_form": "전진짜무서워요",
          "speaker_id": "2",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031154.1.20",
          "form": "1차라서떨려여..",
          "original_form": "1차라서떨려여..",
          "speaker_id": "2",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031154.1.21",
          "form": "그쵸 무섭습니다 하지만 괜찮아요 금방 끝나요",
          "original_form": "그쵸 무섭습니다 하지만 괜찮아요 금방 끝나요",
          "speaker_id": "1",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031154.1.22",
          "form": "다들 금방지나갔다구하니 좋게생각해야겠죵",
          "original_form": "다들 금방지나갔다구하니 좋게생각해야겠죵",
          "speaker_id": "2",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031154.1.23",
          "form": "예약할때진짜난리도아니엇는데",
          "original_form": "예약할때진짜난리도아니엇는데",
          "speaker_id": "2",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031154.1.24",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031154.1.25",
          "form": "난 그날 휴가 쓰는게 더 신나요",
          "original_form": "난 그날 휴가 쓰는게 더 신나요",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031154.1.26",
          "form": "그냥 영원히 쉬고싶네요 회사를",
          "original_form": "그냥 영원히 쉬고싶네요 회사를",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031154.1.27",
          "form": "개꿀이겠다..ㅋㅋㅋㅋ",
          "original_form": "개꿀이겠다..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031154.1.28",
          "form": "2차맞고 안아프고 그냥 쉬고파요",
          "original_form": "2차맞고 안아프고 그냥 쉬고파요",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031154.1.29",
          "form": "급잠들었다왔어요ㅋㅋㄱㅋ",
          "original_form": "급잠들었다왔어요ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100031154.1.30",
          "form": "그러게ㅣ요안아팟음좋겟어요",
          "original_form": "그러게ㅣ요안아팟음좋겟어요",
          "speaker_id": "2",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100031154.1.31",
          "form": "친구만나고왔어요 백신 안아플거에요",
          "original_form": "친구만나고왔어요 백신 안아플거에요",
