      "utterance": [
        {
          "id": "MDRW2100013011.1.1",
          "form": "여기 비 많이 와요",
          "original_form": "여기 비 많이 와요",
          "speaker_id": "1",
          "time": "20210825 10:25"
        },
        {
          "id": "MDRW2100013011.1.2",
          "form": "경기도는 안오네요 ㅎㅎ",
          "original_form": "경기도는 안오네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100013011.1.3",
          "form": "장마ㅠㅠ",
          "original_form": "장마ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 11:44"
        },
        {
          "id": "MDRW2100013011.1.4",
          "form": "더울땐 비오길 바랬는데",
          "original_form": "더울땐 비오길 바랬는데",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100013011.1.5",
          "form": "막상 많이 오니까 ㅋㅋㅋ",
          "original_form": "막상 많이 오니까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100013011.1.6",
          "form": "그니깐요ㅜㅜ",
          "original_form": "그니깐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013011.1.7",
          "form": "ㅋㅋ 습해서 별루",
          "original_form": "ㅋㅋ 습해서 별루",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100013011.1.8",
          "form": "비오니깐 너무 답답해요",
          "original_form": "비오니깐 너무 답답해요",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013011.1.9",
          "form": "작년에도 시끕했는데",
          "original_form": "작년에도 시끕했는데",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013011.1.10",
          "form": "올해도......",
          "original_form": "올해도......",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013011.1.11",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013011.1.12",
          "form": "ㅋㅋ 얼른 가을이 오길",
          "original_form": "ㅋㅋ 얼른 가을이 오길",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013011.1.13",
          "form": "그러니깐요ㅠㅠ",
          "original_form": "그러니깐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013011.1.14",
          "form": "시원해졌으면좋겠어요",
          "original_form": "시원해졌으면좋겠어요",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013011.1.15",
          "form": "ㅋㅋ근데 또 추워질듯",
          "original_form": "ㅋㅋ근데 또 추워질듯",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013011.1.16",
          "form": "9월넘으면 시원해지겠죠?",
          "original_form": "9월넘으면 시원해지겠죠?",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013011.1.17",
          "form": "네 지금도 선선하긴해요",
          "original_form": "네 지금도 선선하긴해요",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013011.1.18",
          "form": "계속 시윈했으면좋겠어요",
          "original_form": "계속 시윈했으면좋겠어요",
