      "utterance": [
        {
          "id": "MDRW2100002809.1.1",
          "form": "처음에 보다가 요즘은 안봐요",
          "original_form": "처음에 보다가 요즘은 안봐요",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002809.1.2",
          "form": "내용이 너무 심해서ㅋㅋ",
          "original_form": "내용이 너무 심해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002809.1.3",
          "form": "아 완전 막장이죠",
          "original_form": "아 완전 막장이죠",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002809.1.4",
          "form": "죽은 사람도 살아나는 호러물같아요",
          "original_form": "죽은 사람도 살아나는 호러물같아요",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002809.1.5",
          "form": "네~~ㄱㅋㅋㄱ 시즌 1이랑 2는 보셨아요?",
          "original_form": "네~~ㄱㅋㅋㄱ 시즌 1이랑 2는 보셨아요?",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002809.1.6",
          "form": "띄엄띄엄 봤어요",
          "original_form": "띄엄띄엄 봤어요",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002809.1.7",
          "form": "2까진 그래도 어찌어찌봤는데",
          "original_form": "2까진 그래도 어찌어찌봤는데",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002809.1.8",
          "form": "로건리 폭탄터지고 끝나서 황당ㅋㅋㅋ",
          "original_form": "로건리 폭탄터지고 끝나서 황당ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002809.1.9",
          "form": "아~ㅎㅎㅎ 맞아요 ㅎㅎ 다시 살아난거 아시죠?",
          "original_form": "아~ㅎㅎㅎ 맞아요 ㅎㅎ 다시 살아난거 아시죠?",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002809.1.10",
          "form": "네 ㅋㅋ그게 더웃겨요",
          "original_form": "네 ㅋㅋ그게 더웃겨요",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002809.1.11",
          "form": "네 ㄱㅋ 죽은 사람들이 계속 살아나요 ㅎㅎ",
          "original_form": "네 ㄱㅋ 죽은 사람들이 계속 살아나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002809.1.12",
          "form": "설마 오윤희도 살아온건 아니죠",
          "original_form": "설마 오윤희도 살아온건 아니죠",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002809.1.13",
          "form": "윽 설마 오윤희까지;;그건 아닐꺼에요 시체도 나왔는걸요",
          "original_form": "윽 설마 오윤희까지;;그건 아닐꺼에요 시체도 나왔는걸요",
          "speaker_id": "1",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002809.1.14",
          "form": "이제 거의 끝나가요 팬하3",
          "original_form": "이제 거의 끝나가요 팬하3",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002809.1.15",
          "form": "4도 또 나올 분위기인가요?",
          "original_form": "4도 또 나올 분위기인가요?",
          "speaker_id": "2",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002809.1.16",
          "form": "아닐것 같아요",
          "original_form": "아닐것 같아요",
          "speaker_id": "1",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002809.1.17",
          "form": "드디어 끝나는군요",
          "original_form": "드디어 끝나는군요",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002809.1.18",
          "form": "마지막은 챙겨봐야겠네요",
          "original_form": "마지막은 챙겨봐야겠네요",
          "speaker_id": "2",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002809.1.19",
          "form": "결말이 궁금해요 ㅎ",
          "original_form": "결말이 궁금해요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002809.1.20",
          "form": "누가 또 죽는 그런건 아니겠죠ㄷㄷ",
          "original_form": "누가 또 죽는 그런건 아니겠죠ㄷㄷ",
