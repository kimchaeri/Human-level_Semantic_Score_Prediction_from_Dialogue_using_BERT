      "utterance": [
        {
          "id": "MDRW2100002731.1.1",
          "form": "너 대학다닐때 엠티 가봤어?",
          "original_form": "너 대학다닐때 엠티 가봤어?",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002731.1.2",
          "form": "나 과 활동은 열심히 안해서",
          "original_form": "나 과 활동은 열심히 안해서",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002731.1.3",
          "form": "동아리 활동을 많아 했어",
          "original_form": "동아리 활동을 많아 했어",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002731.1.4",
          "form": "많이 했어",
          "original_form": "많이 했어",
          "speaker_id": "2",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002731.1.5",
          "form": "무슨 동아리였는데??",
          "original_form": "무슨 동아리였는데??",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002731.1.6",
          "form": "정말 안어울리지만",
          "original_form": "정말 안어울리지만",
          "speaker_id": "2",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002731.1.7",
          "form": "검도 동아리야",
          "original_form": "검도 동아리야",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002731.1.8",
          "form": "검도??",
          "original_form": "검도??",
          "speaker_id": "1",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002731.1.9",
          "form": "응 검도",
          "original_form": "응 검도",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002731.1.10",
          "form": "진짜 의외다",
          "original_form": "진짜 의외다",
          "speaker_id": "1",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002731.1.11",
          "form": "동아리에서 주로 뭐했어??",
          "original_form": "동아리에서 주로 뭐했어??",
          "speaker_id": "1",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002731.1.12",
          "form": "운동도 하고",
          "original_form": "운동도 하고",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002731.1.13",
          "form": "뭐 시켜먹고",
          "original_form": "뭐 시켜먹고",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002731.1.14",
          "form": "게임도 하고 그랬지",
          "original_form": "게임도 하고 그랬지",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002731.1.15",
          "form": "그말 들으니까 운동이 절반이 아니라 시켜먹고 게임을 절반 이상한것 같은데",
          "original_form": "그말 들으니까 운동이 절반이 아니라 시켜먹고 게임을 절반 이상한것 같은데",
          "speaker_id": "1",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002731.1.16",
          "form": "놀려고 운동하러 갔지ㅋㅋㅋ",
          "original_form": "놀려고 운동하러 갔지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002731.1.17",
          "form": "그건 운동이 아니잖아 ㅋㅋ",
          "original_form": "그건 운동이 아니잖아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002731.1.18",
          "form": "그래도 나름 열심히 했어",
          "original_form": "그래도 나름 열심히 했어",
          "speaker_id": "2",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002731.1.19",
          "form": "검도 기술같은거 가르쳐 줘??",
          "original_form": "검도 기술같은거 가르쳐 줘??",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002731.1.20",
          "form": "응 배웠지",
          "original_form": "응 배웠지",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002731.1.21",
          "form": "지금도 할 수 있을지 모르겠지만",
          "original_form": "지금도 할 수 있을지 모르겠지만",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002731.1.22",
          "form": "오오 나도 중딩때 친구가 검도 배우기도 했고 놀러 가보기도 했는데",
          "original_form": "오오 나도 중딩때 친구가 검도 배우기도 했고 놀러 가보기도 했는데",
          "speaker_id": "1",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002731.1.23",
          "form": "담에 만나면 보여주는거야?",
          "original_form": "담에 만나면 보여주는거야?",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002731.1.24",
          "form": "막대기 하나 쥐여주면 되나?ㅋㅋ",
          "original_form": "막대기 하나 쥐여주면 되나?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002731.1.25",
          "form": "하하.. 아니요",
          "original_form": "하하.. 아니요",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002731.1.26",
          "form": "에잉 쯧",
          "original_form": "에잉 쯧",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002731.1.27",
          "form": "보고 싶었는데",
          "original_form": "보고 싶었는데",
          "speaker_id": "1",
          "time": "20210820 15:16"
        }
      ]
    }
  ]
