      "utterance": [
        {
          "id": "MDRW2100043280.1.1",
          "form": "제가 은근히 민초파라 ㅋㅋㅋ",
          "original_form": "제가 은근히 민초파라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100043280.1.2",
          "form": "전 민초파는 아니지만 맛있어요 ㅋㅋㅋㅋ",
          "original_form": "전 민초파는 아니지만 맛있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100043280.1.3",
          "form": "저도 완전 민초파는 아니라 ㅋㅋㅋ",
          "original_form": "저도 완전 민초파는 아니라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100043280.1.4",
          "form": "민트초코파이는 은은하니 좋더라고요",
          "original_form": "민트초코파이는 은은하니 좋더라고요",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100043280.1.5",
          "form": "생각해보니 민초단은 안좋아할수도 있겠어요 안강해서",
          "original_form": "생각해보니 민초단은 안좋아할수도 있겠어요 안강해서",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100043280.1.6",
          "form": "오래오민초도 맛있읍니다",
          "original_form": "오래오민초도 맛있읍니다",
          "speaker_id": "2",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100043280.1.7",
          "form": "아 맛있었어요!!!",
          "original_form": "아 맛있었어요!!!",
          "speaker_id": "1",
          "time": "20210907 16:52"
        },
        {
          "id": "MDRW2100043280.1.8",
          "form": "오레오는 거의다 맛있더라고요",
          "original_form": "오레오는 거의다 맛있더라고요",
          "speaker_id": "1",
          "time": "20210907 16:52"
        },
        {
          "id": "MDRW2100043280.1.9",
          "form": "개인적으로는 씬즈가 취향이긴 한데",
          "original_form": "개인적으로는 씬즈가 취향이긴 한데",
          "speaker_id": "1",
          "time": "20210907 16:52"
        },
        {
          "id": "MDRW2100043280.1.10",
          "form": "대부분 맛있어요",
          "original_form": "대부분 맛있어요",
          "speaker_id": "1",
          "time": "20210907 16:53"
        },
        {
          "id": "MDRW2100043280.1.11",
          "form": "그러나 제 최애는 쌀로별과자예요ㅋㅋㅋㅋ",
          "original_form": "그러나 제 최애는 쌀로별과자예요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100043280.1.12",
          "form": "그냥 먹어도 맛있고 술안주로 먹어도 좋고 ㅎㅎ",
          "original_form": "그냥 먹어도 맛있고 술안주로 먹어도 좋고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100043280.1.13",
          "form": "아 쌀로별 맛있죠!",
          "original_form": "아 쌀로별 맛있죠!",
          "speaker_id": "1",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100043280.1.14",
          "form": "저는 감자칩인데 요즘엔 수미칩이 맛있더라고요",
          "original_form": "저는 감자칩인데 요즘엔 수미칩이 맛있더라고요",
          "speaker_id": "1",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100043280.1.15",
          "form": "감자과자 좋아하면 돌아가며 땡기는게 생기는거 같아요ㅋㅋ",
          "original_form": "감자과자 좋아하면 돌아가며 땡기는게 생기는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043280.1.16",
          "form": "전 감자과자는 무뚝뚝이 맛있어요️",
          "original_form": "전 감자과자는 무뚝뚝이 맛있어요{emoji:☺}️",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043280.1.17",
          "form": "네 돌아가며 먹고 있어요 무뚝뚝도 맛있죠! 눈을 감자도!!",
          "original_form": "네 돌아가며 먹고 있어요 무뚝뚝도 맛있죠! 눈을 감자도!!",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043280.1.18",
          "form": "눈을감자도 맛있어요 ㅎㅎ",
          "original_form": "눈을감자도 맛있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043280.1.19",
          "form": "혹시 곰표 아이스크림 먹어보셨나요?",
          "original_form": "혹시 곰표 아이스크림 먹어보셨나요?",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100043280.1.20",
          "form": "저 사먹어보려다가 말았는데 맛있나요??",
          "original_form": "저 사먹어보려다가 말았는데 맛있나요??",
          "speaker_id": "1",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100043280.1.21",
          "form": "아뇨.. 아직이요.. 묶음판매도 안해서 왠지 손해보는거 같아서ㅋㅋㅋㅋ",
          "original_form": "아뇨.. 아직이요.. 묶음판매도 안해서 왠지 손해보는거 같아서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100043280.1.22",
          "form": "ㅋㅋㅋ 아하 온라인 이마트에서 사볼까 하다가 말았어요",
          "original_form": "ㅋㅋㅋ 아하 온라인 이마트에서 사볼까 하다가 말았어요",
          "speaker_id": "1",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043280.1.23",
          "form": "그냥 그럴거 같아서...",
          "original_form": "그냥 그럴거 같아서...",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043280.1.24",
          "form": "ㅋㅋㅋ 맥주는 맛있다던데 드셔보셨나요?",
          "original_form": "ㅋㅋㅋ 맥주는 맛있다던데 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043280.1.25",
          "form": "네네 맥주는 기회가 되서요ㅋㅋㅋㅋ",
          "original_form": "네네 맥주는 기회가 되서요ㅋㅋㅋㅋ",
