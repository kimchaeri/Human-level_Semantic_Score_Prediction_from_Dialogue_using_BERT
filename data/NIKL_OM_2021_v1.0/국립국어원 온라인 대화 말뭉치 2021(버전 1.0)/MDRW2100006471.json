        },
        {
          "id": "MDRW2100006471.1.2",
          "form": "성곤했어요",
          "original_form": "성곤했어요",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006471.1.3",
          "form": "시간표자랑하고싶은데",
          "original_form": "시간표자랑하고싶은데",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006471.1.4",
          "form": "사진전송불가,,",
          "original_form": "사진전송불가,,",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100006471.1.5",
          "form": "와아 진짜 축하해요!!",
          "original_form": "와아 진짜 축하해요!!",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006471.1.6",
          "form": "매번 수강신청 성공하는 편이에요?",
          "original_form": "매번 수강신청 성공하는 편이에요?",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006471.1.7",
          "form": "네네 항상 올클이에요!",
          "original_form": "네네 항상 올클이에요!",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006471.1.8",
          "form": "이.. 훌륭하신분...",
          "original_form": "이.. 훌륭하신분...",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006471.1.9",
          "form": "원래 오전수업을 많이 듣는 편이세요?",
          "original_form": "원래 오전수업을 많이 듣는 편이세요?",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006471.1.10",
          "form": "음..전공겨수님이",
          "original_form": "음..전공겨수님이",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006471.1.11",
          "form": "항상",
          "original_form": "항상",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006471.1.12",
          "form": "오전을선호하셔서,,,",
          "original_form": "오전을선호하셔서,,,",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006471.1.13",
          "form": "으아아 ㅠ 역시 교수님들의 픽이었군요",
          "original_form": "으아아 ㅠ 역시 교수님들의 픽이었군요",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006471.1.14",
          "form": "이번에오전 수업 많아요?",
          "original_form": "이번에오전 수업 많아요?",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006471.1.15",
          "form": "맞아여....강제 오전수업..",
          "original_form": "맞아여....강제 오전수업..",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006471.1.16",
          "form": "전공 3개다 오전이에요!",
          "original_form": "전공 3개다 오전이에요!",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006471.1.17",
          "form": "오잉 오늘 수뇰이군요",
          "original_form": "오잉 오늘 수뇰이군요",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006471.1.18",
          "form": "월수 공강잉에ㅛ!ㅋㅋㅋㅋ",
          "original_form": "월수 공강잉에ㅛ!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006471.1.19",
          "form": "앜ㅋㅋㅋㅋㅋ 맞아요 수요일이에요",
          "original_form": "앜ㅋㅋㅋㅋㅋ 맞아요 수요일이에요",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006471.1.20",
          "form": "공강까지 챙기셨단말이에요?",
          "original_form": "공강까지 챙기셨단말이에요?",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006471.1.21",
          "form": "요새 대부분 줌수업으로 하긴 하지만 진짜 통학하면 힘들지 않아요?",
          "original_form": "요새 대부분 줌수업으로 하긴 하지만 진짜 통학하면 힘들지 않아요?",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006471.1.22",
          "form": "맞아여...^^퐁당퐁당...",
          "original_form": "맞아여...^^퐁당퐁당...",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100006471.1.23",
          "form": "아무리 생각해도 학과 유니콘이신게 틀림없다며",
          "original_form": "아무리 생각해도 학과 유니콘이신게 틀림없다며",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006471.1.24",
          "form": "못하는게 대체 뭐에요?",
          "original_form": "못하는게 대체 뭐에요?",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006471.1.25",
          "form": "에이....걸어다니는 행인 1이에요....",
          "original_form": "에이....걸어다니는 행인 1이에요....",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006471.1.26",
          "form": "공부못하구요....",
          "original_form": "공부못하구요....",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006471.1.27",
          "form": "또 어디보자....ㅋㅋㅋㅋ",
          "original_form": "또 어디보자....ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006471.1.28",
          "form": "아니 저기요 ㅋㅋㅋㅋ 저기요?",
          "original_form": "아니 저기요 ㅋㅋㅋㅋ 저기요?",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100006471.1.29",
          "form": "아니잖아요 ㅋㅋㅋㅋ",
          "original_form": "아니잖아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006471.1.30",
          "form": "그 학점 들고 어디가서 공부 못한다고 그러면 혼나요 ㅋㅋㅋ",
          "original_form": "그 학점 들고 어디가서 공부 못한다고 그러면 혼나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006471.1.31",
          "form": "과에서20등인걸여...38명중에",
          "original_form": "과에서20등인걸여...38명중에",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006471.1.32",
          "form": "못하는거죠,,,,",
          "original_form": "못하는거죠,,,,",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006471.1.33",
          "form": "이번 학기에 총점 4,0 함 넘겨보시죠!",
          "original_form": "이번 학기에 총점 4,0 함 넘겨보시죠!",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100006471.1.34",
          "form": "왠지 하실 수 있을것 같아요",
          "original_form": "왠지 하실 수 있을것 같아요",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
