      "utterance": [
        {
          "id": "MDRW2100024536.1.1",
          "form": "둘째아이가 고양이를 엄청 좋아해서",
          "original_form": "둘째아이가 고양이를 엄청 좋아해서",
          "speaker_id": "2",
          "time": "20210831 13:27"
        },
        {
          "id": "MDRW2100024536.1.2",
          "form": "매일 키우자고 얘기하거든요",
          "original_form": "매일 키우자고 얘기하거든요",
          "speaker_id": "2",
          "time": "20210831 13:27"
        },
        {
          "id": "MDRW2100024536.1.3",
          "form": "너무 좋은데 개키우는것 보다는 고양이 냄새가 더 심하더라구요",
          "original_form": "너무 좋은데 개키우는것 보다는 고양이 냄새가 더 심하더라구요",
          "speaker_id": "1",
          "time": "20210831 13:28"
        },
        {
          "id": "MDRW2100024536.1.4",
          "form": "깨끗한 애들이지만",
          "original_form": "깨끗한 애들이지만",
          "speaker_id": "1",
          "time": "20210831 13:29"
        },
        {
          "id": "MDRW2100024536.1.5",
          "form": "똥오줌 냄새가ㅠㅜ",
          "original_form": "똥오줌 냄새가ㅠㅜ",
          "speaker_id": "1",
          "time": "20210831 13:29"
        },
        {
          "id": "MDRW2100024536.1.6",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024536.1.7",
          "form": "ㅠㅠ 제가 일을 해서",
          "original_form": "ㅠㅠ 제가 일을 해서",
          "speaker_id": "2",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024536.1.8",
          "form": "계속 같이 있을수가 없어서 고민하는데",
          "original_form": "계속 같이 있을수가 없어서 고민하는데",
          "speaker_id": "2",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024536.1.9",
          "form": "털 빠짐도 심하고",
          "original_form": "털 빠짐도 심하고",
          "speaker_id": "2",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024536.1.10",
          "form": "냄새도 나나봐요",
          "original_form": "냄새도 나나봐요",
          "speaker_id": "2",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024536.1.11",
          "form": "그래서 사실 키우는데 큰 결심이 필요하더라구요",
          "original_form": "그래서 사실 키우는데 큰 결심이 필요하더라구요",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100024536.1.12",
          "form": "너무 예쁜데",
          "original_form": "너무 예쁜데",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100024536.1.13",
          "form": "그렇죠 ㅠ 정말 쉽지않아요",
          "original_form": "그렇죠 ㅠ 정말 쉽지않아요",
          "speaker_id": "2",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024536.1.14",
          "form": "거북이부터 키워봐야겠어요^^",
          "original_form": "거북이부터 키워봐야겠어요^^",
          "speaker_id": "2",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024536.1.15",
          "form": "거북이도 잘 키울수 있으려나 몰라요;;;",
          "original_form": "거북이도 잘 키울수 있으려나 몰라요;;;",
          "speaker_id": "2",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100024536.1.16",
          "form": "아이가 거북이 밥도 잘 챙겨주고 잘 도봐주면 고양이도 가능하지 않을까요?ㅎㅎ",
          "original_form": "아이가 거북이 밥도 잘 챙겨주고 잘 도봐주면 고양이도 가능하지 않을까요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 13:37"
        },
        {
          "id": "MDRW2100024536.1.17",
          "form": "네 일단 지금은 어려서 ㅎㅎ 5살이라 초등학교 가면 다시 생각해보자고 얘기해놨어요^^",
          "original_form": "네 일단 지금은 어려서 ㅎㅎ 5살이라 초등학교 가면 다시 생각해보자고 얘기해놨어요^^",
          "speaker_id": "2",
          "time": "20210831 13:38"
        },
        {
          "id": "MDRW2100024536.1.18",
          "form": "거북이 잘 키우는지 봐야겠어요^^",
          "original_form": "거북이 잘 키우는지 봐야겠어요^^",
          "speaker_id": "2",
          "time": "20210831 13:38"
        },
        {
          "id": "MDRW2100024536.1.19",
          "form": "아ㅎㅎㅎ아직 어리구나ㅎㅎ\r\n이참에 책임감도 같이 기를 수 있는 좋은 기회겠네요~^^",
          "original_form": "아ㅎㅎㅎ아직 어리구나ㅎㅎ\r\n이참에 책임감도 같이 기를 수 있는 좋은 기회겠네요~^^",
          "speaker_id": "1",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100024536.1.20",
          "form": "네 ㅎㅎ 아직 어려서 ㅎㅎ 애착인형이 고양이라",
          "original_form": "네 ㅎㅎ 아직 어려서 ㅎㅎ 애착인형이 고양이라",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100024536.1.21",
          "form": "고양이를 좋아하는거 같아요^^;;;",
          "original_form": "고양이를 좋아하는거 같아요^^;;;",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100024536.1.22",
          "form": "뭐 키워본적은 없으세요??",
          "original_form": "뭐 키워본적은 없으세요??",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100024536.1.23",
          "form": "전 이십년정도 개를 키웠었는데",
          "original_form": "전 이십년정도 개를 키웠었는데",
          "speaker_id": "1",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024536.1.24",
          "form": "이젠 못 키우겠어요ㅜㅜ",
          "original_form": "이젠 못 키우겠어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024536.1.25",
          "form": "아 개 키우셨구나",
          "original_form": "아 개 키우셨구나",
          "speaker_id": "2",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100024536.1.26",
          "form": "오래 키우셨네요",
          "original_form": "오래 키우셨네요",
          "speaker_id": "2",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100024536.1.27",
          "form": "아 각 다른개였어요",
          "original_form": "아 각 다른개였어요",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024536.1.28",
          "form": "죽는걸 또 못보겟어요ㅜㅜ",
          "original_form": "죽는걸 또 못보겟어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024536.1.29",
          "form": "아 그렇곤요 ㅠ",
          "original_form": "아 그렇곤요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100024536.1.30",
          "form": "힘드셨겠어요 ㅠ",
          "original_form": "힘드셨겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100024536.1.31",
          "form": "밥 주던 길냥이가 없어졌을때도",
          "original_form": "밥 주던 길냥이가 없어졌을때도",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100024536.1.32",
          "form": "슬퍼서 며칠 계속 마음이 안좋던데",
          "original_form": "슬퍼서 며칠 계속 마음이 안좋던데",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100024536.1.33",
          "form": "키우던 개면 ㅠㅠ 정말 너무 마음아플거같아요",
          "original_form": "키우던 개면 ㅠㅠ 정말 너무 마음아플거같아요",
          "speaker_id": "2",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100024536.1.34",
          "form": "네 정말 몇년은 힘들어서 다신 안키우고 싶어요ㅠㅠ",
          "original_form": "네 정말 몇년은 힘들어서 다신 안키우고 싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024536.1.35",
          "form": "그쵸 ㅠ 생각만해도",
          "original_form": "그쵸 ㅠ 생각만해도",
          "speaker_id": "2",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100024536.1.36",
          "form": "너무 힘드셨을거같아요 ㅠ",
          "original_form": "너무 힘드셨을거같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100024536.1.37",
          "form": "저희신랑도 그거 때문에 계속 반대해요",
