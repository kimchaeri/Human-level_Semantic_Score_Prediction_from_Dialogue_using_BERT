      "utterance": [
        {
          "id": "MDRW2100019648.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019648.1.2",
          "form": "아뇨 잘 안봐요 ㅋㅋㅋㅋ",
          "original_form": "아뇨 잘 안봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019648.1.3",
          "form": "근데 이번에 올림픽은 재밌게 봤어요",
          "original_form": "근데 이번에 올림픽은 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019648.1.4",
          "form": "아 맞아요 ㅋㅋ 올림픽 재밌었어요",
          "original_form": "아 맞아요 ㅋㅋ 올림픽 재밌었어요",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019648.1.5",
          "form": "뭘 젤 재밌게 보셨어요?",
          "original_form": "뭘 젤 재밌게 보셨어요?",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019648.1.6",
          "form": "양궁이랑 배구요!",
          "original_form": "양궁이랑 배구요!",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019648.1.7",
          "form": "오 저두요!",
          "original_form": "오 저두요!",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019648.1.8",
          "form": "양궁 결승 실시간으로 보셧어요?",
          "original_form": "양궁 결승 실시간으로 보셧어요?",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019648.1.9",
          "form": "네넼ㅋㄲ 진짜 심장 없어지는줄 알았어요",
          "original_form": "네넼ㅋㄲ 진짜 심장 없어지는줄 알았어요",
          "speaker_id": "1",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100019648.1.10",
          "form": "그쵸 ㅋㅋㅋ 선수보다 제가 더 떨었어요",
          "original_form": "그쵸 ㅋㅋㅋ 선수보다 제가 더 떨었어요",
          "speaker_id": "2",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100019648.1.11",
          "form": "진짜욬ㅋㄱ 아 슛오프 하는데 하 ..",
          "original_form": "진짜욬ㅋㄱ 아 슛오프 하는데 하 ..",
          "speaker_id": "1",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100019648.1.12",
          "form": "멘탈 대단해요 진짜 ㅋㅋ",
          "original_form": "멘탈 대단해요 진짜 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100019648.1.13",
          "form": "맞아요 선수들 멘탈이 젤 부러웠어요",
          "original_form": "맞아요 선수들 멘탈이 젤 부러웠어요",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019648.1.14",
          "form": "배구도 실시간으로 보셨나요?",
          "original_form": "배구도 실시간으로 보셨나요?",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019648.1.15",
          "form": "네네 ! 그 3-4위전빼고 다 실시간으로 봤어요",
          "original_form": "네네 ! 그 3-4위전빼고 다 실시간으로 봤어요",
          "speaker_id": "1",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019648.1.16",
          "form": "저도요 ㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100019648.1.17",
          "form": "한일전이랑 터키전이 젤 재밌었어요",
          "original_form": "한일전이랑 터키전이 젤 재밌었어요",
          "speaker_id": "2",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100019648.1.18",
          "form": "아 맞아요 진짜 둘 다 대박이였어요 ㅋㅋ",
          "original_form": "아 맞아요 진짜 둘 다 대박이였어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019648.1.19",
          "form": "심판때문에 빡쳤지만 ㅜㅜ",
          "original_form": "심판때문에 빡쳤지만 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019648.1.20",
          "form": "한일전 마지막세트는 도저히 생방으로 못보겠어서 친구들이 톡으로 중계해주는 거 봤어요 ㅋㅋㅋ",
          "original_form": "한일전 마지막세트는 도저히 생방으로 못보겠어서 친구들이 톡으로 중계해주는 거 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:05"
        },
        {
          "id": "MDRW2100019648.1.21",
          "form": "맞아요 이번 배구가 유난히 심판 운이 없었어요 ㅠ",
          "original_form": "맞아요 이번 배구가 유난히 심판 운이 없었어요 ㅠ",
