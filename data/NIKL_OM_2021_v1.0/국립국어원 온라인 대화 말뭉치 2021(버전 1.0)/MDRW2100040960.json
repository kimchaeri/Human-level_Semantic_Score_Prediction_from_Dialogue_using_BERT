      "utterance": [
        {
          "id": "MDRW2100040960.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100040960.1.2",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100040960.1.3",
          "form": "넹넹!!",
          "original_form": "넹넹!!",
          "speaker_id": "2",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100040960.1.4",
          "form": "컴터 게임좋아합니당",
          "original_form": "컴터 게임좋아합니당",
          "speaker_id": "2",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100040960.1.5",
          "form": "오오 무슨 게임하세요?",
          "original_form": "오오 무슨 게임하세요?",
          "speaker_id": "1",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100040960.1.6",
          "form": "저배그??",
          "original_form": "저배그??",
          "speaker_id": "2",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100040960.1.7",
          "form": "오 그렇군요 저도 해보고싶긴햇어요",
          "original_form": "오 그렇군요 저도 해보고싶긴햇어요",
          "speaker_id": "1",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100040960.1.8",
          "form": "처음에 게임하는방법 익히기 쉽나요?",
          "original_form": "처음에 게임하는방법 익히기 쉽나요?",
          "speaker_id": "1",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100040960.1.9",
          "form": "어음..",
          "original_form": "어음..",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100040960.1.10",
          "form": "저는 하는데1년정도 걸린거같아요",
          "original_form": "저는 하는데1년정도 걸린거같아요",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100040960.1.11",
          "form": "맨날주고 이랬죠머",
          "original_form": "맨날주고 이랬죠머",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100040960.1.12",
          "form": "아 그럴군요 ㅋㅋㅋㅋ꽤 오래하셧나봐요",
          "original_form": "아 그럴군요 ㅋㅋㅋㅋ꽤 오래하셧나봐요",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100040960.1.13",
          "form": "먼가 너렵더라구용",
          "original_form": "먼가 너렵더라구용",
          "speaker_id": "2",
          "time": "20210905 14:01"
        },
        {
          "id": "MDRW2100040960.1.14",
          "form": "아아 적응이 좀 걸엿군요",
          "original_form": "아아 적응이 좀 걸엿군요",
          "speaker_id": "1",
          "time": "20210905 14:02"
        },
        {
          "id": "MDRW2100040960.1.15",
          "form": "또 다른게임 머하세요?",
          "original_form": "또 다른게임 머하세요?",
          "speaker_id": "1",
          "time": "20210905 14:02"
        },
        {
          "id": "MDRW2100040960.1.16",
          "form": "로아나 메이플?",
          "original_form": "로아나 메이플?",
          "speaker_id": "2",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100040960.1.17",
          "form": "알피지게임합니당",
          "original_form": "알피지게임합니당",
          "speaker_id": "2",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100040960.1.18",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100040960.1.19",
          "form": "아 저는 게임을 잘 안하긴해요ㅋㅋㅋㅋㅋ 예전에 메이플은 했어요",
          "original_form": "아 저는 게임을 잘 안하긴해요ㅋㅋㅋㅋㅋ 예전에 메이플은 했어요",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100040960.1.20",
          "form": "카트도 햇구욬ㅋㅋㅋㅋ",
          "original_form": "카트도 햇구욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100040960.1.21",
          "form": "메이플잼는데재미없어용",
          "original_form": "메이플잼는데재미없어용",
          "speaker_id": "2",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100040960.1.22",
          "form": "뭔지알거같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "뭔지알거같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
