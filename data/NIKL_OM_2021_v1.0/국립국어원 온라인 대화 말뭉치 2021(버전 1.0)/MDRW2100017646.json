      "utterance": [
        {
          "id": "MDRW2100017646.1.1",
          "form": "카레 좋아해?",
          "original_form": "카레 좋아해?",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017646.1.2",
          "form": "매콤한 카레 먹는거 좋아해 너는?",
          "original_form": "매콤한 카레 먹는거 좋아해 너는?",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017646.1.3",
          "form": "나는 카레는 다 좋아",
          "original_form": "나는 카레는 다 좋아",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017646.1.4",
          "form": "카레에 새우튀김 올려 먹으면 맛있겠다",
          "original_form": "카레에 새우튀김 올려 먹으면 맛있겠다",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017646.1.5",
          "form": "소세지도 어울리지 않아?",
          "original_form": "소세지도 어울리지 않아?",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017646.1.6",
          "form": "맞아 구운 소세시도 맛있어",
          "original_form": "맞아 구운 소세시도 맛있어",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017646.1.7",
          "form": "카레에 들어간 당근은 어때?",
          "original_form": "카레에 들어간 당근은 어때?",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017646.1.8",
          "form": "당근은 그 어디에 있어도 달갑지않아 너는?",
          "original_form": "당근은 그 어디에 있어도 달갑지않아 너는?",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017646.1.9",
          "form": "당근은 차라리 생 당근이 나은것같아",
          "original_form": "당근은 차라리 생 당근이 나은것같아",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017646.1.10",
          "form": "맞아 생당근은 달달하니 맛있는듯",
          "original_form": "맞아 생당근은 달달하니 맛있는듯",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017646.1.11",
          "form": "오이도 좋아해?",
          "original_form": "오이도 좋아해?",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017646.1.12",
          "form": "생 오이는 괜찮아,  너는?",
          "original_form": "생 오이는 괜찮아,  너는?",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017646.1.13",
          "form": "나도 당근보다 훨 좋아해",
          "original_form": "나도 당근보다 훨 좋아해",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017646.1.14",
          "form": "역시 오이를 더 좋아하는구나",
          "original_form": "역시 오이를 더 좋아하는구나",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017646.1.15",
          "form": "오이피클도 맛있지",
          "original_form": "오이피클도 맛있지",
          "speaker_id": "2",
          "time": "20210826 21:38"
        }
      ]
