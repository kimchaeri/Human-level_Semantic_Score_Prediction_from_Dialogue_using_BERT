      "utterance": [
        {
          "id": "MDRW2100041914.1.1",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041914.1.2",
          "form": "운동 하시나용",
          "original_form": "운동 하시나용",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041914.1.3",
          "form": "ㅋㅋㅋㅋ저..요즘은 출퇴근 힘들단 핑ㄱㅖ로..",
          "original_form": "ㅋㅋㅋㅋ저..요즘은 출퇴근 힘들단 핑ㄱㅖ로..",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041914.1.4",
          "form": "안하고있어요..근데 심각성을 느낍니다",
          "original_form": "안하고있어요..근데 심각성을 느낍니다",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041914.1.5",
          "form": "아 저두요",
          "original_form": "아 저두요",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041914.1.6",
          "form": "저 살이 정말",
          "original_form": "저 살이 정말",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041914.1.7",
          "form": "저 삼키로 쪗어요..ㅎ",
          "original_form": "저 삼키로 쪗어요..ㅎ",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041914.1.8",
          "form": "전 오키로요",
          "original_form": "전 오키로요",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041914.1.9",
          "form": "먹고 바로 눕게돼여 ㅠㅠㅠㅠㅠ",
          "original_form": "먹고 바로 눕게돼여 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041914.1.10",
          "form": "저돜ㅋㅋㅋㅋㅋ",
          "original_form": "저돜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041914.1.11",
          "form": "밥먹ㄱ고 바로 침댜에서 넷플보고",
          "original_form": "밥먹ㄱ고 바로 침댜에서 넷플보고",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041914.1.12",
          "form": "미쳤어요..",
          "original_form": "미쳤어요..",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041914.1.13",
          "form": "근데 밥도 많이먹어",
          "original_form": "근데 밥도 많이먹어",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041914.1.14",
          "form": "환장",
          "original_form": "환장",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041914.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ 아 저랑 일상이",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아 저랑 일상이",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041914.1.16",
          "form": "비슷하시네요 ㅎ ㅎㅎ",
          "original_form": "비슷하시네요 ㅎ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041914.1.17",
          "form": "살 어찌빼는건지 휴",
          "original_form": "살 어찌빼는건지 휴",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041914.1.18",
          "form": "저 추석지나고부터 킥복싱 배우려고했는데",
          "original_form": "저 추석지나고부터 킥복싱 배우려고했는데",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041914.1.19",
          "form": "영어학원 먼저 다니려고 또 미뤗어요",
          "original_form": "영어학원 먼저 다니려고 또 미뤗어요",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041914.1.20",
          "form": "ㅋㅋㅋㅋ ㅋㅋㅋ 허 킥복싱.. 멋있네요",
          "original_form": "ㅋㅋㅋㅋ ㅋㅋㅋ 허 킥복싱.. 멋있네요",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041914.1.21",
          "form": "저도 운동 다녀야 하는데.흑",
          "original_form": "저도 운동 다녀야 하는데.흑",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041914.1.22",
          "form": "킥복싱 배우던 친구가 엄청 재밋어하더라구요",
          "original_form": "킥복싱 배우던 친구가 엄청 재밋어하더라구요",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041914.1.23",
          "form": "궁금해보여서 배우ㅓ보려구요",
          "original_form": "궁금해보여서 배우ㅓ보려구요",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041914.1.24",
          "form": "배워보고싶은 운동 있으세요?",
          "original_form": "배워보고싶은 운동 있으세요?",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041914.1.25",
          "form": "저 발레요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 발레요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041914.1.26",
          "form": "근데 학원이 다 멀어서요 ㅠㅠ",
          "original_form": "근데 학원이 다 멀어서요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041914.1.27",
          "form": "오 발레! 저 주변에 발레 배웟던 친구들 꽤 있었는데",
          "original_form": "오 발레! 저 주변에 발레 배웟던 친구들 꽤 있었는데",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041914.1.28",
          "form": "와 그거 진짜 엄청 힘들던데요",
          "original_form": "와 그거 진짜 엄청 힘들던데요",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041914.1.29",
          "form": "발레.. 은근 힘든 운동이에요 진짜",
          "original_form": "발레.. 은근 힘든 운동이에요 진짜",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041914.1.30",
          "form": "발레리나분들 근육 진짜",
          "original_form": "발레리나분들 근육 진짜",
          "speaker_id": "1",
