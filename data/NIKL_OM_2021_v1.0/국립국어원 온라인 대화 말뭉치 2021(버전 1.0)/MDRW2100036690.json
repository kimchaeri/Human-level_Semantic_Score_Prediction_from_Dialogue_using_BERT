      "utterance": [
        {
          "id": "MDRW2100036690.1.1",
          "form": "네이버 지난번에 내가",
          "original_form": "네이버 지난번에 내가",
          "speaker_id": "2",
          "time": "20210831 08:55"
        },
        {
          "id": "MDRW2100036690.1.2",
          "form": "김천빵살때 보낸거 있지",
          "original_form": "김천빵살때 보낸거 있지",
          "speaker_id": "2",
          "time": "20210831 08:55"
        },
        {
          "id": "MDRW2100036690.1.3",
          "form": "그게 경북지원쿠폰인데 만원2장 오천원2장받아졌는데 한번 받은사람만 만원 쿠폰이 추가발급이 되더라고",
          "original_form": "그게 경북지원쿠폰인데 만원2장 오천원2장받아졌는데 한번 받은사람만 만원 쿠폰이 추가발급이 되더라고",
          "speaker_id": "2",
          "time": "20210831 08:56"
        },
        {
          "id": "MDRW2100036690.1.4",
          "form": "카페 핫하던데",
          "original_form": "카페 핫하던데",
          "speaker_id": "2",
          "time": "20210831 08:56"
        },
        {
          "id": "MDRW2100036690.1.5",
          "form": "응응ㅋㅋ",
          "original_form": "응응ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:56"
        },
        {
          "id": "MDRW2100036690.1.6",
          "form": "그거였구나",
          "original_form": "그거였구나",
          "speaker_id": "1",
          "time": "20210831 08:57"
        },
        {
          "id": "MDRW2100036690.1.7",
          "form": "쿠폰누르긴 했어??",
          "original_form": "쿠폰누르긴 했어??",
          "speaker_id": "2",
          "time": "20210831 08:57"
        },
        {
          "id": "MDRW2100036690.1.8",
          "form": "그때 내 살짝 뭐가왔을때였음ㅋㅋ",
          "original_form": "그때 내 살짝 뭐가왔을때였음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:57"
        },
        {
          "id": "MDRW2100036690.1.9",
          "form": "지침.ㅋㅋㅋ 만사피곤",
          "original_form": "지침.ㅋㅋㅋ 만사피곤",
          "speaker_id": "2",
          "time": "20210831 08:57"
        },
        {
          "id": "MDRW2100036690.1.10",
          "form": "암것도 하기싫은 ...ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "암것도 하기싫은 ...ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:57"
        },
        {
          "id": "MDRW2100036690.1.11",
          "form": "오빠한테",
          "original_form": "오빠한테",
          "speaker_id": "1",
          "time": "20210831 08:57"
        },
        {
          "id": "MDRW2100036690.1.12",
          "form": "편의점 과자도 김천빵도",
          "original_form": "편의점 과자도 김천빵도",
          "speaker_id": "1",
          "time": "20210831 08:57"
        },
        {
          "id": "MDRW2100036690.1.13",
          "form": "싸게사는 공짜로받는거있었는데",
          "original_form": "싸게사는 공짜로받는거있었는데",
          "speaker_id": "1",
          "time": "20210831 08:58"
        },
        {
          "id": "MDRW2100036690.1.14",
          "form": "ㅋㅋㅋㅋ 오빠가 화나게 해서 ㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 오빠가 화나게 해서 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 08:58"
        },
        {
          "id": "MDRW2100036690.1.15",
          "form": "아무것도 안했다. 하기 귀찮더라. 내가 회원가입하구 싸게얻음 뭐할건데? 그거 내한테 오는게아니라 식구들입으로가는데 ...",
          "original_form": "아무것도 안했다. 하기 귀찮더라. 내가 회원가입하구 싸게얻음 뭐할건데? 그거 내한테 오는게아니라 식구들입으로가는데 ...",
          "speaker_id": "1",
          "time": "20210831 08:59"
        },
        {
          "id": "MDRW2100036690.1.16",
          "form": "그래서 오빠월급으로만 살림굴리게",
          "original_form": "그래서 오빠월급으로만 살림굴리게",
          "speaker_id": "1",
          "time": "20210831 08:59"
        },
        {
          "id": "MDRW2100036690.1.17",
          "form": "ㅋㅋㅋㅋ그러니뭐래>>",
          "original_form": "ㅋㅋㅋㅋ그러니뭐래>>",
          "speaker_id": "2",
          "time": "20210831 08:59"
        },
        {
          "id": "MDRW2100036690.1.18",
          "form": "했더니",
          "original_form": "했더니",
          "speaker_id": "1",
          "time": "20210831 08:59"
        },
        {
          "id": "MDRW2100036690.1.19",
          "form": "어이없어하지",
          "original_form": "어이없어하지",
          "speaker_id": "1",
          "time": "20210831 08:59"
        },
        {
          "id": "MDRW2100036690.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:59"
        },
        {
          "id": "MDRW2100036690.1.21",
          "form": "다 오빠먹는거아이가 그래서 안샀다",
          "original_form": "다 오빠먹는거아이가 그래서 안샀다",
          "speaker_id": "1",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100036690.1.22",
          "form": "했더니 진짜 어이없어해다",
          "original_form": "했더니 진짜 어이없어해다",
          "speaker_id": "1",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100036690.1.23",
          "form": "하대ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "하대ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100036690.1.24",
          "form": "아니 뭐 틀린말은 아니잖아??",
          "original_form": "아니 뭐 틀린말은 아니잖아??",
          "speaker_id": "2",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100036690.1.25",
          "form": "니가 알바해서",
          "original_form": "니가 알바해서",
          "speaker_id": "1",
          "time": "20210831 09:01"
        },
        {
          "id": "MDRW2100036690.1.26",
          "form": "살림에 보탬된다고",
          "original_form": "살림에 보탬된다고",
          "speaker_id": "1",
          "time": "20210831 09:01"
        },
        {
          "id": "MDRW2100036690.1.27",
          "form": "막 하긴하더나",
          "original_form": "막 하긴하더나",
          "speaker_id": "1",
          "time": "20210831 09:01"
        },
        {
          "id": "MDRW2100036690.1.28",
          "form": "라",
          "original_form": "라",
          "speaker_id": "1",
          "time": "20210831 09:01"
        },
        {
          "id": "MDRW2100036690.1.29",
          "form": "웃겼음ㅋㅋ",
          "original_form": "웃겼음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:01"
        },
        {
          "id": "MDRW2100036690.1.30",
          "form": "ㅋㅋ 아니 근데 나도 내 먹는거 아닌데 안사 이러다가도 막상 그거 놓치고나면 비싸게 사야될때 있잖아 같은 물건을.",
          "original_form": "ㅋㅋ 아니 근데 나도 내 먹는거 아닌데 안사 이러다가도 막상 그거 놓치고나면 비싸게 사야될때 있잖아 같은 물건을.",
          "speaker_id": "2",
          "time": "20210831 09:02"
        },
        {
          "id": "MDRW2100036690.1.31",
          "form": "머지가 솔직히 크게왔고ㅋㅋ두번째로 오빠가 또 카고 그라이 아무것도하기싫더라고ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "머지가 솔직히 크게왔고ㅋㅋ두번째로 오빠가 또 카고 그라이 아무것도하기싫더라고ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:02"
        },
        {
          "id": "MDRW2100036690.1.32",
          "form": "그래서 핫딜뜨면 사게 되 ㅠㅠ",
          "original_form": "그래서 핫딜뜨면 사게 되 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 09:02"
        },
        {
          "id": "MDRW2100036690.1.33",
          "form": "머지..에라이",
          "original_form": "머지..에라이",
          "speaker_id": "2",
          "time": "20210831 09:02"
        },
        {
          "id": "MDRW2100036690.1.34",
          "form": "맞어맞어ㅋㅋㅋㅋ",
          "original_form": "맞어맞어ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:02"
        },
        {
          "id": "MDRW2100036690.1.35",
          "form": "여튼 물티슈는 진짜잘샀다이ㅋㅋ빵도ㅋ",
          "original_form": "여튼 물티슈는 진짜잘샀다이ㅋㅋ빵도ㅋ",
