      "utterance": [
        {
          "id": "MDRW2100006566.1.1",
          "form": "옵치 1이랑 2랑 비슷하면 옵치2도 해보긴할것같아",
          "original_form": "옵치 1이랑 2랑 비슷하면 옵치2도 해보긴할것같아",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100006566.1.2",
          "form": "나도 해보긴할듯",
          "original_form": "나도 해보긴할듯",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100006566.1.3",
          "form": "거의 똑같을 거란 말도 있고 뭐",
          "original_form": "거의 똑같을 거란 말도 있고 뭐",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100006566.1.4",
          "form": "그랬었는데 나도 기억은 잘 안난다",
          "original_form": "그랬었는데 나도 기억은 잘 안난다",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100006566.1.5",
          "form": "ㅇㅏ 애쉬나오나ㅎ",
          "original_form": "ㅇㅏ 애쉬나오나ㅎ",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100006566.1.6",
          "form": "시메도 있어야하는데",
          "original_form": "시메도 있어야하는데",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100006566.1.7",
          "form": "나오겠지 영웅들은 그대로 다",
          "original_form": "나오겠지 영웅들은 그대로 다",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100006566.1.8",
          "form": "추가되면 추가되지 빠지지는 않을 것 같아",
          "original_form": "추가되면 추가되지 빠지지는 않을 것 같아",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100006566.1.9",
          "form": "흐엑 그럼 넘 많지않나?",
          "original_form": "흐엑 그럼 넘 많지않나?",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100006566.1.10",
          "form": "뭐 롤같은 경우는 몇백개씩 있으니까",
          "original_form": "뭐 롤같은 경우는 몇백개씩 있으니까",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100006566.1.11",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100006566.1.12",
          "form": "진짜??? 그걸어케해",
          "original_form": "진짜??? 그걸어케해",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100006566.1.13",
          "form": "사실 오버워치 인기가 식은 이유가 영웅이 너무 적어서 라는 말이 있었어",
          "original_form": "사실 오버워치 인기가 식은 이유가 영웅이 너무 적어서 라는 말이 있었어",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100006566.1.14",
          "form": "난 지금도 못하는캐릭 많은데여?!!!?",
          "original_form": "난 지금도 못하는캐릭 많은데여?!!!?",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100006566.1.15",
          "form": "그리고 너도 나보고 하나를 계속하라고 마스터할때까지",
          "original_form": "그리고 너도 나보고 하나를 계속하라고 마스터할때까지",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100006566.1.16",
          "form": "그랫자너",
          "original_form": "그랫자너",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100006566.1.17",
          "form": "ㅋㅋㅋㅋㅋ사람들은 새로운 재미를 원해서 그런가봐",
          "original_form": "ㅋㅋㅋㅋㅋ사람들은 새로운 재미를 원해서 그런가봐",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100006566.1.18",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100006566.1.19",
          "form": "하나 하고 게임을 이해한 후에 다른 거 해보는게 좋지",
          "original_form": "하나 하고 게임을 이해한 후에 다른 거 해보는게 좋지",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100006566.1.20",
          "form": "그래 근데 백개면",
          "original_form": "그래 근데 백개면",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100006566.1.21",
          "form": "평생 해도 안끝날것같은뎅",
          "original_form": "평생 해도 안끝날것같은뎅",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100006566.1.22",
          "form": "ㅋㅋㅋㅋㅋ뭐 그렇게 있다보면 자주 등장하는 몇개가 생긴대",
          "original_form": "ㅋㅋㅋㅋㅋ뭐 그렇게 있다보면 자주 등장하는 몇개가 생긴대",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100006566.1.23",
          "form": "나도 롤은 잘 안해서 모르겠다",
          "original_form": "나도 롤은 잘 안해서 모르겠다",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100006566.1.24",
          "form": "등장??내가 하는거아니구",
