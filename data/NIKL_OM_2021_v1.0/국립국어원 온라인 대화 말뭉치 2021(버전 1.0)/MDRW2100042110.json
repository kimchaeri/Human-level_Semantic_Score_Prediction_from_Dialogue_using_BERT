      "utterance": [
        {
          "id": "MDRW2100042110.1.1",
          "form": "저도 지하철...ㅜㅜ",
          "original_form": "저도 지하철...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042110.1.2",
          "form": "그래도 버스보단 낫겠지해요ㅜ",
          "original_form": "그래도 버스보단 낫겠지해요ㅜ",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042110.1.3",
          "form": "비 오는 날 지하철 힘들죠ㅜ",
          "original_form": "비 오는 날 지하철 힘들죠ㅜ",
          "speaker_id": "1",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042110.1.4",
          "form": "우산 물기가 서로 뭍기도하고",
          "original_form": "우산 물기가 서로 뭍기도하고",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042110.1.5",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042110.1.6",
          "form": "다리에 다 스치고요ㅜㅠ",
          "original_form": "다리에 다 스치고요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042110.1.7",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100042110.1.8",
          "form": "서로의 습기로 눅눅해지고",
          "original_form": "서로의 습기로 눅눅해지고",
          "speaker_id": "1",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100042110.1.9",
          "form": "ㅜㅋㅋ 여름철에 최고죠! ㅋㅋ",
          "original_form": "ㅜㅋㅋ 여름철에 최고죠! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100042110.1.10",
          "form": "그거에 비하면 지금이 훨씬 나은거같기도하고 ㅎㅎ",
          "original_form": "그거에 비하면 지금이 훨씬 나은거같기도하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042110.1.11",
          "form": "아 여름철...",
          "original_form": "아 여름철...",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042110.1.12",
          "form": "서로의 열기와 땀이ㅠ",
          "original_form": "서로의 열기와 땀이ㅠ",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042110.1.13",
          "form": "그래도 요즘은 지하철에서 여름에 냉난방을 잘해주는 편이라",
          "original_form": "그래도 요즘은 지하철에서 여름에 냉난방을 잘해주는 편이라",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042110.1.14",
          "form": "ㅋㅋ 진짜ㅋㅋ 너무 싫어요!!! 악",
          "original_form": "ㅋㅋ 진짜ㅋㅋ 너무 싫어요!!! 악",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042110.1.15",
          "form": "아!! 그래서 요즘 불쾌감 덜 느끼긴해요!",
          "original_form": "아!! 그래서 요즘 불쾌감 덜 느끼긴해요!",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042110.1.16",
          "form": "네 여름에 에어컨은 필수예요",
          "original_form": "네 여름에 에어컨은 필수예요",
          "speaker_id": "1",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100042110.1.17",
          "form": "여름에 오히려 가디건 많이 입는거같아요",
          "original_form": "여름에 오히려 가디건 많이 입는거같아요",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100042110.1.18",
          "form": "네 에어컨이 추워서",
          "original_form": "네 에어컨이 추워서",
          "speaker_id": "1",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100042110.1.19",
          "form": "저희 사무실은 아직도 에어컨 켜거든요",
          "original_form": "저희 사무실은 아직도 에어컨 켜거든요",
          "speaker_id": "1",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100042110.1.20",
          "form": "그래서 가디건은 필수예요",
          "original_form": "그래서 가디건은 필수예요",
          "speaker_id": "1",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100042110.1.21",
          "form": "저희도요ㅋㅋ 추운데 끄기엔 또 아쉽고요",
          "original_form": "저희도요ㅋㅋ 추운데 끄기엔 또 아쉽고요",
          "speaker_id": "2",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100042110.1.22",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100042110.1.23",
          "form": "근데 에어컨 켜면 추운 날씨고요",
          "original_form": "근데 에어컨 켜면 추운 날씨고요",
          "speaker_id": "1",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100042110.1.24",
          "form": "그러니까요 끄면 습하고ㅜ",
          "original_form": "그러니까요 끄면 습하고ㅜ",
