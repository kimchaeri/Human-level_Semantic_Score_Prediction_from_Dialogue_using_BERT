        {
          "id": "MDRW2100026509.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026509.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026509.1.3",
          "form": "동물키우세요?",
          "original_form": "동물키우세요?",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026509.1.4",
          "form": "아직오",
          "original_form": "아직오",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026509.1.5",
          "form": "오 나중에 키우실거에여?",
          "original_form": "오 나중에 키우실거에여?",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026509.1.6",
          "form": "여건되면요",
          "original_form": "여건되면요",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026509.1.7",
          "form": "어떤동물 키우고 싶으세여?",
          "original_form": "어떤동물 키우고 싶으세여?",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026509.1.8",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026509.1.9",
          "form": "어떤 종 키우고싶으세요?",
          "original_form": "어떤 종 키우고싶으세요?",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026509.1.10",
          "form": "종은 잘 몰라서",
          "original_form": "종은 잘 몰라서",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026509.1.11",
          "form": "아항 그럼 소형견? 대형견? 어떤게 좋으세요",
          "original_form": "아항 그럼 소형견? 대형견? 어떤게 좋으세요",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026509.1.12",
          "form": "소형이요",
          "original_form": "소형이요",
          "speaker_id": "1",
          "time": "20210902 07:16"
        },
        {
          "id": "MDRW2100026509.1.13",
          "form": "아항 저는 아무리 작더라도 동물 무서워해서 못 기를것같아요",
          "original_form": "아항 저는 아무리 작더라도 동물 무서워해서 못 기를것같아요",
          "speaker_id": "2",
          "time": "20210902 08:13"
        },
        {
          "id": "MDRW2100026509.1.14",
          "form": "케어가힘들어요",
          "original_form": "케어가힘들어요",
          "speaker_id": "1",
          "time": "20210902 09:42"
        }
