      "utterance": [
        {
          "id": "MDRW2100043514.1.1",
          "form": "좋아하는 음식은요?",
          "original_form": "좋아하는 음식은요?",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043514.1.2",
          "form": "파스타용",
          "original_form": "파스타용",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043514.1.3",
          "form": "사실 다요",
          "original_form": "사실 다요",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043514.1.4",
          "form": "ㅋㅋㅋ 전 고기와 면",
          "original_form": "ㅋㅋㅋ 전 고기와 면",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043514.1.5",
          "form": "ㅋㅋㅋㅋ 완젼좋죠 ...저 면사랑 회원 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 완젼좋죠 ...저 면사랑 회원 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043514.1.6",
          "form": "회원 ㅋㅋ\r\n모임이 잇나요ㅋㅋㅋ",
          "original_form": "회원 ㅋㅋ\r\n모임이 잇나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043514.1.7",
          "form": "아뇨 혼자요",
          "original_form": "아뇨 혼자요",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043514.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043514.1.9",
          "form": "비빔국수 특히 넘넘 조아함니다",
          "original_form": "비빔국수 특히 넘넘 조아함니다",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043514.1.10",
          "form": "칼국수랑여",
          "original_form": "칼국수랑여",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043514.1.11",
          "form": "와 비빔 진리요ㅠ",
          "original_form": "와 비빔 진리요ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043514.1.12",
          "form": "먹고싶네여...",
          "original_form": "먹고싶네여...",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043514.1.13",
          "form": "비빔에  동치미 육수 넣으면 꿀맛이에요",
          "original_form": "비빔에  동치미 육수 넣으면 꿀맛이에요",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043514.1.14",
          "form": "우왘ㅋㅋㅋㅋ 그건 생각못해봤어요",
          "original_form": "우왘ㅋㅋㅋㅋ 그건 생각못해봤어요",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043514.1.15",
          "form": "중독적인 맛이져",
          "original_form": "중독적인 맛이져",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043514.1.16",
          "form": "해먹어바야겟어요",
          "original_form": "해먹어바야겟어요",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100043514.1.17",
          "form": "한번 먹어보고 동치미육수 쟁엿어요ㅋㅋ",
          "original_form": "한번 먹어보고 동치미육수 쟁엿어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
