      "utterance": [
        {
          "id": "MDRW2100028371.1.1",
          "form": "이형종 왜 저래",
          "original_form": "이형종 왜 저래",
          "speaker_id": "1",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028371.1.2",
          "form": "왤케 잘해",
          "original_form": "왤케 잘해",
          "speaker_id": "1",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028371.1.3",
          "form": "뭔데",
          "original_form": "뭔데",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028371.1.4",
          "form": "우리 팀 맞냐고",
          "original_form": "우리 팀 맞냐고",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028371.1.5",
          "form": "이상하다",
          "original_form": "이상하다",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028371.1.6",
          "form": "벌써 5연승인데",
          "original_form": "벌써 5연승인데",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028371.1.7",
          "form": "이제 져야하는데?",
          "original_form": "이제 져야하는데?",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028371.1.8",
          "form": "왤케 꾸준히잘하는거임",
          "original_form": "왤케 꾸준히잘하는거임",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028371.1.9",
          "form": "이제 질 때 됐다는 말을",
          "original_form": "이제 질 때 됐다는 말을",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028371.1.10",
          "form": "3일째 하고 있음",
          "original_form": "3일째 하고 있음",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028371.1.11",
          "form": "아니 이재원진짜",
          "original_form": "아니 이재원진짜",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028371.1.12",
          "form": "길죽한 이형종",
          "original_form": "길죽한 이형종",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028371.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ근데 이형종보다 귀엽게 생김",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ근데 이형종보다 귀엽게 생김",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028371.1.14",
          "form": "아니웃기다",
          "original_form": "아니웃기다",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028371.1.15",
          "form": "어제 올라온 엘튜브보셧나요",
          "original_form": "어제 올라온 엘튜브보셧나요",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028371.1.16",
          "form": "본 듯",
          "original_form": "본 듯",
          "speaker_id": "1",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028371.1.17",
          "form": "정우영이 올스타 마스크 자랑하는 거",
          "original_form": "정우영이 올스타 마스크 자랑하는 거",
          "speaker_id": "1",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028371.1.18",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028371.1.19",
          "form": "웃기는 사람들이야",
          "original_form": "웃기는 사람들이야",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028371.1.20",
          "form": "근데 차단장은",
          "original_form": "근데 차단장은",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028371.1.21",
          "form": "...대충 입입입짤",
          "original_form": "...대충 입입입짤",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028371.1.22",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028371.1.23",
          "form": "재미있네...",
          "original_form": "재미있네...",
          "speaker_id": "2",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100028371.1.24",
          "form": "찬규가 올린 마되겐나 매치 스토리 봄?",
          "original_form": "찬규가 올린 마되겐나 매치 스토리 봄?",
          "speaker_id": "1",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100028371.1.25",
          "form": "아니 뭐야 아뇨",
          "original_form": "아니 뭐야 아뇨",
          "speaker_id": "2",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028371.1.26",
          "form": "웃기다 스토리 올렸는데",
          "original_form": "웃기다 스토리 올렸는데",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028371.1.27",
          "form": "나혼자 개꿀잼 스토리 놓치고 살았네",
          "original_form": "나혼자 개꿀잼 스토리 놓치고 살았네",
          "speaker_id": "2",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028371.1.28",
          "form": "웃겨요 나중에 함 보셈",
          "original_form": "웃겨요 나중에 함 보셈",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028371.1.29",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210902 19:35"
        }
      ]
    }
  ]
