      "utterance": [
        {
          "id": "MDRW2100009409.1.1",
          "form": "결국 못참고 닭똥집이랑 아까먹고남은 제육을 데워왔네요",
          "original_form": "결국 못참고 닭똥집이랑 아까먹고남은 제육을 데워왔네요",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009409.1.2",
          "form": "점심드셨어요?",
          "original_form": "점심드셨어요?",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100009409.1.3",
          "form": "네 지금먹었어요",
          "original_form": "네 지금먹었어요",
          "speaker_id": "2",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100009409.1.4",
          "form": "올만에 된찌끓여서 너비아니굽고 샐러드랑~~~",
          "original_form": "올만에 된찌끓여서 너비아니굽고 샐러드랑~~~",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100009409.1.5",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100009409.1.6",
          "form": "오ㅎ 맛나겠어요",
          "original_form": "오ㅎ 맛나겠어요",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100009409.1.7",
          "form": "전 비가와서 바지락 칼국수",
          "original_form": "전 비가와서 바지락 칼국수",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100009409.1.8",
          "form": "비오는날 딱이네요",
          "original_form": "비오는날 딱이네요",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100009409.1.9",
          "form": "뼈다귀해장국 먹으러 나가려다 귀찮아서 집밥먹었어요",
          "original_form": "뼈다귀해장국 먹으러 나가려다 귀찮아서 집밥먹었어요",
          "speaker_id": "2",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100009409.1.10",
          "form": "면적게달라니 바지락도 적게ㅠㅠ",
          "original_form": "면적게달라니 바지락도 적게ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100009409.1.11",
          "form": "잘하셨네요 비오면 걍 집이 최고인듯",
          "original_form": "잘하셨네요 비오면 걍 집이 최고인듯",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100009409.1.12",
          "form": "헐~~ 바지락많아야 좋은데",
          "original_form": "헐~~ 바지락많아야 좋은데",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100009409.1.13",
          "form": "그니까요 원래 맛집이라 국물은 그래도 시원ㅎ",
          "original_form": "그니까요 원래 맛집이라 국물은 그래도 시원ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100009409.1.14",
          "form": "맛있었겠어요.",
          "original_form": "맛있었겠어요.",
          "speaker_id": "2",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100009409.1.15",
          "form": "바지락도 좋은데 전 감자많이들어간것도 좋아해요",
          "original_form": "바지락도 좋은데 전 감자많이들어간것도 좋아해요",
          "speaker_id": "2",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100009409.1.16",
          "form": "아 저 그 골목식당에 옹심이 먹어보고싶더라구요",
          "original_form": "아 저 그 골목식당에 옹심이 먹어보고싶더라구요",
          "speaker_id": "1",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100009409.1.17",
          "form": "옹심이는 안먹어봤어요",
          "original_form": "옹심이는 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210824 13:54"
        },
        {
          "id": "MDRW2100009409.1.18",
          "form": "저도요 맛있다고 극찬하더라구요",
          "original_form": "저도요 맛있다고 극찬하더라구요",
          "speaker_id": "1",
          "time": "20210824 13:54"
        },
        {
          "id": "MDRW2100009409.1.19",
          "form": "궁금은한데 새로운거에 도전하는걸 무서워해요~~",
          "original_form": "궁금은한데 새로운거에 도전하는걸 무서워해요~~",
          "speaker_id": "2",
          "time": "20210824 13:54"
        },
        {
          "id": "MDRW2100009409.1.20",
          "form": "ㅎㅎ 울가족들이 다 그래요.  먹던것만~~ 놀러가도 가는곳만가요",
          "original_form": "ㅎㅎ 울가족들이 다 그래요.  먹던것만~~ 놀러가도 가는곳만가요",
          "speaker_id": "2",
          "time": "20210824 13:55"
        },
        {
          "id": "MDRW2100009409.1.21",
          "form": "막상 가도 그맛이 그맛이긴해요",
          "original_form": "막상 가도 그맛이 그맛이긴해요",
          "speaker_id": "1",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100009409.1.22",
          "form": "줄서서 먹을정돈가 싶기도",
          "original_form": "줄서서 먹을정돈가 싶기도",
          "speaker_id": "1",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100009409.1.23",
          "form": "ㅎ 줄조금이면 도전하는데 잘안서요.",
          "original_form": "ㅎ 줄조금이면 도전하는데 잘안서요.",
          "speaker_id": "2",
          "time": "20210824 13:57"
        }
      ]
    }
