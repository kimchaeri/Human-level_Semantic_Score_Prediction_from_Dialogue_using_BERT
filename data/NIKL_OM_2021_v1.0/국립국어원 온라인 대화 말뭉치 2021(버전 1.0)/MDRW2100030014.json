        {
          "id": "MDRW2100030014.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100030014.1.2",
          "form": "어행갈래요",
          "original_form": "어행갈래요",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100030014.1.3",
          "form": "박사님 다시 한번 말씀드립니다 연애는 무슨 연애 입니까 공부 하셔야죠",
          "original_form": "박사님 다시 한번 말씀드립니다 연애는 무슨 연애 입니까 공부 하셔야죠",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100030014.1.4",
          "form": "ㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100030014.1.5",
          "form": "ㅋㅋㅋㅋㅋ..... 너무하돠",
          "original_form": "ㅋㅋㅋㅋㅋ..... 너무하돠",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100030014.1.6",
          "form": "어디가고싶음?",
          "original_form": "어디가고싶음?",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100030014.1.7",
          "form": "딱 한군대만",
          "original_form": "딱 한군대만",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100030014.1.8",
          "form": "갈수있음",
          "original_form": "갈수있음",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100030014.1.9",
          "form": "하와이요",
          "original_form": "하와이요",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030014.1.10",
          "form": "오 대박 왜 하와이를 선택하셨나요",
          "original_form": "오 대박 왜 하와이를 선택하셨나요",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030014.1.11",
          "form": "하와이!멋지자나요",
          "original_form": "하와이!멋지자나요",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030014.1.12",
          "form": "혹시 홀라 홀라 홀라 춤을 추고 싶어서",
          "original_form": "혹시 홀라 홀라 홀라 춤을 추고 싶어서",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030014.1.13",
          "form": "휴양하고 쇼핑하고싶어요",
          "original_form": "휴양하고 쇼핑하고싶어요",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030014.1.14",
          "form": "아 좋은 곳입니다",
          "original_form": "아 좋은 곳입니다",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030014.1.15",
          "form": "저 쇼핑하러 한번 가셨습니다",
          "original_form": "저 쇼핑하러 한번 가셨습니다",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030014.1.16",
          "form": "아 쇼핑 아니고 서핑",
          "original_form": "아 쇼핑 아니고 서핑",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030014.1.17",
          "form": "대박!!!!",
          "original_form": "대박!!!!",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030014.1.18",
          "form": "서핑하세용 ?",
          "original_form": "서핑하세용 ?",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030014.1.19",
          "form": "잘 못하는 왜 해보려구요",
          "original_form": "잘 못하는 왜 해보려구요",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030014.1.20",
          "form": "저도 햇엇눈데",
          "original_form": "저도 햇엇눈데",
          "speaker_id": "1",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100030014.1.21",
          "form": "한 열 번 했습니다 초짜 초짜",
          "original_form": "한 열 번 했습니다 초짜 초짜",
          "speaker_id": "2",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100030014.1.22",
          "form": "하와이서핑 멋지네용",
          "original_form": "하와이서핑 멋지네용",
          "speaker_id": "1",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100030014.1.23",
          "form": "하와이는",
          "original_form": "하와이는",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030014.1.24",
          "form": "저두...파도보기 힘들던데용",
          "original_form": "저두...파도보기 힘들던데용",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030014.1.25",
          "form": "파도 고를수가 없어요",
          "original_form": "파도 고를수가 없어요",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030014.1.26",
          "form": "그게 너무 어럽습니다",
          "original_form": "그게 너무 어럽습니다",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030014.1.27",
          "form": "서핑하시는 여자분들 완전 오우야던데",
          "original_form": "서핑하시는 여자분들 완전 오우야던데",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030014.1.28",
          "form": "오우야 이신가요?",
          "original_form": "오우야 이신가요?",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030014.1.29",
          "form": "아니요...",
          "original_form": "아니요...",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030014.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030014.1.31",
          "form": "그래서 그만둠",
          "original_form": "그래서 그만둠",
          "speaker_id": "1",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100030014.1.32",
          "form": "하와이 ~부럽다",
          "original_form": "하와이 ~부럽다",
          "speaker_id": "1",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100030014.1.33",
          "form": "ㅋㅋㅋ솔직하시네요",
          "original_form": "ㅋㅋㅋ솔직하시네요",
          "speaker_id": "2",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100030014.1.34",
          "form": "박사님 여유좀 되시면 하와이 가세요",
          "original_form": "박사님 여유좀 되시면 하와이 가세요",
          "speaker_id": "2",
          "time": "20210903 18:40"
        },
