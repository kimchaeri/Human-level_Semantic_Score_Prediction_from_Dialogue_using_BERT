          "id": "MDRW2100030014.1.35",
          "form": "백신도 놔준답니다",
          "original_form": "백신도 놔준답니다",
          "speaker_id": "2",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100030014.1.36",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100030014.1.37",
          "form": "여유가 ....없어요 언제가셧어요",
          "original_form": "여유가 ....없어요 언제가셧어요",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100030014.1.38",
          "form": "음 한 3년전?",
          "original_form": "음 한 3년전?",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
