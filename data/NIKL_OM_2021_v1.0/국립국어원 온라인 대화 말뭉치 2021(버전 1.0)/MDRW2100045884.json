      "utterance": [
        {
          "id": "MDRW2100045884.1.1",
          "form": "국화차 쟈스민차 먹어요",
          "original_form": "국화차 쟈스민차 먹어요",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045884.1.2",
          "form": "인터넷 구매해서 먹고 있어요",
          "original_form": "인터넷 구매해서 먹고 있어요",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045884.1.3",
          "form": "꽃차는 향이 좋죠 떡이랑 먹어야 왠지 어울려요ㅋ",
          "original_form": "꽃차는 향이 좋죠 떡이랑 먹어야 왠지 어울려요ㅋ",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045884.1.4",
          "form": "국화 향 넘 좋아요",
          "original_form": "국화 향 넘 좋아요",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045884.1.5",
          "form": "네 맞아요 마음이 평온해지는 향이요",
          "original_form": "네 맞아요 마음이 평온해지는 향이요",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045884.1.6",
          "form": "쟈스민은 스트레스 우울증 에 좋다고 해서 먹고 있죠",
          "original_form": "쟈스민은 스트레스 우울증 에 좋다고 해서 먹고 있죠",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045884.1.7",
          "form": "오 스트레스\r\n저도 먹어봐야겟네요",
          "original_form": "오 스트레스\r\n저도 먹어봐야겟네요",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045884.1.8",
          "form": "여성 생리통 에도 좋아요",
          "original_form": "여성 생리통 에도 좋아요",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045884.1.9",
          "form": "꼭 먹어야겟네요",
          "original_form": "꼭 먹어야겟네요",
          "speaker_id": "1",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045884.1.10",
          "form": "아마 혈액순환이 좋게 하니깐 ?\r\n 스트레스 생리통에도 효과 가 있나봐요",
          "original_form": "아마 혈액순환이 좋게 하니깐 ?\r\n 스트레스 생리통에도 효과 가 있나봐요",
          "speaker_id": "2",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100045884.1.11",
          "form": "아아 그런것도 알고 먹으면 좋을거같아요",
          "original_form": "아아 그런것도 알고 먹으면 좋을거같아요",
          "speaker_id": "1",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100045884.1.12",
          "form": "그냥 물도 많이 먹어야 한다고 하죠",
          "original_form": "그냥 물도 많이 먹어야 한다고 하죠",
          "speaker_id": "2",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100045884.1.13",
          "form": "네 그게 어려워요ㅋ",
          "original_form": "네 그게 어려워요ㅋ",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045884.1.14",
          "form": "저도 물먹는게 엄청 어려워요",
          "original_form": "저도 물먹는게 엄청 어려워요",
