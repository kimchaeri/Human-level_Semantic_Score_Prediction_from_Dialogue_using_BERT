      "utterance": [
        {
          "id": "MDRW2100024580.1.1",
          "form": "대학원생활도 학교생활이니까요..! (?)",
          "original_form": "대학원생활도 학교생활이니까요..! (?)",
          "speaker_id": "1",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100024580.1.2",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100024580.1.3",
          "form": "name1님 저 아까 폐수 버리러 가는 길에",
          "original_form": "&name1&님 저 아까 폐수 버리러 가는 길에",
          "speaker_id": "2",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100024580.1.4",
          "form": "옆 연구실을 잠깐 들렀는데",
          "original_form": "옆 연구실을 잠깐 들렀는데",
          "speaker_id": "2",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100024580.1.5",
          "form": "거기는... 오늘 교수님이랑 같이 폐수를 버리더라구요?!",
          "original_form": "거기는... 오늘 교수님이랑 같이 폐수를 버리더라구요?!",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100024580.1.6",
          "form": "앗 정말요??",
          "original_form": "앗 정말요??",
          "speaker_id": "1",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100024580.1.7",
          "form": "교수님이 왜 그러셨을까요...? 정말 처음 듣는 경우네요!!",
          "original_form": "교수님이 왜 그러셨을까요...? 정말 처음 듣는 경우네요!!",
          "speaker_id": "1",
          "time": "20210831 15:39"
        },
        {
          "id": "MDRW2100024580.1.8",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100024580.1.9",
          "form": "뭔가 쓸 데 없는 호의? 같지 않나요?",
          "original_form": "뭔가 쓸 데 없는 호의? 같지 않나요?",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100024580.1.10",
          "form": "그러니까 학생이 자퇴를 하지...",
          "original_form": "그러니까 학생이 자퇴를 하지...",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100024580.1.11",
          "form": "너무 과해요 그 교수님은...",
          "original_form": "너무 과해요 그 교수님은...",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100024580.1.12",
          "form": "그러게요ㅜㅜ",
          "original_form": "그러게요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024580.1.13",
          "form": "자퇴라니ㅜㅜㅠ",
          "original_form": "자퇴라니ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024580.1.14",
          "form": "얼마나 힘들었으면... 자퇴까지하나요ㅜㅜㅜ",
          "original_form": "얼마나 힘들었으면... 자퇴까지하나요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024580.1.15",
          "form": "그러게 말이에요...",
          "original_form": "그러게 말이에요...",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024580.1.16",
          "form": "그 학생의 고충은 본인만이 알거예요 ㅠㅠ",
          "original_form": "그 학생의 고충은 본인만이 알거예요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024580.1.17",
          "form": "정말정말 마음이 안좋아요...",
          "original_form": "정말정말 마음이 안좋아요...",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024580.1.18",
          "form": "미음고생 진짜 심했을 것 같던데...",
          "original_form": "미음고생 진짜 심했을 것 같던데...",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024580.1.19",
          "form": "화장실에서 만날 때",
          "original_form": "화장실에서 만날 때",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024580.1.20",
          "form": "혼잣말로",
          "original_form": "혼잣말로",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024580.1.21",
          "form": "'아... 자퇴하고 싶다~'",
          "original_form": "'아... 자퇴하고 싶다~'",
          "speaker_id": "2",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024580.1.22",
          "form": "라는 말 들은 적이 있거든요...",
          "original_form": "라는 말 들은 적이 있거든요...",
          "speaker_id": "2",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024580.1.23",
          "form": "아휴ㅠㅠ 그게 정말 진심이었군요ㅜㅜ",
          "original_form": "아휴ㅠㅠ 그게 정말 진심이었군요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 16:29"
        },
        {
          "id": "MDRW2100024580.1.24",
          "form": "물론 다들 진심리 섞여 있겠지만...",
          "original_form": "물론 다들 진심리 섞여 있겠지만...",
          "speaker_id": "1",
          "time": "20210831 16:29"
        },
        {
          "id": "MDRW2100024580.1.25",
          "form": "정말 진지하게 하는 말이었군요ㅜㅜ",
          "original_form": "정말 진지하게 하는 말이었군요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 16:29"
        },
        {
          "id": "MDRW2100024580.1.26",
          "form": "그러게요...",
          "original_form": "그러게요...",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024580.1.27",
          "form": "그리고 아까 언뜻 보니까",
          "original_form": "그리고 아까 언뜻 보니까",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024580.1.28",
          "form": "폐수 예약 이런 것도 name3씨가 다 했던 것 같더라구요",
          "original_form": "폐수 예약 이런 것도 &name3&씨가 다 했던 것 같더라구요",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024580.1.29",
          "form": "물론 서로 나눠서 했겠지만?!",
          "original_form": "물론 서로 나눠서 했겠지만?!",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024580.1.30",
          "form": "번호를 안물어본 게 아쉽네요ㅠ",
          "original_form": "번호를 안물어본 게 아쉽네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024580.1.31",
          "form": "자퇴하신 분 업무량이 많았나봐요ㅜㅜㅠ",
          "original_form": "자퇴하신 분 업무량이 많았나봐요ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100024580.1.32",
          "form": "상상하기도 싫은 힘듦이네요...",
          "original_form": "상상하기도 싫은 힘듦이네요...",
          "speaker_id": "1",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024580.1.33",
          "form": "스트레스가 진짜 장난이 아닐거같아요",
          "original_form": "스트레스가 진짜 장난이 아닐거같아요",
          "speaker_id": "1",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024580.1.34",
          "form": "대학원이라는게 정말 좁은 바닥이라ㅠㅠㅠ",
          "original_form": "대학원이라는게 정말 좁은 바닥이라ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100024580.1.35",
          "form": "맞아요 그리고",
          "original_form": "맞아요 그리고",
          "speaker_id": "2",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100024580.1.36",
          "form": "사람 때문에도 힘들었을수도...",
          "original_form": "사람 때문에도 힘들었을수도...",
          "speaker_id": "2",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100024580.1.37",
          "form": "예를 들면.. 교수님이라든지 교수님이라든지 ㅎㅎㅎ",
          "original_form": "예를 들면.. 교수님이라든지 교수님이라든지 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100024580.1.38",
          "form": "진짜 진학 전에 고려해야하는 요소 1순위죠ㅜㅜ",
          "original_form": "진짜 진학 전에 고려해야하는 요소 1순위죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100024580.1.39",
          "form": "사람이 힘들게하면 진짜 답도 없어요...",
          "original_form": "사람이 힘들게하면 진짜 답도 없어요...",
          "speaker_id": "1",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100024580.1.40",
          "form": "맞아요.....",
          "original_form": "맞아요.....",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024580.1.41",
          "form": "으하 으하 으하",
          "original_form": "으하 으하 으하",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024580.1.42",
          "form": "맞다 오늘 name4 교수님 OT 영상 올라왔는데",
          "original_form": "맞다 오늘 &name4& 교수님 OT 영상 올라왔는데",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024580.1.43",
          "form": "내일 보신다 하셨죠~!!!",
          "original_form": "내일 보신다 하셨죠~!!!",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024580.1.44",
          "form": "순간 깜빡했어요 ㅋㅋㅋㅋ",
          "original_form": "순간 깜빡했어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024580.1.45",
          "form": "네! 개강은 내일이니까요~",
          "original_form": "네! 개강은 내일이니까요~",
          "speaker_id": "1",
          "time": "20210831 18:35"
        },
        {
          "id": "MDRW2100024580.1.46",
          "form": "이럴때만 엄격해요 ㅋㅋㅋㅋ",
          "original_form": "이럴때만 엄격해요 ㅋㅋㅋㅋ",
