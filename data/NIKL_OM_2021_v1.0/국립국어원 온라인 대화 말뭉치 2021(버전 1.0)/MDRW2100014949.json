      "utterance": [
        {
          "id": "MDRW2100014949.1.1",
          "form": "저녁뭐드셨나요",
          "original_form": "저녁뭐드셨나요",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014949.1.2",
          "form": "저 닭갈비요!",
          "original_form": "저 닭갈비요!",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014949.1.3",
          "form": "난님은요?",
          "original_form": "난님은요?",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014949.1.4",
          "form": "오 친구랑 돈까스요 든든하게드셨네요",
          "original_form": "오 친구랑 돈까스요 든든하게드셨네요",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014949.1.5",
          "form": "돈가스도 맛있죠",
          "original_form": "돈가스도 맛있죠",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014949.1.6",
          "form": "말하니까 먹고싶네요",
          "original_form": "말하니까 먹고싶네요",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014949.1.7",
          "form": "오 내일은 돈가스로 드셔야겠네요",
          "original_form": "오 내일은 돈가스로 드셔야겠네요",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014949.1.8",
          "form": "ㅋㅋㅋㅋ근데 얼마전에 먹었어요",
          "original_form": "ㅋㅋㅋㅋ근데 얼마전에 먹었어요",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014949.1.9",
          "form": "냉면이랑 세트로",
          "original_form": "냉면이랑 세트로",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014949.1.10",
          "form": "ㅋㅋ오 굿",
          "original_form": "ㅋㅋ오 굿",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014949.1.11",
          "form": "저는 친구가 먹자는거 따라먹었어요",
          "original_form": "저는 친구가 먹자는거 따라먹었어요",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014949.1.12",
          "form": "돈가스도 좋고..저는 일식은 다 맛있는거같아요",
          "original_form": "돈가스도 좋고..저는 일식은 다 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014949.1.13",
          "form": "저두 좋아해요 좀 짠거 빼고",
          "original_form": "저두 좋아해요 좀 짠거 빼고",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014949.1.14",
          "form": "짠거좋아하세요?",
          "original_form": "짠거좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014949.1.15",
          "form": "아니요ㅜ 매운거는 좋아해요",
          "original_form": "아니요ㅜ 매운거는 좋아해요",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100014949.1.16",
          "form": "마라탕 좋아하시겠당",
          "original_form": "마라탕 좋아하시겠당",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100014949.1.17",
          "form": "아 맛은있는데 넘 자극적이에요ㅋㅋㅋ",
          "original_form": "아 맛은있는데 넘 자극적이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100014949.1.18",
          "form": "땅콩버터를 많이넣으면 덜맵지않을까요",
          "original_form": "땅콩버터를 많이넣으면 덜맵지않을까요",
          "speaker_id": "2",
          "time": "20210825 21:57"
        }
      ]
