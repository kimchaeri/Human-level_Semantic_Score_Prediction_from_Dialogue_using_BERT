      "utterance": [
        {
          "id": "MDRW2100033913.1.1",
          "form": "전 카레요",
          "original_form": "전 카레요",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100033913.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100033913.1.3",
          "form": "전 짜장이요ㅋㅋㅋㅋ",
          "original_form": "전 짜장이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100033913.1.4",
          "form": "둘다사실 그닥이지만",
          "original_form": "둘다사실 그닥이지만",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100033913.1.5",
          "form": "둘중 고르자면요",
          "original_form": "둘중 고르자면요",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100033913.1.6",
          "form": "ㅋㅋㅋ저도 그렇게 좋아하진 않아요",
          "original_form": "ㅋㅋㅋ저도 그렇게 좋아하진 않아요",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100033913.1.7",
          "form": "카레는 그나마 매운 맛 있어서 더 나아요",
          "original_form": "카레는 그나마 매운 맛 있어서 더 나아요",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100033913.1.8",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100033913.1.9",
          "form": "카레는 아비꼬가 그나마",
          "original_form": "카레는 아비꼬가 그나마",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100033913.1.10",
          "form": "제일나은듯요",
          "original_form": "제일나은듯요",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100033913.1.11",
          "form": "오 저 아비꼬 자주 가요",
          "original_form": "오 저 아비꼬 자주 가요",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100033913.1.12",
          "form": "아비꼬 요즘 맛있나요?",
          "original_form": "아비꼬 요즘 맛있나요?",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100033913.1.13",
          "form": "제가 가는 곳은 괜찮아요ㅋㅋ",
          "original_form": "제가 가는 곳은 괜찮아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100033913.1.14",
          "form": "학생때가보고",
          "original_form": "학생때가보고",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100033913.1.15",
          "form": "꽤된거같네요",
          "original_form": "꽤된거같네요",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100033913.1.16",
          "form": "name1님은 저녁 뭐 드실건가요?",
          "original_form": "&name1&님은 저녁 뭐 드실건가요?",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100033913.1.17",
          "form": "고민중이예요 초밥이랑 부찌중에요ㅋㅋㅋ",
          "original_form": "고민중이예요 초밥이랑 부찌중에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100033913.1.18",
          "form": "두 개가 완전 다르네요 저라면 따뜻한 거 먹을래요",
          "original_form": "두 개가 완전 다르네요 저라면 따뜻한 거 먹을래요",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100033913.1.19",
          "form": "ㅋㅋㅋ 초밥을 제가",
          "original_form": "ㅋㅋㅋ 초밥을 제가",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100033913.1.20",
          "form": "제일좋아하거든요 모든음식중에",
          "original_form": "제일좋아하거든요 모든음식중에",
          "speaker_id": "1",
