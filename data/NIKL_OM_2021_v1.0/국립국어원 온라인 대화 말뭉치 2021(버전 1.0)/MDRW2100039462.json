      "utterance": [
        {
          "id": "MDRW2100039462.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039462.1.2",
          "form": "아니요 ㄲㅋ미역국했어요ㅋ",
          "original_form": "아니요 ㄲㅋ미역국했어요ㅋ",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039462.1.3",
          "form": "드셨어요?",
          "original_form": "드셨어요?",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039462.1.4",
          "form": "저도 아직이요",
          "original_form": "저도 아직이요",
          "speaker_id": "1",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039462.1.5",
          "form": "뭐드세요?",
          "original_form": "뭐드세요?",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100039462.1.6",
          "form": "뭐 먹을지 고민이네요",
          "original_form": "뭐 먹을지 고민이네요",
          "speaker_id": "1",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100039462.1.7",
          "form": "매일고민이죠ㅠㅋ",
          "original_form": "매일고민이죠ㅠㅋ",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100039462.1.8",
          "form": "맞아요 요즘은 같은 반찬 먹으면 물려서 더 고민되요",
          "original_form": "맞아요 요즘은 같은 반찬 먹으면 물려서 더 고민되요",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039462.1.9",
          "form": "반찬 직접 해서 드시나요?",
          "original_form": "반찬 직접 해서 드시나요?",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039462.1.10",
          "form": "네 왠만하면 직접해요ㅋ",
          "original_form": "네 왠만하면 직접해요ㅋ",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039462.1.11",
          "form": "오늘은 미역국에 밑반찬이요ㅋ",
          "original_form": "오늘은 미역국에 밑반찬이요ㅋ",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039462.1.12",
          "form": "사드세요?",
          "original_form": "사드세요?",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039462.1.13",
          "form": "네 주로 사서 먹어요 ㅠ",
          "original_form": "네 주로 사서 먹어요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039462.1.14",
          "form": "아ㅜ사는비용도 장난아니죠ㅜ",
          "original_form": "아ㅜ사는비용도 장난아니죠ㅜ",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039462.1.15",
          "form": "재료가 남는 걸 관리를 못하겠더라구요",
          "original_form": "재료가 남는 걸 관리를 못하겠더라구요",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039462.1.16",
          "form": "그래서 사는 것이 낫더라고요",
          "original_form": "그래서 사는 것이 낫더라고요",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039462.1.17",
          "form": "네 맞아요ㅜ사는게 편하고 돈 덜들때있더라구요",
          "original_form": "네 맞아요ㅜ사는게 편하고 돈 덜들때있더라구요",
          "speaker_id": "2",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039462.1.18",
          "form": "그래도 직접 만드는 게 더 건강하고 좋은 것 같네요 내 맞춤이니까요 ㅎㅎ",
          "original_form": "그래도 직접 만드는 게 더 건강하고 좋은 것 같네요 내 맞춤이니까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039462.1.19",
          "form": "네 맞아요ㅋㅋ",
          "original_form": "네 맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:59"
        }
