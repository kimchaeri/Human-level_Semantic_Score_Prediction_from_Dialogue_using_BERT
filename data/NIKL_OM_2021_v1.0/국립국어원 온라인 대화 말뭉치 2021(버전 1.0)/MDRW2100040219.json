      "utterance": [
        {
          "id": "MDRW2100040219.1.1",
          "form": "name2님 봄 조아하세요?",
          "original_form": "&name2&님 봄 조아하세요?",
          "speaker_id": "1",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100040219.1.2",
          "form": "네 저는 봄 좋아해요",
          "original_form": "네 저는 봄 좋아해요",
          "speaker_id": "2",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100040219.1.3",
          "form": "요즘은 미세먼지 때문에 좀 덜하지만",
          "original_form": "요즘은 미세먼지 때문에 좀 덜하지만",
          "speaker_id": "2",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100040219.1.4",
          "form": "5월 조아하시나요",
          "original_form": "5월 조아하시나요",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040219.1.5",
          "form": "따뜻한 날씨와 봄꽃들이 좋아요ㅎㅎ",
          "original_form": "따뜻한 날씨와 봄꽃들이 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040219.1.6",
          "form": "미세먼지 문제죠",
          "original_form": "미세먼지 문제죠",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040219.1.7",
          "form": "네 저는 특히 5월을 좋아합니다ㅎㅎ",
          "original_form": "네 저는 특히 5월을 좋아합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040219.1.8",
          "form": "역시~",
          "original_form": "역시~",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040219.1.9",
          "form": "알아봤어요",
          "original_form": "알아봤어요",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040219.1.10",
          "form": "봄꽃 좋죠",
          "original_form": "봄꽃 좋죠",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040219.1.11",
          "form": "봄옷 조아하세요?",
          "original_form": "봄옷 조아하세요?",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040219.1.12",
          "form": "봄옷도 좋아해요",
          "original_form": "봄옷도 좋아해요",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100040219.1.13",
          "form": "뭔가 파스텔톤 옷들이 많아요",
          "original_form": "뭔가 파스텔톤 옷들이 많아요",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100040219.1.14",
          "form": "오 여성스러우실거같네요",
          "original_form": "오 여성스러우실거같네요",
          "speaker_id": "1",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100040219.1.15",
          "form": "파스텔트렌치 잇나요",
          "original_form": "파스텔트렌치 잇나요",
          "speaker_id": "1",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100040219.1.16",
          "form": "트렌치는 그냥 베이지랑 남색인데",
          "original_form": "트렌치는 그냥 베이지랑 남색인데",
          "speaker_id": "2",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100040219.1.17",
          "form": "파스텔톤 니트랑 자켓, 원피스.. 이런 봄옷들이 있어요ㅎㅎ",
          "original_form": "파스텔톤 니트랑 자켓, 원피스.. 이런 봄옷들이 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100040219.1.18",
          "form": "우와 구경하고싶어요",
          "original_form": "우와 구경하고싶어요",
          "speaker_id": "1",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100040219.1.19",
          "form": "근데 가을에 입기는 너무 봄스타일이라",
          "original_form": "근데 가을에 입기는 너무 봄스타일이라",
          "speaker_id": "2",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100040219.1.20",
          "form": "아하 가을옷 따로 사셔야겟내요",
          "original_form": "아하 가을옷 따로 사셔야겟내요",
          "speaker_id": "1",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100040219.1.21",
          "form": "새로운 한 주는 잘 시작하셨나요?",
          "original_form": "새로운 한 주는 잘 시작하셨나요?",
          "speaker_id": "2",
          "time": "20210907 08:18"
        },
        {
          "id": "MDRW2100040219.1.22",
          "form": "오늘은 아침부터 비가 오네요",
          "original_form": "오늘은 아침부터 비가 오네요",
