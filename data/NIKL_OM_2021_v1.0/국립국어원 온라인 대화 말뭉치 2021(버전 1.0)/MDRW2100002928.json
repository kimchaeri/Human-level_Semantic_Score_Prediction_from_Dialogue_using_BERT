      "utterance": [
        {
          "id": "MDRW2100002928.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002928.1.2",
          "form": "아니요 어릴때만 키워봤어요",
          "original_form": "아니요 어릴때만 키워봤어요",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002928.1.3",
          "form": "강아지요?",
          "original_form": "강아지요?",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002928.1.4",
          "form": "강아지랑 고양이요",
          "original_form": "강아지랑 고양이요",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002928.1.5",
          "form": "우와~~~어떤 종류요?",
          "original_form": "우와~~~어떤 종류요?",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002928.1.6",
          "form": "마당있는 집에 살았을때 키웠었는데",
          "original_form": "마당있는 집에 살았을때 키웠었는데",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002928.1.7",
          "form": "오~마당있는집!!",
          "original_form": "오~마당있는집!!",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002928.1.8",
          "form": "너무 옛날이라 기억이ㅋㅋ",
          "original_form": "너무 옛날이라 기억이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002928.1.9",
          "form": "초등학교때요",
          "original_form": "초등학교때요",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002928.1.10",
          "form": "키워본적 있으세요?",
          "original_form": "키워본적 있으세요?",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002928.1.11",
          "form": "아~~1마리씩요?",
          "original_form": "아~~1마리씩요?",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002928.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002928.1.13",
          "form": "애들 있는집에는 반려동물이 많더라고요",
          "original_form": "애들 있는집에는 반려동물이 많더라고요",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002928.1.14",
          "form": "애들이 사달라고 안하나요?",
          "original_form": "애들이 사달라고 안하나요?",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002928.1.15",
          "form": "애들이 없어서요ㅋㅋ",
          "original_form": "애들이 없어서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002928.1.16",
          "form": "그리고 키우면서도 무서워했어요",
          "original_form": "그리고 키우면서도 무서워했어요",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002928.1.17",
          "form": "헙 ㅋㅋ 죄송해요",
          "original_form": "헙 ㅋㅋ 죄송해요",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002928.1.18",
          "form": "집앞에 있으면 집에 못들어간ㅋㅋ",
          "original_form": "집앞에 있으면 집에 못들어간ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002928.1.19",
          "form": "아~~~ 큰 개였나봐요",
          "original_form": "아~~~ 큰 개였나봐요",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002928.1.20",
          "form": "아니요 작았는데 그것도 무섭더라고요",
          "original_form": "아니요 작았는데 그것도 무섭더라고요",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002928.1.21",
          "form": "고양이는 괜찮은데ㅋㅋ",
          "original_form": "고양이는 괜찮은데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002928.1.22",
          "form": "아~~강아지를 무서워하시는구나!!그럴수 있을것 같아요",
          "original_form": "아~~강아지를 무서워하시는구나!!그럴수 있을것 같아요",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002928.1.23",
          "form": "한번 무섭다 생각하니까 안고쳐지더라고요",
