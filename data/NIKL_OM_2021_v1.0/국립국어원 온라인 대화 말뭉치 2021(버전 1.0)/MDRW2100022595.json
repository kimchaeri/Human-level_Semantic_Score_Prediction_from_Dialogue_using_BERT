      "utterance": [
        {
          "id": "MDRW2100022595.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100022595.1.2",
          "form": "혹시 헌터물 좋아하시나요?",
          "original_form": "혹시 헌터물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100022595.1.3",
          "form": "헌터물은..ㅎㅎ 어떤장르인가요?ㅎㅎ",
          "original_form": "헌터물은..ㅎㅎ 어떤장르인가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022595.1.4",
          "form": "전 거의 제태크 책만읽어요ㅎㅎ",
          "original_form": "전 거의 제태크 책만읽어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022595.1.5",
          "form": "앗...닉네임이 name2시길래 한번 찍어봤어요..ㅎㅎ",
          "original_form": "앗...닉네임이 &name2&시길래 한번 찍어봤어요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100022595.1.6",
          "form": "아아ㅎㅎㅎ",
          "original_form": "아아ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100022595.1.7",
          "form": "name2가 이벤트name2에요ㅎㅎ",
          "original_form": "&name2&가 이벤트&name2&에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100022595.1.8",
          "form": "독서도 잘 읽어요ㅎ",
          "original_form": "독서도 잘 읽어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100022595.1.9",
          "form": "아ㅋㅋㅋㅋㅋ나름의 포부를 담으신 닉네임이셨군ㅇ요",
          "original_form": "아ㅋㅋㅋㅋㅋ나름의 포부를 담으신 닉네임이셨군ㅇ요",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100022595.1.10",
          "form": "독서를 왜하는지? 한번 대화해볼까요?ㅎ",
          "original_form": "독서를 왜하는지? 한번 대화해볼까요?ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100022595.1.11",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100022595.1.12",
          "form": "ㄱ굉장히 생각지 못했던 질문이네요",
          "original_form": "ㄱ굉장히 생각지 못했던 질문이네요",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100022595.1.13",
          "form": "저는 일단 학생이라 전공책 위주로 읽는게 가장 많구요",
          "original_form": "저는 일단 학생이라 전공책 위주로 읽는게 가장 많구요",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100022595.1.14",
          "form": "그 외에도 관련 분야 책들만 읽게 되는거같아요..!",
          "original_form": "그 외에도 관련 분야 책들만 읽게 되는거같아요..!",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100022595.1.15",
          "form": "맞아요ㅎ 전공책위주로 읽는게 가장많죠ㅎ",
          "original_form": "맞아요ㅎ 전공책위주로 읽는게 가장많죠ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100022595.1.16",
          "form": "독서는 필요에 의해서 하는것같애요ㅎ",
          "original_form": "독서는 필요에 의해서 하는것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100022595.1.17",
          "form": "우리가 공부를 하는이유랄까?",
          "original_form": "우리가 공부를 하는이유랄까?",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100022595.1.18",
          "form": "뭘알고싶으면 공부를해야하는것처럼ㅎ",
          "original_form": "뭘알고싶으면 공부를해야하는것처럼ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100022595.1.19",
          "form": "맞아ㅏ요 맞아요",
          "original_form": "맞아ㅏ요 맞아요",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100022595.1.20",
          "form": "F를 맞지 않으려면 책을 봐야하는 것 처럼 말이ㅣ죠...ㅎ",
          "original_form": "F를 맞지 않으려면 책을 봐야하는 것 처럼 말이ㅣ죠...ㅎ",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100022595.1.21",
          "form": "맞아요 맞아요ㅎㅎ",
          "original_form": "맞아요 맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100022595.1.22",
          "form": "독서는 그리고 뽑내기용?ㅎ",
          "original_form": "독서는 그리고 뽑내기용?ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100022595.1.23",
          "form": "남자가 여자에게 잘보일때?ㅎㅎ",
          "original_form": "남자가 여자에게 잘보일때?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100022595.1.24",
          "form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100022595.1.25",
          "form": "그런용으로도 쓰일ㄹ수 있군요",
          "original_form": "그런용으로도 쓰일ㄹ수 있군요",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100022595.1.26",
          "form": "지대넓얕 같은 책 보시나요 그럴때는",
          "original_form": "지대넓얕 같은 책 보시나요 그럴때는",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100022595.1.27",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100022595.1.28",
          "form": "당연히 그렇게 보는거죠ㅎㅎ",
          "original_form": "당연히 그렇게 보는거죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100022595.1.29",
          "form": "하긴ㄴ 말 잘 통하고 지적인 사람은 어디서나 매력있다는 소리를 듣기 마련이죠",
          "original_form": "하긴ㄴ 말 잘 통하고 지적인 사람은 어디서나 매력있다는 소리를 듣기 마련이죠",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100022595.1.30",
          "form": "요즘 서점 보니까ㅏ",
          "original_form": "요즘 서점 보니까ㅏ",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100022595.1.31",
          "form": "꿈백화점? 이라는 소설책이 핫한거같던데",
          "original_form": "꿈백화점? 이라는 소설책이 핫한거같던데",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100022595.1.32",
          "form": "근데 지적인 남자를 여자들도 좋아해요?",
          "original_form": "근데 지적인 남자를 여자들도 좋아해요?",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100022595.1.33",
          "form": "혹시 읽어보셨어요??",
          "original_form": "혹시 읽어보셨어요??",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100022595.1.34",
          "form": "말잘하는사람이 좋은거아니에요 ?ㅎ",
          "original_form": "말잘하는사람이 좋은거아니에요 ?ㅎ",
