      "utterance": [
        {
          "id": "MDRW2100008310.1.1",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008310.1.2",
          "form": "시간 날 때 종종 보는 편이에요",
          "original_form": "시간 날 때 종종 보는 편이에요",
          "speaker_id": "2",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008310.1.3",
          "form": "최근에 읽은 책이 있나요?",
          "original_form": "최근에 읽은 책이 있나요?",
          "speaker_id": "2",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008310.1.4",
          "form": "아침이 온다 라는 책",
          "original_form": "아침이 온다 라는 책",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008310.1.5",
          "form": "오늘 다읽었네요",
          "original_form": "오늘 다읽었네요",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008310.1.6",
          "form": "아침이 온다는 어떤 내용인가요?",
          "original_form": "아침이 온다는 어떤 내용인가요?",
          "speaker_id": "2",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008310.1.7",
          "form": "츠지무라 미즈키 작가님거예요 혹시 아시나요?",
          "original_form": "츠지무라 미즈키 작가님거예요 혹시 아시나요?",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008310.1.8",
          "form": "음.. 가족에 관한 이야기였어요",
          "original_form": "음.. 가족에 관한 이야기였어요",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008310.1.9",
          "form": "가족이라..",
          "original_form": "가족이라..",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008310.1.10",
          "form": "시간이 날 때 한번 읽어봐야겠어요",
          "original_form": "시간이 날 때 한번 읽어봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008310.1.11",
          "form": "근데 후반부로갈수록",
          "original_form": "근데 후반부로갈수록",
          "speaker_id": "1",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008310.1.12",
          "form": "어땠나요",
          "original_form": "어땠나요",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008310.1.13",
          "form": "일본 여성 인권 문제가 자꾸 머릿속을 맴돌더라고요 ㅠㅠ",
          "original_form": "일본 여성 인권 문제가 자꾸 머릿속을 맴돌더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008310.1.14",
          "form": "음 기억에 남는 장면 있었나요?",
          "original_form": "음 기억에 남는 장면 있었나요?",
          "speaker_id": "2",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008310.1.15",
          "form": "스포일러가 될 수도 있어서 좀 삼가는데 암울해지는 구간이 있었어요",
          "original_form": "스포일러가 될 수도 있어서 좀 삼가는데 암울해지는 구간이 있었어요",
          "speaker_id": "1",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008310.1.16",
          "form": "좋았던건",
          "original_form": "좋았던건",
          "speaker_id": "1",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008310.1.17",
          "form": "주인공 가정의 아이가 한명있는데",
          "original_form": "주인공 가정의 아이가 한명있는데",
          "speaker_id": "1",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008310.1.18",
          "form": "아이가 있었군요",
          "original_form": "아이가 있었군요",
          "speaker_id": "2",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008310.1.19",
          "form": "그 아이의 존재사 너무 따뜻해서 좋았어요",
          "original_form": "그 아이의 존재사 너무 따뜻해서 좋았어요",
          "speaker_id": "1",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008310.1.20",
          "form": "딱 아침이 온다. 라는 제목에 걸맞는 느낌",
          "original_form": "딱 아침이 온다. 라는 제목에 걸맞는 느낌",
          "speaker_id": "1",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100008310.1.21",
          "form": "듣기만 해도 궁금해지네요 시간 날 때 한 번 봐봐야겠어요",
          "original_form": "듣기만 해도 궁금해지네요 시간 날 때 한 번 봐봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100008310.1.22",
          "form": "저는 책이 좋았던건 여러번 읽는 편인데",
          "original_form": "저는 책이 좋았던건 여러번 읽는 편인데",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008310.1.23",
          "form": "최근에 모모 다시 읽었어요",
          "original_form": "최근에 모모 다시 읽었어요",
          "speaker_id": "2",
          "time": "20210822 15:03"
        }
      ]
