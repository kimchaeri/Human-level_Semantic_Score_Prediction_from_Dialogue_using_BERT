      "utterance": [
        {
          "id": "MDRW2100008559.1.1",
          "form": "탄산음료중 어떤걸 좋아하시나요?",
          "original_form": "탄산음료중 어떤걸 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008559.1.2",
          "form": "저는 환타를좋아합니다",
          "original_form": "저는 환타를좋아합니다",
          "speaker_id": "2",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008559.1.3",
          "form": "환타 무슨맛이요?",
          "original_form": "환타 무슨맛이요?",
          "speaker_id": "1",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008559.1.4",
          "form": "포도맛이요",
          "original_form": "포도맛이요",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008559.1.5",
          "form": "무슨맛좋아하세요",
          "original_form": "무슨맛좋아하세요",
          "speaker_id": "2",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008559.1.6",
          "form": "저는 파인애플이요",
          "original_form": "저는 파인애플이요",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008559.1.7",
          "form": "고깃집가면 파인애플환타로",
          "original_form": "고깃집가면 파인애플환타로",
          "speaker_id": "2",
          "time": "20210822 17:00"
        },
        {
          "id": "MDRW2100008559.1.8",
          "form": "싹내려야해요ㅋ",
          "original_form": "싹내려야해요ㅋ",
          "speaker_id": "2",
          "time": "20210822 17:00"
        },
        {
          "id": "MDRW2100008559.1.9",
          "form": "맞아요 환타만한게 없더라고요",
          "original_form": "맞아요 환타만한게 없더라고요",
          "speaker_id": "1",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008559.1.10",
          "form": "환타는 다 맛있는거같아요",
          "original_form": "환타는 다 맛있는거같아요",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008559.1.11",
          "form": "오렌지도",
          "original_form": "오렌지도",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008559.1.12",
          "form": "환타 오렌지는 안먹어봤어요",
          "original_form": "환타 오렌지는 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008559.1.13",
          "form": "다음에드셔보세요",
          "original_form": "다음에드셔보세요",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008559.1.14",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008559.1.15",
          "form": "환타먹고싶네요갑자기",
          "original_form": "환타먹고싶네요갑자기",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008559.1.16",
          "form": "저도 맛별로 환타구매하려고요ㅎㅎ",
          "original_form": "저도 맛별로 환타구매하려고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008559.1.17",
          "form": "해외에는 환타메론맛도 있대요",
          "original_form": "해외에는 환타메론맛도 있대요",
          "speaker_id": "1",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008559.1.18",
          "form": "우와신기하네요 환타메론이라니",
          "original_form": "우와신기하네요 환타메론이라니",
          "speaker_id": "2",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100008559.1.19",
          "form": "우리나라엔왜안들어올까요",
          "original_form": "우리나라엔왜안들어올까요",
          "speaker_id": "2",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100008559.1.20",
          "form": "메론소다도잘없구",
          "original_form": "메론소다도잘없구",
          "speaker_id": "2",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100008559.1.21",
          "form": "메론맛드셔보셨나요",
          "original_form": "메론맛드셔보셨나요",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100008559.1.22",
          "form": "환타메론맛 엄청 맛있어요",
          "original_form": "환타메론맛 엄청 맛있어요",
