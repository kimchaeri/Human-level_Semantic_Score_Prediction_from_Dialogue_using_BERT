      "utterance": [
        {
          "id": "MDRW2100026623.1.1",
          "form": "최근에 뭐 사셨어요?",
          "original_form": "최근에 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026623.1.2",
          "form": "저요 바람막이요 ㅋㅋ",
          "original_form": "저요 바람막이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026623.1.3",
          "form": "아 어디건가요?ㅋㅋ",
          "original_form": "아 어디건가요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026623.1.4",
          "form": "나이키!!ㅋㅋㅋ",
          "original_form": "나이키!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026623.1.5",
          "form": "무난한거루 하나삿음다",
          "original_form": "무난한거루 하나삿음다",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026623.1.6",
          "form": "name1음요",
          "original_form": "&name1&음요",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026623.1.7",
          "form": "전 오늘 비비고 만두 샀어요ㅋㅋㅈ",
          "original_form": "전 오늘 비비고 만두 샀어요ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026623.1.8",
          "form": "할인하더라구요",
          "original_form": "할인하더라구요",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026623.1.9",
          "form": "우옹비비고!",
          "original_form": "우옹비비고!",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026623.1.10",
          "form": "무슨맛이요",
          "original_form": "무슨맛이요",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026623.1.11",
          "form": "왕교자랑 한섬만두요!",
          "original_form": "왕교자랑 한섬만두요!",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026623.1.12",
          "form": "만두 좋아하세요?",
          "original_form": "만두 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026623.1.13",
          "form": "한섬만두는 첨드러바요",
          "original_form": "한섬만두는 첨드러바요",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026623.1.14",
          "form": "한섬도 맛있더라구요ㅎㅎ저도 피 얇은거 좋아해요",
          "original_form": "한섬도 맛있더라구요ㅎㅎ저도 피 얇은거 좋아해요",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026623.1.15",
          "form": "지금 지마켓 빅스마일데이라 할인행사 많더라구요",
          "original_form": "지금 지마켓 빅스마일데이라 할인행사 많더라구요",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026623.1.16",
          "form": "오호 또 머 사셧나요",
          "original_form": "오호 또 머 사셧나요",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026623.1.17",
          "form": "비타민 앰플 샀어요ㅎㅎ여름에 많이 타서ㅜㅜ",
          "original_form": "비타민 앰플 샀어요ㅎㅎ여름에 많이 타서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026623.1.18",
          "form": "그게 도움이 되나요?",
          "original_form": "그게 도움이 되나요?",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026623.1.19",
          "form": "저도 첨 사봤어요 효과가 있으면 좋겠어요",
          "original_form": "저도 첨 사봤어요 효과가 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026623.1.20",
          "form": "오홍.. 저도 찾아바야겟네요 ㅋㅋ",
          "original_form": "오홍.. 저도 찾아바야겟네요 ㅋㅋ",
