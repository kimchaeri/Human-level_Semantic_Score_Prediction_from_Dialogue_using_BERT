      "utterance": [
        {
          "id": "MDRW2100040539.1.1",
          "form": "저 지금 대화에서 수학밖에 생각이 안나요",
          "original_form": "저 지금 대화에서 수학밖에 생각이 안나요",
          "speaker_id": "2",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040539.1.2",
          "form": "저는 아르바이트 경험이 다양하지 않거든요",
          "original_form": "저는 아르바이트 경험이 다양하지 않거든요",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040539.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040539.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040539.1.5",
          "form": "ㅋㅋㅋㅋㅋ 무슨 알바 하셨어요?*",
          "original_form": "ㅋㅋㅋㅋㅋ 무슨 알바 하셨어요?*",
          "speaker_id": "2",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040539.1.6",
          "form": "카페 알바 아주 잠깐 해보고 다 과외했어요",
          "original_form": "카페 알바 아주 잠깐 해보고 다 과외했어요",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040539.1.7",
          "form": "아무래도 수학과다보니까..",
          "original_form": "아무래도 수학과다보니까..",
          "speaker_id": "1",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100040539.1.8",
          "form": "저는 도서관 야구장 백화점",
          "original_form": "저는 도서관 야구장 백화점",
          "speaker_id": "2",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100040539.1.9",
          "form": "전공과는 전혀 상관이 없었네여",
          "original_form": "전공과는 전혀 상관이 없었네여",
          "speaker_id": "2",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100040539.1.10",
          "form": "ㅋㅋㅋㅋ 과외가 진짜 좋져",
          "original_form": "ㅋㅋㅋㅋ 과외가 진짜 좋져",
          "speaker_id": "2",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100040539.1.11",
          "form": "부러워요 그런 다양한 경험",
          "original_form": "부러워요 그런 다양한 경험",
          "speaker_id": "1",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040539.1.12",
          "form": "고3때 꿈은 주유소 알바였는데",
          "original_form": "고3때 꿈은 주유소 알바였는데",
          "speaker_id": "1",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040539.1.13",
          "form": "에이 이건 경험도 아니고 그냥 알바였어요",
          "original_form": "에이 이건 경험도 아니고 그냥 알바였어요",
          "speaker_id": "2",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040539.1.14",
          "form": "그때만 해도 대학생들이 주유알바 많이 했거든요",
          "original_form": "그때만 해도 대학생들이 주유알바 많이 했거든요",
          "speaker_id": "1",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040539.1.15",
          "form": "오 주유소 알바여??",
          "original_form": "오 주유소 알바여??",
          "speaker_id": "2",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040539.1.16",
          "form": "근데 알바 구하는것도 쉽지않고",
          "original_form": "근데 알바 구하는것도 쉽지않고",
          "speaker_id": "1",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040539.1.17",
          "form": "다 알음알음 많이 들어가서",
          "original_form": "다 알음알음 많이 들어가서",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040539.1.18",
          "form": "생각보다 알바를 구하기 힘들더라고요",
          "original_form": "생각보다 알바를 구하기 힘들더라고요",
          "speaker_id": "1",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040539.1.19",
          "form": "아는 사람 많이 쓰더라고요",
          "original_form": "아는 사람 많이 쓰더라고요",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040539.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040539.1.21",
          "form": "믿음직한 알바 친구의 친구의 친구",
          "original_form": "믿음직한 알바 친구의 친구의 친구",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040539.1.22",
          "form": "이런식?ㅋㅋㅋ",
          "original_form": "이런식?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040539.1.23",
          "form": "그쵸 ㅎㅎ 야구장 알바는 뭐하는거에요?",
          "original_form": "그쵸 ㅎㅎ 야구장 알바는 뭐하는거에요?",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040539.1.24",
          "form": "야구장 주차장에서 야구장 매점까지 갈수있는 알바였어욬ㅋㅋㅋ",
          "original_form": "야구장 주차장에서 야구장 매점까지 갈수있는 알바였어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100040539.1.25",
          "form": "그치만 저는 야구장 주차장에서 끝났지요ㅋㅋㅋㅋ",
          "original_form": "그치만 저는 야구장 주차장에서 끝났지요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100040539.1.26",
          "form": "와 주차장에서 알바하셨으면 수신호도 뿅뿅 하시고 그러셨나요",
          "original_form": "와 주차장에서 알바하셨으면 수신호도 뿅뿅 하시고 그러셨나요",
