      "utterance": [
        {
          "id": "MDRW2100032918.1.1",
          "form": "일어났나",
          "original_form": "일어났나",
          "speaker_id": "1",
          "time": "20210906 07:03"
        },
        {
          "id": "MDRW2100032918.1.2",
          "form": "아침 먹을래",
          "original_form": "아침 먹을래",
          "speaker_id": "1",
          "time": "20210906 07:03"
        },
        {
          "id": "MDRW2100032918.1.3",
          "form": "아니 그냥 갈래 더 자고싶구만",
          "original_form": "아니 그냥 갈래 더 자고싶구만",
          "speaker_id": "2",
          "time": "20210906 07:04"
        },
        {
          "id": "MDRW2100032918.1.4",
          "form": "가서 배고프면 어떡할래",
          "original_form": "가서 배고프면 어떡할래",
          "speaker_id": "1",
          "time": "20210906 07:07"
        },
        {
          "id": "MDRW2100032918.1.5",
          "form": "그럼 편의점가서 뭐 사먹으면 되지",
          "original_form": "그럼 편의점가서 뭐 사먹으면 되지",
          "speaker_id": "2",
          "time": "20210906 07:08"
        },
        {
          "id": "MDRW2100032918.1.6",
          "form": "오늘 오후에 비온다던대",
          "original_form": "오늘 오후에 비온다던대",
          "speaker_id": "1",
          "time": "20210906 07:09"
        },
        {
          "id": "MDRW2100032918.1.7",
          "form": "우산 챙겨가",
          "original_form": "우산 챙겨가",
          "speaker_id": "1",
          "time": "20210906 07:09"
        },
        {
          "id": "MDRW2100032918.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210906 07:14"
        },
        {
          "id": "MDRW2100032918.1.9",
          "form": "어제 준비하다 좀 늦게잤더니 너무 피곤하구만",
          "original_form": "어제 준비하다 좀 늦게잤더니 너무 피곤하구만",
          "speaker_id": "2",
          "time": "20210906 07:14"
        },
        {
          "id": "MDRW2100032918.1.10",
          "form": "버스에서 좀 자야겠네",
          "original_form": "버스에서 좀 자야겠네",
          "speaker_id": "1",
          "time": "20210906 07:15"
        },
        {
          "id": "MDRW2100032918.1.11",
          "form": "버스가 한번에 가서 너무 다행이야",
          "original_form": "버스가 한번에 가서 너무 다행이야",
          "speaker_id": "2",
          "time": "20210906 07:15"
        },
        {
          "id": "MDRW2100032918.1.12",
          "form": "아침먹기 귀찮으면 두유를 좀 살까",
          "original_form": "아침먹기 귀찮으면 두유를 좀 살까",
          "speaker_id": "1",
          "time": "20210906 07:18"
        },
        {
          "id": "MDRW2100032918.1.13",
          "form": "내가 사놓을게 난 탈모니까 검은콩으로 사야지",
          "original_form": "내가 사놓을게 난 탈모니까 검은콩으로 사야지",
          "speaker_id": "2",
          "time": "20210906 07:19"
        },
        {
          "id": "MDRW2100032918.1.14",
          "form": "아침에 가면서 한팩씩 먹으면 딱이네",
          "original_form": "아침에 가면서 한팩씩 먹으면 딱이네",
          "speaker_id": "1",
          "time": "20210906 07:19"
        },
        {
          "id": "MDRW2100032918.1.15",
          "form": "오늘은 첫날이라 좀 일찍가서 수강증을 받아야해",
          "original_form": "오늘은 첫날이라 좀 일찍가서 수강증을 받아야해",
          "speaker_id": "2",
          "time": "20210906 07:20"
        },
        {
          "id": "MDRW2100032918.1.16",
          "form": "그럼 지금 나가야겠네",
          "original_form": "그럼 지금 나가야겠네",
          "speaker_id": "1",
          "time": "20210906 07:20"
        }
      ]
