      "utterance": [
        {
          "id": "MDRW2100032487.1.1",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 19:24"
        },
        {
          "id": "MDRW2100032487.1.2",
          "form": "진짜 해먹기도 편하고 너무 좋아요",
          "original_form": "진짜 해먹기도 편하고 너무 좋아요",
          "speaker_id": "2",
          "time": "20210905 19:24"
        },
        {
          "id": "MDRW2100032487.1.3",
          "form": "안먹는 야채 먹기도 너무 편하구요",
          "original_form": "안먹는 야채 먹기도 너무 편하구요",
          "speaker_id": "2",
          "time": "20210905 19:24"
        },
        {
          "id": "MDRW2100032487.1.4",
          "form": "네 칠리소스만 찍으면 무조건 맛잇어요",
          "original_form": "네 칠리소스만 찍으면 무조건 맛잇어요",
          "speaker_id": "1",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100032487.1.5",
          "form": "맞아요 땅콩소스도 맛있어요",
          "original_form": "맞아요 땅콩소스도 맛있어요",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100032487.1.6",
          "form": "저는 두개를 주로 먹는것같아요 소스!!",
          "original_form": "저는 두개를 주로 먹는것같아요 소스!!",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100032487.1.7",
          "form": "아 땅콩도 맛잇져\r\n간만에 먹고싶네요",
          "original_form": "아 땅콩도 맛잇져\r\n간만에 먹고싶네요",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032487.1.8",
          "form": "저도 먹고싶네요 ㅎㅎ",
          "original_form": "저도 먹고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 11:17"
        },
        {
          "id": "MDRW2100032487.1.9",
          "form": "저는 유부초밥이랑 미숫가루 먹었어요",
          "original_form": "저는 유부초밥이랑 미숫가루 먹었어요",
          "speaker_id": "2",
          "time": "20210906 11:18"
        },
        {
          "id": "MDRW2100032487.1.10",
          "form": "미숫가루 좋아하시나요?",
          "original_form": "미숫가루 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 11:18"
        },
        {
          "id": "MDRW2100032487.1.11",
          "form": "맛있죠 우유에타먹으면",
          "original_form": "맛있죠 우유에타먹으면",
          "speaker_id": "1",
          "time": "20210906 11:18"
        },
        {
          "id": "MDRW2100032487.1.12",
          "form": "맞아요 우유에 타먹었어요 ㅎㅎ",
          "original_form": "맞아요 우유에 타먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100032487.1.13",
          "form": "오랜만이라 더 맛있더라구요",
          "original_form": "오랜만이라 더 맛있더라구요",
          "speaker_id": "2",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100032487.1.14",
          "form": "고기좋아하세요?",
          "original_form": "고기좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100032487.1.15",
          "form": "고기는 사랑이죠",
          "original_form": "고기는 사랑이죠",
          "speaker_id": "1",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100032487.1.16",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100032487.1.17",
          "form": "양고기도 좋아하시나요??",
          "original_form": "양고기도 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100032487.1.18",
          "form": "앗 양고긴 못먹어봣어요",
          "original_form": "앗 양고긴 못먹어봣어요",
          "speaker_id": "1",
          "time": "20210906 11:26"
        },
        {
          "id": "MDRW2100032487.1.19",
          "form": "전 마라탕이나 구이도 양고기 자주먹는데 장말 맛있어요",
          "original_form": "전 마라탕이나 구이도 양고기 자주먹는데 장말 맛있어요",
          "speaker_id": "2",
          "time": "20210906 11:27"
        },
        {
          "id": "MDRW2100032487.1.20",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210906 11:27"
        },
        {
          "id": "MDRW2100032487.1.21",
          "form": "맛잇으시다니 먹어봐야겠네요",
          "original_form": "맛잇으시다니 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210906 11:28"
        },
        {
          "id": "MDRW2100032487.1.22",
          "form": "네 꼭 드셔보세요 너무 맛있어요",
          "original_form": "네 꼭 드셔보세요 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210906 12:38"
        },
        {
          "id": "MDRW2100032487.1.23",
          "form": "대신 잘하는곳에 가야 남새가 안나더라구요 ㅜ",
          "original_form": "대신 잘하는곳에 가야 남새가 안나더라구요 ㅜ",
