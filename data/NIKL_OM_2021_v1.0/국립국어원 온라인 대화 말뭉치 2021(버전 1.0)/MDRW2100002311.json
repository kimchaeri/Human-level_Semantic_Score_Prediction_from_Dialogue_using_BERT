      "utterance": [
        {
          "id": "MDRW2100002311.1.1",
          "form": "요새 날씨 많이 풀린거 같아요",
          "original_form": "요새 날씨 많이 풀린거 같아요",
          "speaker_id": "1",
          "time": "20210820 09:07"
        },
        {
          "id": "MDRW2100002311.1.2",
          "form": "하늘도 높아지고",
          "original_form": "하늘도 높아지고",
          "speaker_id": "1",
          "time": "20210820 09:08"
        },
        {
          "id": "MDRW2100002311.1.3",
          "form": "그러니까 바람이 시원해",
          "original_form": "그러니까 바람이 시원해",
          "speaker_id": "2",
          "time": "20210820 09:08"
        },
        {
          "id": "MDRW2100002311.1.4",
          "form": "아침이랑 저녁은 진짜 시원해여",
          "original_form": "아침이랑 저녁은 진짜 시원해여",
          "speaker_id": "1",
          "time": "20210820 09:10"
        },
        {
          "id": "MDRW2100002311.1.5",
          "form": "이제 가을되는건가여",
          "original_form": "이제 가을되는건가여",
          "speaker_id": "1",
          "time": "20210820 09:10"
        },
        {
          "id": "MDRW2100002311.1.6",
          "form": "원래 이렇게 빨리 가을이 왔나싶어",
          "original_form": "원래 이렇게 빨리 가을이 왔나싶어",
          "speaker_id": "2",
          "time": "20210820 09:11"
        },
        {
          "id": "MDRW2100002311.1.7",
          "form": "이제 하루에 에어컨 30분도 안 키는듯?",
          "original_form": "이제 하루에 에어컨 30분도 안 키는듯?",
          "speaker_id": "2",
          "time": "20210820 09:11"
        },
        {
          "id": "MDRW2100002311.1.8",
          "form": "밤에 진짜 추워",
          "original_form": "밤에 진짜 추워",
          "speaker_id": "2",
          "time": "20210820 09:11"
        },
        {
          "id": "MDRW2100002311.1.9",
          "form": "한낮 빼고는 안틀어도 될듯ㅇㆍ니",
          "original_form": "한낮 빼고는 안틀어도 될듯ㅇㆍ니",
          "speaker_id": "1",
          "time": "20210820 09:11"
        },
        {
          "id": "MDRW2100002311.1.10",
          "form": "그래도 9월까지는 덥겠죠",
          "original_form": "그래도 9월까지는 덥겠죠",
          "speaker_id": "1",
          "time": "20210820 09:12"
        },
        {
          "id": "MDRW2100002311.1.11",
          "form": "가을되면 다시 미세먼지 시작되려나",
          "original_form": "가을되면 다시 미세먼지 시작되려나",
          "speaker_id": "1",
          "time": "20210820 09:12"
        },
        {
          "id": "MDRW2100002311.1.12",
          "form": "아 맞다 미세먼지를  잊었느",
          "original_form": "아 맞다 미세먼지를  잊었느",
          "speaker_id": "2",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100002311.1.13",
          "form": "봄말고 가을도 미세먼지가 많았던가??",
          "original_form": "봄말고 가을도 미세먼지가 많았던가??",
          "speaker_id": "2",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100002311.1.14",
          "form": "많아던거 같아요",
          "original_form": "많아던거 같아요",
          "speaker_id": "1",
          "time": "20210820 09:14"
        },
        {
          "id": "MDRW2100002311.1.15",
          "form": "요새 날씨는 짐작을 못하겠어요",
          "original_form": "요새 날씨는 짐작을 못하겠어요",
          "speaker_id": "1",
          "time": "20210820 09:14"
        },
        {
          "id": "MDRW2100002311.1.16",
          "form": "맑았다가 다시 비가 쏟아지고",
          "original_form": "맑았다가 다시 비가 쏟아지고",
          "speaker_id": "1",
          "time": "20210820 09:14"
        },
        {
          "id": "MDRW2100002311.1.17",
          "form": "그러니가",
          "original_form": "그러니가",
          "speaker_id": "2",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002311.1.18",
          "form": "주말에 2차 장마온대",
          "original_form": "주말에 2차 장마온대",
          "speaker_id": "2",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002311.1.19",
          "form": "꺄악 시러",
          "original_form": "꺄악 시러",
          "speaker_id": "1",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002311.1.20",
          "form": "비오는거 넘 시러요",
          "original_form": "비오는거 넘 시러요",
          "speaker_id": "1",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002311.1.21",
          "form": "저번에 흰 운동화 5천원주고 맡긴거",
          "original_form": "저번에 흰 운동화 5천원주고 맡긴거",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002311.1.22",
          "form": "세탁 후 첨 신었는데",
          "original_form": "세탁 후 첨 신었는데",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002311.1.23",
          "form": "퇴근 때 폭우와서ㅋㅋㅋ",
          "original_form": "퇴근 때 폭우와서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002311.1.24",
          "form": "더러워짐...또르르",
          "original_form": "더러워짐...또르르",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002311.1.25",
          "form": "헐 짱났겠어요",
          "original_form": "헐 짱났겠어요",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002311.1.26",
          "form": "요새는 우산을 항상 들고 다녀야되는건지",
          "original_form": "요새는 우산을 항상 들고 다녀야되는건지",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002311.1.27",
          "form": "양우산 없어???",
          "original_form": "양우산 없어???",
          "speaker_id": "2",
          "time": "20210820 09:18"
        },
        {
          "id": "MDRW2100002311.1.28",
          "form": "작은우산 있어여",
          "original_form": "작은우산 있어여",
          "speaker_id": "1",
          "time": "20210820 09:18"
        },
        {
          "id": "MDRW2100002311.1.29",
          "form": "근데 그냥 짐 늘리는게 시러요",
          "original_form": "근데 그냥 짐 늘리는게 시러요",
          "speaker_id": "1",
          "time": "20210820 09:18"
        }
      ]
