      "utterance": [
        {
          "id": "MDRW2100045094.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045094.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045094.1.3",
          "form": "일하시나요?",
          "original_form": "일하시나요?",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045094.1.4",
          "form": "아니요 이직 알아보고있어요 님은요?",
          "original_form": "아니요 이직 알아보고있어요 님은요?",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045094.1.5",
          "form": "근무중이요!",
          "original_form": "근무중이요!",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045094.1.6",
          "form": "경력이 얼마나 되셨어요?",
          "original_form": "경력이 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045094.1.7",
          "form": "이직은 보통 3년 이상 잘 쳐주는 걸 알아서 3년이상요",
          "original_form": "이직은 보통 3년 이상 잘 쳐주는 걸 알아서 3년이상요",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045094.1.8",
          "form": "어떤 일 하세요?",
          "original_form": "어떤 일 하세요?",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045094.1.9",
          "form": "사무직이요!",
          "original_form": "사무직이요!",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045094.1.10",
          "form": "회계",
          "original_form": "회계",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045094.1.11",
          "form": "회계 어렵겠어요 학교 다닐때 회계원리만 배웠거든요",
          "original_form": "회계 어렵겠어요 학교 다닐때 회계원리만 배웠거든요",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045094.1.12",
          "form": "지금도 잘모르겠어요",
          "original_form": "지금도 잘모르겠어요",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045094.1.13",
          "form": "전공 어떤거예요?",
          "original_form": "전공 어떤거예요?",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045094.1.14",
          "form": "국제통상요",
          "original_form": "국제통상요",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045094.1.15",
          "form": "어! 저도 국제경영!!",
          "original_form": "어! 저도 국제경영!!",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045094.1.16",
          "form": "오 어쩌다 회계팀을 가신건가요",
          "original_form": "오 어쩌다 회계팀을 가신건가요",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045094.1.17",
          "form": "토익없이 갈 수 있는 데 넣다보니",
          "original_form": "토익없이 갈 수 있는 데 넣다보니",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045094.1.18",
          "form": "아하 토익 진짜 지겨워요 전 다시 봐야하는데 눈물나네요",
          "original_form": "아하 토익 진짜 지겨워요 전 다시 봐야하는데 눈물나네요",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045094.1.19",
          "form": "아..전 아마 신발사이즈...",
          "original_form": "아..전 아마 신발사이즈...",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045094.1.20",
          "form": "전 다시 하려니 막막하네요",
          "original_form": "전 다시 하려니 막막하네요",
          "speaker_id": "2",
          "time": "20210908 01:10"
        }
      ]
