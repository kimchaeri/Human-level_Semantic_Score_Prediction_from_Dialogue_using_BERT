      "utterance": [
        {
          "id": "MDRW2100035243.1.1",
          "form": "감독이 되게 웃긴다",
          "original_form": "감독이 되게 웃긴다",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035243.1.2",
          "form": "그러네. 장영남 나온다",
          "original_form": "그러네. 장영남 나온다",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035243.1.3",
          "form": "장영남 저분도 엄청 연기 잘하시더라",
          "original_form": "장영남 저분도 엄청 연기 잘하시더라",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100035243.1.4",
          "form": "어 정말 잘하드라",
          "original_form": "어 정말 잘하드라",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100035243.1.5",
          "form": "여기에 이경영도 나온다고 하더라",
          "original_form": "여기에 이경영도 나온다고 하더라",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100035243.1.6",
          "form": "나오는 출연진은 괜찮네",
          "original_form": "나오는 출연진은 괜찮네",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100035243.1.7",
          "form": "엉 연기구멍이 없는듯",
          "original_form": "엉 연기구멍이 없는듯",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100035243.1.8",
          "form": "요샌 티비엔이나 제이티비씨꺼 보니깐",
          "original_form": "요샌 티비엔이나 제이티비씨꺼 보니깐",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100035243.1.9",
          "form": "요즘은 스타출연한다고 드라마가 잘 되는게 없더라 무조건 연기랑 스토리가 좋아야하는듯",
          "original_form": "요즘은 스타출연한다고 드라마가 잘 되는게 없더라 무조건 연기랑 스토리가 좋아야하는듯",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100035243.1.10",
          "form": "그래 맞다. 입소문을 타니깐",
          "original_form": "그래 맞다. 입소문을 타니깐",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100035243.1.11",
          "form": "엉 거기다 시청률도 그렇게 많이 안올라가고",
          "original_form": "엉 거기다 시청률도 그렇게 많이 안올라가고",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100035243.1.12",
          "form": "워낙 방송사가 많고. 본방사수를 안하니",
          "original_form": "워낙 방송사가 많고. 본방사수를 안하니",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100035243.1.13",
          "form": "난 저 원슈타인은 놀면뭐하니에서 첨봄",
          "original_form": "난 저 원슈타인은 놀면뭐하니에서 첨봄",
          "speaker_id": "2",
          "time": "20210829 12:42"
        }
