        {
          "id": "MDRW2100018573.1.1",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210827 08:35"
        },
        {
          "id": "MDRW2100018573.1.2",
          "form": "저지금 카드값  너무많아서 가을옷도 못사구",
          "original_form": "저지금 카드값  너무많아서 가을옷도 못사구",
          "speaker_id": "2",
          "time": "20210827 08:35"
        },
        {
          "id": "MDRW2100018573.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:35"
        },
        {
          "id": "MDRW2100018573.1.4",
          "form": "사고싶은 옷 있어?",
          "original_form": "사고싶은 옷 있어?",
          "speaker_id": "1",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100018573.1.5",
          "form": "그냥....새로 다...",
          "original_form": "그냥....새로 다...",
          "speaker_id": "2",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100018573.1.6",
          "form": "사고싶네요 가을이니까요",
          "original_form": "사고싶네요 가을이니까요",
          "speaker_id": "2",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100018573.1.7",
          "form": "가을은 이쁜게많아요",
          "original_form": "가을은 이쁜게많아요",
          "speaker_id": "2",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100018573.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100018573.1.9",
          "form": "맞아 체크도이쁘구",
          "original_form": "맞아 체크도이쁘구",
          "speaker_id": "1",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100018573.1.10",
          "form": "셔츠도이쁘고 슬랙스도...",
          "original_form": "셔츠도이쁘고 슬랙스도...",
          "speaker_id": "1",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100018573.1.11",
          "form": "그냥 흰티에 자켓만 착 올려도...크",
          "original_form": "그냥 흰티에 자켓만 착 올려도...크",
          "speaker_id": "2",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100018573.1.12",
          "form": "그래 슬랙스를 좀 사야할거같아",
          "original_form": "그래 슬랙스를 좀 사야할거같아",
          "speaker_id": "2",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100018573.1.13",
          "form": "셔츠는 잘안어울려서 좀 고급진 티셔츠...블라우스같은 티셔츠..",
          "original_form": "셔츠는 잘안어울려서 좀 고급진 티셔츠...블라우스같은 티셔츠..",
          "speaker_id": "2",
          "time": "20210827 08:37"
        },
        {
          "id": "MDRW2100018573.1.14",
          "form": "헉 그런게 있어?",
          "original_form": "헉 그런게 있어?",
          "speaker_id": "1",
          "time": "20210827 08:37"
        },
        {
          "id": "MDRW2100018573.1.15",
          "form": "블라우스같은 티셔츠 넘 조은데?",
          "original_form": "블라우스같은 티셔츠 넘 조은데?",
          "speaker_id": "1",
          "time": "20210827 08:38"
        },
        {
          "id": "MDRW2100018573.1.16",
          "form": "일하러갔군",
          "original_form": "일하러갔군",
          "speaker_id": "1",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018573.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 늦어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 늦어서",
          "speaker_id": "2",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018573.1.18",
          "form": "비까지오네 후",
          "original_form": "비까지오네 후",
          "speaker_id": "2",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018573.1.19",
          "form": "이제들어왓어요",
          "original_form": "이제들어왓어요",
          "speaker_id": "2",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018573.1.20",
          "form": "그 티셔츠인데 좀 소재 좋은거 그런거 사야할거같아 ㅋㅋㅋ",
          "original_form": "그 티셔츠인데 좀 소재 좋은거 그런거 사야할거같아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100018573.1.21",
          "form": "올해산거중엔 에어랩이 제일 최고야..",
          "original_form": "올해산거중엔 에어랩이 제일 최고야..",
          "speaker_id": "2",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100018573.1.22",
          "form": "에어랩 자랑좀 해주세요",
          "original_form": "에어랩 자랑좀 해주세요",
          "speaker_id": "1",
          "time": "20210827 08:54"
        },
        {
          "id": "MDRW2100018573.1.23",
          "form": "난 단발할거라 사지말까 띵킹중",
          "original_form": "난 단발할거라 사지말까 띵킹중",
          "speaker_id": "1",
          "time": "20210827 08:54"
        },
        {
          "id": "MDRW2100018573.1.24",
          "form": "아 단발이면 솔직히모르겠는데",
          "original_form": "아 단발이면 솔직히모르겠는데",
          "speaker_id": "2",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018573.1.25",
          "form": "일단 그냥 드라이기가 좋아요",
          "original_form": "일단 그냥 드라이기가 좋아요",
          "speaker_id": "2",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018573.1.26",
          "form": "그츄...아무래도..?",
          "original_form": "그츄...아무래도..?",
          "speaker_id": "1",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018573.1.27",
          "form": "오... 그럼 드라이만 살까...",
          "original_form": "오... 그럼 드라이만 살까...",
          "speaker_id": "1",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018573.1.28",
          "form": "난 진짜 그것도 괜찮은거같아",
          "original_form": "난 진짜 그것도 괜찮은거같아",
          "speaker_id": "2",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018573.1.29",
          "form": "이번에 그 새로나온튤",
          "original_form": "이번에 그 새로나온튤",
          "speaker_id": "2",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018573.1.30",
          "form": "그거 넘 궁금해",
          "original_form": "그거 넘 궁금해",
          "speaker_id": "2",
          "time": "20210827 08:56"
        },
        {
          "id": "MDRW2100018573.1.31",
          "form": "에어랩에있는게 그냥 드라이기보단 약하다고하던데 ...그럼 드라이기 얼매나 좋은거죠...",
          "original_form": "에어랩에있는게 그냥 드라이기보단 약하다고하던데 ...그럼 드라이기 얼매나 좋은거죠...",
          "speaker_id": "2",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018573.1.32",
          "form": "그리고 막 말리다보면 나 전에는 뒤에 머리카락 몇번 껴봤거든 ..ㅎ",
          "original_form": "그리고 막 말리다보면 나 전에는 뒤에 머리카락 몇번 껴봤거든 ..ㅎ",
          "speaker_id": "2",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018573.1.33",
          "form": "그게 없으니까 걍 안으로 쓱 넣어서 말릴수있고 넘좋앙",
          "original_form": "그게 없으니까 걍 안으로 쓱 넣어서 말릴수있고 넘좋앙",
          "speaker_id": "2",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018573.1.34",
          "form": "안으로 쓱이 모지?",
          "original_form": "안으로 쓱이 모지?",
          "speaker_id": "1",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018573.1.35",
          "form": "씨컬 말하는건가?",
          "original_form": "씨컬 말하는건가?",
          "speaker_id": "1",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018573.1.36",
          "form": "아 그냥 머리 안으로 드라이기 집어넣어서",
          "original_form": "아 그냥 머리 안으로 드라이기 집어넣어서",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018573.1.37",
          "form": "두피말릴때 ㅋㅋㅋㅋ",
          "original_form": "두피말릴때 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018573.1.38",
          "form": "다른드라이기는 겉에서만 하고 머리 뒤집어까고 해야 안쪽 두피 말릴수있으니까",
          "original_form": "다른드라이기는 겉에서만 하고 머리 뒤집어까고 해야 안쪽 두피 말릴수있으니까",
          "speaker_id": "2",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018573.1.39",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018573.1.40",
          "form": "대박?",
          "original_form": "대박?",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018573.1.41",
          "form": "대박좋다잉",
          "original_form": "대박좋다잉",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018573.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그치",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그치",
          "speaker_id": "2",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018573.1.43",
          "form": "난 두피 빨리말릴수있는데 젤좋았어",
          "original_form": "난 두피 빨리말릴수있는데 젤좋았어",
          "speaker_id": "2",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018573.1.44",
          "form": "두피가중요해....",
          "original_form": "두피가중요해....",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018573.1.45",
          "form": "머리카락은 밖에 나가면 말라..",
          "original_form": "머리카락은 밖에 나가면 말라..",
          "speaker_id": "1",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018573.1.46",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018573.1.47",
          "form": "그래서 내가 머리숱많아서 두피가 잘안마르니까",
          "original_form": "그래서 내가 머리숱많아서 두피가 잘안마르니까",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018573.1.48",
          "form": "밤에 머리감는게싫었던건데",
          "original_form": "밤에 머리감는게싫었던건데",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018573.1.49",
          "form": "그게 해결되서 요즘은 밤에 머리감고 잘때많아 ㅋㅋㅋㅋ",
          "original_form": "그게 해결되서 요즘은 밤에 머리감고 잘때많아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:03"
        }
      ]
