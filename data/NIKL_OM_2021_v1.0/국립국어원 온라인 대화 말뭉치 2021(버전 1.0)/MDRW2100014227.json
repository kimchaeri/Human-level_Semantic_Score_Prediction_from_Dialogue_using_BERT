      "utterance": [
        {
          "id": "MDRW2100014227.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014227.1.2",
          "form": "요즘 비가 오니까 우중충 하네요",
          "original_form": "요즘 비가 오니까 우중충 하네요",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014227.1.3",
          "form": "그러니까요 날씨가 더웠다 비왔다 하네요",
          "original_form": "그러니까요 날씨가 더웠다 비왔다 하네요",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014227.1.4",
          "form": "그래서 요즘 우양산들고다니는게 일상이에요",
          "original_form": "그래서 요즘 우양산들고다니는게 일상이에요",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014227.1.5",
          "form": "오 저랑 똑같아요!! 양산필수템이라서 ㅜㅜ",
          "original_form": "오 저랑 똑같아요!! 양산필수템이라서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014227.1.6",
          "form": "빨리 추워졌음 좋겟네요",
          "original_form": "빨리 추워졌음 좋겟네요",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014227.1.7",
          "form": "요새 더운것도 더운데",
          "original_form": "요새 더운것도 더운데",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014227.1.8",
          "form": "너무 습해서",
          "original_form": "너무 습해서",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014227.1.9",
          "form": "날씨가 더 별로로 느껴지는거같아요",
          "original_form": "날씨가 더 별로로 느껴지는거같아요",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014227.1.10",
          "form": "맞아요 습한게 크죠!!",
          "original_form": "맞아요 습한게 크죠!!",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014227.1.11",
          "form": "이번주도 비가 온다는 소식이있어요 ㅠ",
          "original_form": "이번주도 비가 온다는 소식이있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014227.1.12",
          "form": "ㅠㅜㅜㅜ...",
          "original_form": "ㅠㅜㅜㅜ...",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014227.1.13",
          "form": "이번주도 우양산 필수군요",
          "original_form": "이번주도 우양산 필수군요",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014227.1.14",
          "form": "차라리 좀 건조하면 나을텐데",
          "original_form": "차라리 좀 건조하면 나을텐데",
          "speaker_id": "1",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014227.1.15",
          "form": "제습기를 빨리 사던가 해야겠어요",
          "original_form": "제습기를 빨리 사던가 해야겠어요",
          "speaker_id": "1",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014227.1.16",
          "form": "우아 맞아요 요즘 제습기 많이 사드라고요",
          "original_form": "우아 맞아요 요즘 제습기 많이 사드라고요",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014227.1.17",
          "form": "날씨가 너무 건조해도 문제고요 ㅜㅜ",
          "original_form": "날씨가 너무 건조해도 문제고요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014227.1.18",
          "form": "적당한게 제일 좋은거같아요",
          "original_form": "적당한게 제일 좋은거같아요",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014227.1.19",
          "form": "여름엔 습하고 겨울엔건조하고...",
          "original_form": "여름엔 습하고 겨울엔건조하고...",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014227.1.20",
          "form": "맞아요!!! 봄이나 가을 ! ㅜ 선선한게 최고인거같아요",
          "original_form": "맞아요!!! 봄이나 가을 ! ㅜ 선선한게 최고인거같아요",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014227.1.21",
          "form": "선선한게 제일 좋은거같아요",
          "original_form": "선선한게 제일 좋은거같아요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014227.1.22",
          "form": "봄이나 가을 비중이 더 높았으면 더 좋았을텐데",
          "original_form": "봄이나 가을 비중이 더 높았으면 더 좋았을텐데",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014227.1.23",
          "form": "우리나라 날씨는 봄가을이 스쳐지나가서...ㅋㅋㅋㅋ",
          "original_form": "우리나라 날씨는 봄가을이 스쳐지나가서...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014227.1.24",
          "form": "맞아요 ㅜ 그래도 이번 여름은 생각보다 안더운거같아요!",
          "original_form": "맞아요 ㅜ 그래도 이번 여름은 생각보다 안더운거같아요!",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100014227.1.25",
          "form": "이번에는 덥지 않고 습하게 지나가는거같아요",
          "original_form": "이번에는 덥지 않고 습하게 지나가는거같아요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100014227.1.26",
          "form": "저번이랑 저저번 여름이 정말 힘들었었죠",
          "original_form": "저번이랑 저저번 여름이 정말 힘들었었죠",
          "speaker_id": "1",
          "time": "20210825 22:03"
        }
      ]
