        },
        {
          "id": "MDRW2100039797.1.5",
          "form": "name1님은 어느 여행지가 가장 기억에 남으세요?",
          "original_form": "&name1&님은 어느 여행지가 가장 기억에 남으세요?",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039797.1.6",
          "form": "자주다니시나요~~?",
          "original_form": "자주다니시나요~~?",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039797.1.7",
          "form": "음 저는 화순이요",
          "original_form": "음 저는 화순이요",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039797.1.8",
          "form": "한동안은 안다녔는데요 틈날 때 마다 돌아다니기는 해요",
          "original_form": "한동안은 안다녔는데요 틈날 때 마다 돌아다니기는 해요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039797.1.9",
          "form": "화순이면 전라도 였던가요??",
          "original_form": "화순이면 전라도 였던가요??",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039797.1.10",
          "form": "네 음식이",
          "original_form": "네 음식이",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039797.1.11",
          "form": "기가막혔어요ㅋㅋㅋ",
          "original_form": "기가막혔어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039797.1.12",
          "form": "아무곳이나 들어가도 맛집이였어서ㅋㅋ",
          "original_form": "아무곳이나 들어가도 맛집이였어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039797.1.13",
          "form": "헐 그래요????",
          "original_form": "헐 그래요????",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039797.1.14",
          "form": "그냥 일반 한식 메뉴인가요????",
          "original_form": "그냥 일반 한식 메뉴인가요????",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039797.1.15",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039797.1.16",
          "form": "다 맛있었어요",
          "original_form": "다 맛있었어요",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039797.1.17",
          "form": "와 부럽네요....",
          "original_form": "와 부럽네요....",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039797.1.18",
          "form": "사실 제가 저번주에 순천에 있었는데요",
          "original_form": "사실 제가 저번주에 순천에 있었는데요",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039797.1.19",
          "form": "바로 옆동네네요 세상에 ㅋㅋㅋㅋ",
          "original_form": "바로 옆동네네요 세상에 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039797.1.20",
          "form": "와!",
          "original_form": "와!",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039797.1.21",
          "form": "순천도 너무좋죠?",
          "original_form": "순천도 너무좋죠?",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039797.1.22",
          "form": "네 순천 국가정원 진짜 예쁘더라구요",
          "original_form": "네 순천 국가정원 진짜 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039797.1.23",
          "form": "근데 제가 밥을 한끼도 못먹었어요ㅠㅠㅠ",
          "original_form": "근데 제가 밥을 한끼도 못먹었어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
