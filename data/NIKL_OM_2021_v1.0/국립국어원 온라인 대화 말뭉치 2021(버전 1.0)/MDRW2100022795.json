      "utterance": [
        {
          "id": "MDRW2100022795.1.1",
          "form": "없어요..",
          "original_form": "없어요..",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022795.1.2",
          "form": "키우고는싶은데 ㅜ",
          "original_form": "키우고는싶은데 ㅜ",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022795.1.3",
          "form": "저도 없어요ㅠ",
          "original_form": "저도 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022795.1.4",
          "form": "유튜브로만 보고 있어요",
          "original_form": "유튜브로만 보고 있어요",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022795.1.5",
          "form": "ㅋㅋ동물 키우고 싶은거 있으세요?",
          "original_form": "ㅋㅋ동물 키우고 싶은거 있으세요?",
          "speaker_id": "2",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022795.1.6",
          "form": "저는 고양이요! 강아지는 털알레르기가 있거든요",
          "original_form": "저는 고양이요! 강아지는 털알레르기가 있거든요",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022795.1.7",
          "form": "저두 개냥이 입양하고싶어요 ㅋㅋ",
          "original_form": "저두 개냥이 입양하고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022795.1.8",
          "form": "동물 유튜브도 보세요??",
          "original_form": "동물 유튜브도 보세요??",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022795.1.9",
          "form": "아니요 보시는거있나요?",
          "original_form": "아니요 보시는거있나요?",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022795.1.10",
          "form": ":)",
          "original_form": ":)",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022795.1.11",
          "form": "저는 하하하랑 똥괭이네랑 모카밀크 봐요",
          "original_form": "저는 하하하랑 똥괭이네랑 모카밀크 봐요",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022795.1.12",
          "form": "전부 귀여워요!",
          "original_form": "전부 귀여워요!",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022795.1.13",
          "form": "모카밀크는 강아지 색인지..",
          "original_form": "모카밀크는 강아지 색인지..",
          "speaker_id": "2",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022795.1.14",
          "form": "벌써부터 귀엽네요 동물들 생각에ㅎㅎ",
          "original_form": "벌써부터 귀엽네요 동물들 생각에ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022795.1.15",
          "form": "네 모카는 갈색 포메고 밀크는 흰색 사모예드랍니다",
          "original_form": "네 모카는 갈색 포메고 밀크는 흰색 사모예드랍니다",
          "speaker_id": "1",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022795.1.16",
          "form": "진짜 귀여워요",
          "original_form": "진짜 귀여워요",
          "speaker_id": "1",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022795.1.17",
          "form": "동물 알레르기는 없으세요??",
          "original_form": "동물 알레르기는 없으세요??",
          "speaker_id": "1",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022795.1.18",
          "form": "아아 진짜 귀엽겠어요",
          "original_form": "아아 진짜 귀엽겠어요",
          "speaker_id": "2",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100022795.1.19",
          "form": "네 알레르기는 없는데",
          "original_form": "네 알레르기는 없는데",
          "speaker_id": "2",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100022795.1.20",
          "form": "집에 있을 사람이 없어서 키울 형편은 안돼요 ㅠ",
          "original_form": "집에 있을 사람이 없어서 키울 형편은 안돼요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100022795.1.21",
          "form": "저도 마찬가지예요ㅠ",
          "original_form": "저도 마찬가지예요ㅠ",
          "speaker_id": "1",
          "time": "20210830 09:35"
        },
        {
          "id": "MDRW2100022795.1.22",
          "form": "키울 수 있다면 어떤 동물 키우고 싶으세요??",
          "original_form": "키울 수 있다면 어떤 동물 키우고 싶으세요??",
          "speaker_id": "1",
          "time": "20210830 09:35"
        }
      ]
