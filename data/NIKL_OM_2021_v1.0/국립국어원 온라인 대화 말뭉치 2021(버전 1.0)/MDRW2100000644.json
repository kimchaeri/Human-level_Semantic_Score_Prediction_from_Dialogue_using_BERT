      "utterance": [
        {
          "id": "MDRW2100000644.1.1",
          "form": "안녕하세요 가장 최근에 간 여행지가어디인가요?",
          "original_form": "안녕하세요 가장 최근에 간 여행지가어디인가요?",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000644.1.2",
          "form": "안녕하세요! \r\n여행 간 적이 없어요ㅜㅠ 굳이 따지자면 작년에 인천쪽 해수욕장 당일치기 ..?",
          "original_form": "안녕하세요! \r\n여행 간 적이 없어요ㅜㅠ 굳이 따지자면 작년에 인천쪽 해수욕장 당일치기 ..?",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000644.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000644.1.4",
          "form": "저는 혼자서 여수 갔었어요",
          "original_form": "저는 혼자서 여수 갔었어요",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000644.1.5",
          "form": "사람들 없을때요!",
          "original_form": "사람들 없을때요!",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000644.1.6",
          "form": "우와 여수!! 여수 좋죠?",
          "original_form": "우와 여수!! 여수 좋죠?",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000644.1.7",
          "form": "네 엄청 좋았어요",
          "original_form": "네 엄청 좋았어요",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000644.1.8",
          "form": "사람들도 착하고 한적하고 바다구경하기에 좋았어요!",
          "original_form": "사람들도 착하고 한적하고 바다구경하기에 좋았어요!",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000644.1.9",
          "form": "여수밤바다 노래필수인데 들으셨나요ㅋㅋ",
          "original_form": "여수밤바다 노래필수인데 들으셨나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000644.1.10",
          "form": "아그럼요 ㅋㅋㅋ",
          "original_form": "아그럼요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000644.1.11",
          "form": "모래 위에 앉아서 노래들으면서 바다구경하는게 로망입니다...ㅜ",
          "original_form": "모래 위에 앉아서 노래들으면서 바다구경하는게 로망입니다...ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000644.1.12",
          "form": "여수밤바다 들으면서 밤바다 구경했어요",
          "original_form": "여수밤바다 들으면서 밤바다 구경했어요",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000644.1.13",
          "form": "모래는 아니고 카페안에서 ㅎㅎ",
          "original_form": "모래는 아니고 카페안에서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000644.1.14",
          "form": "카페도 좋져 음료마시면서 보는 것도 운치있어요",
          "original_form": "카페도 좋져 음료마시면서 보는 것도 운치있어요",
          "speaker_id": "2",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000644.1.15",
          "form": "맞아요 전 계획형인간이라",
          "original_form": "맞아요 전 계획형인간이라",
          "speaker_id": "1",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000644.1.16",
          "form": "카페갈 계획이없었는데",
          "original_form": "카페갈 계획이없었는데",
          "speaker_id": "1",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000644.1.17",
          "form": "핸드폰 충전하느라 급하게 들어간 카페였는데",
          "original_form": "핸드폰 충전하느라 급하게 들어간 카페였는데",
          "speaker_id": "1",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000644.1.18",
          "form": "되게 괜찮았어요 ㅎㅎ",
          "original_form": "되게 괜찮았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000644.1.19",
          "form": "ㅋㅋㅋ저도 작년에 인천 해수욕장 갔을 때 카페에서 음료 마시면서 구경했었어요",
          "original_form": "ㅋㅋㅋ저도 작년에 인천 해수욕장 갔을 때 카페에서 음료 마시면서 구경했었어요",
          "speaker_id": "2",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000644.1.20",
          "form": "낮이라 사람이 많아서.. 카페로 피신했죠...ㅋㅋ",
          "original_form": "낮이라 사람이 많아서.. 카페로 피신했죠...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000644.1.21",
          "form": "근데 오히려 높은곳에서 내려다보니까 좋더라구요",
          "original_form": "근데 오히려 높은곳에서 내려다보니까 좋더라구요",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000644.1.22",
          "form": "높은곳에가면 기분이 좋더라구요",
          "original_form": "높은곳에가면 기분이 좋더라구요",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000644.1.23",
          "form": "바람도 살랑살랑 불고 경치도 좋구요",
          "original_form": "바람도 살랑살랑 불고 경치도 좋구요",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000644.1.24",
          "form": "나중에 갈 수 있다면 파리 에펠탑에 올라가보고싶어요",
          "original_form": "나중에 갈 수 있다면 파리 에펠탑에 올라가보고싶어요",
