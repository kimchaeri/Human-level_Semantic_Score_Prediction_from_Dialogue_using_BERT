      "utterance": [
        {
          "id": "MDRW2100025988.1.1",
          "form": "저는최근에 책 선물받았어요!",
          "original_form": "저는최근에 책 선물받았어요!",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025988.1.2",
          "form": "어떤책인가요?!",
          "original_form": "어떤책인가요?!",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025988.1.3",
          "form": "에스에프소설입니다!",
          "original_form": "에스에프소설입니다!",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025988.1.4",
          "form": "김초엽작가아시나요?!",
          "original_form": "김초엽작가아시나요?!",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025988.1.5",
          "form": "오..아니요! 그쪽 장르에사 유명한 작가님이신가요?",
          "original_form": "오..아니요! 그쪽 장르에사 유명한 작가님이신가요?",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025988.1.6",
          "form": "네! 그러신것같아요",
          "original_form": "네! 그러신것같아요",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025988.1.7",
          "form": "이번에 신작이 나와서",
          "original_form": "이번에 신작이 나와서",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025988.1.8",
          "form": "재밌을것같아요",
          "original_form": "재밌을것같아요",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025988.1.9",
          "form": "오 제목이뭔가요?",
          "original_form": "오 제목이뭔가요?",
          "speaker_id": "2",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025988.1.10",
          "form": "지구끝의온실 이고",
          "original_form": "지구끝의온실 이고",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025988.1.11",
          "form": "저작가제일유명한책은 우리가빛의속도로 갈수없다면",
          "original_form": "저작가제일유명한책은 우리가빛의속도로 갈수없다면",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025988.1.12",
          "form": "이에요!",
          "original_form": "이에요!",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025988.1.13",
          "form": "한번 찾아볼게요!",
          "original_form": "한번 찾아볼게요!",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025988.1.14",
          "form": "좋아하는 장르가 sf인가요?",
          "original_form": "좋아하는 장르가 sf인가요?",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025988.1.15",
          "form": "장르 상관없이 읽으려고 노력중인데",
          "original_form": "장르 상관없이 읽으려고 노력중인데",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025988.1.16",
          "form": "제일좋아하는건 소설이에요ㅠ",
          "original_form": "제일좋아하는건 소설이에요ㅠ",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025988.1.17",
          "form": "name2님은요???",
          "original_form": "&name2&님은요???",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025988.1.18",
          "form": "저도 책을 읽으려고 노력중인데",
          "original_form": "저도 책을 읽으려고 노력중인데",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100025988.1.19",
          "form": "에세이나 자기계발서 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "에세이나 자기계발서 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100025988.1.20",
          "form": "최근에 읽은 에세이 있으세요?!!",
          "original_form": "최근에 읽은 에세이 있으세요?!!",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
