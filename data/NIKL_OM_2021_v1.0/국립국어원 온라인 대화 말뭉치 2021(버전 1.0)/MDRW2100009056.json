        },
        {
          "id": "MDRW2100009056.1.2",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009056.1.3",
          "form": "네 요새는 못하지만",
          "original_form": "네 요새는 못하지만",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009056.1.4",
          "form": "뭐로 주로하세요? 컴퓨터? 폰?",
          "original_form": "뭐로 주로하세요? 컴퓨터? 폰?",
          "speaker_id": "1",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009056.1.5",
          "form": "요샌 폰으로 잠깐잠깐 해요",
          "original_form": "요샌 폰으로 잠깐잠깐 해요",
          "speaker_id": "2",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009056.1.6",
          "form": "캔디크러쉬요",
          "original_form": "캔디크러쉬요",
          "speaker_id": "2",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009056.1.7",
          "form": "오 옛날에 저도 했었는데!! 아직 하시는구나~",
          "original_form": "오 옛날에 저도 했었는데!! 아직 하시는구나~",
          "speaker_id": "1",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100009056.1.8",
          "form": "ㅎㅎ 지금 6937탄이예요",
          "original_form": "ㅎㅎ 지금 6937탄이예요",
          "speaker_id": "2",
          "time": "20210822 20:54"
        },
        {
          "id": "MDRW2100009056.1.9",
          "form": "와!!!! 그게 나온거 전부 깨신건가요? 대단하세요",
          "original_form": "와!!!! 그게 나온거 전부 깨신건가요? 대단하세요",
          "speaker_id": "1",
          "time": "20210822 20:55"
        },
        {
          "id": "MDRW2100009056.1.10",
          "form": "매주업데이트 한대요 멀었을듯요 ㅜ",
          "original_form": "매주업데이트 한대요 멀었을듯요 ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009056.1.11",
          "form": "와 매주 업데이트라니... 게임사도 참 부지런하네요",
          "original_form": "와 매주 업데이트라니... 게임사도 참 부지런하네요",
          "speaker_id": "1",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009056.1.12",
          "form": "그거 다따라가는 유저들도 대단하구요",
          "original_form": "그거 다따라가는 유저들도 대단하구요",
          "speaker_id": "1",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009056.1.13",
          "form": "그니깐요 ㅜ",
          "original_form": "그니깐요 ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009056.1.14",
          "form": "그래도 광고보면 아이템주서 다깨고있긴해요",
          "original_form": "그래도 광고보면 아이템주서 다깨고있긴해요",
          "speaker_id": "2",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009056.1.15",
          "form": "하시는게임 있으세요?",
          "original_form": "하시는게임 있으세요?",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009056.1.16",
          "form": "저는 요새 쿠키런 킹덤합니다!",
          "original_form": "저는 요새 쿠키런 킹덤합니다!",
          "speaker_id": "1",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009056.1.17",
          "form": "저도 예전에 했었는데 ㅎ",
          "original_form": "저도 예전에 했었는데 ㅎ",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009056.1.18",
          "form": "요새는 의리로 하고있어요^_^",
          "original_form": "요새는 의리로 하고있어요^_^",
          "speaker_id": "1",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009056.1.19",
          "form": "새로운 쿠키가 나와도 이제는 그냥저냥..",
