        },
        {
          "id": "MDRW2100000485.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000485.1.3",
          "form": "사고싶은거 있엉?",
          "original_form": "사고싶은거 있엉?",
          "speaker_id": "1",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000485.1.4",
          "form": "음요즘 네이버에서 여름원피스 하나 구매하고싶음",
          "original_form": "음요즘 네이버에서 여름원피스 하나 구매하고싶음",
          "speaker_id": "2",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000485.1.5",
          "form": "여름원피스?",
          "original_form": "여름원피스?",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000485.1.6",
          "form": "여름 다지나갔는데?",
          "original_form": "여름 다지나갔는데?",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000485.1.7",
          "form": "응 여름원피스 얇은거",
          "original_form": "응 여름원피스 얇은거",
          "speaker_id": "2",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000485.1.8",
          "form": "가을걸 사지그래",
          "original_form": "가을걸 사지그래",
          "speaker_id": "1",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100000485.1.9",
          "form": "아니 들어갈때 사야세일많을듯해서",
          "original_form": "아니 들어갈때 사야세일많을듯해서",
          "speaker_id": "2",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100000485.1.10",
          "form": "그건 그렇네 요즘 겨울파카 싸드라",
          "original_form": "그건 그렇네 요즘 겨울파카 싸드라",
          "speaker_id": "1",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100000485.1.11",
          "form": "겨울파카? 패딩?",
          "original_form": "겨울파카? 패딩?",
          "speaker_id": "2",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100000485.1.12",
          "form": "ㅇㅇ 역시즌이라 싸다고",
          "original_form": "ㅇㅇ 역시즌이라 싸다고",
          "speaker_id": "1",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100000485.1.13",
          "form": "까페에 글올라옴",
          "original_form": "까페에 글올라옴",
          "speaker_id": "1",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100000485.1.14",
          "form": "어디에서?",
          "original_form": "어디에서?",
          "speaker_id": "2",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100000485.1.15",
          "form": "퍼렐이였나?",
          "original_form": "퍼렐이였나?",
          "speaker_id": "1",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100000485.1.16",
          "form": "퍼렐꺼 좋은가?",
          "original_form": "퍼렐꺼 좋은가?",
          "speaker_id": "2",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100000485.1.17",
          "form": "페럴꺼 좋아?",
          "original_form": "페럴꺼 좋아?",
