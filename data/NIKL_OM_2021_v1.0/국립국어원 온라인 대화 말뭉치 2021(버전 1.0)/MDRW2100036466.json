      "utterance": [
        {
          "id": "MDRW2100036466.1.1",
          "form": "맞아 그리고 행복페이도 쓸 수 있어",
          "original_form": "맞아 그리고 행복페이도 쓸 수 있어",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.2",
          "form": "아 대구로는 배달어플인거 알지?",
          "original_form": "아 대구로는 배달어플인거 알지?",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.3",
          "form": "행복페이 맞아",
          "original_form": "행복페이 맞아",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.4",
          "form": "배달음식은 너무 비싼데",
          "original_form": "배달음식은 너무 비싼데",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.5",
          "form": "행복페이는 5퍼 추가할인이야",
          "original_form": "행복페이는 5퍼 추가할인이야",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.6",
          "form": "나도 그걸로 찜닭을 먹었어",
          "original_form": "나도 그걸로 찜닭을 먹었어",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.7",
          "form": "맞아 할인될수록 너무 좋지",
          "original_form": "맞아 할인될수록 너무 좋지",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.8",
          "form": "양이 너무 많아서 3일간 먹었어",
          "original_form": "양이 너무 많아서 3일간 먹었어",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.9",
          "form": "이제 닭은 질려",
          "original_form": "이제 닭은 질려",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.10",
          "form": "세상에 어디찜닭을 먹었어?",
          "original_form": "세상에 어디찜닭을 먹었어?",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.11",
          "form": "치킨도 안먹고싶더라",
          "original_form": "치킨도 안먹고싶더라",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.12",
          "form": "난 치킨을 좋아하거듣",
          "original_form": "난 치킨을 좋아하거듣",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.13",
          "form": "달봉찜닭이엇나?",
          "original_form": "달봉찜닭이엇나?",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.14",
          "form": "내 최애 찜닭집이야",
          "original_form": "내 최애 찜닭집이야",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.15",
          "form": "나는 1인 찜닭 메뉴가 있길래 그걸 먹었어",
          "original_form": "나는 1인 찜닭 메뉴가 있길래 그걸 먹었어",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.16",
          "form": "대구로에 입점했더라",
          "original_form": "대구로에 입점했더라",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.17",
          "form": "당면 양이 많아",
          "original_form": "당면 양이 많아",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.18",
          "form": "나는 찜닭안에 양파도 좋아해",
          "original_form": "나는 찜닭안에 양파도 좋아해",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.19",
          "form": "달봉찜닭은 정말 촤고야",
          "original_form": "달봉찜닭은 정말 촤고야",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.20",
          "form": "양파 많이 주세요",
          "original_form": "양파 많이 주세요",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036466.1.21",
          "form": "당면도 좋은데 소화가 잘언되더라 요즘",
          "original_form": "당면도 좋은데 소화가 잘언되더라 요즘",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036466.1.22",
          "form": "그거 무조건 요청사항에 적어야지",
          "original_form": "그거 무조건 요청사항에 적어야지",
          "speaker_id": "2",
          "time": "20210830 23:26"
        }
      ]
