      "utterance": [
        {
          "id": "MDRW2100011298.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 10:51"
        },
        {
          "id": "MDRW2100011298.1.2",
          "form": "안녕하세요. 결혼은 하셨나요",
          "original_form": "안녕하세요. 결혼은 하셨나요",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100011298.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100011298.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100011298.1.5",
          "form": "저도 미혼입니다",
          "original_form": "저도 미혼입니다",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100011298.1.6",
          "form": "애인은 있으신가요?",
          "original_form": "애인은 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100011298.1.7",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100011298.1.8",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100011298.1.9",
          "form": "없습니다",
          "original_form": "없습니다",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100011298.1.10",
          "form": "결혼할 생각은 있으신가요?",
          "original_form": "결혼할 생각은 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100011298.1.11",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100011298.1.12",
          "form": "저는 있어요",
          "original_form": "저는 있어요",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100011298.1.13",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100011298.1.14",
          "form": "저는 사실 결혼에 회의적이에요",
          "original_form": "저는 사실 결혼에 회의적이에요",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100011298.1.15",
          "form": "제 한몸 건사하기 어렵기도 하고.",
          "original_form": "제 한몸 건사하기 어렵기도 하고.",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100011298.1.16",
          "form": "끌리는 사람이 있던 것도 아니고요",
          "original_form": "끌리는 사람이 있던 것도 아니고요",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100011298.1.17",
          "form": "이상형은 어떻게 되세요?",
          "original_form": "이상형은 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100011298.1.18",
          "form": "ㅠㅠ요즘 대부분 제주위도 결혼에 그닥인사람들 많더라고요",
          "original_form": "ㅠㅠ요즘 대부분 제주위도 결혼에 그닥인사람들 많더라고요",
          "speaker_id": "1",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100011298.1.19",
          "form": "저는 내적인게 중요하다고생각해요",
          "original_form": "저는 내적인게 중요하다고생각해요",
          "speaker_id": "1",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100011298.1.20",
          "form": "맞아요 외적인건 한때라",
          "original_form": "맞아요 외적인건 한때라",
          "speaker_id": "2",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100011298.1.21",
          "form": "연애감정은 느낀적 없어도 결국 호감가는건 말통하는 사람들이더라고요",
          "original_form": "연애감정은 느낀적 없어도 결국 호감가는건 말통하는 사람들이더라고요",
          "speaker_id": "2",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100011298.1.22",
          "form": "맞아요 그리고",
          "original_form": "맞아요 그리고",
          "speaker_id": "1",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100011298.1.23",
          "form": "인성이 너무중요하다생각해요!",
          "original_form": "인성이 너무중요하다생각해요!",
          "speaker_id": "1",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100011298.1.24",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100011298.1.25",
          "form": "근데 사람 속은 알기가 너무 어려워요ㅜ",
          "original_form": "근데 사람 속은 알기가 너무 어려워요ㅜ",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100011298.1.26",
          "form": "그게 제일",
          "original_form": "그게 제일",
          "speaker_id": "1",
          "time": "20210825 00:23"
        },
        {
          "id": "MDRW2100011298.1.27",
          "form": "큰 문제죠",
          "original_form": "큰 문제죠",
          "speaker_id": "1",
          "time": "20210825 00:23"
        }
      ]
    }
