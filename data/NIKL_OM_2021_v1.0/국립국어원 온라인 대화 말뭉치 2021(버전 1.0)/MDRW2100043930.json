      "utterance": [
        {
          "id": "MDRW2100043930.1.1",
          "form": "사고싶은물건있으시나여",
          "original_form": "사고싶은물건있으시나여",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100043930.1.2",
          "form": "저는 요즘 가을옷 사고싶네요",
          "original_form": "저는 요즘 가을옷 사고싶네요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100043930.1.3",
          "form": "저두요 가을옷 어떤옷이여",
          "original_form": "저두요 가을옷 어떤옷이여",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100043930.1.4",
          "form": "니트나 후드 맨투맨이요ㅎㅎ",
          "original_form": "니트나 후드 맨투맨이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100043930.1.5",
          "form": "사실 후드티는 이미 샀어요~",
          "original_form": "사실 후드티는 이미 샀어요~",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100043930.1.6",
          "form": "전 이제사야해서 고민이에요",
          "original_form": "전 이제사야해서 고민이에요",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100043930.1.7",
          "form": "어떤색 사세요?",
          "original_form": "어떤색 사세요?",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100043930.1.8",
          "form": "아이보리 샀어요! 밝은색 사고싶어서요 ㅋㅋ",
          "original_form": "아이보리 샀어요! 밝은색 사고싶어서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100043930.1.9",
          "form": "오오 아이보리이쁘져 저도 아이보리랑 연보라고민중이에요",
          "original_form": "오오 아이보리이쁘져 저도 아이보리랑 연보라고민중이에요",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100043930.1.10",
          "form": "오 맞아요 연보라도 예쁘죠!",
          "original_form": "오 맞아요 연보라도 예쁘죠!",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100043930.1.11",
          "form": "근데 전 무난한게 좋을 것같아사 아이보리샀어요 ㅋㅋ",
          "original_form": "근데 전 무난한게 좋을 것같아사 아이보리샀어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100043930.1.12",
          "form": "근데연보라 이상한색올때있더라구여ㅠㅋㅋㅋㅋㅋ",
          "original_form": "근데연보라 이상한색올때있더라구여ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100043930.1.13",
          "form": "맞아요 무난한거면 아이보리랑 회색인데",
          "original_form": "맞아요 무난한거면 아이보리랑 회색인데",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100043930.1.14",
          "form": "그쵸 맞아요 사실 후드티는 회색이긴 하죠",
          "original_form": "그쵸 맞아요 사실 후드티는 회색이긴 하죠",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100043930.1.15",
          "form": "네 회후가 젤무난하고 그래서 어렵네요 ㅋㅋㅋㅋ",
          "original_form": "네 회후가 젤무난하고 그래서 어렵네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100043930.1.16",
          "form": "그쵸 ㅋㅋ 옷 쇼핑할때 색이 참 고민돼요",
          "original_form": "그쵸 ㅋㅋ 옷 쇼핑할때 색이 참 고민돼요",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100043930.1.17",
          "form": "네 ㅋㅋㅋㅋ아무래도 보정도있고 그색이아닌게많더라구여",
          "original_form": "네 ㅋㅋㅋㅋ아무래도 보정도있고 그색이아닌게많더라구여",
          "speaker_id": "1",
          "time": "20210907 22:51"
        }
      ]
