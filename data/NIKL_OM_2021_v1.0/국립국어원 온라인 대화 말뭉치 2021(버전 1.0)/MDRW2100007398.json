      "utterance": [
        {
          "id": "MDRW2100007398.1.1",
          "form": "게임하니?",
          "original_form": "게임하니?",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007398.1.2",
          "form": "게임중?",
          "original_form": "게임중?",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007398.1.3",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007398.1.4",
          "form": "이제 돌아옴",
          "original_form": "이제 돌아옴",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007398.1.5",
          "form": "한시간이나 했네 ㄷㄷ",
          "original_form": "한시간이나 했네 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007398.1.6",
          "form": "킹덤 아레나 좀 깬다고",
          "original_form": "킹덤 아레나 좀 깬다고",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007398.1.7",
          "form": "아레나 티켓을 뭔 30개나 줘서ㅠ",
          "original_form": "아레나 티켓을 뭔 30개나 줘서ㅠ",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007398.1.8",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007398.1.9",
          "form": "너무오래하네",
          "original_form": "너무오래하네",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007398.1.10",
          "form": "아레나티켓 그거 내일쓰면되지!!!",
          "original_form": "아레나티켓 그거 내일쓰면되지!!!",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007398.1.11",
          "form": "근데 내가 오늘 다 안쓰면",
          "original_form": "근데 내가 오늘 다 안쓰면",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007398.1.12",
          "form": "새벽동안 얻게될 10개분량의 티켓을",
          "original_form": "새벽동안 얻게될 10개분량의 티켓을",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007398.1.13",
          "form": "날리게되는셈이잖아",
          "original_form": "날리게되는셈이잖아",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007398.1.14",
          "form": "아니ㅋㅋㅋㅋ",
          "original_form": "아니ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007398.1.15",
          "form": "마찍먹은 이런 마음가짐이구나",
          "original_form": "마찍먹은 이런 마음가짐이구나",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007398.1.16",
          "form": "이게 랭커라는 거구나",
          "original_form": "이게 랭커라는 거구나",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007398.1.17",
          "form": "근데 그럼 패배떠도 다 눌러?",
          "original_form": "근데 그럼 패배떠도 다 눌러?",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007398.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007398.1.19",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007398.1.20",
          "form": "근데 요즘 이기면 7정도 쥬고",
          "original_form": "근데 요즘 이기면 7정도 쥬고",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007398.1.21",
          "form": "져도 3~4정도 잃어서",
          "original_form": "져도 3~4정도 잃어서",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007398.1.22",
          "form": "계속 도전하는게 이득이야",
          "original_form": "계속 도전하는게 이득이야",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007398.1.23",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.24",
          "form": "킹덤패스사면",
          "original_form": "킹덤패스사면",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.25",
          "form": "텀이 10분일 텐데",
          "original_form": "텀이 10분일 텐데",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.26",
          "form": "적절하게 섞어쓸수있겠네",
          "original_form": "적절하게 섞어쓸수있겠네",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.27",
          "form": "ㅇㅇ그치그치",
          "original_form": "ㅇㅇ그치그치",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.28",
          "form": "오늘자 파르페 미션도 다 했어!",
          "original_form": "오늘자 파르페 미션도 다 했어!",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.29",
          "form": "뿌듯해!",
          "original_form": "뿌듯해!",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.30",
          "form": "야 근데 너 혹시 배그도 한적잇음?",
          "original_form": "야 근데 너 혹시 배그도 한적잇음?",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.31",
          "form": "좋겠네",
          "original_form": "좋겠네",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.32",
          "form": "모바일만",
          "original_form": "모바일만",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.33",
          "form": "얼마전에 name3이랑 피방갔는데",
          "original_form": "얼마전에 &name3&이랑 피방갔는데",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.34",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007398.1.35",
          "form": "너랑 배그를 해봤는데 니 렙이 300이었댕",
          "original_form": "너랑 배그를 해봤는데 니 렙이 300이었댕",
          "speaker_id": "2",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007398.1.36",
          "form": "옵치랑 착각한거겠지?",
          "original_form": "옵치랑 착각한거겠지?",
          "speaker_id": "2",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007398.1.37",
          "form": "헐ㅋㅋ걔가 게임도해?",
          "original_form": "헐ㅋㅋ걔가 게임도해?",
          "speaker_id": "1",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007398.1.38",
          "form": "응 옵치임",
          "original_form": "응 옵치임",
          "speaker_id": "1",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007398.1.39",
          "form": "컴배그 한판은 했나",
          "original_form": "컴배그 한판은 했나",
          "speaker_id": "1",
          "time": "20210822 02:52"
        }
      ]
