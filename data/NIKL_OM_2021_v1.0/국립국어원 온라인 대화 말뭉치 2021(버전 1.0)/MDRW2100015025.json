      "utterance": [
        {
          "id": "MDRW2100015025.1.1",
          "form": "무슨생각인지모르겠어",
          "original_form": "무슨생각인지모르겠어",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015025.1.2",
          "form": "아빠도 어릴때 사랑 못 받아서 그래",
          "original_form": "아빠도 어릴때 사랑 못 받아서 그래",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015025.1.3",
          "form": "맞다...징징대는스타일 이제",
          "original_form": "맞다...징징대는스타일 이제",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015025.1.4",
          "form": "그래ㅜ그리고 식탐 강하고",
          "original_form": "그래ㅜ그리고 식탐 강하고",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100015025.1.5",
          "form": "식탐이...대리만족이가",
          "original_form": "식탐이...대리만족이가",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015025.1.6",
          "form": "욕구불만이겠지",
          "original_form": "욕구불만이겠지",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015025.1.7",
          "form": "아 그렇네 욕구불만이 맞겠다",
          "original_form": "아 그렇네 욕구불만이 맞겠다",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015025.1.8",
          "form": "아이를 잘 키워야된다ㅜ저거보면",
          "original_form": "아이를 잘 키워야된다ㅜ저거보면",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015025.1.9",
          "form": "ㅇㅇ예전에 누가그러더라",
          "original_form": "ㅇㅇ예전에 누가그러더라",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015025.1.10",
          "form": "돈도중요하지만 아이잘키우는것도 중요하다고",
          "original_form": "돈도중요하지만 아이잘키우는것도 중요하다고",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015025.1.11",
          "form": "나중에 부자랑 자식 잘 키운 사람 옆에 붙는다잖아",
          "original_form": "나중에 부자랑 자식 잘 키운 사람 옆에 붙는다잖아",
          "speaker_id": "2",
          "time": "20210826 06:51"
        },
        {
          "id": "MDRW2100015025.1.12",
          "form": "그래 그렇다고하데\r\n공부잘하면 자연스레 옆에붙는다데",
          "original_form": "그래 그렇다고하데\r\n공부잘하면 자연스레 옆에붙는다데",
          "speaker_id": "1",
          "time": "20210826 08:20"
        },
        {
          "id": "MDRW2100015025.1.13",
          "form": "응 그렇다하더라~",
          "original_form": "응 그렇다하더라~",
          "speaker_id": "2",
          "time": "20210826 08:22"
        },
        {
          "id": "MDRW2100015025.1.14",
          "form": "결국 자식복이 최고봉 인건가",
          "original_form": "결국 자식복이 최고봉 인건가",
          "speaker_id": "1",
          "time": "20210826 08:23"
        },
        {
          "id": "MDRW2100015025.1.15",
          "form": "인성도 중요하고..",
          "original_form": "인성도 중요하고..",
          "speaker_id": "1",
          "time": "20210826 08:23"
        },
        {
          "id": "MDRW2100015025.1.16",
          "form": "자식 잘 키우는게 최고겠지",
          "original_form": "자식 잘 키우는게 최고겠지",
          "speaker_id": "2",
          "time": "20210826 08:47"
        }
      ]
