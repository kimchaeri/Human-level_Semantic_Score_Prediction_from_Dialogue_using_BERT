      "utterance": [
        {
          "id": "MDRW2100018879.1.1",
          "form": "너가 달이보다 놀이공원 좋아하는 거 같아...",
          "original_form": "너가 달이보다 놀이공원 좋아하는 거 같아...",
          "speaker_id": "2",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018879.1.2",
          "form": "거기에 무서운것만 있진 않을거아니야",
          "original_form": "거기에 무서운것만 있진 않을거아니야",
          "speaker_id": "1",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018879.1.3",
          "form": "가자가자ㅋㅋ",
          "original_form": "가자가자ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018879.1.4",
          "form": "나 바이킹빼고 다 좋아해",
          "original_form": "나 바이킹빼고 다 좋아해",
          "speaker_id": "1",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018879.1.5",
          "form": "일단 가면 무서운거 태울거잖아",
          "original_form": "일단 가면 무서운거 태울거잖아",
          "speaker_id": "2",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018879.1.6",
          "form": "아니야 다른거 뭐있나 함 보자",
          "original_form": "아니야 다른거 뭐있나 함 보자",
          "speaker_id": "1",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018879.1.7",
          "form": "보기만 하지 않을거잖아..",
          "original_form": "보기만 하지 않을거잖아..",
          "speaker_id": "2",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018879.1.8",
          "form": "범퍼카 이런건 재밌자낭",
          "original_form": "범퍼카 이런건 재밌자낭",
          "speaker_id": "1",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018879.1.9",
          "form": "디스코팡팡도 있네ㅋㅋ 이것도 싫어?",
          "original_form": "디스코팡팡도 있네ㅋㅋ 이것도 싫어?",
          "speaker_id": "1",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018879.1.10",
          "form": "헐 디스코팡팡도 있어? 대박",
          "original_form": "헐 디스코팡팡도 있어? 대박",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018879.1.11",
          "form": "그건 타보고 싶어",
          "original_form": "그건 타보고 싶어",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018879.1.12",
          "form": "미로탐험도 있다",
          "original_form": "미로탐험도 있다",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018879.1.13",
          "form": "런닝맨에 나왔다는데 뭐지ㅋㅋ",
          "original_form": "런닝맨에 나왔다는데 뭐지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018879.1.14",
          "form": "런닝맨에 나왔대?",
          "original_form": "런닝맨에 나왔대?",
          "speaker_id": "2",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018879.1.15",
          "form": "미로탐험이 디스코팡팡이라는거지?",
          "original_form": "미로탐험이 디스코팡팡이라는거지?",
          "speaker_id": "2",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018879.1.16",
          "form": "아니아니",
          "original_form": "아니아니",
          "speaker_id": "1",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018879.1.17",
          "form": "아 맞아ㅋㅋㅋ",
          "original_form": "아 맞아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018879.1.18",
          "form": "아니네?^^ㅋㅋ",
          "original_form": "아니네?^^ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018879.1.19",
          "form": "미로탐험이랑 디스코팡팡이랑 달라!",
          "original_form": "미로탐험이랑 디스코팡팡이랑 달라!",
          "speaker_id": "1",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018879.1.20",
          "form": "근데 운휴기종이라네.. 운행 안하는건가?",
          "original_form": "근데 운휴기종이라네.. 운행 안하는건가?",
          "speaker_id": "1",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018879.1.21",
          "form": "이런..위험해서 그런가?",
          "original_form": "이런..위험해서 그런가?",
          "speaker_id": "2",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018879.1.22",
          "form": "ㅋㅋㅋ아니여 다른것들도 몇몇개 운휴인거있는거봐서는 흠.. 오래돼서?뭘까",
          "original_form": "ㅋㅋㅋ아니여 다른것들도 몇몇개 운휴인거있는거봐서는 흠.. 오래돼서?뭘까",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018879.1.23",
          "form": "그럼 더 불안한뎈ㅋㅋㅋ",
          "original_form": "그럼 더 불안한뎈ㅋㅋㅋ",
