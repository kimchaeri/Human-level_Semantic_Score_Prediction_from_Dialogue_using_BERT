      "utterance": [
        {
          "id": "MDRW2100005733.1.1",
          "form": "칸막이도 하고",
          "original_form": "칸막이도 하고",
          "speaker_id": "2",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.2",
          "form": "마스크도 쓰고",
          "original_form": "마스크도 쓰고",
          "speaker_id": "2",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.3",
          "form": "대화도 잘 못해요",
          "original_form": "대화도 잘 못해요",
          "speaker_id": "2",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.4",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.5",
          "form": "넘 별로예요",
          "original_form": "넘 별로예요",
          "speaker_id": "1",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.6",
          "form": "쉬는시간에 친구들이랑 잘 놀지도 못하고",
          "original_form": "쉬는시간에 친구들이랑 잘 놀지도 못하고",
          "speaker_id": "2",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.7",
          "form": "체육수업은요,",
          "original_form": "체육수업은요,",
          "speaker_id": "1",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.8",
          "form": "소풍이나",
          "original_form": "소풍이나",
          "speaker_id": "2",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.9",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.10",
          "form": "견학 같은 것도 없고",
          "original_form": "견학 같은 것도 없고",
          "speaker_id": "2",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.11",
          "form": "체육수업도",
          "original_form": "체육수업도",
          "speaker_id": "2",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.12",
          "form": "딱 한 번 했어요",
          "original_form": "딱 한 번 했어요",
          "speaker_id": "2",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.13",
          "form": "ㅠㅜㅜ 안돼요",
          "original_form": "ㅠㅜㅜ 안돼요",
          "speaker_id": "1",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005733.1.14",
          "form": "운동장에 나가서 어울리는 거 자체를",
          "original_form": "운동장에 나가서 어울리는 거 자체를",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.15",
          "form": "안하더라구요",
          "original_form": "안하더라구요",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.16",
          "form": "너무 안타깝죠",
          "original_form": "너무 안타깝죠",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.17",
          "form": "보물찾기 피구 그런거도 못하구",
          "original_form": "보물찾기 피구 그런거도 못하구",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.18",
          "form": "네 ㅠ",
          "original_form": "네 ㅠ",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.19",
          "form": "소풍도 없는 초딩이라니",
          "original_form": "소풍도 없는 초딩이라니",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.20",
          "form": "선생님들도",
          "original_form": "선생님들도",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.21",
          "form": "마스크에 예민해져 있으니",
          "original_form": "마스크에 예민해져 있으니",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.22",
          "form": "아이들이 서로 붙어 있지 못하게 해요",
          "original_form": "아이들이 서로 붙어 있지 못하게 해요",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.23",
          "form": "그러게요 ㅠ",
          "original_form": "그러게요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.24",
          "form": "그렇겠어요",
          "original_form": "그렇겠어요",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.25",
          "form": "둘러앉아서 김밥두먹고",
          "original_form": "둘러앉아서 김밥두먹고",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.26",
          "form": "장기자랑도 하고",
          "original_form": "장기자랑도 하고",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005733.1.27",
          "form": "애들끼리 싸움도 말싸움으로만??? ㅎㅎ",
          "original_form": "애들끼리 싸움도 말싸움으로만??? ㅎㅎ",
