      "utterance": [
        {
          "id": "MDRW2100007254.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007254.1.2",
          "form": "게임뭐하셔요",
          "original_form": "게임뭐하셔요",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007254.1.3",
          "form": "배틀 그라운드요",
          "original_form": "배틀 그라운드요",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007254.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007254.1.5",
          "form": "저는 배그하다가 욕먹어서",
          "original_form": "저는 배그하다가 욕먹어서",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007254.1.6",
          "form": "오버워치 쿠킹덤요",
          "original_form": "오버워치 쿠킹덤요",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007254.1.7",
          "form": "누가 욕했어요ㅠㅠ",
          "original_form": "누가 욕했어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007254.1.8",
          "form": "제가 얼탔어요",
          "original_form": "제가 얼탔어요",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007254.1.9",
          "form": "착지를 이상한데했어요",
          "original_form": "착지를 이상한데했어요",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007254.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007254.1.11",
          "form": "그렇다고 욕까지!!!",
          "original_form": "그렇다고 욕까지!!!",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007254.1.12",
          "form": "ㅋㅋㅋㅋ배그친구는 화가많더라구요",
          "original_form": "ㅋㅋㅋㅋ배그친구는 화가많더라구요",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007254.1.13",
          "form": "배그 주식도 이번에나왔던데",
          "original_form": "배그 주식도 이번에나왔던데",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007254.1.14",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007254.1.15",
          "form": "40에서 샀었어야 하는데",
          "original_form": "40에서 샀었어야 하는데",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007254.1.16",
          "form": "요새 마니 올랐더라고요",
          "original_form": "요새 마니 올랐더라고요",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007254.1.17",
          "form": "배그밖에 없는줄알았는데",
          "original_form": "배그밖에 없는줄알았는데",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007254.1.18",
          "form": "인기가 꽤 있더라고요",
          "original_form": "인기가 꽤 있더라고요",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007254.1.19",
          "form": "하긴 20대들 배그안해본사람없으니...",
          "original_form": "하긴 20대들 배그안해본사람없으니...",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007254.1.20",
          "form": "배그 공모주로 넣을걸그랬어요",
          "original_form": "배그 공모주로 넣을걸그랬어요",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007254.1.21",
          "form": "그건 아니에요ㅎㅎ",
          "original_form": "그건 아니에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007254.1.22",
          "form": "올라서 공모가에요ㅎㅎ",
          "original_form": "올라서 공모가에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007254.1.23",
          "form": "앗ㅎㅎ",
          "original_form": "앗ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007254.1.24",
          "form": "배그가 인기가 많아져야ㅠ",
          "original_form": "배그가 인기가 많아져야ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007254.1.25",
          "form": "매출 대부분이 중국이라ㅎㅎ",
          "original_form": "매출 대부분이 중국이라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:14"
        }
      ]
