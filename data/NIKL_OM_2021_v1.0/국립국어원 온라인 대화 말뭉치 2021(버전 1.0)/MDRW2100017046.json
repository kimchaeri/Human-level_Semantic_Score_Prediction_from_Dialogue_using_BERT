      "utterance": [
        {
          "id": "MDRW2100017046.1.1",
          "form": "어제 먹었는데 또 먹고 싶어",
          "original_form": "어제 먹었는데 또 먹고 싶어",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017046.1.2",
          "form": "나도 마라탕",
          "original_form": "나도 마라탕",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017046.1.3",
          "form": "나아도",
          "original_form": "나아도",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017046.1.4",
          "form": "마라탕 몇 단계로 먹어?",
          "original_form": "마라탕 몇 단계로 먹어?",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017046.1.5",
          "form": "난 2단계?",
          "original_form": "난 2단계?",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017046.1.6",
          "form": "2단계가 제일 적당한 거 같아",
          "original_form": "2단계가 제일 적당한 거 같아",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017046.1.7",
          "form": "나 3단계 울면서 먹었어",
          "original_form": "나 3단계 울면서 먹었어",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017046.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017046.1.9",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017046.1.10",
          "form": "나중에 청량리에 있는",
          "original_form": "나중에 청량리에 있는",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017046.1.11",
          "form": "로운샤브가서",
          "original_form": "로운샤브가서",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017046.1.12",
          "form": "홍탕으로 ㄱ?",
          "original_form": "홍탕으로 ㄱ?",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017046.1.13",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017046.1.14",
          "form": "거기 또 가고 싶었어",
          "original_form": "거기 또 가고 싶었어",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017046.1.15",
          "form": "가자가자",
          "original_form": "가자가자",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017046.1.16",
          "form": "월남쌈도 먹고 싶다",
          "original_form": "월남쌈도 먹고 싶다",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017046.1.17",
          "form": "마라탕 매장은 보통 어디로 가?",
          "original_form": "마라탕 매장은 보통 어디로 가?",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017046.1.18",
          "form": "나 그냥 애들이랑 같이 가던곳 아니면",
          "original_form": "나 그냥 애들이랑 같이 가던곳 아니면",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017046.1.19",
          "form": "지나가다가 보이는 곳",
          "original_form": "지나가다가 보이는 곳",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017046.1.20",
          "form": "학교 근처에도 있어?",
          "original_form": "학교 근처에도 있어?",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017046.1.21",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017046.1.22",
          "form": "안양1번가에 있음",
          "original_form": "안양1번가에 있음",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017046.1.23",
          "form": "좋다!",
          "original_form": "좋다!",
          "speaker_id": "1",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017046.1.24",
          "form": "나 방금 3000원 쿠폰 받았는데 마라탕 시킬까 말까...",
          "original_form": "나 방금 3000원 쿠폰 받았는데 마라탕 시킬까 말까...",
          "speaker_id": "1",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017046.1.25",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017046.1.26",
          "form": "어제 먹었다며",
          "original_form": "어제 먹었다며",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017046.1.27",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017046.1.28",
          "form": "참아야지",
