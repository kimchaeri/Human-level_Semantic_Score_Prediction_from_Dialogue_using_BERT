      "utterance": [
        {
          "id": "MDRW2100012469.1.1",
          "form": "으윽....",
          "original_form": "으윽....",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012469.1.2",
          "form": "레알 김치요?",
          "original_form": "레알 김치요?",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012469.1.3",
          "form": "네 초콜릿속에 말린 건조김치 들은 거였어요",
          "original_form": "네 초콜릿속에 말린 건조김치 들은 거였어요",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012469.1.4",
          "form": "그나마 건조김치라 다행",
          "original_form": "그나마 건조김치라 다행",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012469.1.5",
          "form": "어으.. 무슨맛이져 대체",
          "original_form": "어으.. 무슨맛이져 대체",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012469.1.6",
          "form": "외국인들이 정말 우리가 그런거 먹는 사람으로 생각할까봐 걱정됐어요",
          "original_form": "외국인들이 정말 우리가 그런거 먹는 사람으로 생각할까봐 걱정됐어요",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012469.1.7",
          "form": "제주도기념품중에서도 약간 그런거 있지않나요 ㅋㅋㅋ",
          "original_form": "제주도기념품중에서도 약간 그런거 있지않나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012469.1.8",
          "form": "ㅡㅠㅡ",
          "original_form": "ㅡㅠㅡ",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012469.1.9",
          "form": "근데 외국에서 더 난리잖아요 김치로 ㅋㅋㅋㅋ",
          "original_form": "근데 외국에서 더 난리잖아요 김치로 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012469.1.10",
          "form": "궁금하지만 모르고 살려구요ㅎ",
          "original_form": "궁금하지만 모르고 살려구요ㅎ",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012469.1.11",
          "form": "ㅎㅎ김치파우더 인기라 재밌더라고요",
          "original_form": "ㅎㅎ김치파우더 인기라 재밌더라고요",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012469.1.12",
          "form": "김치쨈? 아아 김치음료수!",
          "original_form": "김치쨈? 아아 김치음료수!",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012469.1.13",
          "form": "하...... 대체 무슨맛",
          "original_form": "하...... 대체 무슨맛",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012469.1.14",
          "form": "아앗 잼이라니",
          "original_form": "아앗 잼이라니",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012469.1.15",
          "form": "동남아 어디선 김치육수만도 판다던데",
          "original_form": "동남아 어디선 김치육수만도 판다던데",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012469.1.16",
          "form": "김치파우더는 과자나 소시지에 뿌려먹기도 하고",
          "original_form": "김치파우더는 과자나 소시지에 뿌려먹기도 하고",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012469.1.17",
          "form": "음.. 약간 열무김치 국물 그런 맛일까요?",
          "original_form": "음.. 약간 열무김치 국물 그런 맛일까요?",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012469.1.18",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012469.1.19",
          "form": "김치파우더는 잘 사용하면 맛은 괜찮을것 같아요~~",
          "original_form": "김치파우더는 잘 사용하면 맛은 괜찮을것 같아요~~",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012469.1.20",
          "form": "전 설렁탕에 깍두기국물 넣어먹는 사람처럼 그렇게 육수를 쓰나 했어요",
          "original_form": "전 설렁탕에 깍두기국물 넣어먹는 사람처럼 그렇게 육수를 쓰나 했어요",
          "speaker_id": "1",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012469.1.21",
          "form": "그쵸 아마존서 일본 시치미 누르고 조미료류 1위 했다니 신기",
          "original_form": "그쵸 아마존서 일본 시치미 누르고 조미료류 1위 했다니 신기",
          "speaker_id": "1",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100012469.1.22",
          "form": "그거 한국분이 연구해서 만드셨다는데 진짜 대단",
          "original_form": "그거 한국분이 연구해서 만드셨다는데 진짜 대단",
          "speaker_id": "2",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100012469.1.23",
          "form": "그러게요. 어떻게 그런 생각을 하셨는지",
          "original_form": "그러게요. 어떻게 그런 생각을 하셨는지",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012469.1.24",
          "form": "제대로 자리잡아서 일본누르고 차별성을 보여줬으면 좋겠어요 ㅋㅋ",
          "original_form": "제대로 자리잡아서 일본누르고 차별성을 보여줬으면 좋겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:24"
        }
      ]
