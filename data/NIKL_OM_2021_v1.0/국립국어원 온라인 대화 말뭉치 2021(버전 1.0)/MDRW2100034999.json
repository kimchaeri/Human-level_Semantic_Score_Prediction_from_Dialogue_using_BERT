      "utterance": [
        {
          "id": "MDRW2100034999.1.1",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100034999.1.2",
          "form": "요즘 추워졌던데",
          "original_form": "요즘 추워졌던데",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100034999.1.3",
          "form": "이불 바꾸셨어요?",
          "original_form": "이불 바꾸셨어요?",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100034999.1.4",
          "form": "비많이올때 이미",
          "original_form": "비많이올때 이미",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100034999.1.5",
          "form": "두꺼운거 꺼냈어요",
          "original_form": "두꺼운거 꺼냈어요",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100034999.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100034999.1.7",
          "form": "저도 이불 두꺼운거 꺼냈어요",
          "original_form": "저도 이불 두꺼운거 꺼냈어요",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100034999.1.8",
          "form": "원래 선풍기 회전으로 틀고잤는데",
          "original_form": "원래 선풍기 회전으로 틀고잤는데",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100034999.1.9",
          "form": "추워서",
          "original_form": "추워서",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100034999.1.10",
          "form": "이불 덮고 자요ㅋㅋㅋ",
          "original_form": "이불 덮고 자요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100034999.1.11",
          "form": "선풍기도 끄셔야겠네요",
          "original_form": "선풍기도 끄셔야겠네요",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034999.1.12",
          "form": "아ㅋㅋㅋㅌ 넹넹",
          "original_form": "아ㅋㅋㅋㅌ 넹넹",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034999.1.13",
          "form": "추워졌어요ㅜㅜ",
          "original_form": "추워졌어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034999.1.14",
          "form": "에어컨도 이제 슬슬 안녕할때가 된것같아요",
          "original_form": "에어컨도 이제 슬슬 안녕할때가 된것같아요",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034999.1.15",
          "form": "근데 또 이러다가 다시 더워질까봐",
          "original_form": "근데 또 이러다가 다시 더워질까봐",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034999.1.16",
          "form": "저는이제 목에수건두르고다녀요",
          "original_form": "저는이제 목에수건두르고다녀요",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034999.1.17",
          "form": "아직 청소는 못했어요ㅋㅋ",
          "original_form": "아직 청소는 못했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100034999.1.18",
          "form": "아침에 목이 좀 칼칼한것같더라구요ㅠ",
          "original_form": "아침에 목이 좀 칼칼한것같더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100034999.1.19",
          "form": "아 ㅋ오늘은 좀 덥더라구요",
          "original_form": "아 ㅋ오늘은 좀 덥더라구요",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100034999.1.20",
          "form": "해떠서그런지",
          "original_form": "해떠서그런지",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100034999.1.21",
          "form": "맞아요ㅋㅋㅋ 이럴때만 선풍기 잠깐 틀어요",
          "original_form": "맞아요ㅋㅋㅋ 이럴때만 선풍기 잠깐 틀어요",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100034999.1.22",
          "form": "저도 낮에 선풍기틀었어요",
          "original_form": "저도 낮에 선풍기틀었어요",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034999.1.23",
          "form": "다음주에 보니까",
          "original_form": "다음주에 보니까",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034999.1.24",
          "form": "계속 비온대요",
          "original_form": "계속 비온대요",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034999.1.25",
          "form": "에어컨청소 부지런히 하시네요",
          "original_form": "에어컨청소 부지런히 하시네요",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034999.1.26",
          "form": "빨래하실거 있으시면",
          "original_form": "빨래하실거 있으시면",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034999.1.27",
          "form": "안돼",
          "original_form": "안돼",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034999.1.28",
          "form": "내일 하세용",
          "original_form": "내일 하세용",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034999.1.29",
          "form": "빨래 알겠습니다ㅜㅜㅜ",
          "original_form": "빨래 알겠습니다ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034999.1.30",
          "form": "계속 비가 내내 주륵주륵이더라구요ㅋㅋㅋ",
          "original_form": "계속 비가 내내 주륵주륵이더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034999.1.31",
          "form": "아직안모였는데ㅜㅜㅜㅜ안돼",
          "original_form": "아직안모였는데ㅜㅜㅜㅜ안돼",
