      "utterance": [
        {
          "id": "MDRW2100012870.1.1",
          "form": "동물 좋아하시나요?",
          "original_form": "동물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 05:22"
        },
        {
          "id": "MDRW2100012870.1.2",
          "form": "보는거만 좋아해요 혹시 기르시나요?",
          "original_form": "보는거만 좋아해요 혹시 기르시나요?",
          "speaker_id": "2",
          "time": "20210825 05:23"
        },
        {
          "id": "MDRW2100012870.1.3",
          "form": "진돗개 한마리 기르고 있어요",
          "original_form": "진돗개 한마리 기르고 있어요",
          "speaker_id": "1",
          "time": "20210825 05:24"
        },
        {
          "id": "MDRW2100012870.1.4",
          "form": "오오 이름이 뭔가요?",
          "original_form": "오오 이름이 뭔가요?",
          "speaker_id": "2",
          "time": "20210825 05:24"
        },
        {
          "id": "MDRW2100012870.1.5",
          "form": "name3이에요",
          "original_form": "&name3&이에요",
          "speaker_id": "1",
          "time": "20210825 05:24"
        },
        {
          "id": "MDRW2100012870.1.6",
          "form": "진돗개 좋아하시나요?",
          "original_form": "진돗개 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 05:24"
        },
        {
          "id": "MDRW2100012870.1.7",
          "form": "진도개 최고죠!! name3이 이름 누가 지으셨나요 ㅎㅎ",
          "original_form": "진도개 최고죠!! &name3&이 이름 누가 지으셨나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 05:24"
        },
        {
          "id": "MDRW2100012870.1.8",
          "form": "아빠가 지어주셨어요",
          "original_form": "아빠가 지어주셨어요",
          "speaker_id": "1",
          "time": "20210825 05:25"
        },
        {
          "id": "MDRW2100012870.1.9",
          "form": "오 산책 많이 나가야하죠?",
          "original_form": "오 산책 많이 나가야하죠?",
          "speaker_id": "2",
          "time": "20210825 05:25"
        },
        {
          "id": "MDRW2100012870.1.10",
          "form": "진돗개 파워가!!",
          "original_form": "진돗개 파워가!!",
          "speaker_id": "2",
          "time": "20210825 05:25"
        },
        {
          "id": "MDRW2100012870.1.11",
          "form": "아무래도 에너지가 남아놀아 산채ㅣ량이 많더라구요",
          "original_form": "아무래도 에너지가 남아놀아 산채ㅣ량이 많더라구요",
          "speaker_id": "1",
          "time": "20210825 05:25"
        },
        {
          "id": "MDRW2100012870.1.12",
          "form": "남아돌아 산책량이 많더라구요",
          "original_form": "남아돌아 산책량이 많더라구요",
          "speaker_id": "1",
          "time": "20210825 05:26"
        },
        {
          "id": "MDRW2100012870.1.13",
          "form": "ㅋㅋㅋ댕댕이 키우려면 진짜 주인님이 튼튼해야하죠",
          "original_form": "ㅋㅋㅋ댕댕이 키우려면 진짜 주인님이 튼튼해야하죠",
          "speaker_id": "2",
          "time": "20210825 05:26"
        },
        {
          "id": "MDRW2100012870.1.14",
          "form": "맞아요 주인의 체력도 아주 중요해요. 강아지 말고 좋아하는 동물 있으신가요?",
          "original_form": "맞아요 주인의 체력도 아주 중요해요. 강아지 말고 좋아하는 동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 05:26"
        },
        {
          "id": "MDRW2100012870.1.15",
          "form": "파충류빼고 다 좋아하는 데 제가 게을러서 못키우겠더라구요",
          "original_form": "파충류빼고 다 좋아하는 데 제가 게을러서 못키우겠더라구요",
          "speaker_id": "2",
          "time": "20210825 05:27"
        },
        {
          "id": "MDRW2100012870.1.16",
          "form": "파충류요? 도마뱀이나 뱀종류 좋아하시나 봐요",
          "original_form": "파충류요? 도마뱀이나 뱀종류 좋아하시나 봐요",
          "speaker_id": "1",
          "time": "20210825 05:28"
        },
        {
          "id": "MDRW2100012870.1.17",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 05:28"
        },
        {
          "id": "MDRW2100012870.1.18",
          "form": "ㅎㅎㅎ주인님되는거 아무나 못하죠..name3이는 몇살인가요?",
          "original_form": "ㅎㅎㅎ주인님되는거 아무나 못하죠..&name3&이는 몇살인가요?",
          "speaker_id": "2",
          "time": "20210825 05:28"
        },
        {
          "id": "MDRW2100012870.1.19",
          "form": "빼고 좋아하신다고 하셨구나",
          "original_form": "빼고 좋아하신다고 하셨구나",
          "speaker_id": "1",
          "time": "20210825 05:28"
        },
        {
          "id": "MDRW2100012870.1.20",
          "form": "드래곤류는 차가워서요 ㅎㅎ",
          "original_form": "드래곤류는 차가워서요 ㅎㅎ",
