      "utterance": [
        {
          "id": "MDRW2100038117.1.1",
          "form": "너무 예쁘고 규ㅣ여워요 ㅎ.ㅎ",
          "original_form": "너무 예쁘고 규ㅣ여워요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038117.1.2",
          "form": "우와 이름부터 남다른데요?!",
          "original_form": "우와 이름부터 남다른데요?!",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100038117.1.3",
          "form": "왠지 짭조름할 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "왠지 짭조름할 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100038117.1.4",
          "form": "쿠키들 너무 귀여워서 먹을 수 없을 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "쿠키들 너무 귀여워서 먹을 수 없을 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100038117.1.5",
          "form": "그건 그래요 다들 넘 귀엽게 생겨거",
          "original_form": "그건 그래요 다들 넘 귀엽게 생겨거",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100038117.1.6",
          "form": "먹으면 좀 많이 미안할 것 같더라거요 ㅎ.ㅎ !!",
          "original_form": "먹으면 좀 많이 미안할 것 같더라거요 ㅎ.ㅎ !!",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100038117.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100038117.1.8",
          "form": "뭔가 귀여운 대화예요 지금!!!",
          "original_form": "뭔가 귀여운 대화예요 지금!!!",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100038117.1.9",
          "form": "쿠킹덤 하니까 생각난건데",
          "original_form": "쿠킹덤 하니까 생각난건데",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100038117.1.10",
          "form": "예전에 스머프 빌리지도 했었어요!!!!",
          "original_form": "예전에 스머프 빌리지도 했었어요!!!!",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100038117.1.11",
          "form": "허엇! 그건 무슨 게임인가요? 쿠킹덤이랑 비슷해요?!",
          "original_form": "허엇! 그건 무슨 게임인가요? 쿠킹덤이랑 비슷해요?!",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100038117.1.12",
          "form": "네네 아마 비슷할거예요!",
          "original_form": "네네 아마 비슷할거예요!",
          "speaker_id": "2",
          "time": "20210902 08:05"
        },
        {
          "id": "MDRW2100038117.1.13",
          "form": "마을 꾸미는건데",
          "original_form": "마을 꾸미는건데",
          "speaker_id": "2",
          "time": "20210902 08:05"
        },
        {
          "id": "MDRW2100038117.1.14",
          "form": "작물 키우기도 하고... 건물 짓기도 하고...",
          "original_form": "작물 키우기도 하고... 건물 짓기도 하고...",
          "speaker_id": "2",
          "time": "20210902 08:05"
        },
        {
          "id": "MDRW2100038117.1.15",
          "form": "나름 스토리라인도 있어요 ㅎㅎㅎ",
          "original_form": "나름 스토리라인도 있어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:05"
        },
        {
          "id": "MDRW2100038117.1.16",
          "form": "헐 저 그런 게임 좋아하는데 당장 해봐야겠어요!",
          "original_form": "헐 저 그런 게임 좋아하는데 당장 해봐야겠어요!",
          "speaker_id": "1",
          "time": "20210902 08:17"
        },
        {
          "id": "MDRW2100038117.1.17",
          "form": "꿈의 집 아시나요? 저 그것도 완전 열심히 했고든요",
          "original_form": "꿈의 집 아시나요? 저 그것도 완전 열심히 했고든요",
          "speaker_id": "1",
          "time": "20210902 08:17"
        },
        {
          "id": "MDRW2100038117.1.18",
          "form": "어!! 저도 그거 조금 했었어요!!",
          "original_form": "어!! 저도 그거 조금 했었어요!!",
          "speaker_id": "2",
          "time": "20210902 08:43"
        },
        {
          "id": "MDRW2100038117.1.19",
          "form": "갤탭으로 했었는데",
          "original_form": "갤탭으로 했었는데",
          "speaker_id": "2",
          "time": "20210902 08:44"
        },
        {
          "id": "MDRW2100038117.1.20",
          "form": "제가 좋아하는 게임은 너무 가끔씩 나오더라구요ㅠ",
          "original_form": "제가 좋아하는 게임은 너무 가끔씩 나오더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210902 08:44"
        },
        {
          "id": "MDRW2100038117.1.21",
          "form": "앗 그 탈출하는 게임 맞죠!! 꿈의집 중간중간에 나오는!",
          "original_form": "앗 그 탈출하는 게임 맞죠!! 꿈의집 중간중간에 나오는!",
          "speaker_id": "1",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100038117.1.22",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100038117.1.23",
          "form": "그 탈출하는 게임이 메인인 줄 알았는데",
          "original_form": "그 탈출하는 게임이 메인인 줄 알았는데",
          "speaker_id": "2",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100038117.1.24",
          "form": "캔디크러시사가 같은 게임이 더 많더라구요ㅠㅠㅠ",
          "original_form": "캔디크러시사가 같은 게임이 더 많더라구요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100038117.1.25",
          "form": "맞어요 그거 예전에 탈출 게임 없을 때도",
          "original_form": "맞어요 그거 예전에 탈출 게임 없을 때도",
          "speaker_id": "1",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100038117.1.26",
          "form": "탈출 게임인 것처럼 광고하더라고요",
          "original_form": "탈출 게임인 것처럼 광고하더라고요",
          "speaker_id": "1",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100038117.1.27",
          "form": "뻥광고",
