      "utterance": [
        {
          "id": "MDRW2100014085.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014085.1.2",
          "form": "최근에 여행가신곳이 어디예요?",
          "original_form": "최근에 여행가신곳이 어디예요?",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014085.1.3",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014085.1.4",
          "form": "저는 포항이요",
          "original_form": "저는 포항이요",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014085.1.5",
          "form": "해수욕장 갔다왓어요",
          "original_form": "해수욕장 갔다왓어요",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014085.1.6",
          "form": "오 포항이요?",
          "original_form": "오 포항이요?",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014085.1.7",
          "form": "수영도 하셨나요?? ㅎㅎ",
          "original_form": "수영도 하셨나요?? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014085.1.8",
          "form": "그럼요  제가 물을 좋아해요",
          "original_form": "그럼요  제가 물을 좋아해요",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014085.1.9",
          "form": "캠핑 2박햇죠",
          "original_form": "캠핑 2박햇죠",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014085.1.10",
          "form": "오 캠핑도 다니시나봐요!",
          "original_form": "오 캠핑도 다니시나봐요!",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014085.1.11",
          "form": "저는 캠린이예요",
          "original_form": "저는 캠린이예요",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014085.1.12",
          "form": "가끔 쉬는 날가쥬",
          "original_form": "가끔 쉬는 날가쥬",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014085.1.13",
          "form": "해외여행 좋아해요?",
          "original_form": "해외여행 좋아해요?",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014085.1.14",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014085.1.15",
          "form": "그래두1년에 한번은 나갔는데",
          "original_form": "그래두1년에 한번은 나갔는데",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014085.1.16",
          "form": "이제는언제쯤가볼지",
          "original_form": "이제는언제쯤가볼지",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014085.1.17",
          "form": "마지막이 어디였어요?",
          "original_form": "마지막이 어디였어요?",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014085.1.18",
          "form": "체코 프라하요!",
          "original_form": "체코 프라하요!",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014085.1.19",
          "form": "일주일여행갔는데 일정 빡빡하게 세우지않구",
          "original_form": "일주일여행갔는데 일정 빡빡하게 세우지않구",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014085.1.20",
          "form": "돌아다녀서좋았어요!",
          "original_form": "돌아다녀서좋았어요!",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014085.1.21",
          "form": "거긴. 명소가 어딘가요",
          "original_form": "거긴. 명소가 어딘가요",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014085.1.22",
          "form": "체코는 뭐가 유명햇어요?",
          "original_form": "체코는 뭐가 유명햇어요?",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014085.1.23",
          "form": "카를교라고 제일유명한 다리가있어요",
          "original_form": "카를교라고 제일유명한 다리가있어요",
          "speaker_id": "1",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014085.1.24",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014085.1.25",
          "form": "멋찐 곳이네요",
          "original_form": "멋찐 곳이네요",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014085.1.26",
          "form": "마지막여행 어디였어요?",
          "original_form": "마지막여행 어디였어요?",
          "speaker_id": "1",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014085.1.27",
          "form": "동남아 쪽이요",
          "original_form": "동남아 쪽이요",
          "speaker_id": "2",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014085.1.28",
          "form": "다낭 불꽃축제 보러",
          "original_form": "다낭 불꽃축제 보러",
          "speaker_id": "2",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014085.1.29",
          "form": "갔어요",
          "original_form": "갔어요",
          "speaker_id": "2",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014085.1.30",
          "form": "다낭!",
          "original_form": "다낭!",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014085.1.31",
          "form": "저도 다낭 간적있는데 불꽃 축제는 못봤어요",
          "original_form": "저도 다낭 간적있는데 불꽃 축제는 못봤어요",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014085.1.32",
          "form": "세계불꽃 대회같은 거라서 엄청 크게 해요",
          "original_form": "세계불꽃 대회같은 거라서 엄청 크게 해요",
          "speaker_id": "2",
          "time": "20210825 18:11"
        }
      ]
    }
