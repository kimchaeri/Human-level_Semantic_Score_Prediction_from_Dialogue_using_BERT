      "utterance": [
        {
          "id": "MDRW2100027131.1.1",
          "form": "둘다 맛나요",
          "original_form": "둘다 맛나요",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100027131.1.2",
          "form": "고를수가 없네요",
          "original_form": "고를수가 없네요",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100027131.1.3",
          "form": "단무지도 같이먹음좋아요",
          "original_form": "단무지도 같이먹음좋아요",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100027131.1.4",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027131.1.5",
          "form": "깍두기도 같이 드세요",
          "original_form": "깍두기도 같이 드세요",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027131.1.6",
          "form": "짬뽕먹고싶네요",
          "original_form": "짬뽕먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027131.1.7",
          "form": "홍콩반점도 생겼어요",
          "original_form": "홍콩반점도 생겼어요",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027131.1.8",
          "form": "짬짜면좋지요",
          "original_form": "짬짜면좋지요",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027131.1.9",
          "form": "거기 단무지가 얇아요",
          "original_form": "거기 단무지가 얇아요",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027131.1.10",
          "form": "거긴 탕수육이 맛있지요",
          "original_form": "거긴 탕수육이 맛있지요",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027131.1.11",
          "form": "단무지 인색하던데요",
          "original_form": "단무지 인색하던데요",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027131.1.12",
          "form": "무제한이에요",
          "original_form": "무제한이에요",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027131.1.13",
          "form": "인색하지않아요",
          "original_form": "인색하지않아요",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027131.1.14",
          "form": "메뉴가 많지않아요",
          "original_form": "메뉴가 많지않아요",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027131.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027131.1.16",
          "form": "중국냉면 맛있었는데",
          "original_form": "중국냉면 맛있었는데",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027131.1.17",
          "form": "두둔하시는거보니 단골이신가봐요",
          "original_form": "두둔하시는거보니 단골이신가봐요",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027131.1.18",
          "form": "올해없어졌더라구요",
          "original_form": "올해없어졌더라구요",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027131.1.19",
          "form": "네 단골입니다",
          "original_form": "네 단골입니다",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027131.1.20",
          "form": "쟁반짜장 매운맛으로 드셔보세요",
          "original_form": "쟁반짜장 매운맛으로 드셔보세요",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027131.1.21",
          "form": "진짜맛나요",
          "original_form": "진짜맛나요",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027131.1.22",
          "form": "쟁반짜장은 2인분인가요",
          "original_form": "쟁반짜장은 2인분인가요",
          "speaker_id": "1",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027131.1.23",
          "form": "그냥시키면 보통맛인데 그건별로에요",
          "original_form": "그냥시키면 보통맛인데 그건별로에요",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027131.1.24",
          "form": "네2인분기준이요",
          "original_form": "네2인분기준이요",
