        {
          "id": "MDRW2100002084.1.4",
          "form": "난 알바중 ㅠ",
          "original_form": "난 알바중 ㅠ",
          "speaker_id": "2",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100002084.1.5",
          "form": "고생이많아요",
          "original_form": "고생이많아요",
          "speaker_id": "1",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100002084.1.6",
          "form": "뭐하고계세요?",
          "original_form": "뭐하고계세요?",
          "speaker_id": "2",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100002084.1.7",
          "form": "세나게보는중",
          "original_form": "세나게보는중",
          "speaker_id": "1",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100002084.1.8",
          "form": "세상에 나쁜개는 없다?",
          "original_form": "세상에 나쁜개는 없다?",
          "speaker_id": "2",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100002084.1.9",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100002084.1.10",
          "form": "리트리버나와요 귀욤",
          "original_form": "리트리버나와요 귀욤",
          "speaker_id": "1",
          "time": "20210821 11:38"
        },
        {
          "id": "MDRW2100002084.1.11",
          "form": "오늘은 쉬는날인가 보네요 ㅋㅋㅋ",
          "original_form": "오늘은 쉬는날인가 보네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:38"
        },
        {
          "id": "MDRW2100002084.1.12",
          "form": "토요일이잔아요",
          "original_form": "토요일이잔아요",
          "speaker_id": "1",
          "time": "20210821 11:44"
        },
        {
          "id": "MDRW2100002084.1.13",
          "form": "토요일날도 일하는 곳 많던데 무슨일 하세요?",
          "original_form": "토요일날도 일하는 곳 많던데 무슨일 하세요?",
