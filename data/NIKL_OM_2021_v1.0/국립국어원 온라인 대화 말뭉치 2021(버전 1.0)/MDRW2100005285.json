          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.3",
          "form": "좋습니다",
          "original_form": "좋습니다",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.4",
          "form": "저도 없지만요",
          "original_form": "저도 없지만요",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.5",
          "form": "저는 고양이가 좋아요",
          "original_form": "저는 고양이가 좋아요",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.6",
          "form": "강아지도요",
          "original_form": "강아지도요",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.7",
          "form": "뱀도 좋습니다",
          "original_form": "뱀도 좋습니다",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.8",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.9",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.10",
          "form": "여우도좋아요",
          "original_form": "여우도좋아요",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.11",
          "form": "저도 뱀좋아해요",
          "original_form": "저도 뱀좋아해요",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.12",
          "form": "여우는...",
          "original_form": "여우는...",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.13",
          "form": "이유알거같음",
          "original_form": "이유알거같음",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.14",
          "form": "파충류 사랑해요",
          "original_form": "파충류 사랑해요",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005285.1.15",
          "form": "이쁨",
          "original_form": "이쁨",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.16",
          "form": "맞아요 그이유",
          "original_form": "맞아요 그이유",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.17",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.18",
          "form": "사랑해",
          "original_form": "사랑해",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.19",
          "form": "아니 혼자산다는게 왜 나쁜말이지",
          "original_form": "아니 혼자산다는게 왜 나쁜말이지",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.20",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.22",
          "form": "뱀 키우고싶어요",
          "original_form": "뱀 키우고싶어요",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.23",
          "form": "그렇개 안보내지는것들잇음",
          "original_form": "그렇개 안보내지는것들잇음",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.24",
          "form": "ㅈ ㅓ ㅈ 다",
          "original_form": "ㅈ ㅓ ㅈ 다",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.25",
          "form": "핸들링 해서",
          "original_form": "핸들링 해서",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.26",
          "form": "도 못보내요",
          "original_form": "도 못보내요",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.27",
          "form": "보스처럼 목에두르고",
          "original_form": "보스처럼 목에두르고",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.28",
          "form": "뱀 매끈매끈",
          "original_form": "뱀 매끈매끈",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.29",
          "form": "친구맞이하고싶어",
          "original_form": "친구맞이하고싶어",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.30",
          "form": "너무 예뻐뇨",
          "original_form": "너무 예뻐뇨",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.31",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005285.1.32",
          "form": "간지 철철",
          "original_form": "간지 철철",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.33",
          "form": "반지도 여러개 껴주새요",
          "original_form": "반지도 여러개 껴주새요",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.34",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.35",
          "form": "다음 캐로 어떠새요 아차차 쉿",
          "original_form": "다음 캐로 어떠새요 아차차 쉿",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.36",
          "form": "반려동물 이름을 짓는다면",
          "original_form": "반려동물 이름을 짓는다면",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.37",
          "form": "근데 뱀 먹이를 못 줄것같음",
          "original_form": "근데 뱀 먹이를 못 줄것같음",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.38",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.39",
          "form": "어떤이르믕ㄹ",
          "original_form": "어떤이르믕ㄹ",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.40",
          "form": "주실건가요",
          "original_form": "주실건가요",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.41",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.43",
          "form": "먹히지만 마새요",
          "original_form": "먹히지만 마새요",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.44",
          "form": "뱀이 슬슬 몸 감는거",
          "original_form": "뱀이 슬슬 몸 감는거",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.45",
          "form": "크기 쟈는거람서요",
          "original_form": "크기 쟈는거람서요",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005285.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005285.1.47",
          "form": "귀엽지않나요",
          "original_form": "귀엽지않나요",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005285.1.48",
          "form": "사실 이름정하는게 너무어려워요",
          "original_form": "사실 이름정하는게 너무어려워요",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005285.1.49",
          "form": "반려동물 비슷한 캐릭터들도 이름을 못지어주는데",
          "original_form": "반려동물 비슷한 캐릭터들도 이름을 못지어주는데",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005285.1.50",
          "form": "뱀키우면... 천리 라고 지을래요",
          "original_form": "뱀키우면... 천리 라고 지을래요",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005285.1.51",
          "form": "발없는 뱀이 천리간다",
          "original_form": "발없는 뱀이 천리간다",
          "speaker_id": "1",
          "time": "20210821 21:41"
