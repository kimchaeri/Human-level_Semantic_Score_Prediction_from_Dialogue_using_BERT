      "utterance": [
        {
          "id": "MDRW2100022926.1.1",
          "form": "일식 모밀이요 한식 모밀이요? 어떤거요?",
          "original_form": "일식 모밀이요 한식 모밀이요? 어떤거요?",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100022926.1.2",
          "form": "둘이 많이 다른가요?",
          "original_form": "둘이 많이 다른가요?",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100022926.1.3",
          "form": "전 그냥",
          "original_form": "전 그냥",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100022926.1.4",
          "form": "모밀이면 다 좋아요",
          "original_form": "모밀이면 다 좋아요",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100022926.1.5",
          "form": "양념마시 살짝 달라요",
          "original_form": "양념마시 살짝 달라요",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100022926.1.6",
          "form": "그리고 한국메밀국수는 파 무우 양념장을 듬북 넣구 김도 듬뿍 넣는 느낌?",
          "original_form": "그리고 한국메밀국수는 파 무우 양념장을 듬북 넣구 김도 듬뿍 넣는 느낌?",
          "speaker_id": "2",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100022926.1.7",
          "form": "아하 전 한국만 먹어봤어요",
          "original_form": "아하 전 한국만 먹어봤어요",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100022926.1.8",
          "form": "메밀 고수신가요",
          "original_form": "메밀 고수신가요",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100022926.1.9",
          "form": "아뇨 ㅋㅋㅋ 그냥 일본 요리만화에",
          "original_form": "아뇨 ㅋㅋㅋ 그냥 일본 요리만화에",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100022926.1.10",
          "form": "소바가 자주 나오니까요",
          "original_form": "소바가 자주 나오니까요",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100022926.1.11",
          "form": "ㅋㅋㅋㅋㅋ아하 일본식이",
          "original_form": "ㅋㅋㅋㅋㅋ아하 일본식이",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100022926.1.12",
          "form": "약간 담가먹는 느낌이죠?",
          "original_form": "약간 담가먹는 느낌이죠?",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100022926.1.13",
          "form": "한국보다 좀",
          "original_form": "한국보다 좀",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100022926.1.14",
          "form": "덜 찍어먹는 느낌 네 네",
          "original_form": "덜 찍어먹는 느낌 네 네",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100022926.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ 아 저 그렁 거 보고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아 저 그렁 거 보고",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100022926.1.16",
          "form": "메밀에 약간 환상생겼었어요..",
          "original_form": "메밀에 약간 환상생겼었어요..",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100022926.1.17",
          "form": "왜여 ㅋㅋㅋㅋ",
          "original_form": "왜여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100022926.1.18",
          "form": "뭔가 담궈먹는데 맛있어보여서요 ㅎ",
          "original_form": "뭔가 담궈먹는데 맛있어보여서요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100022926.1.19",
          "form": "그죠 ㅋㅋ그리고 다들 여름에 만화에서그걸 먹구",
          "original_form": "그죠 ㅋㅋ그리고 다들 여름에 만화에서그걸 먹구",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100022926.1.20",
          "form": "ㅋㅋㅋㅋㅋ 네 ㅠ 그래서 먹고싶었어요",
          "original_form": "ㅋㅋㅋㅋㅋ 네 ㅠ 그래서 먹고싶었어요",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100022926.1.21",
          "form": "근데 너무 국수와 장만 딱 있는게 일식이라 ㅋㅋ",
          "original_form": "근데 너무 국수와 장만 딱 있는게 일식이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100022926.1.22",
          "form": "심심한 느낌은 있어요",
          "original_form": "심심한 느낌은 있어요",
          "speaker_id": "2",
          "time": "20210901 17:10"
        }
      ]
    }
