      "utterance": [
        {
          "id": "MDRW2100045149.1.1",
          "form": "참지못하고 샤인을거의 다먹어버렸다\r\n야금야금",
          "original_form": "참지못하고 샤인을거의 다먹어버렸다\r\n야금야금",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045149.1.2",
          "form": "오 난 배고픈데 참았당ㅜㅜ",
          "original_form": "오 난 배고픈데 참았당ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045149.1.3",
          "form": "잘했다!",
          "original_form": "잘했다!",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045149.1.4",
          "form": "굿 칭찬해",
          "original_form": "굿 칭찬해",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045149.1.5",
          "form": "샤인 나도 먹어보고싶네",
          "original_form": "샤인 나도 먹어보고싶네",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045149.1.6",
          "form": "니가 계속 말하니 궁금하다",
          "original_form": "니가 계속 말하니 궁금하다",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045149.1.7",
          "form": "어떤맛일지",
          "original_form": "어떤맛일지",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045149.1.8",
          "form": "ㅎㅎ나도 잘 못 설명해서ㅎ",
          "original_form": "ㅎㅎ나도 잘 못 설명해서ㅎ",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045149.1.9",
          "form": "아녀아녀 ㅋㅋ 직접 먹어봐야알듯",
          "original_form": "아녀아녀 ㅋㅋ 직접 먹어봐야알듯",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045149.1.10",
          "form": "나 갑자기양념치킨땡기네",
          "original_form": "나 갑자기양념치킨땡기네",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045149.1.11",
          "form": "시장통닭",
          "original_form": "시장통닭",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045149.1.12",
          "form": "엇 양념치킨은 맛있는곳모르겠다",
          "original_form": "엇 양념치킨은 맛있는곳모르겠다",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045149.1.13",
          "form": "난 너가 매간알려줘서 윙봉매간 아주 잘먹고있다ㅋ",
          "original_form": "난 너가 매간알려줘서 윙봉매간 아주 잘먹고있다ㅋ",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045149.1.14",
          "form": "별미통닭 양념치킨 내 취향",
          "original_form": "별미통닭 양념치킨 내 취향",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045149.1.15",
          "form": "매간?",
          "original_form": "매간?",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045149.1.16",
          "form": "뭐였지?",
          "original_form": "뭐였지?",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045149.1.17",
          "form": "거기 이번에 창원가면 먹어볼께",
          "original_form": "거기 이번에 창원가면 먹어볼께",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045149.1.18",
          "form": "매운간장",
          "original_form": "매운간장",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045149.1.19",
          "form": "아 ㅋㅋ 호식이 ㅋㅋ",
          "original_form": "아 ㅋㅋ 호식이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045149.1.20",
          "form": "호식이 요즘 윙봉 안팔더라",
          "original_form": "호식이 요즘 윙봉 안팔더라",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045149.1.21",
          "form": "매번 매진이라고 되어있어서",
          "original_form": "매번 매진이라고 되어있어서",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045149.1.22",
          "form": "여긴 말했다싶이 체인이 많이없어\r\n근데 호식인 3군데나 있어ㅋ",
          "original_form": "여긴 말했다싶이 체인이 많이없어\r\n근데 호식인 3군데나 있어ㅋ",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045149.1.23",
          "form": "걍. 일반 닭 시켜ㅜㅜ",
          "original_form": "걍. 일반 닭 시켜ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045149.1.24",
          "form": "아 맞다 그랬지 ㅋㅋ",
          "original_form": "아 맞다 그랬지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045149.1.25",
          "form": "그지점안팔렸나 점주가 닫았나본데",
          "original_form": "그지점안팔렸나 점주가 닫았나본데",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045149.1.26",
          "form": "어쩌다 그렇게 됐을까",
          "original_form": "어쩌다 그렇게 됐을까",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045149.1.27",
          "form": "윙봉?",
          "original_form": "윙봉?",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045149.1.28",
          "form": "사람들이 윙봉 매력을 몰라서",
          "original_form": "사람들이 윙봉 매력을 몰라서",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045149.1.29",
          "form": "그런건가ㅜㅜ",
          "original_form": "그런건가ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045149.1.30",
          "form": "아니 이렇게 맛있는데 안타깝구만",
          "original_form": "아니 이렇게 맛있는데 안타깝구만",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045149.1.31",
          "form": "구미 와서 호식이 먹어보고\r\n창원가서 찾아볼땟 없었는데ㅋ",
          "original_form": "구미 와서 호식이 먹어보고\r\n창원가서 찾아볼땟 없었는데ㅋ",
          "speaker_id": "2",
          "time": "20210908 01:07"
        }
      ]
    }
