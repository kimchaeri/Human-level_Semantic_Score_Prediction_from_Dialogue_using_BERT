      "utterance": [
        {
          "id": "MDRW2100004653.1.1",
          "form": "편의점 알바 퇴근햇는데 좀이따 추가업무 또감ㅠ",
          "original_form": "편의점 알바 퇴근햇는데 좀이따 추가업무 또감ㅠ",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004653.1.2",
          "form": "고된인생",
          "original_form": "고된인생",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004653.1.3",
          "form": "와 힘들겠다",
          "original_form": "와 힘들겠다",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004653.1.4",
          "form": "몇시간일해",
          "original_form": "몇시간일해",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004653.1.5",
          "form": "보통 6시간 안팍? 근데 다른알바 병원간다고 오늘 7시에 재출근",
          "original_form": "보통 6시간 안팍? 근데 다른알바 병원간다고 오늘 7시에 재출근",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004653.1.6",
          "form": "한다음 12시?까지볼듯",
          "original_form": "한다음 12시?까지볼듯",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004653.1.7",
          "form": "으 개힘들겠다 잠은언제잔담",
          "original_form": "으 개힘들겠다 잠은언제잔담",
          "speaker_id": "2",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004653.1.8",
          "form": "요즘 쪽잠 자는중",
          "original_form": "요즘 쪽잠 자는중",
          "speaker_id": "1",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004653.1.9",
          "form": "너는 알바할 생각없니?",
          "original_form": "너는 알바할 생각없니?",
          "speaker_id": "1",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004653.1.10",
          "form": "편의점 알바 진상만없으면 일 자체는 갠찬은데",
          "original_form": "편의점 알바 진상만없으면 일 자체는 갠찬은데",
          "speaker_id": "1",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004653.1.11",
          "form": "알바... 하면좋긴한데 안해도되면 굳이하지말잔생각",
          "original_form": "알바... 하면좋긴한데 안해도되면 굳이하지말잔생각",
          "speaker_id": "2",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004653.1.12",
          "form": "알바안해도 시간 개허투로쓰는데 알바하면얼마나그럴까",
          "original_form": "알바안해도 시간 개허투로쓰는데 알바하면얼마나그럴까",
          "speaker_id": "2",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004653.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004653.1.14",
          "form": "아정말",
          "original_form": "아정말",
          "speaker_id": "1",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004653.1.15",
          "form": "논리적이다",
          "original_form": "논리적이다",
          "speaker_id": "1",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004653.1.16",
          "form": "근데이미허투로쓸빠에",
          "original_form": "근데이미허투로쓸빠에",
          "speaker_id": "1",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004653.1.17",
          "form": "돈이라도벌면",
          "original_form": "돈이라도벌면",
          "speaker_id": "1",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004653.1.18",
          "form": "성격따라일듯?",
          "original_form": "성격따라일듯?",
          "speaker_id": "1",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004653.1.19",
          "form": "아니야 그나마하던것도안하게되잖아",
          "original_form": "아니야 그나마하던것도안하게되잖아",
          "speaker_id": "2",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004653.1.20",
          "form": "나아는",
          "original_form": "나아는",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004653.1.21",
          "form": "누워잇는시간이많앗어서",
          "original_form": "누워잇는시간이많앗어서",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004653.1.22",
          "form": "알바처에서 아 이거 할 시간에 이거하고싶다 욕구가 땡기는듯",
          "original_form": "알바처에서 아 이거 할 시간에 이거하고싶다 욕구가 땡기는듯",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004653.1.23",
          "form": "실현하는건적지만",
          "original_form": "실현하는건적지만",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004653.1.24",
          "form": "와 그게되네",
          "original_form": "와 그게되네",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004653.1.25",
          "form": "쩝",
          "original_form": "쩝",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004653.1.26",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004653.1.27",
          "form": "돈쌓이면선택지가많겟지",
          "original_form": "돈쌓이면선택지가많겟지",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004653.1.28",
          "form": "난거지라 안되긴하는데",
          "original_form": "난거지라 안되긴하는데",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004653.1.29",
          "form": "내친구는 뭐할때 돈걱정안하더라",
          "original_form": "내친구는 뭐할때 돈걱정안하더라",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004653.1.30",
          "form": "ㅋ돈쌓이면당연히 선택지가많지",
          "original_form": "ㅋ돈쌓이면당연히 선택지가많지",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004653.1.31",
          "form": "그건 ㄹㅇ부럽네",
          "original_form": "그건 ㄹㅇ부럽네",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004653.1.32",
          "form": "일안해도되고",
          "original_form": "일안해도되고",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004653.1.33",
          "form": "나도학교인턴정도는 하고싶은데",
          "original_form": "나도학교인턴정도는 하고싶은데",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004653.1.34",
          "form": "모르겠다",
          "original_form": "모르겠다",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004653.1.35",
          "form": "리얼",
          "original_form": "리얼",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004653.1.36",
          "form": "학교알바들도잉는데",
          "original_form": "학교알바들도잉는데",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004653.1.37",
          "form": "요즘은그닥안구하는듯",
          "original_form": "요즘은그닥안구하는듯",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004653.1.38",
          "form": "학교알바는 매번구할텐데",
          "original_form": "학교알바는 매번구할텐데",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004653.1.39",
          "form": "그것도국장임",
          "original_form": "그것도국장임",
          "speaker_id": "2",
          "time": "20210821 18:39"
        }
      ]
    }
