          "form": "있으면",
          "original_form": "있으면",
          "speaker_id": "1",
          "time": "20210901 15:08"
        },
        {
          "id": "MDRW2100047157.1.19",
          "form": "그것도괜찮겠다",
          "original_form": "그것도괜찮겠다",
          "speaker_id": "1",
          "time": "20210901 15:08"
        },
        {
          "id": "MDRW2100047157.1.20",
