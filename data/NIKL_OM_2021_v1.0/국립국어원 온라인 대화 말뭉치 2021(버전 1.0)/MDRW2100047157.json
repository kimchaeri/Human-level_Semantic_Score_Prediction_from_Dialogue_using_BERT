          "speaker_id": "1",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100047157.1.10",
          "form": "목소리끄는기능도있나",
          "original_form": "목소리끄는기능도있나",
          "speaker_id": "1",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100047157.1.11",
          "form": "아 있을 걸",
          "original_form": "아 있을 걸",
          "speaker_id": "2",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100047157.1.12",
          "form": "시간만 알려주게",
          "original_form": "시간만 알려주게",
          "speaker_id": "2",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100047157.1.13",
          "form": "근데막상없으면",
          "original_form": "근데막상없으면",
          "speaker_id": "1",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100047157.1.14",
          "form": "지루할까보ㅓ",
          "original_form": "지루할까보ㅓ",
