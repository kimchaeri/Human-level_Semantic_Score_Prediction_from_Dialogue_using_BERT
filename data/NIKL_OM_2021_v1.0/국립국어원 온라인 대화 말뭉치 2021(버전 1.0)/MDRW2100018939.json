      "utterance": [
        {
          "id": "MDRW2100018939.1.1",
          "form": "네이버 멤버십데이 기다리고 있어요",
          "original_form": "네이버 멤버십데이 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018939.1.2",
          "form": "텀블러 스포츠브라 우산 사려고 대기중이에요",
          "original_form": "텀블러 스포츠브라 우산 사려고 대기중이에요",
          "speaker_id": "2",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018939.1.3",
          "form": "아 저도 네이버 멤버십 기다리는 중이에요",
          "original_form": "아 저도 네이버 멤버십 기다리는 중이에요",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018939.1.4",
          "form": "저는 먹을 거 좀 사려구요",
          "original_form": "저는 먹을 거 좀 사려구요",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018939.1.5",
          "form": "텀블러는 어떤거 보세요?",
          "original_form": "텀블러는 어떤거 보세요?",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018939.1.6",
          "form": "클린켄틴꺼 쓰고 있는데 쓰고 있는게 코팅이 너무 벗겨져서 새로 사는 거거든요",
          "original_form": "클린켄틴꺼 쓰고 있는데 쓰고 있는게 코팅이 너무 벗겨져서 새로 사는 거거든요",
          "speaker_id": "2",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018939.1.7",
          "form": "근데 이게 편해서 같은거 색 코팅 안된거로 다시 사려구요",
          "original_form": "근데 이게 편해서 같은거 색 코팅 안된거로 다시 사려구요",
          "speaker_id": "2",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018939.1.8",
          "form": "그런데 최근 나오는건 스트로우 리드가 기본이라 마음에 안드네요 빨대 구멍 없어도 되는데..",
          "original_form": "그런데 최근 나오는건 스트로우 리드가 기본이라 마음에 안드네요 빨대 구멍 없어도 되는데..",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018939.1.9",
          "form": "스트로우 리드라는 것도 있군요",
          "original_form": "스트로우 리드라는 것도 있군요",
          "speaker_id": "1",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100018939.1.10",
          "form": "첨듣는 거네요ㅋㅋㅋ",
          "original_form": "첨듣는 거네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100018939.1.11",
          "form": "저는 고양이 캔도 좀 사려구요",
          "original_form": "저는 고양이 캔도 좀 사려구요",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018939.1.12",
          "form": "고양이 좋겠네요ㅋㅋ",
          "original_form": "고양이 좋겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018939.1.13",
          "form": "아직 멤버십데이 일자가 공지 안떴나보더라구요",
          "original_form": "아직 멤버십데이 일자가 공지 안떴나보더라구요",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018939.1.14",
          "form": "월요일에 하겠죠..?ㅜㅜ",
          "original_form": "월요일에 하겠죠..?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018939.1.15",
          "form": "그쵸 그거 기간 예고가 없어서 좀 불편해요",
          "original_form": "그쵸 그거 기간 예고가 없어서 좀 불편해요",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018939.1.16",
          "form": "월요일에는 꼭 하기를 빌어봅시다",
          "original_form": "월요일에는 꼭 하기를 빌어봅시다",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018939.1.17",
          "form": "우산은 어떤거 고르셨어요?",
          "original_form": "우산은 어떤거 고르셨어요?",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018939.1.18",
          "form": "자동우산을 살껀데 이제 골라야해요..ㅎ",
          "original_form": "자동우산을 살껀데 이제 골라야해요..ㅎ",
          "speaker_id": "2",
          "time": "20210827 14:17"
        },
        {
          "id": "MDRW2100018939.1.19",
          "form": "먹을꺼는 뭐 사세요?",
          "original_form": "먹을꺼는 뭐 사세요?",
          "speaker_id": "2",
          "time": "20210827 14:18"
        },
        {
          "id": "MDRW2100018939.1.20",
          "form": "먹을거... 두부텐더를 사볼까요?",
          "original_form": "먹을거... 두부텐더를 사볼까요?",
          "speaker_id": "1",
          "time": "20210827 14:19"
        },
        {
          "id": "MDRW2100018939.1.21",
          "form": "냉동음식을 사볼까해요",
          "original_form": "냉동음식을 사볼까해요",
          "speaker_id": "1",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100018939.1.22",
          "form": "아 두부텐더 저도 궁금한데",
          "original_form": "아 두부텐더 저도 궁금한데",
          "speaker_id": "2",
          "time": "20210827 14:27"
        },
        {
          "id": "MDRW2100018939.1.23",
          "form": "쿠팡에 재고가 안들어와서 아직 못사봤어요",
          "original_form": "쿠팡에 재고가 안들어와서 아직 못사봤어요",
          "speaker_id": "2",
          "time": "20210827 14:27"
        },
        {
          "id": "MDRW2100018939.1.24",
          "form": "일반 쇼핑몰에서 냉동식품 주문하면",
          "original_form": "일반 쇼핑몰에서 냉동식품 주문하면",
          "speaker_id": "2",
          "time": "20210827 14:28"
        },
        {
          "id": "MDRW2100018939.1.25",
          "form": "스티로폼 박스 처리가 귀찮아서",
          "original_form": "스티로폼 박스 처리가 귀찮아서",
          "speaker_id": "2",
          "time": "20210827 14:28"
        },
        {
          "id": "MDRW2100018939.1.26",
          "form": "하 맞아요...",
          "original_form": "하 맞아요...",
          "speaker_id": "1",
          "time": "20210827 14:28"
        },
        {
          "id": "MDRW2100018939.1.27",
          "form": "왠만하면 프레시백 같은데 담아주는 곳에서 사거든요",
          "original_form": "왠만하면 프레시백 같은데 담아주는 곳에서 사거든요",
          "speaker_id": "2",
          "time": "20210827 14:28"
        },
        {
          "id": "MDRW2100018939.1.28",
          "form": "엄청 많이 들어오죠",
          "original_form": "엄청 많이 들어오죠",
          "speaker_id": "1",
          "time": "20210827 14:28"
        },
        {
          "id": "MDRW2100018939.1.29",
          "form": "저희집은 다행히 재활용이 돼서",
          "original_form": "저희집은 다행히 재활용이 돼서",
          "speaker_id": "1",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018939.1.30",
          "form": "냉장고에 아이스팩이 쌓여있답니다..",
          "original_form": "냉장고에 아이스팩이 쌓여있답니다..",
          "speaker_id": "1",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018939.1.31",
          "form": "재활용되서 잘됐네요",
          "original_form": "재활용되서 잘됐네요",
          "speaker_id": "2",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018939.1.32",
          "form": "창고에도 스티로폼 박스가 한가득이에요...",
          "original_form": "창고에도 스티로폼 박스가 한가득이에요...",
          "speaker_id": "1",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018939.1.33",
          "form": "공간차지가 많아서..",
          "original_form": "공간차지가 많아서..",
          "speaker_id": "2",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018939.1.34",
          "form": "귀찮겠어요",
          "original_form": "귀찮겠어요",
          "speaker_id": "2",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018939.1.35",
          "form": "네ㅠㅠ 뭐 찾으려면 그거 다 치워야해요",
          "original_form": "네ㅠㅠ 뭐 찾으려면 그거 다 치워야해요",
