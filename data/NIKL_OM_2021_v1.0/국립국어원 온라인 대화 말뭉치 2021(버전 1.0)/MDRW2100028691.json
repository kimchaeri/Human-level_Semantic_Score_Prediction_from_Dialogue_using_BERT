      "utterance": [
        {
          "id": "MDRW2100028691.1.1",
          "form": "읽고싶은 책 있으신가여?",
          "original_form": "읽고싶은 책 있으신가여?",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028691.1.2",
          "form": "글쎄요. 산 책도 아직 못읽고 있네요ㅋㅋ",
          "original_form": "글쎄요. 산 책도 아직 못읽고 있네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028691.1.3",
          "form": "ㅋㅋㅋㅋ아 책 집중 안되져 ㅠ",
          "original_form": "ㅋㅋㅋㅋ아 책 집중 안되져 ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028691.1.4",
          "form": "네 만화책만 재밌어요ㅋㅋ",
          "original_form": "네 만화책만 재밌어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100028691.1.5",
          "form": "아 저두요",
          "original_form": "아 저두요",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028691.1.6",
          "form": "만홫9ㄱ 뭐 보세요?!",
          "original_form": "만홫9ㄱ 뭐 보세요?!",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028691.1.7",
          "form": "만화책",
          "original_form": "만화책",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028691.1.8",
          "form": "웹툰 단행본이요",
          "original_form": "웹툰 단행본이요",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028691.1.9",
          "form": "이번엔 전독시 샀어요",
          "original_form": "이번엔 전독시 샀어요",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028691.1.10",
          "form": "전독시 줄임말인가요.?!!",
          "original_form": "전독시 줄임말인가요.?!!",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028691.1.11",
          "form": "무승 웹툰인가요",
          "original_form": "무승 웹툰인가요",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028691.1.12",
          "form": "전지적 독자 시점이라구 네이버 웹툰이에요",
          "original_form": "전지적 독자 시점이라구 네이버 웹툰이에요",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100028691.1.13",
          "form": "판타지요ㅋㅋㅋ",
          "original_form": "판타지요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100028691.1.14",
          "form": "오.. 판타지",
          "original_form": "오.. 판타지",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100028691.1.15",
          "form": "좀 끌리는데요",
          "original_form": "좀 끌리는데요",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100028691.1.16",
          "form": "연재중인거니까 한번 보세요ㅋㅋ",
          "original_form": "연재중인거니까 한번 보세요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100028691.1.17",
          "form": "연재 많이 됐나여???!",
          "original_form": "연재 많이 됐나여???!",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100028691.1.18",
          "form": "기다리다가 지칠까봐 하하",
          "original_form": "기다리다가 지칠까봐 하하",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100028691.1.19",
          "form": "웹툰은 오래 연재된건 아니에요",
          "original_form": "웹툰은 오래 연재된건 아니에요",
          "speaker_id": "1",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100028691.1.20",
          "form": "소설이 원작이어서ㅋㅋ 소설은 완결났구요",
          "original_form": "소설이 원작이어서ㅋㅋ 소설은 완결났구요",
          "speaker_id": "1",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100028691.1.21",
          "form": "끝날 때까지 기다려야겠어요 ㅎㅎ",
          "original_form": "끝날 때까지 기다려야겠어요 ㅎㅎ",
          "speaker_id": "2",
