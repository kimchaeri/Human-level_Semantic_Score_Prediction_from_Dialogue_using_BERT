      "utterance": [
        {
          "id": "MDRW2100013308.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013308.1.2",
          "form": "안녕하세요! 반려동물 키우시나요??",
          "original_form": "안녕하세요! 반려동물 키우시나요??",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013308.1.3",
          "form": "넵 강아지 두마리 키워용",
          "original_form": "넵 강아지 두마리 키워용",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013308.1.4",
          "form": "우와 부럽네요ㅜ 저는 아버지가 비염이 심하셔서",
          "original_form": "우와 부럽네요ㅜ 저는 아버지가 비염이 심하셔서",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013308.1.5",
          "form": "종이 뭔가요???",
          "original_form": "종이 뭔가요???",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013308.1.6",
          "form": "저희 가족도 비염있는데 이겨내고 사는중입니다 ㅠㅠㅋㅋ",
          "original_form": "저희 가족도 비염있는데 이겨내고 사는중입니다 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013308.1.7",
          "form": "요크셔테리어예요! 엄마와 아들이랍니당",
          "original_form": "요크셔테리어예요! 엄마와 아들이랍니당",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013308.1.8",
          "form": "우오오오",
          "original_form": "우오오오",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013308.1.9",
          "form": "저희 집에서 키운다는 느낌은 아니지만",
          "original_form": "저희 집에서 키운다는 느낌은 아니지만",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013308.1.10",
          "form": "집 앞 테니스장에서 사는 고양이 2마리에게 밥을 주고 있어요!",
          "original_form": "집 앞 테니스장에서 사는 고양이 2마리에게 밥을 주고 있어요!",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013308.1.11",
          "form": "오오 좋은일 하시네요..!",
          "original_form": "오오 좋은일 하시네요..!",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013308.1.12",
          "form": "길고양이들이 많이 경계하지는 않나요?",
          "original_form": "길고양이들이 많이 경계하지는 않나요?",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013308.1.13",
          "form": "엄마냥인데 주변에 먹을것도 없어보이고 넘 마르고",
          "original_form": "엄마냥인데 주변에 먹을것도 없어보이고 넘 마르고",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013308.1.14",
          "form": "쓰레기통 뒤적거리는게 안쓰러워서ㅜ 시작한건데",
          "original_form": "쓰레기통 뒤적거리는게 안쓰러워서ㅜ 시작한건데",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013308.1.15",
          "form": "벌써 3개월?? 된것같네요",
          "original_form": "벌써 3개월?? 된것같네요",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013308.1.16",
          "form": "벌써 3개월이라니 대단하시네요!",
          "original_form": "벌써 3개월이라니 대단하시네요!",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013308.1.17",
          "form": "길고양이들 급여는 보통 사료를 주시는 건가요? 캔이려나?",
          "original_form": "길고양이들 급여는 보통 사료를 주시는 건가요? 캔이려나?",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013308.1.18",
          "form": "밥주는 사람은 알아보나봐요",
          "original_form": "밥주는 사람은 알아보나봐요",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013308.1.19",
          "form": "맞아요 처음엔 경계해도 나중에는 알아보는 것 같더라구요",
          "original_form": "맞아요 처음엔 경계해도 나중에는 알아보는 것 같더라구요",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013308.1.20",
          "form": "그러게요 넘 신기한거같아요",
          "original_form": "그러게요 넘 신기한거같아요",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013308.1.21",
          "form": "제가 강아지를 키우다보니 고양이랑 다른 점이 많아서 신기하더라구요",
          "original_form": "제가 강아지를 키우다보니 고양이랑 다른 점이 많아서 신기하더라구요",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013308.1.22",
          "form": "오오 저는 고양이만 주로 봐서 강아지 넘 궁금해요 ㅜㅠ 넘 귀엽구",
          "original_form": "오오 저는 고양이만 주로 봐서 강아지 넘 궁금해요 ㅜㅠ 넘 귀엽구",
          "speaker_id": "2",
          "time": "20210825 14:25"
        }
      ]
    }
