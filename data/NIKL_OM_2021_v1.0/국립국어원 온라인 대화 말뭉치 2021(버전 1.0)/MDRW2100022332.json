      "utterance": [
        {
          "id": "MDRW2100022332.1.1",
          "form": "피자헛은 시간제한도 있으면서 너무 피자가 안나와서",
          "original_form": "피자헛은 시간제한도 있으면서 너무 피자가 안나와서",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022332.1.2",
          "form": "아무래도 취향대로 고를 수 있으니까요",
          "original_form": "아무래도 취향대로 고를 수 있으니까요",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022332.1.3",
          "form": "많이 못먹었던 거 같네요",
          "original_form": "많이 못먹었던 거 같네요",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022332.1.4",
          "form": "아 점바점이구나",
          "original_form": "아 점바점이구나",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022332.1.5",
          "form": "저희는 회전율겁나 빨랐거든요",
          "original_form": "저희는 회전율겁나 빨랐거든요",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022332.1.6",
          "form": "ㅠㅠㅠ혹시 그",
          "original_form": "ㅠㅠㅠ혹시 그",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022332.1.7",
          "form": "피자몰 알아요?",
          "original_form": "피자몰 알아요?",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022332.1.8",
          "form": "이랜드꺼요?",
          "original_form": "이랜드꺼요?",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022332.1.9",
          "form": "알아요",
          "original_form": "알아요",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022332.1.10",
          "form": "거기가 종류도 다양한데 회전률도 대박이에요",
          "original_form": "거기가 종류도 다양한데 회전률도 대박이에요",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022332.1.11",
          "form": "옥수수피자 진짜 맛있거든요 그쵸?",
          "original_form": "옥수수피자 진짜 맛있거든요 그쵸?",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022332.1.12",
          "form": "거기 바로바로 나오죠ㅋㅋ",
          "original_form": "거기 바로바로 나오죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022332.1.13",
          "form": "거긴ㅈ옥수수피자 먹으러 가는 곳이잖아요",
          "original_form": "거긴ㅈ옥수수피자 먹으러 가는 곳이잖아요",
          "speaker_id": "1",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022332.1.14",
          "form": "맞아요 ㅠ 피자몰 또 가고싶어요 가격도 착하잖아요",
          "original_form": "맞아요 ㅠ 피자몰 또 가고싶어요 가격도 착하잖아요",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022332.1.15",
          "form": "평일 하루종일 9900원 말도안되는 가격이에요",
          "original_form": "평일 하루종일 9900원 말도안되는 가격이에요",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022332.1.16",
          "form": "거기 싼맛에 가기 좋은거같아요",
          "original_form": "거기 싼맛에 가기 좋은거같아요",
          "speaker_id": "1",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022332.1.17",
          "form": "가장 좋은건 1인 입장도 받아주고 ㅋㅋㅋ",
          "original_form": "가장 좋은건 1인 입장도 받아주고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022332.1.18",
          "form": "많이 먹기 딱좋은ㅋㅋ",
          "original_form": "많이 먹기 딱좋은ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022332.1.19",
          "form": "근데 갈수록 메뉴가 좀 줄어드는 거 같은 느낌이에요",
          "original_form": "근데 갈수록 메뉴가 좀 줄어드는 거 같은 느낌이에요",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022332.1.20",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "1",
          "time": "20210829 12:43"
        }
