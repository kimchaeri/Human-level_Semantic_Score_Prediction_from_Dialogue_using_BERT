      "utterance": [
        {
          "id": "MDRW2100023407.1.1",
          "form": "시애틀이랑 한국이랑 제일 다른 점이 있나용?",
          "original_form": "시애틀이랑 한국이랑 제일 다른 점이 있나용?",
          "speaker_id": "1",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023407.1.2",
          "form": "오홍 해외여행 다녀오신 곳 있으세요?",
          "original_form": "오홍 해외여행 다녀오신 곳 있으세요?",
          "speaker_id": "2",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023407.1.3",
          "form": "저는 그때 어렸어서ㅎㅎ",
          "original_form": "저는 그때 어렸어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023407.1.4",
          "form": "저는 일본 오사카랑 후쿠오카 두번갔아용!",
          "original_form": "저는 일본 오사카랑 후쿠오카 두번갔아용!",
          "speaker_id": "1",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023407.1.5",
          "form": "아항ㅋㅋㅋ 그래도 너무 좋은 추억이시겠어요",
          "original_form": "아항ㅋㅋㅋ 그래도 너무 좋은 추억이시겠어요",
          "speaker_id": "1",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023407.1.6",
          "form": "와 저도 일본 식도락",
          "original_form": "와 저도 일본 식도락",
          "speaker_id": "2",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023407.1.7",
          "form": "한번 해보는 게 꿈인데",
          "original_form": "한번 해보는 게 꿈인데",
          "speaker_id": "2",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023407.1.8",
          "form": "저도 막 우왕 규카츠! 우왕 라멘!",
          "original_form": "저도 막 우왕 규카츠! 우왕 라멘!",
          "speaker_id": "1",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023407.1.9",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023407.1.10",
          "form": "이러고 갔는데",
          "original_form": "이러고 갔는데",
          "speaker_id": "1",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023407.1.11",
          "form": "아니었어요...쥬룩...",
          "original_form": "아니었어요...쥬룩...",
          "speaker_id": "1",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023407.1.12",
          "form": "벌써침고이는데",
          "original_form": "벌써침고이는데",
          "speaker_id": "2",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023407.1.13",
          "form": "오잉? 왜요?",
          "original_form": "오잉? 왜요?",
          "speaker_id": "2",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023407.1.14",
          "form": "한국사람들이 일본음식중에 맛있는것만 잘 골라서 들여온건지",
          "original_form": "한국사람들이 일본음식중에 맛있는것만 잘 골라서 들여온건지",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023407.1.15",
          "form": "규카츠도 한국에서 잘하는집보다 맛없었구",
          "original_form": "규카츠도 한국에서 잘하는집보다 맛없었구",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023407.1.16",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023407.1.17",
          "form": "라멘은 느끼했어요ㅠ퓨",
          "original_form": "라멘은 느끼했어요ㅠ퓨",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023407.1.18",
          "form": "대체적으로 일본 음식이 달고 짜고 달거나 짜고",
          "original_form": "대체적으로 일본 음식이 달고 짜고 달거나 짜고",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023407.1.19",
          "form": "아 일본식 라멘은 국물이 진하죵",
          "original_form": "아 일본식 라멘은 국물이 진하죵",
          "speaker_id": "2",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023407.1.20",
          "form": "간장베이스가 많아서",
          "original_form": "간장베이스가 많아서",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023407.1.21",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023407.1.22",
          "form": "설탕도 왕창!ㅋㅋㅋ",
          "original_form": "설탕도 왕창!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023407.1.23",
          "form": "가서",
          "original_form": "가서",
          "speaker_id": "1",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023407.1.24",
          "form": "신라면 컵라면으로 가져간거",
          "original_form": "신라면 컵라면으로 가져간거",
          "speaker_id": "1",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023407.1.25",
          "form": "까먹었어요....",
          "original_form": "까먹었어요....",
          "speaker_id": "1",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023407.1.26",
          "form": "코로나전이라 한국인도 되게 많았구",
          "original_form": "코로나전이라 한국인도 되게 많았구",
          "speaker_id": "1",
          "time": "20210830 01:39"
        }
      ]
    }
  ]
}
