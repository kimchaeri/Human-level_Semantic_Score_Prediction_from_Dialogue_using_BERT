      "utterance": [
        {
          "id": "MDRW2100010011.1.1",
          "form": "난 아프니깐 청춘이다 이후로 기억이 없네 ㅋㅋㅋㅋ",
          "original_form": "난 아프니깐 청춘이다 이후로 기억이 없네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010011.1.2",
          "form": "급변 ㅋㅋㅋ",
          "original_form": "급변{censored} ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010011.1.3",
          "form": "ㅋㅋㅋ 아 웃겨ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 아 웃겨ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010011.1.4",
          "form": "요즘 넘 건조해가 오일을 좀 써야긋네~",
          "original_form": "요즘 넘 건조해가 오일을 좀 써야긋네~",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010011.1.5",
          "form": "그러이 진짜 건조하다ㅜ",
          "original_form": "그러이 진짜 건조하다ㅜ",
          "speaker_id": "1",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010011.1.6",
          "form": "하도 슬리퍼신고 다녔디 발이 진짜 건조함 ㅠ",
          "original_form": "하도 슬리퍼신고 다녔디 발이 진짜 건조함 ㅠ",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010011.1.7",
          "form": "맞어 나도 몸이 건조ㅜㅜ",
          "original_form": "맞어 나도 몸이 건조ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 13:15"
        },
        {
          "id": "MDRW2100010011.1.8",
          "form": "보습제 바르고 양말신어야되는데..안신게 되네 ㅋㅋㅋㅋ",
          "original_form": "보습제 바르고 양말신어야되는데..안신게 되네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:15"
        },
        {
          "id": "MDRW2100010011.1.9",
          "form": "ㅋㅋ아직은 안신어도될만하니ㅋㅋ",
          "original_form": "ㅋㅋ아직은 안신어도될만하니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:15"
        },
        {
          "id": "MDRW2100010011.1.10",
          "form": "비오니 양말신고 하는것도 젖으니 더 찝찝",
          "original_form": "비오니 양말신고 하는것도 젖으니 더 찝찝",
          "speaker_id": "1",
          "time": "20210823 13:15"
        },
        {
          "id": "MDRW2100010011.1.11",
          "form": "요즘에 둘째가 팔,다리 접히는 부분에 건조해서 신경쓰인다",
          "original_form": "요즘에 둘째가 팔,다리 접히는 부분에 건조해서 신경쓰인다",
          "speaker_id": "1",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100010011.1.12",
          "form": "그건 맞다 ㅋㅋㅋㅋ집에가서 발씻으면 개운한데~",
          "original_form": "그건 맞다 ㅋㅋㅋㅋ집에가서 발씻으면 개운한데~",
          "speaker_id": "2",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100010011.1.13",
          "form": "접히는 부분에 건조하믄 걱정된다이ㅠ",
          "original_form": "접히는 부분에 건조하믄 걱정된다이ㅠ",
          "speaker_id": "2",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100010011.1.14",
          "form": "난도 둘찌그캐가 걱정많았..ㅠㅠ",
          "original_form": "난도 둘찌그캐가 걱정많았..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100010011.1.15",
          "form": "그래서 크림밤 사서 발라주는데",
          "original_form": "그래서 크림밤 사서 발라주는데",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100010011.1.16",
          "form": "아 둘째도 그랬나?",
          "original_form": "아 둘째도 그랬나?",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100010011.1.17",
          "form": "좋아졌어?",
          "original_form": "좋아졌어?",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100010011.1.18",
          "form": "응~계속 바르고 리도맥스도 바르고했다~",
          "original_form": "응~계속 바르고 리도맥스도 바르고했다~",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100010011.1.19",
          "form": "야들도 피부가 진짜 안좋더ㅠㅠ",
          "original_form": "야들도 피부가 진짜 안좋더ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100010011.1.20",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100010011.1.21",
          "form": "우리애들은 더우면 그렇더라~",
          "original_form": "우리애들은 더우면 그렇더라~",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100010011.1.22",
          "form": "오히려 좀 서늘해지니 좀 낫다ㅠ",
          "original_form": "오히려 좀 서늘해지니 좀 낫다ㅠ",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010011.1.23",
          "form": "아 아토피로 될까봐",
          "original_form": "아 아토피로 될까봐",
          "speaker_id": "1",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010011.1.24",
          "form": "신경쓰이더라구",
          "original_form": "신경쓰이더라구",
          "speaker_id": "1",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010011.1.25",
          "form": "아직 땀띠도 많다ㅠ",
          "original_form": "아직 땀띠도 많다ㅠ",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010011.1.26",
          "form": "그래 아토피가 진짜 걱정..ㅠㅠ",
          "original_form": "그래 아토피가 진짜 걱정..ㅠㅠ",
          "speaker_id": "2",
