      "utterance": [
        {
          "id": "MDRW2100020851.1.1",
          "form": "다이어트?",
          "original_form": "다이어트?",
          "speaker_id": "1",
          "time": "20210828 13:27"
        },
        {
          "id": "MDRW2100020851.1.2",
          "form": "네네 다이어트는 항상 하는것같아요",
          "original_form": "네네 다이어트는 항상 하는것같아요",
          "speaker_id": "2",
          "time": "20210828 13:27"
        },
        {
          "id": "MDRW2100020851.1.3",
          "form": "빼고 요요오고 반복...",
          "original_form": "빼고 요요오고 반복...",
          "speaker_id": "2",
          "time": "20210828 13:27"
        },
        {
          "id": "MDRW2100020851.1.4",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020851.1.5",
          "form": "얼마나 오래 했어여?",
          "original_form": "얼마나 오래 했어여?",
          "speaker_id": "1",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020851.1.6",
          "form": "올해부터 했는데 수술할 일이 있어서 두달 쉬고 살이 또 붙었네요",
          "original_form": "올해부터 했는데 수술할 일이 있어서 두달 쉬고 살이 또 붙었네요",
          "speaker_id": "2",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020851.1.7",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020851.1.8",
          "form": "어머 ㅠㅠ 힘드셨겠어요",
          "original_form": "어머 ㅠㅠ 힘드셨겠어요",
          "speaker_id": "1",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020851.1.9",
          "form": "건강이 중요한데.. 어디 아프신거에요?",
          "original_form": "건강이 중요한데.. 어디 아프신거에요?",
          "speaker_id": "1",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020851.1.10",
          "form": "아 큰병은 아니구요 ㅋㅋ 혹이 생겨서 제거술했어요 ㅋㅋ",
          "original_form": "아 큰병은 아니구요 ㅋㅋ 혹이 생겨서 제거술했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020851.1.11",
          "form": "휴 그럼 다행이네요",
          "original_form": "휴 그럼 다행이네요",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020851.1.12",
          "form": "빨리 발견했나봐요?",
          "original_form": "빨리 발견했나봐요?",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020851.1.13",
          "form": "빠른편 이었을거예요 이래서 아프면 빨리 병원 가야되나봐요",
          "original_form": "빠른편 이었을거예요 이래서 아프면 빨리 병원 가야되나봐요",
          "speaker_id": "2",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020851.1.14",
          "form": "오 다행이다 ㅋㅋㅋ",
          "original_form": "오 다행이다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020851.1.15",
          "form": "그거 나중에 암으로 발전하고 그러기도 하죠?",
          "original_form": "그거 나중에 암으로 발전하고 그러기도 하죠?",
          "speaker_id": "1",
          "time": "20210828 13:54"
        },
        {
          "id": "MDRW2100020851.1.16",
          "form": "아 저는 다행히 암은 아니었어요! 혹이 커지면 옆 장기에 협착되서 수술이 더 어려워져서 작을때 수술하는게 좋대요",
          "original_form": "아 저는 다행히 암은 아니었어요! 혹이 커지면 옆 장기에 협착되서 수술이 더 어려워져서 작을때 수술하는게 좋대요",
          "speaker_id": "2",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100020851.1.17",
          "form": "아 그렇구나 처음 알았어요~!",
          "original_form": "아 그렇구나 처음 알았어요~!",
          "speaker_id": "1",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100020851.1.18",
          "form": "근데 어떻게 발견하신거에요?",
          "original_form": "근데 어떻게 발견하신거에요?",
          "speaker_id": "1",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100020851.1.19",
          "form": "초음파 보고 씨티 찍었어요!",
          "original_form": "초음파 보고 씨티 찍었어요!",
          "speaker_id": "2",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020851.1.20",
          "form": "아~ 건강검진이에요?",
          "original_form": "아~ 건강검진이에요?",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020851.1.21",
          "form": "아뇨 배가 아파서 병원 갔었어요",
          "original_form": "아뇨 배가 아파서 병원 갔었어요",
          "speaker_id": "2",
          "time": "20210828 14:11"
        }
      ]
