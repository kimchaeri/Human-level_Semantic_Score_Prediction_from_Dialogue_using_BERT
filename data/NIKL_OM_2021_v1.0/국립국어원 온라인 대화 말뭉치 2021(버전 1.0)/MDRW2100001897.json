      "utterance": [
        {
          "id": "MDRW2100001897.1.1",
          "form": "반려동물 키우시나요????!",
          "original_form": "반려동물 키우시나요????!",
          "speaker_id": "1",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001897.1.2",
          "form": "아뇨 저는 안 키워요",
          "original_form": "아뇨 저는 안 키워요",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001897.1.3",
          "form": "정말 키우고 싶은데 가족이 알레르기 있어요ㅠㅠ",
          "original_form": "정말 키우고 싶은데 가족이 알레르기 있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001897.1.4",
          "form": "저도 키우고 있지 않아요 ㅎ",
          "original_form": "저도 키우고 있지 않아요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001897.1.5",
          "form": "혹시 키운다면 어떤 동물을 원하세요?",
          "original_form": "혹시 키운다면 어떤 동물을 원하세요?",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001897.1.6",
          "form": "오 전 강아지나 고양이요!",
          "original_form": "오 전 강아지나 고양이요!",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100001897.1.7",
          "form": "둘 다 너무 좋아해요ㅠㅠ",
          "original_form": "둘 다 너무 좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100001897.1.8",
          "form": "저두요 ㅎ 근데 요새 최애는 고양이입니다",
          "original_form": "저두요 ㅎ 근데 요새 최애는 고양이입니다",
          "speaker_id": "1",
          "time": "20210819 23:09"
        },
        {
          "id": "MDRW2100001897.1.9",
          "form": "헉 저도 요샌 고양이 너무 좋아해요",
          "original_form": "헉 저도 요샌 고양이 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100001897.1.10",
          "form": "근처에 고양이 카페가 있거든요 유기묘들 구조해오고 아픈 친구들도 적응할 수 있게 해주는 곳인데",
          "original_form": "근처에 고양이 카페가 있거든요 유기묘들 구조해오고 아픈 친구들도 적응할 수 있게 해주는 곳인데",
          "speaker_id": "2",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100001897.1.11",
          "form": "아이들이 너무 예뻐요",
          "original_form": "아이들이 너무 예뻐요",
          "speaker_id": "2",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100001897.1.12",
          "form": "우와 거기 정말 좋은 곳이네요 ㅠㅠㅠㅠㅠ",
          "original_form": "우와 거기 정말 좋은 곳이네요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001897.1.13",
          "form": "고양이는 보기만해도 힐링 돼요",
          "original_form": "고양이는 보기만해도 힐링 돼요",
          "speaker_id": "1",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001897.1.14",
          "form": "그쵸 너무 귀여워요",
          "original_form": "그쵸 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001897.1.15",
          "form": "근데 털 뿜을땐 놀랍긴 하더라구요",
          "original_form": "근데 털 뿜을땐 놀랍긴 하더라구요",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001897.1.16",
          "form": "나중에 보면 여기저기 털이 꽂혀있어요ㅋㅋㅋ",
          "original_form": "나중에 보면 여기저기 털이 꽂혀있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001897.1.17",
          "form": "ㅋㅋㅋㅋㅋ 털이 아무곳이나 다 붙어요",
          "original_form": "ㅋㅋㅋㅋㅋ 털이 아무곳이나 다 붙어요",
          "speaker_id": "1",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100001897.1.18",
          "form": "제 지인은 밥 먹을 때 밥에도 붙는다고 하더라규요",
          "original_form": "제 지인은 밥 먹을 때 밥에도 붙는다고 하더라규요",
          "speaker_id": "1",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100001897.1.19",
          "form": "밥먹을땐 진짜 신경쓰이겠어요",
          "original_form": "밥먹을땐 진짜 신경쓰이겠어요",
          "speaker_id": "2",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100001897.1.20",
          "form": "친구랑 여행간 적 있는데 친구 짐에서 막 고양이털 나왔었어요",
          "original_form": "친구랑 여행간 적 있는데 친구 짐에서 막 고양이털 나왔었어요",
          "speaker_id": "2",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100001897.1.21",
          "form": "고양이 키우는 집사들한테는 털이 필수더라구요 ㅎㅎ",
          "original_form": "고양이 키우는 집사들한테는 털이 필수더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100001897.1.22",
          "form": "나중에 되면 그냥 그러려니 할 거 같아요",
          "original_form": "나중에 되면 그냥 그러려니 할 거 같아요",
          "speaker_id": "2",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100001897.1.23",
          "form": "청소기나 건조기에도 장난 아니던데",
          "original_form": "청소기나 건조기에도 장난 아니던데",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100001897.1.24",
          "form": "고양이 때문에라도 건조기랑은 진짜 필수품으로 해야할 거 같아요",
          "original_form": "고양이 때문에라도 건조기랑은 진짜 필수품으로 해야할 거 같아요",
          "speaker_id": "1",
          "time": "20210819 23:31"
        }
      ]
    }
  ]
