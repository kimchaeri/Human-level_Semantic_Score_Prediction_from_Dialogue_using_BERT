      "utterance": [
        {
          "id": "MDRW2100042913.1.1",
          "form": "냉장고자석같은것도 수집하시나요?",
          "original_form": "냉장고자석같은것도 수집하시나요?",
          "speaker_id": "2",
          "time": "20210907 11:22"
        },
        {
          "id": "MDRW2100042913.1.2",
          "form": "네 마그넷 모아요",
          "original_form": "네 마그넷 모아요",
          "speaker_id": "1",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100042913.1.3",
          "form": "국가별로ㅎㅎ",
          "original_form": "국가별로ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100042913.1.4",
          "form": "아 많이 모으셨나요? 지인도 한쪽면 다 모으더라고요",
          "original_form": "아 많이 모으셨나요? 지인도 한쪽면 다 모으더라고요",
          "speaker_id": "2",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100042913.1.5",
          "form": "냉장고에 꽤 붙여놓았어요",
          "original_form": "냉장고에 꽤 붙여놓았어요",
          "speaker_id": "1",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042913.1.6",
          "form": "깨지면 엄청 속상하더라고요",
          "original_form": "깨지면 엄청 속상하더라고요",
          "speaker_id": "2",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042913.1.7",
          "form": "그쵸ㅜ 여행의 추억이 담긴거라",
          "original_form": "그쵸ㅜ 여행의 추억이 담긴거라",
          "speaker_id": "1",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100042913.1.8",
          "form": "ㅜ 다시 그거 사러 갈수도 없고요",
          "original_form": "ㅜ 다시 그거 사러 갈수도 없고요",
          "speaker_id": "2",
          "time": "20210907 12:45"
        },
        {
          "id": "MDRW2100042913.1.9",
          "form": "네ㅜ 여행 기념품이니",
          "original_form": "네ㅜ 여행 기념품이니",
          "speaker_id": "1",
          "time": "20210907 12:46"
        },
        {
          "id": "MDRW2100042913.1.10",
          "form": "두개씩 사야겠어요!",
          "original_form": "두개씩 사야겠어요!",
          "speaker_id": "2",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100042913.1.11",
          "form": "아 그것도 좋은 생각이네요",
          "original_form": "아 그것도 좋은 생각이네요",
          "speaker_id": "1",
          "time": "20210907 13:02"
        },
        {
          "id": "MDRW2100042913.1.12",
          "form": "ㅋㅋ앞으로 가게되면...코로나 끝나면ㅜㅠ",
          "original_form": "ㅋㅋ앞으로 가게되면...코로나 끝나면ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 14:09"
        },
        {
          "id": "MDRW2100042913.1.13",
          "form": "네 코로나 이후 여행에서는",
          "original_form": "네 코로나 이후 여행에서는",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100042913.1.14",
          "form": "기념품 2개씩",
          "original_form": "기념품 2개씩",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100042913.1.15",
          "form": "ㅋㅋ 좋은 생각이에요",
          "original_form": "ㅋㅋ 좋은 생각이에요",
          "speaker_id": "2",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100042913.1.16",
          "form": "안사고 후회하면 또 가기 힘들어요ㅜ",
          "original_form": "안사고 후회하면 또 가기 힘들어요ㅜ",
