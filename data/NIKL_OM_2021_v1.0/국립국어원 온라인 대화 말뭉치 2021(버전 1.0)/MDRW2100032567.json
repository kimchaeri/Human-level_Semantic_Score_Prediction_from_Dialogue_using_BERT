      "utterance": [
        {
          "id": "MDRW2100032567.1.1",
          "form": "자기 말 하는거만 좋아하는",
          "original_form": "자기 말 하는거만 좋아하는",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032567.1.2",
          "form": "사람인것 같기도 하고",
          "original_form": "사람인것 같기도 하고",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032567.1.3",
          "form": "어우...",
          "original_form": "어우...",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032567.1.4",
          "form": "진짜name3 생각나긴 한다ㅋㅋㅋㅋ",
          "original_form": "진짜&name3& 생각나긴 한다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032567.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032567.1.6",
          "form": "맞다 너 그",
          "original_form": "맞다 너 그",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032567.1.7",
          "form": "내일 신청이네?",
          "original_form": "내일 신청이네?",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.8",
          "form": "아니 그리고 아침에 어떤걸 a 방식으로 하면",
          "original_form": "아니 그리고 아침에 어떤걸 a 방식으로 하면",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.9",
          "form": "내일 맞지",
          "original_form": "내일 맞지",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.10",
          "form": "지원금",
          "original_form": "지원금",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.11",
          "form": "잘했다 하면서",
          "original_form": "잘했다 하면서",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.12",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.13",
          "form": "아 맞아맞아",
          "original_form": "아 맞아맞아",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.14",
          "form": "오후에 똑같이 a로 하면",
          "original_form": "오후에 똑같이 a로 하면",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.15",
          "form": "예인 그렇게 하면..",
          "original_form": "예인 그렇게 하면..",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.16",
          "form": "이러면서 짜증냄 ㅠㅠ",
          "original_form": "이러면서 짜증냄 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.18",
          "form": "에효",
          "original_form": "에효",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.19",
          "form": "어디다가 맞추라는거임",
          "original_form": "어디다가 맞추라는거임",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032567.1.20",
          "form": "내말이 그말임 ㅠㅠ",
          "original_form": "내말이 그말임 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032567.1.21",
          "form": "기분이 태도가되는거",
          "original_form": "기분이 태도가되는거",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032567.1.22",
          "form": "진짜안좋은 습관인디",
          "original_form": "진짜안좋은 습관인디",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032567.1.23",
          "form": "딱 그거네...",
          "original_form": "딱 그거네...",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032567.1.24",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032567.1.25",
          "form": "고치기 어려운 태도 중에 하나인듯",
          "original_form": "고치기 어려운 태도 중에 하나인듯",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032567.1.26",
          "form": "그치...",
