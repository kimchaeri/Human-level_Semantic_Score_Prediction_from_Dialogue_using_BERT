      "utterance": [
        {
          "id": "MDRW2100037146.1.1",
          "form": "저녁은 드셨어요?",
          "original_form": "저녁은 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 20:38"
        },
        {
          "id": "MDRW2100037146.1.2",
          "form": "네 갈치조림해서 먹었어요ㅎㅎ",
          "original_form": "네 갈치조림해서 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100037146.1.3",
          "form": "와 저 갈치 엄청 좋아하는데 ㅠㅠㅠ",
          "original_form": "와 저 갈치 엄청 좋아하는데 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100037146.1.4",
          "form": "최애 생선입니다ㅜㅜ",
          "original_form": "최애 생선입니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100037146.1.5",
          "form": "조림도 맛있고 구이도 맛있는 생선이죠ㅎㅎㅎ",
          "original_form": "조림도 맛있고 구이도 맛있는 생선이죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 20:49"
        },
        {
          "id": "MDRW2100037146.1.6",
          "form": "맞아요 ㅎㅎ 조림에 밥 비벼먹어야되는데 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ 조림에 밥 비벼먹어야되는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100037146.1.7",
          "form": "무 올려서 !",
          "original_form": "무 올려서 !",
          "speaker_id": "1",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100037146.1.8",
          "form": "ㅋㅋㅋㅋㅋ 저 오늘 밥에 무 으깨서 갈치살 올려 먹었어요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 저 오늘 밥에 무 으깨서 갈치살 올려 먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037146.1.9",
          "form": "아 완전 맛나게 드셨네요!!!!",
          "original_form": "아 완전 맛나게 드셨네요!!!!",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037146.1.10",
          "form": "ㅜㅜ 하 먹고싶네요",
          "original_form": "ㅜㅜ 하 먹고싶네요",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037146.1.11",
          "form": "제주도에서 전 갈치조림을 젤 맛나게 먹을정도로 ㅠㅠ 좋아해요",
          "original_form": "제주도에서 전 갈치조림을 젤 맛나게 먹을정도로 ㅠㅠ 좋아해요",
          "speaker_id": "1",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100037146.1.12",
          "form": "입맛없을때 갈치조림 하나면 만사 ok죠ㅋㅋㅋ",
          "original_form": "입맛없을때 갈치조림 하나면 만사 ok죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100037146.1.13",
          "form": "맞아요 ㅋㅋ 고등어 조림도 좋아합니다!",
          "original_form": "맞아요 ㅋㅋ 고등어 조림도 좋아합니다!",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100037146.1.14",
          "form": "고등어 조림에 시래기 들어가면 끝장이죠ㅋㅋㅋ",
          "original_form": "고등어 조림에 시래기 들어가면 끝장이죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100037146.1.15",
          "form": "헐 맞아요 !! 하 아시는군요 ㅎㅎ",
          "original_form": "헐 맞아요 !! 하 아시는군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100037146.1.16",
          "form": "김치 넣어도 맛나구여 ㅎㅎ",
          "original_form": "김치 넣어도 맛나구여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100037146.1.17",
          "form": "그쵸그쵸ㅋㅋㅋㅋ 그리고 바로 먹는거 보다 두세번 데워서 졸아들면 더 맛있어져욬ㅋㅋㅋ",
          "original_form": "그쵸그쵸ㅋㅋㅋㅋ 그리고 바로 먹는거 보다 두세번 데워서 졸아들면 더 맛있어져욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100037146.1.18",
          "form": "맞아요 ㅋㅋㅋ 조림은 끓을수록 맛있죠 ㅋㅋㅋ !",
          "original_form": "맞아요 ㅋㅋㅋ 조림은 끓을수록 맛있죠 ㅋㅋㅋ !",
          "speaker_id": "1",
