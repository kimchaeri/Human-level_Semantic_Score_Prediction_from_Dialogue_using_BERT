      "utterance": [
        {
          "id": "MDRW2100008047.1.1",
          "form": "강릉에 카페거리 가봤오?",
          "original_form": "강릉에 카페거리 가봤오?",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008047.1.2",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008047.1.3",
          "form": "못갔네 ㅡㅡ",
          "original_form": "못갔네 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008047.1.4",
          "form": "아 강릉 갔을때",
          "original_form": "아 강릉 갔을때",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008047.1.5",
          "form": "예쁜 커피숍 쭉있던데 바다에",
          "original_form": "예쁜 커피숍 쭉있던데 바다에",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008047.1.6",
          "form": "거기?",
          "original_form": "거기?",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008047.1.7",
          "form": "나도못가봤는데 가보고파",
          "original_form": "나도못가봤는데 가보고파",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008047.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ유명하다더라고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ유명하다더라고",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008047.1.9",
          "form": "막 4시간씩 기더려서도 먹구",
          "original_form": "막 4시간씩 기더려서도 먹구",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008047.1.10",
          "form": "거기산 맞는지 모르는데",
          "original_form": "거기산 맞는지 모르는데",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008047.1.11",
          "form": "이뿌더라",
          "original_form": "이뿌더라",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008047.1.12",
          "form": "차마시고 왔지",
          "original_form": "차마시고 왔지",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008047.1.13",
          "form": "진짜 외관이 다양해",
          "original_form": "진짜 외관이 다양해",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008047.1.14",
          "form": "보는 재미가 있어",
          "original_form": "보는 재미가 있어",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008047.1.15",
          "form": "나 아점먹고 귀찮아서",
          "original_form": "나 아점먹고 귀찮아서",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008047.1.16",
          "form": "카페간다 ㅡㅡ",
          "original_form": "카페간다 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008047.1.17",
          "form": "ㅋㅋ샌드위치 디저트로 점저 때우게 ㅋㅋ",
          "original_form": "ㅋㅋ샌드위치 디저트로 점저 때우게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008047.1.18",
          "form": "지금? ㅋㅋㅋ카페간다구",
          "original_form": "지금? ㅋㅋㅋ카페간다구",
          "speaker_id": "2",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008047.1.19",
          "form": "아ㅋㅋㅋㅋ혼자가?",
          "original_form": "아ㅋㅋㅋㅋ혼자가?",
          "speaker_id": "2",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100008047.1.20",
          "form": "아니 혼자 싫다",
          "original_form": "아니 혼자 싫다",
          "speaker_id": "1",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100008047.1.21",
          "form": "다 끌고가게",
          "original_form": "다 끌고가게",
          "speaker_id": "1",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100008047.1.22",
          "form": "안간대서 막뭐라해서 가기로함 ㅋㅋ",
          "original_form": "안간대서 막뭐라해서 가기로함 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100008047.1.23",
          "form": "여기 지방이라 카페가 은근 많아",
          "original_form": "여기 지방이라 카페가 은근 많아",
          "speaker_id": "1",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100008047.1.24",
          "form": "옛날 추억돋네",
          "original_form": "옛날 추억돋네",
          "speaker_id": "1",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008047.1.25",
          "form": "오ㅋㅋㅋㅋㅋ이쁜 카페들?",
          "original_form": "오ㅋㅋㅋㅋㅋ이쁜 카페들?",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008047.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ카페에 안락하게 앉아서 한끼 해결하면 딱 좋지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ카페에 안락하게 앉아서 한끼 해결하면 딱 좋지",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008047.1.27",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008047.1.28",
          "form": "아 맛난 에이드 먹을래",
          "original_form": "아 맛난 에이드 먹을래",
          "speaker_id": "1",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008047.1.29",
          "form": "넌 ?이따 뭐먹게?",
          "original_form": "넌 ?이따 뭐먹게?",
          "speaker_id": "1",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100008047.1.30",
          "form": "난ㅋㅋㅋ점심 이미 끝냈다ㅋㅋㅋ",
          "original_form": "난ㅋㅋㅋ점심 이미 끝냈다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100008047.1.31",
          "form": "난 아침겸 이제 먹음 ㅜㅜ",
          "original_form": "난 아침겸 이제 먹음 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100008047.1.32",
          "form": "저녁 정했어",
          "original_form": "저녁 정했어",
          "speaker_id": "1",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100008047.1.33",
          "form": "된장에 콩나물 무생채 들어간 비빔밤에",
          "original_form": "된장에 콩나물 무생채 들어간 비빔밤에",
          "speaker_id": "2",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100008047.1.34",
          "form": "정하기도 귀찮아",
          "original_form": "정하기도 귀찮아",
          "speaker_id": "1",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100008047.1.35",
          "form": "뭐먹게ㅋㅋㅋ",
          "original_form": "뭐먹게ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100008047.1.36",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "1",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100008047.1.37",
          "form": "맛나겠다",
          "original_form": "맛나겠다",
          "speaker_id": "1",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100008047.1.38",
          "form": "샌드위치 만 먹고 말고 싶다",
          "original_form": "샌드위치 만 먹고 말고 싶다",
          "speaker_id": "1",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100008047.1.39",
