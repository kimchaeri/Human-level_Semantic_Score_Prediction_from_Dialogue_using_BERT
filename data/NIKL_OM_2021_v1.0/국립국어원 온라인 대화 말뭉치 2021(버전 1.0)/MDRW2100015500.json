      "utterance": [
        {
          "id": "MDRW2100015500.1.1",
          "form": "최근에 뭐 사셨나요",
          "original_form": "최근에 뭐 사셨나요",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015500.1.2",
          "form": "음 운동화",
          "original_form": "음 운동화",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015500.1.3",
          "form": "아들 가방이요",
          "original_form": "아들 가방이요",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015500.1.4",
          "form": "전 요새 쇼핑을 다른사람꺼만 산거 같네요",
          "original_form": "전 요새 쇼핑을 다른사람꺼만 산거 같네요",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015500.1.5",
          "form": "뭐 사셨어요?",
          "original_form": "뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015500.1.6",
          "form": "저 지금 버즈 특가딜 노려봐요",
          "original_form": "저 지금 버즈 특가딜 노려봐요",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015500.1.7",
          "form": "사볼까해서 ㅋㅋ",
          "original_form": "사볼까해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015500.1.8",
          "form": "ㅋㅋ저는 뭐 청소도구?",
          "original_form": "ㅋㅋ저는 뭐 청소도구?",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015500.1.9",
          "form": "버즈 얼마에 나오나요",
          "original_form": "버즈 얼마에 나오나요",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015500.1.10",
          "form": "13이래요",
          "original_form": "13이래요",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015500.1.11",
          "form": "사전예약",
          "original_form": "사전예약",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015500.1.12",
          "form": "버즈 프로가요?",
          "original_form": "버즈 프로가요?",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015500.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015500.1.14",
          "form": "가끔 더싸게 뜬대요",
          "original_form": "가끔 더싸게 뜬대요",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015500.1.15",
          "form": "청소도구 라니 의외인데요 ㅎㅎ",
          "original_form": "청소도구 라니 의외인데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015500.1.16",
          "form": "와 와이프 10후반에 샀었는데",
          "original_form": "와 와이프 10후반에 샀었는데",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015500.1.17",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015500.1.18",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015500.1.19",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015500.1.20",
          "form": "넘 비싸서ㅠ속상하시겠다 ㅡㅡ",
          "original_form": "넘 비싸서ㅠ속상하시겠다 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015500.1.21",
          "form": "한참 전에 사서ㅋㅋㅋㅋ",
          "original_form": "한참 전에 사서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015500.1.22",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015500.1.23",
          "form": "근데 잃어먹었어요",
          "original_form": "근데 잃어먹었어요",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015500.1.24",
          "form": "ㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015500.1.25",
          "form": "요새 쫌 싸게 나온데요",
          "original_form": "요새 쫌 싸게 나온데요",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015500.1.26",
          "form": "헉 그비싼걸 ㅜㅜ",
          "original_form": "헉 그비싼걸 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015500.1.27",
          "form": "어떻게요",
          "original_form": "어떻게요",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015500.1.28",
          "form": "아 중국 어떤사람은 잃어버렸는데 배속에서 나왔데요",
          "original_form": "아 중국 어떤사람은 잃어버렸는데 배속에서 나왔데요",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015500.1.29",
          "form": "어이가 없어서 급 생각났네요",
          "original_form": "어이가 없어서 급 생각났네요",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015500.1.30",
          "form": "ㅋㅋㅋ배는 아니고",
          "original_form": "ㅋㅋㅋ배는 아니고",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015500.1.31",
          "form": "아이팟이요",
          "original_form": "아이팟이요",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015500.1.32",
          "form": "마지막 위치보면",
          "original_form": "마지막 위치보면",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015500.1.33",
          "form": "집인거 같긴한데",
          "original_form": "집인거 같긴한데",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015500.1.34",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015500.1.35",
          "form": "이사갈때",
          "original_form": "이사갈때",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015500.1.36",
          "form": "나오겠거니....",
          "original_form": "나오겠거니....",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015500.1.37",
          "form": "그렇데요 대부분",
          "original_form": "그렇데요 대부분",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015500.1.38",
          "form": "분명있을텐대",
          "original_form": "분명있을텐대",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015500.1.39",
          "form": "청소기 사고싶으세요?",
          "original_form": "청소기 사고싶으세요?",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015500.1.40",
          "form": "아무리 찾아도 안나와서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아무리 찾아도 안나와서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015500.1.41",
          "form": "일단 qcy로 대체했어요",
          "original_form": "일단 qcy로 대체했어요",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015500.1.42",
          "form": "청소기요?",
          "original_form": "청소기요?",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015500.1.43",
          "form": "있어용ㅋㅋ",
          "original_form": "있어용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015500.1.44",
          "form": "아 언제나오려나요",
          "original_form": "아 언제나오려나요",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015500.1.45",
          "form": "참",
          "original_form": "참",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015500.1.46",
          "form": "잊을만 하면 나와요",
          "original_form": "잊을만 하면 나와요",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015500.1.47",
          "form": "청소용품 뭐요?",
          "original_form": "청소용품 뭐요?",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015500.1.48",
          "form": "청소도구 뭔지 궁금한대요 ㅎㅎ",
          "original_form": "청소도구 뭔지 궁금한대요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015500.1.49",
          "form": "그 변기 청소하는거요!",
          "original_form": "그 변기 청소하는거요!",
          "speaker_id": "2",
          "time": "20210826 00:06"
        }
      ]
    }
