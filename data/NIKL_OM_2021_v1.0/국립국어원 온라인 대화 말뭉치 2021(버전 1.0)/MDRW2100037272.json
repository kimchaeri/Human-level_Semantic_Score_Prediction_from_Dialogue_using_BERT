        },
        {
          "id": "MDRW2100037272.1.4",
          "form": "재밌었어",
          "original_form": "재밌었어",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037272.1.5",
          "form": "그거 실제 있었던 일이래",
          "original_form": "그거 실제 있었던 일이래",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037272.1.6",
          "form": "아 몰랐네",
          "original_form": "아 몰랐네",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037272.1.7",
          "form": "실제 이야기도 재미있더라",
          "original_form": "실제 이야기도 재미있더라",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037272.1.8",
          "form": "김윤석 멋있어",
          "original_form": "김윤석 멋있어",
          "speaker_id": "1",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037272.1.9",
          "form": "구 교환 눈에 띄더라",
          "original_form": "구 교환 눈에 띄더라",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037272.1.10",
          "form": "밉상으로 나오던데",
          "original_form": "밉상으로 나오던데",
          "speaker_id": "1",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037272.1.11",
          "form": "거기 팀에서 멋있는 역할이잖아",
          "original_form": "거기 팀에서 멋있는 역할이잖아",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
