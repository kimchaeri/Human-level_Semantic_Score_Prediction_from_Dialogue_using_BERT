      "utterance": [
        {
          "id": "MDRW2100004776.1.1",
          "form": "저번에 우리 올림픽 보면서 군대 면제 얘기했잔항",
          "original_form": "저번에 우리 올림픽 보면서 군대 면제 얘기했잔항",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004776.1.2",
          "form": "엉 그랬지",
          "original_form": "엉 그랬지",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004776.1.3",
          "form": "근데 메달리스트 군대 면제 맞단다",
          "original_form": "근데 메달리스트 군대 면제 맞단다",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004776.1.4",
          "form": "진짜로?",
          "original_form": "진짜로?",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004776.1.5",
          "form": "응 예술 체육 복무요원이 4주 교육하고",
          "original_form": "응 예술 체육 복무요원이 4주 교육하고",
          "speaker_id": "2",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004776.1.6",
          "form": "복무요원이라 없어진지 알았지",
          "original_form": "복무요원이라 없어진지 알았지",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004776.1.7",
          "form": "그리고 3년동안 같은 계열에 종사하면 되는거래",
          "original_form": "그리고 3년동안 같은 계열에 종사하면 되는거래",
          "speaker_id": "2",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004776.1.8",
          "form": "어떻게 보면 군대면제네 그게",
          "original_form": "어떻게 보면 군대면제네 그게",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004776.1.9",
          "form": "엉 메달을 따야 면제 된다",
          "original_form": "엉 메달을 따야 면제 된다",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004776.1.10",
          "form": "야구보니 난리던데",
          "original_form": "야구보니 난리던데",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004776.1.11",
          "form": "야구는 6개팀이 출전하니 메달순위권이 50% 확률이었는데",
          "original_form": "야구는 6개팀이 출전하니 메달순위권이 50% 확률이었는데",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004776.1.12",
          "form": "50%엄청 많았네",
          "original_form": "50%엄청 많았네",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004776.1.13",
          "form": "근데 군대면제자들이 너무 많아서 열심히 안해서",
          "original_form": "근데 군대면제자들이 너무 많아서 열심히 안해서",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004776.1.14",
          "form": "진짜 열심히 안하더라",
          "original_form": "진짜 열심히 안하더라",
          "speaker_id": "1",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004776.1.15",
          "form": "응 야구대표팀 중에서 6명 빼고 다 군대면제자",
          "original_form": "응 야구대표팀 중에서 6명 빼고 다 군대면제자",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004776.1.16",
          "form": "없는 사람들 위해서 열심히 좀 하지",
          "original_form": "없는 사람들 위해서 열심히 좀 하지",
          "speaker_id": "1",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004776.1.17",
          "form": "그건 다른 사람들 사정이니",
          "original_form": "그건 다른 사람들 사정이니",
