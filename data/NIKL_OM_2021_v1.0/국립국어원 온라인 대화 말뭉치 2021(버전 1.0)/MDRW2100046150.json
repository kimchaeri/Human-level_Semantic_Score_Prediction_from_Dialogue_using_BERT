      "utterance": [
        {
          "id": "MDRW2100046150.1.1",
          "form": "힘들어요",
          "original_form": "힘들어요",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046150.1.2",
          "form": "티도안나고요",
          "original_form": "티도안나고요",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046150.1.3",
          "form": "전에는 가사도우미 썼는데",
          "original_form": "전에는 가사도우미 썼는데",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046150.1.4",
          "form": "지금은 안쓰니 더 힘드네요",
          "original_form": "지금은 안쓰니 더 힘드네요",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046150.1.5",
          "form": "쓰다 안쓰면 힘들죠",
          "original_form": "쓰다 안쓰면 힘들죠",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046150.1.6",
          "form": "왜 안쓰시나요",
          "original_form": "왜 안쓰시나요",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046150.1.7",
          "form": "가사도우미 구하기 힘들어서요 ㅎ",
          "original_form": "가사도우미 구하기 힘들어서요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046150.1.8",
          "form": "코로나 때문 인가요",
          "original_form": "코로나 때문 인가요",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046150.1.9",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046150.1.10",
          "form": "오시던분이 그만두시겠다하셔서",
          "original_form": "오시던분이 그만두시겠다하셔서",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046150.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046150.1.12",
          "form": "다시 구하려니",
          "original_form": "다시 구하려니",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046150.1.13",
          "form": "업체이용하니 영 못믿겠더라구요",
          "original_form": "업체이용하니 영 못믿겠더라구요",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046150.1.14",
          "form": "참 어렵네요",
          "original_form": "참 어렵네요",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046150.1.15",
          "form": "네 사람쓰는게 젤어려운거같아요",
          "original_form": "네 사람쓰는게 젤어려운거같아요",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046150.1.16",
          "form": "건조기 쓰세요,,",
          "original_form": "건조기 쓰세요,,",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046150.1.17",
          "form": "아니요 건조기는 없어요",
          "original_form": "아니요 건조기는 없어요",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046150.1.18",
          "form": "아 안쓰시는구낭",
          "original_form": "아 안쓰시는구낭",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046150.1.19",
          "form": "있으면 편해요",
          "original_form": "있으면 편해요",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046150.1.20",
          "form": "특히 요즘같이 장마철엔",
          "original_form": "특히 요즘같이 장마철엔",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046150.1.21",
          "form": "지인이 편하다고 해서 구매 고려중 이예요",
          "original_form": "지인이 편하다고 해서 구매 고려중 이예요",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046150.1.22",
          "form": "강추해요 ㅎ",
          "original_form": "강추해요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046150.1.23",
          "form": "신랑이 더 좋아해요 ㅋ",
          "original_form": "신랑이 더 좋아해요 ㅋ",
          "speaker_id": "2",
          "time": "20210908 11:07"
        }
