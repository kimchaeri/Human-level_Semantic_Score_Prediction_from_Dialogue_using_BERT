      "utterance": [
        {
          "id": "MDRW2100022037.1.1",
          "form": "좋아하는음식있으세요~?",
          "original_form": "좋아하는음식있으세요~?",
          "speaker_id": "1",
          "time": "20210829 01:52"
        },
        {
          "id": "MDRW2100022037.1.2",
          "form": "싫어하는거빼고다요",
          "original_form": "싫어하는거빼고다요",
          "speaker_id": "2",
          "time": "20210829 10:27"
        },
        {
          "id": "MDRW2100022037.1.3",
          "form": "뭘싫어하시나요?",
          "original_form": "뭘싫어하시나요?",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022037.1.4",
          "form": "음~ 식초나 케챱 마요네즈 들어간 거는 못먹어요",
          "original_form": "음~ 식초나 케챱 마요네즈 들어간 거는 못먹어요",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100022037.1.5",
          "form": "시큼상콤한걸 싫어하시는군요#",
          "original_form": "시큼상콤한걸 싫어하시는군요#",
          "speaker_id": "1",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100022037.1.6",
          "form": "네 감귤계는 좋아하는데 식초류는 쫌",
          "original_form": "네 감귤계는 좋아하는데 식초류는 쫌",
          "speaker_id": "2",
          "time": "20210830 09:26"
        },
        {
          "id": "MDRW2100022037.1.7",
          "form": "레몬도ㅡ날로 먹는 사람인데",
          "original_form": "레몬도ㅡ날로 먹는 사람인데",
          "speaker_id": "2",
          "time": "20210830 09:26"
        },
        {
          "id": "MDRW2100022037.1.8",
          "form": "아 ~ 그런분있죠 다 맛이 다르니 !!",
          "original_form": "아 ~ 그런분있죠 다 맛이 다르니 !!",
          "speaker_id": "1",
          "time": "20210830 09:27"
        },
        {
          "id": "MDRW2100022037.1.9",
          "form": "혹시 알러지나는 음식있으세요?",
          "original_form": "혹시 알러지나는 음식있으세요?",
          "speaker_id": "2",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100022037.1.10",
          "form": "특정음식에 알러지나는거 같지않아요!",
          "original_form": "특정음식에 알러지나는거 같지않아요!",
          "speaker_id": "1",
          "time": "20210830 10:42"
        },
        {
          "id": "MDRW2100022037.1.11",
          "form": "오~ 그래요 ? 요즘 알러지있는 분들이 많더라구요",
          "original_form": "오~ 그래요 ? 요즘 알러지있는 분들이 많더라구요",
          "speaker_id": "2",
          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100022037.1.12",
          "form": "안먹는 게 아니라 못먹는 패턴",
          "original_form": "안먹는 게 아니라 못먹는 패턴",
          "speaker_id": "2",
          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100022037.1.13",
          "form": "맞아요~ 알러지있으면 조심해야하죠ㅠ",
          "original_form": "맞아요~ 알러지있으면 조심해야하죠ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:48"
        },
        {
          "id": "MDRW2100022037.1.14",
          "form": "맛점하셨나요? 뭐드셨어요?",
          "original_form": "맛점하셨나요? 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100022037.1.15",
          "form": "네~배고파서 였는지 집에서 반찬꺼내 먹었는데 맛있게먹었네요ㅠㅠ",
          "original_form": "네~배고파서 였는지 집에서 반찬꺼내 먹었는데 맛있게먹었네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 15:02"
        },
        {
          "id": "MDRW2100022037.1.16",
          "form": "전. 떡볶이 먹었는데. 짜게 먹었는지 계속 커피마시네요",
          "original_form": "전. 떡볶이 먹었는데. 짜게 먹었는지 계속 커피마시네요",
          "speaker_id": "2",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100022037.1.17",
          "form": "음식이 좀잤나보네요ㅠ~",
          "original_form": "음식이 좀잤나보네요ㅠ~",
          "speaker_id": "1",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100022037.1.18",
          "form": "벌써저녁시간이에요ㅠ",
          "original_form": "벌써저녁시간이에요ㅠ",
          "speaker_id": "1",
          "time": "20210830 17:34"
        }
      ]
