      "utterance": [
        {
          "id": "MDRW2100014207.1.1",
          "form": "응 날씨 좋은데?",
          "original_form": "응 날씨 좋은데?",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014207.1.2",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014207.1.3",
          "form": "응 쭉 이 날씨면 좋겠다",
          "original_form": "응 쭉 이 날씨면 좋겠다",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014207.1.4",
          "form": "그러게 너무 덥지도 않고 비도 안 오면 딱 좋지",
          "original_form": "그러게 너무 덥지도 않고 비도 안 오면 딱 좋지",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014207.1.5",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014207.1.6",
          "form": "태풍 올라온다더니 걔는 갔나?",
          "original_form": "태풍 올라온다더니 걔는 갔나?",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014207.1.7",
          "form": "응 갔다는데 다행히",
          "original_form": "응 갔다는데 다행히",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014207.1.8",
          "form": "되게 조용히 지나간 거 같다",
          "original_form": "되게 조용히 지나간 거 같다",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014207.1.9",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014207.1.10",
          "form": "가을 장마라는 얘기가 있던데",
          "original_form": "가을 장마라는 얘기가 있던데",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014207.1.11",
          "form": "한창 장마 올 때 됬지",
          "original_form": "한창 장마 올 때 됬지",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014207.1.12",
          "form": "여름엔 집에만 있어서 장마여도 장마인 줄도 몰랐어",
          "original_form": "여름엔 집에만 있어서 장마여도 장마인 줄도 몰랐어",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014207.1.13",
          "form": "그니까 거의 안나가니까",
          "original_form": "그니까 거의 안나가니까",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014207.1.14",
          "form": "비 오는데 나가야 될 때 제일 짜증나는데",
          "original_form": "비 오는데 나가야 될 때 제일 짜증나는데",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014207.1.15",
          "form": "나는 학교 다닐 때 하굣길에 비 엄청 많이 와서 비 쫄딱 맞았던 거 생각나",
