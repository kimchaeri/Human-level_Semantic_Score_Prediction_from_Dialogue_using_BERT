      "utterance": [
        {
          "id": "MDRW2100002844.1.1",
          "form": "전 귤 정말 사랑합니다",
          "original_form": "전 귤 정말 사랑합니다",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002844.1.2",
          "form": "특히 겨울에 먹는 귤이요 ㅎ",
          "original_form": "특히 겨울에 먹는 귤이요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002844.1.3",
          "form": "귤 좋아하시는구나",
          "original_form": "귤 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002844.1.4",
          "form": "그럼 귤과 대부분 좋아하시나요?",
          "original_form": "그럼 귤과 대부분 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002844.1.5",
          "form": "네 대부분이요 name1님은 어떤 과일 좋아하세요?",
          "original_form": "네 대부분이요 &name1&님은 어떤 과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002844.1.6",
          "form": "저 망고되게 좋아했는데 최근에 실패한 후로는 사과가 좋더라구요",
          "original_form": "저 망고되게 좋아했는데 최근에 실패한 후로는 사과가 좋더라구요",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002844.1.7",
          "form": "아삭아삭하고 실패도 없고ㅎ",
          "original_form": "아삭아삭하고 실패도 없고ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002844.1.8",
          "form": "헐 망고 맛있죠 근데 계절을 타는 거 같기도 ㅎ",
          "original_form": "헐 망고 맛있죠 근데 계절을 타는 거 같기도 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002844.1.9",
          "form": "사과는 거의 맛있는 거 같아요",
          "original_form": "사과는 거의 맛있는 거 같아요",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002844.1.10",
          "form": "맞아요. 사과는 실패가없어요",
          "original_form": "맞아요. 사과는 실패가없어요",
          "speaker_id": "1",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002844.1.11",
          "form": "과일로 만든 음료나 청같은 것도 좋아하나요?",
          "original_form": "과일로 만든 음료나 청같은 것도 좋아하나요?",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002844.1.12",
          "form": "네 완전 좋아합니다",
          "original_form": "네 완전 좋아합니다",
          "speaker_id": "2",
          "time": "20210820 16:08"
        },
        {
          "id": "MDRW2100002844.1.13",
          "form": "저 드래곤후르츠 사랑해요",
          "original_form": "저 드래곤후르츠 사랑해요",
          "speaker_id": "2",
          "time": "20210820 16:08"
        },
        {
          "id": "MDRW2100002844.1.14",
          "form": "아 뭘 좀 아시네요",
          "original_form": "아 뭘 좀 아시네요",
          "speaker_id": "1",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002844.1.15",
          "form": "새콤하면서 톡톡씹히는 씨가 매력적이죠",
          "original_form": "새콤하면서 톡톡씹히는 씨가 매력적이죠",
          "speaker_id": "1",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002844.1.16",
          "form": "그냥 물에 타먹어도 맛있더라구요 ㅎ",
          "original_form": "그냥 물에 타먹어도 맛있더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002844.1.17",
          "form": "탄산수 넣어먹어도 맛있구요",
          "original_form": "탄산수 넣어먹어도 맛있구요",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002844.1.18",
          "form": "에이드처럼요",
          "original_form": "에이드처럼요",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002844.1.19",
          "form": "맞아요ㅋㅋ정말 뭘 아시는뷴이세요 ㅎ",
          "original_form": "맞아요ㅋㅋ정말 뭘 아시는뷴이세요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002844.1.20",
          "form": "맛있는건 놓칠 수 없으니까요",
          "original_form": "맛있는건 놓칠 수 없으니까요",
          "speaker_id": "1",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002844.1.21",
          "form": "과일은 진짜 어떻게 먹어도 맛있는 거 같아요",
          "original_form": "과일은 진짜 어떻게 먹어도 맛있는 거 같아요",
          "speaker_id": "1",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002844.1.22",
          "form": "그니까요!!!!! 과일 이야기하니까 먹거싶네요",
          "original_form": "그니까요!!!!! 과일 이야기하니까 먹거싶네요",
