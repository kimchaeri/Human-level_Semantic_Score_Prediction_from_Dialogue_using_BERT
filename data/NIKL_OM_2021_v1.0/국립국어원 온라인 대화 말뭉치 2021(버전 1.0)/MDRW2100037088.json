      "utterance": [
        {
          "id": "MDRW2100037088.1.1",
          "form": "저녁 뭐 드셔용,",
          "original_form": "저녁 뭐 드셔용,",
          "speaker_id": "1",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100037088.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100037088.1.3",
          "form": "좀 전에 미역국이랑 반찬이랑 먹었어요",
          "original_form": "좀 전에 미역국이랑 반찬이랑 먹었어요",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100037088.1.4",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100037088.1.5",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "1",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100037088.1.6",
          "form": "전이랑 된장국 먹었어요!",
          "original_form": "전이랑 된장국 먹었어요!",
          "speaker_id": "1",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100037088.1.7",
          "form": "맛있으셨겠어요! 전은 직접 만드신 건가요?",
          "original_form": "맛있으셨겠어요! 전은 직접 만드신 건가요?",
          "speaker_id": "2",
          "time": "20210831 19:27"
        },
        {
          "id": "MDRW2100037088.1.8",
          "form": "엄마표 입니당!",
          "original_form": "엄마표 입니당!",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100037088.1.9",
          "form": "name2님은 요리하시나용?",
          "original_form": "&name2&님은 요리하시나용?",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100037088.1.10",
          "form": "가끔 하는데 저도 할머니가 대부분 해주세요ㅎㅎ",
          "original_form": "가끔 하는데 저도 할머니가 대부분 해주세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100037088.1.11",
          "form": "어떤 전 드셨나요?",
          "original_form": "어떤 전 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100037088.1.12",
          "form": "할머니의 손맛은 따라갈 수 없죵",
          "original_form": "할머니의 손맛은 따라갈 수 없죵",
          "speaker_id": "1",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100037088.1.13",
          "form": "김치전에 오징어 추가해서 먹었어용",
          "original_form": "김치전에 오징어 추가해서 먹었어용",
          "speaker_id": "1",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100037088.1.14",
          "form": "김치전 너무 맛있죠!",
          "original_form": "김치전 너무 맛있죠!",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100037088.1.15",
          "form": "비오는 날이라 더 맛있었을 거 같아요",
          "original_form": "비오는 날이라 더 맛있었을 거 같아요",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100037088.1.16",
          "form": "넹넹!",
          "original_form": "넹넹!",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100037088.1.17",
          "form": "비오는 날 전만큼 최고인게 없긴하죱",
          "original_form": "비오는 날 전만큼 최고인게 없긴하죱",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100037088.1.18",
          "form": "김치전이랑 파전 중에 어떤 걸 더 좋아하세요?",
          "original_form": "김치전이랑 파전 중에 어떤 걸 더 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100037088.1.19",
          "form": "저는 김치전이욥!",
          "original_form": "저는 김치전이욥!",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100037088.1.20",
          "form": "밖에서 먹는거는 해물파전을 더 많이 먹어요",
          "original_form": "밖에서 먹는거는 해물파전을 더 많이 먹어요",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100037088.1.21",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100037088.1.22",
          "form": "저도 김치전 더 좋아해요",
          "original_form": "저도 김치전 더 좋아해요",
          "speaker_id": "2",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100037088.1.23",
          "form": "밖에서 먹을 때는 모둠전 시켜서 이것저것 먹는 거 같아요ㅎㅎ",
          "original_form": "밖에서 먹을 때는 모둠전 시켜서 이것저것 먹는 거 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100037088.1.24",
          "form": "모둠전!",
          "original_form": "모둠전!",
          "speaker_id": "1",
          "time": "20210831 19:37"
        },
        {
          "id": "MDRW2100037088.1.25",
