      "utterance": [
        {
          "id": "MDRW2100021916.1.1",
          "form": "아니 안갔어",
          "original_form": "아니 안갔어",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021916.1.2",
          "form": "오늘 날씨 딱 적당했던 거 같은데",
          "original_form": "오늘 날씨 딱 적당했던 거 같은데",
          "speaker_id": "1",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021916.1.3",
          "form": "근데 주말이라 사람 많을거 같아서",
          "original_form": "근데 주말이라 사람 많을거 같아서",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021916.1.4",
          "form": "맞아 좀 많긴 하더라",
          "original_form": "맞아 좀 많긴 하더라",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021916.1.5",
          "form": "주말은 피해야지",
          "original_form": "주말은 피해야지",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021916.1.6",
          "form": "주말엔 나가서 좋을 게 없는 거 같아",
          "original_form": "주말엔 나가서 좋을 게 없는 거 같아",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021916.1.7",
          "form": "차라리 평일에 나가야지",
          "original_form": "차라리 평일에 나가야지",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021916.1.8",
          "form": "평일은 확실히 사람이 좀 적지 근데 오늘 날씨가 좋았어",
          "original_form": "평일은 확실히 사람이 좀 적지 근데 오늘 날씨가 좋았어",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021916.1.9",
          "form": "근데 요즘 날씨 괜찮지 않나?",
          "original_form": "근데 요즘 날씨 괜찮지 않나?",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021916.1.10",
          "form": "요즘 날씨 괜찮지 않나?",
          "original_form": "요즘 날씨 괜찮지 않나?",
          "speaker_id": "2",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021916.1.11",
          "form": "맞아 요즘 날씨 딱 적당하지",
          "original_form": "맞아 요즘 날씨 딱 적당하지",
          "speaker_id": "1",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021916.1.12",
          "form": "그래서 좋은거 같아",
          "original_form": "그래서 좋은거 같아",
          "speaker_id": "2",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021916.1.13",
