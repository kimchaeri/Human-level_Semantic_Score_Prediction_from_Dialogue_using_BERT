      "utterance": [
        {
          "id": "MDRW2100018098.1.1",
          "form": "최근 구매 희망하시는 물건 있으세요?",
          "original_form": "최근 구매 희망하시는 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018098.1.2",
          "form": "글쎄요 최근에 쇼핑을 너무 해서요 ㅋㅋㅋ",
          "original_form": "글쎄요 최근에 쇼핑을 너무 해서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018098.1.3",
          "form": "ㅋㅋ최근 구매하신건 어떤거에요?",
          "original_form": "ㅋㅋ최근 구매하신건 어떤거에요?",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018098.1.4",
          "form": "가을 옷 구입했어요",
          "original_form": "가을 옷 구입했어요",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018098.1.5",
          "form": "날씨가 좋아지니 옷 구매할때죠",
          "original_form": "날씨가 좋아지니 옷 구매할때죠",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018098.1.6",
          "form": "맞아요 가을옷 장만하셨어요??",
          "original_form": "맞아요 가을옷 장만하셨어요??",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018098.1.7",
          "form": "아직요! 최근에 음식만 계속 산거같아요ㅋㅋ",
          "original_form": "아직요! 최근에 음식만 계속 산거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018098.1.8",
          "form": "음식은 끝없는 굴레죠..",
          "original_form": "음식은 끝없는 굴레죠..",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018098.1.9",
          "form": "뭐사셨어요??",
          "original_form": "뭐사셨어요??",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018098.1.10",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018098.1.11",
          "form": "냉동야채들이요ㅋㅋ",
          "original_form": "냉동야채들이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018098.1.12",
          "form": "야채들 쟁여두면 든든할거같아요",
          "original_form": "야채들 쟁여두면 든든할거같아요",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018098.1.13",
          "form": "맞아요ㅋㅋ 귀찮아서 샀는데 꽤 괜찮더라구요",
          "original_form": "맞아요ㅋㅋ 귀찮아서 샀는데 꽤 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018098.1.14",
          "form": "냉동식품도 계속 쟁여두게 되는거같아요",
          "original_form": "냉동식품도 계속 쟁여두게 되는거같아요",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018098.1.15",
          "form": "그쵸그쵸 다음엔 냉동 새우 사려구요",
          "original_form": "그쵸그쵸 다음엔 냉동 새우 사려구요",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018098.1.16",
