      "utterance": [
        {
          "id": "MDRW2100009245.1.1",
          "form": "진정령을 보신 모양이군요",
          "original_form": "진정령을 보신 모양이군요",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009245.1.2",
          "form": "솔직히 끝까지 보지는 못했는데",
          "original_form": "솔직히 끝까지 보지는 못했는데",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009245.1.3",
          "form": "둘은 사석에서도 사귀던데요?",
          "original_form": "둘은 사석에서도 사귀던데요?",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009245.1.4",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009245.1.5",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ사석에서도 사귄다가 뭔소리야...",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ사석에서도 사귄다가 뭔소리야...",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009245.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ하튼 \"찐\"이다...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ하튼 \"찐\"이다...",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009245.1.7",
          "form": "아니 저도 다 보지는 못했는데",
          "original_form": "아니 저도 다 보지는 못했는데",
          "speaker_id": "1",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009245.1.8",
          "form": "그 비하인드만 봤거든요",
          "original_form": "그 비하인드만 봤거든요",
          "speaker_id": "1",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009245.1.9",
          "form": "하 참 숨길생각을 안하던데요",
          "original_form": "하 참 숨길생각을 안하던데요",
          "speaker_id": "1",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009245.1.10",
          "form": "아니 솔직히 그정도면 중국이 걍 모른척 하는거라고요",
          "original_form": "아니 솔직히 그정도면 중국이 걍 모른척 하는거라고요",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009245.1.11",
          "form": "루한아 돌아와라 한국은...괜찮더라",
          "original_form": "루한아 돌아와라 한국은...괜찮더라",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009245.1.12",
          "form": "성동애강국",
          "original_form": "성동애강국",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009245.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
