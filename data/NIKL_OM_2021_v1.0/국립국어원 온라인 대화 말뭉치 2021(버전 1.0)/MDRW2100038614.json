      "utterance": [
        {
          "id": "MDRW2100038614.1.1",
          "form": "음료수 뭐좋아하세요",
          "original_form": "음료수 뭐좋아하세요",
          "speaker_id": "1",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100038614.1.2",
          "form": "전 솔의 눈이요",
          "original_form": "전 솔의 눈이요",
          "speaker_id": "1",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100038614.1.3",
          "form": "아저씨같아여",
          "original_form": "아저씨같아여",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100038614.1.4",
          "form": "전 녹차라떼요 말차라떼",
          "original_form": "전 녹차라떼요 말차라떼",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100038614.1.5",
          "form": "아줌마에요",
          "original_form": "아줌마에요",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100038614.1.6",
          "form": "솔의눈 매대에 잘 안보이던데요",
          "original_form": "솔의눈 매대에 잘 안보이던데요",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100038614.1.7",
          "form": "있어요",
          "original_form": "있어요",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100038614.1.8",
          "form": "여름에 얼음타서 드셔보세요",
          "original_form": "여름에 얼음타서 드셔보세요",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100038614.1.9",
          "form": "먹어볼케요 여름이 가버렸지만",
          "original_form": "먹어볼케요 여름이 가버렸지만",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100038614.1.10",
          "form": "가격도 안비싸니 한번 마셔봐요",
          "original_form": "가격도 안비싸니 한번 마셔봐요",
          "speaker_id": "1",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100038614.1.11",
          "form": "조아용",
          "original_form": "조아용",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100038614.1.12",
          "form": "꼭이요ㅋㅋ",
          "original_form": "꼭이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100038614.1.13",
          "form": "저:솔의눈 보면 인증샷보내드림ㅋ",
          "original_form": "저:솔의눈 보면 인증샷보내드림ㅋ",
          "speaker_id": "2",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100038614.1.14",
          "form": "인증샷 기다리겠습니다ㅋㅋㅋㅋ",
          "original_form": "인증샷 기다리겠습니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038614.1.15",
          "form": "얼음도 넣어드세요",
          "original_form": "얼음도 넣어드세요",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038614.1.16",
          "form": "ㅋㅋ 영 안비드만",
          "original_form": "ㅋㅋ 영 안비드만",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038614.1.17",
          "form": "왜죠",
          "original_form": "왜죠",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038614.1.18",
          "form": "언니네 가게는 파능가",
          "original_form": "언니네 가게는 파능가",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038614.1.19",
          "form": "팔죠ㅋㅋ",
          "original_form": "팔죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038614.1.20",
          "form": "잘찾아봐요!",
          "original_form": "잘찾아봐요!",
          "speaker_id": "1",
