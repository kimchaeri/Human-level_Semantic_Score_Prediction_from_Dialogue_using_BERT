        },
        {
          "id": "MDRW2100033250.1.2",
          "form": "30대 후반이라 일한지 10년 넘었습니다 ㅋㅋㅋ",
          "original_form": "30대 후반이라 일한지 10년 넘었습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100033250.1.3",
          "form": "오래되셨네요 적성에 맞으시나요",
          "original_form": "오래되셨네요 적성에 맞으시나요",
          "speaker_id": "1",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100033250.1.4",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100033250.1.5",
          "form": "글쎄요",
          "original_form": "글쎄요",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100033250.1.6",
          "form": "그냥 버티는거죠 머",
          "original_form": "그냥 버티는거죠 머",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100033250.1.7",
          "form": "저도 비슷합니다...ㅎ",
          "original_form": "저도 비슷합니다...ㅎ",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100033250.1.8",
          "form": "맞벌이 하세요?",
          "original_form": "맞벌이 하세요?",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100033250.1.9",
          "form": "네 맞 벌 이 입니다",
          "original_form": "네 맞 벌 이 입니다",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100033250.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100033250.1.11",
          "form": "퇴근하고 저녁드시고 오신건가요?",
          "original_form": "퇴근하고 저녁드시고 오신건가요?",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100033250.1.12",
          "form": "아니요 퇴근하고 집에서 저녁먹습니다",
          "original_form": "아니요 퇴근하고 집에서 저녁먹습니다",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100033250.1.13",
          "form": "전 방금 먹었는데",
          "original_form": "전 방금 먹었는데",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100033250.1.14",
          "form": "비와서 퇴근길이 막히더라구요",
          "original_form": "비와서 퇴근길이 막히더라구요",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100033250.1.15",
          "form": "늦게 오셨군요 퇴근길도 힘드셨겠어요",
          "original_form": "늦게 오셨군요 퇴근길도 힘드셨겠어요",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100033250.1.16",
          "form": "5시반 퇴근인데",
          "original_form": "5시반 퇴근인데",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100033250.1.17",
          "form": "6시쯤 나왔더니",
          "original_form": "6시쯤 나왔더니",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100033250.1.18",
          "form": "바로 막히더라구요",
          "original_form": "바로 막히더라구요",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100033250.1.19",
          "form": "비오는날은 출퇴근이 몇배 복잡하죠 ㅠ",
          "original_form": "비오는날은 출퇴근이 몇배 복잡하죠 ㅠ",
