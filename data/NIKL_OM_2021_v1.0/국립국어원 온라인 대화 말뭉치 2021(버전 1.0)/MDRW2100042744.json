      "utterance": [
        {
          "id": "MDRW2100042744.1.1",
          "form": "음 손이잘안가요",
          "original_form": "음 손이잘안가요",
          "speaker_id": "2",
          "time": "20210907 08:17"
        },
        {
          "id": "MDRW2100042744.1.2",
          "form": "레샤는요",
          "original_form": "레샤는요",
          "speaker_id": "1",
          "time": "20210907 08:17"
        },
        {
          "id": "MDRW2100042744.1.3",
          "form": "전안조아하는데 가족들이조아해서 꼭삽니다",
          "original_form": "전안조아하는데 가족들이조아해서 꼭삽니다",
          "speaker_id": "2",
          "time": "20210907 08:21"
        },
        {
          "id": "MDRW2100042744.1.4",
          "form": "님의 최애 맛은 ??",
          "original_form": "님의 최애 맛은 ??",
          "speaker_id": "1",
          "time": "20210907 08:21"
        },
        {
          "id": "MDRW2100042744.1.5",
          "form": "흐음 최근엔 북극곰폴라베어용",
          "original_form": "흐음 최근엔 북극곰폴라베어용",
          "speaker_id": "2",
          "time": "20210907 08:24"
        },
        {
          "id": "MDRW2100042744.1.6",
          "form": "한정메뉴죠?",
          "original_form": "한정메뉴죠?",
          "speaker_id": "1",
          "time": "20210907 08:24"
        },
        {
          "id": "MDRW2100042744.1.7",
          "form": "넹 여름에만 ㅠㅠ",
          "original_form": "넹 여름에만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 08:24"
        },
        {
          "id": "MDRW2100042744.1.8",
          "form": "그럼 그거말고는용?",
          "original_form": "그럼 그거말고는용?",
          "speaker_id": "1",
          "time": "20210907 08:25"
        },
        {
          "id": "MDRW2100042744.1.9",
          "form": "그러규나선 사실 최애는 없어용 베스킨질림 ㅠ",
          "original_form": "그러규나선 사실 최애는 없어용 베스킨질림 ㅠ",
          "speaker_id": "2",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042744.1.10",
          "form": "걍 사오면 먹긴하는데",
          "original_form": "걍 사오면 먹긴하는데",
          "speaker_id": "2",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042744.1.11",
          "form": "베스킨보다 초코퍼지가 더 맛닛어요 쫀득",
          "original_form": "베스킨보다 초코퍼지가 더 맛닛어요 쫀득",
          "speaker_id": "2",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042744.1.12",
          "form": "글쿠나 그렇다면 벤엔제리스?",
          "original_form": "글쿠나 그렇다면 벤엔제리스?",
          "speaker_id": "1",
          "time": "20210907 10:42"
        },
        {
          "id": "MDRW2100042744.1.13",
          "form": "밴앤제리도 맛있긴한디 가끔만 끌려요 ㅋㅋ",
          "original_form": "밴앤제리도 맛있긴한디 가끔만 끌려요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:43"
        },
        {
          "id": "MDRW2100042744.1.14",
          "form": "오 그렇다면 하겐다즈?",
          "original_form": "오 그렇다면 하겐다즈?",
          "speaker_id": "1",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100042744.1.15",
          "form": "음 아 굳이 나누자면 하겐다즈가 더 좋긴하네용",
          "original_form": "음 아 굳이 나누자면 하겐다즈가 더 좋긴하네용",
          "speaker_id": "2",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100042744.1.16",
          "form": "뭔가 요즘 유크림베이스가 너무 느끼하달까",
          "original_form": "뭔가 요즘 유크림베이스가 너무 느끼하달까",
          "speaker_id": "2",
          "time": "20210907 10:46"
        }
      ]
