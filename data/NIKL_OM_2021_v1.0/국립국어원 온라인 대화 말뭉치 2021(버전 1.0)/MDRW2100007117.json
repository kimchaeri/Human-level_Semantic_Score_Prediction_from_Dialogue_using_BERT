      "utterance": [
        {
          "id": "MDRW2100007117.1.1",
          "form": "확실히 외모도 좋으면 좋지만",
          "original_form": "확실히 외모도 좋으면 좋지만",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007117.1.2",
          "form": "그외의 매력도 중요한 것 같아요",
          "original_form": "그외의 매력도 중요한 것 같아요",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007117.1.3",
          "form": "ㅋㅋㅋㅋㅋ맞아요 어쩔 땐 좀 잘생겨 보일 때도 있고",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 어쩔 땐 좀 잘생겨 보일 때도 있고",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007117.1.4",
          "form": "그런거죠!",
          "original_form": "그런거죠!",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007117.1.5",
          "form": "결혼이라 저랑은 좀 먼 얘기 같아요",
          "original_form": "결혼이라 저랑은 좀 먼 얘기 같아요",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007117.1.6",
          "form": "주변에 결혼하는 친구들은 있는데",
          "original_form": "주변에 결혼하는 친구들은 있는데",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007117.1.7",
          "form": "저는 딱히 아직 생각이 없네요",
          "original_form": "저는 딱히 아직 생각이 없네요",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007117.1.8",
          "form": "네... 저는 연애는 생각 있어도",
          "original_form": "네... 저는 연애는 생각 있어도",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007117.1.9",
          "form": "결혼은 생각 없는 것 같아요",
          "original_form": "결혼은 생각 없는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007117.1.10",
          "form": "결혼 안 하고 엄마랑 같이 살겠더고 하는데",
          "original_form": "결혼 안 하고 엄마랑 같이 살겠더고 하는데",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007117.1.11",
          "form": "이제 결혼 얘기 많이 들어서",
          "original_form": "이제 결혼 얘기 많이 들어서",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007117.1.12",
          "form": "헉 그렇군요ㅠㅠ",
          "original_form": "헉 그렇군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007117.1.13",
          "form": "저의 비혼주의를 어떻게 어필할지 고민중이에요",
          "original_form": "저의 비혼주의를 어떻게 어필할지 고민중이에요",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007117.1.14",
          "form": "코로나 덕분에 친척들은 안 모여거",
          "original_form": "코로나 덕분에 친척들은 안 모여거",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007117.1.15",
          "form": "다행이죠...",
          "original_form": "다행이죠...",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007117.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007117.1.17",
          "form": "그렇렜네요ㅠㅠ",
          "original_form": "그렇렜네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007117.1.18",
          "form": "저도 비혼주의르",
          "original_form": "저도 비혼주의르",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007117.1.19",
          "form": "결혼적령기 됐을 때 어떻게 처리할지 고민이네여...",
          "original_form": "결혼적령기 됐을 때 어떻게 처리할지 고민이네여...",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007117.1.20",
          "form": "근데 점점 결혼이 꼭 해야하는 게 아닌 거처럼 되어서",
          "original_form": "근데 점점 결혼이 꼭 해야하는 게 아닌 거처럼 되어서",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007117.1.21",
          "form": "조금씩 변하고 있는 거 같아요",
          "original_form": "조금씩 변하고 있는 거 같아요",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007117.1.22",
          "form": "저는 아직 덜 변해서 그런 말을 듣는거구",
          "original_form": "저는 아직 덜 변해서 그런 말을 듣는거구",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007117.1.23",
          "form": "그건 맞아요",
          "original_form": "그건 맞아요",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007117.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007117.1.25",
          "form": "요새 비혼식도 꽤 하고",
          "original_form": "요새 비혼식도 꽤 하고",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007117.1.26",
          "form": "결혼이 당연하지 않은 걸 인정하는 사람들이 늘어나서 좋아요",
          "original_form": "결혼이 당연하지 않은 걸 인정하는 사람들이 늘어나서 좋아요",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007117.1.27",
          "form": "저도 5년 후에 친구들이랑 비혼식해서 어머니아버지의 축의금 회수하기 프로젝트를...",
          "original_form": "저도 5년 후에 친구들이랑 비혼식해서 어머니아버지의 축의금 회수하기 프로젝트를...",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007117.1.28",
          "form": "요즘엔 혼자서도 사는 게 어렵지 않아서",
          "original_form": "요즘엔 혼자서도 사는 게 어렵지 않아서",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007117.1.29",
          "form": "비혼 나쁘지 않은 거 같아요",
          "original_form": "비혼 나쁘지 않은 거 같아요",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007117.1.30",
          "form": "비혼 친구들끼리 서로 챙기고 살면",
          "original_form": "비혼 친구들끼리 서로 챙기고 살면",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007117.1.31",
          "form": "재밌을 거 같고 그러네요!",
          "original_form": "재밌을 거 같고 그러네요!",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007117.1.32",
          "form": "맞아요ㅠ 저도 잘 맞는 친구들이랑 동거 아니면 가까이 자취하는 걸로 해서 다같이 밥도 해먹고 장도 보고 하는 거 꿈꾸고 있어요",
          "original_form": "맞아요ㅠ 저도 잘 맞는 친구들이랑 동거 아니면 가까이 자취하는 걸로 해서 다같이 밥도 해먹고 장도 보고 하는 거 꿈꾸고 있어요",
          "speaker_id": "1",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100007117.1.33",
          "form": "와 너무 좋을 거 같아요! 돈은 많이 모아놔야겠지만 어찌보면 그렇게 살다보면 친구도 가족 되는 거 아닐까 싶어서 너무 좋네요!",
          "original_form": "와 너무 좋을 거 같아요! 돈은 많이 모아놔야겠지만 어찌보면 그렇게 살다보면 친구도 가족 되는 거 아닐까 싶어서 너무 좋네요!",
          "speaker_id": "2",
          "time": "20210822 17:35"
        }
      ]
    }
