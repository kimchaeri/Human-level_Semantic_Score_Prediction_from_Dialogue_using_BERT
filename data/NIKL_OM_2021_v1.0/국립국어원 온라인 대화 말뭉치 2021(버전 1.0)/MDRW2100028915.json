      "utterance": [
        {
          "id": "MDRW2100028915.1.1",
          "form": "열날때도 패치 붙으셔요?",
          "original_form": "열날때도 패치 붙으셔요?",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028915.1.2",
          "form": "아뇨 안붙혀요 다 떼버려서",
          "original_form": "아뇨 안붙혀요 다 떼버려서",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028915.1.3",
          "form": "티키타가가 중요함요ㅎㅎㅎ",
          "original_form": "티키타가가 중요함요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028915.1.4",
          "form": "아아 왜요?ㅎ",
          "original_form": "아아 왜요?ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100028915.1.5",
          "form": "걸리는지 다 떼버려서 그냥 찜질해요 미온슈",
          "original_form": "걸리는지 다 떼버려서 그냥 찜질해요 미온슈",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100028915.1.6",
          "form": "아아아아ㅎㅎ",
          "original_form": "아아아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100028915.1.7",
          "form": "애기가 떼는군요ㅎ",
          "original_form": "애기가 떼는군요ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100028915.1.8",
          "form": "네 싫어해요  밴드는 크니 그나마 나은데",
          "original_form": "네 싫어해요  밴드는 크니 그나마 나은데",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100028915.1.9",
          "form": "맞아요 ㅎ",
          "original_form": "맞아요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028915.1.10",
          "form": "우리애도그래요 ㅡㅎㅎ",
          "original_form": "우리애도그래요 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028915.1.11",
          "form": "애기가 병원에는 입원 잘했어요?ㅎ",
          "original_form": "애기가 병원에는 입원 잘했어요?ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028915.1.12",
          "form": "의사마다 열패치 효과 없다는 뷴도 많이 봐서 ㅎㅎ",
          "original_form": "의사마다 열패치 효과 없다는 뷴도 많이 봐서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028915.1.13",
          "form": "아그렇군요ㅎㅎ 전 그냥 붙여요ㅎ 일종에 부적같다고해야하나?",
          "original_form": "아그렇군요ㅎㅎ 전 그냥 붙여요ㅎ 일종에 부적같다고해야하나?",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028915.1.14",
          "form": "빨리 열내려라ㅎㅎ",
          "original_form": "빨리 열내려라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028915.1.15",
          "form": "ㅎㅎㅎ  그럴수도 잇죠",
          "original_form": "ㅎㅎㅎ  그럴수도 잇죠",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028915.1.16",
