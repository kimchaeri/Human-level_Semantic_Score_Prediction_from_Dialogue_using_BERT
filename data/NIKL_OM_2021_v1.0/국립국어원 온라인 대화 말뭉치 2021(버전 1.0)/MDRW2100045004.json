      "utterance": [
        {
          "id": "MDRW2100045004.1.1",
          "form": "??? 무슨 신메뉴가 나왔어요??",
          "original_form": "??? 무슨 신메뉴가 나왔어요??",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045004.1.2",
          "form": "민초",
          "original_form": "민초",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045004.1.3",
          "form": "같은거 으ㅡㅡ",
          "original_form": "같은거 으ㅡㅡ",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045004.1.4",
          "form": "? 민초파들이 또 ;;;",
          "original_form": "? 민초파들이 또 ;;;",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045004.1.5",
          "form": "그니까여",
          "original_form": "그니까여",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045004.1.6",
          "form": "인절미랑 치ㅖ최고",
          "original_form": "인절미랑 치ㅖ최고",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045004.1.7",
          "form": "반민초파 시죠?",
          "original_form": "반민초파 시죠?",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045004.1.8",
          "form": "그정돈 아님",
          "original_form": "그정돈 아님",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045004.1.9",
          "form": "주면먹는파",
          "original_form": "주면먹는파",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045004.1.10",
          "form": "아 처먹파는 인정",
          "original_form": "아 처먹파는 인정",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045004.1.11",
          "form": "근대 민초빙수는 좀...",
          "original_form": "근대 민초빙수는 좀...",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045004.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045004.1.13",
          "form": "겸상 못할 뻔 했네요",
          "original_form": "겸상 못할 뻔 했네요",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045004.1.14",
          "form": "기왕이면 치케빙수로 처먹자",
          "original_form": "기왕이면 치케빙수로 처먹자",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045004.1.15",
          "form": "치약에 초코를 더한 빙수 ... 댓츠 노노",
          "original_form": "치약에 초코를 더한 빙수 ... 댓츠 노노",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045004.1.16",
          "form": "마카롱이나 메론빙수는 ㄱㅊ",
          "original_form": "마카롱이나 메론빙수는 ㄱㅊ",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045004.1.17",
          "form": "치케도 ;; 별로 인절미 빙수가 최고",
          "original_form": "치케도 ;; 별로 인절미 빙수가 최고",
