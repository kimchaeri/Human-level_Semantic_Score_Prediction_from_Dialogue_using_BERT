      "utterance": [
        {
          "id": "MDRW2100003271.1.1",
          "form": "여행 좋아하세요???",
          "original_form": "여행 좋아하세요???",
          "speaker_id": "1",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003271.1.2",
          "form": "네 저 좋아해요!",
          "original_form": "네 저 좋아해요!",
          "speaker_id": "2",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003271.1.3",
          "form": "제주도 가고 싶어요ㅠㅠ",
          "original_form": "제주도 가고 싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003271.1.4",
          "form": "아~~~ 제주도 좋죠 ㅠㅠ",
          "original_form": "아~~~ 제주도 좋죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003271.1.5",
          "form": "또 가도 또 좋은",
          "original_form": "또 가도 또 좋은",
          "speaker_id": "1",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003271.1.6",
          "form": "사실 다녀온지 얼마 안 됐는데",
          "original_form": "사실 다녀온지 얼마 안 됐는데",
          "speaker_id": "2",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003271.1.7",
          "form": "여름에 가고 싶었거든요",
          "original_form": "여름에 가고 싶었거든요",
          "speaker_id": "2",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003271.1.8",
          "form": "원래 휴가때 부산 가려고 했는데 장렬히 취소됐어요",
          "original_form": "원래 휴가때 부산 가려고 했는데 장렬히 취소됐어요",
          "speaker_id": "2",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003271.1.9",
          "form": "하.. 사실 저는 부산 7월에 다녀왔어요 ㅠㅠㅋㅋㅋ",
          "original_form": "하.. 사실 저는 부산 7월에 다녀왔어요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003271.1.10",
          "form": "숙소를 너무 미리 잡아놨는지 환불불가는 못 놓겠더라구요 ㅠㅠ",
          "original_form": "숙소를 너무 미리 잡아놨는지 환불불가는 못 놓겠더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003271.1.11",
          "form": "아 맞아요ㅠㅠㅠ환불불가면 저라도 다녀왔을 거 같아요",
          "original_form": "아 맞아요ㅠㅠㅠ환불불가면 저라도 다녀왔을 거 같아요",
          "speaker_id": "2",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003271.1.12",
          "form": "부산 어디 다녀오셨어요?",
          "original_form": "부산 어디 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210820 21:52"
        },
        {
          "id": "MDRW2100003271.1.13",
          "form": "가긴 가되 돌아다니는거라도 자제하자싶어서 광안리에서만 있었어요 ㅋㅋㅋ",
          "original_form": "가긴 가되 돌아다니는거라도 자제하자싶어서 광안리에서만 있었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:52"
        },
        {
          "id": "MDRW2100003271.1.14",
          "form": "광안대교 바로 앞에 숙소들이 참 많더라구요",
          "original_form": "광안대교 바로 앞에 숙소들이 참 많더라구요",
          "speaker_id": "1",
          "time": "20210820 21:53"
        },
        {
          "id": "MDRW2100003271.1.15",
          "form": "그럼 숙소에서 광안대교 보였겠어요",
          "original_form": "그럼 숙소에서 광안대교 보였겠어요",
          "speaker_id": "2",
          "time": "20210820 21:54"
        },
        {
          "id": "MDRW2100003271.1.16",
          "form": "바다뷰 너무 좋아요",
          "original_form": "바다뷰 너무 좋아요",
          "speaker_id": "2",
          "time": "20210820 21:54"
        },
        {
          "id": "MDRW2100003271.1.17",
          "form": "네네 ㅜ 뷰 하나 보고 간거같아요 거의 ㅋㅋㅋ",
          "original_form": "네네 ㅜ 뷰 하나 보고 간거같아요 거의 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:56"
        },
        {
          "id": "MDRW2100003271.1.18",
          "form": "제주도도 진짜 가고싶네요",
          "original_form": "제주도도 진짜 가고싶네요",
          "speaker_id": "1",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003271.1.19",
          "form": "저두요ㅠㅠ저 제주도가서 해물라면 먹어보고 싶어요",
          "original_form": "저두요ㅠㅠ저 제주도가서 해물라면 먹어보고 싶어요",
          "speaker_id": "2",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003271.1.20",
          "form": "다 아는 맛이라는데 아는 맛이 맛있잖아요",
          "original_form": "다 아는 맛이라는데 아는 맛이 맛있잖아요",
          "speaker_id": "2",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003271.1.21",
          "form": "그쵸... 아는맛이라 미치는거죠",
          "original_form": "그쵸... 아는맛이라 미치는거죠",
          "speaker_id": "1",
          "time": "20210820 22:00"
        },
        {
          "id": "MDRW2100003271.1.22",
          "form": "모르면 솔직히 그 맛 어떨까에서 끝나는데 알면 힘들어요 ㅎㅎ",
          "original_form": "모르면 솔직히 그 맛 어떨까에서 끝나는데 알면 힘들어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003271.1.23",
          "form": "맞아요 차라리 모르는 맛이 낫지ㅠㅠ",
          "original_form": "맞아요 차라리 모르는 맛이 낫지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003271.1.24",
          "form": "저 빵도 좋아해서 빵 여행도 가보고 싶어요",
          "original_form": "저 빵도 좋아해서 빵 여행도 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003271.1.25",
          "form": "오 ㅎㅎ 빵 좋죠 유명 빵집 지역 곳곳에 많잖아요",
          "original_form": "오 ㅎㅎ 빵 좋죠 유명 빵집 지역 곳곳에 많잖아요",
          "speaker_id": "1",
          "time": "20210820 22:24"
        },
        {
          "id": "MDRW2100003271.1.26",
          "form": "아직 전 성심당 못가봤어요 ㅠㅠ",
          "original_form": "아직 전 성심당 못가봤어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 22:24"
        }
      ]
