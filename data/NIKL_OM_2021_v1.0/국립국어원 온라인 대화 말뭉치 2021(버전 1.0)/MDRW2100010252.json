      "utterance": [
        {
          "id": "MDRW2100010252.1.1",
          "form": "안녕하세요 name2 님!",
          "original_form": "안녕하세요 &name2& 님!",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010252.1.2",
          "form": "이번 올림픽 보셨나요?",
          "original_form": "이번 올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010252.1.3",
          "form": "네! 처음에는 관심 없다가 점점 보게됐어요",
          "original_form": "네! 처음에는 관심 없다가 점점 보게됐어요",
          "speaker_id": "2",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010252.1.4",
          "form": "혹시 어떤 경기 보셨어요?",
          "original_form": "혹시 어떤 경기 보셨어요?",
          "speaker_id": "2",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010252.1.5",
          "form": "배구랑 양궁은 거의 다 봤고 펜싱도 좀 봤어요!",
          "original_form": "배구랑 양궁은 거의 다 봤고 펜싱도 좀 봤어요!",
          "speaker_id": "1",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010252.1.6",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010252.1.7",
          "form": "저도 비슷해요 ㅎㅎㅎ",
          "original_form": "저도 비슷해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010252.1.8",
          "form": "그리고 클라이밍도 봤어요!",
          "original_form": "그리고 클라이밍도 봤어요!",
          "speaker_id": "2",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010252.1.9",
          "form": "아 저도 클라이밍 하이라이트 영상은 챙겨봤어요!",
          "original_form": "아 저도 클라이밍 하이라이트 영상은 챙겨봤어요!",
          "speaker_id": "1",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010252.1.10",
          "form": "진짜 대단하더라고요",
          "original_form": "진짜 대단하더라고요",
          "speaker_id": "1",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100010252.1.11",
          "form": "특히 재밌게 본 경기 있으셨나요?",
          "original_form": "특히 재밌게 본 경기 있으셨나요?",
          "speaker_id": "1",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100010252.1.12",
          "form": "저는 제일 재밌었던건 아무래도",
          "original_form": "저는 제일 재밌었던건 아무래도",
          "speaker_id": "2",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100010252.1.13",
          "form": "여자배구 한일전이었어요!",
          "original_form": "여자배구 한일전이었어요!",
          "speaker_id": "2",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100010252.1.14",
          "form": "아 너무 재밌었죠!",
          "original_form": "아 너무 재밌었죠!",
          "speaker_id": "1",
          "time": "20210823 16:22"
        },
        {
          "id": "MDRW2100010252.1.15",
          "form": "생방송으로 보셨나요?",
          "original_form": "생방송으로 보셨나요?",
          "speaker_id": "1",
          "time": "20210823 16:22"
        },
        {
          "id": "MDRW2100010252.1.16",
          "form": "네네 앞부분은 놓치고 뒷부분만 봤어요!",
          "original_form": "네네 앞부분은 놓치고 뒷부분만 봤어요!",
          "speaker_id": "2",
          "time": "20210823 16:23"
        },
        {
          "id": "MDRW2100010252.1.17",
          "form": "너무 직접가서 보고싶더라구요ㅠㅠ",
          "original_form": "너무 직접가서 보고싶더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 16:23"
        },
        {
          "id": "MDRW2100010252.1.18",
          "form": "맞아요 직관 꼭 가고 싶어요",
          "original_form": "맞아요 직관 꼭 가고 싶어요",
          "speaker_id": "1",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010252.1.19",
          "form": "올림픽 전에도 배구 자주 챙겨 보셨나요?",
          "original_form": "올림픽 전에도 배구 자주 챙겨 보셨나요?",
          "speaker_id": "1",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010252.1.20",
          "form": "아뇨ㅠㅠ 사실 그전에는 별로 관심없었어요",
          "original_form": "아뇨ㅠㅠ 사실 그전에는 별로 관심없었어요",
          "speaker_id": "2",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010252.1.21",
          "form": "원래도 보셨나요??",
          "original_form": "원래도 보셨나요??",
          "speaker_id": "2",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010252.1.22",
          "form": "저도 이번에 올림픽 보고 빠졌어요",
          "original_form": "저도 이번에 올림픽 보고 빠졌어요",
          "speaker_id": "1",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010252.1.23",
          "form": "스포츠 경기 직관 본 적 있으신가요?",
          "original_form": "스포츠 경기 직관 본 적 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010252.1.24",
          "form": "음 엄청 어릴때 kbo 경기를 보러간적 읶어요!",
          "original_form": "음 엄청 어릴때 kbo 경기를 보러간적 읶어요!",
          "speaker_id": "2",
          "time": "20210823 17:05"
        },
        {
          "id": "MDRW2100010252.1.25",
          "form": "재밌게 보신 직관 경기 있으세요?",
          "original_form": "재밌게 보신 직관 경기 있으세요?",
          "speaker_id": "2",
          "time": "20210823 17:05"
        },
        {
          "id": "MDRW2100010252.1.26",
          "form": "저는 한번도 직관해본 적이 없어서",
          "original_form": "저는 한번도 직관해본 적이 없어서",
          "speaker_id": "1",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100010252.1.27",
          "form": "코로나 이후에 여자 배구 직관 가보려고요!",
          "original_form": "코로나 이후에 여자 배구 직관 가보려고요!",
          "speaker_id": "1",
