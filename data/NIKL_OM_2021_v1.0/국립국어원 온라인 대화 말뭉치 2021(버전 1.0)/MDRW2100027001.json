      "utterance": [
        {
          "id": "MDRW2100027001.1.1",
          "form": "안녕하세요~ name2은 좋아하는 계절 있나요?",
          "original_form": "안녕하세요~ &name2&은 좋아하는 계절 있나요?",
          "speaker_id": "1",
          "time": "20210902 06:24"
        },
        {
          "id": "MDRW2100027001.1.2",
          "form": "저는 가을을 제일 좋아해요",
          "original_form": "저는 가을을 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100027001.1.3",
          "form": "선선하고 나무도 예뻐지고",
          "original_form": "선선하고 나무도 예뻐지고",
          "speaker_id": "2",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100027001.1.4",
          "form": "가디건 입기에도 좋고",
          "original_form": "가디건 입기에도 좋고",
          "speaker_id": "2",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100027001.1.5",
          "form": "name2은 어느 계절?",
          "original_form": "&name2&은 어느 계절?",
          "speaker_id": "2",
          "time": "20210902 08:02"
        },
        {
          "id": "MDRW2100027001.1.6",
          "form": "저랑 같네요.. 저도 가을 좋아해요",
          "original_form": "저랑 같네요.. 저도 가을 좋아해요",
          "speaker_id": "1",
          "time": "20210902 08:05"
        },
        {
          "id": "MDRW2100027001.1.7",
          "form": "단풍을 좋아해서요",
          "original_form": "단풍을 좋아해서요",
          "speaker_id": "1",
          "time": "20210902 08:05"
        },
        {
          "id": "MDRW2100027001.1.8",
          "form": "등산 자주하세요?",
          "original_form": "등산 자주하세요?",
          "speaker_id": "2",
          "time": "20210902 08:17"
        },
        {
          "id": "MDRW2100027001.1.9",
          "form": "자주 했는데.. 작년부턴 못하고 있어요",
          "original_form": "자주 했는데.. 작년부턴 못하고 있어요",
          "speaker_id": "1",
          "time": "20210902 08:18"
        },
        {
          "id": "MDRW2100027001.1.10",
          "form": "그렇구나 저는 올해 등산 좀 해보려고요",
          "original_form": "그렇구나 저는 올해 등산 좀 해보려고요",
          "speaker_id": "2",
          "time": "20210902 08:18"
        },
        {
          "id": "MDRW2100027001.1.11",
          "form": "올 4월에 등산화 선물받았는데 아직 한번도 못써봤어요",
          "original_form": "올 4월에 등산화 선물받았는데 아직 한번도 못써봤어요",
          "speaker_id": "2",
          "time": "20210902 08:19"
        },
        {
          "id": "MDRW2100027001.1.12",
          "form": "등산 좋아요... 기분도 상쾌해지구요 피톤치드도 많이 마시고",
          "original_form": "등산 좋아요... 기분도 상쾌해지구요 피톤치드도 많이 마시고",
          "speaker_id": "1",
          "time": "20210902 08:21"
        },
        {
          "id": "MDRW2100027001.1.13",
          "form": "산 좋아하시나보네요",
          "original_form": "산 좋아하시나보네요",
          "speaker_id": "2",
          "time": "20210902 08:36"
        },
        {
          "id": "MDRW2100027001.1.14",
          "form": "산을 특별히 좋아한다니 보다 운동겸 다닌 거였죠",
          "original_form": "산을 특별히 좋아한다니 보다 운동겸 다닌 거였죠",
          "speaker_id": "1",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100027001.1.15",
          "form": "동네에도 갈만한  산이 있어서요",
          "original_form": "동네에도 갈만한  산이 있어서요",
          "speaker_id": "1",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100027001.1.16",
          "form": "다른 운동 하시는 것 있나요?",
          "original_form": "다른 운동 하시는 것 있나요?",
          "speaker_id": "2",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100027001.1.17",
          "form": "수영 많이 했어요",
          "original_form": "수영 많이 했어요",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027001.1.18",
          "form": "수영도 요새는 못하시겠네요",
          "original_form": "수영도 요새는 못하시겠네요",
          "speaker_id": "2",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027001.1.19",
          "form": "코로나때문에 정말 삻의 질 떨어져요",
          "original_form": "코로나때문에 정말 삻의 질 떨어져요",
          "speaker_id": "2",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027001.1.20",
          "form": "맞아요.. 운동 다 끊었어요..",
          "original_form": "맞아요.. 운동 다 끊었어요..",
