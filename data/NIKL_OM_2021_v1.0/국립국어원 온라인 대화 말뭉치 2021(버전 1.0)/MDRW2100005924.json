      "utterance": [
        {
          "id": "MDRW2100005924.1.1",
          "form": "어떤 과목을 좋아하세요?",
          "original_form": "어떤 과목을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005924.1.2",
          "form": "사회과목 좋아해요!",
          "original_form": "사회과목 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100005924.1.3",
          "form": "사회! 그중에 어떤 부분 제일 좋아해요?",
          "original_form": "사회! 그중에 어떤 부분 제일 좋아해요?",
          "speaker_id": "1",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005924.1.4",
          "form": "정치와 법 공부하니까 재밌더라고요",
          "original_form": "정치와 법 공부하니까 재밌더라고요",
          "speaker_id": "2",
          "time": "20210907 16:43"
        },
        {
          "id": "MDRW2100005924.1.5",
          "form": "저도 정치와 법 부분을 좋아해요",
          "original_form": "저도 정치와 법 부분을 좋아해요",
          "speaker_id": "1",
          "time": "20210907 16:51"
        },
        {
          "id": "MDRW2100005924.1.6",
          "form": "그러면 문과? 인거죠?",
          "original_form": "그러면 문과? 인거죠?",
          "speaker_id": "1",
          "time": "20210907 16:51"
        },
        {
          "id": "MDRW2100005924.1.7",
          "form": "네 문과에요!",
          "original_form": "네 문과에요!",
          "speaker_id": "2",
          "time": "20210907 16:52"
        },
        {
          "id": "MDRW2100005924.1.8",
          "form": "그래도 재미있는 분야가 있다고 하니까 다행이에요!",
          "original_form": "그래도 재미있는 분야가 있다고 하니까 다행이에요!",
          "speaker_id": "1",
          "time": "20210907 16:53"
        },
        {
          "id": "MDRW2100005924.1.9",
          "form": "저는 이과거든요",
          "original_form": "저는 이과거든요",
          "speaker_id": "1",
          "time": "20210907 16:53"
        },
        {
          "id": "MDRW2100005924.1.10",
          "form": "그래서 정치와 법 부분은 살면서 사기당하지 않는 수준으로 ㅋㅋ 선생님께서 가르쳐주셨어요",
          "original_form": "그래서 정치와 법 부분은 살면서 사기당하지 않는 수준으로 ㅋㅋ 선생님께서 가르쳐주셨어요",
          "speaker_id": "1",
          "time": "20210907 16:53"
        },
        {
          "id": "MDRW2100005924.1.11",
          "form": "그렇지만 상당히 재미있는 과목 같아요",
          "original_form": "그렇지만 상당히 재미있는 과목 같아요",
          "speaker_id": "1",
          "time": "20210907 16:53"
        },
        {
          "id": "MDRW2100005924.1.12",
          "form": "이과에서도 문과 수업을 하셨군요!",
          "original_form": "이과에서도 문과 수업을 하셨군요!",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100005924.1.13",
          "form": "저희 학교에도 생활과 과학 수업이 있긴 한데 맨날 자습만 해요...",
          "original_form": "저희 학교에도 생활과 과학 수업이 있긴 한데 맨날 자습만 해요...",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100005924.1.14",
          "form": "이런... 맨날 자습만 한다니 ㅠ",
          "original_form": "이런... 맨날 자습만 한다니 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100005924.1.15",
          "form": "저희는 선생님이 상당히 재미있는 분이셔서",
          "original_form": "저희는 선생님이 상당히 재미있는 분이셔서",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100005924.1.16",
          "form": "드라마 보면서 저 과목을 배웠어요!",
          "original_form": "드라마 보면서 저 과목을 배웠어요!",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100005924.1.17",
          "form": "와 진짜 재밌었겠네요!",
          "original_form": "와 진짜 재밌었겠네요!",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100005924.1.18",
          "form": "우리 학교는 왜 그런 수업이 없지...",
          "original_form": "우리 학교는 왜 그런 수업이 없지...",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100005924.1.19",
          "form": "이건 사실 선생님들 케바케라서 ㅋㅋㅋ",
          "original_form": "이건 사실 선생님들 케바케라서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100005924.1.20",
          "form": "그치만 약간 실생활에서 쓰일 수 있는 법과",
          "original_form": "그치만 약간 실생활에서 쓰일 수 있는 법과",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100005924.1.21",
          "form": "조심해야하는 부분을 더 쉽게 이해하면서 배울 수 있어서 좋았어요",
          "original_form": "조심해야하는 부분을 더 쉽게 이해하면서 배울 수 있어서 좋았어요",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100005924.1.22",
          "form": "법정 드라마 잘 안나오는데 그래서 더 재미있게 수업 듣기도 했구요 ㅋㅋ",
          "original_form": "법정 드라마 잘 안나오는데 그래서 더 재미있게 수업 듣기도 했구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100005924.1.23",
          "form": "법정이 그런 부분이 좋죠",
          "original_form": "법정이 그런 부분이 좋죠",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100005924.1.24",
          "form": "실생활과 직접 관련되어 있다는게",
          "original_form": "실생활과 직접 관련되어 있다는게",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100005924.1.25",
          "form": "그쵸! 드디어 공감해주는 분을 만났네요",
          "original_form": "그쵸! 드디어 공감해주는 분을 만났네요",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100005924.1.26",
          "form": "다들 말 꺼내기 무섭게 이해가 안된다는 듯이 바라보더라구요",
          "original_form": "다들 말 꺼내기 무섭게 이해가 안된다는 듯이 바라보더라구요",
          "speaker_id": "1",
          "time": "20210907 21:47"
        }
      ]
    }
