      "utterance": [
        {
          "id": "MDRW2100036894.1.1",
          "form": "가아끔 트위터 구경하고... 혼자 웃는 삶...",
          "original_form": "가아끔 트위터 구경하고... 혼자 웃는 삶...",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036894.1.2",
          "form": "그래도 이제 name1님이랑 이야기하면 돼서 좋아요 ^.^",
          "original_form": "그래도 이제 &name1&님이랑 이야기하면 돼서 좋아요 ^.^",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036894.1.3",
          "form": "name1님 비에이피도 좋아하셨었나요?",
          "original_form": "&name1&님 비에이피도 좋아하셨었나요?",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036894.1.4",
          "form": "저는.. 제 첫 아이돌이었어요",
          "original_form": "저는.. 제 첫 아이돌이었어요",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036894.1.5",
          "form": "저두여 !! 동지를 찾아서 좋아요",
          "original_form": "저두여 !! 동지를 찾아서 좋아요",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036894.1.6",
          "form": "아 비에이삐",
          "original_form": "아 비에이삐",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036894.1.7",
          "form": "하 아픈손가락이에여",
          "original_form": "하 아픈손가락이에여",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036894.1.8",
          "form": "ㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036894.1.9",
          "form": "아 그니까요... 1004 진짜 대박곡이었는데...",
          "original_form": "아 그니까요... 1004 진짜 대박곡이었는데...",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036894.1.10",
          "form": "소속사가 원망스러운...",
          "original_form": "소속사가 원망스러운...",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036894.1.11",
          "form": "아 진짜요",
          "original_form": "아 진짜요",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036894.1.12",
          "form": "저 1004로 뮤뱅 1위하는거보구",
          "original_form": "저 1004로 뮤뱅 1위하는거보구",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036894.1.13",
          "form": "드디어 뜨는구나 !!! 했는데",
          "original_form": "드디어 뜨는구나 !!! 했는데",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036894.1.14",
          "form": "그대로 사라졌죠...",
          "original_form": "그대로 사라졌죠...",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036894.1.15",
          "form": "그니까요 ^.^.... 갑자기 서글프네",
          "original_form": "그니까요 ^.^.... 갑자기 서글프네",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036894.1.16",
          "form": "제 최애곡은 하지마였어요 ㅎㅎ",
          "original_form": "제 최애곡은 하지마였어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036894.1.17",
          "form": "마자요...",
          "original_form": "마자요...",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036894.1.18",
          "form": "소속사 쓰레기...",
          "original_form": "소속사 쓰레기...",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036894.1.19",
          "form": "헐 하지마",
          "original_form": "헐 하지마",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036894.1.20",
          "form": "전 요즘도 가끔 들어요",
          "original_form": "전 요즘도 가끔 들어요",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036894.1.21",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036894.1.22",
          "form": "다들 대박사건만 알지만",
          "original_form": "다들 대박사건만 알지만",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036894.1.23",
          "form": "하지마도 좋다구여ㅜㅜ",
          "original_form": "하지마도 좋다구여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036894.1.24",
          "form": "ㅜ.^ 대박사건이랑 노멀씨? 다 너무 좋아해요",
          "original_form": "ㅜ.^ 대박사건이랑 노멀씨? 다 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036894.1.25",
          "form": "아 노멀씨",
          "original_form": "아 노멀씨",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036894.1.26",
          "form": "사투리랩",
          "original_form": "사투리랩",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036894.1.27",
          "form": "넘 조아여ㅜㅜ",
          "original_form": "넘 조아여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036894.1.28",
          "form": "그때 한창 라이징이였는데 ...",
          "original_form": "그때 한창 라이징이였는데 ...",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036894.1.29",
          "form": "그니께요 -.- !!!!",
          "original_form": "그니께요 -.- !!!!",
          "speaker_id": "2",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100036894.1.30",
          "form": "전 비에이피 비원에이퍼 덩시에 좋아해서",
          "original_form": "전 비에이피 비원에이퍼 덩시에 좋아해서",
          "speaker_id": "2",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100036894.1.31",
          "form": "끝장나는 덕질을 했옷어요... ^.^",
          "original_form": "끝장나는 덕질을 했옷어요... ^.^",
          "speaker_id": "2",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100036894.1.32",
          "form": "아 헐 완전 초통령들 이였자나욬ㅋㅋ",
          "original_form": "아 헐 완전 초통령들 이였자나욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100036894.1.33",
          "form": "b 그룹들..",
          "original_form": "b 그룹들..",
          "speaker_id": "1",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100036894.1.34",
          "form": "비원에이포 저 못된것만 배워서",
          "original_form": "비원에이포 저 못된것만 배워서",
          "speaker_id": "1",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100036894.1.35",
          "form": "짱조아해여..",
          "original_form": "짱조아해여..",
          "speaker_id": "1",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100036894.1.36",
          "form": "오마이갓 저 그 노래 지낮 좋아해요",
          "original_form": "오마이갓 저 그 노래 지낮 좋아해요",
          "speaker_id": "2",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100036894.1.37",
          "form": "오케이였나",
          "original_form": "오케이였나",
          "speaker_id": "2",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100036894.1.38",
          "form": "그 곡 다음으로 좋아하는 차애곡",
          "original_form": "그 곡 다음으로 좋아하는 차애곡",
          "speaker_id": "2",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100036894.1.39",
          "form": "아 데뷔곡!!",
          "original_form": "아 데뷔곡!!",
          "speaker_id": "1",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100036894.1.40",
          "form": "오케이 저두 하",
          "original_form": "오케이 저두 하",
          "speaker_id": "1",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100036894.1.41",
          "form": "좋아합니다요",
          "original_form": "좋아합니다요",
          "speaker_id": "1",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100036894.1.42",
          "form": "오케이걸~",
          "original_form": "오케이걸~",
          "speaker_id": "1",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100036894.1.43",
          "form": "진짜 대박 명곡 ㅠ",
          "original_form": "진짜 대박 명곡 ㅠ",
          "speaker_id": "2",
          "time": "20210831 14:46"
        },
        {
          "id": "MDRW2100036894.1.44",
          "form": "전 그 잘자요 굿나잇도",
          "original_form": "전 그 잘자요 굿나잇도",
          "speaker_id": "2",
          "time": "20210831 14:46"
        },
        {
          "id": "MDRW2100036894.1.45",
          "form": "엄청 좋아했어요",
          "original_form": "엄청 좋아했어요",
          "speaker_id": "2",
          "time": "20210831 14:46"
        },
        {
          "id": "MDRW2100036894.1.46",
