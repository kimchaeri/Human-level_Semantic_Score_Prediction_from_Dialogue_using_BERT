      "utterance": [
        {
          "id": "MDRW2100043264.1.1",
          "form": "동물은 좋아해?",
          "original_form": "동물은 좋아해?",
          "speaker_id": "1",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100043264.1.2",
          "form": "나 강아지 키워",
          "original_form": "나 강아지 키워",
          "speaker_id": "2",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100043264.1.3",
          "form": "강아지 이름이 뭐야?",
          "original_form": "강아지 이름이 뭐야?",
          "speaker_id": "1",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100043264.1.4",
          "form": "name3이야",
          "original_form": "&name3&이야",
          "speaker_id": "2",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043264.1.5",
          "form": "아ㅋㅋ 대화명이랑 같네",
          "original_form": "아ㅋㅋ 대화명이랑 같네",
          "speaker_id": "1",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043264.1.6",
          "form": "맞아 웃기지 ㅋㅋㅋ",
          "original_form": "맞아 웃기지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100043264.1.7",
          "form": "너무 귀여워",
          "original_form": "너무 귀여워",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043264.1.8",
          "form": "ㅋㅋ그러게 종류라고 하나? 그건 뭐야?",
          "original_form": "ㅋㅋ그러게 종류라고 하나? 그건 뭐야?",
          "speaker_id": "1",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043264.1.9",
          "form": "장모 치와와야",
          "original_form": "장모 치와와야",
          "speaker_id": "2",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043264.1.10",
          "form": "장모는 뭐야? 머리가 길다는의민가 ..",
          "original_form": "장모는 뭐야? 머리가 길다는의민가 ..",
          "speaker_id": "1",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100043264.1.11",
          "form": "어어 맞아 롱 헤어",
          "original_form": "어어 맞아 롱 헤어",
          "speaker_id": "2",
          "time": "20210907 17:36"
        },
        {
          "id": "MDRW2100043264.1.12",
          "form": "치와와가 단모 장모 2가지얌",
          "original_form": "치와와가 단모 장모 2가지얌",
          "speaker_id": "2",
          "time": "20210907 17:36"
        },
        {
          "id": "MDRW2100043264.1.13",
          "form": "잘몰라서 ㅋㅋ 신기하다",
          "original_form": "잘몰라서 ㅋㅋ 신기하다",
          "speaker_id": "1",
          "time": "20210907 17:36"
        },
        {
          "id": "MDRW2100043264.1.14",
          "form": "어어 나도 잘 몰라",
          "original_form": "어어 나도 잘 몰라",
          "speaker_id": "2",
          "time": "20210907 17:37"
        },
        {
          "id": "MDRW2100043264.1.15",
          "form": "키우게 돼서 알게됐어 ㅋㅋ",
          "original_form": "키우게 돼서 알게됐어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:37"
        },
        {
          "id": "MDRW2100043264.1.16",
          "form": "ㅋㅋㅋ난 애기진돗개들 그렇게 귀엽더라 ㅠ",
          "original_form": "ㅋㅋㅋ난 애기진돗개들 그렇게 귀엽더라 ㅠ",
          "speaker_id": "1",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043264.1.17",
          "form": "맞아 인절미들 귀 접혀 잇잖아ㅠㅠ",
          "original_form": "맞아 인절미들 귀 접혀 잇잖아ㅠㅠ",
