      "utterance": [
        {
          "id": "MDRW2100007249.1.1",
          "form": "오늘 비 많이 왔네요",
          "original_form": "오늘 비 많이 왔네요",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007249.1.2",
          "form": "그르게요. 베란다 물이 안빠져서 빼느라 고생했어요ㅋㅋ",
          "original_form": "그르게요. 베란다 물이 안빠져서 빼느라 고생했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007249.1.3",
          "form": "이제 비 오고 완전 가을이겠죠?",
          "original_form": "이제 비 오고 완전 가을이겠죠?",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007249.1.4",
          "form": "네 시원해지니 너무좋네요",
          "original_form": "네 시원해지니 너무좋네요",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007249.1.5",
          "form": "겨울 여름 고른다면?",
          "original_form": "겨울 여름 고른다면?",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007249.1.6",
          "form": "겨울입니당",
          "original_form": "겨울입니당",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007249.1.7",
          "form": "어릴땐 여름이좋았는데",
          "original_form": "어릴땐 여름이좋았는데",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007249.1.8",
          "form": "허얼",
          "original_form": "허얼",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007249.1.9",
          "form": "여름 너무짜증나네요",
          "original_form": "여름 너무짜증나네요",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007249.1.10",
          "form": "전 아직은 여름이요ㅋㅋㅋㅋ",
          "original_form": "전 아직은 여름이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007249.1.11",
          "form": "겨울은 뼈가 시려서ㅋㅋㅋㅋ",
          "original_form": "겨울은 뼈가 시려서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007249.1.12",
          "form": "진짜용?여름중에 뭐가 좋으신가요?",
          "original_form": "진짜용?여름중에 뭐가 좋으신가요?",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007249.1.13",
          "form": "그리구 겨울은 보일러비 넘 마니 나온다구요ㅋㅋㅋ",
          "original_form": "그리구 겨울은 보일러비 넘 마니 나온다구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007249.1.14",
          "form": "그렇긴하네요ㅎ 전기요금과는 비교도 안되는ㅋㅋ",
          "original_form": "그렇긴하네요ㅎ 전기요금과는 비교도 안되는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007249.1.15",
          "form": "요즘은 아닌데 예전에 좀 덜 더울땐 에컨 밖으로 나올때 훅한 느낌 좋았어요",
          "original_form": "요즘은 아닌데 예전에 좀 덜 더울땐 에컨 밖으로 나올때 훅한 느낌 좋았어요",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007249.1.16",
          "form": "ㅇㅇ 에컨도 막 빵빵 트는 건 아니지만 보일러처럼 애껴틀진 않으니ㅋㅋ",
          "original_form": "ㅇㅇ 에컨도 막 빵빵 트는 건 아니지만 보일러처럼 애껴틀진 않으니ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007249.1.17",
          "form": "넓은데 사셔서 그렇겠죠?ㅋㅋ",
          "original_form": "넓은데 사셔서 그렇겠죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007249.1.18",
          "form": "아 아니에요 집은 쪼매난데ㅋㅋㅋㅋ",
          "original_form": "아 아니에요 집은 쪼매난데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007249.1.19",
          "form": "우풍이 센가요?ㅠㅠ",
          "original_form": "우풍이 센가요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007249.1.20",
          "form": "것도 그렇고 아 꼭데기 층이라 더ㅋㅋㅋ",
          "original_form": "것도 그렇고 아 꼭데기 층이라 더ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007249.1.21",
          "form": "이번 겨울은 좀 들 추웠으면 좋겠어요ㅋㅋㅋ",
          "original_form": "이번 겨울은 좀 들 추웠으면 좋겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007249.1.22",
          "form": "여름에 마니 더웠으니까 보상으로..",
          "original_form": "여름에 마니 더웠으니까 보상으로..",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007249.1.23",
          "form": "겨울이 왜 좋으신데요?",
          "original_form": "겨울이 왜 좋으신데요?",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007249.1.24",
          "form": "그러게요 갈수록 봄가을이 없어지니 원ㅠ",
          "original_form": "그러게요 갈수록 봄가을이 없어지니 원ㅠ",
          "speaker_id": "1",
          "time": "20210822 01:10"
        }
      ]
    }
