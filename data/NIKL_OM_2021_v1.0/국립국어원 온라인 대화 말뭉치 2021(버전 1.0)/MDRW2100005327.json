      "utterance": [
        {
          "id": "MDRW2100005327.1.1",
          "form": "너 저녁 먹음???",
          "original_form": "너 저녁 먹음???",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005327.1.2",
          "form": "배달시켜 먹음",
          "original_form": "배달시켜 먹음",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005327.1.3",
          "form": "배달??? 뭐먹었어???",
          "original_form": "배달??? 뭐먹었어???",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005327.1.4",
          "form": "요즘 유행하는 로제떡볶이 !!",
          "original_form": "요즘 유행하는 로제떡볶이 !!",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005327.1.5",
          "form": "와 뭐야 맛있는거 먹었네",
          "original_form": "와 뭐야 맛있는거 먹었네",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005327.1.6",
          "form": "그치 짱 맛있더라",
          "original_form": "그치 짱 맛있더라",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005327.1.7",
          "form": "언니는 뭐 먹었어 ??",
          "original_form": "언니는 뭐 먹었어 ??",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005327.1.8",
          "form": "나는 비빔밥 먹었는데",
          "original_form": "나는 비빔밥 먹었는데",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005327.1.9",
          "form": "맛있더라",
          "original_form": "맛있더라",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005327.1.10",
          "form": "아 비빔밥도 맛있지 ~",
          "original_form": "아 비빔밥도 맛있지 ~",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005327.1.11",
          "form": "비빔밥에 고사리가 짱맛",
          "original_form": "비빔밥에 고사리가 짱맛",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005327.1.12",
          "form": "맞아 너 고사리 잘 볶는법 아니??",
          "original_form": "맞아 너 고사리 잘 볶는법 아니??",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005327.1.13",
          "form": "그걸 내가 어떻게 알아 ??;",
          "original_form": "그걸 내가 어떻게 알아 ??;",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005327.1.14",
          "form": "난 요리사가 아님",
          "original_form": "난 요리사가 아님",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005327.1.15",
          "form": "고사리 들기름에 볶아야 맛있다",
          "original_form": "고사리 들기름에 볶아야 맛있다",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005327.1.16",
          "form": "오 꿀팁이네",
          "original_form": "오 꿀팁이네",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005327.1.17",
          "form": "그치 너는 뭐 알고있는 꿀팁 없어??",
          "original_form": "그치 너는 뭐 알고있는 꿀팁 없어??",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005327.1.18",
          "form": "오 .. 나는 요리를 안하는걸 ㅠ",
          "original_form": "오 .. 나는 요리를 안하는걸 ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005327.1.19",
          "form": "자랑이다 이놈아",
          "original_form": "자랑이다 이놈아",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005327.1.20",
          "form": "다음에는 쌀국수 먹으러 가자",
          "original_form": "다음에는 쌀국수 먹으러 가자",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005327.1.21",
          "form": "헐 완전 좋지 쌀국수 너무 좋아",
          "original_form": "헐 완전 좋지 쌀국수 너무 좋아",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005327.1.22",
          "form": "고수는 안먹지만 ..",
          "original_form": "고수는 안먹지만 ..",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005327.1.23",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210821 21:53"
        }
      ]
