      "utterance": [
        {
          "id": "MDRW2100016449.1.1",
          "form": "결혼했어요?",
          "original_form": "결혼했어요?",
          "speaker_id": "1",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100016449.1.2",
          "form": "아뇨 결혼 하셨어요?",
          "original_form": "아뇨 결혼 하셨어요?",
          "speaker_id": "2",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100016449.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100016449.1.4",
          "form": "그렇군요 전 애인도 없어서..",
          "original_form": "그렇군요 전 애인도 없어서..",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016449.1.5",
          "form": "인연이ㅜ있죠",
          "original_form": "인연이ㅜ있죠",
          "speaker_id": "1",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016449.1.6",
          "form": "그러겠죠?ㅋㅋ",
          "original_form": "그러겠죠?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016449.1.7",
          "form": "배우자분은 어떻게 만나셨어요?",
          "original_form": "배우자분은 어떻게 만나셨어요?",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016449.1.8",
          "form": "친구소개려뇨",
          "original_form": "친구소개려뇨",
          "speaker_id": "1",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100016449.1.9",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210826 12:59"
        },
        {
          "id": "MDRW2100016449.1.10",
          "form": "두분이 결혼까지 했을 때 소개해준 친구분이 뿌듯했겠네요ㅎㅎ",
          "original_form": "두분이 결혼까지 했을 때 소개해준 친구분이 뿌듯했겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100016449.1.11",
          "form": "네 자주 만나요",
          "original_form": "네 자주 만나요",
          "speaker_id": "1",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016449.1.12",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016449.1.13",
          "form": "결혼하시고 나서 달라진 점이 있나요?",
          "original_form": "결혼하시고 나서 달라진 점이 있나요?",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016449.1.14",
          "form": "매일 행복해지요",
          "original_form": "매일 행복해지요",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100016449.1.15",
          "form": "아 가장 큰 장점이네요ㅎㅎ",
          "original_form": "아 가장 큰 장점이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
