      "utterance": [
        {
          "id": "MDRW2100042328.1.1",
          "form": "비가 또 오네요",
          "original_form": "비가 또 오네요",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042328.1.2",
          "form": "주말동안 해가 나서 초가을 같고 좋았는데요",
          "original_form": "주말동안 해가 나서 초가을 같고 좋았는데요",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042328.1.3",
          "form": "그쵸. 장대비가 쏟아지네요",
          "original_form": "그쵸. 장대비가 쏟아지네요",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042328.1.4",
          "form": "바람도 꽤 불어요",
          "original_form": "바람도 꽤 불어요",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042328.1.5",
          "form": "네. 습도가 너무 높아서 그게 좀 힘드네요",
          "original_form": "네. 습도가 너무 높아서 그게 좀 힘드네요",
          "speaker_id": "1",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042328.1.6",
          "form": "비가 오면 습한게 좀 답답해요",
          "original_form": "비가 오면 습한게 좀 답답해요",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042328.1.7",
          "form": "공기가 시원하고 상쾌해야 좋은데",
          "original_form": "공기가 시원하고 상쾌해야 좋은데",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042328.1.8",
          "form": "그러게요. 안 그래도 짧은 가을이 더 짧아질 것 같아 섭섭하네요",
          "original_form": "그러게요. 안 그래도 짧은 가을이 더 짧아질 것 같아 섭섭하네요",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042328.1.9",
          "form": "그러게요 이렇게 자주 비오다 바로 겨울 넘어갈까봐 걱정이네요",
          "original_form": "그러게요 이렇게 자주 비오다 바로 겨울 넘어갈까봐 걱정이네요",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042328.1.10",
          "form": "네 이번주부터는 완연한 가을 날씨가 되길 바랐는데 그것도 어렵겠어요",
          "original_form": "네 이번주부터는 완연한 가을 날씨가 되길 바랐는데 그것도 어렵겠어요",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042328.1.11",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042328.1.12",
          "form": "단풒이야 10월쯤 돼야 할테니까 그건 좀 기다려야 하고",
          "original_form": "단풒이야 10월쯤 돼야 할테니까 그건 좀 기다려야 하고",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042328.1.13",
          "form": "그전에 높고 푸른 하늘 좀 많이 보고 싶었는데요",
          "original_form": "그전에 높고 푸른 하늘 좀 많이 보고 싶었는데요",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042328.1.14",
          "form": "23일이 추분이라니 그때까지는 기다려야하나봐요",
          "original_form": "23일이 추분이라니 그때까지는 기다려야하나봐요",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042328.1.15",
          "form": "네 추석 지나면 가을 느낌이 확실히 나겠죠?",
          "original_form": "네 추석 지나면 가을 느낌이 확실히 나겠죠?",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042328.1.16",
          "form": "올해 추석이 좀 빠른 편이잖아요",
          "original_form": "올해 추석이 좀 빠른 편이잖아요",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042328.1.17",
          "form": "그럴 것 같아요~ 봄 가을은 항상 너무 짧으니 아쉽네요",
          "original_form": "그럴 것 같아요~ 봄 가을은 항상 너무 짧으니 아쉽네요",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042328.1.18",
          "form": "매년 한해 한해 갈수록 짧아지는 느낌이에요",
          "original_form": "매년 한해 한해 갈수록 짧아지는 느낌이에요",
          "speaker_id": "2",
          "time": "20210906 22:09"
        }
