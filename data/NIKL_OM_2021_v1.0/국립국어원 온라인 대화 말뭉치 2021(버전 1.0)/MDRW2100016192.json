      "utterance": [
        {
          "id": "MDRW2100016192.1.1",
          "form": "요새 티비 뭐 봐요",
          "original_form": "요새 티비 뭐 봐요",
          "speaker_id": "1",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100016192.1.2",
          "form": "난 슬의만 보고 있음",
          "original_form": "난 슬의만 보고 있음",
          "speaker_id": "1",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100016192.1.3",
          "form": "나도 슬의",
          "original_form": "나도 슬의",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016192.1.4",
          "form": "꼬꼬무",
          "original_form": "꼬꼬무",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016192.1.5",
          "form": "아 꼬꼬무 재밌지",
          "original_form": "아 꼬꼬무 재밌지",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016192.1.6",
          "form": "신랑이랑 레플에서 보는데",
          "original_form": "신랑이랑 레플에서 보는데",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016192.1.7",
          "form": "난 그것이 알고 싶다 같은 거는 잘 안 보는데",
          "original_form": "난 그것이 알고 싶다 같은 거는 잘 안 보는데",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016192.1.8",
          "form": "꼬꼬무는 자극적이지 않아서 좋음",
          "original_form": "꼬꼬무는 자극적이지 않아서 좋음",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016192.1.9",
          "form": "영화보고싶다",
          "original_form": "영화보고싶다",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016192.1.10",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016192.1.11",
          "form": "벌거벗은세계사",
          "original_form": "벌거벗은세계사",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016192.1.12",
          "form": "그것도 보다가 말았는데",
          "original_form": "그것도 보다가 말았는데",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016192.1.13",
          "form": "영화는 언제봤는지도 모르겠네",
          "original_form": "영화는 언제봤는지도 모르겠네",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016192.1.14",
          "form": "류준열 나오는 드라마 기다리고 있음",
          "original_form": "류준열 나오는 드라마 기다리고 있음",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016192.1.15",
          "form": "드라마찍나?",
          "original_form": "드라마찍나?",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016192.1.16",
          "form": "보고싶다",
          "original_form": "보고싶다",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016192.1.17",
          "form": "엉 류준열",
          "original_form": "엉 류준열",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016192.1.18",
          "form": "전도연나오는",
          "original_form": "전도연나오는",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016192.1.19",
          "form": "인간실격",
          "original_form": "인간실격",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016192.1.20",
          "form": "그렇구만 ㄴ",
          "original_form": "그렇구만 ㄴ",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016192.1.21",
          "form": "넷플릭스 좋은데 구독료있어서ㅋ",
          "original_form": "넷플릭스 좋은데 구독료있어서ㅋ",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016192.1.22",
          "form": "나도 신랑 친구꺼로 같이 보고 있음",
          "original_form": "나도 신랑 친구꺼로 같이 보고 있음",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016192.1.23",
          "form": "넷플 좋습니다",
          "original_form": "넷플 좋습니다",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016192.1.24",
          "form": "요즘신곡?",
          "original_form": "요즘신곡?",
          "speaker_id": "2",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100016192.1.25",
          "form": "노래 똥 밟았네",
          "original_form": "노래 똥 밟았네",
          "speaker_id": "1",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016192.1.26",
          "form": "아침 먹고 땡 집을 나서려는데",
          "original_form": "아침 먹고 땡 집을 나서려는데",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016192.1.27",
          "form": "이 노래 알아요??",
          "original_form": "이 노래 알아요??",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016192.1.28",
          "form": "알지ㅋ",
          "original_form": "알지ㅋ",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016192.1.29",
          "form": "춤도추던데",
          "original_form": "춤도추던데",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016192.1.30",
          "form": "춤 추지 춤도 같이 춰야 됨",
          "original_form": "춤 추지 춤도 같이 춰야 됨",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016192.1.31",
          "form": "어제 우리 투가 놀이터에서 이 노래를 아주 크게 불러서 부끄러웠음",
          "original_form": "어제 우리 투가 놀이터에서 이 노래를 아주 크게 불러서 부끄러웠음",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016192.1.32",
          "form": "투피엠 컴백했잖아",
          "original_form": "투피엠 컴백했잖아",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016192.1.33",
          "form": "준호 너무 잘생겨져서",
          "original_form": "준호 너무 잘생겨져서",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016192.1.34",
          "form": "한창 투피엠 노래에 빠져 있었지",
          "original_form": "한창 투피엠 노래에 빠져 있었지",
          "speaker_id": "1",
          "time": "20210826 15:40"
        }
      ]
