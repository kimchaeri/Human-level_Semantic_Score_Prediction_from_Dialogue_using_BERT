      "utterance": [
        {
          "id": "MDRW2100029229.1.1",
          "form": "귀신같군여",
          "original_form": "귀신같군여",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029229.1.2",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029229.1.3",
          "form": "귀신여?",
          "original_form": "귀신여?",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029229.1.4",
          "form": "맥 커터",
          "original_form": "맥 커터",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029229.1.5",
          "form": "자 이제 내일 이 시간에",
          "original_form": "자 이제 내일 이 시간에",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029229.1.6",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029229.1.7",
          "form": "아 모야",
          "original_form": "아 모야",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029229.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029229.1.9",
          "form": "계속해여?",
          "original_form": "계속해여?",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029229.1.10",
          "form": "여친하고 헤어질때쯤 회사에서",
          "original_form": "여친하고 헤어질때쯤 회사에서",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029229.1.11",
          "form": "역시 남의 연애 얘기가 젤 재밌쥐",
          "original_form": "역시 남의 연애 얘기가 젤 재밌쥐",
          "speaker_id": "1",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029229.1.12",
          "form": "우연히 만난거에요?",
          "original_form": "우연히 만난거에요?",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029229.1.13",
          "form": "아니용",
          "original_form": "아니용",
          "speaker_id": "1",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029229.1.14",
          "form": "뺐었나요?",
          "original_form": "뺐었나요?",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029229.1.15",
          "form": "동반입대한 친구한테\r\n제 의사를 전했죠 ㅋㅋㅋㅋㅋ",
          "original_form": "동반입대한 친구한테\r\n제 의사를 전했죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029229.1.16",
          "form": "이남자 내꺼에요",
          "original_form": "이남자 내꺼에요",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029229.1.17",
          "form": "ㅋㅋㅋ그러기엔 그때 여친도",
          "original_form": "ㅋㅋㅋ그러기엔 그때 여친도",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029229.1.18",
          "form": "제가 아는 사람이었습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "제가 아는 사람이었습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029229.1.19",
          "form": "무슨",
          "original_form": "무슨",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029229.1.20",
          "form": "저희는 많은걸 공유하고 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저희는 많은걸 공유하고 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029229.1.21",
          "form": "다 알아  ㅋㅋㅋ",
          "original_form": "다 알아  ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029229.1.22",
          "form": "원래 한쿡이 건너건너 다 아는 사이에여",
          "original_form": "원래 한쿡이 건너건너 다 아는 사이에여",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029229.1.23",
          "form": "자만추 비추",
          "original_form": "자만추 비추",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029229.1.24",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029229.1.25",
          "form": "의사 전달후",
          "original_form": "의사 전달후",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029229.1.26",
          "form": "무튼 다시 재회를 하게 됩니다",
          "original_form": "무튼 다시 재회를 하게 됩니다",
          "speaker_id": "1",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029229.1.27",
          "form": "답장이 왔군여",
          "original_form": "답장이 왔군여",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029229.1.28",
          "form": "그리고 만나서 제 마음속\r\n이야기들을 꺼냈는데",
          "original_form": "그리고 만나서 제 마음속\r\n이야기들을 꺼냈는데",
          "speaker_id": "1",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029229.1.29",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029229.1.30",
          "form": "지금 만나는 여친이랑\r\n결혼까지 생각하고 있다",
          "original_form": "지금 만나는 여친이랑\r\n결혼까지 생각하고 있다",
          "speaker_id": "1",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029229.1.31",
          "form": "라고 이야길 하더라구염",
          "original_form": "라고 이야길 하더라구염",
          "speaker_id": "1",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029229.1.32",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029229.1.33",
          "form": "너무 단호박에 칼같이 말해서\r\n더 뭘 말할수도 없었어요...ㅋㅋㅋㅋㅋ",
          "original_form": "너무 단호박에 칼같이 말해서\r\n더 뭘 말할수도 없었어요...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029229.1.34",
          "form": "그래서 그렇게 짧은 만남을 갖고 헤어졌죠",
          "original_form": "그래서 그렇게 짧은 만남을 갖고 헤어졌죠",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029229.1.35",
          "form": "그리고 3년이 지났나",
          "original_form": "그리고 3년이 지났나",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029229.1.36",
          "form": "밥한먹자고 연락이 왔었어요",
          "original_form": "밥한먹자고 연락이 왔었어요",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029229.1.37",
          "form": "남편은 빛이나는솔로",
          "original_form": "남편은 빛이나는솔로",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029229.1.38",
          "form": "헤어졌구나",
          "original_form": "헤어졌구나",
          "speaker_id": "2",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029229.1.39",
          "form": "저는 남친이 있는 상황",
          "original_form": "저는 남친이 있는 상황",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029229.1.40",
          "form": "남친한테 물어봤었죠",
          "original_form": "남친한테 물어봤었죠",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029229.1.41",
          "form": "밥한번 먹자고\r\n회사근처로 온다는데\r\n만나도 되냐고",
          "original_form": "밥한번 먹자고\r\n회사근처로 온다는데\r\n만나도 되냐고",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029229.1.42",
          "form": "그때 남친이 오케이 하지 않았더라면...\r\n많은게 달라졌을텐데요",
          "original_form": "그때 남친이 오케이 하지 않았더라면...\r\n많은게 달라졌을텐데요",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029229.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029229.1.44",
          "form": "그선택",
          "original_form": "그선택",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029229.1.45",
          "form": "난 안보낸다",
          "original_form": "난 안보낸다",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029229.1.46",
          "form": "name2은 안보낸다구요?",
          "original_form": "&name2&은 안보낸다구요?",
          "speaker_id": "1",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029229.1.47",
          "form": "쿨한척한거네요",
          "original_form": "쿨한척한거네요",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029229.1.48",
          "form": "네 안보내요",
          "original_form": "네 안보내요",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029229.1.49",
          "form": "안보냈어야 했어요",
          "original_form": "안보냈어야 했어요",
          "speaker_id": "1",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029229.1.50",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029229.1.51",
          "form": "그런 깊은 사연이....",
          "original_form": "그런 깊은 사연이....",
          "speaker_id": "2",
