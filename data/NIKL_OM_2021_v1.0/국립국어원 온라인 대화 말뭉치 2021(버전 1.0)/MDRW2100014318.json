      "utterance": [
        {
          "id": "MDRW2100014318.1.1",
          "form": "최근에 하고있는게임이 있으신가요?",
          "original_form": "최근에 하고있는게임이 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014318.1.2",
          "form": "없어요. 당신은요?",
          "original_form": "없어요. 당신은요?",
          "speaker_id": "1",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014318.1.3",
          "form": "저도 없어요.",
          "original_form": "저도 없어요.",
          "speaker_id": "2",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014318.1.4",
          "form": "최근에 외국게임 몇개 시도하다가 금방질려서 지웠어요",
          "original_form": "최근에 외국게임 몇개 시도하다가 금방질려서 지웠어요",
          "speaker_id": "2",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014318.1.5",
          "form": "저는 일본 게임을 하나 했었는데 올해초 접었네요",
          "original_form": "저는 일본 게임을 하나 했었는데 올해초 접었네요",
          "speaker_id": "1",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014318.1.6",
          "form": "올해초에서나 접었다고요??",
          "original_form": "올해초에서나 접었다고요??",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014318.1.7",
          "form": "예 접은지 오래됐어요",
          "original_form": "예 접은지 오래됐어요",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014318.1.8",
          "form": "그렇네요. 몇년하셧는데요?",
          "original_form": "그렇네요. 몇년하셧는데요?",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014318.1.9",
          "form": "3년은 했네요",
          "original_form": "3년은 했네요",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014318.1.10",
          "form": "오 오래하셨네요. 게임회사에서 뭐줘야하는거아닌가요",
          "original_form": "오 오래하셨네요. 게임회사에서 뭐줘야하는거아닌가요",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014318.1.11",
          "form": "그쵸 저 장기 유저였는데 말이죠",
          "original_form": "그쵸 저 장기 유저였는데 말이죠",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014318.1.12",
          "form": "멋지네요. 끈기가",
          "original_form": "멋지네요. 끈기가",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014318.1.13",
          "form": "하루종일 붙잡고 있어야 하는 게임이 아니라 30분 안팎만 쓰는거라서 오래할수 있었나봐요",
          "original_form": "하루종일 붙잡고 있어야 하는 게임이 아니라 30분 안팎만 쓰는거라서 오래할수 있었나봐요",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014318.1.14",
          "form": "비결이뭔가요",
          "original_form": "비결이뭔가요",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014318.1.15",
          "form": "오래하면 쉽게 질리죠",
          "original_form": "오래하면 쉽게 질리죠",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014318.1.16",
          "form": "저랑 성향이 반대시군요.",
          "original_form": "저랑 성향이 반대시군요.",
          "speaker_id": "2",
          "time": "20210825 19:57"
        }
      ]
