      "utterance": [
        {
          "id": "MDRW2100039423.1.1",
          "form": "대충 공브니가 이걸로 하자",
          "original_form": "대충 공브니가 이걸로 하자",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039423.1.2",
          "form": "학원 다닌 건 중국어고",
          "original_form": "학원 다닌 건 중국어고",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039423.1.3",
          "form": "일본어는 학교에서 배운 게 다야",
          "original_form": "일본어는 학교에서 배운 게 다야",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039423.1.4",
          "form": "일본어 노래 부른거밖에 생각안나",
          "original_form": "일본어 노래 부른거밖에 생각안나",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039423.1.5",
          "form": "노래도 기억못하는디",
          "original_form": "노래도 기억못하는디",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039423.1.6",
          "form": "나 노래 기억 나 내가 추천했어",
          "original_form": "나 노래 기억 나 내가 추천했어",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039423.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039423.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ오타쿠 티를 내었구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋ오타쿠 티를 내었구나",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039423.1.9",
          "form": "아니야",
          "original_form": "아니야",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039423.1.10",
          "form": "유명한 거 했어",
          "original_form": "유명한 거 했어",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039423.1.11",
          "form": "뭐여ㅆ지 카제노 어쩌구 했던건 기억나는데",
          "original_form": "뭐여ㅆ지 카제노 어쩌구 했던건 기억나는데",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039423.1.12",
          "form": "이게 가사야 제목이야ㅋㅋㅋㅋ",
          "original_form": "이게 가사야 제목이야ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039423.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039423.1.14",
          "form": "그 고양이의 보은 오에스티 아냐?",
          "original_form": "그 고양이의 보은 오에스티 아냐?",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039423.1.15",
          "form": "오 맞다",
          "original_form": "오 맞다",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039423.1.16",
          "form": "바람이 되어라 래ㅋㅋㅋㅋ",
          "original_form": "바람이 되어라 래ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039423.1.17",
          "form": "근데 이게 어뜨케 오타쿠티안낸거람",
          "original_form": "근데 이게 어뜨케 오타쿠티안낸거람",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039423.1.18",
          "form": "라라라라라 카제니 나루~",
          "original_form": "라라라라라 카제니 나루~",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039423.1.19",
          "form": "그거 내가 추천한 거 아니야",
          "original_form": "그거 내가 추천한 거 아니야",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039423.1.20",
          "form": "그리고 지브리면 덜 오타쿠 같지",
          "original_form": "그리고 지브리면 덜 오타쿠 같지",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039423.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039423.1.22",
          "form": "지브리.. 자꾸 오타쿠들은 연막으로 사용됨",
          "original_form": "지브리.. 자꾸 오타쿠들은 연막으로 사용됨",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039423.1.23",
          "form": "들의",
          "original_form": "들의",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039423.1.24",
          "form": "학교에서도 지브리 많이 틀어줬던거 같애ㅋㅋㅋ",
          "original_form": "학교에서도 지브리 많이 틀어줬던거 같애ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039423.1.25",
          "form": "근데 내가 추천한거 시달소 오에스티여서",
          "original_form": "근데 내가 추천한거 시달소 오에스티여서",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039423.1.26",
          "form": "지브리보다 더 덕내남",
          "original_form": "지브리보다 더 덕내남",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039423.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039423.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039423.1.29",
          "form": "숨길 마음이 없었지?",
          "original_form": "숨길 마음이 없었지?",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039423.1.30",
          "form": "아냐 그거 나름 유명작이라구ㅠㅠ",
          "original_form": "아냐 그거 나름 유명작이라구ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039423.1.31",
          "form": "심지어 나는 보 지도 안 았어",
          "original_form": "심지어 나는 보 지도 안 았어",
          "speaker_id": "1",
          "time": "20210903 17:45"
        }
      ]
