      "utterance": [
        {
          "id": "MDRW2100030283.1.1",
          "form": "어쩌다가 그런생각햇어?",
          "original_form": "어쩌다가 그런생각햇어?",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030283.1.2",
          "form": "그냥 갑자기 건강을 챙겨야겟다는 생각이들었어",
          "original_form": "그냥 갑자기 건강을 챙겨야겟다는 생각이들었어",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030283.1.3",
          "form": "계단 올라가는게 너무 힘들더라구",
          "original_form": "계단 올라가는게 너무 힘들더라구",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030283.1.4",
          "form": "그렇군,, 미리 챙기는게 좋긴 하지 ㅋㅋ",
          "original_form": "그렇군,, 미리 챙기는게 좋긴 하지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030283.1.5",
          "form": "나이가 들수록 팍 힘드니깐...",
          "original_form": "나이가 들수록 팍 힘드니깐...",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030283.1.6",
          "form": "맞아 그리구 근육도 많이 만들어놔야할거같아",
          "original_form": "맞아 그리구 근육도 많이 만들어놔야할거같아",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030283.1.7",
          "form": "근육은 왜??",
          "original_form": "근육은 왜??",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030283.1.8",
          "form": "운동을 열심히 해서",
          "original_form": "운동을 열심히 해서",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030283.1.9",
          "form": "왜냐면 힘 세면 할수있는게많잖아",
          "original_form": "왜냐면 힘 세면 할수있는게많잖아",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030283.1.10",
          "form": "체육도 잘할수있고 일상생활에서",
          "original_form": "체육도 잘할수있고 일상생활에서",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030283.1.11",
          "form": "ㅋㅋㅋ약한것보단 훨씬 낫긴 하쥐",
          "original_form": "ㅋㅋㅋ약한것보단 훨씬 낫긴 하쥐",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030283.1.12",
          "form": "무거운 짐을 들수도있구",
          "original_form": "무거운 짐을 들수도있구",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030283.1.13",
          "form": "게다가 머랭같은것도 만들어볼수있겠지",
          "original_form": "게다가 머랭같은것도 만들어볼수있겠지",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030283.1.14",
          "form": "아무튼 건강이나 운동은 중요한거같아",
          "original_form": "아무튼 건강이나 운동은 중요한거같아",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.15",
          "form": "머랭만드는것도 운동이",
          "original_form": "머랭만드는것도 운동이",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.16",
          "form": "될 수 잇을지도 ㅋㅋ",
          "original_form": "될 수 잇을지도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.17",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.18",
          "form": "어려서부터 건강관리 해야대...",
          "original_form": "어려서부터 건강관리 해야대...",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.19",
          "form": "나중에 하기엔 이미 늦엇을수도 잇으니깐...!",
          "original_form": "나중에 하기엔 이미 늦엇을수도 잇으니깐...!",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.20",
          "form": "맞아... 크면 건강관리 하기 힘들수도있지",
          "original_form": "맞아... 크면 건강관리 하기 힘들수도있지",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.21",
          "form": "맞아 그럴수잇어",
          "original_form": "맞아 그럴수잇어",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.22",
          "form": "시간도 쩍을거구",
          "original_form": "시간도 쩍을거구",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.23",
          "form": "미리미리 챙겨야지",
          "original_form": "미리미리 챙겨야지",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.24",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.25",
          "form": "웅 ㅎㅎ 건강 챙기좌",
          "original_form": "웅 ㅎㅎ 건강 챙기좌",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.26",
          "form": "홧팅",
          "original_form": "홧팅",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.27",
          "form": "웅! 좋아 ㅎㅎ",
          "original_form": "웅! 좋아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030283.1.28",
          "form": "다이어트 말이야",
          "original_form": "다이어트 말이야",
          "speaker_id": "1",
