      "utterance": [
        {
          "id": "MDRW2100015055.1.1",
          "form": "저는모히또좋아요",
          "original_form": "저는모히또좋아요",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015055.1.2",
          "form": "너무안타까워요",
          "original_form": "너무안타까워요",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015055.1.3",
          "form": "name1님내일은꼭드시길",
          "original_form": "&name1&님내일은꼭드시길",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015055.1.4",
          "form": "모히또는 생각보다 풀떼기맛이라 전 별로더라고요",
          "original_form": "모히또는 생각보다 풀떼기맛이라 전 별로더라고요",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015055.1.5",
          "form": "그럼제가먹겠습니다",
          "original_form": "그럼제가먹겠습니다",
          "speaker_id": "2",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015055.1.6",
          "form": "칵테일불쇼한번보고싶네요",
          "original_form": "칵테일불쇼한번보고싶네요",
          "speaker_id": "2",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015055.1.7",
          "form": "칵테일불먹방은 어떠신가요",
          "original_form": "칵테일불먹방은 어떠신가요",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015055.1.8",
          "form": "유투브각이네요",
          "original_form": "유투브각이네요",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015055.1.9",
          "form": "칵테일의안좋은추억이잇어서리",
          "original_form": "칵테일의안좋은추억이잇어서리",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015055.1.10",
          "form": "무섭네요",
          "original_form": "무섭네요",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015055.1.11",
          "form": "저도 칵테일먹고 식은땀흘렸어서",
          "original_form": "저도 칵테일먹고 식은땀흘렸어서",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015055.1.12",
          "form": "저는속이안좋아서",
          "original_form": "저는속이안좋아서",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015055.1.13",
          "form": "양이 적은 칵테일은 도수가 쎄니",
          "original_form": "양이 적은 칵테일은 도수가 쎄니",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015055.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015055.1.15",
          "form": "털어넣다훅갑닏ㄱㆍ",
          "original_form": "털어넣다훅갑닏ㄱㆍ",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015055.1.16",
          "form": "니다",
          "original_form": "니다",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015055.1.17",
          "form": "양많아서 힘들더라고요",
          "original_form": "양많아서 힘들더라고요",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015055.1.18",
          "form": "똥기저귀를 차고 가야겠어요",
          "original_form": "똥기저귀를 차고 가야겠어요",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015055.1.19",
          "form": "너무많이드신건아니신지",
          "original_form": "너무많이드신건아니신지",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015055.1.20",
          "form": "하는 일이 있어 즐기면서 못마시네요ㅜ",
          "original_form": "하는 일이 있어 즐기면서 못마시네요ㅜ",
