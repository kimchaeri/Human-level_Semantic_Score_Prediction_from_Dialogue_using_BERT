      "utterance": [
        {
          "id": "MDRW2100010058.1.1",
          "form": "날씨가 흐리고 저기압이네",
          "original_form": "날씨가 흐리고 저기압이네",
          "speaker_id": "1",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010058.1.2",
          "form": "여기는 비오고 있어",
          "original_form": "여기는 비오고 있어",
          "speaker_id": "1",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010058.1.3",
          "form": "아직은 추적추적",
          "original_form": "아직은 추적추적",
          "speaker_id": "1",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010058.1.4",
          "form": "어 태풍영향이 확실히 있는듯",
          "original_form": "어 태풍영향이 확실히 있는듯",
          "speaker_id": "2",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010058.1.5",
          "form": "거긴 비와? 여기도 오나",
          "original_form": "거긴 비와? 여기도 오나",
          "speaker_id": "2",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010058.1.6",
          "form": "태풍전야인가...",
          "original_form": "태풍전야인가...",
          "speaker_id": "1",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010058.1.7",
          "form": "내다보니 여기도 비 내리네",
          "original_form": "내다보니 여기도 비 내리네",
          "speaker_id": "2",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010058.1.8",
          "form": "사람들 우산쓰고 다니는구먼",
          "original_form": "사람들 우산쓰고 다니는구먼",
          "speaker_id": "2",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010058.1.9",
          "form": "이따 퇴근할 때까진 세차게 내리지 않음 좋겠구먼",
          "original_form": "이따 퇴근할 때까진 세차게 내리지 않음 좋겠구먼",
          "speaker_id": "1",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010058.1.10",
          "form": "그러게 우산 들고갔지?",
          "original_form": "그러게 우산 들고갔지?",
          "speaker_id": "2",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100010058.1.11",
          "form": "응 왕우산 챙겼어",
          "original_form": "응 왕우산 챙겼어",
          "speaker_id": "1",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100010058.1.12",
          "form": "잘했어 역시 이시기엔 우산이 필수지",
          "original_form": "잘했어 역시 이시기엔 우산이 필수지",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010058.1.13",
          "form": "장화도 많이들 사나봐",
          "original_form": "장화도 많이들 사나봐",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010058.1.14",
          "form": "응 안그래도 장화 사려다",
          "original_form": "응 안그래도 장화 사려다",
          "speaker_id": "1",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010058.1.15",
          "form": "장마가 짧아서 안샀지",
          "original_form": "장마가 짧아서 안샀지",
          "speaker_id": "1",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010058.1.16",
          "form": "근데 가을 장마가 올 줄이야",
          "original_form": "근데 가을 장마가 올 줄이야",
          "speaker_id": "1",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010058.1.17",
          "form": "이젠 슬슬 사도 되겠어 매년 사용할듯",
          "original_form": "이젠 슬슬 사도 되겠어 매년 사용할듯",
          "speaker_id": "2",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010058.1.18",
          "form": "비오는 날씨에 어울리게 기름에 밥 볶아서 아빠랑 먹었다",
          "original_form": "비오는 날씨에 어울리게 기름에 밥 볶아서 아빠랑 먹었다",
          "speaker_id": "2",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010058.1.19",
          "form": "그러게 비가 자주 내리니까",
          "original_form": "그러게 비가 자주 내리니까",
          "speaker_id": "1",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010058.1.20",
          "form": "지글지글",
          "original_form": "지글지글",
          "speaker_id": "2",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010058.1.21",
          "form": "오오 잘했네",
          "original_form": "오오 잘했네",
          "speaker_id": "1",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010058.1.22",
          "form": "날씨와 어울리는 메뉴선택",
          "original_form": "날씨와 어울리는 메뉴선택",
          "speaker_id": "1",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010058.1.23",
          "form": "부침개나 국물 땡기는 날씨라 혹시 드시고 싶은거 있냐 여쭸는데",
          "original_form": "부침개나 국물 땡기는 날씨라 혹시 드시고 싶은거 있냐 여쭸는데",
          "speaker_id": "2",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010058.1.24",
          "form": "별 반응 없으셔서 열심히 볶아드림",
          "original_form": "별 반응 없으셔서 열심히 볶아드림",
          "speaker_id": "2",
          "time": "20210823 14:36"
        },
        {
          "id": "MDRW2100010058.1.25",
          "form": "잘했네",
          "original_form": "잘했네",
          "speaker_id": "1",
          "time": "20210823 14:36"
        },
        {
          "id": "MDRW2100010058.1.26",
          "form": "날씨 흐릴수록 잘 먹고 잘 쉬어야지",
          "original_form": "날씨 흐릴수록 잘 먹고 잘 쉬어야지",
          "speaker_id": "1",
          "time": "20210823 14:36"
        }
      ]
    }
