        },
        {
          "id": "MDRW2100027027.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 07:42"
        },
        {
          "id": "MDRW2100027027.1.3",
          "form": "배달앱을자주쓰나요?",
          "original_form": "배달앱을자주쓰나요?",
          "speaker_id": "2",
          "time": "20210902 07:42"
        },
        {
          "id": "MDRW2100027027.1.4",
          "form": "저는 배달앱 써본적이 아예 없어요",
          "original_form": "저는 배달앱 써본적이 아예 없어요",
          "speaker_id": "1",
          "time": "20210902 07:42"
        },
        {
          "id": "MDRW2100027027.1.5",
          "form": "아진짜요??",
          "original_form": "아진짜요??",
          "speaker_id": "2",
          "time": "20210902 07:52"
        },
        {
          "id": "MDRW2100027027.1.6",
          "form": "그럼 시켜먹을때 어떠케 하나요?",
          "original_form": "그럼 시켜먹을때 어떠케 하나요?",
          "speaker_id": "2",
          "time": "20210902 07:53"
        },
        {
          "id": "MDRW2100027027.1.7",
          "form": "주문해놓고 픽업하고 있어요",
          "original_form": "주문해놓고 픽업하고 있어요",
          "speaker_id": "1",
          "time": "20210902 07:54"
        },
        {
          "id": "MDRW2100027027.1.8",
          "form": "아하!!!주문을그럼저나로하나여?",
          "original_form": "아하!!!주문을그럼저나로하나여?",
          "speaker_id": "2",
          "time": "20210902 08:14"
        },
        {
          "id": "MDRW2100027027.1.9",
          "form": "네 전화로 해요",
          "original_form": "네 전화로 해요",
          "speaker_id": "1",
          "time": "20210902 08:15"
        },
        {
          "id": "MDRW2100027027.1.10",
          "form": "오... 저는 저나로못해서 앱을써요 ㅋㅋㅋ",
          "original_form": "오... 저는 저나로못해서 앱을써요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 08:18"
        },
        {
          "id": "MDRW2100027027.1.11",
          "form": "아하... 앱 쓰면 장점이 있나요?",
          "original_form": "아하... 앱 쓰면 장점이 있나요?",
          "speaker_id": "1",
          "time": "20210902 08:22"
        },
        {
          "id": "MDRW2100027027.1.12",
          "form": "저나를안해도되고몇분에 도착??",
          "original_form": "저나를안해도되고몇분에 도착??",
          "speaker_id": "2",
          "time": "20210902 08:23"
        },
        {
          "id": "MDRW2100027027.1.13",
          "form": "이런거 떠서 좋아요!",
          "original_form": "이런거 떠서 좋아요!",
          "speaker_id": "2",
          "time": "20210902 08:23"
        },
        {
          "id": "MDRW2100027027.1.14",
          "form": "그런 장점이 있군요",
          "original_form": "그런 장점이 있군요",
          "speaker_id": "1",
          "time": "20210902 08:24"
        },
        {
          "id": "MDRW2100027027.1.15",
          "form": "저같이 말못하는사람들한테 정말 좋은앱이죠!",
          "original_form": "저같이 말못하는사람들한테 정말 좋은앱이죠!",
          "speaker_id": "2",
          "time": "20210902 08:25"
        },
        {
          "id": "MDRW2100027027.1.16",
          "form": "저도 말을 잘하는 편은 아닌데",
          "original_form": "저도 말을 잘하는 편은 아닌데",
          "speaker_id": "1",
          "time": "20210902 08:32"
        },
        {
          "id": "MDRW2100027027.1.17",
          "form": "이번 기회에 앱으로 옮길까봐요",
          "original_form": "이번 기회에 앱으로 옮길까봐요",
          "speaker_id": "1",
          "time": "20210902 08:32"
        },
        {
          "id": "MDRW2100027027.1.18",
          "form": "진짜 편해요 픽업할수일는것도있으니 이용해버세오!",
          "original_form": "진짜 편해요 픽업할수일는것도있으니 이용해버세오!",
          "speaker_id": "2",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100027027.1.19",
          "form": "ㅋㅋ 감사합니다 꼭 써볼게요",
          "original_form": "ㅋㅋ 감사합니다 꼭 써볼게요",
          "speaker_id": "1",
          "time": "20210902 08:52"
        },
        {
          "id": "MDRW2100027027.1.20",
          "form": "음식은 머좋아하시나요",
          "original_form": "음식은 머좋아하시나요",
          "speaker_id": "2",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100027027.1.21",
          "form": "떡볶이 좋아해요",
          "original_form": "떡볶이 좋아해요",
          "speaker_id": "1",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100027027.1.22",
          "form": "저도요!!",
          "original_form": "저도요!!",
          "speaker_id": "2",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100027027.1.23",
          "form": "어디 떡볶이 제일 좋아하세요?",
          "original_form": "어디 떡볶이 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100027027.1.24",
          "form": "저 청년다방이랑",
          "original_form": "저 청년다방이랑",
          "speaker_id": "2",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100027027.1.25",
          "form": "시장떡볶이",
          "original_form": "시장떡볶이",
          "speaker_id": "2",
          "time": "20210902 09:02"
        },
        {
          "id": "MDRW2100027027.1.26",
          "form": "떡볶이는안가려요",
          "original_form": "떡볶이는안가려요",
          "speaker_id": "2",
          "time": "20210902 09:02"
        },
        {
          "id": "MDRW2100027027.1.27",
          "form": "저는 엽떡이랑 청년다방이요",
          "original_form": "저는 엽떡이랑 청년다방이요",
          "speaker_id": "1",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100027027.1.28",
          "form": "주변에 있는 떡볶이 맛집도 좋아하구요",
          "original_form": "주변에 있는 떡볶이 맛집도 좋아하구요",
          "speaker_id": "1",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100027027.1.29",
          "form": "맞아요 은근 안알려진게맛있는게만터라구요",
          "original_form": "맞아요 은근 안알려진게맛있는게만터라구요",
          "speaker_id": "2",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100027027.1.30",
          "form": "그래서 가끔은 안가본데 일부러 찾아가요",
          "original_form": "그래서 가끔은 안가본데 일부러 찾아가요",
          "speaker_id": "1",
          "time": "20210902 09:07"
        }
      ]
    }
