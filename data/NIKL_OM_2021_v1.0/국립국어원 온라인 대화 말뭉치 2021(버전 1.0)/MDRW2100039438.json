      "utterance": [
        {
          "id": "MDRW2100039438.1.1",
          "form": "맞아 뮤지컬보는 사람 많더라고",
          "original_form": "맞아 뮤지컬보는 사람 많더라고",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039438.1.2",
          "form": "저도 보는 거 좋아하는데 솔직히 남이 보여주는 거도 아니고 비싸서 못가겠던데요",
          "original_form": "저도 보는 거 좋아하는데 솔직히 남이 보여주는 거도 아니고 비싸서 못가겠던데요",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039438.1.3",
          "form": "전 살면서 딱 한번 뮤 지인이 보여줘서 봤어요",
          "original_form": "전 살면서 딱 한번 뮤 지인이 보여줘서 봤어요",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039438.1.4",
          "form": "15만원이면 최현석 식당에서 코스요리 먹을 수 있어",
          "original_form": "15만원이면 최현석 식당에서 코스요리 먹을 수 있어",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039438.1.5",
          "form": "솔직히 뮤지컬 돈이없어서글치 볼수있으면 볼듯",
          "original_form": "솔직히 뮤지컬 돈이없어서글치 볼수있으면 볼듯",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039438.1.6",
          "form": "응 돈이 없어",
          "original_form": "응 돈이 없어",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039438.1.7",
          "form": "다들 부잣집인가봐",
          "original_form": "다들 부잣집인가봐",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039438.1.8",
          "form": "타이타닉 한번 봤는데 완전 재밌게봤어",
          "original_form": "타이타닉 한번 봤는데 완전 재밌게봤어",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039438.1.9",
          "form": "다들 부자서울사람인가바",
          "original_form": "다들 부자서울사람인가바",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039438.1.10",
          "form": "뮤 한 번에 vip로 본다고 치고 10~15만원 잡는데",
          "original_form": "뮤 한 번에 vip로 본다고 치고 10~15만원 잡는데",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039438.1.11",
          "form": "연극은 비교적 더 싸죠",
          "original_form": "연극은 비교적 더 싸죠",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039438.1.12",
          "form": "중소기업기준 초봉 월급이 180만원일거잖아",
          "original_form": "중소기업기준 초봉 월급이 180만원일거잖아",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039438.1.13",
          "form": "어떻게 본대",
          "original_form": "어떻게 본대",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039438.1.14",
          "form": "연극 대학다닐때 동아리에서 연극 단체표끊어서 봤는데 재밌더라",
          "original_form": "연극 대학다닐때 동아리에서 연극 단체표끊어서 봤는데 재밌더라",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039438.1.15",
          "form": "연극은 저도 자주봤어요 셰익스피어 연극은 거진 본 거 같아",
          "original_form": "연극은 저도 자주봤어요 셰익스피어 연극은 거진 본 거 같아",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039438.1.16",
          "form": "부르주아들의 취미 뮤지컬",
          "original_form": "부르주아들의 취미 뮤지컬",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039438.1.17",
          "form": "전 자주는 아니구... 한두번ㅎ 셰익스피어 연극도 올리는구나",
          "original_form": "전 자주는 아니구... 한두번ㅎ 셰익스피어 연극도 올리는구나",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039438.1.18",
          "form": "서울까지 ktx타고 가서 보는데",
          "original_form": "서울까지 ktx타고 가서 보는데",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039438.1.19",
          "form": "저 가난해요ㅠㅠ 하면 약간 음... 네... 됨",
          "original_form": "저 가난해요ㅠㅠ 하면 약간 음... 네... 됨",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039438.1.20",
          "form": "저도 중산층인데 뮤 같은 거 보고 안 자랐다고요",
          "original_form": "저도 중산층인데 뮤 같은 거 보고 안 자랐다고요",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039438.1.21",
          "form": "마자..... 음.. 네.....,.. 그렇게까지 갈아서 사는 쪽이든 있어서 쓰는 쪽이든 우리랑은 먼 문화생활 향유임",
          "original_form": "마자..... 음.. 네.....,.. 그렇게까지 갈아서 사는 쪽이든 있어서 쓰는 쪽이든 우리랑은 먼 문화생활 향유임",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039438.1.22",
          "form": "지방민은 영화 정도로 대충 보고살았습니다",
          "original_form": "지방민은 영화 정도로 대충 보고살았습니다",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039438.1.23",
          "form": "맞아... 걍 일년에 한 번쯤 보면 모를까",
          "original_form": "맞아... 걍 일년에 한 번쯤 보면 모를까",
