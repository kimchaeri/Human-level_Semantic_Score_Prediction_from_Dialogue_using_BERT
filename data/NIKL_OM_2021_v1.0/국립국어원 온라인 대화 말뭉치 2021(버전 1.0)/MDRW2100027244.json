      "utterance": [
        {
          "id": "MDRW2100027244.1.1",
          "form": "여행얘기하실래요?!",
          "original_form": "여행얘기하실래요?!",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027244.1.2",
          "form": "네네ㅋㅋㅋ 좋아하는 여행지 있나요?",
          "original_form": "네네ㅋㅋㅋ 좋아하는 여행지 있나요?",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027244.1.3",
          "form": "코로나 전에는 태국이여 ㅠㅠ",
          "original_form": "코로나 전에는 태국이여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027244.1.4",
          "form": "태국음식도 좋아하는 편이고 마사지받고 호텔놀이 하는 것도 좋아서..",
          "original_form": "태국음식도 좋아하는 편이고 마사지받고 호텔놀이 하는 것도 좋아서..",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027244.1.5",
          "form": "다시 가고 싶네요 ㅠ",
          "original_form": "다시 가고 싶네요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027244.1.6",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027244.1.7",
          "form": "전 해외는 일본만 가봤어요ㅋㅋ",
          "original_form": "전 해외는 일본만 가봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027244.1.8",
          "form": "일본도 여행하기 좋죠 ㅠㅠ",
          "original_form": "일본도 여행하기 좋죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027244.1.9",
          "form": "특히 우리나라랑 가깝고 싸서..",
          "original_form": "특히 우리나라랑 가깝고 싸서..",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027244.1.10",
          "form": "국내여행지 추천 있으신가용",
          "original_form": "국내여행지 추천 있으신가용",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027244.1.11",
          "form": "강릉이요",
          "original_form": "강릉이요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027244.1.12",
          "form": "바다보러 잘 갔다와요",
          "original_form": "바다보러 잘 갔다와요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027244.1.13",
          "form": "와 요새 강릉이 그렇게 핫하다면서요",
          "original_form": "와 요새 강릉이 그렇게 핫하다면서요",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027244.1.14",
          "form": "저 제대로 못가봐서 궁금해요",
          "original_form": "저 제대로 못가봐서 궁금해요",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027244.1.15",
          "form": "전 안목해변 주로 가는데",
          "original_form": "전 안목해변 주로 가는데",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027244.1.16",
          "form": "커피거리로 유명해서 원래도 사람들 많았어요",
          "original_form": "커피거리로 유명해서 원래도 사람들 많았어요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027244.1.17",
          "form": "커피거리 들어봤어요.",
          "original_form": "커피거리 들어봤어요.",
          "speaker_id": "1",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027244.1.18",
          "form": "사실 작년에 강릉 가려고 했다가... 코로나 단계 격상되면서 취소했네요.",
          "original_form": "사실 작년에 강릉 가려고 했다가... 코로나 단계 격상되면서 취소했네요.",
          "speaker_id": "1",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027244.1.19",
          "form": "이번 겨울에 꼭 가보고 싶어요",
          "original_form": "이번 겨울에 꼭 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027244.1.20",
          "form": "그러게요 좀 잦아들면ㅎㅎ 백신 다 맞거나요",
          "original_form": "그러게요 좀 잦아들면ㅎㅎ 백신 다 맞거나요",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027244.1.21",
          "form": "커피거리 카페 추천 가능한가요 ㅋㅋ",
          "original_form": "커피거리 카페 추천 가능한가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027244.1.22",
          "form": "ㅋㅋ산토리니라고 카페 인테리어 예쁜데 있어요",
          "original_form": "ㅋㅋ산토리니라고 카페 인테리어 예쁜데 있어요",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027244.1.23",
          "form": "다른곳도 루프탑 있고 해서 경치보기 좋구여",
          "original_form": "다른곳도 루프탑 있고 해서 경치보기 좋구여",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027244.1.24",
          "form": "앱에 저장했습니다. 친구랑 가야겠어요 ㅋㅋㅋ",
          "original_form": "앱에 저장했습니다. 친구랑 가야겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027244.1.25",
          "form": "전 작년에 담양 다녀왔는데 너무 좋았어요",
          "original_form": "전 작년에 담양 다녀왔는데 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210902 12:19"
        }
      ]
    }
  ]
