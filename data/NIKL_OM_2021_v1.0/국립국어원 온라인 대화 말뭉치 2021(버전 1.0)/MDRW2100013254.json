      "utterance": [
        {
          "id": "MDRW2100013254.1.1",
          "form": "어떤 영화 좋아하세요?",
          "original_form": "어떤 영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013254.1.2",
          "form": "저 로맨스나 액션 좋아해요",
          "original_form": "저 로맨스나 액션 좋아해요",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013254.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013254.1.4",
          "form": "저도 액션 좋아해요",
          "original_form": "저도 액션 좋아해요",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013254.1.5",
          "form": "히어로 영화 많잖아요",
          "original_form": "히어로 영화 많잖아요",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013254.1.6",
          "form": "히어로는 잘 안봐요",
          "original_form": "히어로는 잘 안봐요",
          "speaker_id": "2",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100013254.1.7",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100013254.1.8",
          "form": "유명한건 좀 봤어요",
          "original_form": "유명한건 좀 봤어요",
          "speaker_id": "1",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013254.1.9",
          "form": "최근에 블랙위도우? 이것도 보셨나요",
          "original_form": "최근에 블랙위도우? 이것도 보셨나요",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013254.1.10",
          "form": "못봤어요ㅠ 재밌다던데",
          "original_form": "못봤어요ㅠ 재밌다던데",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013254.1.11",
          "form": "그니까요 다들 재밌다더라구요~",
          "original_form": "그니까요 다들 재밌다더라구요~",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013254.1.12",
          "form": "나중에라도 봐야겠어요",
          "original_form": "나중에라도 봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013254.1.13",
          "form": "재밌게 본 영화 있으세요?",
          "original_form": "재밌게 본 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013254.1.14",
          "form": "해리포터 좋아해요!",
          "original_form": "해리포터 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013254.1.15",
          "form": "해리포터 시리즈 다 재밌죠ㅋㅋ",
          "original_form": "해리포터 시리즈 다 재밌죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013254.1.16",
          "form": "판타지 좋아하세요?",
          "original_form": "판타지 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013254.1.17",
          "form": "네 판타지도 즐겨보는 편이에요!!",
          "original_form": "네 판타지도 즐겨보는 편이에요!!",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013254.1.18",
          "form": "저도요. 반지의 제왕도 재밌었어요",
          "original_form": "저도요. 반지의 제왕도 재밌었어요",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013254.1.19",
          "form": "참 그건 유명한데 아직 못봤네요",
