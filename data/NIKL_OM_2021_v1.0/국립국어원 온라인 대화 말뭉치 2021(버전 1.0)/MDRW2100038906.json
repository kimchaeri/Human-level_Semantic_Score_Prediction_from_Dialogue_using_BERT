      "utterance": [
        {
          "id": "MDRW2100038906.1.1",
          "form": "야식 먹고싶네요",
          "original_form": "야식 먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038906.1.2",
          "form": "지금 야식으로 먹을만한거 있나유?",
          "original_form": "지금 야식으로 먹을만한거 있나유?",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100038906.1.3",
          "form": "어느새 낮이 되어버렸네요...ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어느새 낮이 되어버렸네요...ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:19"
        },
        {
          "id": "MDRW2100038906.1.4",
          "form": "저는 야식을 자주 먹는편은 아니지만",
          "original_form": "저는 야식을 자주 먹는편은 아니지만",
          "speaker_id": "2",
          "time": "20210903 13:19"
        },
        {
          "id": "MDRW2100038906.1.5",
          "form": "밤되면 육회가 그렇게 먹고싶어지더라구요",
          "original_form": "밤되면 육회가 그렇게 먹고싶어지더라구요",
          "speaker_id": "2",
          "time": "20210903 13:20"
        },
        {
          "id": "MDRW2100038906.1.6",
          "form": "육회 안먹은지 너무 오래됐는데 먹고싶다ㅠㅠㅠㅠ",
          "original_form": "육회 안먹은지 너무 오래됐는데 먹고싶다ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 13:52"
        },
        {
          "id": "MDRW2100038906.1.7",
          "form": "전 야식 안먹다가 최근 계속 먹고있어요...ㅎ...",
          "original_form": "전 야식 안먹다가 최근 계속 먹고있어요...ㅎ...",
          "speaker_id": "1",
          "time": "20210903 13:53"
        },
        {
          "id": "MDRW2100038906.1.8",
          "form": "핫 어제도 드셨나요?",
          "original_form": "핫 어제도 드셨나요?",
          "speaker_id": "2",
          "time": "20210903 13:57"
        },
        {
          "id": "MDRW2100038906.1.9",
          "form": "어제 홍차아이스티랑 포카칩을 호로록 했어요",
          "original_form": "어제 홍차아이스티랑 포카칩을 호로록 했어요",
          "speaker_id": "1",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100038906.1.10",
          "form": "와 메뉴 대박 ㅠㅠㅠㅠ 홈카페 느낌 낭낭하네요 ㅠㅠㅠ",
          "original_form": "와 메뉴 대박 ㅠㅠㅠㅠ 홈카페 느낌 낭낭하네요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100038906.1.11",
          "form": "홍차 아이스티는 실론티 같은건가요",
          "original_form": "홍차 아이스티는 실론티 같은건가요",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100038906.1.12",
          "form": "실론티를 못마셔봐서...ㅋㅋㅋㅋ 홍차+복숭아맛",
          "original_form": "실론티를 못마셔봐서...ㅋㅋㅋㅋ 홍차+복숭아맛",
          "speaker_id": "1",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100038906.1.13",
          "form": "?! 실론티 안마셔봤어? 완젼 맛있어유!",
          "original_form": "?! 실론티 안마셔봤어? 완젼 맛있어유!",
          "speaker_id": "2",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100038906.1.14",
          "form": "마셔보는거 추천추천",
          "original_form": "마셔보는거 추천추천",
          "speaker_id": "2",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100038906.1.15",
          "form": "홍차에 과일맛 나는거 그렇게 좋아하는 편은 아닌데",
          "original_form": "홍차에 과일맛 나는거 그렇게 좋아하는 편은 아닌데",
          "speaker_id": "2",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100038906.1.16",
          "form": "복숭아는 뭔가 잘어울릴것 같다!",
          "original_form": "복숭아는 뭔가 잘어울릴것 같다!",
          "speaker_id": "2",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100038906.1.17",
          "form": "야식 하면 족발 치킨 피자 이런거 생각햇는데",
          "original_form": "야식 하면 족발 치킨 피자 이런거 생각햇는데",
          "speaker_id": "2",
          "time": "20210903 15:23"
        },
        {
          "id": "MDRW2100038906.1.18",
          "form": "메뉴가 우아하네요!",
          "original_form": "메뉴가 우아하네요!",
          "speaker_id": "2",
          "time": "20210903 15:23"
        },
        {
          "id": "MDRW2100038906.1.19",
          "form": "가족들 다 있으니 혼자 배달시켜서 먹기도 그렇고",
          "original_form": "가족들 다 있으니 혼자 배달시켜서 먹기도 그렇고",
          "speaker_id": "1",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100038906.1.20",
          "form": "집에 있는게 과자뿐이여서ㅋㅋㅋㅋ",
          "original_form": "집에 있는게 과자뿐이여서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100038906.1.21",
          "form": "실론티 나중에 한번 마셔보겠어유!!",
          "original_form": "실론티 나중에 한번 마셔보겠어유!!",
          "speaker_id": "1",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100038906.1.22",
          "form": "넹 꼭 마셔봐유!",
          "original_form": "넹 꼭 마셔봐유!",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100038906.1.23",
          "form": "집에 과자 많나요?",
          "original_form": "집에 과자 많나요?",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100038906.1.24",
          "form": "져희집은 엄마가 박스채로 시키는걸 좋아해서",
          "original_form": "져희집은 엄마가 박스채로 시키는걸 좋아해서",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100038906.1.25",
          "form": "얼마전 까지만해도 몽쉘이 여섯박스있엇네료",
          "original_form": "얼마전 까지만해도 몽쉘이 여섯박스있엇네료",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100038906.1.26",
          "form": "엄마가 음식은 박스로 주문하는데 과자는 제가 먹고싶으면 사요ㅋㅋㅋ",
          "original_form": "엄마가 음식은 박스로 주문하는데 과자는 제가 먹고싶으면 사요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100038906.1.27",
          "form": "오늘도 주말을 위해 간식을 샀습니다ㅎㅅㅎ",
          "original_form": "오늘도 주말을 위해 간식을 샀습니다ㅎㅅㅎ",
          "speaker_id": "1",
          "time": "20210903 16:12"
        }
      ]
