      "utterance": [
        {
          "id": "MDRW2100029331.1.1",
          "form": "안녕히 주무셨습니까",
          "original_form": "안녕히 주무셨습니까",
          "speaker_id": "2",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100029331.1.2",
          "form": "완전 꿀잠 잤죠ㅎㅎ 오늘도 회사 쉬는 날이라 강아지와 산책중",
          "original_form": "완전 꿀잠 잤죠ㅎㅎ 오늘도 회사 쉬는 날이라 강아지와 산책중",
          "speaker_id": "1",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100029331.1.3",
          "form": "님은 출근하셨나요?",
          "original_form": "님은 출근하셨나요?",
          "speaker_id": "1",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100029331.1.4",
          "form": "네 ㅋㅋ커피하나 들고 출근했습니다",
          "original_form": "네 ㅋㅋ커피하나 들고 출근했습니다",
          "speaker_id": "2",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100029331.1.5",
          "form": "커피는 스타벅스 캡슐?",
          "original_form": "커피는 스타벅스 캡슐?",
          "speaker_id": "2",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100029331.1.6",
          "form": "그걸로 하나 내렸습니다",
          "original_form": "그걸로 하나 내렸습니다",
          "speaker_id": "2",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100029331.1.7",
          "form": "역시 부르주아",
          "original_form": "역시 부르주아",
          "speaker_id": "1",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100029331.1.8",
          "form": "아침은 뭐 드셨나유",
          "original_form": "아침은 뭐 드셨나유",
          "speaker_id": "1",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100029331.1.9",
          "form": "그 카스테라",
          "original_form": "그 카스테라",
          "speaker_id": "2",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100029331.1.10",
          "form": "남은거 세조각 묵엇서요",
          "original_form": "남은거 세조각 묵엇서요",
          "speaker_id": "2",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100029331.1.11",
          "form": "식사 하셨어용?",
          "original_form": "식사 하셨어용?",
          "speaker_id": "2",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100029331.1.12",
          "form": "요즘 날좋아서 산책하기 좋겠습니당 ㅋㅋ",
          "original_form": "요즘 날좋아서 산책하기 좋겠습니당 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100029331.1.13",
          "form": "앗 카스테라가 아직 남았었군요!",
          "original_form": "앗 카스테라가 아직 남았었군요!",
          "speaker_id": "1",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100029331.1.14",
          "form": "소시민인 저는 카누로 떼웠죠ㅎ",
          "original_form": "소시민인 저는 카누로 떼웠죠ㅎ",
          "speaker_id": "1",
          "time": "20210903 09:30"
        },
        {
          "id": "MDRW2100029331.1.15",
          "form": "날 진짜 좋아요!",
          "original_form": "날 진짜 좋아요!",
          "speaker_id": "1",
          "time": "20210903 09:30"
        },
        {
          "id": "MDRW2100029331.1.16",
          "form": "저는 노비",
          "original_form": "저는 노비",
          "speaker_id": "2",
          "time": "20210903 09:32"
        },
        {
          "id": "MDRW2100029331.1.17",
          "form": "노 예가 안되네요 ㅋㅋ",
          "original_form": "노 예가 안되네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:32"
        },
        {
          "id": "MDRW2100029331.1.18",
          "form": "카누 깔끔하드라구요 ㅋㅋ",
          "original_form": "카누 깔끔하드라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:32"
        },
        {
          "id": "MDRW2100029331.1.19",
          "form": "공노비시군요ㅎㅎ",
          "original_form": "공노비시군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 09:33"
        },
        {
          "id": "MDRW2100029331.1.20",
          "form": "스벅 드시고 그런 말 하면 뭐해욧!",
          "original_form": "스벅 드시고 그런 말 하면 뭐해욧!",
          "speaker_id": "1",
          "time": "20210903 09:33"
        },
        {
          "id": "MDRW2100029331.1.21",
          "form": "스벅 쌉니다 ㅋㅋ",
          "original_form": "스벅 쌉니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100029331.1.22",
          "form": "오전회의 하면서 이디야 가루로 된거 타서 마시고있네요",
          "original_form": "오전회의 하면서 이디야 가루로 된거 타서 마시고있네요",
          "speaker_id": "2",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029331.1.23",
          "form": "요즘 화상회으로 하니까",
          "original_form": "요즘 화상회으로 하니까",
          "speaker_id": "2",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029331.1.24",
          "form": "간식먹으면서 회의하네요 ㅋㅋ",
          "original_form": "간식먹으면서 회의하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029331.1.25",
          "form": "스벅캡슐이 싸다니ㅎㅎ  눈높이 교육 안 받으셨남유ㅎㅎ",
          "original_form": "스벅캡슐이 싸다니ㅎㅎ  눈높이 교육 안 받으셨남유ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100029331.1.26",
          "form": "간식 먹으며 회의라니 역시 노비 중 노비는 공노비네요ㅎㅎ",
          "original_form": "간식 먹으며 회의라니 역시 노비 중 노비는 공노비네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100029331.1.27",
          "form": "회사에서 무슨 간식 줬나요.!",
          "original_form": "회사에서 무슨 간식 줬나요.!",
          "speaker_id": "1",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100029331.1.28",
          "form": "회장님 다금바리 사쥬세여 ㅠ",
          "original_form": "회장님 다금바리 사쥬세여 ㅠ",
          "speaker_id": "2",
          "time": "20210903 11:19"
        },
        {
          "id": "MDRW2100029331.1.29",
          "form": "회의할때 다과 잇져?",
          "original_form": "회의할때 다과 잇져?",
          "speaker_id": "2",
          "time": "20210903 11:19"
        },
        {
          "id": "MDRW2100029331.1.30",
          "form": "참ing있네요 ㅎㅎ",
          "original_form": "참ing있네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:19"
        },
        {
          "id": "MDRW2100029331.1.31",
          "form": "제스타일입니다",
          "original_form": "제스타일입니다",
          "speaker_id": "2",
          "time": "20210903 11:19"
        },
        {
          "id": "MDRW2100029331.1.32",
          "form": "우리는 회의할 때 다과 없습니다ㅎㅎ",
          "original_form": "우리는 회의할 때 다과 없습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029331.1.33",
          "form": "먹다 체할 분위기입니다ㅎㅎ",
          "original_form": "먹다 체할 분위기입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029331.1.34",
          "form": "차밍이라니 참 입맛이네요",
          "original_form": "차밍이라니 참 입맛이네요",
          "speaker_id": "1",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029331.1.35",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029331.1.36",
          "form": "네 아재입맛인듯 ㅋㅋ",
          "original_form": "네 아재입맛인듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029331.1.37",
          "form": "얼굴은 대학생인데",
          "original_form": "얼굴은 대학생인데",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029331.1.38",
          "form": "몸속 깊은곳에서 아재미가 ㅋㅋ",
          "original_form": "몸속 깊은곳에서 아재미가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029331.1.39",
          "form": "얼굴이 대학생이라니 거울 보셨슴꽈!",
          "original_form": "얼굴이 대학생이라니 거울 보셨슴꽈!",
          "speaker_id": "1",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029331.1.40",
          "form": "참크래커는 좋아하시나요 아이비나",
          "original_form": "참크래커는 좋아하시나요 아이비나",
          "speaker_id": "1",
          "time": "20210903 11:36"
        }
      ]
    }
  ]
