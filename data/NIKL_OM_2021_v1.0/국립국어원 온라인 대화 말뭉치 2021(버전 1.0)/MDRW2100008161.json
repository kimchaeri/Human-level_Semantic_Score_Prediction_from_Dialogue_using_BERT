      "utterance": [
        {
          "id": "MDRW2100008161.1.1",
          "form": "name3어머님~",
          "original_form": "&name3&어머님~",
          "speaker_id": "1",
          "time": "20210822 14:22"
        },
        {
          "id": "MDRW2100008161.1.2",
          "form": "자유시간 입니꺼??",
          "original_form": "자유시간 입니꺼??",
          "speaker_id": "1",
          "time": "20210822 14:22"
        },
        {
          "id": "MDRW2100008161.1.3",
          "form": "빨래정리중ㅋㅋ",
          "original_form": "빨래정리중ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008161.1.4",
          "form": "날좋은날은 빨래지ㅋㅋ",
          "original_form": "날좋은날은 빨래지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008161.1.5",
          "form": "빨래는 건조기 아녀요??ㅋㅋㅋㅋㅋ",
          "original_form": "빨래는 건조기 아녀요??ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008161.1.6",
          "form": "난도 집에 빨래 한가득인데ㅠ",
          "original_form": "난도 집에 빨래 한가득인데ㅠ",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008161.1.7",
          "form": "날이 아깝다",
          "original_form": "날이 아깝다",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008161.1.8",
          "form": "바람도좋고",
          "original_form": "바람도좋고",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008161.1.9",
          "form": "그건맞아요~",
          "original_form": "그건맞아요~",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008161.1.10",
          "form": "애들은 과자ㅋㅋ",
          "original_form": "애들은 과자ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008161.1.11",
          "form": "햇볕에 바싹 말리면",
          "original_form": "햇볕에 바싹 말리면",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008161.1.12",
          "form": "오늘과자파티",
          "original_form": "오늘과자파티",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008161.1.13",
          "form": "기부니가 조크든여 ㅋㅋㅋㅋ",
          "original_form": "기부니가 조크든여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008161.1.14",
          "form": "지는 친정와가 5키로는 찐듯..ㅠㅠ",
          "original_form": "지는 친정와가 5키로는 찐듯..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008161.1.15",
          "form": "집에가서 많이 움직녀야긋어요~",
          "original_form": "집에가서 많이 움직녀야긋어요~",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008161.1.16",
          "form": "집에서 홈트하자ㅋㅋ",
          "original_form": "집에서 홈트하자ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008161.1.17",
          "form": "밖에는 못나감ㅋ",
          "original_form": "밖에는 못나감ㅋ",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008161.1.18",
          "form": "ㅋㅋㅋㅋ홈트는 의지박약 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ홈트는 의지박약 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008161.1.19",
          "form": "밖에는 나가면 걍 걸어가는데",
          "original_form": "밖에는 나가면 걍 걸어가는데",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008161.1.20",
          "form": "집에선 쫌만하면 안허게되요 ㅋㅋ",
          "original_form": "집에선 쫌만하면 안허게되요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008161.1.21",
          "form": "아하ㅜ",
          "original_form": "아하ㅜ",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008161.1.22",
          "form": "나도 그렇긴하다 아파트 또 끊으까ㅋㅋ",
          "original_form": "나도 그렇긴하다 아파트 또 끊으까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008161.1.23",
          "form": "헬스",
          "original_form": "헬스",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008161.1.24",
          "form": "ㅋㅋㅋㅋㅋ언니네는 돈내고 할만해여~우리 아팟은...",
          "original_form": "ㅋㅋㅋㅋㅋ언니네는 돈내고 할만해여~우리 아팟은...",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008161.1.25",
          "form": "만원이니ㅋㅋ",
          "original_form": "만원이니ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008161.1.26",
          "form": "하면좋긴한데 기부하는 느낌적인느낌",
          "original_form": "하면좋긴한데 기부하는 느낌적인느낌",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008161.1.27",
          "form": "육퇴하고 하면좋은데",
          "original_form": "육퇴하고 하면좋은데",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008161.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ육퇴하고나믄 솔직히 운동하러 멀리가기도 그러니~",
          "original_form": "ㅋㅋㅋㅋㅋㅋ육퇴하고나믄 솔직히 운동하러 멀리가기도 그러니~",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008161.1.29",
          "form": "아팟안에 헬스장이 딱인듯요..",
          "original_form": "아팟안에 헬스장이 딱인듯요..",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008161.1.30",
          "form": "그렇지ㅋㅋ 활동하자 ㅋㅋ",
          "original_form": "그렇지ㅋㅋ 활동하자 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008161.1.31",
          "form": "우리아팟은 진짜 허접..관리도 안되고ㅠ",
          "original_form": "우리아팟은 진짜 허접..관리도 안되고ㅠ",
          "speaker_id": "1",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008161.1.32",
          "form": "헐ㅡㅡ;;",
          "original_form": "헐ㅡㅡ;;",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008161.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ대신 공짜임..한번가고 안가여 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ대신 공짜임..한번가고 안가여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008161.1.34",
          "form": "오호 공짜니깐 그렇구먼",
          "original_form": "오호 공짜니깐 그렇구먼",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008161.1.35",
          "form": "편한 주제로 돌아왔습죠 ㅋㅋㅋㅋㅋ",
          "original_form": "편한 주제로 돌아왔습죠 ㅋㅋㅋㅋㅋ",
