      "utterance": [
        {
          "id": "MDRW2100013175.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100013175.1.2",
          "form": "안녕하세요! 무슨 음식 좋아하세요?",
          "original_form": "안녕하세요! 무슨 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013175.1.3",
          "form": "전 초밥이요ㅋㅋㅋ",
          "original_form": "전 초밥이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013175.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013175.1.5",
          "form": "아 저도 초밥 진짜 좋아해요 ㅠㅠ",
          "original_form": "아 저도 초밥 진짜 좋아해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013175.1.6",
          "form": "연어초밥이랑 생새우초밥 너무 맛있어요",
          "original_form": "연어초밥이랑 생새우초밥 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013175.1.7",
          "form": "와우 저랑조금다르시네요",
          "original_form": "와우 저랑조금다르시네요",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013175.1.8",
          "form": "저는 연어vs광어하면",
          "original_form": "저는 연어vs광어하면",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013175.1.9",
          "form": "광어파랍니다ㅋㅋㅋ",
          "original_form": "광어파랍니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013175.1.10",
          "form": "광어도 맛있죠ㅠㅠㅠㅠㅠ쫄깃담백!!",
          "original_form": "광어도 맛있죠ㅠㅠㅠㅠㅠ쫄깃담백!!",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013175.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100013175.1.12",
          "form": "보쌈vs족발 어떤거좋아하세요?",
          "original_form": "보쌈vs족발 어떤거좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100013175.1.13",
          "form": "저는 족발이요!!!!!@8!",
          "original_form": "저는 족발이요!!!!!@8!",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013175.1.14",
          "form": "보쌈은 약간 퍽퍽하고 이에 끼더라구요..",
          "original_form": "보쌈은 약간 퍽퍽하고 이에 끼더라구요..",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013175.1.15",
          "form": "글구 제가 약간 쫄깃탱탱한 식감을 좋아해요",
          "original_form": "글구 제가 약간 쫄깃탱탱한 식감을 좋아해요",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013175.1.16",
          "form": "ㅋㅋㅋㅋ저랑다르군요!!!!!",
          "original_form": "ㅋㅋㅋㅋ저랑다르군요!!!!!",
          "speaker_id": "1",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100013175.1.17",
          "form": "저는 보쌈파랍니다",
          "original_form": "저는 보쌈파랍니다",
          "speaker_id": "1",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100013175.1.18",
          "form": "보쌈도 맛있죵 ㅣㅎㅎ무말랭이랑 같이먹으면 꿀맛",
          "original_form": "보쌈도 맛있죵 ㅣㅎㅎ무말랭이랑 같이먹으면 꿀맛",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100013175.1.19",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100013175.1.20",
          "form": "저녁은 잘드셨나요?",
          "original_form": "저녁은 잘드셨나요?",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100013175.1.21",
          "form": "아뇨 하..건강 챙기겠다교 양배추 볶아먹었는데 넘 맛없었어요",
          "original_form": "아뇨 하..건강 챙기겠다교 양배추 볶아먹었는데 넘 맛없었어요",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100013175.1.22",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100013175.1.23",
          "form": "양배추맛있지않나요?",
          "original_form": "양배추맛있지않나요?",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100013175.1.24",
          "form": "아니 분명 저번에는 진짜 맛있게 됐었는데",
          "original_form": "아니 분명 저번에는 진짜 맛있게 됐었는데",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100013175.1.25",
          "form": "이번엔 양배추가 오래돼서 상태가 좀 안좋았어서 그랬나봐요",
          "original_form": "이번엔 양배추가 오래돼서 상태가 좀 안좋았어서 그랬나봐요",
