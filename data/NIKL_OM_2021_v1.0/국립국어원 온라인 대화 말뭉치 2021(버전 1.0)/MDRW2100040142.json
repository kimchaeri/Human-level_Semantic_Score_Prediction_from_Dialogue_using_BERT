      "utterance": [
        {
          "id": "MDRW2100040142.1.1",
          "form": "여행 조아하세요?",
          "original_form": "여행 조아하세요?",
          "speaker_id": "2",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100040142.1.2",
          "form": "네..많이",
          "original_form": "네..많이",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100040142.1.3",
          "form": "추천여행지있나요",
          "original_form": "추천여행지있나요",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100040142.1.4",
          "form": "코로나로..우리나라로..",
          "original_form": "코로나로..우리나라로..",
          "speaker_id": "1",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100040142.1.5",
          "form": "경주도좋고..부산도좋고",
          "original_form": "경주도좋고..부산도좋고",
          "speaker_id": "1",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100040142.1.6",
          "form": "원래는 해외 자주 가셨나요",
          "original_form": "원래는 해외 자주 가셨나요",
          "speaker_id": "2",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100040142.1.7",
          "form": "대학교땐 배낭여행으로",
          "original_form": "대학교땐 배낭여행으로",
          "speaker_id": "1",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100040142.1.8",
          "form": "결혼해서는 애들위주로요",
          "original_form": "결혼해서는 애들위주로요",
          "speaker_id": "1",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100040142.1.9",
          "form": "오 그렇균요 배낭여행도 다녀오셨구나 어디가셨나요",
          "original_form": "오 그렇균요 배낭여행도 다녀오셨구나 어디가셨나요",
          "speaker_id": "2",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100040142.1.10",
          "form": "유럽이랑 홍콩요",
          "original_form": "유럽이랑 홍콩요",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100040142.1.11",
          "form": "우와 ㅠㅠ 유럽 어디다녀오셨나요",
          "original_form": "우와 ㅠㅠ 유럽 어디다녀오셨나요",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100040142.1.12",
          "form": "잔 못가봤어요",
          "original_form": "잔 못가봤어요",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100040142.1.13",
          "form": "한달동안 여러곳요",
          "original_form": "한달동안 여러곳요",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100040142.1.14",
          "form": "스위스.영국.프랑스등 7개국정도요",
          "original_form": "스위스.영국.프랑스등 7개국정도요",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100040142.1.15",
          "form": "우와 정말 멋지시다 ... 노르웨이 가보셨나요",
          "original_form": "우와 정말 멋지시다 ... 노르웨이 가보셨나요",
          "speaker_id": "2",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100040142.1.16",
          "form": "아니요..",
          "original_form": "아니요..",
          "speaker_id": "1",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100040142.1.17",
          "form": "전 오스트리아.네델란드.이탈리아. 모나코...기억이잘 안나네요",
          "original_form": "전 오스트리아.네델란드.이탈리아. 모나코...기억이잘 안나네요",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100040142.1.18",
          "form": "오 그렇군요 정말 좋은 경험이에요",
          "original_form": "오 그렇군요 정말 좋은 경험이에요",
