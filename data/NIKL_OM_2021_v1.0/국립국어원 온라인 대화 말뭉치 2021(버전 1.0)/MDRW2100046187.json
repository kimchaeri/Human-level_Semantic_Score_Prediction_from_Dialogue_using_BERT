      "utterance": [
        {
          "id": "MDRW2100046187.1.1",
          "form": "본인이 고기를 구워야 직성이 풀리는 스타일이에요",
          "original_form": "본인이 고기를 구워야 직성이 풀리는 스타일이에요",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046187.1.2",
          "form": "우아 네넨  너무 복받으셧네요 ㅋㅋㅋ",
          "original_form": "우아 네넨  너무 복받으셧네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046187.1.3",
          "form": "덕분에 주위 사람은 편하죠",
          "original_form": "덕분에 주위 사람은 편하죠",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046187.1.4",
          "form": "ㅋㅋㅋㅋ 맞아요 고기 굽는사람이ㅛ으면",
          "original_form": "ㅋㅋㅋㅋ 맞아요 고기 굽는사람이ㅛ으면",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046187.1.5",
          "form": "요리하는 건 좋아하는데",
          "original_form": "요리하는 건 좋아하는데",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046187.1.6",
          "form": "치우는 건 못해서",
          "original_form": "치우는 건 못해서",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046187.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046187.1.8",
          "form": "앜ㅋㅋㅋㅋ 그래도 한가지라도 잘하셔서 ㅠㅜ",
          "original_form": "앜ㅋㅋㅋㅋ 그래도 한가지라도 잘하셔서 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046187.1.9",
          "form": "치우는 건 늘 제 몫 이랍니다ㅋㅋㅋㅋㅋㄱ",
          "original_form": "치우는 건 늘 제 몫 이랍니다ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046187.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ 네 전 청소는 다행히 좋아해서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ 네 전 청소는 다행히 좋아해서",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046187.1.11",
          "form": "갸우겨우 맞춰살고 있어요 ㅎㅎ",
          "original_form": "갸우겨우 맞춰살고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046187.1.12",
          "form": "ㅋㅋㅋㅋㅋ 쌈 싸드시는것도 좋아하세요??",
          "original_form": "ㅋㅋㅋㅋㅋ 쌈 싸드시는것도 좋아하세요??",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046187.1.13",
          "form": "어후",
          "original_form": "어후",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046187.1.14",
          "form": "그럼요 ㅎㅎㅎ",
          "original_form": "그럼요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046187.1.15",
          "form": "저번주 맛있는 쌈밥 집 찾아 헤매다가",
          "original_form": "저번주 맛있는 쌈밥 집 찾아 헤매다가",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046187.1.16",
          "form": "우아 ㅋㅋㅋ",
          "original_form": "우아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046187.1.17",
          "form": "맛있는 두루치기 집 발견해서",
          "original_form": "맛있는 두루치기 집 발견해서",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046187.1.18",
          "form": "정말 행복했어요",
          "original_form": "정말 행복했어요",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046187.1.19",
          "form": "네넵!! 쌈밥집 !!",
          "original_form": "네넵!! 쌈밥집 !!",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046187.1.20",
          "form": "금요일날 두루치기먹고",
          "original_form": "금요일날 두루치기먹고",
