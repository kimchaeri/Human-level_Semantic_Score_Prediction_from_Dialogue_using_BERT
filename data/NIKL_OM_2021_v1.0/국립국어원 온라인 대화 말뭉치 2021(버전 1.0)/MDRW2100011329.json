      "utterance": [
        {
          "id": "MDRW2100011329.1.1",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100011329.1.2",
          "form": "좋아하는데 요즘은 못가죠ㅠ",
          "original_form": "좋아하는데 요즘은 못가죠ㅠ",
          "speaker_id": "2",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100011329.1.3",
          "form": "많이 가볼껄 후회돼요",
          "original_form": "많이 가볼껄 후회돼요",
          "speaker_id": "2",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100011329.1.4",
          "form": "저도요ㅠ",
          "original_form": "저도요ㅠ",
          "speaker_id": "1",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100011329.1.5",
          "form": "해외가고싶어요",
          "original_form": "해외가고싶어요",
          "speaker_id": "1",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100011329.1.6",
          "form": "해외여행은 어떤스타일좋아하세요?",
          "original_form": "해외여행은 어떤스타일좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100011329.1.7",
          "form": "저는 휴양지 위주로ㅋㅋ",
          "original_form": "저는 휴양지 위주로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100011329.1.8",
          "form": "저도요ㅋㅋㅋ 전",
          "original_form": "저도요ㅋㅋㅋ 전",
          "speaker_id": "1",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011329.1.9",
          "form": "괌가고싶어요",
          "original_form": "괌가고싶어요",
          "speaker_id": "1",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011329.1.10",
          "form": "괌 좋죠ㅜ 실내에만 있다보니 이제는 탁트인 곳에 가고싶어요",
          "original_form": "괌 좋죠ㅜ 실내에만 있다보니 이제는 탁트인 곳에 가고싶어요",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011329.1.11",
          "form": "호캉스는 가셨나요",
          "original_form": "호캉스는 가셨나요",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011329.1.12",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100011329.1.13",
          "form": "싱가폴도 가고싶어요",
          "original_form": "싱가폴도 가고싶어요",
          "speaker_id": "1",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100011329.1.14",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100011329.1.15",
          "form": "지난번에 1단계일때 한번 갔어요ㅋㅋ",
          "original_form": "지난번에 1단계일때 한번 갔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100011329.1.16",
          "form": "부산으로요!",
          "original_form": "부산으로요!",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100011329.1.17",
          "form": "부산좋죠",
          "original_form": "부산좋죠",
          "speaker_id": "1",
          "time": "20210824 11:35"
        },
        {
          "id": "MDRW2100011329.1.18",
          "form": "뭐드시고오셨나요?",
          "original_form": "뭐드시고오셨나요?",
          "speaker_id": "1",
          "time": "20210824 11:35"
        },
        {
          "id": "MDRW2100011329.1.19",
          "form": "회먹고 왔죠 국밥도 먹었어요ㅎㅎ",
          "original_form": "회먹고 왔죠 국밥도 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:36"
        },
        {
          "id": "MDRW2100011329.1.20",
          "form": "부산가보셨나요?",
          "original_form": "부산가보셨나요?",
          "speaker_id": "2",
          "time": "20210824 11:41"
        },
        {
          "id": "MDRW2100011329.1.21",
          "form": "네네 가봤어요",
          "original_form": "네네 가봤어요",
          "speaker_id": "1",
          "time": "20210824 11:49"
        },
        {
          "id": "MDRW2100011329.1.22",
          "form": "회좋죠ㅋㅋ저킬러예요",
          "original_form": "회좋죠ㅋㅋ저킬러예요",
          "speaker_id": "1",
          "time": "20210824 11:49"
        },
        {
          "id": "MDRW2100011329.1.23",
          "form": "ㅋㅋㅋ 부산 바닷가서 먹는 회 존맛이에요",
          "original_form": "ㅋㅋㅋ 부산 바닷가서 먹는 회 존맛이에요",
          "speaker_id": "2",
          "time": "20210824 11:50"
        },
        {
          "id": "MDRW2100011329.1.24",
          "form": "그쵸ㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:59"
        }
      ]
