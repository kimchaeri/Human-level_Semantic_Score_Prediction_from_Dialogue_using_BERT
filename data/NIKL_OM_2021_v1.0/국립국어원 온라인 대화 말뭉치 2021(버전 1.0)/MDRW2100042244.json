      "utterance": [
        {
          "id": "MDRW2100042244.1.1",
          "form": "넷플릭스 보세여?",
          "original_form": "넷플릭스 보세여?",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042244.1.2",
          "form": "넷플릭스 최근에 봤어요!",
          "original_form": "넷플릭스 최근에 봤어요!",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042244.1.3",
          "form": "보세요?",
          "original_form": "보세요?",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042244.1.4",
          "form": "뭐 보셜어요",
          "original_form": "뭐 보셜어요",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042244.1.5",
          "form": "저는 디피 켰다가 무서워서 껐어요",
          "original_form": "저는 디피 켰다가 무서워서 껐어요",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042244.1.6",
          "form": "ㅎㅎㅎ저는",
          "original_form": "ㅎㅎㅎ저는",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042244.1.7",
          "form": "에밀리파리에가다 요거봤어요",
          "original_form": "에밀리파리에가다 요거봤어요",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042244.1.8",
          "form": "아아 저도 그거 올초에 한참 봤어요",
          "original_form": "아아 저도 그거 올초에 한참 봤어요",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042244.1.9",
          "form": "재밌죠",
          "original_form": "재밌죠",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042244.1.10",
          "form": "여주인공 너무 이쁘더라고요",
          "original_form": "여주인공 너무 이쁘더라고요",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042244.1.11",
          "form": "맞죠 에밀리ㅠㅠ 배경도 넘이쁘고",
          "original_form": "맞죠 에밀리ㅠㅠ 배경도 넘이쁘고",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042244.1.12",
          "form": "작년에 재택근무 할때 항상 틀어놨어요",
          "original_form": "작년에 재택근무 할때 항상 틀어놨어요",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042244.1.13",
          "form": "배우가이쁘니까옷도찰떡이더라고요",
          "original_form": "배우가이쁘니까옷도찰떡이더라고요",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042244.1.14",
          "form": "그쵸 베레모같은것도 어찌나 이쁘던지",
          "original_form": "그쵸 베레모같은것도 어찌나 이쁘던지",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042244.1.15",
          "form": "되게똑소리나는캐릭터라ㅎㅎ더호감이더라고요",
          "original_form": "되게똑소리나는캐릭터라ㅎㅎ더호감이더라고요",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042244.1.16",
          "form": "맞죠 그런 커리어우먼 좋아요",
          "original_form": "맞죠 그런 커리어우먼 좋아요",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042244.1.17",
          "form": "사랑스럽고 똑부러지고",
          "original_form": "사랑스럽고 똑부러지고",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042244.1.18",
          "form": "맞아요 공감해요ㅎㅎ",
          "original_form": "맞아요 공감해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:39"
        }
      ]
