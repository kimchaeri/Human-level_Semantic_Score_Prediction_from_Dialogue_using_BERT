          "time": "20210722 16:47"
        },
        {
          "id": "MDRW2100001049.1.4",
          "form": "아 진짜...",
          "original_form": "아 진짜...",
          "speaker_id": "1",
          "time": "20210722 16:48"
        },
        {
          "id": "MDRW2100001049.1.5",
          "form": "여행 예전에는 많이 갔는데...",
          "original_form": "여행 예전에는 많이 갔는데...",
          "speaker_id": "1",
          "time": "20210722 16:48"
        },
        {
          "id": "MDRW2100001049.1.6",
          "form": "여행 어디어디갔어?",
          "original_form": "여행 어디어디갔어?",
          "speaker_id": "2",
          "time": "20210722 16:48"
        },
        {
          "id": "MDRW2100001049.1.7",
          "form": "해외 4번정도갔어!!",
          "original_form": "해외 4번정도갔어!!",
          "speaker_id": "1",
          "time": "20210722 16:49"
        },
        {
          "id": "MDRW2100001049.1.8",
          "form": "우와~어느나라?",
          "original_form": "우와~어느나라?",
          "speaker_id": "2",
          "time": "20210722 16:49"
        },
        {
          "id": "MDRW2100001049.1.9",
          "form": "다 동남아",
