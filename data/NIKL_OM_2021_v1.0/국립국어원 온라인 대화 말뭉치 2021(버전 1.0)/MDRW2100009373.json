      "utterance": [
        {
          "id": "MDRW2100009373.1.1",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009373.1.2",
          "form": "어렸을 때 햄스터 키운 적 있어요",
          "original_form": "어렸을 때 햄스터 키운 적 있어요",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009373.1.3",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009373.1.4",
          "form": "너무 귀엽잖아요 햄스터",
          "original_form": "너무 귀엽잖아요 햄스터",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009373.1.5",
          "form": "저는 닭을 키웠었어요",
          "original_form": "저는 닭을 키웠었어요",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009373.1.6",
          "form": "수컷",
          "original_form": "수컷",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009373.1.7",
          "form": "어엇 닭이요?ㅋㅋㅋㅋ 어디서 데려오셨나요?",
          "original_form": "어엇 닭이요?ㅋㅋㅋㅋ 어디서 데려오셨나요?",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009373.1.8",
          "form": "그그 길에서 주워가지고",
          "original_form": "그그 길에서 주워가지고",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009373.1.9",
          "form": "늠름하게 키웠답니다",
          "original_form": "늠름하게 키웠답니다",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009373.1.10",
          "form": "흰색 닭이었는데",
          "original_form": "흰색 닭이었는데",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009373.1.11",
          "form": "암컷인줄 알고 데려왔다가",
          "original_form": "암컷인줄 알고 데려왔다가",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009373.1.12",
          "form": "수컷이어서",
          "original_form": "수컷이어서",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009373.1.13",
          "form": "계란은 못얹고 새벽 4시마다 모닝콜 동물은 하나 얻었네요",
          "original_form": "계란은 못얹고 새벽 4시마다 모닝콜 동물은 하나 얻었네요",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009373.1.14",
          "form": "얻고",
          "original_form": "얻고",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009373.1.15",
          "form": "길줍한 수탉ㅋㅋㅋㅋ 재밌는 일화네요",
          "original_form": "길줍한 수탉ㅋㅋㅋㅋ 재밌는 일화네요",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009373.1.16",
          "form": "진짜로 아침마다 수탉이 우나요?",
          "original_form": "진짜로 아침마다 수탉이 우나요?",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009373.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009373.1.18",
          "form": "근데 아침에만 우는게 아니라",
          "original_form": "근데 아침에만 우는게 아니라",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009373.1.19",
          "form": "시도때도 없이 울어요",
          "original_form": "시도때도 없이 울어요",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009373.1.20",
          "form": "귀여워서 증말",
          "original_form": "귀여워서 증말",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009373.1.21",
          "form": "그리고 엄청 쪼고 다녀요",
          "original_form": "그리고 엄청 쪼고 다녀요",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009373.1.22",
          "form": "저희 어머님은",
          "original_form": "저희 어머님은",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009373.1.23",
          "form": "잇몸을 쪼였답니다",
          "original_form": "잇몸을 쪼였답니다",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009373.1.24",
          "form": "어억ㅋㅋㅋ 어머님 잇몸 괜찮으신가요ㅠㅠㅋㅋㅋ",
          "original_form": "어억ㅋㅋㅋ 어머님 잇몸 괜찮으신가요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009373.1.25",
          "form": "스케일링이라고 생각하라고(?)했어요",
          "original_form": "스케일링이라고 생각하라고(?)했어요",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009373.1.26",
          "form": "햄스터는 한아이 키우셨나요?",
          "original_form": "햄스터는 한아이 키우셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009373.1.27",
          "form": "아니요 햄스터는 두마리 키웠어요!",
          "original_form": "아니요 햄스터는 두마리 키웠어요!",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009373.1.28",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009373.1.29",
          "form": "귀여운 두마리면 귀여운게 두배",
          "original_form": "귀여운 두마리면 귀여운게 두배",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009373.1.30",
          "form": "햄스터는 씨앗 위주로 먹나요?",
          "original_form": "햄스터는 씨앗 위주로 먹나요?",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009373.1.31",
          "form": "닭은 잡식이라 다먹던데",
          "original_form": "닭은 잡식이라 다먹던데",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009373.1.32",
          "form": "네 해씨를 제일 좋아하고 건식 사료가 따로 있어요 ㅋㅋㅋ",
          "original_form": "네 해씨를 제일 좋아하고 건식 사료가 따로 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:55"
        }
      ]
