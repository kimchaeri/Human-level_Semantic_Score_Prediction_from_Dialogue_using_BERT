      "utterance": [
        {
          "id": "MDRW2100030171.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030171.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030171.1.3",
          "form": "반려동물 키워보신 적 있나요",
          "original_form": "반려동물 키워보신 적 있나요",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030171.1.4",
          "form": "아뇨 없어용ㅎㅎ",
          "original_form": "아뇨 없어용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030171.1.5",
          "form": "키우고 싳은 동물은요??",
          "original_form": "키우고 싳은 동물은요??",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030171.1.6",
          "form": "저 개인적으로 고양이 너무너무 키우고 싶어요ㅠㅠ",
          "original_form": "저 개인적으로 고양이 너무너무 키우고 싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030171.1.7",
          "form": "길냥이 보면 그릏게 귀엽더라구요",
          "original_form": "길냥이 보면 그릏게 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030171.1.8",
          "form": "와 고양이 너무 좋죠",
          "original_form": "와 고양이 너무 좋죠",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030171.1.9",
          "form": "다홍이 아세요?",
          "original_form": "다홍이 아세요?",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030171.1.10",
          "form": "알아요!!! 다홍이 진짜 너무너무귀여워요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "알아요!!! 다홍이 진짜 너무너무귀여워요ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030171.1.11",
          "form": "애가 진짜 똑똑하고 그냥 너무귀엽더라구요..",
          "original_form": "애가 진짜 똑똑하고 그냥 너무귀엽더라구요..",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030171.1.12",
          "form": "글고 하하하 채널에 나오는 고양이들도 진짜로 좋아해요",
          "original_form": "글고 하하하 채널에 나오는 고양이들도 진짜로 좋아해요",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030171.1.13",
          "form": "크집사도 좋아하실것 같네요 ㅎㅎ",
          "original_form": "크집사도 좋아하실것 같네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030171.1.14",
          "form": "크림히어로즈 말하시는거죠?? 보진 않았는데 기억해둘게요!",
          "original_form": "크림히어로즈 말하시는거죠?? 보진 않았는데 기억해둘게요!",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030171.1.15",
          "form": "반려동물 키워보신 적 있으신가요? 없으시면 머 키우고 싶으신지도 궁금해요!",
          "original_form": "반려동물 키워보신 적 있으신가요? 없으시면 머 키우고 싶으신지도 궁금해요!",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030171.1.16",
          "form": "전 없어요 ㅋㅋ",
          "original_form": "전 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030171.1.17",
          "form": "키우고 싶은 동물이 없어요",
          "original_form": "키우고 싶은 동물이 없어요",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030171.1.18",
          "form": "책임질 자신이 없어서요 ㅠ",
          "original_form": "책임질 자신이 없어서요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030171.1.19",
          "form": "동물 관련 알러진 없으신가요",
          "original_form": "동물 관련 알러진 없으신가요",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030171.1.20",
          "form": "악 사실 저도 그런 마음이 있긴 해요 ㅋㅋ큐ㅠㅠ 그래서 키우고 싶다라는 생각만 하는..!!",
          "original_form": "악 사실 저도 그런 마음이 있긴 해요 ㅋㅋ큐ㅠㅠ 그래서 키우고 싶다라는 생각만 하는..!!",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030171.1.21",
          "form": "네네 없는 것 같아요!",
          "original_form": "네네 없는 것 같아요!",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030171.1.22",
          "form": "동물키우는 게임은 좀했어요",
          "original_form": "동물키우는 게임은 좀했어요",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030171.1.23",
          "form": "물고기 키우기 아세요?",
          "original_form": "물고기 키우기 아세요?",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030171.1.24",
          "form": "헉 저 방금 쳐보고 생각났어요!",
          "original_form": "헉 저 방금 쳐보고 생각났어요!",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030171.1.25",
          "form": "추억이죸ㅋㅋㅋ",
          "original_form": "추억이죸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030171.1.26",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030171.1.27",
          "form": "예전에 했었는데",
          "original_form": "예전에 했었는데",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030171.1.28",
          "form": "아시는거보니",
          "original_form": "아시는거보니",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030171.1.29",
          "form": "백구 만화도 보셨을 수 있겟네요",
          "original_form": "백구 만화도 보셨을 수 있겟네요",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030171.1.30",
          "form": "ㅋㅋㅋㅋ 반려동물 만화인데",
          "original_form": "ㅋㅋㅋㅋ 반려동물 만화인데",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030171.1.31",
          "form": "맞아요ㅋㅋㅋ아 다 생각나는게 웃기네욬ㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ아 다 생각나는게 웃기네욬ㅋㅋ",
