      "utterance": [
        {
          "id": "MDRW2100047395.1.1",
          "form": "좋아하시는 음식 있으신가요?",
          "original_form": "좋아하시는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100047395.1.2",
          "form": "저는 요즘 마라탕이요",
          "original_form": "저는 요즘 마라탕이요",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100047395.1.3",
          "form": "헉 저도 마라탕 좋아해요",
          "original_form": "헉 저도 마라탕 좋아해요",
          "speaker_id": "1",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100047395.1.4",
          "form": "마라탕에 필수로 넣는거 있으신가요?",
          "original_form": "마라탕에 필수로 넣는거 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100047395.1.5",
          "form": "숙죽, 고기, 배추, 중당은 꼭 넣어요",
          "original_form": "숙죽, 고기, 배추, 중당은 꼭 넣어요",
          "speaker_id": "2",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100047395.1.6",
          "form": "중당은 모르는데 뭔가요?",
          "original_form": "중당은 모르는데 뭔가요?",
          "speaker_id": "1",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100047395.1.7",
          "form": "중국당면이요ㅎㅎ 쫄깃한 식감이 좋더라구요!",
          "original_form": "중국당면이요ㅎㅎ 쫄깃한 식감이 좋더라구요!",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100047395.1.8",
          "form": "아 중국당면ㅋㅋㅋㅋ",
          "original_form": "아 중국당면ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100047395.1.9",
          "form": "전 고구마떡이랑 중국당면 분모자 푸주 고기 옥수수면 필수로 넣어요!",
          "original_form": "전 고구마떡이랑 중국당면 분모자 푸주 고기 옥수수면 필수로 넣어요!",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100047395.1.10",
          "form": "고구마떡도 맛있죠! 식감때문에 떡, 면 종류 많이 넣어요",
          "original_form": "고구마떡도 맛있죠! 식감때문에 떡, 면 종류 많이 넣어요",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100047395.1.11",
          "form": "저두요 마라탕 자주 드시는 편인가요?",
          "original_form": "저두요 마라탕 자주 드시는 편인가요?",
          "speaker_id": "1",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100047395.1.12",
          "form": "주에 한 번은 배달시키는 것 같아요",
          "original_form": "주에 한 번은 배달시키는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100047395.1.13",
          "form": "헉 부러워요...",
          "original_form": "헉 부러워요...",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100047395.1.14",
          "form": "저희 집은 배달을 안시켜서 시키는게 눈치 보여요ㅠㅋㅋㅋㅋ",
          "original_form": "저희 집은 배달을 안시켜서 시키는게 눈치 보여요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100047395.1.15",
          "form": "아ㅠㅠ 배달 전문점 음식은 드시기 어려우시겠어요ㅠㅠ",
          "original_form": "아ㅠㅠ 배달 전문점 음식은 드시기 어려우시겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100047395.1.16",
          "form": "그래서 먹으러 나갈때도 있어요ㅎㅎ",
          "original_form": "그래서 먹으러 나갈때도 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:11"
        }
      ]
