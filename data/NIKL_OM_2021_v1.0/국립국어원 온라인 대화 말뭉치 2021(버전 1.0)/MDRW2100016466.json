        },
        {
          "id": "MDRW2100016466.1.4",
          "form": "모바일 게임 밖에 안해요",
          "original_form": "모바일 게임 밖에 안해요",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100016466.1.5",
          "form": "저두요ㅋㅋ요즘은 게임 뭐하세요?",
          "original_form": "저두요ㅋㅋ요즘은 게임 뭐하세요?",
          "speaker_id": "1",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100016466.1.6",
          "form": "뀸의정원 아세여??",
          "original_form": "뀸의정원 아세여??",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016466.1.7",
          "form": "오 알죠알죠ㅋㅋㄱㄱㄲ",
          "original_form": "오 알죠알죠ㅋㅋㄱㄱㄲ",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016466.1.8",
          "form": "전 꿈의집해요ㅋㄱㄲ",
          "original_form": "전 꿈의집해요ㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016466.1.9",
          "form": "오 ㅋㅋㅋㅋ 전 꿈의 정원 하거든여 ㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋ 전 꿈의 정원 하거든여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016466.1.10",
          "form": "지금 5000레벨 좀 넘엇어여 ㅋㅋㅋㅋ",
          "original_form": "지금 5000레벨 좀 넘엇어여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016466.1.11",
          "form": "ㅋㅋㅋㅋㅋ헉 전 1000레벨 좀 넘었는데ㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ헉 전 1000레벨 좀 넘었는데ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016466.1.12",
          "form": "오래하셨나봐요!!",
          "original_form": "오래하셨나봐요!!",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016466.1.13",
          "form": "올해 시작햇어여",
          "original_form": "올해 시작햇어여",
          "speaker_id": "2",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016466.1.14",
          "form": "헉 그렇구낭ㅎㅎ 근데 이 꿈의 어쩌구시리즈가 못깨서 짜증나다가도 오랜만에 들어가면 무제한하트랑 아이템 줘서 못끊겠더라구요",
          "original_form": "헉 그렇구낭ㅎㅎ 근데 이 꿈의 어쩌구시리즈가 못깨서 짜증나다가도 오랜만에 들어가면 무제한하트랑 아이템 줘서 못끊겠더라구요",
          "speaker_id": "1",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016466.1.15",
          "form": "아 마자요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 마자요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100016466.1.16",
          "form": "오랜만에 가면 아이템 빵빵하게 주져 ㅋㅋㅋㅋㅋ",
          "original_form": "오랜만에 가면 아이템 빵빵하게 주져 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100016466.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그래서 그때 깨버리고 최대한 많이 깨려고 그것만 하게돼요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그래서 그때 깨버리고 최대한 많이 깨려고 그것만 하게돼요",
          "speaker_id": "1",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016466.1.18",
          "form": "마자요 ㅋㅋㅋㅋㅋ",
          "original_form": "마자요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016466.1.19",
          "form": "이거말고 다른 게임은",
          "original_form": "이거말고 다른 게임은",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016466.1.20",
          "form": "거의 안 하느거 같아여",
          "original_form": "거의 안 하느거 같아여",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016466.1.21",
          "form": "저두요 가끔씩 심심풀이로 가볍게 하니까 한개만 있어도 충분하더라구요",
          "original_form": "저두요 가끔씩 심심풀이로 가볍게 하니까 한개만 있어도 충분하더라구요",
