      "utterance": [
        {
          "id": "MDRW2100043009.1.1",
          "form": "다이어트하시나요?ㅋ",
          "original_form": "다이어트하시나요?ㅋ",
          "speaker_id": "1",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100043009.1.2",
          "form": "ㅋㅋㅋ 안하는데 나이드니 먹는거 줄어서 살빠졋어여",
          "original_form": "ㅋㅋㅋ 안하는데 나이드니 먹는거 줄어서 살빠졋어여",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100043009.1.3",
          "form": "ㅋㅋㅋ 하 정답이네\r\n근데 찐살은 안먹는데 왜안빠지죠?ㅋㅋ",
          "original_form": "ㅋㅋㅋ 하 정답이네\r\n근데 찐살은 안먹는데 왜안빠지죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100043009.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ... 나잇살...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ... 나잇살...",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100043009.1.5",
          "form": "운동하시나요",
          "original_form": "운동하시나요",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100043009.1.6",
          "form": "히히 나잇살....",
          "original_form": "히히 나잇살....",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100043009.1.7",
          "form": "저 이번주부터 1대1 실내테니스배워요",
          "original_form": "저 이번주부터 1대1 실내테니스배워요",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100043009.1.8",
          "form": "재미나요ㅋㅋ 실내에 선생이랑 둘이잇음ㅋㅋ",
          "original_form": "재미나요ㅋㅋ 실내에 선생이랑 둘이잇음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100043009.1.9",
          "form": "대박! 얼마에영",
          "original_form": "대박! 얼마에영",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100043009.1.10",
          "form": "8회 한달 20이요ㅋㅋ",
          "original_form": "8회 한달 20이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100043009.1.11",
          "form": "뽕뽑을려구요...! 아자아자ㅋㅋ",
          "original_form": "뽕뽑을려구요...! 아자아자ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100043009.1.12",
          "form": "운동 해보신거잇으세요?",
          "original_form": "운동 해보신거잇으세요?",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100043009.1.13",
          "form": "테니스요",
          "original_form": "테니스요",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043009.1.14",
          "form": "비싸서끊음 ㅠㅠ",
          "original_form": "비싸서끊음 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043009.1.15",
          "form": "1주 몇번 이요?",
          "original_form": "1주 몇번 이요?",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043009.1.16",
          "form": "아 근데 실외의 거의2배네요",
          "original_form": "아 근데 실외의 거의2배네요",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043009.1.17",
          "form": "20분이죠?",
          "original_form": "20분이죠?",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043009.1.18",
          "form": "ㅋㅋㅋㅋ 2번이요 화목인데\r\n매일가서 연습해도된다는데 두번도 벅찬듯",
          "original_form": "ㅋㅋㅋㅋ 2번이요 화목인데\r\n매일가서 연습해도된다는데 두번도 벅찬듯",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043009.1.19",
          "form": "네네! 20분이여",
          "original_form": "네네! 20분이여",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043009.1.20",
          "form": "코시국이라",
          "original_form": "코시국이라",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100043009.1.21",
          "form": "전체를 저만 다써서 비싼듯...",
          "original_form": "전체를 저만 다써서 비싼듯...",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100043009.1.22",
          "form": "아항... 코트 몇개요?",
          "original_form": "아항... 코트 몇개요?",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100043009.1.23",
          "form": "2개요ㅋㅋ",
          "original_form": "2개요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043009.1.24",
          "form": "ㅋㅋㅋ 저도 등록하고싶네요",
          "original_form": "ㅋㅋㅋ 저도 등록하고싶네요",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043009.1.25",
          "form": "가격이 부담 ㅠㅠㅠ",
          "original_form": "가격이 부담 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043009.1.26",
          "form": "그 야구장 겜장처럼",
          "original_form": "그 야구장 겜장처럼",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043009.1.27",
          "form": "자동공나오는기계도잇더라구요 신기하더라구욬",
          "original_form": "자동공나오는기계도잇더라구요 신기하더라구욬",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043009.1.28",
          "form": "나잇살 더 쳐지지않기위해 해봅니다ㅋㅋ",
          "original_form": "나잇살 더 쳐지지않기위해 해봅니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043009.1.29",
          "form": "그거 연습 할때 쓸수잇음 좋겟네용",
          "original_form": "그거 연습 할때 쓸수잇음 좋겟네용",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100043009.1.30",
          "form": "네네 20분하고 30분 혼자하고가래요",
          "original_form": "네네 20분하고 30분 혼자하고가래요",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100043009.1.31",
          "form": "초보한테ㅋㅋ",
          "original_form": "초보한테ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:01"
        }
      ]
