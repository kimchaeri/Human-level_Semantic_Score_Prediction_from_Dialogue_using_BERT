      "utterance": [
        {
          "id": "MDRW2100005676.1.1",
          "form": "요즘 시원한거 같아",
          "original_form": "요즘 시원한거 같아",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005676.1.2",
          "form": "난 별로",
          "original_form": "난 별로",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005676.1.3",
          "form": "아냐 얼마 전에 비가 온 뒤로 훨씬 시원해졌어",
          "original_form": "아냐 얼마 전에 비가 온 뒤로 훨씬 시원해졌어",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005676.1.4",
          "form": "비기 언제 왔었지?",
          "original_form": "비기 언제 왔었지?",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005676.1.5",
          "form": "오늘도 왔고 어제도 왔어",
          "original_form": "오늘도 왔고 어제도 왔어",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005676.1.6",
          "form": "낼도 올 것 같더라",
          "original_form": "낼도 올 것 같더라",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005676.1.7",
          "form": "비가 자주 오는 것 같다",
          "original_form": "비가 자주 오는 것 같다",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005676.1.8",
          "form": "우산 챙기고 다녀야겠다",
          "original_form": "우산 챙기고 다녀야겠다",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005676.1.9",
          "form": "그러게 언제 비가 올지 모르겠어 이젠",
          "original_form": "그러게 언제 비가 올지 모르겠어 이젠",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005676.1.10",
          "form": "일기예보 좀 검색해보고 다녀",
          "original_form": "일기예보 좀 검색해보고 다녀",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005676.1.11",
          "form": "ㅋㅋㅋㅋㅋ 알겠어",
          "original_form": "ㅋㅋㅋㅋㅋ 알겠어",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005676.1.12",
          "form": "검색해봤는데 내일 오전에 비오나봐",
          "original_form": "검색해봤는데 내일 오전에 비오나봐",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005676.1.13",
          "form": "아침에 비 오면 하루종일 어두워서 별로더라",
          "original_form": "아침에 비 오면 하루종일 어두워서 별로더라",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005676.1.14",
          "form": "맞아 나도 비오는 날은 졸리고 힘이 없어",
          "original_form": "맞아 나도 비오는 날은 졸리고 힘이 없어",
          "speaker_id": "2",
          "time": "20210821 23:50"
