      "utterance": [
        {
          "id": "MDRW2100010271.1.1",
          "form": "저녁 뭐드실건가요?!",
          "original_form": "저녁 뭐드실건가요?!",
          "speaker_id": "1",
          "time": "20210823 16:17"
        },
        {
          "id": "MDRW2100010271.1.2",
          "form": "고민중이에요 ㅠㅠ 요즘 다이어트중이라 .ㅡ",
          "original_form": "고민중이에요 ㅠㅠ 요즘 다이어트중이라 .ㅡ",
          "speaker_id": "2",
          "time": "20210823 16:19"
        },
        {
          "id": "MDRW2100010271.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210823 16:19"
        },
        {
          "id": "MDRW2100010271.1.4",
          "form": "오 다이어트중이신가요?",
          "original_form": "오 다이어트중이신가요?",
          "speaker_id": "1",
          "time": "20210823 16:21"
        },
        {
          "id": "MDRW2100010271.1.5",
          "form": "힘드시겠어요ㅠㅠ",
          "original_form": "힘드시겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 16:21"
        },
        {
          "id": "MDRW2100010271.1.6",
          "form": "살이 너무쪄서 ..",
          "original_form": "살이 너무쪄서 ..",
          "speaker_id": "2",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100010271.1.7",
          "form": "ㅠㅠ 코로나라",
          "original_form": "ㅠㅠ 코로나라",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010271.1.8",
          "form": "아무래도",
          "original_form": "아무래도",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010271.1.9",
          "form": "저녁 메뉴가 어떻게되세요?",
          "original_form": "저녁 메뉴가 어떻게되세요?",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010271.1.10",
          "form": "저는 부대찌개",
          "original_form": "저는 부대찌개",
          "speaker_id": "1",
          "time": "20210823 17:46"
        },
        {
          "id": "MDRW2100010271.1.11",
          "form": "일것같아요",
          "original_form": "일것같아요",
          "speaker_id": "1",
          "time": "20210823 17:46"
        },
        {
          "id": "MDRW2100010271.1.12",
          "form": "저는 계란밥 간단히먹구",
          "original_form": "저는 계란밥 간단히먹구",
          "speaker_id": "2",
          "time": "20210823 19:05"
        },
        {
          "id": "MDRW2100010271.1.13",
          "form": "땅끄햇어용 ㅋㅋ",
          "original_form": "땅끄햇어용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100010271.1.14",
          "form": "땅끄가",
          "original_form": "땅끄가",
          "speaker_id": "1",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100010271.1.15",
          "form": "무엇인가요?",
          "original_form": "무엇인가요?",
          "speaker_id": "1",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100010271.1.16",
          "form": "ㅋㅋㅋㅋ홈트용 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ홈트용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:52"
        },
        {
          "id": "MDRW2100010271.1.17",
          "form": "요즘 먹기위해 운동하거든용",
          "original_form": "요즘 먹기위해 운동하거든용",
          "speaker_id": "2",
          "time": "20210824 11:52"
        },
        {
          "id": "MDRW2100010271.1.18",
          "form": "ㅋㅋㅋ좋네요",
          "original_form": "ㅋㅋㅋ좋네요",
          "speaker_id": "1",
          "time": "20210824 12:14"
        },
        {
          "id": "MDRW2100010271.1.19",
          "form": "음식뭐좋아하세요?",
          "original_form": "음식뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 12:14"
        },
        {
          "id": "MDRW2100010271.1.20",
          "form": "한식파예요!",
          "original_form": "한식파예요!",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100010271.1.21",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
