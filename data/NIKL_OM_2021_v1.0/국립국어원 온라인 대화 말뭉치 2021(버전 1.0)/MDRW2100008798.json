      "utterance": [
        {
          "id": "MDRW2100008798.1.1",
          "form": "김모양님은 겜하는거 있음?",
          "original_form": "김모양님은 겜하는거 있음?",
          "speaker_id": "2",
          "time": "20210823 09:56"
        },
        {
          "id": "MDRW2100008798.1.2",
          "form": "아직 포켓몬고는 핫한가여",
          "original_form": "아직 포켓몬고는 핫한가여",
          "speaker_id": "1",
          "time": "20210823 09:56"
        },
        {
          "id": "MDRW2100008798.1.3",
          "form": "핫한진 모르겠지만 나는 계속 하고있어",
          "original_form": "핫한진 모르겠지만 나는 계속 하고있어",
          "speaker_id": "2",
          "time": "20210823 09:56"
        },
        {
          "id": "MDRW2100008798.1.4",
          "form": "어제도 길에서 하는사람들 봄 아직 핫한가봐",
          "original_form": "어제도 길에서 하는사람들 봄 아직 핫한가봐",
          "speaker_id": "2",
          "time": "20210823 09:57"
        },
        {
          "id": "MDRW2100008798.1.5",
          "form": "그래도 네개였는데 하나 줄임 ㅎㅎ",
          "original_form": "그래도 네개였는데 하나 줄임 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 09:57"
        },
        {
          "id": "MDRW2100008798.1.6",
          "form": "오 다른건 또 뭐해요?",
          "original_form": "오 다른건 또 뭐해요?",
          "speaker_id": "1",
          "time": "20210823 10:07"
        },
        {
          "id": "MDRW2100008798.1.7",
          "form": "넘 24시간 깨어서 하려고 하더라고..",
          "original_form": "넘 24시간 깨어서 하려고 하더라고..",
          "speaker_id": "2",
          "time": "20210823 10:09"
        },
        {
          "id": "MDRW2100008798.1.8",
          "form": "지쳐버림ㅋㅋㅋㅋㅋ",
          "original_form": "지쳐버림ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:09"
        },
        {
          "id": "MDRW2100008798.1.9",
          "form": "얼마전에 보니까 포켓몬고가 벌써 5주년이라 하더라고요 ㄷㄷ",
          "original_form": "얼마전에 보니까 포켓몬고가 벌써 5주년이라 하더라고요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210823 10:10"
        },
        {
          "id": "MDRW2100008798.1.10",
          "form": "마자마자 잘아네",
          "original_form": "마자마자 잘아네",
          "speaker_id": "2",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100008798.1.11",
          "form": "그래도 그건 계속 무료로 할수있나요? 소액이라도 결제없이?",
          "original_form": "그래도 그건 계속 무료로 할수있나요? 소액이라도 결제없이?",
          "speaker_id": "1",
          "time": "20210823 10:18"
        },
        {
          "id": "MDRW2100008798.1.12",
          "form": "무료징 아니믄안하징ㅋ",
          "original_form": "무료징 아니믄안하징ㅋ",
          "speaker_id": "2",
          "time": "20210823 10:19"
        },
        {
          "id": "MDRW2100008798.1.13",
          "form": "나 예전에 체인크로니클 했었는데",
          "original_form": "나 예전에 체인크로니클 했었는데",
          "speaker_id": "1",
          "time": "20210823 10:22"
        },
        {
          "id": "MDRW2100008798.1.14",
          "form": "그건 소액이지만 결제했었던 기억이 남",
          "original_form": "그건 소액이지만 결제했었던 기억이 남",
          "speaker_id": "1",
          "time": "20210823 10:22"
        },
        {
          "id": "MDRW2100008798.1.15",
          "form": "아 그거 카카오게임아님?",
          "original_form": "아 그거 카카오게임아님?",
          "speaker_id": "2",
          "time": "20210823 10:23"
        },
        {
          "id": "MDRW2100008798.1.16",
          "form": "광고봄",
          "original_form": "광고봄",
          "speaker_id": "2",
          "time": "20210823 10:23"
        },
        {
          "id": "MDRW2100008798.1.17",
          "form": "지금은 서브종료했을건데",
          "original_form": "지금은 서브종료했을건데",
