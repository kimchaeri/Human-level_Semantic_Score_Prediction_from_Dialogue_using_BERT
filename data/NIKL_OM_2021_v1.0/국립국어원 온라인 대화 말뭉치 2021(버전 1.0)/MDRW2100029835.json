      "utterance": [
        {
          "id": "MDRW2100029835.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210903 14:36"
        },
        {
          "id": "MDRW2100029835.1.2",
          "form": "네 집안일 매일 하죠",
          "original_form": "네 집안일 매일 하죠",
          "speaker_id": "2",
          "time": "20210903 14:36"
        },
        {
          "id": "MDRW2100029835.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210903 14:37"
        },
        {
          "id": "MDRW2100029835.1.4",
          "form": "주부이기때문에 ㅋㅋㅋ 집안일은 매일 해야죠 ㅋㅋㅋ안하면 더러워져요 ㅋㅋ",
          "original_form": "주부이기때문에 ㅋㅋㅋ 집안일은 매일 해야죠 ㅋㅋㅋ안하면 더러워져요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:37"
        },
        {
          "id": "MDRW2100029835.1.5",
          "form": "저는 거의 담당하고 ㅋㅋㅋㅋ 신랑이 쓰레기분리수거랑, 애들 목욕담당이요 ㅋㅋㅋ",
          "original_form": "저는 거의 담당하고 ㅋㅋㅋㅋ 신랑이 쓰레기분리수거랑, 애들 목욕담당이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:37"
        },
        {
          "id": "MDRW2100029835.1.6",
          "form": "화장실이대박이죠",
          "original_form": "화장실이대박이죠",
          "speaker_id": "1",
          "time": "20210903 14:38"
        },
        {
          "id": "MDRW2100029835.1.7",
          "form": "네 화장실 너무 싫어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 화장실 너무 싫어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:38"
        },
        {
          "id": "MDRW2100029835.1.8",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100029835.1.9",
          "form": "네ㅠㅜ하기도싫구요ㅋ",
          "original_form": "네ㅠㅜ하기도싫구요ㅋ",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029835.1.10",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100029835.1.11",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100029835.1.12",
          "form": "락스도 조심해야돼요",
          "original_form": "락스도 조심해야돼요",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100029835.1.13",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029835.1.14",
          "form": "머리가아파요",
          "original_form": "머리가아파요",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029835.1.15",
