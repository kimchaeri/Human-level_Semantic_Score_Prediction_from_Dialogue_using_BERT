      "utterance": [
        {
          "id": "MDRW2100045202.1.1",
          "form": "하트시그널 또 안하나",
          "original_form": "하트시그널 또 안하나",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045202.1.2",
          "form": "코로나라 안할거다",
          "original_form": "코로나라 안할거다",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045202.1.3",
          "form": "다른 연애프로 많이 하는데 ㅠㅜ",
          "original_form": "다른 연애프로 많이 하는데 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045202.1.4",
          "form": "어떤거",
          "original_form": "어떤거",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045202.1.5",
          "form": "환승연애 돌싱글즈",
          "original_form": "환승연애 돌싱글즈",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045202.1.6",
          "form": "돌싱글즈는 뭐냐",
          "original_form": "돌싱글즈는 뭐냐",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045202.1.7",
          "form": "이혼한 남녀가 연애하는 프로 !",
          "original_form": "이혼한 남녀가 연애하는 프로 !",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045202.1.8",
          "form": "그거 티비에서 하드라",
          "original_form": "그거 티비에서 하드라",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045202.1.9",
          "form": "나는 넷플로 정주행했지",
          "original_form": "나는 넷플로 정주행했지",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045202.1.10",
          "form": "재밋나",
