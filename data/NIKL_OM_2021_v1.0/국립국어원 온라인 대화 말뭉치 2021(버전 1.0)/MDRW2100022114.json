      "utterance": [
        {
          "id": "MDRW2100022114.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210829 07:11"
        },
        {
          "id": "MDRW2100022114.1.2",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 07:17"
        },
        {
          "id": "MDRW2100022114.1.3",
          "form": "내일 개강이네요 ..",
          "original_form": "내일 개강이네요 ..",
          "speaker_id": "2",
          "time": "20210829 07:17"
        },
        {
          "id": "MDRW2100022114.1.4",
          "form": "아ㅎㅎ수강신청은",
          "original_form": "아ㅎㅎ수강신청은",
          "speaker_id": "1",
          "time": "20210829 07:19"
        },
        {
          "id": "MDRW2100022114.1.5",
          "form": "잘하셨어요?",
          "original_form": "잘하셨어요?",
          "speaker_id": "1",
          "time": "20210829 07:19"
        },
        {
          "id": "MDRW2100022114.1.6",
          "form": "노잼강의만 들어서 ㅎ.. 쉽게 따냈내요",
          "original_form": "노잼강의만 들어서 ㅎ.. 쉽게 따냈내요",
          "speaker_id": "2",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100022114.1.7",
          "form": "학생이새요?",
          "original_form": "학생이새요?",
          "speaker_id": "2",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100022114.1.8",
          "form": "아뇨 저는 졸업했고",
          "original_form": "아뇨 저는 졸업했고",
          "speaker_id": "1",
          "time": "20210829 15:00"
        },
        {
          "id": "MDRW2100022114.1.9",
          "form": "동생이 신입생이라 수강신청도와줬는데",
          "original_form": "동생이 신입생이라 수강신청도와줬는데",
          "speaker_id": "1",
          "time": "20210829 15:00"
        },
        {
          "id": "MDRW2100022114.1.10",
          "form": "실패했어요ㅜ",
          "original_form": "실패했어요ㅜ",
          "speaker_id": "1",
          "time": "20210829 15:00"
        },
        {
          "id": "MDRW2100022114.1.11",
          "form": "꿀강의 들으려면 빡세죠 ㅠㅠ",
          "original_form": "꿀강의 들으려면 빡세죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022114.1.12",
          "form": "동아리같은건 하셨었나요?",
          "original_form": "동아리같은건 하셨었나요?",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022114.1.13",
          "form": "아뇨 저희는 과동아리는 없었고",
          "original_form": "아뇨 저희는 과동아리는 없었고",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022114.1.14",
          "form": "하고싶은 동아리도 없었을뿐더러 학교다닐땐 알바하느라 바빠서",
          "original_form": "하고싶은 동아리도 없었을뿐더러 학교다닐땐 알바하느라 바빠서",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022114.1.15",
          "form": "동아리는 꿈도 못꿨어요 ㅎ",
          "original_form": "동아리는 꿈도 못꿨어요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022114.1.16",
          "form": "아이고 바쁘게 다니셨네요 ㅠㅠ",
          "original_form": "아이고 바쁘게 다니셨네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022114.1.17",
          "form": "저는 졸업전인데 이제와서 동아리 안한개 후회돼서 ㅜㅜ",
          "original_form": "저는 졸업전인데 이제와서 동아리 안한개 후회돼서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022114.1.18",
          "form": "졸업예정은 동아리 못들어가나요?ㅜㅜ",
          "original_form": "졸업예정은 동아리 못들어가나요?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022114.1.19",
          "form": "어차피 활동 많이 못해서 좀 그런가...",
          "original_form": "어차피 활동 많이 못해서 좀 그런가...",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022114.1.20",
          "form": "네 그것 때문에 ㅠㅠ",
          "original_form": "네 그것 때문에 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022114.1.21",
          "form": "반년.. 만하기에는 좀 그렇더라고요",
          "original_form": "반년.. 만하기에는 좀 그렇더라고요",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022114.1.22",
          "form": "그러면 지역소모임 가입해보세요!",
          "original_form": "그러면 지역소모임 가입해보세요!",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100022114.1.23",
          "form": "지역소모임!!",
          "original_form": "지역소모임!!",
          "speaker_id": "2",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100022114.1.24",
          "form": "얘기만 들어봤는대 해보샸어요?",
          "original_form": "얘기만 들어봤는대 해보샸어요?",
          "speaker_id": "2",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100022114.1.25",
          "form": "네~ 학교다닐때 사람들이랑 같이 만들어서 해요",
          "original_form": "네~ 학교다닐때 사람들이랑 같이 만들어서 해요",
          "speaker_id": "1",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100022114.1.26",
          "form": "오늘 수업 어떠셨어요!",
          "original_form": "오늘 수업 어떠셨어요!",
          "speaker_id": "1",
          "time": "20210830 13:52"
        }
      ]
    }
  ]
