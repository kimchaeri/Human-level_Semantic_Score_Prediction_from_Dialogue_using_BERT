      "utterance": [
        {
          "id": "MDRW2100015510.1.1",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015510.1.2",
          "form": "요즘에 관심있는거 있으시나요?",
          "original_form": "요즘에 관심있는거 있으시나요?",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015510.1.3",
          "form": "신발이요!",
          "original_form": "신발이요!",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015510.1.4",
          "form": "오 신발 좋아하시는구낭",
          "original_form": "오 신발 좋아하시는구낭",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015510.1.5",
          "form": "어떤 스타일 좋아하세요?",
          "original_form": "어떤 스타일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015510.1.6",
          "form": "운동화요ㅎㅎ",
          "original_form": "운동화요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015510.1.7",
          "form": "아하 그러시군요ㅎㅎ",
          "original_form": "아하 그러시군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015510.1.8",
          "form": "운동을 좋아하시나보네요?",
          "original_form": "운동을 좋아하시나보네요?",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015510.1.9",
          "form": "러닝화도 사려구요",
          "original_form": "러닝화도 사려구요",
          "speaker_id": "2",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100015510.1.10",
          "form": "그러시군요ㅎㅎ",
          "original_form": "그러시군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100015510.1.11",
          "form": "러닝화있으면 확실히 걸어다닐때나 뛰어다닐때ㅠ) 좋더라구요!",
          "original_form": "러닝화있으면 확실히 걸어다닐때나 뛰어다닐때ㅠ) 좋더라구요!",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100015510.1.12",
          "form": "최근 뭐 구매하셨나요",
          "original_form": "최근 뭐 구매하셨나요",
          "speaker_id": "2",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100015510.1.13",
          "form": "제가 최근에 구매한건 운동할때 입을 티 구매했었어요!",
          "original_form": "제가 최근에 구매한건 운동할때 입을 티 구매했었어요!",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100015510.1.14",
          "form": "신발은 나이키에서 저렴한거 샀던거같어요!ㅎㅎ",
          "original_form": "신발은 나이키에서 저렴한거 샀던거같어요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100015510.1.15",
          "form": "오 저도나이키 좋아해요",
          "original_form": "오 저도나이키 좋아해요",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100015510.1.16",
          "form": "나이키 제품들이 운동할때 되게 좋은거같아요!",
          "original_form": "나이키 제품들이 운동할때 되게 좋은거같아요!",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100015510.1.17",
          "form": "마자요 디자인도이쁘고",
          "original_form": "마자요 디자인도이쁘고",
          "speaker_id": "2",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100015510.1.18",
          "form": "그러게요! 운동용품살때 나이키를 제일 먼저보는거같아요",
          "original_form": "그러게요! 운동용품살때 나이키를 제일 먼저보는거같아요",
          "speaker_id": "1",
          "time": "20210826 11:52"
        }
      ]
    }
