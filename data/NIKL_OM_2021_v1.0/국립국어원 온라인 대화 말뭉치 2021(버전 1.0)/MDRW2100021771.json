      "utterance": [
        {
          "id": "MDRW2100021771.1.1",
          "form": "여행조아하시나요",
          "original_form": "여행조아하시나요",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021771.1.2",
          "form": "네 좋아해요 최근여행지어디세욤",
          "original_form": "네 좋아해요 최근여행지어디세욤",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021771.1.3",
          "form": "외국이요??",
          "original_form": "외국이요??",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021771.1.4",
          "form": "어디든요 ㅋㅋ",
          "original_form": "어디든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021771.1.5",
          "form": "코로나전 일본이요ㅎㅎ",
          "original_form": "코로나전 일본이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021771.1.6",
          "form": "우와 딱 전에다녀오셨네요",
          "original_form": "우와 딱 전에다녀오셨네요",
          "speaker_id": "1",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021771.1.7",
          "form": "후쿠오카 너무 좋았어요저는",
          "original_form": "후쿠오카 너무 좋았어요저는",
          "speaker_id": "1",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021771.1.8",
          "form": "오저도후쿠오카!",
          "original_form": "오저도후쿠오카!",
          "speaker_id": "2",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100021771.1.9",
          "form": "부모님 꼭 모시고 가규팠는데",
          "original_form": "부모님 꼭 모시고 가규팠는데",
          "speaker_id": "1",
          "time": "20210829 11:03"
        },
        {
          "id": "MDRW2100021771.1.10",
          "form": "함바그 맛있었네요ㅎㅎ",
          "original_form": "함바그 맛있었네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100021771.1.11",
          "form": "저는 료칸 좋았어요 모모치해변이랑",
          "original_form": "저는 료칸 좋았어요 모모치해변이랑",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100021771.1.12",
          "form": "와 료칸도 가보셨군요",
          "original_form": "와 료칸도 가보셨군요",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100021771.1.13",
          "form": "모모치는 가봤네요저도ㅎㅎ",
          "original_form": "모모치는 가봤네요저도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100021771.1.14",
          "form": "아 얼른 코로나 끝나서 가고파요ㅠㅠ",
          "original_form": "아 얼른 코로나 끝나서 가고파요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 11:30"
