      "utterance": [
        {
          "id": "MDRW2100028359.1.1",
          "form": "요즘 책 사고 싶은데 돈이 없어서 문제인",
          "original_form": "요즘 책 사고 싶은데 돈이 없어서 문제인",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100028359.1.2",
          "form": "저는 책을 사고 싶은 건 아니지맘 돈이 없어서 문제인",
          "original_form": "저는 책을 사고 싶은 건 아니지맘 돈이 없어서 문제인",
          "speaker_id": "2",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100028359.1.3",
          "form": "아오",
          "original_form": "아오",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100028359.1.4",
          "form": "전자책 값 너무 많이 올랐어",
          "original_form": "전자책 값 너무 많이 올랐어",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028359.1.5",
          "form": "전자책 잘 안 사서",
          "original_form": "전자책 잘 안 사서",
          "speaker_id": "2",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028359.1.6",
          "form": "얼마나 올랐는데?",
          "original_form": "얼마나 올랐는데?",
          "speaker_id": "2",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028359.1.7",
          "form": "이북이 종북이랑 별 차이가 없음",
          "original_form": "이북이 종북이랑 별 차이가 없음",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028359.1.8",
          "form": "방금 리디 들어가서 보고 왔는데 그",
          "original_form": "방금 리디 들어가서 보고 왔는데 그",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028359.1.9",
          "form": "그럼 이북 왜 산담",
          "original_form": "그럼 이북 왜 산담",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028359.1.10",
          "form": "종북이 13800원이고",
          "original_form": "종북이 13800원이고",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028359.1.11",
          "form": "이북이 9660원이네",
          "original_form": "이북이 9660원이네",
          "speaker_id": "1",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028359.1.12",
          "form": "30퍼 싸대",
          "original_form": "30퍼 싸대",
          "speaker_id": "1",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028359.1.13",
          "form": "너무 비싼데",
          "original_form": "너무 비싼데",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028359.1.14",
          "form": "근데 종북도 너무 비싸",
          "original_form": "근데 종북도 너무 비싸",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028359.1.15",
          "form": "도서정가제가 문제임 망얼",
          "original_form": "도서정가제가 문제임 망얼",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028359.1.16",
          "form": "나 절대 종북 파였는데 나이 드니까 이북이 더 땡겨 리더기도 있고",
          "original_form": "나 절대 종북 파였는데 나이 드니까 이북이 더 땡겨 리더기도 있고",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028359.1.17",
          "form": "근데 너무 비싸........",
          "original_form": "근데 너무 비싸........",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028359.1.18",
          "form": "나도 리더기 있으면 이북 쓸 것 같은데",
          "original_form": "나도 리더기 있으면 이북 쓸 것 같은데",
          "speaker_id": "2",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028359.1.19",
          "form": "ㄹㅇ 너무 비싸서...",
          "original_form": "ㄹㅇ 너무 비싸서...",
          "speaker_id": "2",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100028359.1.20",
          "form": "리더기 사봤는데 후기 알려주자면... 좋긴 한데 걍 아이패드 써라",
          "original_form": "리더기 사봤는데 후기 알려주자면... 좋긴 한데 걍 아이패드 써라",
          "speaker_id": "1",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100028359.1.21",
          "form": "그치만 아이패드로 글씨 오래 보는 건 눈아파",
          "original_form": "그치만 아이패드로 글씨 오래 보는 건 눈아파",
          "speaker_id": "2",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100028359.1.22",
          "form": "난 폰으로도 소설 몇 권씩 문제없이 읽어서",
          "original_form": "난 폰으로도 소설 몇 권씩 문제없이 읽어서",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028359.1.23",
          "form": "폰으로는 오히려 읽겠는데",
          "original_form": "폰으로는 오히려 읽겠는데",
          "speaker_id": "2",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028359.1.24",
          "form": "패드가 더 불편한 느낌",
          "original_form": "패드가 더 불편한 느낌",
          "speaker_id": "2",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028359.1.25",
          "form": "폰으로도 호흡 긴 건 잘 못 읽겟음",
          "original_form": "폰으로도 호흡 긴 건 잘 못 읽겟음",
          "speaker_id": "2",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028359.1.26",
          "form": "역시 종북 짱",
          "original_form": "역시 종북 짱",
          "speaker_id": "2",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028359.1.27",
          "form": "나는 전자책에 슬슬 길들여졌나벼",
          "original_form": "나는 전자책에 슬슬 길들여졌나벼",
          "speaker_id": "1",
          "time": "20210902 19:50"
        }
      ]
    }
