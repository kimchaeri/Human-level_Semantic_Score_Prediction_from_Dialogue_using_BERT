      "utterance": [
        {
          "id": "MDRW2100017582.1.1",
          "form": "오늘 슬의 5분 일찍한대요!",
          "original_form": "오늘 슬의 5분 일찍한대요!",
          "speaker_id": "1",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017582.1.2",
          "form": "앗 정말요? 감사해요!!! ㅇㅁㅇ",
          "original_form": "앗 정말요? 감사해요!!! ㅇㅁㅇ{emoji:♡}",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017582.1.3",
          "form": "주말에 즐겨보는 프로그램 있으세용?",
          "original_form": "주말에 즐겨보는 프로그램 있으세용?",
          "speaker_id": "2",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100017582.1.4",
          "form": "주말 완전 바쁘죠~~",
          "original_form": "주말 완전 바쁘죠~~",
          "speaker_id": "1",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100017582.1.5",
          "form": "놀면뭐하니도 봐야하고",
          "original_form": "놀면뭐하니도 봐야하고",
          "speaker_id": "1",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100017582.1.6",
          "form": "그것이 알고싶다랑",
          "original_form": "그것이 알고싶다랑",
          "speaker_id": "1",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100017582.1.7",
          "form": "드라마요!",
          "original_form": "드라마요!",
          "speaker_id": "1",
          "time": "20210829 06:45"
        },
        {
          "id": "MDRW2100017582.1.8",
          "form": "뭐 자주 보세요?",
          "original_form": "뭐 자주 보세요?",
          "speaker_id": "1",
          "time": "20210829 06:45"
        },
        {
          "id": "MDRW2100017582.1.9",
          "form": "저도놀면뭐하니 놀라운토요일 아는형님  이순으로 쫙돌아요ㅋㄱㅋ",
          "original_form": "저도놀면뭐하니 놀라운토요일 아는형님  이순으로 쫙돌아요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100017582.1.10",
          "form": "아ㅋㅋㅋㅋ제가 예전에 그렇게 봤어요ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ제가 예전에 그렇게 봤어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100017582.1.11",
          "form": "아형은 원래 쭉보다가 살짝 노잼시기와서",
          "original_form": "아형은 원래 쭉보다가 살짝 노잼시기와서",
          "speaker_id": "1",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100017582.1.12",
          "form": "흥미로운 게스트 나올때만 보는ㅎㅎ",
          "original_form": "흥미로운 게스트 나올때만 보는ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100017582.1.13",
          "form": "맞아요 지금 좀 노잼시기와서 저도 아형은 거르기도해요;-;",
          "original_form": "맞아요 지금 좀 노잼시기와서 저도 아형은 거르기도해요;-;",
          "speaker_id": "2",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100017582.1.14",
          "form": "요즘은 런닝맨이 진짜 재밌어요",
          "original_form": "요즘은 런닝맨이 진짜 재밌어요",
          "speaker_id": "1",
          "time": "20210829 17:07"
        },
        {
          "id": "MDRW2100017582.1.15",
          "form": "아주가끔 노잼 게스트나오긴 하는데",
          "original_form": "아주가끔 노잼 게스트나오긴 하는데",
          "speaker_id": "1",
          "time": "20210829 17:07"
        },
        {
          "id": "MDRW2100017582.1.16",
          "form": "맞아요 런닝맨재밌어요 ㅜㅜㅋㅋㅋ 멤버들 합이좋아가지구",
          "original_form": "맞아요 런닝맨재밌어요 ㅜㅜㅋㅋㅋ 멤버들 합이좋아가지구",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100017582.1.17",
          "form": "그쵸?ㅋㅋㅋ",
          "original_form": "그쵸?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100017582.1.18",
          "form": "전소민이랑 양세찬 들어오고 더더 재밌어진거같아요",
          "original_form": "전소민이랑 양세찬 들어오고 더더 재밌어진거같아요",
          "speaker_id": "1",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100017582.1.19",
          "form": "김종국이랑 유재석 합도 너무 좋고ㅋㅋㅋㅋ",
          "original_form": "김종국이랑 유재석 합도 너무 좋고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100017582.1.20",
          "form": "윤은혜로계속놀리는것도너무웃기고ㅋㄱㅋㅋㄱ\n깡깡이라구하는것도 너무웃깁니다ㅋㅋㅋ",
          "original_form": "윤은혜로계속놀리는것도너무웃기고ㅋㄱㅋㅋㄱ\n깡깡이라구하는것도 너무웃깁니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100017582.1.21",
          "form": "ㅋㅋㅋㅋㅋ아 맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아 맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100017582.1.22",
          "form": "근데 그게 막진짜 무식하다고 놀리는느낌보다는",
          "original_form": "근데 그게 막진짜 무식하다고 놀리는느낌보다는",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100017582.1.23",
          "form": "예능적으로 잘 웃기게 편집해서 보기 좋은거같아요",
          "original_form": "예능적으로 잘 웃기게 편집해서 보기 좋은거같아요",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100017582.1.24",
          "form": "네네ㅋㅋㅋㅋㅋ편집도 잘한거같구ㅋㅋㄱㅋ진짜너무웃겨요ㄱㅋㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋㅋㅋ편집도 잘한거같구ㅋㅋㄱㅋ진짜너무웃겨요ㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100017582.1.25",
          "form": "유튜브도 보세요?",
          "original_form": "유튜브도 보세요?",
          "speaker_id": "1",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100017582.1.26",
