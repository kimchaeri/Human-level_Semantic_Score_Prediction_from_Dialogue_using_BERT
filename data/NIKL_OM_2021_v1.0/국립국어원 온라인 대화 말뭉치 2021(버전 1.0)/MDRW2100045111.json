        {
          "id": "MDRW2100045111.1.1",
          "form": "기억에 남는 여행 있으세요?",
          "original_form": "기억에 남는 여행 있으세요?",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045111.1.2",
          "form": "신혼여행이 젤 좋았던것 같아여ㅠ",
          "original_form": "신혼여행이 젤 좋았던것 같아여ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045111.1.3",
          "form": "자유로이..ㅋㅋㅋ",
          "original_form": "자유로이..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045111.1.4",
          "form": "어디로 가셨어요?",
          "original_form": "어디로 가셨어요?",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045111.1.5",
          "form": "푸켓이여@",
          "original_form": "푸켓이여@",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045111.1.6",
          "form": "거기도 바다가 예쁘죠?",
          "original_form": "거기도 바다가 예쁘죠?",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045111.1.7",
          "form": "네 ~ ㅎㅎㅎ 깨끗한던것 같아요",
          "original_form": "네 ~ ㅎㅎㅎ 깨끗한던것 같아요",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045111.1.8",
          "form": "전 발리 갔었어요",
          "original_form": "전 발리 갔었어요",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045111.1.9",
          "form": "와 ! 해외여행은 그게 첨이자 마지막 이였어여 저 ㅠㅠ",
          "original_form": "와 ! 해외여행은 그게 첨이자 마지막 이였어여 저 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045111.1.10",
          "form": "애낳고 키우느라ㅠㅠ 뭐 애 위주로만 다니니ㅠ",
          "original_form": "애낳고 키우느라ㅠㅠ 뭐 애 위주로만 다니니ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045111.1.11",
          "form": "맞아요 이젠 어렵죠 ㅠ",
          "original_form": "맞아요 이젠 어렵죠 ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045111.1.12",
          "form": "맨날 롯데월드 에버랜드.ㅠㅠ",
          "original_form": "맨날 롯데월드 에버랜드.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045111.1.13",
          "form": "서울 사시나봐요",
          "original_form": "서울 사시나봐요",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045111.1.14",
          "form": "아니여ㅠㅠ 지방인데 멀리나갈때 ㅠ 두어번",
          "original_form": "아니여ㅠㅠ 지방인데 멀리나갈때 ㅠ 두어번",
