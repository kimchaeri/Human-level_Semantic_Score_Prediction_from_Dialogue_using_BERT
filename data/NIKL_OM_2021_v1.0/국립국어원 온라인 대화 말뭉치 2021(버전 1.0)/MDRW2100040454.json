      "utterance": [
        {
          "id": "MDRW2100040454.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100040454.1.2",
          "form": "어제오늘 날씨가 좋네요",
          "original_form": "어제오늘 날씨가 좋네요",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100040454.1.3",
          "form": "안녕하세요 가을이 물씬 다가온 느낌이에요 ;)",
          "original_form": "안녕하세요 가을이 물씬 다가온 느낌이에요 ;)",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100040454.1.4",
          "form": "오늘 날이 참 좋았는데 어떠셨나요?",
          "original_form": "오늘 날이 참 좋았는데 어떠셨나요?",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100040454.1.5",
          "form": "네 저희 동네도 하늘이 파랗고",
          "original_form": "네 저희 동네도 하늘이 파랗고",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100040454.1.6",
          "form": "선선하니 좋았어요ㅎㅎ",
          "original_form": "선선하니 좋았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100040454.1.7",
          "form": "어떤 계절을 좋아하시나요?",
          "original_form": "어떤 계절을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040454.1.8",
          "form": "봄 좋아했는데",
          "original_form": "봄 좋아했는데",
          "speaker_id": "1",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100040454.1.9",
          "form": "요즘은 가을도 좋아요",
          "original_form": "요즘은 가을도 좋아요",
          "speaker_id": "1",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100040454.1.10",
          "form": "두 계절 모두 좋아하는데 짧아지는 것 같아서 아쉬워요 ㅠ",
          "original_form": "두 계절 모두 좋아하는데 짧아지는 것 같아서 아쉬워요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 20:50"
        },
        {
          "id": "MDRW2100040454.1.11",
          "form": "네 봄 가을이 요즘 너무 짧죠ㅜ",
          "original_form": "네 봄 가을이 요즘 너무 짧죠ㅜ",
          "speaker_id": "1",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100040454.1.12",
          "form": "그래서 이 좋은 날씨를 흠뻑 느끼고 싶네요",
          "original_form": "그래서 이 좋은 날씨를 흠뻑 느끼고 싶네요",
          "speaker_id": "1",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100040454.1.13",
          "form": "가을과 봄이 길다면 참 좋을텐데 말이에요",
          "original_form": "가을과 봄이 길다면 참 좋을텐데 말이에요",
          "speaker_id": "2",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100040454.1.14",
          "form": "네 저도 바라는 바예요",
          "original_form": "네 저도 바라는 바예요",
          "speaker_id": "1",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100040454.1.15",
          "form": "가을엔 언제 하늘을 봐도 예쁜 것 같아여",
          "original_form": "가을엔 언제 하늘을 봐도 예쁜 것 같아여",
          "speaker_id": "2",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100040454.1.16",
          "form": "네 오늘도 하늘이 파~랗고",
          "original_form": "네 오늘도 하늘이 파~랗고",
          "speaker_id": "1",
          "time": "20210904 21:04"
        },
        {
          "id": "MDRW2100040454.1.17",
          "form": "흰구름이 뭉게뭉게 예쁘더라고요",
          "original_form": "흰구름이 뭉게뭉게 예쁘더라고요",
          "speaker_id": "1",
          "time": "20210904 21:04"
        },
        {
          "id": "MDRW2100040454.1.18",
          "form": "왠지 이런날 소풍이 가고 싶은데 못 나가서 이렇게 서러울 수가 없어요",
          "original_form": "왠지 이런날 소풍이 가고 싶은데 못 나가서 이렇게 서러울 수가 없어요",
          "speaker_id": "2",
          "time": "20210904 21:07"
        },
        {
          "id": "MDRW2100040454.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100040454.1.20",
          "form": "전 어제 백신접종까지 해서",
          "original_form": "전 어제 백신접종까지 해서",
          "speaker_id": "1",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100040454.1.21",
          "form": "꼼짝없이 집콕중이예요ㅜ",
          "original_form": "꼼짝없이 집콕중이예요ㅜ",
          "speaker_id": "1",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100040454.1.22",
          "form": "날씨가 좋아서 더 아쉽네요",
          "original_form": "날씨가 좋아서 더 아쉽네요",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100040454.1.23",
          "form": "에궁 팔은 괜찮으신가요?",
          "original_form": "에궁 팔은 괜찮으신가요?",
          "speaker_id": "2",
          "time": "20210904 21:15"
        },
        {
          "id": "MDRW2100040454.1.24",
          "form": "저희 동네는 비가 많이 와요",
          "original_form": "저희 동네는 비가 많이 와요",
          "speaker_id": "1",
          "time": "20210906 20:24"
        }
      ]
