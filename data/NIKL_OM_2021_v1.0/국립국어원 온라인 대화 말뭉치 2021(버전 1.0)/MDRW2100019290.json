      "utterance": [
        {
          "id": "MDRW2100019290.1.1",
          "form": "안녕하셔요 ^^!!!",
          "original_form": "안녕하셔요 ^^!!!",
          "speaker_id": "1",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100019290.1.2",
          "form": "혹시 반려동물이랑 함께 사시나요",
          "original_form": "혹시 반려동물이랑 함께 사시나요",
          "speaker_id": "1",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100019290.1.3",
          "form": "안녕하세요 !",
          "original_form": "안녕하세요 !",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019290.1.4",
          "form": "아뇨ㅠ 좋아하긴 하는데 키우진 않습니다",
          "original_form": "아뇨ㅠ 좋아하긴 하는데 키우진 않습니다",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019290.1.5",
          "form": "name1님은 반려동물이랑 사시나요?",
          "original_form": "&name1&님은 반려동물이랑 사시나요?",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019290.1.6",
          "form": "아이구 ㅠ 저도 같이 살진 않아요ㅜㅜ",
          "original_form": "아이구 ㅠ 저도 같이 살진 않아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019290.1.7",
          "form": "저는 임보만 여러번 했어요!",
          "original_form": "저는 임보만 여러번 했어요!",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019290.1.8",
          "form": "새 가족 찾아주는 일만...ㅎㅎ....",
          "original_form": "새 가족 찾아주는 일만...ㅎㅎ....",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019290.1.9",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019290.1.10",
          "form": "고양이요 강아지요?",
          "original_form": "고양이요 강아지요?",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019290.1.11",
          "form": "고양이요!",
          "original_form": "고양이요!",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019290.1.12",
          "form": "강아지들은 텐션감당잌ㅋㅋㅋㅋㅋㅋ",
          "original_form": "강아지들은 텐션감당잌ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019290.1.13",
          "form": "아ㅋㅋㅋ그렇군요",
          "original_form": "아ㅋㅋㅋ그렇군요",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019290.1.14",
          "form": "저는 주변에 강아지 키우는 사람들만 있어서",
          "original_form": "저는 주변에 강아지 키우는 사람들만 있어서",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019290.1.15",
          "form": "아하 강아지 키유시는분들 너무 대단해요 ㅋㅋㅋㅋㅋ",
          "original_form": "아하 강아지 키유시는분들 너무 대단해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019290.1.16",
          "form": "ㅋㅋㅋㅋ텐션이 엄청나요..",
          "original_form": "ㅋㅋㅋㅋ텐션이 엄청나요..",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019290.1.17",
          "form": "방방 뛰어 다닌답니다",
          "original_form": "방방 뛰어 다닌답니다",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019290.1.18",
          "form": "그래서 귀여운가봐요 키키키",
          "original_form": "그래서 귀여운가봐요 키키키",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019290.1.19",
          "form": "고양이는 어때요??",
          "original_form": "고양이는 어때요??",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019290.1.20",
          "form": "조용히 사고쳐요....",
          "original_form": "조용히 사고쳐요....",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019290.1.21",
          "form": "아ㅋㅋㅋ그게 매력이네요..",
          "original_form": "아ㅋㅋㅋ그게 매력이네요..",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019290.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 귀엽긴 한데 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 귀엽긴 한데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019290.1.23",
          "form": "완전 말랑콩떡들",
          "original_form": "완전 말랑콩떡들",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019290.1.24",
          "form": "임보 하시면 아가들 좋은 분한테 가는 건 좋은데 좀 아쉽겠어요",
          "original_form": "임보 하시면 아가들 좋은 분한테 가는 건 좋은데 좀 아쉽겠어요",
          "speaker_id": "2",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100019290.1.25",
          "form": "맞아요ㅜㅜ 근데 제가 데리고 살기엔 좁디좁은 원룸이라",
          "original_form": "맞아요ㅜㅜ 근데 제가 데리고 살기엔 좁디좁은 원룸이라",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019290.1.26",
          "form": "애기들이 답답해할 것 같더라구요",
          "original_form": "애기들이 답답해할 것 같더라구요",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019290.1.27",
          "form": "아하 그렇군요ㅠㅠ 그래도 임보 하신다니 대단하세요",
          "original_form": "아하 그렇군요ㅠㅠ 그래도 임보 하신다니 대단하세요",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019290.1.28",
          "form": "저보다 좋은가족 찾으라며 히히",
          "original_form": "저보다 좋은가족 찾으라며 히히",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100019290.1.29",
          "form": "투룸 이상으로 이사가면 꼭 입양해올거에요",
          "original_form": "투룸 이상으로 이사가면 꼭 입양해올거에요",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019290.1.30",
          "form": "그날이 오길 응원하겠습니다",
          "original_form": "그날이 오길 응원하겠습니다",
          "speaker_id": "2",
          "time": "20210827 22:14"
        }
      ]
