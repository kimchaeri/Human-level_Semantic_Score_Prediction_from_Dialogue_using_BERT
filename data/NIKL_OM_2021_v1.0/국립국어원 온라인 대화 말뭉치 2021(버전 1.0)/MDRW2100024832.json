        },
        {
          "id": "MDRW2100024832.1.2",
          "form": "맞아맞아 ㅋㅋㅋ!!",
          "original_form": "맞아맞아 ㅋㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024832.1.3",
          "form": "ㅋㅋ그거 했슈?",
          "original_form": "ㅋㅋ그거 했슈?",
          "speaker_id": "2",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100024832.1.4",
          "form": "그게 진짜 짝게 출첵도잇고",
          "original_form": "그게 진짜 짝게 출첵도잇고",
          "speaker_id": "1",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100024832.1.5",
          "form": "서베이잇우면 알람뜨는데",
          "original_form": "서베이잇우면 알람뜨는데",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024832.1.6",
          "form": "거의매일잇긴한데 해당안되면 50포받는단말이야 ㅋㅋㅋ",
          "original_form": "거의매일잇긴한데 해당안되면 50포받는단말이야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024832.1.7",
          "form": "오홍ㅋㅋㅋ응응",
          "original_form": "오홍ㅋㅋㅋ응응",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024832.1.8",
          "form": "근데 아까 무슨 캐릭터엿나보ㅓ",
          "original_form": "근데 아까 무슨 캐릭터엿나보ㅓ",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024832.1.9",
          "form": "ㅋㅋㅋㅋㅋ싫은캐릭터는 왜싫은지도 쓰라고 주관식도나옴 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ싫은캐릭터는 왜싫은지도 쓰라고 주관식도나옴 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024832.1.10",
          "form": "이게 5천포이상이면 현금인출되나 하는데",
          "original_form": "이게 5천포이상이면 현금인출되나 하는데",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024832.1.11",
          "form": "ㅋㅋㅋ왜 싫은지 쓰라니 잔인하군",
          "original_form": "ㅋㅋㅋ왜 싫은지 쓰라니 잔인하군",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024832.1.12",
          "form": "어느정도 분량이 되야",
          "original_form": "어느정도 분량이 되야",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024832.1.13",
          "form": "넘어가더라 ㅋㅋㅋㅋ",
          "original_form": "넘어가더라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024832.1.14",
          "form": "뮤튼 ㅋㅋㅋㅋ아싸 자뎅 100원에",
          "original_form": "뮤튼 ㅋㅋㅋㅋ아싸 자뎅 100원에",
          "speaker_id": "1",
          "time": "20210831 19:36"
        },
        {
