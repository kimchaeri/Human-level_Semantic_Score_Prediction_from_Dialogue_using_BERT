        {
          "id": "MDRW2100044933.1.1",
          "form": "name2님은 치즈케이크 맛집 아는 곳 잇으세요?",
          "original_form": "&name2&님은 치즈케이크 맛집 아는 곳 잇으세요?",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044933.1.2",
          "form": "요즘 분점 좀 냈던데",
          "original_form": "요즘 분점 좀 냈던데",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044933.1.3",
          "form": "도봉관 좋아해요!",
          "original_form": "도봉관 좋아해요!",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044933.1.4",
          "form": "아 바스크치즈케이크 파는 곳 맞나요?",
          "original_form": "아 바스크치즈케이크 파는 곳 맞나요?",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044933.1.5",
          "form": "네네 맞아요!",
          "original_form": "네네 맞아요!",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044933.1.6",
          "form": "작고 비싼데 맛있더라구요 ㅍ",
          "original_form": "작고 비싼데 맛있더라구요 ㅍ",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044933.1.7",
          "form": "오 들어만보고 아직 안가봣어요",
          "original_form": "오 들어만보고 아직 안가봣어요",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044933.1.8",
          "form": "작고 비싸고 맛잇군요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "작고 비싸고 맛잇군요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044933.1.9",
          "form": "네 ㅋㅋㅋㅋ 가심비를 따지러 가는 곳이에요 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ 가심비를 따지러 가는 곳이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044933.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그래도 작고 비싸고 맛없는곳도 많은데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그래도 작고 비싸고 맛없는곳도 많은데",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044933.1.11",
          "form": "맛잇다는게 중요하죠",
          "original_form": "맛잇다는게 중요하죠",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044933.1.12",
          "form": "맞아요 ㅋㅋㅋ 일단 확실하게 맛있는 집이 중요하죠",
          "original_form": "맞아요 ㅋㅋㅋ 일단 확실하게 맛있는 집이 중요하죠",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044933.1.13",
          "form": "마카롱이나 마들렌같은 다른 디저트도 좋아하세요?",
          "original_form": "마카롱이나 마들렌같은 다른 디저트도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044933.1.14",
          "form": "넵 없어서 못먹어요 ㅋㅋㅋㅋ",
          "original_form": "넵 없어서 못먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044933.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ마카롱 맛집도 아시나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ마카롱 맛집도 아시나요",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044933.1.16",
          "form": "아뇨 ㅋㅋㅋㅋㅋ ㅠ",
          "original_form": "아뇨 ㅋㅋㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044933.1.17",
          "form": "사실 제 최애 마카롱집은",
          "original_form": "사실 제 최애 마카롱집은",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044933.1.18",
          "form": "라뒤레 느낌이 났던",
          "original_form": "라뒤레 느낌이 났던",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044933.1.19",
          "form": "홍대 마카롱인데",
          "original_form": "홍대 마카롱인데",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044933.1.20",
          "form": "16년인가 17년에 문닫았어요 ㅠㅠㅠ",
          "original_form": "16년인가 17년에 문닫았어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044933.1.21",
          "form": "헐 홍대마카롱 진짜 맛잇엇죠 ㅠㅠ",
          "original_form": "헐 홍대마카롱 진짜 맛잇엇죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044933.1.22",
          "form": "ㅠㅜㅜㅠㅜ 저 그 이후로 계속 방황중이에요...",
          "original_form": "ㅠㅜㅜㅠㅜ 저 그 이후로 계속 방황중이에요...",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100044933.1.23",
          "form": "저는 첫 마카롱이 홍대마카롱이엇거든요",
          "original_form": "저는 첫 마카롱이 홍대마카롱이엇거든요",
