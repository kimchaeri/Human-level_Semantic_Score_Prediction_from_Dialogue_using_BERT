        {
          "id": "MDRW2100027955.1.1",
          "form": "영양제 먹는거 있니",
          "original_form": "영양제 먹는거 있니",
          "speaker_id": "2",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100027955.1.2",
          "form": "비타민이랑",
          "original_form": "비타민이랑",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100027955.1.3",
          "form": "오메가쓰리 먹어",
          "original_form": "오메가쓰리 먹어",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100027955.1.4",
          "form": "난 탈모때문에 맥주효모 먹어",
          "original_form": "난 탈모때문에 맥주효모 먹어",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027955.1.5",
          "form": "영양제는 건강을 위해 먹어",
          "original_form": "영양제는 건강을 위해 먹어",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027955.1.6",
          "form": "하지만 과하게 복용해도 안좋아",
          "original_form": "하지만 과하게 복용해도 안좋아",
          "speaker_id": "2",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027955.1.7",
          "form": "본인의 부족한 건강에 맞는걸 먹어야겠지",
          "original_form": "본인의 부족한 건강에 맞는걸 먹어야겠지",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027955.1.8",
          "form": "건강하다고 과신하면 안돼",
          "original_form": "건강하다고 과신하면 안돼",
          "speaker_id": "2",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027955.1.9",
          "form": "건강하니",
          "original_form": "건강하니",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027955.1.10",
          "form": "응. 꾸준히 건강검진도 받아야해",
          "original_form": "응. 꾸준히 건강검진도 받아야해",
          "speaker_id": "2",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027955.1.11",
          "form": "건강검진이 초기질병을 잡지",
          "original_form": "건강검진이 초기질병을 잡지",
          "speaker_id": "1",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027955.1.12",
          "form": "조기에 치료하는게 중요해",
          "original_form": "조기에 치료하는게 중요해",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027955.1.13",
          "form": "아프지않고 건강해야지",
          "original_form": "아프지않고 건강해야지",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027955.1.14",
          "form": "건강한게 가장큰 복이야",
          "original_form": "건강한게 가장큰 복이야",
          "speaker_id": "1",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027955.1.15",
          "form": "나도 동의해",
          "original_form": "나도 동의해",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027955.1.16",
          "form": "운동도 해야해",
          "original_form": "운동도 해야해",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027955.1.17",
