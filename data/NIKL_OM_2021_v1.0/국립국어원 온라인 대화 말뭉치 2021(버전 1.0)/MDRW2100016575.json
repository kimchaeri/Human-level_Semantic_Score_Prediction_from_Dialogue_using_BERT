      "utterance": [
        {
          "id": "MDRW2100016575.1.1",
          "form": "연극이나 뮤지컬 보시나요?",
          "original_form": "연극이나 뮤지컬 보시나요?",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016575.1.2",
          "form": "자주는 아니더라도 보긴 봐요ㅎㅎ",
          "original_form": "자주는 아니더라도 보긴 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100016575.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100016575.1.4",
          "form": "저는 둘다 실제로 본적 없어요..",
          "original_form": "저는 둘다 실제로 본적 없어요..",
          "speaker_id": "1",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016575.1.5",
          "form": "뮤지컬은 직관해보고 싶긴해요",
          "original_form": "뮤지컬은 직관해보고 싶긴해요",
          "speaker_id": "1",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016575.1.6",
          "form": "스케일이 큰 뮤지컬을 보면 압도당하는 것같아요",
          "original_form": "스케일이 큰 뮤지컬을 보면 압도당하는 것같아요",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016575.1.7",
          "form": "퀄리티가 좋으면 진짜 그 속에 있는 기분도 들구요",
          "original_form": "퀄리티가 좋으면 진짜 그 속에 있는 기분도 들구요",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016575.1.8",
          "form": "직관하고 싶은 뮤지컬 있으신가요?",
          "original_form": "직관하고 싶은 뮤지컬 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016575.1.9",
          "form": "레미제 영화 넘 재밌게 봐서..",
          "original_form": "레미제 영화 넘 재밌게 봐서..",
          "speaker_id": "1",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016575.1.10",
          "form": "헋 그 영화 너무 인상깊죠ㅠ",
          "original_form": "헋 그 영화 너무 인상깊죠ㅠ",
          "speaker_id": "2",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016575.1.11",
          "form": "저도 레미제라블 뮤지컬은 안봐서 한번 보고 싶네요",
          "original_form": "저도 레미제라블 뮤지컬은 안봐서 한번 보고 싶네요",
          "speaker_id": "2",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016575.1.12",
          "form": "오페라 유령 같은건 너무 무겆고..",
          "original_form": "오페라 유령 같은건 너무 무겆고..",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016575.1.13",
          "form": "예전에 해외팀 와서 공연하던데..",
          "original_form": "예전에 해외팀 와서 공연하던데..",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016575.1.14",
          "form": "A급 해외팀이 오면 정말 보러 가고 싶죺",
          "original_form": "A급 해외팀이 오면 정말 보러 가고 싶죺",
          "speaker_id": "2",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016575.1.15",
          "form": "레미제도 즐겨보셨다면 오페라 유령도 재밌어하실 것같은데요?",
          "original_form": "레미제도 즐겨보셨다면 오페라 유령도 재밌어하실 것같은데요?",
          "speaker_id": "2",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016575.1.16",
          "form": "오페라 유령은 너무 음침해요.. ㅋㅋㅋ",
          "original_form": "오페라 유령은 너무 음침해요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100016575.1.17",
          "form": "길기는 또 엄청 길고..",
          "original_form": "길기는 또 엄청 길고..",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016575.1.18",
          "form": "그렇게 느낄 수도 있을 것같아요",
          "original_form": "그렇게 느낄 수도 있을 것같아요",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016575.1.19",
          "form": "저는 브로드웨이에서 오페라 유령을 보게 되었는데",
          "original_form": "저는 브로드웨이에서 오페라 유령을 보게 되었는데",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016575.1.20",
          "form": "너무 감미롭고 눈이 즐거웠던 것같아요",
          "original_form": "너무 감미롭고 눈이 즐거웠던 것같아요",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016575.1.21",
          "form": "아메리카에서요?",
          "original_form": "아메리카에서요?",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016575.1.22",
          "form": "그 23번 스트릿? ㅋㅋㅋ",
          "original_form": "그 23번 스트릿? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016575.1.23",
          "form": "네네ㅋㅋㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016575.1.24",
          "form": "전 언젠가 위키드랑 시카고도 보고 싶어요",
          "original_form": "전 언젠가 위키드랑 시카고도 보고 싶어요",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016575.1.25",
          "form": "아.. 시카고도 가슴이 웅장한데..",
          "original_form": "아.. 시카고도 가슴이 웅장한데..",
          "speaker_id": "1",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016575.1.26",
          "form": "뮤지컬 영화가 좋아서 보기는 다 본듯.. ㅎㅎ",
          "original_form": "뮤지컬 영화가 좋아서 보기는 다 본듯.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016575.1.27",
          "form": "그쵸 흡입력이 장난 아니더라구요",
          "original_form": "그쵸 흡입력이 장난 아니더라구요",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016575.1.28",
          "form": "그럼 디즈니 영화도 좋아하시나요?",
          "original_form": "그럼 디즈니 영화도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016575.1.29",
          "form": "뮤지컬 영화가 많으니까ㅎㅎ",
          "original_form": "뮤지컬 영화가 많으니까ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016575.1.30",
          "form": "오.. 좋죠...\r\n특히 옛날 시리즈가.. \r\n그 알라딘. 야수. 뮬란",
          "original_form": "오.. 좋죠...\r\n특히 옛날 시리즈가.. \r\n그 알라딘. 야수. 뮬란",
          "speaker_id": "1",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100016575.1.31",
          "form": "맞아요 옛날 디즈니 영화가 힐링되면서 푹 빠지게 하는 것같아요",
          "original_form": "맞아요 옛날 디즈니 영화가 힐링되면서 푹 빠지게 하는 것같아요",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016575.1.32",
          "form": "리메이크된걸 보면 반가우면서 새롭기도 하고요",
          "original_form": "리메이크된걸 보면 반가우면서 새롭기도 하고요",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016575.1.33",
          "form": "이번에 크루엘라 보셨나요?",
          "original_form": "이번에 크루엘라 보셨나요?",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016575.1.34",
          "form": "네... 봤죠...",
          "original_form": "네... 봤죠...",
          "speaker_id": "1",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016575.1.35",
          "form": "그.. 주인공이... 그.. 그.. ㅎㅎㄹ",
          "original_form": "그.. 주인공이... 그.. 그.. ㅎㅎㄹ",
          "speaker_id": "1",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016575.1.36",
          "form": "넹?",
          "original_form": "넹?",
          "speaker_id": "2",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016575.1.37",
          "form": "생각이 안나네.. 유명한 여배우였는데..",
          "original_form": "생각이 안나네.. 유명한 여배우였는데..",
          "speaker_id": "1",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016575.1.38",
          "form": "엠마 스톤이요?",
          "original_form": "엠마 스톤이요?",
          "speaker_id": "2",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016575.1.39",
          "form": "아.. 라라랜드였엇나요?",
          "original_form": "아.. 라라랜드였엇나요?",
          "speaker_id": "1",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016575.1.40",
          "form": "네네 라라랜드 여주인공도 엠마 스톤이었죵",
          "original_form": "네네 라라랜드 여주인공도 엠마 스톤이었죵",
          "speaker_id": "2",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016575.1.41",
          "form": "라라랜드가 초대박... 너무 아름다훠효",
          "original_form": "라라랜드가 초대박... 너무 아름다훠효",
