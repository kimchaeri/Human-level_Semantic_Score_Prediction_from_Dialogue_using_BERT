      "utterance": [
        {
          "id": "MDRW2100021595.1.1",
          "form": "요즘 하는거야??",
          "original_form": "요즘 하는거야??",
          "speaker_id": "2",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021595.1.2",
          "form": "노논",
          "original_form": "노논",
          "speaker_id": "1",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021595.1.3",
          "form": "작년 5월인가 햇더라고",
          "original_form": "작년 5월인가 햇더라고",
          "speaker_id": "1",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021595.1.4",
          "form": "정일우나오는거!!",
          "original_form": "정일우나오는거!!",
          "speaker_id": "1",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021595.1.5",
          "form": "오 전혀 몰랐네 ㅋㅋㅋ",
          "original_form": "오 전혀 몰랐네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100021595.1.6",
          "form": "어제티비에서 1화2화하길래",
          "original_form": "어제티비에서 1화2화하길래",
          "speaker_id": "1",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021595.1.7",
          "form": "보다가jtbc꺼라 티빙에서 정주행중 ㅋㅋ 여기서 달팽이요리잠깐 잡아줌 ㅋㅋ",
          "original_form": "보다가jtbc꺼라 티빙에서 정주행중 ㅋㅋ 여기서 달팽이요리잠깐 잡아줌 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021595.1.8",
          "form": "ㅋㅋㅋ오 달팽이요리도 하다니 ㅋㅋ",
          "original_form": "ㅋㅋㅋ오 달팽이요리도 하다니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021595.1.9",
          "form": "정일우 요리 잘하잖아! ㅋㅋ 그래서 요리 드라마도 찍었구만",
          "original_form": "정일우 요리 잘하잖아! ㅋㅋ 그래서 요리 드라마도 찍었구만",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021595.1.10",
          "form": "소개팅장면에서 잠깐 잡아줌 ㅋㅋ정일우가 세프인데",
          "original_form": "소개팅장면에서 잠깐 잡아줌 ㅋㅋ정일우가 세프인데",
          "speaker_id": "1",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021595.1.11",
          "form": "걘 달팽이요리를 해줌 ㅋㅋ",
          "original_form": "걘 달팽이요리를 해줌 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021595.1.12",
          "form": "무츤 막 잼있는건 아닌데 그냥 정일우좋아서 ㅋㅋㅋㅋ",
          "original_form": "무츤 막 잼있는건 아닌데 그냥 정일우좋아서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021595.1.13",
          "form": "글쿠먼ㅋㅋㅋ 재밌어?",
          "original_form": "글쿠먼ㅋㅋㅋ 재밌어?",
          "speaker_id": "2",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021595.1.14",
          "form": "위에.답이되버렷네요",
          "original_form": "위에.답이되버렷네요",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021595.1.15",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021595.1.16",
          "form": "ㅋㅋ정일우는 하이킥이 리즈였지",
          "original_form": "ㅋㅋ정일우는 하이킥이 리즈였지",
          "speaker_id": "2",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021595.1.17",
          "form": "ㅋㅋㅋㅋ추억이다ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ추억이다ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021595.1.18",
          "form": "요즘은 시트콤 없어서 아쉬워",
          "original_form": "요즘은 시트콤 없어서 아쉬워",
          "speaker_id": "2",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021595.1.19",
          "form": "생각해보니 그렇네!!",
          "original_form": "생각해보니 그렇네!!",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021595.1.20",
          "form": "또 재밌는거 나오면 좋겠다 ㅋㅋ",
          "original_form": "또 재밌는거 나오면 좋겠다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021595.1.21",
          "form": "나 내가좋아하는거 외면하지 않으려고",
          "original_form": "나 내가좋아하는거 외면하지 않으려고",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021595.1.22",
          "form": "니가 좋아하는게 뭔데",
          "original_form": "니가 좋아하는게 뭔데",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021595.1.23",
          "form": "ㅋㅋㅋㅋㅋ저기요 대사 연습하시나요?",
          "original_form": "ㅋㅋㅋㅋㅋ저기요 대사 연습하시나요?",
