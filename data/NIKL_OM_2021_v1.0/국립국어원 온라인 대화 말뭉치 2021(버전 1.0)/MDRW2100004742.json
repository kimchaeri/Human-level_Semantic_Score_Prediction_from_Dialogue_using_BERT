      "utterance": [
        {
          "id": "MDRW2100004742.1.1",
          "form": "키우시는 동물있으세요?",
          "original_form": "키우시는 동물있으세요?",
          "speaker_id": "1",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004742.1.2",
          "form": "개 고양이 닭이요",
          "original_form": "개 고양이 닭이요",
          "speaker_id": "2",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004742.1.3",
          "form": "오 닭도 키우세요?",
          "original_form": "오 닭도 키우세요?",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004742.1.4",
          "form": "신선한 계란도 드실수 있겠어요",
          "original_form": "신선한 계란도 드실수 있겠어요",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004742.1.5",
          "form": "암닭이 세마리라 하루 닭알 세개 낳아요.\r\n닭알은 안 사먹어도 돼요.",
          "original_form": "암닭이 세마리라 하루 닭알 세개 낳아요.\r\n닭알은 안 사먹어도 돼요.",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004742.1.6",
          "form": "오 좋은데요 요즘 계란값이 금값인데",
          "original_form": "오 좋은데요 요즘 계란값이 금값인데",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004742.1.7",
          "form": "저번에 계란 파동인가 였을 때도 울 집은 끄덕 없었죠",
          "original_form": "저번에 계란 파동인가 였을 때도 울 집은 끄덕 없었죠",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004742.1.8",
          "form": "오 복받으셨네요ㅎ 개는 진도개키우시는거예요?",
          "original_form": "오 복받으셨네요ㅎ 개는 진도개키우시는거예요?",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004742.1.9",
          "form": "풍산개랑 진도 믹스요",
          "original_form": "풍산개랑 진도 믹스요",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004742.1.10",
          "form": "귀엽겠어요 저희부모님댁도 한마리 키워요",
          "original_form": "귀엽겠어요 저희부모님댁도 한마리 키워요",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004742.1.11",
          "form": "믹스견",
          "original_form": "믹스견",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004742.1.12",
          "form": "네 동물을 좋아하는 편이라 강아지 고양이 다 이뻐요",
          "original_form": "네 동물을 좋아하는 편이라 강아지 고양이 다 이뻐요",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004742.1.13",
          "form": "저도요 고양이 발이 왜그렇게 귀여운지",
          "original_form": "저도요 고양이 발이 왜그렇게 귀여운지",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004742.1.14",
          "form": "젤리같아요ㅎ",
          "original_form": "젤리같아요ㅎ",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004742.1.15",
          "form": "맞아요 솜방망이 핑크젤리",
          "original_form": "맞아요 솜방망이 핑크젤리",
          "speaker_id": "2",
          "time": "20210821 20:15"
        }
      ]
