      "utterance": [
        {
          "id": "MDRW2100016393.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016393.1.2",
          "form": "ㅎㅇ",
          "original_form": "ㅎㅇ",
          "speaker_id": "2",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100016393.1.3",
          "form": "나는 반려동물이 없어",
          "original_form": "나는 반려동물이 없어",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100016393.1.4",
          "form": "그럼 뭐 키우고 싶어?",
          "original_form": "그럼 뭐 키우고 싶어?",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100016393.1.5",
          "form": "나는 강아지 1,  고양이 1 씩 키우고 싶어",
          "original_form": "나는 강아지 1,  고양이 1 씩 키우고 싶어",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100016393.1.6",
          "form": "난 강아지 키우긴 한데 사실 고양이를 더 좋아하긴 해",
          "original_form": "난 강아지 키우긴 한데 사실 고양이를 더 좋아하긴 해",
          "speaker_id": "1",
          "time": "20210827 13:06"
        },
        {
          "id": "MDRW2100016393.1.7",
          "form": "너는 약간의 고양이 상이야",
          "original_form": "너는 약간의 고양이 상이야",
          "speaker_id": "2",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100016393.1.8",
          "form": "그래서 고양이를 더 좋아하는구나",
          "original_form": "그래서 고양이를 더 좋아하는구나",
          "speaker_id": "2",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100016393.1.9",
          "form": "그런가?ㅋㅋ",
          "original_form": "그런가?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100016393.1.10",
          "form": "그냥 고양이 매력있어",
          "original_form": "그냥 고양이 매력있어",
          "speaker_id": "1",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100016393.1.11",
          "form": "나는 고양이상이 참 좋아",
          "original_form": "나는 고양이상이 참 좋아",
          "speaker_id": "2",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100016393.1.12",
          "form": "나도 고양이상이 되고싶어",
          "original_form": "나도 고양이상이 되고싶어",
          "speaker_id": "2",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100016393.1.13",
          "form": "난 좋아하는 얼굴은 강아지상",
          "original_form": "난 좋아하는 얼굴은 강아지상",
          "speaker_id": "1",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100016393.1.14",
          "form": "그래서 너가 내 쌩얼을 좋아하는구나?",
          "original_form": "그래서 너가 내 쌩얼을 좋아하는구나?",
          "speaker_id": "2",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100016393.1.15",
          "form": "주제에 젓어난 대화 아니냐구",
          "original_form": "주제에 젓어난 대화 아니냐구",
          "speaker_id": "1",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100016393.1.16",
          "form": "어릴때 반려동물 키운적 없어?",
          "original_form": "어릴때 반려동물 키운적 없어?",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100016393.1.17",
          "form": "나는 아마도 없을거야",
          "original_form": "나는 아마도 없을거야",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100016393.1.18",
          "form": "우리 아빠가 군대에 계셨을 때 큰 개를 키우셨어",
          "original_form": "우리 아빠가 군대에 계셨을 때 큰 개를 키우셨어",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100016393.1.19",
          "form": "헐 진짜?",
          "original_form": "헐 진짜?",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100016393.1.20",
          "form": "무슨 종류였어?",
          "original_form": "무슨 종류였어?",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100016393.1.21",
          "form": "그래서 아빠는 키우면 큰 개를 키우고 싶으시대",
          "original_form": "그래서 아빠는 키우면 큰 개를 키우고 싶으시대",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100016393.1.22",
          "form": "종류는 잘 모르겠어",
          "original_form": "종류는 잘 모르겠어",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100016393.1.23",
          "form": "나도 큰 개 키우고 싶다",
          "original_form": "나도 큰 개 키우고 싶다",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100016393.1.24",
          "form": "용맹하게 생겼어",
          "original_form": "용맹하게 생겼어",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100016393.1.25",
          "form": "골든 리트리버 같은",
          "original_form": "골든 리트리버 같은",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100016393.1.26",
          "form": "나도 리트리버 좋아",
          "original_form": "나도 리트리버 좋아",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100016393.1.27",
          "form": "근데 포메를 더 키우고싶어",
          "original_form": "근데 포메를 더 키우고싶어",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100016393.1.28",
          "form": "예쁘게 생겼거든",
          "original_form": "예쁘게 생겼거든",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100016393.1.29",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100016393.1.30",
          "form": "포메도 용맹해",
          "original_form": "포메도 용맹해",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100016393.1.31",
          "form": "누가 나보고 흰색에 황토색 살짝  섞인 포메닮았대 ㅎㅋ",
          "original_form": "누가 나보고 흰색에 황토색 살짝  섞인 포메닮았대 ㅎㅋ",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100016393.1.32",
          "form": "음 뭔 말하는지 알겠다 ㅋㅋㅋ",
          "original_form": "음 뭔 말하는지 알겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100016393.1.33",
          "form": "난 닥스훈트도 좋아",
          "original_form": "난 닥스훈트도 좋아",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100016393.1.34",
          "form": "나는 알러지도 없어서 좋아",
          "original_form": "나는 알러지도 없어서 좋아",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100016393.1.35",
          "form": "어릴 때 키웠거든",
          "original_form": "어릴 때 키웠거든",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100016393.1.36",
          "form": "닥스훈트는 내 취향  아니야",
          "original_form": "닥스훈트는 내 취향  아니야",
          "speaker_id": "2",
          "time": "20210827 13:14"
        },
        {
          "id": "MDRW2100016393.1.37",
          "form": "난 시골 자브종도 좋아",
          "original_form": "난 시골 자브종도 좋아",
          "speaker_id": "2",
          "time": "20210827 13:14"
        }
      ]
