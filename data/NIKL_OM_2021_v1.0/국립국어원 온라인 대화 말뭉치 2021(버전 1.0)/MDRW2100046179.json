      "utterance": [
        {
          "id": "MDRW2100046179.1.1",
          "form": "곱창 좋아하세요?",
          "original_form": "곱창 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046179.1.2",
          "form": "헐?",
          "original_form": "헐?",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046179.1.3",
          "form": "제 최애푸드요",
          "original_form": "제 최애푸드요",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046179.1.4",
          "form": "저 막창이랑",
          "original_form": "저 막창이랑",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046179.1.5",
          "form": "전골좋아해요!",
          "original_form": "전골좋아해요!",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046179.1.6",
          "form": "저는 곱창대창이요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 곱창대창이요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046179.1.7",
          "form": "전골도!!!",
          "original_form": "전골도!!!",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046179.1.8",
          "form": "근데 집에 냄비가 없어서",
          "original_form": "근데 집에 냄비가 없어서",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046179.1.9",
          "form": "곱창 못먹고있어요ㅠㅠ",
          "original_form": "곱창 못먹고있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046179.1.10",
          "form": "밖에 나가서 드시먄듀ㅐ요!",
          "original_form": "밖에 나가서 드시먄듀ㅐ요!",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046179.1.11",
          "form": "동네 곱창맛집으로ㅋㅋ",
          "original_form": "동네 곱창맛집으로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046179.1.12",
          "form": "아직 백신을 못맞아서",
          "original_form": "아직 백신을 못맞아서",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046179.1.13",
          "form": "밖에서 먹기는... 좀 그래요 ㅠㅠ",
          "original_form": "밖에서 먹기는... 좀 그래요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046179.1.14",
          "form": "요즘 인터넷에도 곱창잘나오던데",
          "original_form": "요즘 인터넷에도 곱창잘나오던데",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046179.1.15",
          "form": "집에 에어프라이어잇으세요?",
          "original_form": "집에 에어프라이어잇으세요?",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046179.1.16",
          "form": "있어요!!",
          "original_form": "있어요!!",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046179.1.17",
          "form": "집 바로근처에 전골잘하는집 있어서....",
          "original_form": "집 바로근처에 전골잘하는집 있어서....",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046179.1.18",
          "form": "그러면 유명한거 사다가",
          "original_form": "그러면 유명한거 사다가",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046179.1.19",
          "form": "해드세여!",
          "original_form": "해드세여!",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046179.1.20",
          "form": "그럴까요?!",
          "original_form": "그럴까요?!",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046179.1.21",
          "form": "오늘 주문해야겠어요!!",
          "original_form": "오늘 주문해야겠어요!!",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046179.1.22",
          "form": "검색하면",
          "original_form": "검색하면",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046179.1.23",
          "form": "주문수젤많은곳으로",
          "original_form": "주문수젤많은곳으로",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046179.1.24",
          "form": "시켜보세요ㅋㅋㅋ",
          "original_form": "시켜보세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046179.1.25",
          "form": "제가 소는 먹는데",
          "original_form": "제가 소는 먹는데",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046179.1.26",
          "form": "돼지는 못먹어서",
          "original_form": "돼지는 못먹어서",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046179.1.27",
          "form": "소곱창도 포장 잘나와요!",
          "original_form": "소곱창도 포장 잘나와요!",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046179.1.28",
          "form": "잡내안나고",
          "original_form": "잡내안나고",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046179.1.29",
          "form": "소스도 다 주고",
          "original_form": "소스도 다 주고",
          "speaker_id": "2",
          "time": "20210908 11:07"
        }
      ]
    }
  ]
