      "utterance": [
        {
          "id": "MDRW2100043078.1.1",
          "form": "요즘 노래 머들어",
          "original_form": "요즘 노래 머들어",
          "speaker_id": "2",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100043078.1.2",
          "form": "안들어 너는",
          "original_form": "안들어 너는",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043078.1.3",
          "form": "왜안들어드라이브송없어?",
          "original_form": "왜안들어드라이브송없어?",
          "speaker_id": "2",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100043078.1.4",
          "form": "아이돌노래 틀어줘야지",
          "original_form": "아이돌노래 틀어줘야지",
          "speaker_id": "2",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100043078.1.5",
          "form": "사실 지니야 신나는노래틀어줘해서 들어",
          "original_form": "사실 지니야 신나는노래틀어줘해서 들어",
          "speaker_id": "1",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100043078.1.6",
          "form": "ㅋㅋㅋㅋㅋ사실 나도 멜론 탑텐귀",
          "original_form": "ㅋㅋㅋㅋㅋ사실 나도 멜론 탑텐귀",
          "speaker_id": "2",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100043078.1.7",
          "form": "아까 김태호 피디 퇴사한단거 보니까 놀뭐까지는 할건가봐\r\n올해까지?",
          "original_form": "아까 김태호 피디 퇴사한단거 보니까 놀뭐까지는 할건가봐\r\n올해까지?",
          "speaker_id": "2",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100043078.1.8",
          "form": "아 그 뒤에 넘ㄱㅣ려나",
          "original_form": "아 그 뒤에 넘ㄱㅣ려나",
          "speaker_id": "1",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100043078.1.9",
          "form": "그런가봐 20년다녔대",
          "original_form": "그런가봐 20년다녔대",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100043078.1.10",
          "form": "종편으로갈수도있겠다",
          "original_form": "종편으로갈수도있겠다",
          "speaker_id": "1",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100043078.1.11",
          "form": "김태호피디 거의 준연예인아니냐고 ㅋㅋㅋ",
          "original_form": "김태호피디 거의 준연예인아니냐고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100043078.1.12",
          "form": "내생각엔 안묶이고 할수도..프리로?",
          "original_form": "내생각엔 안묶이고 할수도..프리로?",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100043078.1.13",
          "form": "왜 다 나쁜말이래",
          "original_form": "왜 다 나쁜말이래",
          "speaker_id": "1",
          "time": "20210907 16:34"
        },
        {
          "id": "MDRW2100043078.1.14",
          "form": "데려가려고할텐데",
          "original_form": "데려가려고할텐데",
          "speaker_id": "1",
          "time": "20210907 16:34"
        },
        {
          "id": "MDRW2100043078.1.15",
          "form": "모시고 가게찌",
          "original_form": "모시고 가게찌",
          "speaker_id": "2",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100043078.1.16",
          "form": "근데 기획능력진짜좋아",
          "original_form": "근데 기획능력진짜좋아",
          "speaker_id": "1",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100043078.1.17",
          "form": "마자 유느랑 잘맞아",
          "original_form": "마자 유느랑 잘맞아",
          "speaker_id": "2",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100043078.1.18",
          "form": "나가면 어떤거 할지 기대돼 넷플에서 한번 거하게 투자해주라...",
          "original_form": "나가면 어떤거 할지 기대돼 넷플에서 한번 거하게 투자해주라...",
          "speaker_id": "2",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100043078.1.19",
          "form": "오맞아 그런 플랫폼에서 하면 좋겠다",
          "original_form": "오맞아 그런 플랫폼에서 하면 좋겠다",
          "speaker_id": "1",
          "time": "20210907 16:36"
        }
      ]
