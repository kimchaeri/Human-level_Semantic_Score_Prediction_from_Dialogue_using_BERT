      "utterance": [
        {
          "id": "MDRW2100031304.1.1",
          "form": "오 스포츠도 즐기시나요",
          "original_form": "오 스포츠도 즐기시나요",
          "speaker_id": "2",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100031304.1.2",
          "form": "스포츠를 하기보단 보는 걸 좋아해요",
          "original_form": "스포츠를 하기보단 보는 걸 좋아해요",
          "speaker_id": "1",
          "time": "20210904 21:26"
        },
        {
          "id": "MDRW2100031304.1.3",
          "form": "어떤 경기를 주로 보세요",
          "original_form": "어떤 경기를 주로 보세요",
          "speaker_id": "2",
          "time": "20210904 21:42"
        },
        {
          "id": "MDRW2100031304.1.4",
          "form": "야구도 보고",
          "original_form": "야구도 보고",
          "speaker_id": "1",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100031304.1.5",
          "form": "최근에 올림픽 끝나고는 여자배구도",
          "original_form": "최근에 올림픽 끝나고는 여자배구도",
          "speaker_id": "1",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100031304.1.6",
          "form": "바쁘셔도 다 챙겨보시나봐요ㅋㅋㅋ",
          "original_form": "바쁘셔도 다 챙겨보시나봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100031304.1.7",
          "form": "아 실시간으로는 못볼 때도 있는데",
          "original_form": "아 실시간으로는 못볼 때도 있는데",
          "speaker_id": "1",
          "time": "20210904 21:46"
        },
        {
          "id": "MDRW2100031304.1.8",
          "form": "경기 하이라이트나 소식은 챙겨봐요",
          "original_form": "경기 하이라이트나 소식은 챙겨봐요",
          "speaker_id": "1",
          "time": "20210904 21:46"
        },
        {
          "id": "MDRW2100031304.1.9",
          "form": "아버지가 워낙 스포츠 중계 보는 걸 좋아하시기도 하고",
          "original_form": "아버지가 워낙 스포츠 중계 보는 걸 좋아하시기도 하고",
          "speaker_id": "1",
          "time": "20210904 21:47"
        },
        {
          "id": "MDRW2100031304.1.10",
          "form": "아하 아버지랑 같이 보시는군요ㅋㅋㅋ",
          "original_form": "아하 아버지랑 같이 보시는군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031304.1.11",
          "form": "어느 팀 응원하시나요",
          "original_form": "어느 팀 응원하시나요",
          "speaker_id": "2",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031304.1.12",
          "form": "야구는 두산베어스요ㅎ",
          "original_form": "야구는 두산베어스요ㅎ",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031304.1.13",
          "form": "배구는 아직 못정했어요",
          "original_form": "배구는 아직 못정했어요",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031304.1.14",
          "form": "아 두산! 좋은 팀이죠ㅋㅋㅋ",
          "original_form": "아 두산! 좋은 팀이죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100031304.1.15",
          "form": "네네 아버지가 예전부터 두산 팬이셔서",
          "original_form": "네네 아버지가 예전부터 두산 팬이셔서",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031304.1.16",
          "form": "야구 좀 보시나요?",
          "original_form": "야구 좀 보시나요?",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031304.1.17",
          "form": "옛날엔 구장으로 좀 보러다녔는데",
          "original_form": "옛날엔 구장으로 좀 보러다녔는데",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031304.1.18",
          "form": "안간지 좀 됐네요 ㅋㅋㅋ",
          "original_form": "안간지 좀 됐네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031304.1.19",
          "form": "롯데의 연고지라 ㅋㅋㅋ",
          "original_form": "롯데의 연고지라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031304.1.20",
          "form": "오 직관도 많이 다니셨구나",
          "original_form": "오 직관도 많이 다니셨구나",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031304.1.21",
          "form": "앗앗 부산갈매기~",
          "original_form": "앗앗 부산갈매기~",
          "speaker_id": "1",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100031304.1.22",
          "form": "저희 이모부가 롯데 왕팬이신데",
          "original_form": "저희 이모부가 롯데 왕팬이신데",
          "speaker_id": "1",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100031304.1.23",
          "form": "넹 ㅋㅋㅋ 응원하는게 꿀잼이지만",
          "original_form": "넹 ㅋㅋㅋ 응원하는게 꿀잼이지만",
          "speaker_id": "2",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100031304.1.24",
          "form": "경기내용은 안 꿀잼..",
          "original_form": "경기내용은 안 꿀잼..",
          "speaker_id": "2",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100031304.1.25",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100031304.1.26",
          "form": "야구장 가는 것도 재미있는데",
          "original_form": "야구장 가는 것도 재미있는데",
          "speaker_id": "1",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100031304.1.27",
          "form": "요즘은 갈수가 없으니 아쉬워요",
          "original_form": "요즘은 갈수가 없으니 아쉬워요",
          "speaker_id": "1",
          "time": "20210904 22:07"
        }
      ]
    }
