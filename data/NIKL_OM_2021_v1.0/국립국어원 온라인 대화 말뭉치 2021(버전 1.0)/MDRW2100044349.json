      "utterance": [
        {
          "id": "MDRW2100044349.1.1",
          "form": "네 아이스크림 싫어하는 사람도 있나요?",
          "original_form": "네 아이스크림 싫어하는 사람도 있나요?",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044349.1.2",
          "form": "그럴수도?",
          "original_form": "그럴수도?",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044349.1.3",
          "form": "머조아해여",
          "original_form": "머조아해여",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044349.1.4",
          "form": "저 할매입맛이라 비비빅 팥맛이요",
          "original_form": "저 할매입맛이라 비비빅 팥맛이요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044349.1.5",
          "form": "오 저도 조아해요",
          "original_form": "오 저도 조아해요",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044349.1.6",
          "form": "비비빅 다른맛은요?",
          "original_form": "비비빅 다른맛은요?",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044349.1.7",
          "form": "다른맛은 그냥 먹을 만하다 이정도?-ㅎㅎ",
          "original_form": "다른맛은 그냥 먹을 만하다 이정도?-ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044349.1.8",
          "form": "또 돼지바랑 수박바 좋아하구요",
          "original_form": "또 돼지바랑 수박바 좋아하구요",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044349.1.9",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044349.1.10",
          "form": "수박바 초록부분 조아해요?",
          "original_form": "수박바 초록부분 조아해요?",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044349.1.11",
          "form": "거기가 하이라이트 아닌가요? 근데 그 수박바 뒤집어진거 먹어봤는데 오리지날이 더 나아요",
          "original_form": "거기가 하이라이트 아닌가요? 근데 그 수박바 뒤집어진거 먹어봤는데 오리지날이 더 나아요",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044349.1.12",
          "form": "역시 오리지날이 최고야",
          "original_form": "역시 오리지날이 최고야",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044349.1.13",
          "form": "아그래요? ㅋㅋ 전 뒤집은거 안머거봄",
          "original_form": "아그래요? ㅋㅋ 전 뒤집은거 안머거봄",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044349.1.14",
          "form": "쭈쭈바도 좋아해요?",
          "original_form": "쭈쭈바도 좋아해요?",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044349.1.15",
          "form": "완젼이요",
          "original_form": "완젼이요",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044349.1.16",
          "form": "아이스크림마트 가고싶네요",
          "original_form": "아이스크림마트 가고싶네요",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044349.1.17",
          "form": "저는 탱크보이랑 뽕따좋아해요",
          "original_form": "저는 탱크보이랑 뽕따좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044349.1.18",
          "form": "클래식하시네요",
          "original_form": "클래식하시네요",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044349.1.19",
          "form": "뽕따 찾기 힘드러요",
          "original_form": "뽕따 찾기 힘드러요",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044349.1.20",
          "form": "맞아요 동네 슈퍼에서 가끔 발견해서 사먹죵",
          "original_form": "맞아요 동네 슈퍼에서 가끔 발견해서 사먹죵",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044349.1.21",
          "form": "님 중국집 조아해요?",
          "original_form": "님 중국집 조아해요?",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044349.1.22",
          "form": "넹 근데 정말 가끔먹어야 맛있어요 ㅋㅋㅋㅋ",
          "original_form": "넹 근데 정말 가끔먹어야 맛있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044349.1.23",
          "form": "먹었을때 가끔 배아플때고 있고.. 해서..",
          "original_form": "먹었을때 가끔 배아플때고 있고.. 해서..",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044349.1.24",
          "form": "짬뽕 조아해요?",
          "original_form": "짬뽕 조아해요?",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044349.1.25",
          "form": "네 해산물 듬뿍 들어간걸로다가",
          "original_form": "네 해산물 듬뿍 들어간걸로다가",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044349.1.26",
          "form": "크 짜장은요?",
          "original_form": "크 짜장은요?",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044349.1.27",
          "form": "쟁반 짜장 좋아해요 약간 매콤한거요",
          "original_form": "쟁반 짜장 좋아해요 약간 매콤한거요",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044349.1.28",
          "form": "오 간짜장은요",
          "original_form": "오 간짜장은요",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044349.1.29",
          "form": "간짜장은 잘 안먹어요 ㅋㅋㅋㅋ",
          "original_form": "간짜장은 잘 안먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044349.1.30",
          "form": "저 예전에 친구네랑 먹은 짜장밥이 잊혀지질않네요",
          "original_form": "저 예전에 친구네랑 먹은 짜장밥이 잊혀지질않네요",
          "speaker_id": "2",
          "time": "20210907 22:50"
        }
      ]
