        },
        {
          "id": "MDRW2100040783.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210905 06:56"
        },
        {
          "id": "MDRW2100040783.1.3",
          "form": "날씨가 많이 선선해졌어요",
          "original_form": "날씨가 많이 선선해졌어요",
          "speaker_id": "1",
          "time": "20210905 06:58"
        },
        {
          "id": "MDRW2100040783.1.4",
          "form": "네네~저 지금 친정왔는데 너무 춥네요;;",
          "original_form": "네네~저 지금 친정왔는데 너무 춥네요;;",
          "speaker_id": "2",
          "time": "20210905 07:03"
        },
        {
          "id": "MDRW2100040783.1.5",
          "form": "감기걸릴꺼같아요~",
          "original_form": "감기걸릴꺼같아요~",
          "speaker_id": "2",
          "time": "20210905 07:03"
        },
        {
          "id": "MDRW2100040783.1.6",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210905 07:04"
        },
        {
          "id": "MDRW2100040783.1.7",
          "form": "새벽공기는 많이 차요",
          "original_form": "새벽공기는 많이 차요",
          "speaker_id": "1",
          "time": "20210905 07:04"
        },
        {
          "id": "MDRW2100040783.1.8",
          "form": "애들옷도 다 짧은거 챙겼는게 놀랐어요 ㅋㅋㅋㅋ",
          "original_form": "애들옷도 다 짧은거 챙겼는게 놀랐어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:05"
        },
        {
          "id": "MDRW2100040783.1.9",
          "form": "지역마다 날씨가 이렇게 차이 나나요;",
          "original_form": "지역마다 날씨가 이렇게 차이 나나요;",
          "speaker_id": "2",
          "time": "20210905 07:05"
        },
        {
          "id": "MDRW2100040783.1.10",
          "form": "아 먼 지역으로 이동하셨나봐요?",
          "original_form": "아 먼 지역으로 이동하셨나봐요?",
          "speaker_id": "1",
          "time": "20210905 07:06"
        },
        {
          "id": "MDRW2100040783.1.11",
          "form": "저희 동네는 아직 낮엔 따뜻한데",
          "original_form": "저희 동네는 아직 낮엔 따뜻한데",
          "speaker_id": "1",
          "time": "20210905 07:06"
        },
        {
          "id": "MDRW2100040783.1.12",
          "form": "낮엔 여기도 따뜻하던데 밤부터 슬 춥기시작하더니 새벽엔 다들 깼네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "낮엔 여기도 따뜻하던데 밤부터 슬 춥기시작하더니 새벽엔 다들 깼네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:07"
        },
        {
          "id": "MDRW2100040783.1.13",
          "form": "아 새벽공기가 차서 다 깨셨나봐요ㅜ",
          "original_form": "아 새벽공기가 차서 다 깨셨나봐요ㅜ",
          "speaker_id": "1",
          "time": "20210905 07:08"
        },
        {
          "id": "MDRW2100040783.1.14",
          "form": "일교차가 크죠",
          "original_form": "일교차가 크죠",
          "speaker_id": "1",
          "time": "20210905 07:08"
        },
        {
          "id": "MDRW2100040783.1.15",
          "form": "그렇네요~있던 지역은 새벽에도 선풍기틀거나",
          "original_form": "그렇네요~있던 지역은 새벽에도 선풍기틀거나",
          "speaker_id": "2",
          "time": "20210905 07:09"
        },
        {
          "id": "MDRW2100040783.1.16",
          "form": "에어컨 약하게 쭉 돌렸거든요;;",
          "original_form": "에어컨 약하게 쭉 돌렸거든요;;",
          "speaker_id": "2",
          "time": "20210905 07:09"
        },
        {
          "id": "MDRW2100040783.1.17",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210905 07:09"
        },
        {
          "id": "MDRW2100040783.1.18",
          "form": "따뜻한 지역이셨나보다",
          "original_form": "따뜻한 지역이셨나보다",
          "speaker_id": "1",
          "time": "20210905 07:09"
        },
        {
          "id": "MDRW2100040783.1.19",
          "form": "저희 동네는 2주 전부터는 잘때 창문 꼭 닫고 자야했어요",
          "original_form": "저희 동네는 2주 전부터는 잘때 창문 꼭 닫고 자야했어요",
          "speaker_id": "1",
          "time": "20210905 07:10"
        },
        {
          "id": "MDRW2100040783.1.20",
          "form": "밤공기가 서늘해서",
          "original_form": "밤공기가 서늘해서",
          "speaker_id": "1",
          "time": "20210905 07:10"
        },
        {
          "id": "MDRW2100040783.1.21",
          "form": "정말요??",
          "original_form": "정말요??",
          "speaker_id": "2",
          "time": "20210905 07:10"
        },
        {
          "id": "MDRW2100040783.1.22",
          "form": "그렇군요~",
          "original_form": "그렇군요~",
          "speaker_id": "2",
          "time": "20210905 07:10"
        },
        {
          "id": "MDRW2100040783.1.23",
          "form": "이제 정말 가을이네요^^",
          "original_form": "이제 정말 가을이네요^^",
          "speaker_id": "2",
          "time": "20210905 07:10"
        },
        {
          "id": "MDRW2100040783.1.24",
          "form": "네 가을이 성큼 찾아왔어요~",
          "original_form": "네 가을이 성큼 찾아왔어요~",
          "speaker_id": "1",
          "time": "20210905 07:11"
        }
      ]
