      "utterance": [
        {
          "id": "MDRW2100019854.1.1",
          "form": "사람들이 좋은사람도 있고 나쁜 사람도 있는데ㅎ",
          "original_form": "사람들이 좋은사람도 있고 나쁜 사람도 있는데ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019854.1.2",
          "form": "오픈채팅방이라ㅎ 그냥 관망중ㅎ",
          "original_form": "오픈채팅방이라ㅎ 그냥 관망중ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019854.1.3",
          "form": "근데 두분은 너무 좋으셔서ㅎ",
          "original_form": "근데 두분은 너무 좋으셔서ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019854.1.4",
          "form": "나쁘다라기보다 저는  물음표 찍는거 좀그래요ㅠㅠ",
          "original_form": "나쁘다라기보다 저는  물음표 찍는거 좀그래요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019854.1.5",
          "form": "누구신지 아실듯",
          "original_form": "누구신지 아실듯",
          "speaker_id": "1",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019854.1.6",
          "form": "그래도 커뮤니티 활동 할만해요ㅎ",
          "original_form": "그래도 커뮤니티 활동 할만해요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019854.1.7",
          "form": "name1?",
          "original_form": "&name1&?",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019854.1.8",
          "form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019854.1.9",
          "form": "네네 강퇴하라는 말은 아니에요 ㅎㅎㅎ",
          "original_form": "네네 강퇴하라는 말은 아니에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019854.1.10",
          "form": "다름은 인정하는데",
          "original_form": "다름은 인정하는데",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019854.1.11",
          "form": "가끔 물음표 찍으시면 알려주기 싫어요 ㅠㅠ",
          "original_form": "가끔 물음표 찍으시면 알려주기 싫어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019854.1.12",
          "form": "저도 오늘 빡챠서ㅎㅎㅎㅎㅎ",
          "original_form": "저도 오늘 빡챠서ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019854.1.13",
          "form": "뭐암튼 그런사람도 있고 저런사람도 있으니 이해해야죠ㅎ",
          "original_form": "뭐암튼 그런사람도 있고 저런사람도 있으니 이해해야죠ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019854.1.14",
          "form": "조금 뭔가 어울리는거 잘못하시는거 같아요",
          "original_form": "조금 뭔가 어울리는거 잘못하시는거 같아요",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019854.1.15",
          "form": "아무래도 그럴것같애요ㅎ",
          "original_form": "아무래도 그럴것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019854.1.16",
          "form": "뭔가.. 눈치가없다고해야하나ㅎ",
          "original_form": "뭔가.. 눈치가없다고해야하나ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019854.1.17",
          "form": "그래서 저도 그냥 그러려니 하네요 ㅎㅎ",
          "original_form": "그래서 저도 그냥 그러려니 하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019854.1.18",
          "form": "역시 노가리는 뒷담화에요ㅎㅎ",
          "original_form": "역시 노가리는 뒷담화에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019854.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019854.1.20",
          "form": "우리가 공통적인게 커뮤니티 활동이니깐ㅎㅎ",
          "original_form": "우리가 공통적인게 커뮤니티 활동이니깐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019854.1.21",
          "form": "그렇긴 하쥬~",
          "original_form": "그렇긴 하쥬~",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019854.1.22",
          "form": "어제는 와.. 정말 피곤해서 들어올수가없었음ㅎ",
          "original_form": "어제는 와.. 정말 피곤해서 들어올수가없었음ㅎ",
