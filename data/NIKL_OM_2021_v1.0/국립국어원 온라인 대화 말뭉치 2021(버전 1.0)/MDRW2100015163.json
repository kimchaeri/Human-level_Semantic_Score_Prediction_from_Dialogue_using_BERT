      "utterance": [
        {
          "id": "MDRW2100015163.1.1",
          "form": "ㅎㅎ 항상 신중하게ㅎㅎ",
          "original_form": "ㅎㅎ 항상 신중하게ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015163.1.2",
          "form": "어떤 일이든지요ㅎㅎ",
          "original_form": "어떤 일이든지요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015163.1.3",
          "form": "그럼 지금여자친구분은 어떻게 만나셨어요?",
          "original_form": "그럼 지금여자친구분은 어떻게 만나셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015163.1.4",
          "form": "소개팅으로요",
          "original_form": "소개팅으로요",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015163.1.5",
          "form": "소개팅 질리도록 하다가",
          "original_form": "소개팅 질리도록 하다가",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015163.1.6",
          "form": "마지막이다 하고 나갔는데",
          "original_form": "마지막이다 하고 나갔는데",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015163.1.7",
          "form": "지금 여친이었어요 ㅋㅋ",
          "original_form": "지금 여친이었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015163.1.8",
          "form": "오호ㅎㅎㅎ",
          "original_form": "오호ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015163.1.9",
          "form": "엄청 좋았겟네요 ㅎㅎ",
          "original_form": "엄청 좋았겟네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015163.1.10",
          "form": "첫눈에 반하셨어요?",
          "original_form": "첫눈에 반하셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015163.1.11",
          "form": "네 먼저 사진보고 나갔는데",
          "original_form": "네 먼저 사진보고 나갔는데",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015163.1.12",
          "form": "기대를 안하고 나갔어서 그런가",
          "original_form": "기대를 안하고 나갔어서 그런가",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015163.1.13",
          "form": "좋았어요 ㅋㅋㅋ",
          "original_form": "좋았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015163.1.14",
          "form": "ㅎㅎ 첫눈에 반해서 고백은 어떻게 하셨어요?",
          "original_form": "ㅎㅎ 첫눈에 반해서 고백은 어떻게 하셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015163.1.15",
          "form": "3번째 만나고 여친이 먼저 고백해서",
          "original_form": "3번째 만나고 여친이 먼저 고백해서",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015163.1.16",
          "form": "김이 팍 샜죠 ㅋㅋㅋ",
          "original_form": "김이 팍 샜죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015163.1.17",
          "form": "ㅎㅎㅎㅎㅎㅎㅍㅍㅍㅍ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅍㅍㅍㅍ",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015163.1.18",
          "form": "능력자시네여ㅎㅎ",
          "original_form": "능력자시네여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015163.1.19",
          "form": "여자한테 고백받는게 잘생긴사람아니면 안되는데ㅎㅎㅎ",
          "original_form": "여자한테 고백받는게 잘생긴사람아니면 안되는데ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015163.1.20",
          "form": "아니에요 ㅋㅋㅋ",
          "original_form": "아니에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015163.1.21",
          "form": "제가 고백을 안해서",
          "original_form": "제가 고백을 안해서",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015163.1.22",
          "form": "기다리다가 그냥 먼저 했다고",
          "original_form": "기다리다가 그냥 먼저 했다고",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015163.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015163.1.24",
          "form": "ㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015163.1.25",
          "form": "3년은 그럼 우결찍으신거에요?",
          "original_form": "3년은 그럼 우결찍으신거에요?",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015163.1.26",
          "form": "ㅋㅋㅋ 우결 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 우결 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015163.1.27",
          "form": "오랜만에 들어보는",
          "original_form": "오랜만에 들어보는",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015163.1.28",
          "form": "ㅎㅎ 나이가 들어서 그런가봅니당ㅎㅎ",
          "original_form": "ㅎㅎ 나이가 들어서 그런가봅니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015163.1.29",
          "form": "그냥 아는사이엿던거죠?",
          "original_form": "그냥 아는사이엿던거죠?",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015163.1.30",
          "form": "ㅎㅎ 네",
          "original_form": "ㅎㅎ 네",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015163.1.31",
          "form": "이제 나이가 들긴 했네요",
          "original_form": "이제 나이가 들긴 했네요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015163.1.32",
          "form": "근데 첫눈에 반했다고 하지 않으셨어요?ㅎㅎ",
          "original_form": "근데 첫눈에 반했다고 하지 않으셨어요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015163.1.33",
          "form": "근데 왜 우결을?ㅎㅎㅎㅎㅎㅎ",
          "original_form": "근데 왜 우결을?ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015163.1.34",
          "form": "신중에 신중을",
          "original_form": "신중에 신중을",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015163.1.35",
          "form": "연애가 조심스럽기도 하고",
          "original_form": "연애가 조심스럽기도 하고",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015163.1.36",
          "form": "그전에 만났던 사람하고",
          "original_form": "그전에 만났던 사람하고",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015163.1.37",
          "form": "결혼준비중에 헤어져서",
          "original_form": "결혼준비중에 헤어져서",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015163.1.38",
          "form": "더 그랬던거 같네요 ㅎㅎ",
          "original_form": "더 그랬던거 같네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015163.1.39",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015163.1.40",
          "form": "한번의 아품이 있으시군요 ㅍ",
          "original_form": "한번의 아품이 있으시군요 ㅍ",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015163.1.41",
          "form": "결혼준비중이면.. ㅠㅠ",
          "original_form": "결혼준비중이면.. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015163.1.42",
          "form": "뭐때문에?",
          "original_form": "뭐때문에?",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015163.1.43",
          "form": "양가 반대로인해",
          "original_form": "양가 반대로인해",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015163.1.44",
          "form": "아아ㅍㅍ",
          "original_form": "아아ㅍㅍ",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015163.1.45",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015163.1.46",
          "form": "무리하게 진행하다가",
          "original_form": "무리하게 진행하다가",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015163.1.47",
          "form": "힘드셨겠네요ㅠ",
          "original_form": "힘드셨겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015163.1.48",
          "form": "서로 축복받는 결혼을 못할거 같고 너무 힘들어서",
          "original_form": "서로 축복받는 결혼을 못할거 같고 너무 힘들어서",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015163.1.49",
          "form": "헤어졌죠",
          "original_form": "헤어졌죠",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015163.1.50",
          "form": "에고ㅠ",
          "original_form": "에고ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015163.1.51",
          "form": "반대이유가 있었을꺼아니에요?",
          "original_form": "반대이유가 있었을꺼아니에요?",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015163.1.52",
          "form": "평생 부모님 안보고살순 없으니",
          "original_form": "평생 부모님 안보고살순 없으니",
