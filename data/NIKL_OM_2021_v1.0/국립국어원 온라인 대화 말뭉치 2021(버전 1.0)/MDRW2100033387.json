      "utterance": [
        {
          "id": "MDRW2100033387.1.1",
          "form": "할머니 근로장려금 신청해야 되지?",
          "original_form": "할머니 근로장려금 신청해야 되지?",
          "speaker_id": "1",
          "time": "20210907 00:40"
        },
        {
          "id": "MDRW2100033387.1.2",
          "form": "근로장려금 신청해야지",
          "original_form": "근로장려금 신청해야지",
          "speaker_id": "2",
          "time": "20210907 01:20"
        },
        {
          "id": "MDRW2100033387.1.3",
          "form": "내일이나 모레 할 수 있나?",
          "original_form": "내일이나 모레 할 수 있나?",
          "speaker_id": "1",
          "time": "20210907 01:50"
        },
        {
          "id": "MDRW2100033387.1.4",
          "form": "안되면 주말에 하던지",
          "original_form": "안되면 주말에 하던지",
          "speaker_id": "2",
          "time": "20210907 01:51"
        },
        {
          "id": "MDRW2100033387.1.5",
          "form": "신청 어떻게 하더라?",
          "original_form": "신청 어떻게 하더라?",
          "speaker_id": "1",
          "time": "20210907 02:02"
        },
        {
          "id": "MDRW2100033387.1.6",
          "form": "세무서 안내장에 전화로 하라고 나왔지",
          "original_form": "세무서 안내장에 전화로 하라고 나왔지",
          "speaker_id": "2",
          "time": "20210907 02:09"
        },
        {
          "id": "MDRW2100033387.1.7",
          "form": "아아 전화로 할수있구니",
          "original_form": "아아 전화로 할수있구니",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100033387.1.8",
          "form": "간편하네!",
          "original_form": "간편하네!",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100033387.1.9",
          "form": "그럼 요즘 편리해졌지",
          "original_form": "그럼 요즘 편리해졌지",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100033387.1.10",
          "form": "오 진짜?",
          "original_form": "오 진짜?",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100033387.1.11",
          "form": "할머니 내일 꼭 근로 장려금 신청 해드려야껬어",
          "original_form": "할머니 내일 꼭 근로 장려금 신청 해드려야껬어",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100033387.1.12",
          "form": "그러면 좋아 하시겠지",
          "original_form": "그러면 좋아 하시겠지",
          "speaker_id": "2",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100033387.1.13",
          "form": "웅웅 근로 장려금 할머니 받는거 잘쓰시고 좋은듯",
          "original_form": "웅웅 근로 장려금 할머니 받는거 잘쓰시고 좋은듯",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100033387.1.14",
          "form": "지금도 노인 근로를 하시니깐 근로장려금도 나오는거지",
          "original_form": "지금도 노인 근로를 하시니깐 근로장려금도 나오는거지",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100033387.1.15",
          "form": "웅웅 요긴하게 쓰시는것 같아서 좋다구",
          "original_form": "웅웅 요긴하게 쓰시는것 같아서 좋다구",
          "speaker_id": "1",
