      "utterance": [
        {
          "id": "MDRW2100009385.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 술좋아하시나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 술좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009385.1.2",
          "form": "곰표맥주 생각나신거죠? ㅋㅋㅋ",
          "original_form": "곰표맥주 생각나신거죠? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009385.1.3",
          "form": "저는 소주파입니다",
          "original_form": "저는 소주파입니다",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009385.1.4",
          "form": "두꺼비요 ㅎㅎ",
          "original_form": "두꺼비요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009385.1.5",
          "form": "ㅋㅋㅋ 빨강거요?",
          "original_form": "ㅋㅋㅋ 빨강거요?",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009385.1.6",
          "form": "술 안드시죠?",
          "original_form": "술 안드시죠?",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009385.1.7",
          "form": "파란애예요 ..",
          "original_form": "파란애예요 ..",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009385.1.8",
          "form": "저 술마시면 중독자처럼.. ㅋㅋㅋ \r\n술은 절 타락시켜요..",
          "original_form": "저 술마시면 중독자처럼.. ㅋㅋㅋ \r\n술은 절 타락시켜요..",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009385.1.9",
          "form": "안마시면 한두달도 금주하고.. \r\n일단 손대면 몇날며칭..",
          "original_form": "안마시면 한두달도 금주하고.. \r\n일단 손대면 몇날며칭..",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009385.1.10",
          "form": "주량이?",
          "original_form": "주량이?",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009385.1.11",
          "form": "전 소주 2병이요",
          "original_form": "전 소주 2병이요",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009385.1.12",
          "form": "주량이?",
          "original_form": "주량이?",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009385.1.13",
          "form": "헐.. 저도 퍼런거는 2병..",
          "original_form": "헐.. 저도 퍼런거는 2병..",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009385.1.14",
          "form": "엄청 타락하셨네요..",
          "original_form": "엄청 타락하셨네요..",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009385.1.15",
          "form": "ㅋㅋㅋㅋ타락이라뇨,ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ타락이라뇨,ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009385.1.16",
          "form": "천마표 시멘트 팝콘 드셔보심?",
          "original_form": "천마표 시멘트 팝콘 드셔보심?",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009385.1.17",
          "form": "아 네",
          "original_form": "아 네",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009385.1.18",
          "form": "맛있던데용?",
          "original_form": "맛있던데용?",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009385.1.19",
          "form": "요즘 콜라보시대죠 ㅋㅋㅋㅋ",
          "original_form": "요즘 콜라보시대죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009385.1.20",
          "form": "이상한거 많던데...\r\n매직음료수랑",
          "original_form": "이상한거 많던데...\r\n매직음료수랑",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009385.1.21",
          "form": "맥직 같은 음료수랑.. \r\n음.. 매직이랑..",
          "original_form": "맥직 같은 음료수랑.. \r\n음.. 매직이랑..",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009385.1.22",
          "form": "딱히 안떠오르네요..",
          "original_form": "딱히 안떠오르네요..",
