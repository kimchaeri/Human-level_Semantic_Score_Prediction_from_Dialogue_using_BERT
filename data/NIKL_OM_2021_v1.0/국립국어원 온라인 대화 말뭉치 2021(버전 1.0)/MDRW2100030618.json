      "utterance": [
        {
          "id": "MDRW2100030618.1.1",
          "form": "배경이 너무 멋있어요 남녀 주인공 배우들도 멋지고",
          "original_form": "배경이 너무 멋있어요 남녀 주인공 배우들도 멋지고",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100030618.1.2",
          "form": "그춍 오래된 영화인데 지금 봐도 참 잘 만들었어요",
          "original_form": "그춍 오래된 영화인데 지금 봐도 참 잘 만들었어요",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100030618.1.3",
          "form": "제작비가 정말 많이 든 티가 나요",
          "original_form": "제작비가 정말 많이 든 티가 나요",
          "speaker_id": "2",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100030618.1.4",
          "form": "그쵸 리들리스콧이 자본 왕창 끌어와서 큰 맘먹고 만들었어요",
          "original_form": "그쵸 리들리스콧이 자본 왕창 끌어와서 큰 맘먹고 만들었어요",
          "speaker_id": "1",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100030618.1.5",
          "form": "시작했네요",
          "original_form": "시작했네요",
          "speaker_id": "2",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100030618.1.6",
          "form": "아 여기서부터군요",
          "original_form": "아 여기서부터군요",
          "speaker_id": "1",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100030618.1.7",
          "form": "군대가 어마어마하네요",
          "original_form": "군대가 어마어마하네요",
          "speaker_id": "2",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100030618.1.8",
          "form": "에드워드 노든 등장!!!",
          "original_form": "에드워드 노든 등장!!!",
          "speaker_id": "1",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030618.1.9",
          "form": "십자군들 규모도 어마어마하네요",
          "original_form": "십자군들 규모도 어마어마하네요",
          "speaker_id": "2",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030618.1.10",
          "form": "이거 감독판으로 봐야 더 좋은데 이건 편집본인듯해오",
          "original_form": "이거 감독판으로 봐야 더 좋은데 이건 편집본인듯해오",
          "speaker_id": "1",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100030618.1.11",
          "form": "아 디렉터스컷이 따로 있군요",
          "original_form": "아 디렉터스컷이 따로 있군요",
          "speaker_id": "2",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100030618.1.12",
          "form": "네 더 자세하고 감정선이 세심해요",
          "original_form": "네 더 자세하고 감정선이 세심해요",
          "speaker_id": "1",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100030618.1.13",
          "form": "기독교군과 이슬람군의 두 수장이 협상하네요",
          "original_form": "기독교군과 이슬람군의 두 수장이 협상하네요",
          "speaker_id": "2",
          "time": "20210904 10:32"
        }
