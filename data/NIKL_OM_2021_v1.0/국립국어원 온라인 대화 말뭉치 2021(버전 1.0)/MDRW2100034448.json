      "utterance": [
        {
          "id": "MDRW2100034448.1.1",
          "form": "여기도 광고 하고 있다",
          "original_form": "여기도 광고 하고 있다",
          "speaker_id": "2",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100034448.1.2",
          "form": "맨날 틀면 광고다ㅋ",
          "original_form": "맨날 틀면 광고다ㅋ",
          "speaker_id": "1",
          "time": "20210828 11:35"
        },
        {
          "id": "MDRW2100034448.1.3",
          "form": "그러게 광고도 얼마나 길게 하는지",
          "original_form": "그러게 광고도 얼마나 길게 하는지",
          "speaker_id": "2",
          "time": "20210828 11:35"
        },
        {
          "id": "MDRW2100034448.1.4",
          "form": "15 분이다 다른데 봐라",
          "original_form": "15 분이다 다른데 봐라",
          "speaker_id": "1",
          "time": "20210828 11:35"
        },
        {
          "id": "MDRW2100034448.1.5",
          "form": "ㅋㅋ언제 시작하는지 편성표보니 이십세기 힛트송 다음 또 차달남이다",
          "original_form": "ㅋㅋ언제 시작하는지 편성표보니 이십세기 힛트송 다음 또 차달남이다",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100034448.1.6",
          "form": "ㅋㅋ 되돌이표 편성",
          "original_form": "ㅋㅋ 되돌이표 편성",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100034448.1.7",
          "form": "어 그래 지금 방탄 롯데 제과 광고 보니 생각난다",
          "original_form": "어 그래 지금 방탄 롯데 제과 광고 보니 생각난다",
          "speaker_id": "2",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100034448.1.8",
          "form": "롯데제과 광고도 하나?",
          "original_form": "롯데제과 광고도 하나?",
          "speaker_id": "1",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100034448.1.9",
          "form": "빌보드차트 팬이 조작 의심된다고 어제인가 뉴스 자막으로 본듯",
          "original_form": "빌보드차트 팬이 조작 의심된다고 어제인가 뉴스 자막으로 본듯",
          "speaker_id": "2",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100034448.1.10",
          "form": "에이 설마",
          "original_form": "에이 설마",
          "speaker_id": "1",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100034448.1.11",
          "form": "팬이 많아서 그럴수도 있나",
          "original_form": "팬이 많아서 그럴수도 있나",
          "speaker_id": "1",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100034448.1.12",
          "form": "자막으로 본거라 나 확실하게 무슨 내용인지 모르겟다",
          "original_form": "자막으로 본거라 나 확실하게 무슨 내용인지 모르겟다",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100034448.1.13",
          "form": "나도 보 지를 못했네",
          "original_form": "나도 보 지를 못했네",
          "speaker_id": "1",
          "time": "20210828 11:39"
        },
        {
          "id": "MDRW2100034448.1.14",
          "form": "봤어야 했는데 무슨말인지 갑자기 궁금해진다",
          "original_form": "봤어야 했는데 무슨말인지 갑자기 궁금해진다",
          "speaker_id": "2",
          "time": "20210828 11:40"
        }
      ]
