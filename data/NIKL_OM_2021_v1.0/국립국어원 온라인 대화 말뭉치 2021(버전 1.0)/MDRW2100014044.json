          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100014044.1.4",
          "form": "말도 안돼",
          "original_form": "말도 안돼",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100014044.1.5",
          "form": "실화야",
          "original_form": "실화야",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014044.1.6",
          "form": "진짜 시간 왜이렇게 빨라",
          "original_form": "진짜 시간 왜이렇게 빨라",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014044.1.7",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014044.1.8",
          "form": "올림픽 봤어?",
          "original_form": "올림픽 봤어?",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014044.1.9",
          "form": "응 배구랑 양궁 위주로",
          "original_form": "응 배구랑 양궁 위주로",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014044.1.10",
          "form": "나도 펜싱도 좀 봤어",
          "original_form": "나도 펜싱도 좀 봤어",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014044.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014044.1.12",
          "form": "여자 배구 진짜 재밌지 않았어?",
          "original_form": "여자 배구 진짜 재밌지 않았어?",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014044.1.13",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014044.1.14",
          "form": "나 생방송으로 다 봤어",
          "original_form": "나 생방송으로 다 봤어",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014044.1.15",
          "form": "나는 하이라이트 위주로ㅠ",
          "original_form": "나는 하이라이트 위주로ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014044.1.16",
          "form": "그래도 재밌었어",
          "original_form": "그래도 재밌었어",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014044.1.17",
          "form": "다들 너무 잘하더라",
          "original_form": "다들 너무 잘하더라",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014044.1.18",
          "form": "김연경 선수 너무 멋있었어",
          "original_form": "김연경 선수 너무 멋있었어",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014044.1.19",
          "form": "맞아ㅠㅠㅠ",
          "original_form": "맞아ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014044.1.20",
          "form": "이번에 보고 완전 팬됐어",
          "original_form": "이번에 보고 완전 팬됐어",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014044.1.21",
          "form": "나도 다음에 직관 같이가자",
          "original_form": "나도 다음에 직관 같이가자",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014044.1.22",
          "form": "너무 좋아",
          "original_form": "너무 좋아",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014044.1.23",
          "form": "직접 보고 싶어ㅜㅜ",
          "original_form": "직접 보고 싶어ㅜㅜ",
