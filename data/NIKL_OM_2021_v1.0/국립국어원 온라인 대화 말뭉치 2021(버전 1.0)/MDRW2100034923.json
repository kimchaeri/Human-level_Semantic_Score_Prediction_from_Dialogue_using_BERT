        },
        {
          "id": "MDRW2100034923.1.19",
          "form": "오 ........... 저는 둘다 못먹어요",
          "original_form": "오 ........... 저는 둘다 못먹어요",
          "speaker_id": "1",
          "time": "20210828 22:28"
