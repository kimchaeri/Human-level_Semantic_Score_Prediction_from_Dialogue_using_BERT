      "utterance": [
        {
          "id": "MDRW2100022916.1.1",
          "form": "대단하시네요~",
          "original_form": "대단하시네요~",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022916.1.2",
          "form": "드라마 추천 드리자면",
          "original_form": "드라마 추천 드리자면",
          "speaker_id": "1",
          "time": "20210829 19:08"
        },
        {
          "id": "MDRW2100022916.1.3",
          "form": "카이로스 이거 재밌게 봤어요!",
          "original_form": "카이로스 이거 재밌게 봤어요!",
          "speaker_id": "1",
          "time": "20210829 19:08"
        },
        {
          "id": "MDRW2100022916.1.4",
          "form": "아 재밌을거같았어요!",
          "original_form": "아 재밌을거같았어요!",
          "speaker_id": "2",
          "time": "20210829 19:08"
        },
        {
          "id": "MDRW2100022916.1.5",
          "form": "신성록배우님 나오는거였나요?",
          "original_form": "신성록배우님 나오는거였나요?",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022916.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022916.1.7",
          "form": "기본설정 혹시 아시나용?",
          "original_form": "기본설정 혹시 아시나용?",
          "speaker_id": "1",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022916.1.8",
          "form": "어떤 기본설정이용?",
          "original_form": "어떤 기본설정이용?",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100022916.1.9",
          "form": "기본 줄거리요!",
          "original_form": "기본 줄거리요!",
          "speaker_id": "1",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100022916.1.10",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100022916.1.11",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100022916.1.12",
          "form": "이세영 신성록 둘이 쌍방구원 서사 스토리에요ㅋㅋㅋ",
          "original_form": "이세영 신성록 둘이 쌍방구원 서사 스토리에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022916.1.13",
          "form": "시간이 한달간격으로 다른 세상에 살아요",
          "original_form": "시간이 한달간격으로 다른 세상에 살아요",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022916.1.14",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022916.1.15",
          "form": "시그널생각나네요ㅋㅋㅋ",
          "original_form": "시그널생각나네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022916.1.16",
          "form": "약간 그런 장르에요ㅎㅎ",
          "original_form": "약간 그런 장르에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022916.1.17",
          "form": "오 왓챠에있네요",
          "original_form": "오 왓챠에있네요",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022916.1.18",
          "form": "봐보겠습니다 감사해요!",
          "original_form": "봐보겠습니다 감사해요!",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022916.1.19",
          "form": "전 최근에 언더커버 재밌게봤어요",
          "original_form": "전 최근에 언더커버 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022916.1.20",
          "form": "지진희랑 김현주 배우 나오는거죵?ㅎㅎ",
          "original_form": "지진희랑 김현주 배우 나오는거죵?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022916.1.21",
          "form": "이거 재밌다고 하더라구요",
          "original_form": "이거 재밌다고 하더라구요",
          "speaker_id": "1",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022916.1.22",
          "form": "네맞아요!",
          "original_form": "네맞아요!",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022916.1.23",
          "form": "다들 연기잘하고 재밌더라구요ㅋㅋ",
          "original_form": "다들 연기잘하고 재밌더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022916.1.24",
          "form": "지진희 배우가 액션을 잘한다고 하던데",
          "original_form": "지진희 배우가 액션을 잘한다고 하던데",
          "speaker_id": "1",
          "time": "20210829 19:15"
        }
      ]
    }
