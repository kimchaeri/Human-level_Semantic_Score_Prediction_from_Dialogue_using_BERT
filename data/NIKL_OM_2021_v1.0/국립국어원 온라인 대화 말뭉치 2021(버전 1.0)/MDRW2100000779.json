      "utterance": [
        {
          "id": "MDRW2100000779.1.1",
          "form": "우리 신도림에서 본거 뭐였지?",
          "original_form": "우리 신도림에서 본거 뭐였지?",
          "speaker_id": "2",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000779.1.2",
          "form": "뮤지컬 말 하는거야?",
          "original_form": "뮤지컬 말 하는거야?",
          "speaker_id": "1",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000779.1.3",
          "form": "응 그 정세운나온거",
          "original_form": "응 그 정세운나온거",
          "speaker_id": "2",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000779.1.4",
          "form": "그거 제목이 뭐였더라 ᆢ",
          "original_form": "그거 제목이 뭐였더라 ᆢ",
          "speaker_id": "1",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000779.1.5",
          "form": "아 생각났다",
          "original_form": "아 생각났다",
          "speaker_id": "2",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000779.1.6",
          "form": "뭐였는데?",
          "original_form": "뭐였는데?",
          "speaker_id": "1",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000779.1.7",
          "form": "그리스",
          "original_form": "그리스",
          "speaker_id": "2",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000779.1.8",
          "form": "그리스 별루였어",
          "original_form": "그리스 별루였어",
          "speaker_id": "1",
          "time": "20210819 19:19"
        },
        {
          "id": "MDRW2100000779.1.9",
          "form": "서브가 더잘하긴했음",
          "original_form": "서브가 더잘하긴했음",
          "speaker_id": "2",
          "time": "20210819 19:19"
        },
        {
          "id": "MDRW2100000779.1.10",
          "form": "ㅇㅇ 남주가 정세운인디 서브가 더 나음",
          "original_form": "ㅇㅇ 남주가 정세운인디 서브가 더 나음",
          "speaker_id": "1",
          "time": "20210819 19:20"
        },
        {
          "id": "MDRW2100000779.1.11",
          "form": "그때는 프듀때문에 인기있었지",
          "original_form": "그때는 프듀때문에 인기있었지",
          "speaker_id": "2",
          "time": "20210819 19:20"
        },
        {
          "id": "MDRW2100000779.1.12",
          "form": "다른날거 볼걸 그랬어",
          "original_form": "다른날거 볼걸 그랬어",
          "speaker_id": "1",
          "time": "20210819 19:20"
        },
        {
          "id": "MDRW2100000779.1.13",
          "form": "벤허 본데는 어디였지?",
          "original_form": "벤허 본데는 어디였지?",
