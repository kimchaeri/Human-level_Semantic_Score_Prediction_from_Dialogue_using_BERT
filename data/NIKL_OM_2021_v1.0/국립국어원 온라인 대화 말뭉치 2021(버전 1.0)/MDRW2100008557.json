        },
        {
          "id": "MDRW2100008557.1.2",
          "form": "아뇨 요즘은 별로 안 해요",
          "original_form": "아뇨 요즘은 별로 안 해요",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008557.1.3",
          "form": "저는 당근",
          "original_form": "저는 당근",
          "speaker_id": "1",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008557.1.4",
          "form": "거래 많이 하는 편이신가요?",
          "original_form": "거래 많이 하는 편이신가요?",
          "speaker_id": "2",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008557.1.5",
          "form": "가끔씩",
          "original_form": "가끔씩",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008557.1.6",
          "form": "마지막 거래 물품은 뭐였어요?",
          "original_form": "마지막 거래 물품은 뭐였어요?",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008557.1.7",
          "form": "공기청정깅",
          "original_form": "공기청정깅",
          "speaker_id": "1",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008557.1.8",
          "form": "오 공기청정기",
          "original_form": "오 공기청정기",
          "speaker_id": "2",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008557.1.9",
          "form": "판매하신 건가요?",
          "original_form": "판매하신 건가요?",
          "speaker_id": "2",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008557.1.10",
          "form": "네 판매요",
          "original_form": "네 판매요",
          "speaker_id": "1",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008557.1.11",
          "form": "쏠쏠했겠네요",
          "original_form": "쏠쏠했겠네요",
