      "utterance": [
        {
          "id": "MDRW2100045932.1.1",
          "form": "로드샵은 11거리가 젤 싸더리고요",
          "original_form": "로드샵은 11거리가 젤 싸더리고요",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045932.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045932.1.3",
          "form": "화장품은 역시 11거리!",
          "original_form": "화장품은 역시 11거리!",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045932.1.4",
          "form": "화장품에 큰돈들이기가좀",
          "original_form": "화장품에 큰돈들이기가좀",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.5",
          "form": "차라리",
          "original_form": "차라리",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.6",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.7",
          "form": "피부과가는게ㅜ낫다고해서",
          "original_form": "피부과가는게ㅜ낫다고해서",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.8",
          "form": "어차피 다 흡수 안 돼요",
          "original_form": "어차피 다 흡수 안 돼요",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.9",
          "form": "마자영",
          "original_form": "마자영",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.10",
          "form": "피부는 흡수기관이 아니니까",
          "original_form": "피부는 흡수기관이 아니니까",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.11",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.12",
          "form": "차라리 레이저가 낫죠",
          "original_form": "차라리 레이저가 낫죠",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.13",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.14",
          "form": "핫딜러들은 현명하군요",
          "original_form": "핫딜러들은 현명하군요",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.15",
          "form": "운동하거나",
          "original_form": "운동하거나",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.17",
          "form": "생각 비슷해서 넘 웃겨요",
          "original_form": "생각 비슷해서 넘 웃겨요",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.19",
          "form": "바디로션도 걍 세타필...",
          "original_form": "바디로션도 걍 세타필...",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.20",
          "form": "운동은안하지만차라리 운동이낫죠",
          "original_form": "운동은안하지만차라리 운동이낫죠",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045932.1.21",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045932.1.22",
          "form": "하지는 않지만ㅋㅋㅋ",
