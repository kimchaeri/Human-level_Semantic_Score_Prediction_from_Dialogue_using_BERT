      "utterance": [
        {
          "id": "MDRW2100029323.1.1",
          "form": "아니면 나도 유럽가고 싶어",
          "original_form": "아니면 나도 유럽가고 싶어",
          "speaker_id": "2",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029323.1.2",
          "form": "넌 갔다왔잖아",
          "original_form": "넌 갔다왔잖아",
          "speaker_id": "2",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029323.1.3",
          "form": "유럽 좋지 볼거 많고",
          "original_form": "유럽 좋지 볼거 많고",
          "speaker_id": "1",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029323.1.4",
          "form": "그럼 코로나 끝나면 유럽가자",
          "original_form": "그럼 코로나 끝나면 유럽가자",
          "speaker_id": "2",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029323.1.5",
          "form": "근데 유럽은 음식이 엄마 아빠 입맛에 안맞아서 개고생할걸",
          "original_form": "근데 유럽은 음식이 엄마 아빠 입맛에 안맞아서 개고생할걸",
          "speaker_id": "1",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029323.1.6",
          "form": "나 피자좋아해",
          "original_form": "나 피자좋아해",
          "speaker_id": "2",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029323.1.7",
          "form": "피자가 문제가 아냐",
          "original_form": "피자가 문제가 아냐",
          "speaker_id": "1",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029323.1.8",
          "form": "그럼 뭐가 문제야",
          "original_form": "그럼 뭐가 문제야",
          "speaker_id": "2",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029323.1.9",
          "form": "그냥 다 문제야",
          "original_form": "그냥 다 문제야",
          "speaker_id": "1",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029323.1.10",
          "form": "물도 석회수고 음식도 짜고 느끼하고",
          "original_form": "물도 석회수고 음식도 짜고 느끼하고",
          "speaker_id": "1",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029323.1.11",
          "form": "며칠있으면 한식먹고 싶어져",
          "original_form": "며칠있으면 한식먹고 싶어져",
          "speaker_id": "1",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029323.1.12",
          "form": "한식주는 패키지로 가야겠네",
          "original_form": "한식주는 패키지로 가야겠네",
          "speaker_id": "2",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029323.1.13",
          "form": "그러면 좀 괜찮을지도",
          "original_form": "그러면 좀 괜찮을지도",
          "speaker_id": "1",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029323.1.14",
          "form": "무조건 한식있어야돼",
          "original_form": "무조건 한식있어야돼",
          "speaker_id": "1",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029323.1.15",
          "form": "그리고 나도 패키지가 편하지",
          "original_form": "그리고 나도 패키지가 편하지",
          "speaker_id": "1",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029323.1.16",
          "form": "왜 엄마아빠 수발들기 힘들어서???",
          "original_form": "왜 엄마아빠 수발들기 힘들어서???",
          "speaker_id": "2",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029323.1.17",
          "form": "응...내 친구 부모님 모시고 태국갔다가 엄청 싸웠대",
          "original_form": "응...내 친구 부모님 모시고 태국갔다가 엄청 싸웠대",
          "speaker_id": "1",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029323.1.18",
          "form": "엄마는 안까다로워",
          "original_form": "엄마는 안까다로워",
          "speaker_id": "2",
          "time": "20210903 01:36"
        }
      ]
