      "utterance": [
        {
          "id": "MDRW2100028248.1.1",
          "form": "너 가을옷 샀니?",
          "original_form": "너 가을옷 샀니?",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028248.1.2",
          "form": "아니 아직 안샀어. 넌 샀어??",
          "original_form": "아니 아직 안샀어. 넌 샀어??",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028248.1.3",
          "form": "나도 아직",
          "original_form": "나도 아직",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028248.1.4",
          "form": "가을은 넘 짧아서",
          "original_form": "가을은 넘 짧아서",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028248.1.5",
          "form": "안살거야",
          "original_form": "안살거야",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028248.1.6",
          "form": "그럼 뭐입고 다닐거야",
          "original_form": "그럼 뭐입고 다닐거야",
          "speaker_id": "2",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100028248.1.7",
          "form": "그냥 작년 가을에 입던 옷",
          "original_form": "그냥 작년 가을에 입던 옷",
          "speaker_id": "1",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100028248.1.8",
          "form": "난 가을옷이 없어 가디건만 살래",
          "original_form": "난 가을옷이 없어 가디건만 살래",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028248.1.9",
          "form": "나도 가디건 살래",
          "original_form": "나도 가디건 살래",
          "speaker_id": "1",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028248.1.10",
          "form": "그래 이번주 쌀쌀해지지 않았니",
          "original_form": "그래 이번주 쌀쌀해지지 않았니",
          "speaker_id": "2",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028248.1.11",
          "form": "반팔 입고 나갔다가 감기걸릴뻔",
          "original_form": "반팔 입고 나갔다가 감기걸릴뻔",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028248.1.12",
          "form": "감기조심해 열나면 병원도 못가지않니",
          "original_form": "감기조심해 열나면 병원도 못가지않니",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028248.1.13",
          "form": "맞아 요즘 날씨가 흐려서 몸이 쳐지지 않니?",
          "original_form": "맞아 요즘 날씨가 흐려서 몸이 쳐지지 않니?",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100028248.1.14",
          "form": "항상 쳐져있어 올웨이즈",
          "original_form": "항상 쳐져있어 올웨이즈",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028248.1.15",
          "form": "비염때문에 돈다",
          "original_form": "비염때문에 돈다",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028248.1.16",
          "form": "나도 추워지면서 비염 도짐",
          "original_form": "나도 추워지면서 비염 도짐",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028248.1.17",
          "form": "너무 싫다 비염 .. 가을만 되면 귀신같아",
          "original_form": "너무 싫다 비염 .. 가을만 되면 귀신같아",
          "speaker_id": "2",
          "time": "20210902 20:24"
        }
      ]
