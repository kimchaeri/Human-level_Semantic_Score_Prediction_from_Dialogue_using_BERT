      "utterance": [
        {
          "id": "MDRW2100023762.1.1",
          "form": "남자 머리랑 다르게?",
          "original_form": "남자 머리랑 다르게?",
          "speaker_id": "2",
          "time": "20210830 16:53"
        },
        {
          "id": "MDRW2100023762.1.2",
          "form": "그치 남자는 막 엄청 길르는 경우는 잘 없잖아",
          "original_form": "그치 남자는 막 엄청 길르는 경우는 잘 없잖아",
          "speaker_id": "1",
          "time": "20210830 16:53"
        },
        {
          "id": "MDRW2100023762.1.3",
          "form": "귀신도 아니고",
          "original_form": "귀신도 아니고",
          "speaker_id": "2",
          "time": "20210830 16:53"
        },
        {
          "id": "MDRW2100023762.1.4",
          "form": "진짜 귀신 되지 락커처럼 머리를 관리할 거 아니면",
          "original_form": "진짜 귀신 되지 락커처럼 머리를 관리할 거 아니면",
          "speaker_id": "1",
          "time": "20210830 16:54"
        },
        {
          "id": "MDRW2100023762.1.5",
          "form": "난 예전엔 파마가 좋았는데 요즘엔 염색이 더 하고싶더라",
          "original_form": "난 예전엔 파마가 좋았는데 요즘엔 염색이 더 하고싶더라",
          "speaker_id": "2",
          "time": "20210830 16:54"
        },
        {
          "id": "MDRW2100023762.1.6",
          "form": "나는 사실 긴 머리에 웨이브 넣는 게 젤 예쁜데 관리가 안돼",
          "original_form": "나는 사실 긴 머리에 웨이브 넣는 게 젤 예쁜데 관리가 안돼",
          "speaker_id": "1",
          "time": "20210830 16:55"
        },
        {
          "id": "MDRW2100023762.1.7",
          "form": "관리가 많이 힘들어?",
          "original_form": "관리가 많이 힘들어?",
          "speaker_id": "2",
          "time": "20210830 16:55"
        },
        {
          "id": "MDRW2100023762.1.8",
          "form": "응 약간 빛났다 사라져",
          "original_form": "응 약간 빛났다 사라져",
          "speaker_id": "1",
          "time": "20210830 16:55"
        },
        {
          "id": "MDRW2100023762.1.9",
          "form": "ㅋㅋㅋㅋㅋ 그림자야?",
          "original_form": "ㅋㅋㅋㅋㅋ 그림자야?",
          "speaker_id": "2",
          "time": "20210830 16:56"
        },
        {
          "id": "MDRW2100023762.1.10",
          "form": "그러니까 말이야 염색도 붉은색 계열은 금방 물이 빠져",
          "original_form": "그러니까 말이야 염색도 붉은색 계열은 금방 물이 빠져",
          "speaker_id": "1",
          "time": "20210830 16:56"
        },
        {
          "id": "MDRW2100023762.1.11",
          "form": "잘 안빠지는 머리색도 있을거 아냐",
          "original_form": "잘 안빠지는 머리색도 있을거 아냐",
          "speaker_id": "2",
          "time": "20210830 16:56"
        },
        {
          "id": "MDRW2100023762.1.12",
          "form": "예쁜 색은 잘 빠져 누런색 이런 건 안 빠져",
          "original_form": "예쁜 색은 잘 빠져 누런색 이런 건 안 빠져",
          "speaker_id": "1",
          "time": "20210830 16:57"
        },
        {
          "id": "MDRW2100023762.1.13",
          "form": "아 그게 단점이네",
          "original_form": "아 그게 단점이네",
          "speaker_id": "2",
          "time": "20210830 16:57"
        }
      ]
