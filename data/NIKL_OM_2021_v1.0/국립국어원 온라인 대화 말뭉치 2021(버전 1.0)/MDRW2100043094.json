      "utterance": [
        {
          "id": "MDRW2100043094.1.1",
          "form": "오늘 점심 뭐드셨나요",
          "original_form": "오늘 점심 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100043094.1.2",
          "form": "제육볶음 먹었어요",
          "original_form": "제육볶음 먹었어요",
          "speaker_id": "1",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043094.1.3",
          "form": "우앙 직접 해드신거에여?",
          "original_form": "우앙 직접 해드신거에여?",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043094.1.4",
          "form": "아뇨ㅋㅋ 자주가는 식당에서요",
          "original_form": "아뇨ㅋㅋ 자주가는 식당에서요",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100043094.1.5",
          "form": "백반에 제육볶음 해주시더라구요",
          "original_form": "백반에 제육볶음 해주시더라구요",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100043094.1.6",
          "form": "우와 좋네요",
          "original_form": "우와 좋네요",
          "speaker_id": "2",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043094.1.7",
          "form": "후식도 드셨나여",
          "original_form": "후식도 드셨나여",
          "speaker_id": "2",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043094.1.8",
          "form": "저는 파바에서 라떼",
          "original_form": "저는 파바에서 라떼",
          "speaker_id": "2",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043094.1.9",
          "form": "전 근처 카페에서 아이스 아메리카노 먹었어요",
          "original_form": "전 근처 카페에서 아이스 아메리카노 먹었어요",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043094.1.10",
          "form": "오오 그러셨구나",
          "original_form": "오오 그러셨구나",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043094.1.11",
          "form": "과자도 몇개 사먹고요ㅋㅋㅋ",
          "original_form": "과자도 몇개 사먹고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043094.1.12",
          "form": "오후에 간식 드세요?",
          "original_form": "오후에 간식 드세요?",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043094.1.13",
          "form": "글쎄요. 네다섯시쯤 배고파지면 찾을거 같긴해요",
          "original_form": "글쎄요. 네다섯시쯤 배고파지면 찾을거 같긴해요",
          "speaker_id": "1",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043094.1.14",
          "form": "아하 저도요ㅠㅠ 오후에는 또 봉사활동 가서",
          "original_form": "아하 저도요ㅠㅠ 오후에는 또 봉사활동 가서",
          "speaker_id": "2",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043094.1.15",
          "form": "말 마니하면 배고파 질거같아요",
          "original_form": "말 마니하면 배고파 질거같아요",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100043094.1.16",
          "form": "간식먹으면 금방 저녁시간이라ㅎ 항상 고민이에여ㅋㅋㅋ",
          "original_form": "간식먹으면 금방 저녁시간이라ㅎ 항상 고민이에여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043094.1.17",
          "form": "맞아요 ㅋㅋㅋ 하루가 왜케 빠르죠",
          "original_form": "맞아요 ㅋㅋㅋ 하루가 왜케 빠르죠",
          "speaker_id": "2",
          "time": "20210907 14:57"
        }
      ]
