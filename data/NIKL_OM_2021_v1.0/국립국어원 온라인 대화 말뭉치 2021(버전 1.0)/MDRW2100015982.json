        {
          "id": "MDRW2100015982.1.1",
          "form": "동아리해봤어?",
          "original_form": "동아리해봤어?",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100015982.1.2",
          "form": "해봤지",
          "original_form": "해봤지",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100015982.1.3",
          "form": "뭐 해봤어?",
          "original_form": "뭐 해봤어?",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100015982.1.4",
          "form": "천체동아리를 해봤어",
          "original_form": "천체동아리를 해봤어",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100015982.1.5",
          "form": "학교에 천체망원경이 있어?",
          "original_form": "학교에 천체망원경이 있어?",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100015982.1.6",
          "form": "응 신기하지?",
          "original_form": "응 신기하지?",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100015982.1.7",
          "form": "특이하긴하다 낭만적인걸",
          "original_form": "특이하긴하다 낭만적인걸",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100015982.1.8",
          "form": "그래서 유성이 내리는 날에 다같이 밤을 샜어",
          "original_form": "그래서 유성이 내리는 날에 다같이 밤을 샜어",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100015982.1.9",
          "form": "함께 별 내리는걸 본거야?",
          "original_form": "함께 별 내리는걸 본거야?",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100015982.1.10",
          "form": "응 좋은 추억이었어",
          "original_form": "응 좋은 추억이었어",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100015982.1.11",
          "form": "또 뭐해봤어?",
          "original_form": "또 뭐해봤어?",
          "speaker_id": "2",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100015982.1.12",
          "form": "영어동아리를했어",
          "original_form": "영어동아리를했어",
          "speaker_id": "1",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100015982.1.13",
          "form": "나도 영어토론동아리를 해봤어",
          "original_form": "나도 영어토론동아리를 해봤어",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100015982.1.14",
          "form": "오 나도! 똑같네",
          "original_form": "오 나도! 똑같네",
