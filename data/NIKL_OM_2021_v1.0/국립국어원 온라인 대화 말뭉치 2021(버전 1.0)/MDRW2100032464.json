      "utterance": [
        {
          "id": "MDRW2100032464.1.1",
          "form": "요즘에 달걀 값이 너무 비싸",
          "original_form": "요즘에 달걀 값이 너무 비싸",
          "speaker_id": "2",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100032464.1.2",
          "form": "그러게 몇 달 째 내릴 생각을 안하네",
          "original_form": "그러게 몇 달 째 내릴 생각을 안하네",
          "speaker_id": "1",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100032464.1.3",
          "form": "한 판에 8천원은 너무하잖아",
          "original_form": "한 판에 8천원은 너무하잖아",
          "speaker_id": "2",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100032464.1.4",
          "form": "석유 값이 올랐나?",
          "original_form": "석유 값이 올랐나?",
          "speaker_id": "1",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100032464.1.5",
          "form": "거의 두 배야",
          "original_form": "거의 두 배야",
          "speaker_id": "2",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100032464.1.6",
          "form": "곧 명절이라 더 안내리나",
          "original_form": "곧 명절이라 더 안내리나",
          "speaker_id": "1",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100032464.1.7",
          "form": "달걀이 요리에 써먹기 좋은데",
          "original_form": "달걀이 요리에 써먹기 좋은데",
          "speaker_id": "2",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100032464.1.8",
          "form": "달걀 요리 많지",
          "original_form": "달걀 요리 많지",
          "speaker_id": "1",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100032464.1.9",
          "form": "요즘엔 아껴 먹어야해 ㅋㅋㅋㅋㅋ",
          "original_form": "요즘엔 아껴 먹어야해 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100032464.1.10",
          "form": "ㅋㅋㅋㅋ 집에 있는 걸로 먹어서 안 산지 좀 됐네",
          "original_form": "ㅋㅋㅋㅋ 집에 있는 걸로 먹어서 안 산지 좀 됐네",
          "speaker_id": "1",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100032464.1.11",
          "form": "명절 음식에 달걀 엄청 들어갈텐데",
          "original_form": "명절 음식에 달걀 엄청 들어갈텐데",
          "speaker_id": "2",
          "time": "20210905 18:38"
        },
        {
          "id": "MDRW2100032464.1.12",
          "form": "그러게 사야하나",
          "original_form": "그러게 사야하나",
          "speaker_id": "1",
          "time": "20210905 18:38"
        },
        {
          "id": "MDRW2100032464.1.13",
          "form": "달걀은 정말 좋은 식재료인 듯 ㅋㅋㅋㅋ",
          "original_form": "달걀은 정말 좋은 식재료인 듯 ㅋㅋㅋㅋ",
          "speaker_id": "2",
