          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040346.1.13",
          "form": "더 현 대 닷 컴 인 가",
          "original_form": "더 현 대 닷 컴 인 가",
          "speaker_id": "1",
          "time": "20210904 16:57"
        },
