      "utterance": [
        {
          "id": "MDRW2100040346.1.1",
          "form": "아니여 안늙었어 미용 패션",
          "original_form": "아니여 안늙었어 미용 패션",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100040346.1.2",
          "form": "목도리가왔는데!!!!!!",
          "original_form": "목도리가왔는데!!!!!!",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100040346.1.3",
          "form": "어어 드디어 왓니",
          "original_form": "어어 드디어 왓니",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100040346.1.4",
          "form": "색은진짜마음에드는데",
          "original_form": "색은진짜마음에드는데",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100040346.1.5",
          "form": "나도 구두온것같은데 집을 못가네",
          "original_form": "나도 구두온것같은데 집을 못가네",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100040346.1.6",
          "form": "저번에 두르고다니덧 그거보다 재질은별로당",
          "original_form": "저번에 두르고다니덧 그거보다 재질은별로당",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100040346.1.7",
          "form": "아쉽당",
          "original_form": "아쉽당",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100040346.1.8",
          "form": "아핫 구렇니 그친구에게도 애정을 줘봐",
          "original_form": "아핫 구렇니 그친구에게도 애정을 줘봐",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100040346.1.9",
          "form": "애정ㅋㅋ알또..",
          "original_form": "애정ㅋㅋ알또..",
          "speaker_id": "1",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040346.1.10",
          "form": "아이쁘당",
          "original_form": "아이쁘당",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100040346.1.11",
          "form": "재질이 별로균 ㅠㅠ에이치몰 에서 샀어,",
          "original_form": "재질이 별로균 ㅠㅠ에이치몰 에서 샀어,",
