      "utterance": [
        {
          "id": "MDRW2100010370.1.1",
          "form": "너 그거 들었어?",
          "original_form": "너 그거 들었어?",
          "speaker_id": "2",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010370.1.2",
          "form": "아 정말?",
          "original_form": "아 정말?",
          "speaker_id": "2",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010370.1.3",
          "form": "혹시 담 걸리는 이유를 알고있니?",
          "original_form": "혹시 담 걸리는 이유를 알고있니?",
          "speaker_id": "1",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010370.1.4",
          "form": "내친구도 목에 담 왂다던데",
          "original_form": "내친구도 목에 담 왂다던데",
          "speaker_id": "2",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010370.1.5",
          "form": "그 구씨있잖아~",
          "original_form": "그 구씨있잖아~",
          "speaker_id": "2",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010370.1.6",
          "form": "그건 잘 모르겠어",
          "original_form": "그건 잘 모르겠어",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.7",
          "form": "ㅠㅠ나랑 동지구나",
          "original_form": "ㅠㅠ나랑 동지구나",
          "speaker_id": "1",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.8",
          "form": "정말 아프겠두나",
          "original_form": "정말 아프겠두나",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.9",
          "form": "그럼 푸는법은 알고있니?",
          "original_form": "그럼 푸는법은 알고있니?",
          "speaker_id": "1",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.10",
          "form": "그것도 잘 모르겠네.",
          "original_form": "그것도 잘 모르겠네.",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.11",
          "form": "담 이렇게 본격적으로 걸려본거 처음이야",
          "original_form": "담 이렇게 본격적으로 걸려본거 처음이야",
          "speaker_id": "1",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.12",
          "form": "너는 담걸린적 없니?",
          "original_form": "너는 담걸린적 없니?",
          "speaker_id": "1",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.13",
          "form": "내친구는 휴대전화를 사용하다가 그랬댜",
          "original_form": "내친구는 휴대전화를 사용하다가 그랬댜",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.14",
          "form": "너는 왜그랬니?",
          "original_form": "너는 왜그랬니?",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.15",
          "form": "아는게 없구나 담에 대해서는",
          "original_form": "아는게 없구나 담에 대해서는",
          "speaker_id": "1",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.16",
          "form": "나는 운동하다가 그랬어",
          "original_form": "나는 운동하다가 그랬어",
          "speaker_id": "1",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.17",
          "form": "나는 있는데 지금은 잘 모르겠어",
          "original_form": "나는 있는데 지금은 잘 모르겠어",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.18",
          "form": "응..담이랑 안친해",
          "original_form": "응..담이랑 안친해",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.19",
          "form": "아 정말?",
          "original_form": "아 정말?",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010370.1.20",
          "form": "응 아무래도 나는 헬스를 좋아하니까 ㅠㅠ",
          "original_form": "응 아무래도 나는 헬스를 좋아하니까 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 17:16"
        }
      ]
