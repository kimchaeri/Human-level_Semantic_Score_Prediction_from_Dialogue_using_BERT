      "utterance": [
        {
          "id": "MDRW2100035427.1.1",
          "form": "방금 아이스 루프 탑 그린 러어억셔리 라떼먹었습니다",
          "original_form": "방금 아이스 루프 탑 그린 러어억셔리 라떼먹었습니다",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100035427.1.2",
          "form": "네이밍참 외국사나봐 다들",
          "original_form": "네이밍참 외국사나봐 다들",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100035427.1.3",
          "form": "럭셔리까지..ㅋㅋㅋ 고오급진거 드셨네요",
          "original_form": "럭셔리까지..ㅋㅋㅋ 고오급진거 드셨네요",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100035427.1.4",
          "form": "새로나온 신메뉴도 사실 그냥 쿨피스 맛이었는데 이름이 뭐더라",
          "original_form": "새로나온 신메뉴도 사실 그냥 쿨피스 맛이었는데 이름이 뭐더라",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100035427.1.5",
          "form": "ㅋㅋㅋ이 와플은 캬라멜 루프탑 굽스 와플로하죠",
          "original_form": "ㅋㅋㅋ이 와플은 캬라멜 루프탑 굽스 와플로하죠",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100035427.1.6",
          "form": "웃긴다 증맬 ㅋ",
          "original_form": "웃긴다 증맬 ㅋ",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100035427.1.7",
          "form": "아 그건 양호하네요 파인애플 블렌디드",
          "original_form": "아 그건 양호하네요 파인애플 블렌디드",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100035427.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100035427.1.9",
          "form": "스타벅스 슬래머",
          "original_form": "스타벅스 슬래머",
          "speaker_id": "1",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100035427.1.10",
          "form": "이거는 뭔지..",
          "original_form": "이거는 뭔지..",
          "speaker_id": "1",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100035427.1.11",
          "form": "아니 왤케길지",
          "original_form": "아니 왤케길지",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100035427.1.12",
          "form": "러시아어 뺨치겠네요",
          "original_form": "러시아어 뺨치겠네요",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100035427.1.13",
          "form": "러시아어에요??",
          "original_form": "러시아어에요??",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100035427.1.14",
          "form": "한국사람은 이름 석자가 익숙하다구용",
          "original_form": "한국사람은 이름 석자가 익숙하다구용",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100035427.1.15",
          "form": "아니요 러시아어는 아주 길어서 자막이 힘들정도래서 비유해봤어요",
          "original_form": "아니요 러시아어는 아주 길어서 자막이 힘들정도래서 비유해봤어요",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100035427.1.16",
          "form": "아 맞네요 ㅋㅋ 뺨치는 ㅋㅋ",
          "original_form": "아 맞네요 ㅋㅋ 뺨치는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100035427.1.17",
          "form": "스벅에 제친구가 왜 잘안가는지 알거같네요ㅋㅋ",
          "original_form": "스벅에 제친구가 왜 잘안가는지 알거같네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100035427.1.18",
          "form": "환경보호한담소 텀블러는 왜자꾸만들어내는지원 ㅋㅋ",
          "original_form": "환경보호한담소 텀블러는 왜자꾸만들어내는지원 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100035427.1.19",
          "form": "이제 따신거 신상나오겠네요",
          "original_form": "이제 따신거 신상나오겠네요",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100035427.1.20",
          "form": "맞아요 ㅎㅎ 사실 따지고 보면 흑임자줄때 그것도 플라스틱..ㅋㅋ",
          "original_form": "맞아요 ㅎㅎ 사실 따지고 보면 흑임자줄때 그것도 플라스틱..ㅋㅋ",
