      "utterance": [
        {
          "id": "MDRW2100001909.1.1",
          "form": "최근에 구매한 물건이 있나요",
          "original_form": "최근에 구매한 물건이 있나요",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001909.1.2",
          "form": "그것은 샤오미 공기 청정기입니다",
          "original_form": "그것은 샤오미 공기 청정기입니다",
          "speaker_id": "1",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001909.1.3",
          "form": "어때요? 궁금해요",
          "original_form": "어때요? 궁금해요",
          "speaker_id": "2",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001909.1.4",
          "form": "공청기 가격이 있어서 망설여지는데 샤오미는 저렴한쪽에 속히니까",
          "original_form": "공청기 가격이 있어서 망설여지는데 샤오미는 저렴한쪽에 속히니까",
          "speaker_id": "2",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001909.1.5",
          "form": "속하니까",
          "original_form": "속하니까",
          "speaker_id": "2",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001909.1.6",
          "form": "근데 문제가 제 방이 공기가 깨끗해요",
          "original_form": "근데 문제가 제 방이 공기가 깨끗해요",
          "speaker_id": "1",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001909.1.7",
          "form": "처음부터 깨끗했어요 창문이 없어서",
          "original_form": "처음부터 깨끗했어요 창문이 없어서",
          "speaker_id": "1",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001909.1.8",
          "form": "아닌가... 얘가 인식을 안하나??",
          "original_form": "아닌가... 얘가 인식을 안하나??",
          "speaker_id": "1",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001909.1.9",
          "form": "아 지하실이라고 하셨지",
          "original_form": "아 지하실이라고 하셨지",
          "speaker_id": "2",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001909.1.10",
          "form": "전기세같은건 많이 나오나요",
          "original_form": "전기세같은건 많이 나오나요",
          "speaker_id": "2",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001909.1.11",
          "form": "공청기 쓰면요 많이 나온다고들 해서..",
          "original_form": "공청기 쓰면요 많이 나온다고들 해서..",
          "speaker_id": "2",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001909.1.12",
          "form": "작업실 형태라 물세, 전기세 다 집주인이 내줘요",
          "original_form": "작업실 형태라 물세, 전기세 다 집주인이 내줘요",
          "speaker_id": "1",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001909.1.13",
          "form": "그럼 월세만 내고사세요?",
          "original_form": "그럼 월세만 내고사세요?",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001909.1.14",
          "form": "네 그렇습니다",
          "original_form": "네 그렇습니다",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001909.1.15",
          "form": "그래서 전기 많이 먹는거 막 사요",
          "original_form": "그래서 전기 많이 먹는거 막 사요",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001909.1.16",
          "form": "아.. 그럼 요즘 위시리스트있어요?",
          "original_form": "아.. 그럼 요즘 위시리스트있어요?",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001909.1.17",
          "form": "저 안마의자...",
          "original_form": "저 안마의자...",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001909.1.18",
          "form": "안마의자..",
          "original_form": "안마의자..",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001909.1.19",
          "form": "너무 비싸죠..",
          "original_form": "너무 비싸죠..",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001909.1.20",
          "form": "효도템..",
          "original_form": "효도템..",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001909.1.21",
          "form": "여행을 가도 안마의자가 있는 곳을 선호합니다",
          "original_form": "여행을 가도 안마의자가 있는 곳을 선호합니다",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001909.1.22",
          "form": "저번에 간곳은 와 의자가 진공 상태가 되는데",
          "original_form": "저번에 간곳은 와 의자가 진공 상태가 되는데",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001909.1.23",
          "form": "예술이었죠",
          "original_form": "예술이었죠",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001909.1.24",
          "form": "오.. 그러시군요",
          "original_form": "오.. 그러시군요",
          "speaker_id": "2",
          "time": "20210819 22:16"
        }
      ]
