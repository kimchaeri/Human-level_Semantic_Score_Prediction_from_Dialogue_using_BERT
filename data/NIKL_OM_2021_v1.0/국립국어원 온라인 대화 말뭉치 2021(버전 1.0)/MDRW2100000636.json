      "utterance": [
        {
          "id": "MDRW2100000636.1.1",
          "form": "가장 좋아하는 계절이 언제세용~?",
          "original_form": "가장 좋아하는 계절이 언제세용~?",
          "speaker_id": "1",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000636.1.2",
          "form": "가을이 쵝오인듯 합니다 ㅎ",
          "original_form": "가을이 쵝오인듯 합니다 ㅎ",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000636.1.3",
          "form": "어느 계절 좋아하시는지 사계절이 있다는 게 좋다고 생각하시는지??",
          "original_form": "어느 계절 좋아하시는지 사계절이 있다는 게 좋다고 생각하시는지??",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000636.1.4",
          "form": "저는 봄을 가장 좋아해요~",
          "original_form": "저는 봄을 가장 좋아해요~",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000636.1.5",
          "form": "꽃을 좋아하는데 가장 좋아하는 벚꽃이 봄에피니 늘 기다려지더라구요~",
          "original_form": "꽃을 좋아하는데 가장 좋아하는 벚꽃이 봄에피니 늘 기다려지더라구요~",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000636.1.6",
          "form": "네 ",
          "original_form": "네 ",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000636.1.7",
          "form": "봄도 너무 좋지요",
          "original_form": "봄도 너무 좋지요",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000636.1.8",
          "form": "겨울도 좋아하시는지요 저는 추위를 많이 타서 겨울이 유난히 힘든 사람입니다",
          "original_form": "겨울도 좋아하시는지요 저는 추위를 많이 타서 겨울이 유난히 힘든 사람입니다",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000636.1.9",
          "form": "저도 추위를 많이 타서 겨울이 좀 힘든것같아요",
          "original_form": "저도 추위를 많이 타서 겨울이 좀 힘든것같아요",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000636.1.10",
          "form": "오히려 여름 겨울하면 여름이 좋아요~",
          "original_form": "오히려 여름 겨울하면 여름이 좋아요~",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000636.1.11",
          "form": "앗 저도요",
          "original_form": "앗 저도요",
          "speaker_id": "2",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000636.1.12",
          "form": "에어컨은 없어도 되지만, 난방은 빵빵하게",
          "original_form": "에어컨은 없어도 되지만, 난방은 빵빵하게",
          "speaker_id": "2",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000636.1.13",
          "form": "ㅎㅎ저도 전기장판쓰는데 겨울넘어서도 쓰는것같아요~",
          "original_form": "ㅎㅎ저도 전기장판쓰는데 겨울넘어서도 쓰는것같아요~",
          "speaker_id": "1",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100000636.1.14",
          "form": "추운거는 너무 싫어요ㅠ",
          "original_form": "추운거는 너무 싫어요ㅠ",
          "speaker_id": "1",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100000636.1.15",
          "form": "마르셨을듯",
          "original_form": "마르셨을듯",
          "speaker_id": "2",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100000636.1.16",
          "form": "지방이 없어서 겨울이 힘든 ^^;;",
          "original_form": "지방이 없어서 겨울이 힘든 ^^;;",
          "speaker_id": "2",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100000636.1.17",
          "form": "가을  좋은데 너무 짧고 금방 겨울이라 가을은 늘 아쉬워요",
          "original_form": "가을  좋은데 너무 짧고 금방 겨울이라 가을은 늘 아쉬워요",
          "speaker_id": "2",
          "time": "20210819 18:05"
        },
        {
          "id": "MDRW2100000636.1.18",
          "form": "가을 옷사는게 별 의미가 없더라구요~",
          "original_form": "가을 옷사는게 별 의미가 없더라구요~",
          "speaker_id": "1",
          "time": "20210819 18:05"
        },
        {
          "id": "MDRW2100000636.1.19",
          "form": "진짜 금방가는것같아요~",
          "original_form": "진짜 금방가는것같아요~",
          "speaker_id": "1",
          "time": "20210819 18:05"
        },
        {
          "id": "MDRW2100000636.1.20",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210819 18:28"
        },
        {
          "id": "MDRW2100000636.1.21",
          "form": "더더 자꾸자꾸 짧아지는듯",
          "original_form": "더더 자꾸자꾸 짧아지는듯",
          "speaker_id": "2",
          "time": "20210819 18:28"
        },
        {
          "id": "MDRW2100000636.1.22",
          "form": "더위는 많이 안타시나봐요~",
          "original_form": "더위는 많이 안타시나봐요~",
          "speaker_id": "1",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000636.1.23",
          "form": "더위는 그래도 참을수 있어서요 ㅎ",
          "original_form": "더위는 그래도 참을수 있어서요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000636.1.24",
          "form": "추우면  쪼그라드는 느낌이에요ㅋㅋ",
          "original_form": "추우면  쪼그라드는 느낌이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000636.1.25",
          "form": "겨울 다가오는게 무서울것같아요~!!",
          "original_form": "겨울 다가오는게 무서울것같아요~!!",
          "speaker_id": "1",
          "time": "20210819 20:18"
        },
        {
          "id": "MDRW2100000636.1.26",
          "form": "ㅋㅋㅋㅋ 네",
          "original_form": "ㅋㅋㅋㅋ 네",
          "speaker_id": "2",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000636.1.27",
          "form": "내복 필수!",
          "original_form": "내복 필수!",
          "speaker_id": "2",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000636.1.28",
          "form": "맞아용ㅋㅋ 내복하고 겨율에는 늘 패딩만 입는것같아여¥!!",
          "original_form": "맞아용ㅋㅋ 내복하고 겨율에는 늘 패딩만 입는것같아여{emoji:•}¥!!",
          "speaker_id": "1",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000636.1.29",
          "form": "입는거랑 안입는거랑 천지차이에요 ㅎ",
          "original_form": "입는거랑 안입는거랑 천지차이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000636.1.30",
          "form": "입으면 겨울, 안입으면 북극 ㅎ",
          "original_form": "입으면 겨울, 안입으면 북극 ㅎ",
          "speaker_id": "2",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000636.1.31",
