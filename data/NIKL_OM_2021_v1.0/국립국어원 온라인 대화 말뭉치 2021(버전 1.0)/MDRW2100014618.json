      "utterance": [
        {
          "id": "MDRW2100014618.1.1",
          "form": "혹시 판데믹 보셨나요?",
          "original_form": "혹시 판데믹 보셨나요?",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014618.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014618.1.3",
          "form": "들어봤는데",
          "original_form": "들어봤는데",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014618.1.4",
          "form": "왓챠?",
          "original_form": "왓챠?",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014618.1.5",
          "form": "인가요",
          "original_form": "인가요",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014618.1.6",
          "form": "저는 웨이브에서 오늘 위에 떴길래.. 1화 조금 봤는데",
          "original_form": "저는 웨이브에서 오늘 위에 떴길래.. 1화 조금 봤는데",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014618.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014618.1.8",
          "form": "코로나 같은... 느낌이 들더라구요",
          "original_form": "코로나 같은... 느낌이 들더라구요",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014618.1.9",
          "form": "그런거군요",
          "original_form": "그런거군요",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014618.1.10",
          "form": "요새 코로나라 그런가",
          "original_form": "요새 코로나라 그런가",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014618.1.11",
          "form": "그래서 계속 보고싶은데 자막인 관계로...\r\n더이상 못보고 있어요..",
          "original_form": "그래서 계속 보고싶은데 자막인 관계로...\r\n더이상 못보고 있어요..",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014618.1.12",
          "form": "그런 시리즈 나오나봐요",
          "original_form": "그런 시리즈 나오나봐요",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014618.1.13",
          "form": "자막안돼죠 ㅡㅡ",
          "original_form": "자막안돼죠 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014618.1.14",
          "form": "그런거 같애요..",
          "original_form": "그런거 같애요..",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014618.1.15",
          "form": "ㅋㅋㅋㅋㅋ\r\n감기도 얼마전에 영화 다시 봤었는데",
          "original_form": "ㅋㅋㅋㅋㅋ\r\n감기도 얼마전에 영화 다시 봤었는데",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014618.1.16",
          "form": "아 이럴때 더빙 그리워요",
          "original_form": "아 이럴때 더빙 그리워요",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014618.1.17",
          "form": "스컬히 목소리 ㅋㅋ",
          "original_form": "스컬히 목소리 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014618.1.18",
          "form": "확실히 코로나 겪고 나서 보니깐 더 와닿네요",
          "original_form": "확실히 코로나 겪고 나서 보니깐 더 와닿네요",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014618.1.19",
          "form": "그럼요 느낌이 달라요",
          "original_form": "그럼요 느낌이 달라요",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014618.1.20",
          "form": "맞아요!!! ㅎㅎㅎ 예전 티비에서 더빙으로 많이 했었죠",
          "original_form": "맞아요!!! ㅎㅎㅎ 예전 티비에서 더빙으로 많이 했었죠",
          "speaker_id": "1",
