      "utterance": [
        {
          "id": "MDRW2100034533.1.1",
          "form": "그니까요  갑자기 너무 비싸졌어요ㅠ",
          "original_form": "그니까요  갑자기 너무 비싸졌어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034533.1.2",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034533.1.3",
          "form": "서민음식인데 ㅠㅠ",
          "original_form": "서민음식인데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034533.1.4",
          "form": "저 저런 비빔만두 스타일 너무 좋더라구요",
          "original_form": "저 저런 비빔만두 스타일 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034533.1.5",
          "form": "그니까요ㅠㅠ",
          "original_form": "그니까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034533.1.6",
          "form": "물론 양도  많아지긴했지만",
          "original_form": "물론 양도  많아지긴했지만",
          "speaker_id": "2",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100034533.1.7",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100034533.1.8",
          "form": "너무 비싸요ㅠ",
          "original_form": "너무 비싸요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100034533.1.9",
          "form": "혼자서는",
          "original_form": "혼자서는",
          "speaker_id": "1",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100034533.1.10",
          "form": "절대 다 못 먹어요",
          "original_form": "절대 다 못 먹어요",
          "speaker_id": "1",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100034533.1.11",
          "form": "그니까요 혼자서는 시키지도 못하겠더라구요",
          "original_form": "그니까요 혼자서는 시키지도 못하겠더라구요",
          "speaker_id": "2",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100034533.1.12",
          "form": "부담돼요",
          "original_form": "부담돼요",
          "speaker_id": "1",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100034533.1.13",
          "form": "그래서 양적은 신전을 전 자주시키게되더라구요",
          "original_form": "그래서 양적은 신전을 전 자주시키게되더라구요",
          "speaker_id": "1",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100034533.1.14",
          "form": "아맞아요 신전이 양이 딱 좋죠!",
          "original_form": "아맞아요 신전이 양이 딱 좋죠!",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100034533.1.15",
          "form": "사이드도 좋더라구요",
          "original_form": "사이드도 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100034533.1.16",
          "form": "거기 오뎅탕 좋아요 저는 ㅋㅋㅋ",
          "original_form": "거기 오뎅탕 좋아요 저는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100034533.1.17",
          "form": "오 어묵탕은 한번도 안먹어봤는데",
          "original_form": "오 어묵탕은 한번도 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100034533.1.18",
          "form": "맛있군요",
          "original_form": "맛있군요",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100034533.1.19",
          "form": "추천해요 ㅋㅋㅋ",
          "original_form": "추천해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100034533.1.20",
          "form": "네 ㅋㅋㅋ갠찬더라구요",
          "original_form": "네 ㅋㅋㅋ갠찬더라구요",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100034533.1.21",
          "form": "다음에 먹어봐야겠네요",
          "original_form": "다음에 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100034533.1.22",
          "form": "신전 로제는 어때요?",
          "original_form": "신전 로제는 어때요?",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100034533.1.23",
          "form": "신전은 배떡이랑 비슷한데",
          "original_form": "신전은 배떡이랑 비슷한데",
          "speaker_id": "1",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100034533.1.24",
          "form": "약간 더 달달해요!",
          "original_form": "약간 더 달달해요!",
          "speaker_id": "1",
          "time": "20210828 14:40"
        }
      ]
    }
