      "utterance": [
        {
          "id": "MDRW2100044577.1.1",
          "form": "아 점수가 낮음 탈락해?",
          "original_form": "아 점수가 낮음 탈락해?",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044577.1.2",
          "form": "그런가봐",
          "original_form": "그런가봐",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044577.1.3",
          "form": "나두 탈락은 몰랐는데",
          "original_form": "나두 탈락은 몰랐는데",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044577.1.4",
          "form": "아휴 지목당한 애들 인터뷰는 왜하는거야",
          "original_form": "아휴 지목당한 애들 인터뷰는 왜하는거야",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044577.1.5",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044577.1.6",
          "form": "어휴 지금 언니들틈에서 막 하는거 엄청 쫄릴거같은데",
          "original_form": "어휴 지금 언니들틈에서 막 하는거 엄청 쫄릴거같은데",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044577.1.7",
          "form": "리더들이라 자존심 싸움도 엄청나네",
          "original_form": "리더들이라 자존심 싸움도 엄청나네",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044577.1.8",
          "form": "리더들이라 리딩에 답답한 게 보이나봐ㅋㅋ",
          "original_form": "리더들이라 리딩에 답답한 게 보이나봐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044577.1.9",
          "form": "다들 못참아ㅋㅋ",
          "original_form": "다들 못참아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044577.1.10",
          "form": "리더인데 워스트댄서면..",
          "original_form": "리더인데 워스트댄서면..",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044577.1.11",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044577.1.12",
          "form": "내가 리더인데 팀에 감점을 가져온다?",
          "original_form": "내가 리더인데 팀에 감점을 가져온다?",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044577.1.13",
          "form": "헐랭",
          "original_form": "헐랭",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044577.1.14",
          "form": "리더는 리더네",
          "original_form": "리더는 리더네",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044577.1.15",
          "form": "전략이 필요해",
          "original_form": "전략이 필요해",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044577.1.16",
          "form": "멋있는데 탈락말고 끝까지 순위만 정하지",
          "original_form": "멋있는데 탈락말고 끝까지 순위만 정하지",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044577.1.17",
          "form": "바보 엠넷 ㅜ",
          "original_form": "바보 엠넷 ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044577.1.18",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044577.1.19",
          "form": "다 잘하는데",
          "original_form": "다 잘하는데",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044577.1.20",
          "form": "어케골라",
          "original_form": "어케골라",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044577.1.21",
          "form": "최대한 보여주지",
          "original_form": "최대한 보여주지",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044577.1.22",
          "form": "와 제일 잘한 사람이라고",
          "original_form": "와 제일 잘한 사람이라고",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044577.1.23",
          "form": "평가한 사람인데",
          "original_form": "평가한 사람인데",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044577.1.24",
          "form": "어우 무서워",
          "original_form": "어우 무서워",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044577.1.25",
          "form": "뭐야 또 붙이나?",
          "original_form": "뭐야 또 붙이나?",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044577.1.26",
          "form": "헐 배틀하네",
          "original_form": "헐 배틀하네",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044577.1.27",
          "form": "와 배틀",
          "original_form": "와 배틀",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044577.1.28",
          "form": "오늘 탈락은 아닌가봐",
          "original_form": "오늘 탈락은 아닌가봐",
          "speaker_id": "2",
          "time": "20210907 23:23"
        }
      ]
    }
