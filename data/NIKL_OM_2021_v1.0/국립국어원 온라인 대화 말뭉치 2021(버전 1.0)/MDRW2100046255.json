      "utterance": [
        {
          "id": "MDRW2100046255.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211017 11:19"
        },
        {
          "id": "MDRW2100046255.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211017 11:21"
        },
        {
          "id": "MDRW2100046255.1.3",
          "form": "엄청 춥죠",
          "original_form": "엄청 춥죠",
          "speaker_id": "2",
          "time": "20211017 11:22"
        },
        {
          "id": "MDRW2100046255.1.4",
          "form": "네 오늘 많이 추워졌네요",
          "original_form": "네 오늘 많이 추워졌네요",
          "speaker_id": "1",
          "time": "20211017 11:26"
        },
        {
          "id": "MDRW2100046255.1.5",
          "form": "환절기 비염 같은 건 없으신가요?",
          "original_form": "환절기 비염 같은 건 없으신가요?",
          "speaker_id": "2",
          "time": "20211017 11:28"
        },
        {
          "id": "MDRW2100046255.1.6",
          "form": "다행히 비염은 없어요...",
          "original_form": "다행히 비염은 없어요...",
          "speaker_id": "1",
          "time": "20211017 11:29"
        },
        {
          "id": "MDRW2100046255.1.7",
          "form": "와 축복받은 유전자인가봐요!",
          "original_form": "와 축복받은 유전자인가봐요!",
          "speaker_id": "2",
          "time": "20211017 11:29"
        },
        {
          "id": "MDRW2100046255.1.8",
          "form": "그렇죠..ㅎ\r\n주위에 지인 중에 비염이.있어 매번 고생하더라구요.\r\n어린이들은 비염 비율이.더 높은거 같구요..",
          "original_form": "그렇죠..ㅎ\r\n주위에 지인 중에 비염이.있어 매번 고생하더라구요.\r\n어린이들은 비염 비율이.더 높은거 같구요..",
          "speaker_id": "1",
          "time": "20211017 11:30"
        },
        {
          "id": "MDRW2100046255.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20211017 11:31"
        },
        {
          "id": "MDRW2100046255.1.10",
          "form": "전 학생 때",
          "original_form": "전 학생 때",
          "speaker_id": "2",
          "time": "20211017 11:31"
        },
        {
          "id": "MDRW2100046255.1.11",
          "form": "공해 때문인지...",
          "original_form": "공해 때문인지...",
          "speaker_id": "1",
          "time": "20211017 11:31"
        },
        {
          "id": "MDRW2100046255.1.12",
          "form": "며칠에 두루마리 한통씩 쓰곤했어요",
          "original_form": "며칠에 두루마리 한통씩 쓰곤했어요",
          "speaker_id": "2",
          "time": "20211017 11:31"
        },
        {
          "id": "MDRW2100046255.1.13",
          "form": "코는 맨날 헐어있고",
          "original_form": "코는 맨날 헐어있고",
          "speaker_id": "2",
          "time": "20211017 11:31"
        },
        {
          "id": "MDRW2100046255.1.14",
          "form": "에고... 고생이네요.\r\n치료가 안되던데...",
          "original_form": "에고... 고생이네요.\r\n치료가 안되던데...",
          "speaker_id": "1",
          "time": "20211017 11:31"
        },
        {
          "id": "MDRW2100046255.1.15",
          "form": "치료약이 나오면 좋겠어요..",
          "original_form": "치료약이 나오면 좋겠어요..",
          "speaker_id": "1",
          "time": "20211017 11:32"
        },
        {
          "id": "MDRW2100046255.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20211017 11:32"
        },
        {
          "id": "MDRW2100046255.1.17",
          "form": "공해라는 말씀도 와닿는게",
          "original_form": "공해라는 말씀도 와닿는게",
          "speaker_id": "2",
          "time": "20211017 11:32"
        },
        {
          "id": "MDRW2100046255.1.18",
          "form": "확실히 그러다 시골 살던 몇년은 엄청 좋아지더라고요",
          "original_form": "확실히 그러다 시골 살던 몇년은 엄청 좋아지더라고요",
          "speaker_id": "2",
          "time": "20211017 11:32"
        },
        {
          "id": "MDRW2100046255.1.19",
          "form": "저도 TV에서 그런 경우가 있다고 봤어요..",
          "original_form": "저도 TV에서 그런 경우가 있다고 봤어요..",
          "speaker_id": "1",
          "time": "20211017 11:32"
        },
        {
          "id": "MDRW2100046255.1.20",
          "form": "그니까요 ㅡ 혹시 자녀 키우시다 비염 도지면 꼭 시골 고려해보세요 !",
          "original_form": "그니까요 ㅡ 혹시 자녀 키우시다 비염 도지면 꼭 시골 고려해보세요 !",
          "speaker_id": "2",
          "time": "20211017 11:33"
        }
      ]
