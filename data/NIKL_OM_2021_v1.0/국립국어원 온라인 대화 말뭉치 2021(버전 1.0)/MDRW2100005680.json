      "utterance": [
        {
          "id": "MDRW2100005680.1.1",
          "form": "안녕요",
          "original_form": "안녕요",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005680.1.2",
          "form": "얍",
          "original_form": "얍",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005680.1.3",
          "form": "요즘",
          "original_form": "요즘",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005680.1.4",
          "form": "무슨책보세요",
          "original_form": "무슨책보세요",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005680.1.5",
          "form": "요즘은 소설책만 보고 있는데 잡지 사는건 좋아해",
          "original_form": "요즘은 소설책만 보고 있는데 잡지 사는건 좋아해",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005680.1.6",
          "form": "잡지 어떤거요??",
          "original_form": "잡지 어떤거요??",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005680.1.7",
          "form": "전 잡지는 안사봐서..",
          "original_form": "전 잡지는 안사봐서..",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005680.1.8",
          "form": "궁금",
          "original_form": "궁금",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005680.1.9",
          "form": "잡지의 진가는 사실 사은품",
          "original_form": "잡지의 진가는 사실 사은품",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005680.1.10",
          "form": "사은품을 보고 잡지를 고르는데",
          "original_form": "사은품을 보고 잡지를 고르는데",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005680.1.11",
          "form": "ㅇㅎㅇㅎ",
          "original_form": "ㅇㅎㅇㅎ",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005680.1.12",
          "form": "잡지 한번 사봐 은근 사는 재미가 있어",
          "original_form": "잡지 한번 사봐 은근 사는 재미가 있어",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005680.1.13",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005680.1.14",
          "form": "전 근데",
          "original_form": "전 근데",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005680.1.15",
          "form": "단점은 굳이 필요없는데 사은품이 좋아보여서 사게 된다는거?",
          "original_form": "단점은 굳이 필요없는데 사은품이 좋아보여서 사게 된다는거?",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005680.1.16",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.17",
          "form": "소설책사는걸로 빠듯해요 ㅠ",
          "original_form": "소설책사는걸로 빠듯해요 ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.19",
          "form": "이번에",
          "original_form": "이번에",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.20",
          "form": "달러구트 꿈백화점이라고",
          "original_form": "달러구트 꿈백화점이라고",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.21",
          "form": "베셀란 한동안 핫하게 했던 소설",
          "original_form": "베셀란 한동안 핫하게 했던 소설",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.22",
          "form": "처음 들어본다",
          "original_form": "처음 들어본다",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.23",
          "form": "2권이 나왔거든요",
          "original_form": "2권이 나왔거든요",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.24",
          "form": "어진짜요??",
          "original_form": "어진짜요??",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.25",
          "form": "순문학?",
          "original_form": "순문학?",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.26",
          "form": "저거 2권 지금 베스트셀러 1위예요",
          "original_form": "저거 2권 지금 베스트셀러 1위예요",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.27",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005680.1.28",
          "form": "저게 펀딩받고 나온 책인데",
          "original_form": "저게 펀딩받고 나온 책인데",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005680.1.29",
          "form": "순문학이라기엔 애매해요",
          "original_form": "순문학이라기엔 애매해요",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005680.1.30",
          "form": "판타지라서",
          "original_form": "판타지라서",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005680.1.31",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005680.1.32",
          "form": "근데 뭐...따지자면 그쪽 계열이긴함...",
          "original_form": "근데 뭐...따지자면 그쪽 계열이긴함...",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005680.1.33",
          "form": "요즘 재밌는 웹소설 머 없나요",
          "original_form": "요즘 재밌는 웹소설 머 없나요",
          "speaker_id": "2",
          "time": "20210822 13:54"
        }
      ]
    }
