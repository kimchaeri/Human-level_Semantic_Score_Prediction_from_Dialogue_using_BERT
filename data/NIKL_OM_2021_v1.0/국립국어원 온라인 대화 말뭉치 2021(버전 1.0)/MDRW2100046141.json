      "utterance": [
        {
          "id": "MDRW2100046141.1.1",
          "form": "드실때 몇조각까지 드세요",
          "original_form": "드실때 몇조각까지 드세요",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046141.1.2",
          "form": "이번 주에 캠핑을 가는데",
          "original_form": "이번 주에 캠핑을 가는데",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046141.1.3",
          "form": "저는 두 조각 먹어요",
          "original_form": "저는 두 조각 먹어요",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046141.1.4",
          "form": "많이 드시나요??",
          "original_form": "많이 드시나요??",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046141.1.5",
          "form": "오 캠핑!!",
          "original_form": "오 캠핑!!",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046141.1.6",
          "form": "저도 피자 두조각 먹어요 ㅠㅠ",
          "original_form": "저도 피자 두조각 먹어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046141.1.7",
          "form": "고기 드실건가요??",
          "original_form": "고기 드실건가요??",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046141.1.8",
          "form": "ㅎㅎㅎㅎㅎㅎㅇ 비슷하시네요",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅇ 비슷하시네요",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046141.1.9",
          "form": "고기도 굽고",
          "original_form": "고기도 굽고",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046141.1.10",
          "form": "새우도 사 가려구요",
          "original_form": "새우도 사 가려구요",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046141.1.11",
          "form": "지금 새우가 크고 맛있어서",
          "original_form": "지금 새우가 크고 맛있어서",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046141.1.12",
          "form": "우아 너무 좋을거같아요 ㅋㅋㅋ",
          "original_form": "우아 너무 좋을거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046141.1.13",
          "form": "오 새우 철인가요",
          "original_form": "오 새우 철인가요",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046141.1.14",
          "form": "캠핑갈때 필수예요 ㅎㅎ",
          "original_form": "캠핑갈때 필수예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046141.1.15",
          "form": "네 새우가 7월말부터 나오던데",
          "original_form": "네 새우가 7월말부터 나오던데",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046141.1.16",
          "form": "그때는 비싸고 크기도 작은데",
          "original_form": "그때는 비싸고 크기도 작은데",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046141.1.17",
          "form": "지금이 제일 크고 가격도 싸서",
          "original_form": "지금이 제일 크고 가격도 싸서",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046141.1.18",
          "form": "우아",
          "original_form": "우아",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046141.1.19",
          "form": "캠핑갈때 꼭 사가고 있어요",
          "original_form": "캠핑갈때 꼭 사가고 있어요",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046141.1.20",
          "form": "오오 새우 ㅋㅋㅋㅋ",
          "original_form": "오오 새우 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046141.1.21",
          "form": "구우면 너무 맛이ㅛ어요",
          "original_form": "구우면 너무 맛이ㅛ어요",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046141.1.22",
          "form": "네 ㅎㅎㅎㅎ 진짜 너무 맛있죠",
          "original_form": "네 ㅎㅎㅎㅎ 진짜 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046141.1.23",
          "form": "캠핑 가면 맛있는거 많이 먹겟어용",
          "original_form": "캠핑 가면 맛있는거 많이 먹겟어용",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046141.1.24",
          "form": "네 늘 비슷하게 사가는데",
          "original_form": "네 늘 비슷하게 사가는데",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046141.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046141.1.26",
          "form": "가서 먹으면 또 늘 맛있는거같아요 ㅎㅎ",
          "original_form": "가서 먹으면 또 늘 맛있는거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046141.1.27",
          "form": "제가 요리를 못해서",
          "original_form": "제가 요리를 못해서",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046141.1.28",
          "form": "매일 먹던것만 먹어서 ㅎㅎ",
          "original_form": "매일 먹던것만 먹어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046141.1.29",
          "form": "맞아요!!! 오 그럼 신랑분이 하시나용",
          "original_form": "맞아요!!! 오 그럼 신랑분이 하시나용",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046141.1.30",
          "form": "요리는 거의 신랑이 하는 편이에요",
          "original_form": "요리는 거의 신랑이 하는 편이에요",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046141.1.31",
          "form": "캠핑 가서는 다 신랑이 하구요",
          "original_form": "캠핑 가서는 다 신랑이 하구요",
          "speaker_id": "2",
          "time": "20210908 10:45"
        }
      ]
