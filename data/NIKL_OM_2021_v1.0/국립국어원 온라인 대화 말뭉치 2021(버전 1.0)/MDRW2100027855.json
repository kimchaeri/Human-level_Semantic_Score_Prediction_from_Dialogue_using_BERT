          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027855.1.6",
          "form": "웃다보니 내얘기",
          "original_form": "웃다보니 내얘기",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027855.1.7",
          "form": "웃긴데 우리얘기.....",
          "original_form": "웃긴데 우리얘기.....",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027855.1.8",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027855.1.9",
          "form": "교재 사게 하는 교수님도 싫다",
          "original_form": "교재 사게 하는 교수님도 싫다",
          "speaker_id": "1",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027855.1.10",
          "form": "ppt로 끝내주시라고요",
          "original_form": "ppt로 끝내주시라고요",
          "speaker_id": "1",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027855.1.11",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027855.1.12",
          "form": "우리는 강요는 안 하는데",
          "original_form": "우리는 강요는 안 하는데",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027855.1.13",
          "form": "있으면 좋아요/따로 적어주지는 않아요 해서",
          "original_form": "있으면 좋아요/따로 적어주지는 않아요 해서",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027855.1.14",
          "form": "걍 눈물을 머금고 구매함",
          "original_form": "걍 눈물을 머금고 구매함",
          "speaker_id": "2",
