      "utterance": [
        {
          "id": "MDRW2100037373.1.1",
          "form": "근데 제가 실제로 겪은 건",
          "original_form": "근데 제가 실제로 겪은 건",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100037373.1.2",
          "form": "네 어떤 일 겪으신 거예요?",
          "original_form": "네 어떤 일 겪으신 거예요?",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037373.1.3",
          "form": "밥 받으려고 줄 서는데",
          "original_form": "밥 받으려고 줄 서는데",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037373.1.4",
          "form": "제가 뒷걸음질 치다가",
          "original_form": "제가 뒷걸음질 치다가",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037373.1.5",
          "form": "누군가의 발을 밟은거예요",
          "original_form": "누군가의 발을 밟은거예요",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037373.1.6",
          "form": "뒤돌아보니까 명찰이 2학년이길래",
          "original_form": "뒤돌아보니까 명찰이 2학년이길래",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037373.1.7",
          "form": "아 죄송합니다 하면서 꾸벅 인사했는데",
          "original_form": "아 죄송합니다 하면서 꾸벅 인사했는데",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037373.1.8",
          "form": "알고보니 그 연예인이더라구요",
          "original_form": "알고보니 그 연예인이더라구요",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037373.1.9",
          "form": "인사 했는데 뭐라고 해요",
          "original_form": "인사 했는데 뭐라고 해요",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037373.1.10",
          "form": "그래서 속으로",
          "original_form": "그래서 속으로",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037373.1.11",
          "form": "아 그 연습생이네 생각했는데 표정이 안좋았어요",
          "original_form": "아 그 연습생이네 생각했는데 표정이 안좋았어요",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037373.1.12",
          "form": "그러고나서 밥 먹고나서",
          "original_form": "그러고나서 밥 먹고나서",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037373.1.13",
          "form": "연예인 못 알아 봐서 삐졌나 봐요",
          "original_form": "연예인 못 알아 봐서 삐졌나 봐요",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037373.1.14",
          "form": "컵 살균기에서 물 마시는데",
          "original_form": "컵 살균기에서 물 마시는데",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037373.1.15",
          "form": "앜ㅋㅋㅋ 그럴수도 있어요",
          "original_form": "앜ㅋㅋㅋ 그럴수도 있어요",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037373.1.16",
          "form": "뒤에서 누가 툭툭 치더니",
          "original_form": "뒤에서 누가 툭툭 치더니",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.17",
          "form": "그 연예인이",
          "original_form": "그 연예인이",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.18",
          "form": "\"너 아까 내 발 밟았는데 왜 사과 제대로 안해?\"",
          "original_form": "\"너 아까 내 발 밟았는데 왜 사과 제대로 안해?\"",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.19",
          "form": "이러는거예요",
          "original_form": "이러는거예요",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.20",
          "form": "근데 저게 진짜 사과를 바라는 느낌이 아니라",
          "original_form": "근데 저게 진짜 사과를 바라는 느낌이 아니라",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.21",
          "form": "네 그래서 뭐라고 하셨어요",
          "original_form": "네 그래서 뭐라고 하셨어요",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.22",
          "form": "그냥 양아치짓 하는 느낌이어서",
          "original_form": "그냥 양아치짓 하는 느낌이어서",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.23",
          "form": "무서워서",
          "original_form": "무서워서",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.24",
          "form": "아..죄송합니다 한 번 더 했어요",
          "original_form": "아..죄송합니다 한 번 더 했어요",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.25",
          "form": "그러고나서 동네 학원에서",
          "original_form": "그러고나서 동네 학원에서",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.26",
          "form": "친구랑 수업 끝나고 나왔는데",
          "original_form": "친구랑 수업 끝나고 나왔는데",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.27",
          "form": "나중에 또 만나셨 나 봐요",
          "original_form": "나중에 또 만나셨 나 봐요",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037373.1.28",
          "form": "걔가 이번에는",
          "original_form": "걔가 이번에는",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.29",
          "form": "오토바이를 타고 지나가는거예요",
          "original_form": "오토바이를 타고 지나가는거예요",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.30",
          "form": "근데 미성년자잖아요?!",
          "original_form": "근데 미성년자잖아요?!",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.31",
          "form": "그때 미성년자 맞죠",
          "original_form": "그때 미성년자 맞죠",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.32",
          "form": "당연히 얼굴 아는 사람이 오토바이 타고있으니까",
          "original_form": "당연히 얼굴 아는 사람이 오토바이 타고있으니까",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.33",
          "form": "시선이 가서 쳐다보니까",
          "original_form": "시선이 가서 쳐다보니까",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.34",
          "form": "자의식과잉인지 뭔지",
          "original_form": "자의식과잉인지 뭔지",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.35",
          "form": "굳~~~이 멈춰서",
          "original_form": "굳~~~이 멈춰서",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.36",
          "form": "우리를 째려보고 가는거예요 ㅋㅋㅋㅋㅋ",
          "original_form": "우리를 째려보고 가는거예요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.37",
          "form": "그래서 진짜 이미지가 너무 안좋았는데",
          "original_form": "그래서 진짜 이미지가 너무 안좋았는데",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.38",
          "form": "그래서 그 다음에 어떻게 됐어요",
          "original_form": "그래서 그 다음에 어떻게 됐어요",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.39",
          "form": "데뷔 전에 네이트판에",
          "original_form": "데뷔 전에 네이트판에",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037373.1.40",
          "form": "sm 연습생",
          "original_form": "sm 연습생",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037373.1.41",
          "form": "완전 고장에서 나왔나봐요",
          "original_form": "완전 고장에서 나왔나봐요",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037373.1.42",
          "form": "남자 연습생 얼굴이랑 이름이 떴는데",
          "original_form": "남자 연습생 얼굴이랑 이름이 떴는데",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037373.1.43",
          "form": "포장해서",
          "original_form": "포장해서",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037373.1.44",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037373.1.45",
          "form": "거기에 걔가 있었엉5",
          "original_form": "거기에 걔가 있었엉5",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037373.1.46",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037373.1.47",
          "form": "그래서 제가",
          "original_form": "그래서 제가",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037373.1.48",
          "form": "그 연예인 진짜 양아치였다",
          "original_form": "그 연예인 진짜 양아치였다",
