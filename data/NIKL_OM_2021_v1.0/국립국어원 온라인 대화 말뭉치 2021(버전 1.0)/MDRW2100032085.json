        {
          "id": "MDRW2100032085.1.1",
          "form": "네 그래도 영화관엔 늘 공포영화 보러가는데",
          "original_form": "네 그래도 영화관엔 늘 공포영화 보러가는데",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032085.1.2",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032085.1.3",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032085.1.4",
          "form": "남친이 공포영화 안좋아한대서 이제 못보겠네요 ㅋㅋㅋㅋ",
          "original_form": "남친이 공포영화 안좋아한대서 이제 못보겠네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032085.1.5",
          "form": "최근에 보신게 뭐에요?",
          "original_form": "최근에 보신게 뭐에요?",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032085.1.6",
          "form": "최근엔 영화관 말고 그냥 넷플릭스로",
          "original_form": "최근엔 영화관 말고 그냥 넷플릭스로",
          "speaker_id": "2",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032085.1.7",
          "form": "미드소마 봤어요",
          "original_form": "미드소마 봤어요",
          "speaker_id": "2",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032085.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032085.1.9",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032085.1.10",
          "form": "아무나 못보는건데....",
          "original_form": "아무나 못보는건데....",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032085.1.11",
          "form": "여름엔 한편 봐야지 싶어서",
          "original_form": "여름엔 한편 봐야지 싶어서",
          "speaker_id": "2",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032085.1.12",
          "form": "그러게요 아무나 못보겠더라구요ㅋㅋㅋㅋ",
          "original_form": "그러게요 아무나 못보겠더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032085.1.13",
          "form": "그거 안보셨죠?",
          "original_form": "그거 안보셨죠?",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032085.1.14",
          "form": "뭐요",
          "original_form": "뭐요",
          "speaker_id": "2",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032085.1.15",
          "form": "곡성 감독이",
          "original_form": "곡성 감독이",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032085.1.16",
