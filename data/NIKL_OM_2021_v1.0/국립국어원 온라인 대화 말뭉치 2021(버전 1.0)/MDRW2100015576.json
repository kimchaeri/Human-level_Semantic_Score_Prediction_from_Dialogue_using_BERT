      "utterance": [
        {
          "id": "MDRW2100015576.1.1",
          "form": "오 좋네요ㅋㅋㅋㅋㅋ",
          "original_form": "오 좋네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015576.1.2",
          "form": "동네에",
          "original_form": "동네에",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015576.1.3",
          "form": "도장이 있을라나요",
          "original_form": "도장이 있을라나요",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015576.1.4",
          "form": "ㅋㅋㅋ 흠 함 지도에 검색해보세용 ㅋㅋ 대한검도!",
          "original_form": "ㅋㅋㅋ 흠 함 지도에 검색해보세용 ㅋㅋ 대한검도!",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015576.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015576.1.6",
          "form": "아주 꿀팁이네요",
          "original_form": "아주 꿀팁이네요",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015576.1.7",
          "form": "검도장에",
          "original_form": "검도장에",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015576.1.8",
          "form": "샤워실이 있나요?",
          "original_form": "샤워실이 있나요?",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015576.1.9",
          "form": "저 다니는덴 있긴한데용  근데아마 요즘 샤워실이 다 금지아닌가요? 모르겟넹.. 그냥 집에와서 씻어요 ㅋㅋ",
          "original_form": "저 다니는덴 있긴한데용  근데아마 요즘 샤워실이 다 금지아닌가요? 모르겟넹.. 그냥 집에와서 씻어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015576.1.10",
          "form": "아 헐 그러네요 코로나",
          "original_form": "아 헐 그러네요 코로나",
          "speaker_id": "1",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100015576.1.11",
          "form": "보통 가면 아예 기초부터알려주나요?\r\n성인반도 진도가 다들 다를거같아서..!",
          "original_form": "보통 가면 아예 기초부터알려주나요?\r\n성인반도 진도가 다들 다를거같아서..!",
          "speaker_id": "1",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100015576.1.12",
          "form": "냉 수준다른사람들 다같이운동하더라구요 전 첨에 따로 빼서 기초부터 알려주시고 연습하고 그런식으로 하던데용 ㅋㅋ 어제 처음으로 다른분들이랑 같이연습햇어요",
          "original_form": "냉 수준다른사람들 다같이운동하더라구요 전 첨에 따로 빼서 기초부터 알려주시고 연습하고 그런식으로 하던데용 ㅋㅋ 어제 처음으로 다른분들이랑 같이연습햇어요",
          "speaker_id": "2",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100015576.1.13",
          "form": "헐 그렇군요 알려주고따로하고",
          "original_form": "헐 그렇군요 알려주고따로하고",
          "speaker_id": "1",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100015576.1.14",
          "form": "대부분 혼자오나요? 친구랑같이오나요?",
          "original_form": "대부분 혼자오나요? 친구랑같이오나요?",
          "speaker_id": "1",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100015576.1.15",
          "form": "ㅋㅋ 같이오는사람 아직 전 본적 없어용  성인반이라더그런듯",
          "original_form": "ㅋㅋ 같이오는사람 아직 전 본적 없어용  성인반이라더그런듯",
          "speaker_id": "2",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100015576.1.16",
          "form": "나이대도 20초부터 40대까지 다양하구용",
          "original_form": "나이대도 20초부터 40대까지 다양하구용",
          "speaker_id": "2",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100015576.1.17",
          "form": "오 ㅋㅋㅋㅋ 더 용기가나는데요?",
          "original_form": "오 ㅋㅋㅋㅋ 더 용기가나는데요?",
          "speaker_id": "1",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100015576.1.18",
          "form": "전 좀 친구랑 같이 등록하는편이라 원래는",
          "original_form": "전 좀 친구랑 같이 등록하는편이라 원래는",
          "speaker_id": "1",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100015576.1.19",
          "form": "전 혼자라서 다른분들이랑 합류 할수있게 진도 빨리 나가주셨어요 ㅋㅋ",
          "original_form": "전 혼자라서 다른분들이랑 합류 할수있게 진도 빨리 나가주셨어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100015576.1.20",
          "form": "와 좋네요",
          "original_form": "와 좋네요",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100015576.1.21",
          "form": "저진짜뽐뿌와요",
          "original_form": "저진짜뽐뿌와요",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100015576.1.22",
          "form": "ㅋㅋㅋㅋ 도장에 전화먼저 해보세요! ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 도장에 전화먼저 해보세요! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100015576.1.23",
          "form": "ㅋㅋㅋ아직 조금",
          "original_form": "ㅋㅋㅋ아직 조금",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100015576.1.24",
          "form": "다짐이 필요해요",
          "original_form": "다짐이 필요해요",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100015576.1.25",
          "form": "ㅋㅋㅋㅋㅋ 원래하시던 운동은 있나요",
          "original_form": "ㅋㅋㅋㅋㅋ 원래하시던 운동은 있나요",
          "speaker_id": "2",
          "time": "20210826 17:16"
        },
        {
          "id": "MDRW2100015576.1.26",
          "form": "ㅋㅋㅋㅋ 들으면 놀라실걸요",
          "original_form": "ㅋㅋㅋㅋ 들으면 놀라실걸요",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100015576.1.27",
          "form": "숨쉬기정도?",
          "original_form": "숨쉬기정도?",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100015576.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ첨하시면 근육통 마니잇으시긴 하겟다ㅋㅋㅋ 그건 어느운동이나 마찬가지지만요",
