      "utterance": [
        {
          "id": "MDRW2100022179.1.1",
          "form": "계시는 곳 날씨는 어떤지요",
          "original_form": "계시는 곳 날씨는 어떤지요",
          "speaker_id": "1",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100022179.1.2",
          "form": "여긴 날씨가 우중충 합니다",
          "original_form": "여긴 날씨가 우중충 합니다",
          "speaker_id": "1",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100022179.1.3",
          "form": "여기도 그렇습니다",
          "original_form": "여기도 그렇습니다",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100022179.1.4",
          "form": "전국적으로 흐린날씨라고 하네요",
          "original_form": "전국적으로 흐린날씨라고 하네요",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100022179.1.5",
          "form": "조금 있다가 근처 바다에 나갈생각입니다",
          "original_form": "조금 있다가 근처 바다에 나갈생각입니다",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100022179.1.6",
          "form": "모래사장에 가서 모래놀이 해주려고합니다",
          "original_form": "모래사장에 가서 모래놀이 해주려고합니다",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100022179.1.7",
          "form": "그곳은 바다와 가깝나요?",
          "original_form": "그곳은 바다와 가깝나요?",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100022179.1.8",
          "form": "집이 바다 가까이 있나 봐요",
          "original_form": "집이 바다 가까이 있나 봐요",
          "speaker_id": "1",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100022179.1.9",
          "form": "네 집에서 바다가 보입니다",
          "original_form": "네 집에서 바다가 보입니다",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100022179.1.10",
          "form": "멀리 있어요 산이 가깝죠",
          "original_form": "멀리 있어요 산이 가깝죠",
          "speaker_id": "1",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100022179.1.11",
          "form": "정말 좋은 곳에 사시는군요",
          "original_form": "정말 좋은 곳에 사시는군요",
          "speaker_id": "1",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100022179.1.12",
          "form": "언제 한번 초대해 주실래요",
          "original_form": "언제 한번 초대해 주실래요",
          "speaker_id": "1",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100022179.1.13",
          "form": "초대하겠습니다",
          "original_form": "초대하겠습니다",
          "speaker_id": "2",
          "time": "20210829 10:37"
        },
        {
          "id": "MDRW2100022179.1.14",
          "form": "남쪽이라 괜찮으실지 모르겠네요",
          "original_form": "남쪽이라 괜찮으실지 모르겠네요",
          "speaker_id": "2",
          "time": "20210829 10:37"
        },
        {
          "id": "MDRW2100022179.1.15",
          "form": "예 그럼 당장 달려가겠습니다",
          "original_form": "예 그럼 당장 달려가겠습니다",
          "speaker_id": "1",
          "time": "20210829 10:37"
        },
        {
          "id": "MDRW2100022179.1.16",
          "form": "곧 가을이니 가을전어 준비할게요",
          "original_form": "곧 가을이니 가을전어 준비할게요",
          "speaker_id": "2",
          "time": "20210829 10:37"
        },
        {
          "id": "MDRW2100022179.1.17",
          "form": "전 전어를  좋아하지 않습니다",
          "original_form": "전 전어를  좋아하지 않습니다",
          "speaker_id": "1",
          "time": "20210829 10:37"
        },
        {
          "id": "MDRW2100022179.1.18",
          "form": "돼지고기로 부탁드립니다",
          "original_form": "돼지고기로 부탁드립니다",
          "speaker_id": "1",
          "time": "20210829 10:38"
        },
        {
          "id": "MDRW2100022179.1.19",
          "form": "돼지고기 맛집이 있습니다",
          "original_form": "돼지고기 맛집이 있습니다",
          "speaker_id": "2",
          "time": "20210829 10:38"
        },
        {
          "id": "MDRW2100022179.1.20",
          "form": "정말 최고 군요",
          "original_form": "정말 최고 군요",
          "speaker_id": "1",
          "time": "20210829 10:38"
        },
        {
          "id": "MDRW2100022179.1.21",
          "form": "제주도 흑돼지로 구운 코소롱으로 초대할게요",
          "original_form": "제주도 흑돼지로 구운 코소롱으로 초대할게요",
          "speaker_id": "2",
          "time": "20210829 10:38"
        },
        {
          "id": "MDRW2100022179.1.22",
          "form": "멜젓에 한입 하시면 소주가 잘 넘어갑니다",
          "original_form": "멜젓에 한입 하시면 소주가 잘 넘어갑니다",
          "speaker_id": "2",
          "time": "20210829 10:38"
        },
        {
          "id": "MDRW2100022179.1.23",
          "form": "오 너무 좋네요 비 오는 날 먹으면 딱 좋겠군요",
          "original_form": "오 너무 좋네요 비 오는 날 먹으면 딱 좋겠군요",
          "speaker_id": "1",
          "time": "20210829 10:38"
        },
        {
          "id": "MDRW2100022179.1.24",
          "form": "네 거기 비가오나요?",
          "original_form": "네 거기 비가오나요?",
          "speaker_id": "2",
          "time": "20210829 10:38"
        },
        {
          "id": "MDRW2100022179.1.25",
          "form": "아직은 비가 오지 않습니다",
          "original_form": "아직은 비가 오지 않습니다",
          "speaker_id": "1",
          "time": "20210829 10:39"
        },
        {
          "id": "MDRW2100022179.1.26",
          "form": "비오면 해드시는 음식이 있나요?",
          "original_form": "비오면 해드시는 음식이 있나요?",
          "speaker_id": "2",
          "time": "20210829 10:39"
        },
        {
          "id": "MDRW2100022179.1.27",
          "form": "저녁 되면 추워질 거 같아 이제 긴 옷이 필요하겠군요",
          "original_form": "저녁 되면 추워질 거 같아 이제 긴 옷이 필요하겠군요",
          "speaker_id": "1",
          "time": "20210829 10:39"
        },
        {
          "id": "MDRW2100022179.1.28",
          "form": "해물전을 해먹지요",
          "original_form": "해물전을 해먹지요",
          "speaker_id": "1",
          "time": "20210829 10:39"
        },
        {
          "id": "MDRW2100022179.1.29",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210829 10:39"
        },
        {
          "id": "MDRW2100022179.1.30",
          "form": "막걸리를 좋아하시는지요",
          "original_form": "막걸리를 좋아하시는지요",
          "speaker_id": "1",
          "time": "20210829 10:39"
        },
        {
          "id": "MDRW2100022179.1.31",
          "form": "막걸리 없어서 못먹습니다",
          "original_form": "막걸리 없어서 못먹습니다",
          "speaker_id": "2",
          "time": "20210829 10:39"
        }
      ]
    }
