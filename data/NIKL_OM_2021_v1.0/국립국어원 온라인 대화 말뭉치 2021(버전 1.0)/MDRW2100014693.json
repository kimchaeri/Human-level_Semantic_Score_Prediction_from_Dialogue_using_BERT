      "utterance": [
        {
          "id": "MDRW2100014693.1.1",
          "form": "추워지고있군요",
          "original_form": "추워지고있군요",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014693.1.2",
          "form": "가을이 오고있어요",
          "original_form": "가을이 오고있어요",
          "speaker_id": "1",
          "time": "20210826 02:40"
        },
        {
          "id": "MDRW2100014693.1.3",
          "form": "가을에는 목에 수건을 꼭하는데 name1님도 추위타시나요",
          "original_form": "가을에는 목에 수건을 꼭하는데 &name1&님도 추위타시나요",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100014693.1.4",
          "form": "네 완전요 스카프 좋아하시나요?",
          "original_form": "네 완전요 스카프 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100014693.1.5",
          "form": "스카프는 생존이죸ㅋㅋ",
          "original_form": "스카프는 생존이죸ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100014693.1.6",
          "form": "내복을 준비해야겠어요",
          "original_form": "내복을 준비해야겠어요",
          "speaker_id": "2",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100014693.1.7",
          "form": "가을옷 꺼내셨나요",
          "original_form": "가을옷 꺼내셨나요",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100014693.1.8",
          "form": "아직요ㅎㅎ 비가오니 쌀쌀하네요",
          "original_form": "아직요ㅎㅎ 비가오니 쌀쌀하네요",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100014693.1.9",
          "form": "새벽에는 너무 춥더라고요",
          "original_form": "새벽에는 너무 춥더라고요",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100014693.1.10",
          "form": "새벽에  벌써ㅜㅜㅜ",
          "original_form": "새벽에  벌써ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100014693.1.11",
          "form": "살을찌워야겠어요",
          "original_form": "살을찌워야겠어요",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100014693.1.12",
          "form": "좋아하는ㅈ계절 있나요",
          "original_form": "좋아하는ㅈ계절 있나요",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100014693.1.13",
          "form": "겨울빼고 다좋아요ㅋㅋㅋㅋ",
          "original_form": "겨울빼고 다좋아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100014693.1.14",
          "form": "윽 저도 겨울이 싫어요",
          "original_form": "윽 저도 겨울이 싫어요",
          "speaker_id": "1",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100014693.1.15",
          "form": "춥고 밤도 길어요",
          "original_form": "춥고 밤도 길어요",
          "speaker_id": "2",
          "time": "20210826 14:10"
        }
