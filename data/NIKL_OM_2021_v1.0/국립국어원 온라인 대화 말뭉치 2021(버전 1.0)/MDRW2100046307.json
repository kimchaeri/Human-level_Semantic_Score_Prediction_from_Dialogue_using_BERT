      "utterance": [
        {
          "id": "MDRW2100046307.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211019 14:23"
        },
        {
          "id": "MDRW2100046307.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211019 14:23"
        },
        {
          "id": "MDRW2100046307.1.3",
          "form": "가상인간에 대해서 어떻게 생각하시나요",
          "original_form": "가상인간에 대해서 어떻게 생각하시나요",
          "speaker_id": "2",
          "time": "20211019 14:24"
        },
        {
          "id": "MDRW2100046307.1.4",
          "form": "언젠간 ai가 인간을 지배하지 않을까요",
          "original_form": "언젠간 ai가 인간을 지배하지 않을까요",
          "speaker_id": "1",
          "time": "20211019 14:24"
        },
        {
          "id": "MDRW2100046307.1.5",
          "form": "터미네이터가 현실이 될 수도,,",
          "original_form": "터미네이터가 현실이 될 수도,,",
          "speaker_id": "2",
          "time": "20211019 14:24"
        },
        {
          "id": "MDRW2100046307.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 14:24"
        },
        {
          "id": "MDRW2100046307.1.7",
          "form": "그 요즘은",
          "original_form": "그 요즘은",
          "speaker_id": "2",
          "time": "20211019 14:24"
        },
        {
          "id": "MDRW2100046307.1.8",
          "form": "광고에도",
          "original_form": "광고에도",
          "speaker_id": "2",
          "time": "20211019 14:24"
        },
        {
          "id": "MDRW2100046307.1.9",
          "form": "가상인간을 쓰더라구요..",
          "original_form": "가상인간을 쓰더라구요..",
          "speaker_id": "2",
          "time": "20211019 14:24"
        },
        {
          "id": "MDRW2100046307.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20211019 14:24"
        },
        {
          "id": "MDRW2100046307.1.11",
          "form": "인스타 팔로워도 어마어마하던데요",
          "original_form": "인스타 팔로워도 어마어마하던데요",
          "speaker_id": "2",
          "time": "20211019 14:25"
        },
        {
          "id": "MDRW2100046307.1.12",
          "form": "돈도 많이번대요",
          "original_form": "돈도 많이번대요",
          "speaker_id": "2",
          "time": "20211019 14:25"
        },
        {
          "id": "MDRW2100046307.1.13",
          "form": "알아서 추천해서 광고를 띄우더라고요",
          "original_form": "알아서 추천해서 광고를 띄우더라고요",
          "speaker_id": "1",
          "time": "20211019 14:25"
        },
        {
          "id": "MDRW2100046307.1.14",
          "form": "아 그 알고리즘",
          "original_form": "아 그 알고리즘",
          "speaker_id": "2",
          "time": "20211019 14:25"
        },
        {
          "id": "MDRW2100046307.1.15",
          "form": "알고리즘은 모든걸 알고있어요 정말",
          "original_form": "알고리즘은 모든걸 알고있어요 정말",
          "speaker_id": "2",
          "time": "20211019 14:25"
        },
        {
          "id": "MDRW2100046307.1.16",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 14:25"
        },
        {
          "id": "MDRW2100046307.1.17",
          "form": "무서울정도",
          "original_form": "무서울정도",
          "speaker_id": "2",
          "time": "20211019 14:25"
        },
        {
          "id": "MDRW2100046307.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20211019 14:25"
        },
        {
          "id": "MDRW2100046307.1.19",
          "form": "그래서 저도 가끔",
          "original_form": "그래서 저도 가끔",
          "speaker_id": "1",
          "time": "20211019 14:25"
        },
        {
          "id": "MDRW2100046307.1.20",
          "form": "감시당하는거 같아서",
          "original_form": "감시당하는거 같아서",
          "speaker_id": "1",
          "time": "20211019 14:25"
        },
        {
          "id": "MDRW2100046307.1.21",
          "form": "겁나긴하더라고요",
          "original_form": "겁나긴하더라고요",
          "speaker_id": "1",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.22",
          "form": "어떻게 알고 추천해주는지",
          "original_form": "어떻게 알고 추천해주는지",
          "speaker_id": "1",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.23",
          "form": "맞아요... 저 그 때 친구랑 통화하면서",
          "original_form": "맞아요... 저 그 때 친구랑 통화하면서",
          "speaker_id": "2",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.24",
          "form": "아 핸드폰 케이스 사야되는데",
          "original_form": "아 핸드폰 케이스 사야되는데",
          "speaker_id": "2",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.25",
          "form": "뭐 이런 얘기했었다가",
          "original_form": "뭐 이런 얘기했었다가",
          "speaker_id": "2",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.26",
          "form": "그건 정말 감시당하신거 같은데요",
          "original_form": "그건 정말 감시당하신거 같은데요",
          "speaker_id": "1",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.27",
          "form": "유튜브에 바로 핸드폰 케이스 영상 떠서",
          "original_form": "유튜브에 바로 핸드폰 케이스 영상 떠서",
          "speaker_id": "2",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.28",
          "form": "소리질렀어요",
          "original_form": "소리질렀어요",
          "speaker_id": "2",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.30",
          "form": "그 알고리즘이 음성까지 수집한다하더라고요",
          "original_form": "그 알고리즘이 음성까지 수집한다하더라고요",
          "speaker_id": "2",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.31",
          "form": "무섭;",
          "original_form": "무섭;",
          "speaker_id": "2",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.32",
          "form": "아 몰랐네요",
          "original_form": "아 몰랐네요",
          "speaker_id": "1",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.33",
          "form": "음성까지는 처음 들어봤어요",
          "original_form": "음성까지는 처음 들어봤어요",
          "speaker_id": "1",
          "time": "20211019 14:26"
        },
        {
          "id": "MDRW2100046307.1.34",
          "form": "저도 최근에 알았어요 ㅋㅋㅋ\r\n",
          "original_form": "저도 최근에 알았어요 ㅋㅋㅋ\r\n",
          "speaker_id": "2",
          "time": "20211019 14:27"
        }
      ]
