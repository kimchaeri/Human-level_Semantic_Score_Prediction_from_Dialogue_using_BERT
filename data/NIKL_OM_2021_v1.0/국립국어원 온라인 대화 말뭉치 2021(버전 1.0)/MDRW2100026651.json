      "utterance": [
        {
          "id": "MDRW2100026651.1.1",
          "form": "공연이나 전시회 좋아하세요??",
          "original_form": "공연이나 전시회 좋아하세요??",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026651.1.2",
          "form": "아뇨ㅠ",
          "original_form": "아뇨ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026651.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026651.1.4",
          "form": "ㅋㅋㅋ 이거 선택하셨길래 좋아하시나 했어요",
          "original_form": "ㅋㅋㅋ 이거 선택하셨길래 좋아하시나 했어요",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026651.1.5",
          "form": "저는 음악 공연 보러가고 싶네요",
          "original_form": "저는 음악 공연 보러가고 싶네요",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026651.1.6",
          "form": "저는 대학로공연은",
          "original_form": "저는 대학로공연은",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026651.1.7",
          "form": "몇번가봤어요!",
          "original_form": "몇번가봤어요!",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026651.1.8",
          "form": "못보니까 보고 싶어욬ㅋㅋㅋ",
          "original_form": "못보니까 보고 싶어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026651.1.9",
          "form": "못하니까 더 하고싶어지는것같아요",
          "original_form": "못하니까 더 하고싶어지는것같아요",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026651.1.10",
          "form": "뮤지컬좋아하세요?",
          "original_form": "뮤지컬좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026651.1.11",
          "form": "뮤지컬은 본적이 몇번 없는데 좋긴 하더라고요",
          "original_form": "뮤지컬은 본적이 몇번 없는데 좋긴 하더라고요",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026651.1.12",
          "form": "시간과 돈만 있으면 자주 보고 싶었었어요 ㅋㅋㅋ",
          "original_form": "시간과 돈만 있으면 자주 보고 싶었었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026651.1.13",
          "form": "오 저는 뮤지컬 본적이 없어요",
          "original_form": "오 저는 뮤지컬 본적이 없어요",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026651.1.14",
          "form": "나중에 한번 보러가세요 이층도 가봤었는데 괜찮더라고요 분위기가 즐거워서",
          "original_form": "나중에 한번 보러가세요 이층도 가봤었는데 괜찮더라고요 분위기가 즐거워서",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026651.1.15",
          "form": "뮤지컬은 진짜 가보고싶긴해요",
          "original_form": "뮤지컬은 진짜 가보고싶긴해요",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026651.1.16",
          "form": "콘서트가본적있으세요?",
          "original_form": "콘서트가본적있으세요?",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026651.1.17",
          "form": "어느 자리여도 신나더라고요",
          "original_form": "어느 자리여도 신나더라고요",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026651.1.18",
          "form": "커지는기대감",
          "original_form": "커지는기대감",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026651.1.19",
          "form": "네 가본적 있어요 name1은 있으신가요??",
          "original_form": "네 가본적 있어요 &name1&은 있으신가요??",
