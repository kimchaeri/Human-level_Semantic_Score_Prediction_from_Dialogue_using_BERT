      "utterance": [
        {
          "id": "MDRW2100035968.1.1",
          "form": "손목보호하려고 버티컬마우스도 알아보고 있어요!",
          "original_form": "손목보호하려고 버티컬마우스도 알아보고 있어요!",
          "speaker_id": "1",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100035968.1.2",
          "form": "거기 마우스가",
          "original_form": "거기 마우스가",
          "speaker_id": "2",
          "time": "20210830 15:20"
        },
        {
          "id": "MDRW2100035968.1.3",
          "form": "그렇게 좋다는데",
          "original_form": "그렇게 좋다는데",
          "speaker_id": "2",
          "time": "20210830 15:20"
        },
        {
          "id": "MDRW2100035968.1.4",
          "form": "사실인가요!!",
          "original_form": "사실인가요!!",
          "speaker_id": "2",
          "time": "20210830 15:20"
        },
        {
          "id": "MDRW2100035968.1.5",
          "form": "ㅋㅋㅋㅋ 아 써보면 알려드릴게요",
          "original_form": "ㅋㅋㅋㅋ 아 써보면 알려드릴게요",
          "speaker_id": "1",
          "time": "20210830 17:00"
        },
        {
          "id": "MDRW2100035968.1.6",
          "form": "건강하기위해 온갖 장비 동원중입니다",
          "original_form": "건강하기위해 온갖 장비 동원중입니다",
          "speaker_id": "1",
          "time": "20210830 17:00"
        },
        {
          "id": "MDRW2100035968.1.7",
          "form": "손목을 위해서는.. 그게",
          "original_form": "손목을 위해서는.. 그게",
          "speaker_id": "2",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100035968.1.8",
          "form": "필요하네유..",
          "original_form": "필요하네유..",
          "speaker_id": "2",
          "time": "20210830 17:18"
        },
        {
          "id": "MDRW2100035968.1.9",
          "form": "맞아요 ㅋㅋㅋㅋ 그거랑 인체공학적으로 설계된 키보드도 잇다더라구요",
          "original_form": "맞아요 ㅋㅋㅋㅋ 그거랑 인체공학적으로 설계된 키보드도 잇다더라구요",
          "speaker_id": "1",
          "time": "20210830 17:40"
        },
        {
          "id": "MDRW2100035968.1.10",
          "form": "세상이 짐짜",
          "original_form": "세상이 짐짜",
          "speaker_id": "2",
          "time": "20210830 19:11"
        },
        {
          "id": "MDRW2100035968.1.11",
          "form": "많이 발전했네요ㅋㅋㅋㅋ",
          "original_form": "많이 발전했네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100035968.1.12",
          "form": "그쵸 ㅋㅋㅋㅋ 돈으로 건강을 살수잇어요",
          "original_form": "그쵸 ㅋㅋㅋㅋ 돈으로 건강을 살수잇어요",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100035968.1.13",
          "form": "맞아여.. 건강을 사야해요..",
          "original_form": "맞아여.. 건강을 사야해요..",
          "speaker_id": "2",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100035968.1.14",
          "form": "건강에 투자하고 건강으로 돈벌고",
          "original_form": "건강에 투자하고 건강으로 돈벌고",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100035968.1.15",
          "form": "ㅋㅋㅋㅋㅋ건강하면서 돈벌래여 ㅠㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ건강하면서 돈벌래여 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210831 08:34"
        },
        {
          "id": "MDRW2100035968.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ  저 이번주말에 클라이밍가요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ  저 이번주말에 클라이밍가요",
          "speaker_id": "1",
          "time": "20210831 10:21"
        },
        {
          "id": "MDRW2100035968.1.17",
          "form": "다이나믹하게",
          "original_form": "다이나믹하게",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100035968.1.18",
          "form": "운동하네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "운동하네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100035968.1.19",
