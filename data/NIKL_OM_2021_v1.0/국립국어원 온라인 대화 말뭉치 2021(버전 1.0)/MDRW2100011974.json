      "utterance": [
        {
          "id": "MDRW2100011974.1.1",
          "form": "우린저녁은 다먹었는데..뒷정리가 남았구만~",
          "original_form": "우린저녁은 다먹었는데..뒷정리가 남았구만~",
          "speaker_id": "2",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100011974.1.2",
          "form": "아따 먹을때는 신나는데 ㅋㅋㅋㅋ",
          "original_form": "아따 먹을때는 신나는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100011974.1.3",
          "form": "ㅋㅋ 그렇지",
          "original_form": "ㅋㅋ 그렇지",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100011974.1.4",
          "form": "뭐먹었어?",
          "original_form": "뭐먹었어?",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100011974.1.5",
          "form": "신랑은 수술잘됐대?",
          "original_form": "신랑은 수술잘됐대?",
          "speaker_id": "1",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100011974.1.6",
          "form": "우리오늘 무침회랑 통닭시킴 ㅋㅋㅋ",
          "original_form": "우리오늘 무침회랑 통닭시킴 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100011974.1.7",
          "form": "만찬을 즐겼네이~",
          "original_form": "만찬을 즐겼네이~",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100011974.1.8",
          "form": "응 ㅋㅋㅋ이제 팔이.좀 올라가네~",
          "original_form": "응 ㅋㅋㅋ이제 팔이.좀 올라가네~",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100011974.1.9",
          "form": "근데 아파서;;",
          "original_form": "근데 아파서;;",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100011974.1.10",
          "form": "와 무침회 통닭",
          "original_form": "와 무침회 통닭",
          "speaker_id": "1",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100011974.1.11",
          "form": "다 맛있는거네ㅋ",
          "original_form": "다 맛있는거네ㅋ",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100011974.1.12",
          "form": "아아 이제 팔 좀 올라가는구나ㅜ",
          "original_form": "아아 이제 팔 좀 올라가는구나ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100011974.1.13",
          "form": "아구 그래 아프지ㅜ",
          "original_form": "아구 그래 아프지ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100011974.1.14",
          "form": "쉬고싶응데..애들이 또 아빠자꾸 찾아가; 쉬도 몬하네ㅠ",
          "original_form": "쉬고싶응데..애들이 또 아빠자꾸 찾아가; 쉬도 몬하네ㅠ",
          "speaker_id": "2",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100011974.1.15",
          "form": "아고ㅜ진짜 쉬지를못하니 그렇겠다",
          "original_form": "아고ㅜ진짜 쉬지를못하니 그렇겠다",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100011974.1.16",
          "form": "근데..나도 눈에 보이니 자꾸 시키게됨 ㅋㅋㅋ",
          "original_form": "근데..나도 눈에 보이니 자꾸 시키게됨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100011974.1.17",
          "form": "일단 애들있음",
          "original_form": "일단 애들있음",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100011974.1.18",
          "form": "못쉰다고봐야지",
          "original_form": "못쉰다고봐야지",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100011974.1.19",
          "form": "아하..",
          "original_form": "아하..",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100011974.1.20",
          "form": "마음은 안쓰러운데 ㅋㅋㅋㅋ",
          "original_form": "마음은 안쓰러운데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100011974.1.21",
          "form": "습관이 무섭다 ㅋㅋㅋㅋ",
          "original_form": "습관이 무섭다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100011974.1.22",
          "form": "그렇다이ㅋㅋ",
          "original_form": "그렇다이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100011974.1.23",
          "form": "쉬게해줘라ㅋㅋㅋ",
          "original_form": "쉬게해줘라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100011974.1.24",
          "form": "나는 이제 밥 차려야되는데",
          "original_form": "나는 이제 밥 차려야되는데",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100011974.1.25",
          "form": "귀찮다ㅋㅋ",
          "original_form": "귀찮다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100011974.1.26",
          "form": "아직 안먹었나.??",
          "original_form": "아직 안먹었나.??",
          "speaker_id": "2",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100011974.1.27",
          "form": "응 아직 ㅋㅋ",
          "original_form": "응 아직 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100011974.1.28",
          "form": "아까 누구와서 수다떤다고",
          "original_form": "아까 누구와서 수다떤다고",
          "speaker_id": "1",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100011974.1.29",
          "form": "이제 차린다ㅋㅋㅋ",
          "original_form": "이제 차린다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100011974.1.30",
          "form": "난 차라리 밥 차리는게 낫다 ㅋㅋㅋ먹고나면 진짜 꼼짝하기 싫다 ㅋㅋㅋㅋ",
          "original_form": "난 차라리 밥 차리는게 낫다 ㅋㅋㅋ먹고나면 진짜 꼼짝하기 싫다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:24"
        }
      ]
    }
