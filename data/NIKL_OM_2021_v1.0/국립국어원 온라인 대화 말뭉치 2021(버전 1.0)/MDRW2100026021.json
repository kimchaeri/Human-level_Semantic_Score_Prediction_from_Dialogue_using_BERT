      "utterance": [
        {
          "id": "MDRW2100026021.1.1",
          "form": "저 피자샵이랑 피자스쿨요ㅋㅋ",
          "original_form": "저 피자샵이랑 피자스쿨요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100026021.1.2",
          "form": "저도 피자샵 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 피자샵 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100026021.1.3",
          "form": "주로 피자샵에서 무슨피자 드세요??",
          "original_form": "주로 피자샵에서 무슨피자 드세요??",
          "speaker_id": "1",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100026021.1.4",
          "form": "감자 안 좋아하는데요",
          "original_form": "감자 안 좋아하는데요",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100026021.1.5",
          "form": "그그그",
          "original_form": "그그그",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100026021.1.6",
          "form": "치즈후라이요ㅋㅋ",
          "original_form": "치즈후라이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100026021.1.7",
          "form": "아 치즈후라이 맛잇죠",
          "original_form": "아 치즈후라이 맛잇죠",
          "speaker_id": "1",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100026021.1.8",
          "form": "뭐 시.키시나요??",
          "original_form": "뭐 시.키시나요??",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100026021.1.9",
          "form": "저도 치즈후라이 좋아하고 마늘불고기도 좋아해요",
          "original_form": "저도 치즈후라이 좋아하고 마늘불고기도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100026021.1.10",
          "form": "오 마늘불고기도 괜찮죠",
          "original_form": "오 마늘불고기도 괜찮죠",
          "speaker_id": "2",
          "time": "20210901 19:13"
        },
        {
          "id": "MDRW2100026021.1.11",
          "form": "원래 알볼로도 자주 먹었엇는데",
          "original_form": "원래 알볼로도 자주 먹었엇는데",
          "speaker_id": "2",
          "time": "20210901 19:13"
        },
        {
          "id": "MDRW2100026021.1.12",
          "form": "치킨이나 피자 드실때 맥주랑 드시나요?",
          "original_form": "치킨이나 피자 드실때 맥주랑 드시나요?",
          "speaker_id": "1",
          "time": "20210901 19:13"
        },
        {
          "id": "MDRW2100026021.1.13",
          "form": "가격이 너무 올랐어요",
          "original_form": "가격이 너무 올랐어요",
          "speaker_id": "2",
          "time": "20210901 19:13"
        },
        {
          "id": "MDRW2100026021.1.14",
          "form": "맞아요 저도 알볼로 좋아하는데",
          "original_form": "맞아요 저도 알볼로 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 19:13"
        },
        {
          "id": "MDRW2100026021.1.15",
          "form": "반반?ㅋㅋ",
          "original_form": "반반?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:13"
        },
        {
          "id": "MDRW2100026021.1.16",
          "form": "자주못먹어요 ㅠㅠ비싸져서",
          "original_form": "자주못먹어요 ㅠㅠ비싸져서",
          "speaker_id": "1",
          "time": "20210901 19:13"
        },
        {
          "id": "MDRW2100026021.1.17",
          "form": "먹을때도 잇고 다 먹고 생각날 때도 있고 그래요ㅋ",
          "original_form": "먹을때도 잇고 다 먹고 생각날 때도 있고 그래요ㅋ",
          "speaker_id": "2",
          "time": "20210901 19:13"
        },
        {
          "id": "MDRW2100026021.1.18",
          "form": "술이 센 편이신가요?",
          "original_form": "술이 센 편이신가요?",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026021.1.19",
          "form": "아뇨ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026021.1.20",
          "form": "알쓰에요",
          "original_form": "알쓰에요",
          "speaker_id": "2",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026021.1.21",
          "form": "최대 한병반?.. 한병이면 어질어질",
          "original_form": "최대 한병반?.. 한병이면 어질어질",
          "speaker_id": "2",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026021.1.22",
          "form": "한병반을 마시면 알쓰는 아니시죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "한병반을 마시면 알쓰는 아니시죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100026021.1.23",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100026021.1.24",
          "form": "한병으로 하죠ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "한병으로 하죠ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100026021.1.25",
          "form": "술 잘드세요?",
          "original_form": "술 잘드세요?",
          "speaker_id": "2",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100026021.1.26",
          "form": "주종에 따라 달라요 ㅋㅋㅋㅋ",
          "original_form": "주종에 따라 달라요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:23"
        },
        {
          "id": "MDRW2100026021.1.27",
          "form": "맥주나 소주는 약간 마시는데 전 와인을 그렇게 못마셔요",
          "original_form": "맥주나 소주는 약간 마시는데 전 와인을 그렇게 못마셔요",
          "speaker_id": "1",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100026021.1.28",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100026021.1.29",
          "form": "아예요?",
          "original_form": "아예요?",
          "speaker_id": "2",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100026021.1.30",
          "form": "한잔 정도만 마셔요",
          "original_form": "한잔 정도만 마셔요",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026021.1.31",
          "form": "한잔 이상 못마시겟더라구요",
          "original_form": "한잔 이상 못마시겟더라구요",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026021.1.32",
          "form": "과일주랑 안 맞는 사람들이 있더라구요",
          "original_form": "과일주랑 안 맞는 사람들이 있더라구요",
          "speaker_id": "2",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026021.1.33",
          "form": "전 와인 좋아요",
          "original_form": "전 와인 좋아요",
          "speaker_id": "2",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026021.1.34",
          "form": "잘맞음",
          "original_form": "잘맞음",
          "speaker_id": "2",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026021.1.35",
          "form": "오오 저랑 정반대시네요",
          "original_form": "오오 저랑 정반대시네요",
          "speaker_id": "1",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026021.1.36",
          "form": "막걸리엔 취약해요",
          "original_form": "막걸리엔 취약해요",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026021.1.37",
          "form": "기절햿었어요..",
          "original_form": "기절햿었어요..",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026021.1.38",
          "form": "ㄷㄷ...",
          "original_form": "ㄷㄷ...",
          "speaker_id": "1",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026021.1.39",
          "form": "막걸리 드시면 안되겟네요",
          "original_form": "막걸리 드시면 안되겟네요",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026021.1.40",
          "form": "저도 막걸리에도 약해요",
          "original_form": "저도 막걸리에도 약해요",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026021.1.41",
          "form": "맛잇다고 콸콸 마셧다가 3일간 앓아누웟어요 ㅋㅋㅋㅋㅋ",
          "original_form": "맛잇다고 콸콸 마셧다가 3일간 앓아누웟어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026021.1.42",
          "form": "어이쿠;;",
          "original_form": "어이쿠;;",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026021.1.43",
          "form": "막걸리가 숙취 쩌러요",
          "original_form": "막걸리가 숙취 쩌러요",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026021.1.44",
          "form": "그뒤로 콸콸 마시지 않습니다 ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "그뒤로 콸콸 마시지 않습니다 ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026021.1.45",
          "form": "드시긴 하시는군요ㅋㅋㅋ",
          "original_form": "드시긴 하시는군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100026021.1.46",
          "form": "앗...네...ㅋㅋㅋㅋㅋ",
          "original_form": "앗...네...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026021.1.47",
          "form": "막걸리 맛잇어요 ㅠㅠ",
          "original_form": "막걸리 맛잇어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026021.1.48",
          "form": "ㅋㅋㅋㅋ어우 전 싫어요 맛두 읍음",
