      "utterance": [
        {
          "id": "MDRW2100035514.1.1",
          "form": "저는 소고기콩나물뭇국 실패했어요",
          "original_form": "저는 소고기콩나물뭇국 실패했어요",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100035514.1.2",
          "form": "고추가루를 너무많이넣었네요",
          "original_form": "고추가루를 너무많이넣었네요",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100035514.1.3",
          "form": "앗ㅠ",
          "original_form": "앗ㅠ",
          "speaker_id": "1",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100035514.1.4",
          "form": "어쩌다..",
          "original_form": "어쩌다..",
          "speaker_id": "1",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100035514.1.5",
          "form": "처음해봐서 ㅋㅋ그래도 잘 먹었어요",
          "original_form": "처음해봐서 ㅋㅋ그래도 잘 먹었어요",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100035514.1.6",
          "form": "담엔 더 잘할수있을것같아요",
          "original_form": "담엔 더 잘할수있을것같아요",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100035514.1.7",
          "form": "아~ 처음에는 그렇죵",
          "original_form": "아~ 처음에는 그렇죵",
          "speaker_id": "1",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035514.1.8",
          "form": "잘드셨다는거보니 손맛있으신가봐요!",
          "original_form": "잘드셨다는거보니 손맛있으신가봐요!",
          "speaker_id": "1",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035514.1.9",
          "form": "그냥저냥 비슷하게만 하는거같아요 ㅋㅋ",
          "original_form": "그냥저냥 비슷하게만 하는거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:47"
        },
        {
          "id": "MDRW2100035514.1.10",
          "form": "오~  대단하세요",
          "original_form": "오~  대단하세요",
          "speaker_id": "1",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100035514.1.11",
          "form": "그게 은근 어렵다던데요?!",
          "original_form": "그게 은근 어렵다던데요?!",
          "speaker_id": "1",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100035514.1.12",
          "form": "그런가용 ㅋㅋㅋ 다행이네요",
          "original_form": "그런가용 ㅋㅋㅋ 다행이네요",
          "speaker_id": "2",
          "time": "20210829 20:59"
        },
        {
          "id": "MDRW2100035514.1.13",
          "form": "그럼요ㅋㅋ",
          "original_form": "그럼요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035514.1.14",
          "form": "후식도 드셨나요?",
          "original_form": "후식도 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035514.1.15",
          "form": "네 복숭어 먹었어요",
          "original_form": "네 복숭어 먹었어요",
          "speaker_id": "2",
          "time": "20210830 07:45"
        },
        {
          "id": "MDRW2100035514.1.16",
          "form": "벌써 아침이네요ㅠㅠ 아침 드셨나요",
          "original_form": "벌써 아침이네요ㅠㅠ 아침 드셨나요",
          "speaker_id": "2",
          "time": "20210830 07:45"
        },
        {
          "id": "MDRW2100035514.1.17",
          "form": "아직이요ㅠ 늦잠자서 운동을 늦게 나왔더니ㅠㅜ",
          "original_form": "아직이요ㅠ 늦잠자서 운동을 늦게 나왔더니ㅠㅜ",
          "speaker_id": "1",
          "time": "20210830 07:57"
        },
        {
          "id": "MDRW2100035514.1.18",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "1",
          "time": "20210830 07:57"
        },
        {
          "id": "MDRW2100035514.1.19",
          "form": "오 아침 운동 다니시능군요",
          "original_form": "오 아침 운동 다니시능군요",
          "speaker_id": "2",
          "time": "20210830 12:22"
        },
        {
          "id": "MDRW2100035514.1.20",
          "form": "저는 두유랑 복숭아",
          "original_form": "저는 두유랑 복숭아",
          "speaker_id": "2",
          "time": "20210830 12:22"
        },
        {
          "id": "MDRW2100035514.1.21",
          "form": "네! 밤보다 아침이 맞더라구요ㅋㅋ",
          "original_form": "네! 밤보다 아침이 맞더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100035514.1.22",
          "form": "오 복숭아 맛있죠",
          "original_form": "오 복숭아 맛있죠",
          "speaker_id": "1",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100035514.1.23",
          "form": "점심인가요?",
          "original_form": "점심인가요?",
          "speaker_id": "1",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100035514.1.24",
          "form": "아 점심은 회사 급식이에용 ㅋㅋ",
          "original_form": "아 점심은 회사 급식이에용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:45"
        }
      ]
    }
