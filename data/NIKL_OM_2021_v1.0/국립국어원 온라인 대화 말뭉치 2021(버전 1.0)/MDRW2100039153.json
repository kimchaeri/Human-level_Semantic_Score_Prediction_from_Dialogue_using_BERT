      "utterance": [
        {
          "id": "MDRW2100039153.1.1",
          "form": "술 아니요",
          "original_form": "술 아니요",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100039153.1.2",
          "form": "친정식구들은 잘 마시는 편인데",
          "original_form": "친정식구들은 잘 마시는 편인데",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100039153.1.3",
          "form": "저는 그냥 보통",
          "original_form": "저는 그냥 보통",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100039153.1.4",
          "form": "저는 예전엔 잘 마셨는데",
          "original_form": "저는 예전엔 잘 마셨는데",
          "speaker_id": "1",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100039153.1.5",
          "form": "이제는 맥주 반잔도 못마셔요 ㅠ",
          "original_form": "이제는 맥주 반잔도 못마셔요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100039153.1.6",
          "form": "그것도 바뀌더라구요",
          "original_form": "그것도 바뀌더라구요",
          "speaker_id": "2",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100039153.1.7",
          "form": "저는 일반적으로 볼땐 보통 사람들 정도는 마셔요",
          "original_form": "저는 일반적으로 볼땐 보통 사람들 정도는 마셔요",
          "speaker_id": "2",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100039153.1.8",
          "form": "맥주  한두잔정도",
          "original_form": "맥주  한두잔정도",
          "speaker_id": "2",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100039153.1.9",
          "form": "술은 어떤 주류 좋아하세요?",
          "original_form": "술은 어떤 주류 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 13:54"
        },
        {
          "id": "MDRW2100039153.1.10",
          "form": "맥주나 와인? 막걸리?",
          "original_form": "맥주나 와인? 막걸리?",
          "speaker_id": "1",
          "time": "20210903 13:54"
        },
        {
          "id": "MDRW2100039153.1.11",
          "form": "저는 종류 안가려요",
          "original_form": "저는 종류 안가려요",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039153.1.12",
          "form": "사실 친정쪽이 술을 잘 마셔서요",
          "original_form": "사실 친정쪽이 술을 잘 마셔서요",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039153.1.13",
          "form": "우와 ㅋㅋ 사회생활 하다보니 술 쎈게 좋은 것 같아요",
          "original_form": "우와 ㅋㅋ 사회생활 하다보니 술 쎈게 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100039153.1.14",
          "form": "술마셔야하는 직업인가 보네요?",
          "original_form": "술마셔야하는 직업인가 보네요?",
          "speaker_id": "2",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100039153.1.15",
          "form": "친정사람 통틀어서 제가 제일 조금 마시죠..ㅎㅎ",
          "original_form": "친정사람 통틀어서 제가 제일 조금 마시죠..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100039153.1.16",
          "form": "ㅋㅋ아니요 그냥 평범한 회사원이에요",
          "original_form": "ㅋㅋ아니요 그냥 평범한 회사원이에요",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039153.1.17",
          "form": "그치만 회식때 술을 마셔야 사회생활하기 편하더라구요",
          "original_form": "그치만 회식때 술을 마셔야 사회생활하기 편하더라구요",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039153.1.18",
          "form": "저는 술을 잘 못해서 회식없는 지금이 좋아요",
          "original_form": "저는 술을 잘 못해서 회식없는 지금이 좋아요",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039153.1.19",
          "form": "오 그렇군요.. 회사다니는 분들은 회식이 고역이겠어요",
          "original_form": "오 그렇군요.. 회사다니는 분들은 회식이 고역이겠어요",
          "speaker_id": "2",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039153.1.20",
          "form": "네 맞아요~ 그리고 전 사실 술도 맛있는데",
          "original_form": "네 맞아요~ 그리고 전 사실 술도 맛있는데",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100039153.1.21",
          "form": "몸이 술을 안 받아줘서 슬퍼요 ㅠ ㅋㅋㅋ",
          "original_form": "몸이 술을 안 받아줘서 슬퍼요 ㅠ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100039153.1.22",
          "form": "입에서는 맛있군요 그럼 더 안타깝겠어요",
          "original_form": "입에서는 맛있군요 그럼 더 안타깝겠어요",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039153.1.23",
          "form": "네ㅠ 그래도 요즘에는 무알콜 맥주가 맛있게 나오더라구요~ 다행이에요",
          "original_form": "네ㅠ 그래도 요즘에는 무알콜 맥주가 맛있게 나오더라구요~ 다행이에요",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039153.1.24",
          "form": "무알콜이 나와서 다행이네요",
          "original_form": "무알콜이 나와서 다행이네요",
          "speaker_id": "2",
