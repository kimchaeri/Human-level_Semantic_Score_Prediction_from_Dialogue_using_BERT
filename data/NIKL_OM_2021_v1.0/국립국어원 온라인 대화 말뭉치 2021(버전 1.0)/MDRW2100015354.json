        },
        {
          "id": "MDRW2100015354.1.12",
          "form": "웅ㅋㅋㅋ 대만갔었당ㅋㅋㅋㅋ",
          "original_form": "웅ㅋㅋㅋ 대만갔었당ㅋㅋㅋㅋ",
