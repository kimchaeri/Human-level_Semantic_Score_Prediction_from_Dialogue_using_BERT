      "utterance": [
        {
          "id": "MDRW2100024940.1.1",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024940.1.2",
          "form": "아 자가비 좋아해요 일본 과자죠",
          "original_form": "아 자가비 좋아해요 일본 과자죠",
          "speaker_id": "1",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024940.1.3",
          "form": "넹! 그거도",
          "original_form": "넹! 그거도",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024940.1.4",
          "form": "좋아했는데",
          "original_form": "좋아했는데",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024940.1.5",
          "form": "방사능 때문에 걱정돼서 끊었는데 한국과자로 비슷한 게 있더라구요",
          "original_form": "방사능 때문에 걱정돼서 끊었는데 한국과자로 비슷한 게 있더라구요",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024940.1.6",
          "form": "비슷한게 있는 줄 몰랐어요 자가비만 알아서,,",
          "original_form": "비슷한게 있는 줄 몰랐어요 자가비만 알아서,,",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024940.1.7",
          "form": "저도 몰랐는데",
          "original_form": "저도 몰랐는데",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024940.1.8",
          "form": "전남친이 좋아하더라구요",
          "original_form": "전남친이 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024940.1.9",
          "form": "그래서알았어요",
          "original_form": "그래서알았어요",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024940.1.10",
          "form": "아항 남친은 가도 과자는 남았군요",
          "original_form": "아항 남친은 가도 과자는 남았군요",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100024940.1.11",
          "form": "네 걔는 가도 돼요 호호",
          "original_form": "네 걔는 가도 돼요 호호",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024940.1.12",
          "form": "과자 뭐 제일 좋아하세요?",
          "original_form": "과자 뭐 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024940.1.13",
          "form": "ㅋㅋㅋㅋ 제가 그 과자를 찾으면 한 번 사먹어 볼께요",
          "original_form": "ㅋㅋㅋㅋ 제가 그 과자를 찾으면 한 번 사먹어 볼께요",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024940.1.14",
          "form": "전 팝콘이 먹고 싶을땐 코알라의 뻥이요",
          "original_form": "전 팝콘이 먹고 싶을땐 코알라의 뻥이요",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024940.1.15",
          "form": "감자칩이 먹고 싶으면 스윙칩",
          "original_form": "감자칩이 먹고 싶으면 스윙칩",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024940.1.16",
          "form": "커피랑 먹고 싶으면 미쯔,,",
          "original_form": "커피랑 먹고 싶으면 미쯔,,",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024940.1.17",
          "form": "10개이상 얘기가능해요,,",
          "original_form": "10개이상 얘기가능해요,,",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024940.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024940.1.19",
          "form": "뻥이요 풀네임이",
          "original_form": "뻥이요 풀네임이",
          "speaker_id": "2",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024940.1.20",
          "form": "코알라의 뻥이요인가요..?",
          "original_form": "코알라의 뻥이요인가요..?",
          "speaker_id": "2",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024940.1.21",
          "form": "아니오 그냥 뻥이요인데 따라한 제품이 있어서 ㅋㅋ",
          "original_form": "아니오 그냥 뻥이요인데 따라한 제품이 있어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024940.1.22",
          "form": "코알라의 뻥이요라고 그냥 부연해요,,",
          "original_form": "코알라의 뻥이요라고 그냥 부연해요,,",
          "speaker_id": "1",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024940.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024940.1.24",
          "form": "과자에",
          "original_form": "과자에",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024940.1.25",
          "form": "진심이신...!!",
          "original_form": "진심이신...!!",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024940.1.26",
          "form": "저 코알라가 서울식품이란 회사의 브랜드이름인데 주로 파는건 빵 생지에요",
          "original_form": "저 코알라가 서울식품이란 회사의 브랜드이름인데 주로 파는건 빵 생지에요",
          "speaker_id": "1",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024940.1.27",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024940.1.28",
          "form": "그래서 한동안 뻥이요를 못찾을땐 회사에 전화해서 어디서 파냐고 물어본 적이 있어요",
          "original_form": "그래서 한동안 뻥이요를 못찾을땐 회사에 전화해서 어디서 파냐고 물어본 적이 있어요",
          "speaker_id": "1",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024940.1.29",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024940.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024940.1.31",
          "form": "엄청 좋아하시나봐욬ㅋㅋㅋ",
          "original_form": "엄청 좋아하시나봐욬ㅋㅋㅋ",
