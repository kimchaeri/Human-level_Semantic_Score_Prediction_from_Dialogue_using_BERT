      "utterance": [
        {
          "id": "MDRW2100034115.1.1",
          "form": "육아는 어떠세용",
          "original_form": "육아는 어떠세용",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100034115.1.2",
          "form": "육아는.... 헬이에요ㅋㅋ",
          "original_form": "육아는.... 헬이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100034115.1.3",
          "form": "아이가 몇살이에용 ㅋㅋ",
          "original_form": "아이가 몇살이에용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100034115.1.4",
          "form": "직장인일때\r\n제 꿈이 육아휴직이었는데\r\n지금은 출근이 꿈입니다ㅋㅋ",
          "original_form": "직장인일때\r\n제 꿈이 육아휴직이었는데\r\n지금은 출근이 꿈입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100034115.1.5",
          "form": "5살이랑 2살이에요",
          "original_form": "5살이랑 2살이에요",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100034115.1.6",
          "form": "워우... 진짜 헬이겠다ㅠㅠ",
          "original_form": "워우... 진짜 헬이겠다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100034115.1.7",
          "form": "혼자보세요? 도움받으시나요",
          "original_form": "혼자보세요? 도움받으시나요",
          "speaker_id": "1",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100034115.1.8",
          "form": "지금은 혼자 보고 있어요:)\r\n첫째는 기관에 다니는데\r\n코로나 때문에 지금은\r\n가정에서 보육중이에요",
          "original_form": "지금은 혼자 보고 있어요:)\r\n첫째는 기관에 다니는데\r\n코로나 때문에 지금은\r\n가정에서 보육중이에요",
          "speaker_id": "2",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100034115.1.9",
          "form": "더힘드시겠네요 ㅠㅠㅋㅋ 말은 잘듣나용",
          "original_form": "더힘드시겠네요 ㅠㅠㅋㅋ 말은 잘듣나용",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100034115.1.10",
          "form": "아니요...... 말을 듣지 않습니다ㅋㅋㅋㅋㅈ",
          "original_form": "아니요...... 말을 듣지 않습니다ㅋㅋㅋㅋㅈ",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100034115.1.11",
          "form": "저 굉장히 여성스럽고\r\n조신했던 사람이었는데",
          "original_form": "저 굉장히 여성스럽고\r\n조신했던 사람이었는데",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100034115.1.12",
          "form": "제 안에 헐크가 살고 있었더라구요",
          "original_form": "제 안에 헐크가 살고 있었더라구요",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100034115.1.13",
          "form": "아이들 키우면서 헐크와 종종 마주쳐요ㅋㅋㅋ",
          "original_form": "아이들 키우면서 헐크와 종종 마주쳐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100034115.1.14",
          "form": "ㅋㅋㅋㅋ으아 상상만 해돜ㅋㅋㅋㅋㅋ ㅜ 내안의 한계를 마주하기 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ으아 상상만 해돜ㅋㅋㅋㅋㅋ ㅜ 내안의 한계를 마주하기 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100034115.1.15",
          "form": "애들 보시면서 집안일까지 대단하세용...",
          "original_form": "애들 보시면서 집안일까지 대단하세용...",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100034115.1.16",
          "form": "가장 극한 직업이\r\n엄마가 아닐까 싶네요 ㅋㅋㅋㅋㅋ",
          "original_form": "가장 극한 직업이\r\n엄마가 아닐까 싶네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100034115.1.17",
          "form": "차라리 주7일 \r\n회사에 출근하는게 나아요ㅋ",
          "original_form": "차라리 주7일 \r\n회사에 출근하는게 나아요ㅋ",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100034115.1.18",
          "form": "ㅋㅋㅋㅋㅋ 힘내세요ㅜㅜ 육아꿀팁 이런거 있으신가요",
          "original_form": "ㅋㅋㅋㅋㅋ 힘내세요ㅜㅜ 육아꿀팁 이런거 있으신가요",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100034115.1.19",
          "form": "육아꿀팁...이 있다면",
          "original_form": "육아꿀팁...이 있다면",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100034115.1.20",
          "form": "헐크를 영영 만나지 못했겠죠 ㅋㅋ",
          "original_form": "헐크를 영영 만나지 못했겠죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100034115.1.21",
          "form": "육아도 장비빨이란 말은 맞아요",
          "original_form": "육아도 장비빨이란 말은 맞아요{emoji:♡}",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100034115.1.22",
          "form": "ㅋㅋㅋㅋ 아 그런말 들어보기도 했어요 역시 돈이 최고입니다ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋ 아 그런말 들어보기도 했어요 역시 돈이 최고입니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100034115.1.23",
          "form": "맞아요ㅠ 그래서 돈을 버나봅니다",
          "original_form": "맞아요ㅠ 그래서 돈을 버나봅니다",
          "speaker_id": "2",
          "time": "20210827 19:36"
        }
      ]
    }
  ]
