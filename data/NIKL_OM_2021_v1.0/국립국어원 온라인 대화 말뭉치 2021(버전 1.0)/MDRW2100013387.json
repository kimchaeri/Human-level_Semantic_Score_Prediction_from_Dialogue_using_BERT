          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013387.1.5",
          "form": "해뜨고 다시 비오고 천둥때림",
          "original_form": "해뜨고 다시 비오고 천둥때림",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013387.1.6",
          "form": "근데 요즘 미세먼지는 없어",
          "original_form": "근데 요즘 미세먼지는 없어",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013387.1.7",
          "form": "지혼자 화내고난리남",
          "original_form": "지혼자 화내고난리남",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013387.1.8",
          "form": "우산을 항상 챙겨야할듯",
          "original_form": "우산을 항상 챙겨야할듯",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013387.1.9",
          "form": "저번에 천둥칠때",
          "original_form": "저번에 천둥칠때",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013387.1.10",
          "form": "진짜 크게 났었는데",
          "original_form": "진짜 크게 났었는데",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013387.1.11",
          "form": "우리도",
          "original_form": "우리도",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013387.1.12",
          "form": "진짜 빛이 먼저 번쩍하잖아",
          "original_form": "진짜 빛이 먼저 번쩍하잖아",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013387.1.13",
          "form": "겁나 쾅!",
          "original_form": "겁나 쾅!",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013387.1.14",
          "form": "그럼 맘의 준비를",
          "original_form": "그럼 맘의 준비를",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013387.1.15",
          "form": "거림",
          "original_form": "거림",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013387.1.16",
          "form": "한단말이야",
          "original_form": "한단말이야",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013387.1.17",
          "form": "근데도 깜짝 놀랬음",
          "original_form": "근데도 깜짝 놀랬음",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013387.1.18",
          "form": "가스폭발한줄",
          "original_form": "가스폭발한줄",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013387.1.19",
          "form": "난 토르 내려온 줄;",
          "original_form": "난 토르 내려온 줄;",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013387.1.20",
          "form": "토르내한",
          "original_form": "토르내한",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013387.1.21",
          "form": "콰광!",
          "original_form": "콰광!",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013387.1.22",
          "form": "이번주내내",
          "original_form": "이번주내내",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013387.1.23",
          "form": "일기예보 믿질몬하겠음",
          "original_form": "일기예보 믿질몬하겠음",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013387.1.24",
          "form": "비온대",
          "original_form": "비온대",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013387.1.25",
          "form": "또?",
          "original_form": "또?",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013387.1.26",
          "form": "그래도 믿어야지뭐",
          "original_form": "그래도 믿어야지뭐",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013387.1.27",
          "form": "가을장마랬음",
          "original_form": "가을장마랬음",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013387.1.28",
          "form": "빨리 가을와야 가디건을",
          "original_form": "빨리 가을와야 가디건을",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013387.1.29",
          "form": "입을텐데",
          "original_form": "입을텐데",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013387.1.30",
          "form": "어제 비오고나서 진짜덥던데",
          "original_form": "어제 비오고나서 진짜덥던데",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013387.1.31",
          "form": "ㅇㅇ저녁에 걍 더웠음",
          "original_form": "ㅇㅇ저녁에 걍 더웠음",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013387.1.32",
          "form": "찝찝",
          "original_form": "찝찝",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013387.1.33",
          "form": "오늘은 또 시원함",
          "original_form": "오늘은 또 시원함",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013387.1.34",
          "form": "근데 갑자기 에어컨틈",
          "original_form": "근데 갑자기 에어컨틈",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013387.1.35",
          "form": "나도어제에어컨틀었음",
          "original_form": "나도어제에어컨틀었음",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013387.1.36",
          "form": "너무 찝찝해서",
          "original_form": "너무 찝찝해서",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013387.1.37",
          "form": "아니 나는 지금 회사에서",
          "original_form": "아니 나는 지금 회사에서",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013387.1.38",
          "form": "아까 비오더니",
          "original_form": "아까 비오더니",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013387.1.39",
          "form": "이제안오네",
          "original_form": "이제안오네",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013387.1.40",
          "form": "에어컨틀고 방에 들어갔어 상무",
          "original_form": "에어컨틀고 방에 들어갔어 상무",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013387.1.41",
          "form": "머지",
          "original_form": "머지",
          "speaker_id": "1",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013387.1.42",
          "form": "니가 더울까봐",
          "original_form": "니가 더울까봐",
          "speaker_id": "1",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013387.1.43",
          "form": "배려한건가",
          "original_form": "배려한건가",
          "speaker_id": "1",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013387.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013387.1.45",
          "form": "걍 습해서 그런듯",
          "original_form": "걍 습해서 그런듯",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013387.1.46",
          "form": "그런듯",
          "original_form": "그런듯",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013387.1.47",
          "form": "습한건못참지",
          "original_form": "습한건못참지",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013387.1.48",
          "form": "그래서 모기가",
          "original_form": "그래서 모기가",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013387.1.49",
          "form": "나와 이제",
          "original_form": "나와 이제",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013387.1.50",
          "form": "끔찍",
          "original_form": "끔찍",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013387.1.51",
          "form": "근데 나는 잘 안물려",
          "original_form": "근데 나는 잘 안물려",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013387.1.52",
          "form": "모기한번도못봤어",
          "original_form": "모기한번도못봤어",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013387.1.53",
          "form": "거긴 아직 지옥이라 그런가?",
          "original_form": "거긴 아직 지옥이라 그런가?",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013387.1.54",
          "form": "날씨가",
          "original_form": "날씨가",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013387.1.55",
          "form": "그다지",
          "original_form": "그다지",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013387.1.56",
          "form": "덥진않은뎅",
          "original_form": "덥진않은뎅",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013387.1.57",
          "form": "다말라죽었나봐",
          "original_form": "다말라죽었나봐",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013387.1.58",
          "form": "그런가바",
          "original_form": "그런가바",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013387.1.59",
          "form": "오늘 일기예보는",
          "original_form": "오늘 일기예보는",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013387.1.60",
          "form": "좀낫구만",
          "original_form": "좀낫구만",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013387.1.61",
          "form": "여긴 하루종일",
          "original_form": "여긴 하루종일",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013387.1.62",
          "form": "흐려",
          "original_form": "흐려",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013387.1.63",
          "form": "번개도친다던데",
          "original_form": "번개도친다던데",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013387.1.64",
          "form": "난 양산들고왔는뎅",
          "original_form": "난 양산들고왔는뎅",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013387.1.65",
          "form": "아직번개는안침",
          "original_form": "아직번개는안침",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013387.1.66",
          "form": "여긴 걍 소나기",
          "original_form": "여긴 걍 소나기",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013387.1.67",
          "form": "여긴조금씩",
          "original_form": "여긴조금씩",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013387.1.68",
          "form": "쭉오다가",
          "original_form": "쭉오다가",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013387.1.69",
          "form": "이제안오는듯",
          "original_form": "이제안오는듯",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013387.1.70",
          "form": "비가 너무 시러",
          "original_form": "비가 너무 시러",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013387.1.71",
          "form": "근데 집에있을땐 좋아",
          "original_form": "근데 집에있을땐 좋아",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013387.1.72",
          "form": "옛날에는 좋아했는데",
          "original_form": "옛날에는 좋아했는데",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013387.1.73",
          "form": "지금은 너무 시렁",
          "original_form": "지금은 너무 시렁",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013387.1.74",
          "form": "집에있어도?",
          "original_form": "집에있어도?",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013387.1.75",
          "form": "근데 가디건안입나",
          "original_form": "근데 가디건안입나",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
