      "utterance": [
        {
          "id": "MDRW2100018405.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 01:12"
        },
        {
          "id": "MDRW2100018405.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210827 01:15"
        },
        {
          "id": "MDRW2100018405.1.3",
          "form": "혹시 학교 다니세요?",
          "original_form": "혹시 학교 다니세요?",
          "speaker_id": "1",
          "time": "20210827 01:15"
        },
        {
          "id": "MDRW2100018405.1.4",
          "form": "아니요. 저는 주부예요.학교다녔었죠ㅋ",
          "original_form": "아니요. 저는 주부예요.학교다녔었죠ㅋ",
          "speaker_id": "2",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018405.1.5",
          "form": "학교다니세요?",
          "original_form": "학교다니세요?",
          "speaker_id": "2",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018405.1.6",
          "form": "아아 그렇군요 아니요 ㅋㅋㅋㅋ졸업한지 꽤됬어요 ㅠ",
          "original_form": "아아 그렇군요 아니요 ㅋㅋㅋㅋ졸업한지 꽤됬어요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018405.1.7",
          "form": "학교다니실때 기억에 남는거 있나요?",
          "original_form": "학교다니실때 기억에 남는거 있나요?",
          "speaker_id": "1",
          "time": "20210827 01:17"
        },
        {
          "id": "MDRW2100018405.1.8",
          "form": "음 ㅋㅋ 대학교다닐때는 나름공부열심히하고 봉사활동다니고 알바다니고ㅋㅋ자격증따려고 열심히했던것같아요",
          "original_form": "음 ㅋㅋ 대학교다닐때는 나름공부열심히하고 봉사활동다니고 알바다니고ㅋㅋ자격증따려고 열심히했던것같아요",
          "speaker_id": "2",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100018405.1.9",
          "form": "중고등학생때는 신나게놀았죠. ㅋㅋ친구들이랑ㅋ",
          "original_form": "중고등학생때는 신나게놀았죠. ㅋㅋ친구들이랑ㅋ",
          "speaker_id": "2",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100018405.1.10",
          "form": "학교시절 기억에 남는거있으세요,?",
          "original_form": "학교시절 기억에 남는거있으세요,?",
          "speaker_id": "2",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100018405.1.11",
          "form": "비슷하네요 ㅋㅋㅋㅋ",
          "original_form": "비슷하네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100018405.1.12",
          "form": "음 그냥 공강때 여유롭게 친구랑 커피마시고 놀던거생각나네요 ㅋㅋㅋㅋ",
          "original_form": "음 그냥 공강때 여유롭게 친구랑 커피마시고 놀던거생각나네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100018405.1.13",
          "form": "동아리 들으셧어요?",
          "original_form": "동아리 들으셧어요?",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100018405.1.14",
          "form": "네 봉사동아리요",
          "original_form": "네 봉사동아리요",
          "speaker_id": "2",
          "time": "20210827 01:27"
        },
        {
          "id": "MDRW2100018405.1.15",
          "form": "아 그렇군요 봉사동아리에는 사람들 많지않아요?",
          "original_form": "아 그렇군요 봉사동아리에는 사람들 많지않아요?",
          "speaker_id": "1",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100018405.1.16",
          "form": "네 정말많았어요ㅋ",
          "original_form": "네 정말많았어요ㅋ",
          "speaker_id": "2",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100018405.1.17",
          "form": "무슨동아리 들어가셨어요?",
          "original_form": "무슨동아리 들어가셨어요?",
          "speaker_id": "2",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100018405.1.18",
          "form": "ㅋㅋㅋㅋ저는 동아리 안들었어요 ㅠ 나중에 후회돠긴하도라구요",
          "original_form": "ㅋㅋㅋㅋ저는 동아리 안들었어요 ㅠ 나중에 후회돠긴하도라구요",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018405.1.19",
          "form": "아는 사람들 많이 만들엇겟네요?",
          "original_form": "아는 사람들 많이 만들엇겟네요?",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018405.1.20",
          "form": "네네 동아리에서 아는사람많이만들었었어요ㅋ",
          "original_form": "네네 동아리에서 아는사람많이만들었었어요ㅋ",
          "speaker_id": "2",
          "time": "20210827 08:36"
        },
        {
          "id": "MDRW2100018405.1.21",
          "form": "ㅋㅋㅋㅋ동아리때 친구랑 아직 연락하세요?",
          "original_form": "ㅋㅋㅋㅋ동아리때 친구랑 아직 연락하세요?",
          "speaker_id": "1",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100018405.1.22",
          "form": "네 친한친구들이라서요ㅋ",
          "original_form": "네 친한친구들이라서요ㅋ",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100018405.1.23",
          "form": "아아 그때사귄친구들이 절친이 되셧나보네요 ㅋㅋㅋㅋ",
          "original_form": "아아 그때사귄친구들이 절친이 되셧나보네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100018405.1.24",
          "form": "네네 자주못봐서 아쉽지만 톡은 매일해요.ㅋ",
          "original_form": "네네 자주못봐서 아쉽지만 톡은 매일해요.ㅋ",
          "speaker_id": "2",
          "time": "20210828 14:07"
        }
      ]
    }
