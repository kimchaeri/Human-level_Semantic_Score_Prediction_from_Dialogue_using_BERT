      "utterance": [
        {
          "id": "MDRW2100002958.1.1",
          "form": "종교가 어떻게 되시나여",
          "original_form": "종교가 어떻게 되시나여",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002958.1.2",
          "form": "무교요",
          "original_form": "무교요",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002958.1.3",
          "form": "자도 무굡니다 같은 종교네요 반갑습니다",
          "original_form": "자도 무굡니다 같은 종교네요 반갑습니다",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002958.1.4",
          "form": "그렇네요~ 반가워요",
          "original_form": "그렇네요~ 반가워요",
          "speaker_id": "2",
          "time": "20210820 17:07"
        },
        {
          "id": "MDRW2100002958.1.5",
          "form": "서로 무교라서 할말이 없네요 \r\n종교를 갖고 싶었던적 없나요??",
          "original_form": "서로 무교라서 할말이 없네요 \r\n종교를 갖고 싶었던적 없나요??",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002958.1.6",
          "form": "어릴땐 기독교?친구가 있어서 교회 다닌적은 있었어요",
          "original_form": "어릴땐 기독교?친구가 있어서 교회 다닌적은 있었어요",
          "speaker_id": "2",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002958.1.7",
          "form": "근데 지금 딱히 종교가져야겠다는 생각은 없어서요",
          "original_form": "근데 지금 딱히 종교가져야겠다는 생각은 없어서요",
          "speaker_id": "2",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002958.1.8",
          "form": "저도 어릴적에 연필 받으러 갔던거 같네여 ㅋㅋ",
          "original_form": "저도 어릴적에 연필 받으러 갔던거 같네여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002958.1.9",
          "form": "맞아요 선물로~",
          "original_form": "맞아요 선물로~",
          "speaker_id": "2",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002958.1.10",
          "form": "지금른 코로나때매 교회가 욕을 하도 먹고 있으니까 갈 엄두도 안나네요",
          "original_form": "지금른 코로나때매 교회가 욕을 하도 먹고 있으니까 갈 엄두도 안나네요",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002958.1.11",
          "form": "그러죠~",
          "original_form": "그러죠~",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002958.1.12",
          "form": "요즘엔 어디 가기도 좀 그렇고 집콕이네요",
          "original_form": "요즘엔 어디 가기도 좀 그렇고 집콕이네요",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002958.1.13",
          "form": "화이팅 입니다",
          "original_form": "화이팅 입니다",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100002958.1.14",
          "form": "부활절에 교회가면 꿀재미었는데 달란트로 중고거래도 하고",
          "original_form": "부활절에 교회가면 꿀재미었는데 달란트로 중고거래도 하고",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100002958.1.15",
          "form": "와~ 맞아요 어릴때 참여하면 전 달란트로 물건사는거 했거든요(시장)",
          "original_form": "와~ 맞아요 어릴때 참여하면 전 달란트로 물건사는거 했거든요(시장)",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100002958.1.16",
          "form": "맞아요 ㅋㅋ 요즘은 그런것도 없고 정이 너무 없어",
          "original_form": "맞아요 ㅋㅋ 요즘은 그런것도 없고 정이 너무 없어",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002958.1.17",
          "form": "어릴땐 재미있게 해서 더 그런가봐요~ㅎ",
          "original_form": "어릴땐 재미있게 해서 더 그런가봐요~ㅎ",
          "speaker_id": "2",
          "time": "20210820 17:36"
        }
