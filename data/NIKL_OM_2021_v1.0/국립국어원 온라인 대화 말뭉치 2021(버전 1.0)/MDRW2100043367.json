      "utterance": [
        {
          "id": "MDRW2100043367.1.1",
          "form": "재난지원금으로 뭐 샀어요?",
          "original_form": "재난지원금으로 뭐 샀어요?",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043367.1.2",
          "form": "먹을거?",
          "original_form": "먹을거?",
          "speaker_id": "2",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043367.1.3",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043367.1.4",
          "form": "마라탕먹었어요",
          "original_form": "마라탕먹었어요",
          "speaker_id": "2",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043367.1.5",
          "form": "아 저도 빵사먹었어요",
          "original_form": "아 저도 빵사먹었어요",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043367.1.6",
          "form": "맛있는게 최고죠",
          "original_form": "맛있는게 최고죠",
          "speaker_id": "2",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043367.1.7",
          "form": "신발이랑 렌즈도 사긴 할떠지만요",
          "original_form": "신발이랑 렌즈도 사긴 할떠지만요",
          "speaker_id": "1",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043367.1.8",
          "form": "친구는 미용실 간다더라구요",
          "original_form": "친구는 미용실 간다더라구요",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043367.1.9",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043367.1.10",
          "form": "저는 렌즈사려구요",
          "original_form": "저는 렌즈사려구요",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043367.1.11",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043367.1.12",
          "form": "그냥 데일리렌즈로",
          "original_form": "그냥 데일리렌즈로",
          "speaker_id": "2",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043367.1.13",
          "form": "무난한 베이지색 사려구요",
          "original_form": "무난한 베이지색 사려구요",
          "speaker_id": "2",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043367.1.14",
          "form": "렌즈는 아니지만 지금 올영 세일하잖아요",
          "original_form": "렌즈는 아니지만 지금 올영 세일하잖아요",
          "speaker_id": "1",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043367.1.15",
          "form": "맞아요 저도 뭐좀 살까 생각중",
          "original_form": "맞아요 저도 뭐좀 살까 생각중",
          "speaker_id": "2",
          "time": "20210907 18:25"
        }
