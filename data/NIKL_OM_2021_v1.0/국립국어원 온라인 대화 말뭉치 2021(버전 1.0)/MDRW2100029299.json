      "utterance": [
        {
          "id": "MDRW2100029299.1.1",
          "form": "언니가 등짝 스매싱날리겠서요",
          "original_form": "언니가 등짝 스매싱날리겠서요",
          "speaker_id": "2",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029299.1.2",
          "form": "아 맞는편입니다",
          "original_form": "아 맞는편입니다",
          "speaker_id": "2",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029299.1.3",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029299.1.4",
          "form": "영업도 해보고 싶어요",
          "original_form": "영업도 해보고 싶어요",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029299.1.5",
          "form": "잘할것 같은데",
          "original_form": "잘할것 같은데",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029299.1.6",
          "form": "사람만나서 하는것들",
          "original_form": "사람만나서 하는것들",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029299.1.7",
          "form": "아 완전 외향형이시군요ㅎㅎ 정말",
          "original_form": "아 완전 외향형이시군요ㅎㅎ 정말",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029299.1.8",
          "form": "저요...저는 완전 내향적 인간입니다!",
          "original_form": "저요...저는 완전 내향적 인간입니다!",
          "speaker_id": "1",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029299.1.9",
          "form": "재택근무 평생 할수도 있을텐데ㅜ 안시켜줘서 그렇죠",
          "original_form": "재택근무 평생 할수도 있을텐데ㅜ 안시켜줘서 그렇죠",
          "speaker_id": "1",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029299.1.10",
          "form": "아 재택못하겠서요",
          "original_form": "아 재택못하겠서요",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029299.1.11",
          "form": "그 등대지기 아시나요",
          "original_form": "그 등대지기 아시나요",
          "speaker_id": "1",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029299.1.12",
          "form": "소설이요?",
          "original_form": "소설이요?",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029299.1.13",
          "form": "소설 본거 같은데",
          "original_form": "소설 본거 같은데",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029299.1.14",
          "form": "아뇨 직업ㅎㅎ",
          "original_form": "아뇨 직업ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029299.1.15",
          "form": "실제 등대지기 알져 ㅋㅋ공무원이래여",
          "original_form": "실제 등대지기 알져 ㅋㅋ공무원이래여",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029299.1.16",
          "form": "연봉 세거든요ㅎ",
          "original_form": "연봉 세거든요ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029299.1.17",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029299.1.18",
          "form": "오호라",
          "original_form": "오호라",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029299.1.19",
          "form": "전 인터넷만 빵빵 잘 터지면 거기가 적성입니다",
          "original_form": "전 인터넷만 빵빵 잘 터지면 거기가 적성입니다",
          "speaker_id": "1",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029299.1.20",
          "form": "바닷가 외롭지 않을까요?",
          "original_form": "바닷가 외롭지 않을까요?",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029299.1.21",
          "form": "고독을 철근처럼 씹어먹으며 배 두들기고 잘 살 수 있어요",
          "original_form": "고독을 철근처럼 씹어먹으며 배 두들기고 잘 살 수 있어요",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029299.1.22",
          "form": "무슨느낌인지 알겠습니다",
          "original_form": "무슨느낌인지 알겠습니다",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029299.1.23",
          "form": "저 요즘",
          "original_form": "저 요즘",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029299.1.24",
          "form": "혼자 있고 싶어요",
          "original_form": "혼자 있고 싶어요",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029299.1.25",
          "form": "아무것도 안하고",
          "original_form": "아무것도 안하고",
          "speaker_id": "2",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029299.1.26",
          "form": "그냥 아무것도 안하고 가만히 있고싶습니다 ㅋㅋ",
          "original_form": "그냥 아무것도 안하고 가만히 있고싶습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029299.1.27",
          "form": "번아웃인가요ㅜ",
          "original_form": "번아웃인가요ㅜ",
          "speaker_id": "1",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029299.1.28",
          "form": "아님 천사님도 좋지만 가끔은 혼자있고 싶어서?",
          "original_form": "아님 천사님도 좋지만 가끔은 혼자있고 싶어서?",
          "speaker_id": "1",
          "time": "20210903 01:17"
        }
      ]
    }
