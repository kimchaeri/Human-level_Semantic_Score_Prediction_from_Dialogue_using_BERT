      "utterance": [
        {
          "id": "MDRW2100019832.1.1",
          "form": "제출가능해서 바꿨습니다!",
          "original_form": "제출가능해서 바꿨습니다!",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019832.1.2",
          "form": "요즘 뭐보세요? 전 악마판사 보고싶네요 ㅎㅎ",
          "original_form": "요즘 뭐보세요? 전 악마판사 보고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019832.1.3",
          "form": "펜트하우스 시즌3 보고있는데 내용이 점점...",
          "original_form": "펜트하우스 시즌3 보고있는데 내용이 점점...",
          "speaker_id": "1",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019832.1.4",
          "form": "오...ㅋㅋㅋ 왜요 어떤데요",
          "original_form": "오...ㅋㅋㅋ 왜요 어떤데요",
          "speaker_id": "2",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019832.1.5",
          "form": "막장인가요?",
          "original_form": "막장인가요?",
          "speaker_id": "2",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019832.1.6",
          "form": "네 시즌2까지는 재밌었어요",
          "original_form": "네 시즌2까지는 재밌었어요",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019832.1.7",
          "form": "시즌 3이 끝이에요?",
          "original_form": "시즌 3이 끝이에요?",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019832.1.8",
          "form": "아니면 더있나요 앞으로도",
          "original_form": "아니면 더있나요 앞으로도",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019832.1.9",
          "form": "3까지 끝이예요",
          "original_form": "3까지 끝이예요",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019832.1.10",
          "form": "내용이 너무 말도안되게 가요... 의리로 보고있어요",
          "original_form": "내용이 너무 말도안되게 가요... 의리로 보고있어요",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019832.1.11",
          "form": "결말이 기대되네요",
          "original_form": "결말이 기대되네요",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019832.1.12",
          "form": "맞아요 끝은 봐야죠 ㅎㅎ",
          "original_form": "맞아요 끝은 봐야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019832.1.13",
          "form": "드라마 괴물이 정말 재밌었어요",
          "original_form": "드라마 괴물이 정말 재밌었어요",
          "speaker_id": "1",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019832.1.14",
          "form": "오 저도 그거 좀 드문드문 봤어요",
          "original_form": "오 저도 그거 좀 드문드문 봤어요",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019832.1.15",
          "form": "신하균 여진구 맞죠?",
          "original_form": "신하균 여진구 맞죠?",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019832.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019832.1.17",
          "form": "여진구 보러갔다가 신하균에 반하게 되던 드라마",
          "original_form": "여진구 보러갔다가 신하균에 반하게 되던 드라마",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019832.1.18",
          "form": "둘다 좋아요ㅎㅎ 카리스마 있고",
          "original_form": "둘다 좋아요ㅎㅎ 카리스마 있고",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019832.1.19",
          "form": "여진구분 목소리가 정말 매력있죠!",
          "original_form": "여진구분 목소리가 정말 매력있죠!",
          "speaker_id": "1",
          "time": "20210827 22:44"
        }
      ]
