      "utterance": [
        {
          "id": "MDRW2100001082.1.1",
          "form": "형제 자매가 어떻게 되세요?",
          "original_form": "형제 자매가 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210723 11:23"
        },
        {
          "id": "MDRW2100001082.1.2",
          "form": "저는 형있어요",
          "original_form": "저는 형있어요",
          "speaker_id": "1",
          "time": "20210723 11:34"
        },
        {
          "id": "MDRW2100001082.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210723 11:34"
        },
        {
          "id": "MDRW2100001082.1.4",
          "form": "아 그렇군요 저는 언니랑 동생 있어요!",
          "original_form": "아 그렇군요 저는 언니랑 동생 있어요!",
          "speaker_id": "2",
          "time": "20210723 11:36"
        },
        {
          "id": "MDRW2100001082.1.5",
          "form": "잘 안싸워요? ㅋㅋㅋ",
          "original_form": "잘 안싸워요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 11:37"
        },
        {
          "id": "MDRW2100001082.1.6",
          "form": "어우 옛날엔 말도 못했어요",
          "original_form": "어우 옛날엔 말도 못했어요",
          "speaker_id": "2",
          "time": "20210723 11:37"
        },
        {
          "id": "MDRW2100001082.1.7",
          "form": "말 하기만 하면 싸우니까ㅋㅋㅎㅎㅋㅋㅋ",
          "original_form": "말 하기만 하면 싸우니까ㅋㅋㅎㅎㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 11:37"
        },
        {
          "id": "MDRW2100001082.1.8",
          "form": "ㅋㅋㅋㅋㅋ동생은 여동생이에요?",
          "original_form": "ㅋㅋㅋㅋㅋ동생은 여동생이에요?",
          "speaker_id": "1",
          "time": "20210723 11:38"
        },
        {
          "id": "MDRW2100001082.1.9",
          "form": "네! 세자매입니다",
          "original_form": "네! 세자매입니다",
          "speaker_id": "2",
          "time": "20210723 11:39"
        },
        {
          "id": "MDRW2100001082.1.10",
          "form": "완전 딸부자집ㅋㅋㅋㅋ",
          "original_form": "완전 딸부자집ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 11:39"
        },
        {
          "id": "MDRW2100001082.1.11",
          "form": "ㅋㅋㅋㅋ그러네요",
          "original_form": "ㅋㅋㅋㅋ그러네요",
          "speaker_id": "1",
          "time": "20210723 11:41"
        },
        {
          "id": "MDRW2100001082.1.12",
          "form": "그래도 친구들 보니까",
          "original_form": "그래도 친구들 보니까",
          "speaker_id": "1",
          "time": "20210723 11:41"
        },
        {
          "id": "MDRW2100001082.1.13",
          "form": "나중엔 자매가 사이 엄청 좋더라구요",
          "original_form": "나중엔 자매가 사이 엄청 좋더라구요",
          "speaker_id": "1",
          "time": "20210723 11:41"
        },
        {
          "id": "MDRW2100001082.1.14",
          "form": "맞아요 그건 있는 것 같아요",
          "original_form": "맞아요 그건 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210723 11:41"
        },
        {
          "id": "MDRW2100001082.1.15",
          "form": "언니 화장품이랑 옷 빌리기도 하고ㅎㅎ",
          "original_form": "언니 화장품이랑 옷 빌리기도 하고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210723 11:42"
        },
        {
          "id": "MDRW2100001082.1.16",
          "form": "언니도 제꺼 빌리면서 서로 그렇게 친해지는 것 같아요",
          "original_form": "언니도 제꺼 빌리면서 서로 그렇게 친해지는 것 같아요",
          "speaker_id": "2",
          "time": "20210723 11:42"
        },
        {
          "id": "MDRW2100001082.1.17",
          "form": "name1님은 형이랑 안싸워요?",
          "original_form": "&name1&님은 형이랑 안싸워요?",
          "speaker_id": "2",
          "time": "20210723 11:42"
        },
        {
          "id": "MDRW2100001082.1.18",
          "form": "예전엔 싸웠는데",
          "original_form": "예전엔 싸웠는데",
          "speaker_id": "1",
          "time": "20210723 11:43"
        },
        {
          "id": "MDRW2100001082.1.19",
          "form": "지금은 필요할 때만 대화해요 ㅋㅋㅋㅋ",
          "original_form": "지금은 필요할 때만 대화해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 11:43"
        },
        {
          "id": "MDRW2100001082.1.20",
          "form": "아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 11:43"
        },
        {
          "id": "MDRW2100001082.1.21",
          "form": "완전 전형적인 형제네요",
          "original_form": "완전 전형적인 형제네요",
          "speaker_id": "2",
          "time": "20210723 11:43"
        },
        {
          "id": "MDRW2100001082.1.22",
          "form": "그쵸 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋ",
