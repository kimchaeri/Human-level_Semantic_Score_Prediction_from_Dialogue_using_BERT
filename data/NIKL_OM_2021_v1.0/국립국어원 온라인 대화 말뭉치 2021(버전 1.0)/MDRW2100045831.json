      "utterance": [
        {
          "id": "MDRW2100045831.1.1",
          "form": "네 정말 좋아해요",
          "original_form": "네 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045831.1.2",
          "form": "드라마는요",
          "original_form": "드라마는요",
          "speaker_id": "1",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045831.1.3",
          "form": "드라마도 참 좋죠",
          "original_form": "드라마도 참 좋죠",
          "speaker_id": "2",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045831.1.4",
          "form": "음악 듣는 것도 좋아하시나요",
          "original_form": "음악 듣는 것도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045831.1.5",
          "form": "그럼요 음악이 인생의 활력소죠",
          "original_form": "그럼요 음악이 인생의 활력소죠",
          "speaker_id": "2",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045831.1.6",
          "form": "저도 영상 음원 다 좋아해요",
          "original_form": "저도 영상 음원 다 좋아해요",
          "speaker_id": "1",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045831.1.7",
          "form": "최근에 본 것 중에 제일 좋았던게 뭐예요",
          "original_form": "최근에 본 것 중에 제일 좋았던게 뭐예요",
          "speaker_id": "2",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045831.1.8",
          "form": "저 다 좋아서",
          "original_form": "저 다 좋아서",
          "speaker_id": "1",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045831.1.9",
          "form": "미드 언두잉이라고 봤는데",
          "original_form": "미드 언두잉이라고 봤는데",
          "speaker_id": "1",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045831.1.10",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "1",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045831.1.11",
          "form": "처음 들어보네요",
          "original_form": "처음 들어보네요",
          "speaker_id": "2",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045831.1.12",
          "form": "아 그리고 보고 싶은게 있어요",
          "original_form": "아 그리고 보고 싶은게 있어요",
          "speaker_id": "1",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045831.1.13",
          "form": "뭐요?",
          "original_form": "뭐요?",
          "speaker_id": "2",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045831.1.14",
          "form": "요즘 넷플릭스에서 광고하던데",
          "original_form": "요즘 넷플릭스에서 광고하던데",
          "speaker_id": "1",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045831.1.15",
          "form": "킹덤?",
          "original_form": "킹덤?",
          "speaker_id": "2",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045831.1.16",
          "form": "아니요 킹덤 말고",
          "original_form": "아니요 킹덤 말고",
          "speaker_id": "1",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045831.1.17",
          "form": "오징어 게임이요",
          "original_form": "오징어 게임이요",
          "speaker_id": "1",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045831.1.18",
          "form": "처음 듣네요",
          "original_form": "처음 듣네요",
          "speaker_id": "2",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100045831.1.19",
          "form": "이정재가 나오더라고요",
          "original_form": "이정재가 나오더라고요",
          "speaker_id": "1",
          "time": "20210908 07:47"
        }
      ]
