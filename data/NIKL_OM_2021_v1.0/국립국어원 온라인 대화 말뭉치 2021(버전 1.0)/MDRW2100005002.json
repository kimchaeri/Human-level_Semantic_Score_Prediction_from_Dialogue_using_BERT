      "utterance": [
        {
          "id": "MDRW2100005002.1.1",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100005002.1.2",
          "form": "여행 가고싶으세요?",
          "original_form": "여행 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100005002.1.3",
          "form": "네 여행가고싶어요ㅠ",
          "original_form": "네 여행가고싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005002.1.4",
          "form": "어디로요?",
          "original_form": "어디로요?",
          "speaker_id": "2",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005002.1.5",
          "form": "제주도가고싶어요ㅜㅜ푸른바다",
          "original_form": "제주도가고싶어요ㅜㅜ푸른바다",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005002.1.6",
          "form": "님 최근에 가지않았나요?",
          "original_form": "님 최근에 가지않았나요?",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005002.1.7",
          "form": "마자요 하지만 마음같아서는 한 2주간있고싶어요",
          "original_form": "마자요 하지만 마음같아서는 한 2주간있고싶어요",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005002.1.8",
          "form": "마즘마즘 인정킹정",
          "original_form": "마즘마즘 인정킹정",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005002.1.9",
          "form": "제주도 한달살기 하고픔",
          "original_form": "제주도 한달살기 하고픔",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005002.1.10",
          "form": "2주~3달쯤 쉬고싶어용",
          "original_form": "2주~3달쯤 쉬고싶어용",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005002.1.11",
          "form": "마자요 근데 고양이땜에못하겠음ㅜㅜ",
          "original_form": "마자요 근데 고양이땜에못하겠음ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005002.1.12",
          "form": "아 그건 그렇겟다 님 혼자사니깐",
          "original_form": "아 그건 그렇겟다 님 혼자사니깐",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005002.1.13",
          "form": "맞아요.... 고양이를두곤갈수없어...",
          "original_form": "맞아요.... 고양이를두곤갈수없어...",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005002.1.14",
          "form": "그래서 해외여행도못가여",
          "original_form": "그래서 해외여행도못가여",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005002.1.15",
          "form": "고양이 보살펴줄 친구는...",
          "original_form": "고양이 보살펴줄 친구는...",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005002.1.16",
          "form": "근데 제주도는 어케갓어요",
          "original_form": "근데 제주도는 어케갓어요",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005002.1.17",
          "form": "1박2일이라 친구가 저녁에와서 놀아주고갔어용",
          "original_form": "1박2일이라 친구가 저녁에와서 놀아주고갔어용",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005002.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005002.1.19",
          "form": "전 최근에 부산 간 게 마지막이네요 6월에",
          "original_form": "전 최근에 부산 간 게 마지막이네요 6월에",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005002.1.20",
          "form": "오 휴가로가신건가요?",
          "original_form": "오 휴가로가신건가요?",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005002.1.21",
          "form": "출장 겸 휴가요!",
          "original_form": "출장 겸 휴가요!",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100005002.1.22",
          "form": "맛있는건많이먹었나요",
          "original_form": "맛있는건많이먹었나요",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100005002.1.23",
          "form": "네 넘 좋았죠 전부 다 맛있었고... 님은 제주도서 젤 맛잇던 게 뭐예요",
          "original_form": "네 넘 좋았죠 전부 다 맛있었고... 님은 제주도서 젤 맛잇던 게 뭐예요",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100005002.1.24",
          "form": "저는.... 고기국수요 그리고 대게장김밥튀김이요",
          "original_form": "저는.... 고기국수요 그리고 대게장김밥튀김이요",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100005002.1.25",
          "form": "제주도에서 원래 고기국수 팔아요??",
          "original_form": "제주도에서 원래 고기국수 팔아요??",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100005002.1.26",
          "form": "오...대게장김밥까지만 해도 맛있을 것ㅊ같은데 그걸 또 튀겼다...?",
          "original_form": "오...대게장김밥까지만 해도 맛있을 것ㅊ같은데 그걸 또 튀겼다...?",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100005002.1.27",
          "form": "진짜 엄청맛있어요",
          "original_form": "진짜 엄청맛있어요",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100005002.1.28",
          "form": "맛잇ㅇㅅ겡다 진짜......",
          "original_form": "맛잇ㅇㅅ겡다 진짜......",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100005002.1.29",
          "form": "네 고기국수파는데 진짜 최고예요...거기다 제주도김치까지 같이먹으면 최고임",
          "original_form": "네 고기국수파는데 진짜 최고예요...거기다 제주도김치까지 같이먹으면 최고임",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100005002.1.30",
          "form": "맛없으면 불법",
          "original_form": "맛없으면 불법",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100005002.1.31",
          "form": "제주도김치는 머가달라요??",
          "original_form": "제주도김치는 머가달라요??",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100005002.1.32",
          "form": "뭔가 유자를 넣은거같아요",
          "original_form": "뭔가 유자를 넣은거같아요",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100005002.1.33",
          "form": "유자맛이나요...",
          "original_form": "유자맛이나요...",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100005002.1.34",
          "form": "잉?ㅋㅋㅋ특이하네",
          "original_form": "잉?ㅋㅋㅋ특이하네",
          "speaker_id": "2",
          "time": "20210822 00:50"
        }
      ]
    }
