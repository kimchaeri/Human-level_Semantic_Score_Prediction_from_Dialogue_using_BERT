      "utterance": [
        {
          "id": "MDRW2100022663.1.1",
          "form": "/20221 을 채팅창에 쓰거나",
          "original_form": "/20221 을 채팅창에 쓰거나",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022663.1.2",
          "form": "커맨더가 몇개 더 있었음",
          "original_form": "커맨더가 몇개 더 있었음",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022663.1.3",
          "form": "현실이긴 한데 채팅창을 열거자 하면 열리는 그럼??",
          "original_form": "현실이긴 한데 채팅창을 열거자 하면 열리는 그럼??",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022663.1.4",
          "form": "펜트하우스 가는 법이요??",
          "original_form": "펜트하우스 가는 법이요??",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022663.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022663.1.6",
          "form": "그걸 채팅창에 치니까",
          "original_form": "그걸 채팅창에 치니까",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022663.1.7",
          "form": "머그런...",
          "original_form": "머그런...",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022663.1.8",
          "form": "그냥 순간이동되는거에여",
          "original_form": "그냥 순간이동되는거에여",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022663.1.9",
          "form": "그래서 반애들한테 가서",
          "original_form": "그래서 반애들한테 가서",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022663.1.10",
          "form": "애들아 이거 우리끼리 가서 놀자고",
          "original_form": "애들아 이거 우리끼리 가서 놀자고",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022663.1.11",
          "form": "아까 내가 분위기 망쳐놧는데 애들 갑자기",
          "original_form": "아까 내가 분위기 망쳐놧는데 애들 갑자기",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022663.1.12",
          "form": "솔깃해가지고",
          "original_form": "솔깃해가지고",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022663.1.13",
          "form": "막 첨엔 떨떠름해하다가",
          "original_form": "막 첨엔 떨떠름해하다가",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022663.1.14",
          "form": "뭔지도 모르고 그래도 일단",
          "original_form": "뭔지도 모르고 그래도 일단",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022663.1.15",
          "form": "가자는거에 동의하는거임",
          "original_form": "가자는거에 동의하는거임",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022663.1.16",
          "form": "그래서 다 같이",
          "original_form": "그래서 다 같이",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022663.1.17",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022663.1.18",
          "form": "펜트하우스로 순간이동 햇는데",
          "original_form": "펜트하우스로 순간이동 햇는데",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022663.1.19",
          "form": "진짜 잘곳도많고",
          "original_form": "진짜 잘곳도많고",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022663.1.20",
          "form": "무슨 호텔로 수학여행온기분인거임 근데",
          "original_form": "무슨 호텔로 수학여행온기분인거임 근데",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022663.1.21",
          "form": "시설이 진짜 넓도 좋고",
          "original_form": "시설이 진짜 넓도 좋고",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.22",
          "form": "간식도",
          "original_form": "간식도",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.23",
          "form": "그냥 빵 이런거 줄 줄 알앗는데",
          "original_form": "그냥 빵 이런거 줄 줄 알앗는데",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.24",
          "form": "고급 베이커리 디저트 같은거",
          "original_form": "고급 베이커리 디저트 같은거",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.25",
          "form": "준비되어있고",
          "original_form": "준비되어있고",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.26",
          "form": "애드링 신나서 반 분위기가 갑자기 너무 좋아지고",
          "original_form": "애드링 신나서 반 분위기가 갑자기 너무 좋아지고",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.27",
          "form": "대동단결된거에요",
          "original_form": "대동단결된거에요",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.28",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.29",
          "form": "애들=끼리 노래하고 춤추고 먹고",
          "original_form": "애들=끼리 노래하고 춤추고 먹고",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.30",
          "form": "사이다 진짜 좋아짐",
          "original_form": "사이다 진짜 좋아짐",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.31",
          "form": "올",
          "original_form": "올",
          "speaker_id": "1",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.32",
          "form": "근데 저는 애들 놀동안",
          "original_form": "근데 저는 애들 놀동안",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.33",
          "form": "짐 풀어놓고",
          "original_form": "짐 풀어놓고",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022663.1.34",
          "form": "저는 ㅋ 잘때 쓰는 1미터 넘는 베개가",
          "original_form": "저는 ㅋ 잘때 쓰는 1미터 넘는 베개가",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022663.1.35",
          "form": "옆구리에 갑자기 생겨잇어서",
          "original_form": "옆구리에 갑자기 생겨잇어서",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022663.1.36",
          "form": "그거 짐 두는 방에 놓고 옴.. ㅋㅋㅋㅋ",
          "original_form": "그거 짐 두는 방에 놓고 옴.. ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022663.1.37",
          "form": "아무튼",
          "original_form": "아무튼",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022663.1.38",
          "form": "아웃겨",
          "original_form": "아웃겨",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022663.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022663.1.40",
          "form": "애들한테 나 잠깐 다시 학교로 내려갓다올게라고 하고 혼자 다시 커맨더 쓰고 학교로감",
          "original_form": "애들한테 나 잠깐 다시 학교로 내려갓다올게라고 하고 혼자 다시 커맨더 쓰고 학교로감",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022663.1.41",
          "form": "왜내려간다고 표현햇냐면",
          "original_form": "왜내려간다고 표현햇냐면",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022663.1.42",
          "form": "펜트하유스에 몇가지 직접적인 문이 있는데",
          "original_form": "펜트하유스에 몇가지 직접적인 문이 있는데",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022663.1.43",
          "form": "그게 바닥에 있고",
          "original_form": "그게 바닥에 있고",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022663.1.44",
          "form": "바닥 여니까 하늘이랑 사다리가 있었어요",
          "original_form": "바닥 여니까 하늘이랑 사다리가 있었어요",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022663.1.45",
          "form": "그래서 여기가 하늘 위라고 생각햇음",
          "original_form": "그래서 여기가 하늘 위라고 생각햇음",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022663.1.46",
          "form": "암튼 근데 제가 학교로 돌아가고",
          "original_form": "암튼 근데 제가 학교로 돌아가고",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022663.1.47",
          "form": "갑자기 시점이",
          "original_form": "갑자기 시점이",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022663.1.48",
          "form": "올",
          "original_form": "올",
          "speaker_id": "1",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022663.1.49",
          "form": "학교에 있는 모두를 비추는 앵글이되어서",
          "original_form": "학교에 있는 모두를 비추는 앵글이되어서",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022663.1.50",
          "form": "학교에 남은 다른 사람들이 보이는거임",
          "original_form": "학교에 남은 다른 사람들이 보이는거임",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022663.1.51",
          "form": "근데 소문이 나버린거임",
          "original_form": "근데 소문이 나버린거임",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022663.1.52",
          "form": "1반 애들 뭐 어디 놀러가서 빵먹고잇다는데? ㅏ면서",
          "original_form": "1반 애들 뭐 어디 놀러가서 빵먹고잇다는데? ㅏ면서",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022663.1.53",
          "form": "다들 그 커맨더를 알아내겟다고",
          "original_form": "다들 그 커맨더를 알아내겟다고",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022663.1.54",
          "form": "수업시작했는데 복도 뛰어다니고",
          "original_form": "수업시작했는데 복도 뛰어다니고",
          "speaker_id": "2",
          "time": "20210829 16:53"
        }
      ]
