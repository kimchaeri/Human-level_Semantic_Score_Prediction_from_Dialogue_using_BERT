      "utterance": [
        {
          "id": "MDRW2100029011.1.1",
          "form": "고슴도치 귀엽지",
          "original_form": "고슴도치 귀엽지",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029011.1.2",
          "form": "맞아!!",
          "original_form": "맞아!!",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029011.1.3",
          "form": "귀엽겟다 ㅋㅋㅋㅋ",
          "original_form": "귀엽겟다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029011.1.4",
          "form": "고슴도치는 무슨 사료 먹을까?",
          "original_form": "고슴도치는 무슨 사료 먹을까?",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029011.1.5",
          "form": "고슴도치도 코코볼같이 생긴거 먹지 않을까?",
          "original_form": "고슴도치도 코코볼같이 생긴거 먹지 않을까?",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029011.1.6",
          "form": "흠.. 햄스터밥같은거?",
          "original_form": "흠.. 햄스터밥같은거?",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029011.1.7",
          "form": "아니면 과일 먹거나ㅋㅋ",
          "original_form": "아니면 과일 먹거나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029011.1.8",
          "form": "오!! 그러면 식비가 만만찮겠다",
          "original_form": "오!! 그러면 식비가 만만찮겠다",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.9",
          "form": "고슴도치말구 토끼를 키우는게 낫겠어",
          "original_form": "고슴도치말구 토끼를 키우는게 낫겠어",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.10",
          "form": "사람보단 덜 나가겠지",
          "original_form": "사람보단 덜 나가겠지",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.11",
          "form": "아 ㅋㅋ 그러네",
          "original_form": "아 ㅋㅋ 그러네",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.12",
          "form": "토끼가 고슴도치보다 훨씬 많이 먹을걸?",
          "original_form": "토끼가 고슴도치보다 훨씬 많이 먹을걸?",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.13",
          "form": "게다가 전선같은것도",
          "original_form": "게다가 전선같은것도",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.14",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.15",
          "form": "갉아먹을 수 잇어 토끼는",
          "original_form": "갉아먹을 수 잇어 토끼는",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.16",
          "form": "이런.....",
          "original_form": "이런.....",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.17",
          "form": "그래두 귀여우니까 용서가능 ㅎㅎ",
          "original_form": "그래두 귀여우니까 용서가능 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.18",
          "form": "그냥 강아지를 키우는게 낫겠어",
          "original_form": "그냥 강아지를 키우는게 낫겠어",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.20",
          "form": "글킨하지 ㅋㅋ",
          "original_form": "글킨하지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.21",
          "form": "강아지가 최고야",
          "original_form": "강아지가 최고야",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.22",
          "form": "너를 키워주고 잇는 걸로 감사해",
          "original_form": "너를 키워주고 잇는 걸로 감사해",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029011.1.23",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029011.1.24",
          "form": "알겠어",
          "original_form": "알겠어",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029011.1.25",
          "form": "ㅜㅡㅜ",
          "original_form": "ㅜㅡㅜ",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029011.1.26",
          "form": "내가 전선 안갉아먹는것도 감사해",
          "original_form": "내가 전선 안갉아먹는것도 감사해",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029011.1.27",
          "form": "뱀같은거 키우는거에 대해서",
          "original_form": "뱀같은거 키우는거에 대해서",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100029011.1.28",
          "form": "어떻게생각해?",
          "original_form": "어떻게생각해?",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100029011.1.29",
          "form": "희귀한 동물말야",
          "original_form": "희귀한 동물말야",
