        },
        {
          "id": "MDRW2100029948.1.3",
          "form": "업무 스트레스를",
          "original_form": "업무 스트레스를",
          "speaker_id": "1",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029948.1.4",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029948.1.5",
          "form": "잘 해소하기보다는",
          "original_form": "잘 해소하기보다는",
          "speaker_id": "1",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029948.1.6",
          "form": "애당초 잘 안 받는",
          "original_form": "애당초 잘 안 받는",
          "speaker_id": "1",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029948.1.7",
          "form": "쪽인지라...",
          "original_form": "쪽인지라...",
          "speaker_id": "1",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029948.1.8",
          "form": "아 성격이 그래서",
          "original_form": "아 성격이 그래서",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029948.1.9",
          "form": "3년째 신입이신..",
          "original_form": "3년째 신입이신..",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029948.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029948.1.11",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029948.1.12",
          "form": "담아두지 않으시나봐요",
          "original_form": "담아두지 않으시나봐요",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029948.1.13",
          "form": "신입은 틀려도 돼 마인드를 3년째 가지면",
          "original_form": "신입은 틀려도 돼 마인드를 3년째 가지면",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029948.1.14",
          "form": "됩니다",
          "original_form": "됩니다",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029948.1.15",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029948.1.16",
          "form": "너무 웃겨욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 웃겨욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029948.1.17",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "2",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100029948.1.18",
          "form": "좋은 마인드에요!!",
          "original_form": "좋은 마인드에요!!",
          "speaker_id": "2",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100029948.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029948.1.20",
          "form": "알바해보셨어요?",
          "original_form": "알바해보셨어요?",
          "speaker_id": "2",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100029948.1.21",
          "form": "아 알바요",
          "original_form": "아 알바요",
          "speaker_id": "1",
          "time": "20210903 17:05"
        },
        {
          "id": "MDRW2100029948.1.22",
          "form": "과외부터",
          "original_form": "과외부터",
          "speaker_id": "1",
          "time": "20210903 17:05"
        },
        {
          "id": "MDRW2100029948.1.23",
          "form": "막노동까지",
          "original_form": "막노동까지",
          "speaker_id": "1",
          "time": "20210903 17:05"
        },
        {
          "id": "MDRW2100029948.1.24",
          "form": "해봤네요",
          "original_form": "해봤네요",
          "speaker_id": "1",
          "time": "20210903 17:05"
        },
        {
          "id": "MDRW2100029948.1.25",
          "form": "와우 과외는 국어하셨나요?",
          "original_form": "와우 과외는 국어하셨나요?",
          "speaker_id": "2",
          "time": "20210903 17:06"
        },
        {
          "id": "MDRW2100029948.1.26",
          "form": "놀랍게도 과외는",
          "original_form": "놀랍게도 과외는",
          "speaker_id": "1",
          "time": "20210903 17:07"
        },
        {
          "id": "MDRW2100029948.1.27",
          "form": "역사 했어요",
          "original_form": "역사 했어요",
          "speaker_id": "1",
          "time": "20210903 17:07"
        },
        {
          "id": "MDRW2100029948.1.28",
          "form": "헐 저 한국사 완전 취약한데",
          "original_form": "헐 저 한국사 완전 취약한데",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100029948.1.29",
          "form": "역사도 문과네요",
          "original_form": "역사도 문과네요",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100029948.1.30",
          "form": "문과에 강하시네요",
          "original_form": "문과에 강하시네요",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100029948.1.31",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100029948.1.32",
          "form": "아 학과가",
          "original_form": "아 학과가",
          "speaker_id": "1",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100029948.1.33",
          "form": "사학과였습니다",
          "original_form": "사학과였습니다",
          "speaker_id": "1",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100029948.1.34",
          "form": "그러다보니 글은 쓰는데",
          "original_form": "그러다보니 글은 쓰는데",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100029948.1.35",
          "form": "국문과가 아니어서",
          "original_form": "국문과가 아니어서",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100029948.1.36",
          "form": "맞춤법은 약한",
          "original_form": "맞춤법은 약한",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100029948.1.37",
          "form": "그런 그런",
          "original_form": "그런 그런",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100029948.1.38",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100029948.1.39",
          "form": "사학과라니",
          "original_form": "사학과라니",
          "speaker_id": "2",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100029948.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100029948.1.41",
          "form": "과외받아보고 싶네요",
          "original_form": "과외받아보고 싶네요",
          "speaker_id": "2",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100029948.1.42",
