      "utterance": [
        {
          "id": "MDRW2100010784.1.1",
          "form": "안정환이 느리다고 타박한다 ㅋㅋ",
          "original_form": "안정환이 느리다고 타박한다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100010784.1.2",
          "form": "안싸우면 다행이야 프로 보고",
          "original_form": "안싸우면 다행이야 프로 보고",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100010784.1.3",
          "form": "성질 급한 사람이 보면 답답하지",
          "original_form": "성질 급한 사람이 보면 답답하지",
          "speaker_id": "1",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010784.1.4",
          "form": "김병헌이 정말 착한거 같은데 좀 사차원같다",
          "original_form": "김병헌이 정말 착한거 같은데 좀 사차원같다",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010784.1.5",
          "form": "ㅋㅋㅋ 예전부터 사차원이드라",
          "original_form": "ㅋㅋㅋ 예전부터 사차원이드라",
          "speaker_id": "1",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010784.1.6",
          "form": "엉 착하긴 엄청 착하단다",
          "original_form": "엉 착하긴 엄청 착하단다",
          "speaker_id": "2",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010784.1.7",
          "form": "사람이 착하면 됐지 뭐",
          "original_form": "사람이 착하면 됐지 뭐",
          "speaker_id": "1",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010784.1.8",
          "form": "지금 보니 약간 눈치가 없는듯 ㅋㅋ",
          "original_form": "지금 보니 약간 눈치가 없는듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010784.1.9",
          "form": "운동선수들끼리 잘 노네",
          "original_form": "운동선수들끼리 잘 노네",
          "speaker_id": "1",
          "time": "20210823 21:50"
        },
        {
          "id": "MDRW2100010784.1.10",
          "form": "주엽이가 병헌이는 왜 저렇게 기분이 좋은거냐고 허재한테 물어본다 ㅋㅋ",
          "original_form": "주엽이가 병헌이는 왜 저렇게 기분이 좋은거냐고 허재한테 물어본다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:50"
        },
        {
          "id": "MDRW2100010784.1.11",
          "form": "항상 즐거운갑다.  눈치 없겠다",
          "original_form": "항상 즐거운갑다.  눈치 없겠다",
          "speaker_id": "1",
          "time": "20210823 21:50"
        },
        {
          "id": "MDRW2100010784.1.12",
          "form": "운동종목은 다르지만 다 잘 지내더라 근데 그 종목에서 다 1인자였던 사람들",
          "original_form": "운동종목은 다르지만 다 잘 지내더라 근데 그 종목에서 다 1인자였던 사람들",
          "speaker_id": "2",
          "time": "20210823 21:51"
        },
        {
          "id": "MDRW2100010784.1.13",
          "form": "운동선수들끼리는 통하는게 있을듯",
          "original_form": "운동선수들끼리는 통하는게 있을듯",
          "speaker_id": "1",
          "time": "20210823 21:51"
        },
        {
          "id": "MDRW2100010784.1.14",
          "form": "응 그들의만 상황이나 공감이 될듯",
          "original_form": "응 그들의만 상황이나 공감이 될듯",
          "speaker_id": "2",
          "time": "20210823 21:51"
        },
        {
          "id": "MDRW2100010784.1.15",
          "form": "또 운동선수들이 놀기도 엄청 잘 놈",
          "original_form": "또 운동선수들이 놀기도 엄청 잘 놈",
          "speaker_id": "1",
          "time": "20210823 21:52"
        },
        {
          "id": "MDRW2100010784.1.16",
          "form": "응 먹기도 엄청 잘 먹고",
          "original_form": "응 먹기도 엄청 잘 먹고",
          "speaker_id": "2",
          "time": "20210823 21:52"
        }
