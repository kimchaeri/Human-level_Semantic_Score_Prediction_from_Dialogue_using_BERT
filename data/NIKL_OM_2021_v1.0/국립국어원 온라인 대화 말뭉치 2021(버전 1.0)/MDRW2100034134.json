      "utterance": [
        {
          "id": "MDRW2100034134.1.1",
          "form": "최근에 여행 가신 곳 있으신가요?",
          "original_form": "최근에 여행 가신 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100034134.1.2",
          "form": "그나마 최근이 전주예요!",
          "original_form": "그나마 최근이 전주예요!",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100034134.1.3",
          "form": "전주 가보셨나요?",
          "original_form": "전주 가보셨나요?",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100034134.1.4",
          "form": "오오 한옥마을 갔다오셨나요?",
          "original_form": "오오 한옥마을 갔다오셨나요?",
          "speaker_id": "1",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100034134.1.5",
          "form": "넹 전 전주가 고향이에요ㅎ",
          "original_form": "넹 전 전주가 고향이에요ㅎ",
          "speaker_id": "1",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100034134.1.6",
          "form": "네 한옥마을이랑 그옆에 시장이요",
          "original_form": "네 한옥마을이랑 그옆에 시장이요",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100034134.1.7",
          "form": "헉 그러시구나 많이아시겠다",
          "original_form": "헉 그러시구나 많이아시겠다",
          "speaker_id": "2",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100034134.1.8",
          "form": "선생님 맛집 알려주실수있나요",
          "original_form": "선생님 맛집 알려주실수있나요",
          "speaker_id": "2",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100034134.1.9",
          "form": "근데 그 말이 있잖아요",
          "original_form": "근데 그 말이 있잖아요",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100034134.1.10",
          "form": "로컬 사람은 정작 모르고",
          "original_form": "로컬 사람은 정작 모르고",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100034134.1.11",
          "form": "관광객이 오히려 더 잘 안다는...그런...",
          "original_form": "관광객이 오히려 더 잘 안다는...그런...",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100034134.1.12",
          "form": "그거 전주가 어딜가도 맛있어서 그런거 아닐까요...",
          "original_form": "그거 전주가 어딜가도 맛있어서 그런거 아닐까요...",
          "speaker_id": "2",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100034134.1.13",
          "form": "저는 부산사는데 맛집 외우고다녀요...",
          "original_form": "저는 부산사는데 맛집 외우고다녀요...",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100034134.1.14",
          "form": "ㅋㄲㄱㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㄲㄱㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100034134.1.15",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100034134.1.16",
          "form": "전주가 아무곳이나 들어가도 맛있더라구요",
          "original_form": "전주가 아무곳이나 들어가도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100034134.1.17",
          "form": "그냥 너무 바가지 안쓰면 되져 뭐",
          "original_form": "그냥 너무 바가지 안쓰면 되져 뭐",
          "speaker_id": "1",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100034134.1.18",
          "form": "ㅌㅋㅋㅋㅋ맞아영",
          "original_form": "ㅌㅋㅋㅋㅋ맞아영",
          "speaker_id": "1",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100034134.1.19",
          "form": "저 쿨한태도 맛집이 널린지역의 여유...",
          "original_form": "저 쿨한태도 맛집이 널린지역의 여유...",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100034134.1.20",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "2",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100034134.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ앗",
          "original_form": "ㅋㅋㅋㅋㅋㅋ앗",
          "speaker_id": "1",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100034134.1.22",
          "form": "개인적으로 전주는 비빔밥보다",
          "original_form": "개인적으로 전주는 비빔밥보다",
          "speaker_id": "1",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100034134.1.23",
          "form": "콩나물국밥이 더 맛있다구 생각해요",
          "original_form": "콩나물국밥이 더 맛있다구 생각해요",
          "speaker_id": "1",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100034134.1.24",
          "form": "콩나물 국밥 시장에서 먹었는데",
          "original_form": "콩나물 국밥 시장에서 먹었는데",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034134.1.25",
          "form": "수란에 오징어에 너무 최고였어요...",
          "original_form": "수란에 오징어에 너무 최고였어요...",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034134.1.26",
          "form": "오옹 맛있죠ㅠ",
          "original_form": "오옹 맛있죠ㅠ",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034134.1.27",
          "form": "설마 남부시장?",
          "original_form": "설마 남부시장?",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034134.1.28",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034134.1.29",
          "form": "골목사이에 있는 곳갔었어요",
          "original_form": "골목사이에 있는 곳갔었어요",
          "speaker_id": "2",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100034134.1.30",
          "form": "거기 유명하죠ㅎㅎ",
          "original_form": "거기 유명하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100034134.1.31",
          "form": "유명한만큼 바가지 씌우는 곳도 많아요",
          "original_form": "유명한만큼 바가지 씌우는 곳도 많아요",
          "speaker_id": "1",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100034134.1.32",
          "form": "그렇군요....하지만 모르는 비용이라고 생각하고 내겠습니다 맛있다면...",
          "original_form": "그렇군요....하지만 모르는 비용이라고 생각하고 내겠습니다 맛있다면...",
          "speaker_id": "2",
          "time": "20210827 17:10"
        }
      ]
