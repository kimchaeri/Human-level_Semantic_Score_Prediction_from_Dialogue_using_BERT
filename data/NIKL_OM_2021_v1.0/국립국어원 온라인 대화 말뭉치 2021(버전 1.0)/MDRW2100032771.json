      "utterance": [
        {
          "id": "MDRW2100032771.1.1",
          "form": "이제 집도착했어용",
          "original_form": "이제 집도착했어용",
          "speaker_id": "2",
          "time": "20210905 23:42"
        },
        {
          "id": "MDRW2100032771.1.2",
          "form": "오늘 날씨 선선하니 좋네요",
          "original_form": "오늘 날씨 선선하니 좋네요",
          "speaker_id": "2",
          "time": "20210905 23:42"
        },
        {
          "id": "MDRW2100032771.1.3",
          "form": "오래걸렸다~~~ 고생했다~~",
          "original_form": "오래걸렸다~~~ 고생했다~~",
          "speaker_id": "1",
          "time": "20210905 23:42"
        },
        {
          "id": "MDRW2100032771.1.4",
          "form": "오늘 낮에는 더웠는데 저녁엔 바람 불고 선선하더라",
          "original_form": "오늘 낮에는 더웠는데 저녁엔 바람 불고 선선하더라",
          "speaker_id": "1",
          "time": "20210905 23:42"
        },
        {
          "id": "MDRW2100032771.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032771.1.6",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:47"
        },
        {
          "id": "MDRW2100032771.1.7",
          "form": "요즘은 좀 쌀쌀하더라고요",
          "original_form": "요즘은 좀 쌀쌀하더라고요",
          "speaker_id": "2",
          "time": "20210905 23:47"
        },
        {
          "id": "MDRW2100032771.1.8",
          "form": "그치 아침 저녁 쌀쌀한데 낮엔 땀날정도로 덥고",
          "original_form": "그치 아침 저녁 쌀쌀한데 낮엔 땀날정도로 덥고",
          "speaker_id": "1",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100032771.1.9",
          "form": "이럴때 감기걸리기 쉬운듯",
          "original_form": "이럴때 감기걸리기 쉬운듯",
          "speaker_id": "1",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100032771.1.10",
          "form": "언제든지 건강 걱정",
          "original_form": "언제든지 건강 걱정",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032771.1.11",
          "form": "파이팅입니다",
          "original_form": "파이팅입니다",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032771.1.12",
          "form": "옷챙겨다니고",
          "original_form": "옷챙겨다니고",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032771.1.13",
          "form": "웅웅 반팔입고 겉옷은 긴거로 챙겨다님ㅋ",
          "original_form": "웅웅 반팔입고 겉옷은 긴거로 챙겨다님ㅋ",
          "speaker_id": "1",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032771.1.14",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032771.1.15",
          "form": "날씨어플 보니까 내일 오후부터 비오네",
          "original_form": "날씨어플 보니까 내일 오후부터 비오네",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032771.1.16",
          "form": "화요일은 하루종일 오구",
          "original_form": "화요일은 하루종일 오구",
          "speaker_id": "1",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032771.1.17",
          "form": "어후ㅜㅜ 비와요?",
          "original_form": "어후ㅜㅜ 비와요?",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032771.1.18",
          "form": "진짜 출근도안했는데 퇴근하고싶다",
          "original_form": "진짜 출근도안했는데 퇴근하고싶다",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032771.1.19",
          "form": "ㅜㅜㅜ그치",
          "original_form": "ㅜㅜㅜ그치",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032771.1.20",
          "form": "5분뒤에 월요일ㅋ",
          "original_form": "5분뒤에 월요일ㅋ",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032771.1.21",
          "form": "와우..",
          "original_form": "와우..",
          "speaker_id": "2",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032771.1.22",
          "form": "소름끼친다",
          "original_form": "소름끼친다",
          "speaker_id": "2",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032771.1.23",
          "form": "출근할때 우산 꼭 챙겨!!",
          "original_form": "출근할때 우산 꼭 챙겨!!",
          "speaker_id": "1",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100032771.1.24",
          "form": "난 따숩게 자려고 방 보일러 돌렸다",
          "original_form": "난 따숩게 자려고 방 보일러 돌렸다",
          "speaker_id": "1",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100032771.1.25",
          "form": "따숩게요?",
          "original_form": "따숩게요?",
          "speaker_id": "2",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100032771.1.26",
          "form": "아맞다 넘춥더라고요",
          "original_form": "아맞다 넘춥더라고요",
          "speaker_id": "2",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032771.1.27",
          "form": "웅 새벽은 춥더라구",
          "original_form": "웅 새벽은 춥더라구",
          "speaker_id": "1",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032771.1.28",
          "form": "바닥이 차면 잘 못자서ㅜㅜ",
          "original_form": "바닥이 차면 잘 못자서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032771.1.29",
          "form": "으음ㅜㅜ 그렇군요",
          "original_form": "으음ㅜㅜ 그렇군요",
          "speaker_id": "2",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032771.1.30",
          "form": "아이고 벌써 하루가",
          "original_form": "아이고 벌써 하루가",
          "speaker_id": "2",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032771.1.31",
          "form": "웅 그래서 보일러 틀고 선풍기도 틀고 ㅋㅋㅋ",
          "original_form": "웅 그래서 보일러 틀고 선풍기도 틀고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:03"
        },
        {
          "id": "MDRW2100032771.1.32",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:03"
        },
        {
          "id": "MDRW2100032771.1.33",
          "form": "환절기때 감기조심하세요",
          "original_form": "환절기때 감기조심하세요",
          "speaker_id": "2",
          "time": "20210906 00:04"
        },
        {
          "id": "MDRW2100032771.1.34",
          "form": "웅웅 백신도 맞아야 하니까 진짜 더 조심해야할듯~",
          "original_form": "웅웅 백신도 맞아야 하니까 진짜 더 조심해야할듯~",
          "speaker_id": "1",
          "time": "20210906 00:04"
        },
        {
          "id": "MDRW2100032771.1.35",
          "form": "백신..언니는 부작용 없으셨어요?",
          "original_form": "백신..언니는 부작용 없으셨어요?",
          "speaker_id": "2",
          "time": "20210906 08:12"
        },
        {
          "id": "MDRW2100032771.1.36",
          "form": "저는 코로나도 아닌데 요즘 기침이 잦아져서 걱정이에요",
          "original_form": "저는 코로나도 아닌데 요즘 기침이 잦아져서 걱정이에요",
          "speaker_id": "2",
