      "utterance": [
        {
          "id": "MDRW2100003954.1.1",
          "form": "뭐하고 계시나요",
          "original_form": "뭐하고 계시나요",
          "speaker_id": "2",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003954.1.2",
          "form": "침대에 누워있네요..ㅎㅎ",
          "original_form": "침대에 누워있네요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003954.1.3",
          "form": "name2님은 뭐하고 계시나요",
          "original_form": "&name2&님은 뭐하고 계시나요",
          "speaker_id": "1",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003954.1.4",
          "form": "아하 저도 ㅋㅋㅋ 비오는데 밖에 나갔다와서 힘드네요",
          "original_form": "아하 저도 ㅋㅋㅋ 비오는데 밖에 나갔다와서 힘드네요",
          "speaker_id": "2",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003954.1.5",
          "form": "그래도 좀 있다가 힘내서 설거지하려구요",
          "original_form": "그래도 좀 있다가 힘내서 설거지하려구요",
          "speaker_id": "2",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003954.1.6",
          "form": "그릇이 많이 쌓여서",
          "original_form": "그릇이 많이 쌓여서",
          "speaker_id": "2",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003954.1.7",
          "form": "ㅠㅠㅠㅠㅠ 오늘 정말 장난아니게 왔죠",
          "original_form": "ㅠㅠㅠㅠㅠ 오늘 정말 장난아니게 왔죠",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003954.1.8",
          "form": "주말에 집안일 밀린일 해야죠..ㅠㅠ",
          "original_form": "주말에 집안일 밀린일 해야죠..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003954.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003954.1.10",
          "form": "화장실청소나 점심 만드는거나... 하려고 하면 얼마든지 많죠",
          "original_form": "화장실청소나 점심 만드는거나... 하려고 하면 얼마든지 많죠",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003954.1.11",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003954.1.12",
          "form": "화장실 청소 귀찮지만 해야죠...",
          "original_form": "화장실 청소 귀찮지만 해야죠...",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003954.1.13",
          "form": "일주일만 안해도 벌레가 나오니...ㅠㅠ",
          "original_form": "일주일만 안해도 벌레가 나오니...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003954.1.14",
          "form": "제일 귀찮은 건 그래도",
          "original_form": "제일 귀찮은 건 그래도",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003954.1.15",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003954.1.16",
          "form": "음식물 청소같아요",
          "original_form": "음식물 청소같아요",
          "speaker_id": "2",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003954.1.17",
          "form": "ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003954.1.18",
          "form": "여름에 벌레들 장난아니죠....",
          "original_form": "여름에 벌레들 장난아니죠....",
          "speaker_id": "1",
          "time": "20210821 14:24"
        },
        {
          "id": "MDRW2100003954.1.19",
          "form": "어디서 그렇게 날아들어오는지",
          "original_form": "어디서 그렇게 날아들어오는지",
          "speaker_id": "1",
          "time": "20210821 14:24"
        },
        {
          "id": "MDRW2100003954.1.20",
          "form": "제일 공포스러워요",
          "original_form": "제일 공포스러워요",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003954.1.21",
          "form": "저는 벌레 무서워하긴 해도 잘 잡는데",
          "original_form": "저는 벌레 무서워하긴 해도 잘 잡는데",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003954.1.22",
          "form": "바퀴벌레 나오면 정말 못 잡을 것 같은 ㅠㅠ",
          "original_form": "바퀴벌레 나오면 정말 못 잡을 것 같은 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003954.1.23",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003954.1.24",
          "form": "바퀴벌레는 정말....",
          "original_form": "바퀴벌레는 정말....",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003954.1.25",
          "form": "잠 못이루게 만드는",
          "original_form": "잠 못이루게 만드는",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003954.1.26",
          "form": "무서운.... ",
          "original_form": "무서운.... ",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003954.1.27",
          "form": "그래서 최대한 바퀴벌레 약은 많이 받아놓고 있어요",
          "original_form": "그래서 최대한 바퀴벌레 약은 많이 받아놓고 있어요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003954.1.28",
          "form": "그래도 올 놈은 다 오더라구요",
          "original_form": "그래도 올 놈은 다 오더라구요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003954.1.29",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ 너무 싫네요....",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ 너무 싫네요....",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003954.1.30",
          "form": "그 맥스포스 아시나요?",
          "original_form": "그 맥스포스 아시나요?",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003954.1.31",
          "form": "걔가 정말 직빵인데",
          "original_form": "걔가 정말 직빵인데",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003954.1.32",
          "form": "비싸긴한데 효과 정말 좋더라구요",
          "original_form": "비싸긴한데 효과 정말 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003954.1.33",
          "form": "헉 정말요",
          "original_form": "헉 정말요",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003954.1.34",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003954.1.35",
          "form": "감사합니다 캡쳐해놓을게요....",
          "original_form": "감사합니다 캡쳐해놓을게요....",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003954.1.36",
          "form": "꼭 한번 써보세요",
          "original_form": "꼭 한번 써보세요",
