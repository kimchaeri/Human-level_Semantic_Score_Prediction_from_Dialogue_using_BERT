      "utterance": [
        {
          "id": "MDRW2100007453.1.1",
          "form": "가장 관람하고 싶은 공연이 뭐야",
          "original_form": "가장 관람하고 싶은 공연이 뭐야",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007453.1.2",
          "form": "하나만 골라라",
          "original_form": "하나만 골라라",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007453.1.3",
          "form": "나",
          "original_form": "나",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007453.1.4",
          "form": "기다려봐,",
          "original_form": "기다려봐,",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007453.1.5",
          "form": "중요한 문제야.",
          "original_form": "중요한 문제야.",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007453.1.6",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007453.1.7",
          "form": "나부터 말할까?",
          "original_form": "나부터 말할까?",
          "speaker_id": "2",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007453.1.8",
          "form": "지킬앤하이드 스위니토드 헤드윅 레베카 마리퀴리 팬레터 사의찬미 어쩌면햎어그래",
          "original_form": "지킬앤하이드 스위니토드 헤드윅 레베카 마리퀴리 팬레터 사의찬미 어쩌면햎어그래",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007453.1.9",
          "form": "하나만 하라고",
          "original_form": "하나만 하라고",
          "speaker_id": "2",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007453.1.10",
          "form": "나열하고 고를랬는데 니가 말을걸었어",
          "original_form": "나열하고 고를랬는데 니가 말을걸었어",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007453.1.11",
          "form": "미안하다",
          "original_form": "미안하다",
          "speaker_id": "2",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007453.1.12",
          "form": "너는 어떤 뮤지컬을 가장 보고싶어?",
          "original_form": "너는 어떤 뮤지컬을 가장 보고싶어?",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007453.1.13",
          "form": "나는 딱 하나만 고르라면 당연히 팬레터라서 정말 최악의 상황을 가정하고 팬레터에 김경수 배우가 없고 모든 배우가 내 불호 배우라고 하면",
          "original_form": "나는 딱 하나만 고르라면 당연히 팬레터라서 정말 최악의 상황을 가정하고 팬레터에 김경수 배우가 없고 모든 배우가 내 불호 배우라고 하면",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007453.1.14",
          "form": "어그래",
          "original_form": "어그래",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007453.1.15",
          "form": "사의 찬미가 보고싶어",
          "original_form": "사의 찬미가 보고싶어",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007453.1.16",
          "form": "괜렡캐스팅: 김구라 원캐슽",
          "original_form": "괜렡캐스팅: 김구라 원캐슽",
          "speaker_id": "1",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007453.1.17",
          "form": "너는 꼭 그렇게 말을 해야겠니?",
          "original_form": "너는 꼭 그렇게 말을 해야겠니?",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007453.1.18",
          "form": "사의찬미를 보고싶은 이유가 있어?",
          "original_form": "사의찬미를 보고싶은 이유가 있어?",
          "speaker_id": "1",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007453.1.19",
          "form": "난 그런 사랑을 원해가 너무 보고싶어",
          "original_form": "난 그런 사랑을 원해가 너무 보고싶어",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007453.1.20",
          "form": "난 역시 도쿄찬가...................아니? 그가오공있하........ 그냥다보고싶어",
          "original_form": "난 역시 도쿄찬가...................아니? 그가오공있하........ 그냥다보고싶어",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007453.1.21",
          "form": "그 장면에 딱 꽂혔어 절대 OST에서 모 배우가 스페셜 트랙으로 불러서 그런건 아니고",
          "original_form": "그 장면에 딱 꽂혔어 절대 OST에서 모 배우가 스페셜 트랙으로 불러서 그런건 아니고",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007453.1.22",
          "form": "어어",
          "original_form": "어어",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007453.1.23",
          "form": "그가! 오고 있어",
          "original_form": "그가! 오고 있어",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007453.1.24",
          "form": "빰.빰",
          "original_form": "빰.빰",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007453.1.25",
          "form": "나 티켓예약주제로 뭐 하나만 말할게",
          "original_form": "나 티켓예약주제로 뭐 하나만 말할게",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007453.1.26",
          "form": "조드윅 이야기야?",
          "original_form": "조드윅 이야기야?",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007453.1.27",
          "form": "정답이야.",
          "original_form": "정답이야.",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007453.1.28",
          "form": "양심적으로 어떻게 어떻게 그 넓은 극장에 하다못해코로나19인데",
          "original_form": "양심적으로 어떻게 어떻게 그 넓은 극장에 하다못해코로나19인데",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007453.1.29",
          "form": "솔직히 조승우는 본인 공연 티켓팅을 해봐야 된다고 생각해",
          "original_form": "솔직히 조승우는 본인 공연 티켓팅을 해봐야 된다고 생각해",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007453.1.30",
          "form": "안돼그양반 컴맹이라",
          "original_form": "안돼그양반 컴맹이라",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007453.1.31",
          "form": "인터파크로그인도못할거야,",
          "original_form": "인터파크로그인도못할거야,",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007453.1.32",
          "form": "그정도야?",
          "original_form": "그정도야?",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007453.1.33",
          "form": "본인 입으로 맨날 컴맹이라고 하고 다녀",
          "original_form": "본인 입으로 맨날 컴맹이라고 하고 다녀",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007453.1.34",
          "form": "귀여워",
          "original_form": "귀여워",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007453.1.35",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007453.1.36",
          "form": "티켓 예약이 너무 어려워.",
          "original_form": "티켓 예약이 너무 어려워.",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007453.1.37",
          "form": "분명 좌석은 몇백석이 넘고",
          "original_form": "분명 좌석은 몇백석이 넘고",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007453.1.38",
          "form": "공연은 몇달을 하는데",
          "original_form": "공연은 몇달을 하는데",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007453.1.39",
          "form": "단 하루 한좌석도 비지를 않아.",
          "original_form": "단 하루 한좌석도 비지를 않아.",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007453.1.40",
          "form": "소극장 뮤지컬 배우 중에 자기 공연 티켓팅 망했다고 트위터에 중계하던 배우가 있었어",
          "original_form": "소극장 뮤지컬 배우 중에 자기 공연 티켓팅 망했다고 트위터에 중계하던 배우가 있었어",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007453.1.41",
          "form": "정말웃기다",
          "original_form": "정말웃기다",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007453.1.42",
          "form": "그 사람이 괜찮아여 저는 무대에서 보면 되니까 ^^ 하는데",
          "original_form": "그 사람이 괜찮아여 저는 무대에서 보면 되니까 ^^ 하는데",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007453.1.43",
          "form": "진심으로 부러웠어",
          "original_form": "진심으로 부러웠어",
          "speaker_id": "2",
