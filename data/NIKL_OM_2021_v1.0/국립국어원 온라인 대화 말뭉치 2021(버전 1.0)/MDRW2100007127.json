      "utterance": [
        {
          "id": "MDRW2100007127.1.1",
          "form": "그 청정원에서 나온 냉동",
          "original_form": "그 청정원에서 나온 냉동",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007127.1.2",
          "form": "막창도 괜찮더라구요",
          "original_form": "막창도 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007127.1.3",
          "form": "청정원 냉동막창요?",
          "original_form": "청정원 냉동막창요?",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007127.1.4",
          "form": "불막창",
          "original_form": "불막창",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007127.1.5",
          "form": "어머 처음 들어보는데 너무끌리네요",
          "original_form": "어머 처음 들어보는데 너무끌리네요",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007127.1.6",
          "form": "잡내도안나고요?",
          "original_form": "잡내도안나고요?",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007127.1.7",
          "form": "네 마늘도 편으로 들어있어요 ㅋ\r\n안나요 ㅋㅋ",
          "original_form": "네 마늘도 편으로 들어있어요 ㅋ\r\n안나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007127.1.8",
          "form": "렌지 돌려서 녹인다음에 팬에 구우면서 큰거 좀 잘라주고 ..",
          "original_form": "렌지 돌려서 녹인다음에 팬에 구우면서 큰거 좀 잘라주고 ..",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007127.1.9",
          "form": "ㅋㅋㅋ와 진짜 상추깻잎",
          "original_form": "ㅋㅋㅋ와 진짜 상추깻잎",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007127.1.10",
          "form": "맥주랑",
          "original_form": "맥주랑",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007127.1.11",
          "form": "사놔야겠네요",
          "original_form": "사놔야겠네요",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007127.1.12",
          "form": "야식으로굿... 소주랑도어울리죠 불막창은!!",
          "original_form": "야식으로굿... 소주랑도어울리죠 불막창은!!",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007127.1.13",
          "form": "캬 와 깻잎 맞아요 막창은 깻잎이에여",
          "original_form": "캬 와 깻잎 맞아요 막창은 깻잎이에여",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007127.1.14",
          "form": "와와와와왕",
          "original_form": "와와와와왕",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007127.1.15",
          "form": "또 이게 고기마다 맞는 채소가 다르니까ㅋㅋ",
          "original_form": "또 이게 고기마다 맞는 채소가 다르니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007127.1.16",
          "form": "혹시 불닭볶음면도좋아하세요?",
          "original_form": "혹시 불닭볶음면도좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007127.1.17",
          "form": "불닭볶음면은 컵라면이 맛이 낫더라구요",
          "original_form": "불닭볶음면은 컵라면이 맛이 낫더라구요",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007127.1.18",
          "form": "아 맞아요ㅋㅋ거기 치즈넣어서",
          "original_form": "아 맞아요ㅋㅋ거기 치즈넣어서",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007127.1.19",
          "form": "렌지에 1분",
          "original_form": "렌지에 1분",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007127.1.20",
          "form": "끓인거는 뭔가맛이 들해여.. 제기준 ㅋㅋ",
          "original_form": "끓인거는 뭔가맛이 들해여.. 제기준 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007127.1.21",
          "form": "와 너무 똑같....",
          "original_form": "와 너무 똑같....",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007127.1.22",
          "form": "치즈 넣어서 꾸덕하게....비벼서",
          "original_form": "치즈 넣어서 꾸덕하게....비벼서",
          "speaker_id": "1",
          "time": "20210822 00:38"
        }
      ]
