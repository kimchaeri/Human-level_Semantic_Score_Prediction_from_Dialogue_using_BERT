      "utterance": [
        {
          "id": "MDRW2100023478.1.1",
          "form": "물건너갔으니",
          "original_form": "물건너갔으니",
          "speaker_id": "2",
          "time": "20210830 08:40"
        },
        {
          "id": "MDRW2100023478.1.2",
          "form": "이제 쎄타나",
          "original_form": "이제 쎄타나",
          "speaker_id": "2",
          "time": "20210830 08:40"
        },
        {
          "id": "MDRW2100023478.1.3",
          "form": "버텨볼라고",
          "original_form": "버텨볼라고",
          "speaker_id": "2",
          "time": "20210830 08:40"
        },
        {
          "id": "MDRW2100023478.1.4",
          "form": "나는 s t x 조금 오름",
          "original_form": "나는 s t x 조금 오름",
          "speaker_id": "1",
          "time": "20210830 08:40"
        },
        {
          "id": "MDRW2100023478.1.5",
          "form": "샌박 번돈 다 날림",
          "original_form": "샌박 번돈 다 날림",
          "speaker_id": "2",
          "time": "20210830 08:41"
        },
        {
          "id": "MDRW2100023478.1.6",
          "form": "온톨가 도박 + 최저점 손절",
          "original_form": "온톨가 도박 + 최저점 손절",
          "speaker_id": "2",
          "time": "20210830 08:41"
        },
        {
          "id": "MDRW2100023478.1.7",
          "form": "쎄타 많이 안올랐네",
          "original_form": "쎄타 많이 안올랐네",
          "speaker_id": "1",
          "time": "20210830 08:41"
        },
        {
          "id": "MDRW2100023478.1.8",
          "form": "현재 최종 200정도 -",
          "original_form": "현재 최종 200정도 -",
          "speaker_id": "2",
          "time": "20210830 08:41"
        },
        {
          "id": "MDRW2100023478.1.9",
          "form": "이번에 60 도루 날려먹어서",
          "original_form": "이번에 60 도루 날려먹어서",
          "speaker_id": "2",
          "time": "20210830 08:41"
        },
        {
          "id": "MDRW2100023478.1.10",
          "form": "똔똔이여?",
          "original_form": "똔똔이여?",
          "speaker_id": "1",
          "time": "20210830 08:42"
        },
        {
          "id": "MDRW2100023478.1.11",
          "form": "놉 총 -200",
          "original_form": "놉 총 -200",
          "speaker_id": "2",
          "time": "20210830 08:42"
        },
        {
          "id": "MDRW2100023478.1.12",
          "form": "예전 손실금 140",
          "original_form": "예전 손실금 140",
          "speaker_id": "2",
          "time": "20210830 08:42"
        },
        {
          "id": "MDRW2100023478.1.13",
          "form": "쎄타만 비코 올라도",
          "original_form": "쎄타만 비코 올라도",
          "speaker_id": "2",
          "time": "20210830 08:42"
        },
        {
          "id": "MDRW2100023478.1.14",
          "form": "횡보중",
          "original_form": "횡보중",
          "speaker_id": "2",
          "time": "20210830 08:42"
        },
        {
          "id": "MDRW2100023478.1.15",
          "form": "이거 필터 때문에 너무 불편하다",
          "original_form": "이거 필터 때문에 너무 불편하다",
          "speaker_id": "2",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100023478.1.16",
          "form": "그런게 오르지 않을까",
          "original_form": "그런게 오르지 않을까",
          "speaker_id": "1",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100023478.1.17",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100023478.1.18",
          "form": "이런애들은 거래량 때문에",
          "original_form": "이런애들은 거래량 때문에",
          "speaker_id": "2",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100023478.1.19",
          "form": "도지 안오를때 버텼는데 올랐어",
          "original_form": "도지 안오를때 버텼는데 올랐어",
          "speaker_id": "1",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100023478.1.20",
          "form": "불안하지",
          "original_form": "불안하지",
          "speaker_id": "2",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100023478.1.21",
          "form": "그래서 스텔라랑 비체인 2달 버티다가",
          "original_form": "그래서 스텔라랑 비체인 2달 버티다가",
          "speaker_id": "2",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100023478.1.22",
          "form": "손절해서",
          "original_form": "손절해서",
          "speaker_id": "2",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100023478.1.23",
          "form": "50% 먹읠거 10% 잃었주",
          "original_form": "50% 먹읠거 10% 잃었주",
          "speaker_id": "2",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100023478.1.24",
          "form": "손실이여?",
          "original_form": "손실이여?",
          "speaker_id": "1",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100023478.1.25",
          "form": "ㅇㅇ.. 거래량이 없으면",
          "original_form": "ㅇㅇ.. 거래량이 없으면",
          "speaker_id": "2",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100023478.1.26",
          "form": "어느순간 불안해져",
          "original_form": "어느순간 불안해져",
          "speaker_id": "2",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100023478.1.27",
          "form": "다들 그렇쥬 나도 2월달에 단타안하고 사고 묻었으면 많이 벌었쥬",
          "original_form": "다들 그렇쥬 나도 2월달에 단타안하고 사고 묻었으면 많이 벌었쥬",
          "speaker_id": "1",
