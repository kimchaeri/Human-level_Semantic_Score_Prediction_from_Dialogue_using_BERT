      "utterance": [
        {
          "id": "MDRW2100018784.1.1",
          "form": "동호회 뭐 하시나여?",
          "original_form": "동호회 뭐 하시나여?",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018784.1.2",
          "form": "합창동아리 했었는데 지금은 안해요ㅎㅎ",
          "original_form": "합창동아리 했었는데 지금은 안해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018784.1.3",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018784.1.4",
          "form": "학생때 하신거에요?",
          "original_form": "학생때 하신거에요?",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018784.1.5",
          "form": "전 일본어모임 했었어여",
          "original_form": "전 일본어모임 했었어여",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018784.1.6",
          "form": "네 학교 동아리 했어요",
          "original_form": "네 학교 동아리 했어요",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018784.1.7",
          "form": "오 일본어 잘하시나요??",
          "original_form": "오 일본어 잘하시나요??",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018784.1.8",
          "form": "네.. ㅋㅋㅋㅋ 항상 반에서도 탑클래스 ㅍㅎㅎㅎ",
          "original_form": "네.. ㅋㅋㅋㅋ 항상 반에서도 탑클래스 ㅍㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018784.1.9",
          "form": "일본어 모임에서 강사? 같은거 했엇어용.",
          "original_form": "일본어 모임에서 강사? 같은거 했엇어용.",
          "speaker_id": "1",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018784.1.10",
          "form": "오 혹시 일본에서 살다 오셨나요??",
          "original_form": "오 혹시 일본에서 살다 오셨나요??",
          "speaker_id": "2",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018784.1.11",
          "form": "1년유학이요",
          "original_form": "1년유학이요",
          "speaker_id": "1",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100018784.1.12",
          "form": "오 1년밖에 안 갔다오셨는데 되게 잘하시나보네요",
          "original_form": "오 1년밖에 안 갔다오셨는데 되게 잘하시나보네요",
          "speaker_id": "2",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100018784.1.13",
          "form": "언어적인 감각이 좋으신가봐요ㅎㅎ",
          "original_form": "언어적인 감각이 좋으신가봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100018784.1.14",
          "form": "1년가서 기초배우고... \r\n운이 좋아서 계속 일본인 친구 만나고 \r\n일본계 회사에서 일했어요.. \r\n\r\n한국말 잘하면 외국어도 잘하는거 같아요",
          "original_form": "1년가서 기초배우고... \r\n운이 좋아서 계속 일본인 친구 만나고 \r\n일본계 회사에서 일했어요.. \r\n\r\n한국말 잘하면 외국어도 잘하는거 같아요",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018784.1.15",
          "form": "아 그랬군요",
          "original_form": "아 그랬군요",
          "speaker_id": "2",
          "time": "20210827 12:55"
        },
        {
          "id": "MDRW2100018784.1.16",
          "form": "저도 중고등학교때 일본어 배웠었는데 기초수준이에요ㅋㅋ",
          "original_form": "저도 중고등학교때 일본어 배웠었는데 기초수준이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:55"
        },
        {
          "id": "MDRW2100018784.1.17",
          "form": "합창동아리 하셧음 노래 잘하시겟어여?",
          "original_form": "합창동아리 하셧음 노래 잘하시겟어여?",
          "speaker_id": "1",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018784.1.18",
          "form": "아뇨ㅋㅋㅋ 그냥 사람만날려고 한번 해봤어요ㅋㅋ",
          "original_form": "아뇨ㅋㅋㅋ 그냥 사람만날려고 한번 해봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100018784.1.19",
          "form": "그래도 음치는 못들어가잖아요.. ㅋㅋ",
          "original_form": "그래도 음치는 못들어가잖아요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018784.1.20",
          "form": "사람이 부족해서 그런지 아예 음을 못 맞추지만 않으면 다 받아주더라고요ㅋㅋ",
          "original_form": "사람이 부족해서 그런지 아예 음을 못 맞추지만 않으면 다 받아주더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018784.1.21",
          "form": "저는 들어가지도 못하고 쫓겨날듯..",
          "original_form": "저는 들어가지도 못하고 쫓겨날듯..",
          "speaker_id": "1",
