      "utterance": [
        {
          "id": "MDRW2100032063.1.1",
          "form": "오늘은 뭐해드세요?",
          "original_form": "오늘은 뭐해드세요?",
          "speaker_id": "1",
          "time": "20210905 11:44"
        },
        {
          "id": "MDRW2100032063.1.2",
          "form": "오늘 메뉴는 콩불입니다:)",
          "original_form": "오늘 메뉴는 콩불입니다:)",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032063.1.3",
          "form": "name1댁은요?",
          "original_form": "&name1&댁은요?",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032063.1.4",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100032063.1.5",
          "form": "전 라면....",
          "original_form": "전 라면....",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100032063.1.6",
          "form": "라면도 맛있었겠어요:)",
          "original_form": "라면도 맛있었겠어요:)",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032063.1.7",
          "form": "남편이 좋아하는 메뉴라\r\n밥 3공기 먹더라구요",
          "original_form": "남편이 좋아하는 메뉴라\r\n밥 3공기 먹더라구요",
          "speaker_id": "2",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032063.1.8",
          "form": "콩불 맛있겠다",
          "original_form": "콩불 맛있겠다",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032063.1.9",
          "form": "예전에 신촌에서 먹었던기억이",
          "original_form": "예전에 신촌에서 먹었던기억이",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032063.1.10",
          "form": "그 이후로는 없는건가여?ㅋㅋ",
          "original_form": "그 이후로는 없는건가여?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032063.1.11",
          "form": "네 그 이후론 없어요",
          "original_form": "네 그 이후론 없어요",
          "speaker_id": "1",
          "time": "20210905 17:55"
        },
        {
          "id": "MDRW2100032063.1.12",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "1",
          "time": "20210905 17:56"
        },
        {
          "id": "MDRW2100032063.1.13",
          "form": "엄청 간단해요ㅋㅋ",
          "original_form": "엄청 간단해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:06"
        },
        {
          "id": "MDRW2100032063.1.14",
          "form": "name1이 직접 만들어봐요ㅋㅋ",
          "original_form": "&name1&이 직접 만들어봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:07"
        },
        {
          "id": "MDRW2100032063.1.15",
          "form": "어머님도 좋아하실듯",
          "original_form": "어머님도 좋아하실듯",
          "speaker_id": "2",
          "time": "20210905 19:07"
        },
        {
          "id": "MDRW2100032063.1.16",
          "form": "제가요?",
          "original_form": "제가요?",
          "speaker_id": "1",
          "time": "20210905 19:16"
        },
        {
          "id": "MDRW2100032063.1.17",
          "form": "아... 요리는 안하시나요?",
          "original_form": "아... 요리는 안하시나요?",
          "speaker_id": "2",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032063.1.18",
          "form": "엄마주방에서 뭘 하는걸 별로 안좋아하셔서",
          "original_form": "엄마주방에서 뭘 하는걸 별로 안좋아하셔서",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032063.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032063.1.20",
          "form": "걍 조용히 주는대로",
          "original_form": "걍 조용히 주는대로",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032063.1.21",
          "form": "드셔야겠네요 집에서는",
          "original_form": "드셔야겠네요 집에서는",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032063.1.22",
          "form": "네 ㅋㅋ  주는대로 받아먹습니다 ㅋㅋ",
          "original_form": "네 ㅋㅋ  주는대로 받아먹습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032063.1.23",
          "form": "저녁에는 뭘 주시던가요?ㅋㅋ",
          "original_form": "저녁에는 뭘 주시던가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032063.1.24",
          "form": "오늘은 고기를 다같이 구워먹었습니다",
          "original_form": "오늘은 고기를 다같이 구워먹었습니다",
