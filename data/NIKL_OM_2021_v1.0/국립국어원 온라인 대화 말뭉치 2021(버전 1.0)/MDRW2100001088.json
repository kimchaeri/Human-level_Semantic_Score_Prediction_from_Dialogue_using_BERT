      "utterance": [
        {
          "id": "MDRW2100001088.1.1",
          "form": "학교가면 친구들이랑 머해요?",
          "original_form": "학교가면 친구들이랑 머해요?",
          "speaker_id": "1",
          "time": "20210723 16:09"
        },
        {
          "id": "MDRW2100001088.1.2",
          "form": "거의 공부하고.. 그냥 주제 없는 대화해요 저흰!!",
          "original_form": "거의 공부하고.. 그냥 주제 없는 대화해요 저흰!!",
          "speaker_id": "2",
          "time": "20210723 16:10"
        },
        {
          "id": "MDRW2100001088.1.3",
          "form": "요즘 친구들 안만나요?",
          "original_form": "요즘 친구들 안만나요?",
          "speaker_id": "1",
          "time": "20210723 16:10"
        },
        {
          "id": "MDRW2100001088.1.4",
          "form": "음.. 잘 안 만나다가 내일 만나요!",
          "original_form": "음.. 잘 안 만나다가 내일 만나요!",
          "speaker_id": "2",
          "time": "20210723 16:10"
        },
        {
          "id": "MDRW2100001088.1.5",
          "form": "name1 님은!!",
          "original_form": "&name1& 님은!!",
          "speaker_id": "2",
          "time": "20210723 16:11"
        },
        {
          "id": "MDRW2100001088.1.6",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210723 16:11"
        },
        {
          "id": "MDRW2100001088.1.7",
          "form": "뭐하고 노세요??",
          "original_form": "뭐하고 노세요??",
          "speaker_id": "2",
          "time": "20210723 16:11"
        },
        {
          "id": "MDRW2100001088.1.8",
          "form": "저도 가끔ㅠ",
          "original_form": "저도 가끔ㅠ",
          "speaker_id": "1",
          "time": "20210723 16:11"
        },
        {
          "id": "MDRW2100001088.1.9",
          "form": "pc방이나 카페?",
          "original_form": "pc방이나 카페?",
          "speaker_id": "1",
          "time": "20210723 16:11"
        },
        {
          "id": "MDRW2100001088.1.10",
          "form": "피시방! 게임 뭐하세요??",
          "original_form": "피시방! 게임 뭐하세요??",
          "speaker_id": "2",
          "time": "20210723 16:11"
        },
        {
          "id": "MDRW2100001088.1.11",
          "form": "저 롤이나 오버워치해요",
          "original_form": "저 롤이나 오버워치해요",
          "speaker_id": "1",
          "time": "20210723 16:12"
        },
        {
          "id": "MDRW2100001088.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210723 16:12"
        },
        {
          "id": "MDRW2100001088.1.13",
          "form": "아아 전 게임을 안 햐요! 애들이 하는 거 옆에서 보는데 스타듀벨리 많이 하더라고요!!",
          "original_form": "아아 전 게임을 안 햐요! 애들이 하는 거 옆에서 보는데 스타듀벨리 많이 하더라고요!!",
          "speaker_id": "2",
          "time": "20210723 16:13"
        },
        {
          "id": "MDRW2100001088.1.14",
          "form": "잔잔한 게임..",
          "original_form": "잔잔한 게임..",
          "speaker_id": "2",
          "time": "20210723 16:13"
        },
        {
          "id": "MDRW2100001088.1.15",
          "form": "아 근데 게임 보는 건 좋아해요!",
          "original_form": "아 근데 게임 보는 건 좋아해요!",
          "speaker_id": "2",
          "time": "20210723 16:14"
        },
        {
          "id": "MDRW2100001088.1.16",
          "form": "아 어떤게임봐요?",
          "original_form": "아 어떤게임봐요?",
          "speaker_id": "1",
          "time": "20210723 16:15"
        },
        {
          "id": "MDRW2100001088.1.17",
          "form": "스타듀밸리는 안하고 동물의 숲은 해요!",
          "original_form": "스타듀밸리는 안하고 동물의 숲은 해요!",
          "speaker_id": "1",
          "time": "20210723 16:15"
        },
        {
          "id": "MDRW2100001088.1.18",
          "form": "게임 유튜브에 올라오는 스팀게임들이나 배그하는 거 보고.. 동숲도 봐요!",
          "original_form": "게임 유튜브에 올라오는 스팀게임들이나 배그하는 거 보고.. 동숲도 봐요!",
          "speaker_id": "2",
          "time": "20210723 16:16"
        },
        {
          "id": "MDRW2100001088.1.19",
          "form": "동숲 캐릭터들이 넘 귀여워요",
          "original_form": "동숲 캐릭터들이 넘 귀여워요",
          "speaker_id": "2",
          "time": "20210723 16:16"
        },
        {
          "id": "MDRW2100001088.1.20",
          "form": "ㅋㅋ맞아요 거기에 미애라는 캐릭터가 귀여워요",
          "original_form": "ㅋㅋ맞아요 거기에 미애라는 캐릭터가 귀여워요",
