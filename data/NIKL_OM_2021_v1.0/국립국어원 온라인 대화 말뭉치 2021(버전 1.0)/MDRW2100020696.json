      "utterance": [
        {
          "id": "MDRW2100020696.1.1",
          "form": "식당에서 디저트로 주는 아이스크림까지도",
          "original_form": "식당에서 디저트로 주는 아이스크림까지도",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.2",
          "form": "하 진짜 어디까지 절 감동시킬 생각이세요",
          "original_form": "하 진짜 어디까지 절 감동시킬 생각이세요",
          "speaker_id": "1",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.3",
          "form": "저는 식사로 칩니다",
          "original_form": "저는 식사로 칩니다",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.4",
          "form": "와 진짜",
          "original_form": "와 진짜",
          "speaker_id": "1",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.5",
          "form": "배우신분",
          "original_form": "배우신분",
          "speaker_id": "1",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.6",
          "form": "아이스크림 생각도 못했다",
          "original_form": "아이스크림 생각도 못했다",
          "speaker_id": "1",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.7",
          "form": "그 동그랗게 퍼담는 아이스크림",
          "original_form": "그 동그랗게 퍼담는 아이스크림",
          "speaker_id": "1",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.8",
          "form": "배움의 길은 끝이없네요",
          "original_form": "배움의 길은 끝이없네요",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.10",
          "form": "맛은 다 똑같아서 색 의미는 없지만",
          "original_form": "맛은 다 똑같아서 색 의미는 없지만",
          "speaker_id": "1",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.12",
          "form": "그렇죠 저는 굳이",
          "original_form": "그렇죠 저는 굳이",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020696.1.14",
          "form": "보라색을 푸긴 해요",
          "original_form": "보라색을 푸긴 해요",
          "speaker_id": "2",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020696.1.15",
          "form": "보라색 레어해요..",
          "original_form": "보라색 레어해요..",
          "speaker_id": "2",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020696.1.16",
          "form": "ㅇㄴ \r\n뭔가 \r\n보\r\n라\r\n색\r\n있으면\r\n열심히\r\n퍼\r\n담음",
          "original_form": "ㅇㄴ \r\n뭔가 \r\n보\r\n라\r\n색\r\n있으면\r\n열심히\r\n퍼\r\n담음",
          "speaker_id": "1",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020696.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저런",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저런",
          "speaker_id": "2",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020696.1.18",
          "form": "감도 안오넹",
          "original_form": "감도 안오넹",
          "speaker_id": "2",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020696.1.19",
          "form": "네요",
          "original_form": "네요",
          "speaker_id": "2",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020696.1.20",
          "form": "아 내가먼저 멀할 수 있었는데..!",
          "original_form": "아 내가먼저 멀할 수 있었는데..!",
          "speaker_id": "1",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020696.1.21",
          "form": "보라색을 아시다니",
          "original_form": "보라색을 아시다니",
          "speaker_id": "1",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020696.1.22",
          "form": "와 제가 말해서 친거 아닌가요",
          "original_form": "와 제가 말해서 친거 아닌가요",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020696.1.23",
          "form": "미리 생각하셨다고?",
          "original_form": "미리 생각하셨다고?",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020696.1.24",
          "form": "아닙니다 먼저 쳤어요",
          "original_form": "아닙니다 먼저 쳤어요",
          "speaker_id": "1",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020696.1.25",
          "form": "세상에 뭐야...",
          "original_form": "세상에 뭐야...",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020696.1.26",
          "form": "영혼의 단짝인가..",
          "original_form": "영혼의 단짝인가..",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020696.1.27",
          "form": "ㅇㄴ 캡쳐\r\n보여\r\n주고 \r\n싶다",
          "original_form": "ㅇㄴ 캡쳐\r\n보여\r\n주고 \r\n싶다",
          "speaker_id": "1",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020696.1.28",
          "form": "진짜 맛은 사랑하시는 분을 만나게되어 영광입니다",
          "original_form": "진짜 맛은 사랑하시는 분을 만나게되어 영광입니다",
          "speaker_id": "1",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020696.1.29",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210828 11:21"
        }
      ]
    }
  ]
