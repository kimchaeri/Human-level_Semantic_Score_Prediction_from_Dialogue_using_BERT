      "utterance": [
        {
          "id": "MDRW2100041730.1.1",
          "form": "점심컵라면먹었어여!",
          "original_form": "점심컵라면먹었어여!",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041730.1.2",
          "form": "구내식당가셧나용?",
          "original_form": "구내식당가셧나용?",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041730.1.3",
          "form": "네네 마자요 오 라면",
          "original_form": "네네 마자요 오 라면",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041730.1.4",
          "form": "저는 급식",
          "original_form": "저는 급식",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041730.1.5",
          "form": "노맛",
          "original_form": "노맛",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041730.1.6",
          "form": "컵라면도노맛",
          "original_form": "컵라면도노맛",
          "speaker_id": "2",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041730.1.7",
          "form": "맞아 요즘 컵라면 맛없어ㅠㅠ",
          "original_form": "맞아 요즘 컵라면 맛없어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041730.1.8",
          "form": "뭐가 맛있을까?",
          "original_form": "뭐가 맛있을까?",
          "speaker_id": "1",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041730.1.9",
          "form": "응 맨날같은거먹어서 더 노맛",
          "original_form": "응 맨날같은거먹어서 더 노맛",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041730.1.10",
          "form": "뭐먹지 맛있는거먹구싶다",
          "original_form": "뭐먹지 맛있는거먹구싶다",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041730.1.11",
          "form": "오늘올린이벵",
          "original_form": "오늘올린이벵",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041730.1.12",
          "form": "배라 아이스크림모찌  당첨돼서",
          "original_form": "배라 아이스크림모찌  당첨돼서",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041730.1.13",
          "form": "바꾸러갈까생각중ㅎㅁㅎ",
          "original_form": "바꾸러갈까생각중ㅎㅁㅎ",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041730.1.14",
          "form": "우와 모찌 당첨이구나",
          "original_form": "우와 모찌 당첨이구나",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041730.1.15",
          "form": "모찌 맛있어?",
          "original_form": "모찌 맛있어?",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041730.1.16",
          "form": "안먹어봤어!  그래서궁금해 *_*)/",
          "original_form": "안먹어봤어!  그래서궁금해 *_*)/",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041730.1.17",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041730.1.18",
          "form": "찰떡아이스느낌일깍!",
          "original_form": "찰떡아이스느낌일깍!",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041730.1.19",
          "form": "해피포인트앱없어? 간단한데ㅠㅡㅠ힝",
          "original_form": "해피포인트앱없어? 간단한데ㅠㅡㅠ힝",
          "speaker_id": "2",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041730.1.20",
          "form": "아 봤는데 뭐 하라는건지 모르겠어",
          "original_form": "아 봤는데 뭐 하라는건지 모르겠어",
          "speaker_id": "1",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100041730.1.21",
          "form": "아 했는데 도넛 천원 오프 당첨 ㅋㅋ",
          "original_form": "아 했는데 도넛 천원 오프 당첨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041730.1.22",
          "form": "ㅋㅋㅋㄱㄱ주사위사진 잘찾았네그래도?!",
          "original_form": "ㅋㅋㅋㄱㄱ주사위사진 잘찾았네그래도?!",
