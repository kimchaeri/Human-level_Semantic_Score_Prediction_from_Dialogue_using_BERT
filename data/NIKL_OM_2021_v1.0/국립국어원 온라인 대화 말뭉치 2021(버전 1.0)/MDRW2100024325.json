      "utterance": [
        {
          "id": "MDRW2100024325.1.1",
          "form": "내일 아침 뭐 먹을까",
          "original_form": "내일 아침 뭐 먹을까",
          "speaker_id": "1",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024325.1.2",
          "form": "먹고 싶은거 말해봐",
          "original_form": "먹고 싶은거 말해봐",
          "speaker_id": "2",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024325.1.3",
          "form": "콩국수 먹고시퍼",
          "original_form": "콩국수 먹고시퍼",
          "speaker_id": "1",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024325.1.4",
          "form": "안돼 불려놓은 콩이 없어",
          "original_form": "안돼 불려놓은 콩이 없어",
          "speaker_id": "2",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024325.1.5",
          "form": "아침에 불리면 되잖아",
          "original_form": "아침에 불리면 되잖아",
          "speaker_id": "1",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024325.1.6",
          "form": "아침에 불려놓고 갈아놓을테니까 저녁에 먹어",
          "original_form": "아침에 불려놓고 갈아놓을테니까 저녁에 먹어",
          "speaker_id": "2",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024325.1.7",
          "form": "그럼 아침에는 뭐먹어",
          "original_form": "그럼 아침에는 뭐먹어",
          "speaker_id": "1",
          "time": "20210831 00:50"
        },
        {
          "id": "MDRW2100024325.1.8",
          "form": "아침에는 계란찜 해줄게",
          "original_form": "아침에는 계란찜 해줄게",
          "speaker_id": "2",
          "time": "20210831 00:50"
        },
        {
          "id": "MDRW2100024325.1.9",
          "form": "집에 겉절이 있나여?",
          "original_form": "집에 겉절이 있나여?",
          "speaker_id": "1",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024325.1.10",
          "form": "옆집에서 준 거 있어",
          "original_form": "옆집에서 준 거 있어",
          "speaker_id": "2",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024325.1.11",
          "form": "그건 너무 짜던데ㅠ",
          "original_form": "그건 너무 짜던데ㅠ",
          "speaker_id": "1",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100024325.1.12",
          "form": "겉절이는 없고 오이김치는 있어",
          "original_form": "겉절이는 없고 오이김치는 있어",
          "speaker_id": "2",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100024325.1.13",
          "form": "그거라도 먹어야겠다...",
          "original_form": "그거라도 먹어야겠다...",
          "speaker_id": "1",
          "time": "20210831 00:53"
