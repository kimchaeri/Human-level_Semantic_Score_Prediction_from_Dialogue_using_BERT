      "utterance": [
        {
          "id": "MDRW2100044403.1.1",
          "form": "요즘 좋아하는 노래 뭔가용??",
          "original_form": "요즘 좋아하는 노래 뭔가용??",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044403.1.2",
          "form": "ㅠㅠ노래를 잘 안듣네요.....",
          "original_form": "ㅠㅠ노래를 잘 안듣네요.....",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044403.1.3",
          "form": "name1님은요???",
          "original_form": "&name1&님은요???",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044403.1.4",
          "form": "저는 블랙핑크요!! ㅎㅎ",
          "original_form": "저는 블랙핑크요!! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044403.1.5",
          "form": "아이돌 좋아하세요?",
          "original_form": "아이돌 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044403.1.6",
          "form": "아이돌은 잘 몰라요ㅠㅠ",
          "original_form": "아이돌은 잘 몰라요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044403.1.7",
          "form": "저도 요즘 아이돌은 모르겟어요 ㅠㅠ",
          "original_form": "저도 요즘 아이돌은 모르겟어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044403.1.8",
          "form": "예능 어떤거 보세요?",
          "original_form": "예능 어떤거 보세요?",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044403.1.9",
          "form": "우와 예능도 안봐요",
          "original_form": "우와 예능도 안봐요",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044403.1.10",
          "form": "심야괴담회만 유튜브 올라오면 보는 수준이예요",
          "original_form": "심야괴담회만 유튜브 올라오면 보는 수준이예요",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044403.1.11",
          "form": "오 그거 무섭나요,?",
          "original_form": "오 그거 무섭나요,?",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044403.1.12",
          "form": "네 전 무서워요 ㅋㅋㅋ",
          "original_form": "네 전 무서워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044403.1.13",
          "form": "토요미스테리처럼 약간의 재연이 들어가거든요 ㅋㅋㅋ",
          "original_form": "토요미스테리처럼 약간의 재연이 들어가거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044403.1.14",
          "form": "오오 맞아요 재연도 하드라구요!!!",
          "original_form": "오오 맞아요 재연도 하드라구요!!!",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044403.1.15",
          "form": "밤에 보면 안되겟어여 ㅠㅠ",
          "original_form": "밤에 보면 안되겟어여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044403.1.16",
          "form": "맞아요 무조건 낮에 이불 뒤집어쓰고 봐야 ㅋㅋ",
          "original_form": "맞아요 무조건 낮에 이불 뒤집어쓰고 봐야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044403.1.17",
          "form": "옛날에는 남량특집 예능 많이 해줬는데 그거말곤 요즘은 없네요 ㅠㅠ",
          "original_form": "옛날에는 남량특집 예능 많이 해줬는데 그거말곤 요즘은 없네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044403.1.18",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100044403.1.19",
          "form": "대신 유튜브에 많더라구요 ㅋㅋㅋ",
          "original_form": "대신 유튜브에 많더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100044403.1.20",
          "form": "맞아요!!! 유투브가 ㅋㅋ",
