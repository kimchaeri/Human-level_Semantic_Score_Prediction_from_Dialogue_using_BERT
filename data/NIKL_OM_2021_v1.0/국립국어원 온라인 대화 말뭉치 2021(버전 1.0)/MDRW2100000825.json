      "utterance": [
        {
          "id": "MDRW2100000825.1.1",
          "form": "헐 진짜여..?! 설거지 재밌긴해욬ㅋㅋㅋㅋ거품 뽀득뽀득",
          "original_form": "헐 진짜여..?! 설거지 재밌긴해욬ㅋㅋㅋㅋ거품 뽀득뽀득",
          "speaker_id": "1",
          "time": "20210819 19:47"
        },
        {
          "id": "MDRW2100000825.1.2",
          "form": "게임..........하시나여?",
          "original_form": "게임..........하시나여?",
          "speaker_id": "1",
          "time": "20210819 19:47"
        },
        {
          "id": "MDRW2100000825.1.3",
          "form": "저는 휴대폰으로만 소소하게 해요",
          "original_form": "저는 휴대폰으로만 소소하게 해요",
          "speaker_id": "2",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000825.1.4",
          "form": "캔디크러쉬랑 위베어베어스, 사천성 이런거요!",
          "original_form": "캔디크러쉬랑 위베어베어스, 사천성 이런거요!",
          "speaker_id": "2",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000825.1.5",
          "form": "게임하세요?",
          "original_form": "게임하세요?",
          "speaker_id": "2",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000825.1.6",
          "form": "헉 사천성 마니했어요 ㅋㅋㅋㅋ",
          "original_form": "헉 사천성 마니했어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000825.1.7",
          "form": "캔디크러쉬두 그 같은 색 맞추는거죠??? 재밌눈고 ㅋㅋㅋ",
          "original_form": "캔디크러쉬두 그 같은 색 맞추는거죠??? 재밌눈고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:52"
        },
        {
          "id": "MDRW2100000825.1.8",
          "form": "저는 온라인 폰게임 가리지않구 합니당!!!!",
          "original_form": "저는 온라인 폰게임 가리지않구 합니당!!!!",
          "speaker_id": "1",
          "time": "20210819 19:52"
        },
        {
          "id": "MDRW2100000825.1.9",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210819 19:52"
        },
        {
          "id": "MDRW2100000825.1.10",
          "form": "PC방가서 오버워치해 봤어요",
          "original_form": "PC방가서 오버워치해 봤어요",
          "speaker_id": "2",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000825.1.11",
          "form": "잘 못해서 재미는 없었지만ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잘 못해서 재미는 없었지만ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000825.1.12",
          "form": "오버워치....티어는 없지만 열시미했어요 ㅋㅋㅋ",
          "original_form": "오버워치....티어는 없지만 열시미했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000825.1.13",
          "form": "해본거면 그냥 몇번 해본 정도??인거죵?ㅋㅋㅋ",
          "original_form": "해본거면 그냥 몇번 해본 정도??인거죵?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000825.1.14",
          "form": "어떻게 하는지 잘 모르겠더라고요ㅋㅋㅋ",
          "original_form": "어떻게 하는지 잘 모르겠더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000825.1.15",
          "form": "네 두 번!",
          "original_form": "네 두 번!",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000825.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ두번 ㅠㅠㅠㅠ큐티하군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ두번 ㅠㅠㅠㅠ큐티하군요",
          "speaker_id": "1",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000825.1.17",
          "form": "쿠킹덤 아세용?!!!",
          "original_form": "쿠킹덤 아세용?!!!",
          "speaker_id": "1",
          "time": "20210819 20:20"
        },
        {
          "id": "MDRW2100000825.1.18",
          "form": "쿠키런 시리즈 신작인데!",
          "original_form": "쿠키런 시리즈 신작인데!",
