      "utterance": [
        {
          "id": "MDRW2100018495.1.1",
          "form": "음 지금 그 에피소드가 3회 했는데",
          "original_form": "음 지금 그 에피소드가 3회 했는데",
          "speaker_id": "2",
          "time": "20210827 06:28"
        },
        {
          "id": "MDRW2100018495.1.2",
          "form": "가장 최근",
          "original_form": "가장 최근",
          "speaker_id": "2",
          "time": "20210827 06:28"
        },
        {
          "id": "MDRW2100018495.1.3",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "1",
          "time": "20210827 06:28"
        },
        {
          "id": "MDRW2100018495.1.4",
          "form": "그니까 저번주꺼랑 이번주 이어지는데",
          "original_form": "그니까 저번주꺼랑 이번주 이어지는데",
          "speaker_id": "2",
          "time": "20210827 06:28"
        },
        {
          "id": "MDRW2100018495.1.5",
          "form": "이게 제일 재밌고",
          "original_form": "이게 제일 재밌고",
          "speaker_id": "2",
          "time": "20210827 06:28"
        },
        {
          "id": "MDRW2100018495.1.6",
          "form": "앞부분은 쏘쏘했어요",
          "original_form": "앞부분은 쏘쏘했어요",
          "speaker_id": "2",
          "time": "20210827 06:28"
        },
        {
          "id": "MDRW2100018495.1.7",
          "form": "악령감옥 보셨죠?",
          "original_form": "악령감옥 보셨죠?",
          "speaker_id": "2",
          "time": "20210827 06:28"
        },
        {
          "id": "MDRW2100018495.1.8",
          "form": "오!!! 그럼 그에피소드만 먼저 시작해도 무리 없을까요?",
          "original_form": "오!!! 그럼 그에피소드만 먼저 시작해도 무리 없을까요?",
          "speaker_id": "1",
          "time": "20210827 06:28"
        },
        {
          "id": "MDRW2100018495.1.9",
          "form": "네네! 이건 유니버스가 아니라 괜찮아용",
          "original_form": "네네! 이건 유니버스가 아니라 괜찮아용",
          "speaker_id": "2",
          "time": "20210827 06:29"
        },
        {
          "id": "MDRW2100018495.1.10",
          "form": "봐야겠어요 갑자기 넘 궁금하네요",
          "original_form": "봐야겠어요 갑자기 넘 궁금하네요",
          "speaker_id": "1",
          "time": "20210827 06:29"
        },
        {
          "id": "MDRW2100018495.1.11",
          "form": "악령감옥봤어요!",
          "original_form": "악령감옥봤어요!",
          "speaker_id": "1",
          "time": "20210827 06:29"
        },
        {
          "id": "MDRW2100018495.1.12",
          "form": "악령감옥이 시즌3까지 계속 나오는데 시즌3 악령감옥 회차가",
          "original_form": "악령감옥이 시즌3까지 계속 나오는데 시즌3 악령감옥 회차가",
          "speaker_id": "2",
          "time": "20210827 06:29"
        },
        {
          "id": "MDRW2100018495.1.13",
          "form": "그에피랑 비슷한 느낌일까요?",
          "original_form": "그에피랑 비슷한 느낌일까요?",
          "speaker_id": "1",
          "time": "20210827 06:29"
        },
        {
          "id": "MDRW2100018495.1.14",
          "form": "제일 재밌어요",
          "original_form": "제일 재밌어요",
          "speaker_id": "2",
          "time": "20210827 06:29"
        },
        {
          "id": "MDRW2100018495.1.15",
          "form": "헐!",
          "original_form": "헐!",
          "speaker_id": "1",
          "time": "20210827 06:29"
        },
        {
          "id": "MDRW2100018495.1.16",
          "form": "아 이번주껀",
          "original_form": "아 이번주껀",
          "speaker_id": "2",
          "time": "20210827 06:30"
        },
        {
          "id": "MDRW2100018495.1.17",
          "form": "완전 머리쓰는",
          "original_form": "완전 머리쓰는",
          "speaker_id": "2",
          "time": "20210827 06:30"
        },
        {
          "id": "MDRW2100018495.1.18",
          "form": "다른 얘긴데",
          "original_form": "다른 얘긴데",
          "speaker_id": "2",
          "time": "20210827 06:30"
        },
        {
          "id": "MDRW2100018495.1.19",
          "form": "느낌도 달라요!",
          "original_form": "느낌도 달라요!",
          "speaker_id": "2",
          "time": "20210827 06:30"
        },
        {
          "id": "MDRW2100018495.1.20",
          "form": "저 지금 대탈출 완전 영업당했어요",
          "original_form": "저 지금 대탈출 완전 영업당했어요",
          "speaker_id": "1",
          "time": "20210827 06:30"
        },
        {
          "id": "MDRW2100018495.1.21",
          "form": "갑자기 너무 보고싶어졌어요",
          "original_form": "갑자기 너무 보고싶어졌어요",
          "speaker_id": "1",
          "time": "20210827 06:30"
        },
        {
          "id": "MDRW2100018495.1.22",
          "form": "아ㅋㅋㅋ 꼭 보세용 제 최애 프로그램이에요",
          "original_form": "아ㅋㅋㅋ 꼭 보세용 제 최애 프로그램이에요",
          "speaker_id": "2",
          "time": "20210827 06:30"
        },
        {
          "id": "MDRW2100018495.1.23",
          "form": "이거 보시고 재밌으시면 티빙 오리지널 여고추리반도 추천드려용",
          "original_form": "이거 보시고 재밌으시면 티빙 오리지널 여고추리반도 추천드려용",
          "speaker_id": "2",
          "time": "20210827 06:31"
        }
      ]
    }
