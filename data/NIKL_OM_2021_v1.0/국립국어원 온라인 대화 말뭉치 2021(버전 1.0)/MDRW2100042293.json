      "utterance": [
        {
          "id": "MDRW2100042293.1.1",
          "form": "애들이 쨍한색 좋아하는거 같아요 ㅎㅎ",
          "original_form": "애들이 쨍한색 좋아하는거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042293.1.2",
          "form": "아! 그래서 그런건가보네요",
          "original_form": "아! 그래서 그런건가보네요",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042293.1.3",
          "form": "노랑도 좋아하던데요",
          "original_form": "노랑도 좋아하던데요",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042293.1.4",
          "form": "네네 노랑도 좋아해요 ㅎㅎ",
          "original_form": "네네 노랑도 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042293.1.5",
          "form": "근데 우리 작은애는 보라를 좋아합니다.",
          "original_form": "근데 우리 작은애는 보라를 좋아합니다.",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042293.1.6",
          "form": "오앙 보라도 예뿔거 같아요",
          "original_form": "오앙 보라도 예뿔거 같아요",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042293.1.7",
          "form": "여름엔 민트 좋아하긴하더라구요 ㅎ",
          "original_form": "여름엔 민트 좋아하긴하더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042293.1.8",
          "form": "시원해보인데요",
          "original_form": "시원해보인데요",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042293.1.9",
          "form": "민트색을 아는군요~",
          "original_form": "민트색을 아는군요~",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042293.1.10",
          "form": "네 ㅎㅎ   요즘엔 고르고 잇음 본인이 콕 찝고가니",
          "original_form": "네 ㅎㅎ   요즘엔 고르고 잇음 본인이 콕 찝고가니",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042293.1.11",
          "form": "전 청멜방 좋아하는데 아이는 청바지가 좋데여",
          "original_form": "전 청멜방 좋아하는데 아이는 청바지가 좋데여",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042293.1.12",
          "form": "저희애도 작은애는 입을옷을 지가 고르고 큰애는 그냥 주는대로 입어요 ㅎ",
          "original_form": "저희애도 작은애는 입을옷을 지가 고르고 큰애는 그냥 주는대로 입어요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 07:03"
        },
        {
          "id": "MDRW2100042293.1.13",
          "form": "ㅎㅎ 5살들이 패셔니 스타네요 ㅎㅎ",
          "original_form": "ㅎㅎ 5살들이 패셔니 스타네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100042293.1.14",
          "form": "아뇨 말도 안되게 입고 나갑니다 ㅋㅋ 패션 테러",
          "original_form": "아뇨 말도 안되게 입고 나갑니다 ㅋㅋ 패션 테러",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100042293.1.15",
          "form": "캐릭터옷 입을땐 ㅠㅠ  카봇 이런거 안습 ㅠㅁ ㅠ",
          "original_form": "캐릭터옷 입을땐 ㅠㅠ  카봇 이런거 안습 ㅠㅁ ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100042293.1.16",
          "form": "카봇 입으면 다행인데요 내복을 입고 간대요 ㅠ",
          "original_form": "카봇 입으면 다행인데요 내복을 입고 간대요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100042293.1.17",
          "form": "헉 ㅋㅋㅋㅋ이런",
          "original_form": "헉 ㅋㅋㅋㅋ이런",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100042293.1.18",
          "form": "그걸 고집을 부리니 제가 한숨이 절로 나요",
          "original_form": "그걸 고집을 부리니 제가 한숨이 절로 나요",
