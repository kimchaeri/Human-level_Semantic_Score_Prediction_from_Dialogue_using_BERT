      "utterance": [
        {
          "id": "MDRW2100022173.1.1",
          "form": "굿모닝",
          "original_form": "굿모닝",
          "speaker_id": "1",
          "time": "20210829 10:28"
        },
        {
          "id": "MDRW2100022173.1.2",
          "form": "좋은 아침입니다",
          "original_form": "좋은 아침입니다",
          "speaker_id": "1",
          "time": "20210829 10:28"
        },
        {
          "id": "MDRW2100022173.1.3",
          "form": "좋은 아침이네요",
          "original_form": "좋은 아침이네요",
          "speaker_id": "2",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100022173.1.4",
          "form": "비오고 개는 듯한 느낌 좋네요",
          "original_form": "비오고 개는 듯한 느낌 좋네요",
          "speaker_id": "2",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100022173.1.5",
          "form": "이제 여름이가고 가을이 오고있네요",
          "original_form": "이제 여름이가고 가을이 오고있네요",
          "speaker_id": "1",
          "time": "20210831 12:04"
        },
        {
          "id": "MDRW2100022173.1.6",
          "form": "가을 잠자리도 보이더라구요",
          "original_form": "가을 잠자리도 보이더라구요",
          "speaker_id": "1",
          "time": "20210831 12:04"
        },
        {
          "id": "MDRW2100022173.1.7",
          "form": "넹 저도 지난주말에 잠자리봤어요",
          "original_form": "넹 저도 지난주말에 잠자리봤어요",
          "speaker_id": "2",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100022173.1.8",
          "form": "로 즘 아파트에 귀뚜라미가 많이 나타나고 있습니다",
          "original_form": "로 즘 아파트에 귀뚜라미가 많이 나타나고 있습니다",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100022173.1.9",
          "form": "심지어 25층에도 나타나요",
          "original_form": "심지어 25층에도 나타나요",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100022173.1.10",
          "form": "어디서 들어오는 걸까요",
          "original_form": "어디서 들어오는 걸까요",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100022173.1.11",
          "form": "귀뚜라미가 25층요?",
          "original_form": "귀뚜라미가 25층요?",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100022173.1.12",
          "form": "저희집 지하실에도 있는듯",
          "original_form": "저희집 지하실에도 있는듯",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100022173.1.13",
          "form": "아 지하실이 있는집인가요?",
          "original_form": "아 지하실이 있는집인가요?",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100022173.1.14",
          "form": "좋은집입니다 !",
          "original_form": "좋은집입니다 !",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100022173.1.15",
          "form": "여름날 지하실 가면 시원할텐데요",
          "original_form": "여름날 지하실 가면 시원할텐데요",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100022173.1.16",
          "form": "창고로 쓰고있어서 가기 무서워요",
          "original_form": "창고로 쓰고있어서 가기 무서워요",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100022173.1.17",
          "form": "아 그럼 가지마요 무섭습니다",
          "original_form": "아 그럼 가지마요 무섭습니다",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100022173.1.18",
          "form": "오늘 시원했죠?",
          "original_form": "오늘 시원했죠?",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100022173.1.19",
          "form": "비왓나요?",
          "original_form": "비왓나요?",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100022173.1.20",
          "form": "네 비가오다안오다 하더라구요",
          "original_form": "네 비가오다안오다 하더라구요",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100022173.1.21",
          "form": "가을 장마라서 비가 조금씩 조금씩 오는 것 같습니다",
          "original_form": "가을 장마라서 비가 조금씩 조금씩 오는 것 같습니다",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100022173.1.22",
          "form": "생각보다 습하고 그러네요",
          "original_form": "생각보다 습하고 그러네요",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100022173.1.23",
          "form": "이번주 내내 이럴듯요",
          "original_form": "이번주 내내 이럴듯요",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100022173.1.24",
          "form": "오늘도 계속 흐림이네요 ㅜㅠ",
          "original_form": "오늘도 계속 흐림이네요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100022173.1.25",
          "form": "덜 더워서 좋긴합니다",
          "original_form": "덜 더워서 좋긴합니다",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100022173.1.26",
          "form": "얼른 겨울와서 스키타러 고고고",
          "original_form": "얼른 겨울와서 스키타러 고고고",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100022173.1.27",
          "form": "스키타세요?",
          "original_form": "스키타세요?",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100022173.1.28",
          "form": "아뇨 예전에 탔는데 다친 이후로 좀 트라우마가 있어서",
          "original_form": "아뇨 예전에 탔는데 다친 이후로 좀 트라우마가 있어서",
          "speaker_id": "2",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100022173.1.29",
          "form": "아 트라우마 ㅠ",
          "original_form": "아 트라우마 ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100022173.1.30",
          "form": "그거 극복 하기 쉽지 않죠",
          "original_form": "그거 극복 하기 쉽지 않죠",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100022173.1.31",
          "form": "겨울스포츠하다 다치면 몸이 많이 상하더라고요",
          "original_form": "겨울스포츠하다 다치면 몸이 많이 상하더라고요",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100022173.1.32",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100022173.1.33",
          "form": "추우니까 몸이 움츠러든 상태에서하다보니 부상이 더 큰듯해요",
          "original_form": "추우니까 몸이 움츠러든 상태에서하다보니 부상이 더 큰듯해요",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100022173.1.34",
          "form": "그래도 스키장의 어묵은 사랑합니다",
          "original_form": "그래도 스키장의 어묵은 사랑합니다",
          "speaker_id": "2",
          "time": "20210902 10:16"
        }
      ]
    }
