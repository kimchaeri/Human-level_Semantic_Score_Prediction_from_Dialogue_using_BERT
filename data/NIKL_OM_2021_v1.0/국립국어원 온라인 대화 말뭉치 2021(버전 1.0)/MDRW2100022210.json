      "utterance": [
        {
          "id": "MDRW2100022210.1.1",
          "form": "오케이광자ㅁ",
          "original_form": "오케이광자ㅁ",
          "speaker_id": "2",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100022210.1.2",
          "form": "펜트하우스요",
          "original_form": "펜트하우스요",
          "speaker_id": "2",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100022210.1.3",
          "form": "요즘 핫하길래",
          "original_form": "요즘 핫하길래",
          "speaker_id": "2",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100022210.1.4",
          "form": "저도 펜트하우스 봐요",
          "original_form": "저도 펜트하우스 봐요",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022210.1.5",
          "form": "이번주꺼 보셨나요",
          "original_form": "이번주꺼 보셨나요",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022210.1.6",
          "form": "좀전에 봤어요",
          "original_form": "좀전에 봤어요",
          "speaker_id": "2",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022210.1.7",
          "form": "어이가 없는..",
          "original_form": "어이가 없는..",
          "speaker_id": "2",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022210.1.8",
          "form": "좀 전에 보셨구나",
          "original_form": "좀 전에 보셨구나",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022210.1.9",
          "form": "ㅋㅋㅋ 맞아요 어이없어요",
          "original_form": "ㅋㅋㅋ 맞아요 어이없어요",
          "speaker_id": "1",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022210.1.10",
          "form": "억지에 억지를 더해서.. 너무 산으로 가더라구요",
          "original_form": "억지에 억지를 더해서.. 너무 산으로 가더라구요",
          "speaker_id": "1",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022210.1.11",
          "form": "이건 뭐 의리로 보려고 해도",
          "original_form": "이건 뭐 의리로 보려고 해도",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022210.1.12",
          "form": "너무 말이 안되니까ㅋㅋ",
          "original_form": "너무 말이 안되니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022210.1.13",
          "form": "ㅋㅋ 많이 봐줘서 시즌2까지는 그러려니했는데",
          "original_form": "ㅋㅋ 많이 봐줘서 시즌2까지는 그러려니했는데",
          "speaker_id": "1",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022210.1.14",
          "form": "시즌 3는..",
          "original_form": "시즌 3는..",
          "speaker_id": "1",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022210.1.15",
          "form": "특히 저번주 엔딩 이번주 오프닝 너무 잔인하지않았나요",
          "original_form": "특히 저번주 엔딩 이번주 오프닝 너무 잔인하지않았나요",
          "speaker_id": "1",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100022210.1.16",
          "form": "그러니까요..ㅋㅋ",
          "original_form": "그러니까요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022210.1.17",
          "form": "근데 또 멀쩡하게 살아나는게",
          "original_form": "근데 또 멀쩡하게 살아나는게",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022210.1.18",
          "form": "하긴.. 바로 옆에서 폭탄이 터져도",
          "original_form": "하긴.. 바로 옆에서 폭탄이 터져도",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022210.1.19",
          "form": "얼굴에 화상밴드 몇개 붙이고 끝이라",
          "original_form": "얼굴에 화상밴드 몇개 붙이고 끝이라",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022210.1.20",
          "form": "아 로건 진짜ㅋㅋㅋㅋ",
          "original_form": "아 로건 진짜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022210.1.21",
          "form": "그인간이 제일 말이 안되는거 공감합니다",
          "original_form": "그인간이 제일 말이 안되는거 공감합니다",
          "speaker_id": "1",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022210.1.22",
          "form": "재활도안하고 겁나 잘걸어ㅋㅋㅋ",
          "original_form": "재활도안하고 겁나 잘걸어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022210.1.23",
          "form": "그런데도 하박사는",
          "original_form": "그런데도 하박사는",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022210.1.24",
          "form": "머리 쿵 했다고",
          "original_form": "머리 쿵 했다고",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022210.1.25",
          "form": "못일어나고ㅋㅋㅋ",
          "original_form": "못일어나고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022210.1.26",
          "form": "ㅋㅋㅋ진짜ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ진짜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022210.1.27",
          "form": "너무어이없어요ㅋㅋㅋ",
          "original_form": "너무어이없어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100022210.1.28",
          "form": "샹들리에 떨어진 천서진도 멀쩡한데",
          "original_form": "샹들리에 떨어진 천서진도 멀쩡한데",
          "speaker_id": "1",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100022210.1.29",
          "form": "하박사가 못일어나는게ㅋㅋ",
          "original_form": "하박사가 못일어나는게ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100022210.1.30",
          "form": "다음주는 어떨거 같아요?",
          "original_form": "다음주는 어떨거 같아요?",
          "speaker_id": "2",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100022210.1.31",
          "form": "이지아",
          "original_form": "이지아",
          "speaker_id": "2",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100022210.1.32",
          "form": "다음주가 마지막화라..",
          "original_form": "다음주가 마지막화라..",
          "speaker_id": "1",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100022210.1.33",
          "form": "어.. 음.. 살지않을까요",
          "original_form": "어.. 음.. 살지않을까요",
          "speaker_id": "1",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100022210.1.34",
          "form": "로건도 살아나는 마당에ㅋㅋㅋ",
          "original_form": "로건도 살아나는 마당에ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100022210.1.35",
          "form": "펜트하우스부분부터",
          "original_form": "펜트하우스부분부터",
          "speaker_id": "2",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100022210.1.36",
          "form": "헬기 등장할지도...?",
          "original_form": "헬기 등장할지도...?",
          "speaker_id": "1",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100022210.1.37",
          "form": "고꾸라지던데..",
          "original_form": "고꾸라지던데..",
          "speaker_id": "2",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100022210.1.38",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100022210.1.39",
          "form": "헬기썰 낙하산썰",
          "original_form": "헬기썰 낙하산썰",
          "speaker_id": "2",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100022210.1.40",
          "form": "많던데ㅋㅋ",
          "original_form": "많던데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:43"
        }
      ]
    }
