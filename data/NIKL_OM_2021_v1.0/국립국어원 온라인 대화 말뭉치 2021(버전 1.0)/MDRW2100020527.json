      "utterance": [
        {
          "id": "MDRW2100020527.1.1",
          "form": "가루같은거 안넣고",
          "original_form": "가루같은거 안넣고",
          "speaker_id": "2",
          "time": "20210828 08:59"
        },
        {
          "id": "MDRW2100020527.1.2",
          "form": "맑은 맛에 먹어요",
          "original_form": "맑은 맛에 먹어요",
          "speaker_id": "2",
          "time": "20210828 08:59"
        },
        {
          "id": "MDRW2100020527.1.3",
          "form": "혹시 전 부쳐보셨나요",
          "original_form": "혹시 전 부쳐보셨나요",
          "speaker_id": "2",
          "time": "20210828 08:59"
        },
        {
          "id": "MDRW2100020527.1.4",
          "form": "맞아요 어릴 땐 몰랐는데 요즘은 그게 제일 맛있더라구요",
          "original_form": "맞아요 어릴 땐 몰랐는데 요즘은 그게 제일 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 08:59"
        },
        {
          "id": "MDRW2100020527.1.5",
          "form": "네 두부전 많이 뒤집었답니다 후..",
          "original_form": "네 두부전 많이 뒤집었답니다 후..",
          "speaker_id": "1",
          "time": "20210828 08:59"
        },
        {
          "id": "MDRW2100020527.1.6",
          "form": "맞아요 어릴 땐 싫었는데 크니까 세상맛있어",
          "original_form": "맞아요 어릴 땐 싫었는데 크니까 세상맛있어",
          "speaker_id": "2",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020527.1.7",
          "form": "두부전도 하세요?",
          "original_form": "두부전도 하세요?",
          "speaker_id": "2",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020527.1.8",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020527.1.9",
          "form": "네 두부부침!",
          "original_form": "네 두부부침!",
          "speaker_id": "1",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020527.1.10",
          "form": "짱맛있어요",
          "original_form": "짱맛있어요",
          "speaker_id": "1",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020527.1.11",
          "form": "재미님은 어떤 전 좋아하시나요",
          "original_form": "재미님은 어떤 전 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020527.1.12",
          "form": "두부의 고소함이 느껴지겠어요",
          "original_form": "두부의 고소함이 느껴지겠어요",
          "speaker_id": "2",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020527.1.13",
          "form": "저 산적 그 오색전 좋아하거든요",
          "original_form": "저 산적 그 오색전 좋아하거든요",
          "speaker_id": "2",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020527.1.14",
          "form": "맞아요 소금 많이 뿌려야해요",
          "original_form": "맞아요 소금 많이 뿌려야해요",
          "speaker_id": "1",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020527.1.15",
          "form": "앗 명절음식의 기본이죠",
          "original_form": "앗 명절음식의 기본이죠",
          "speaker_id": "1",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020527.1.16",
          "form": "전부치다가 진짜 너무 힘들어서",
          "original_form": "전부치다가 진짜 너무 힘들어서",
          "speaker_id": "2",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020527.1.17",
          "form": "손은 많이가지만...",
          "original_form": "손은 많이가지만...",
          "speaker_id": "1",
          "time": "20210828 09:01"
        },
        {
          "id": "MDRW2100020527.1.18",
          "form": "혹시 산적에 단무지넣으세요",
          "original_form": "혹시 산적에 단무지넣으세요",
          "speaker_id": "2",
          "time": "20210828 09:01"
        },
        {
          "id": "MDRW2100020527.1.19",
          "form": "우엉이나",
          "original_form": "우엉이나",
          "speaker_id": "2",
          "time": "20210828 09:01"
        },
        {
          "id": "MDRW2100020527.1.20",
          "form": "혹시 사서 드시나 그렇다면 더 여쭙지 않겠습니다",
          "original_form": "혹시 사서 드시나 그렇다면 더 여쭙지 않겠습니다",
          "speaker_id": "2",
          "time": "20210828 09:01"
        },
        {
          "id": "MDRW2100020527.1.21",
          "form": "저희는 손많이가서 뺀지 꽤 됐는데 단무지까지 넣은거같아요",
          "original_form": "저희는 손많이가서 뺀지 꽤 됐는데 단무지까지 넣은거같아요",
          "speaker_id": "1",
          "time": "20210828 09:01"
        },
        {
          "id": "MDRW2100020527.1.22",
          "form": "외가는 우엉도 넣습니다",
          "original_form": "외가는 우엉도 넣습니다",
          "speaker_id": "1",
          "time": "20210828 09:01"
        },
        {
          "id": "MDRW2100020527.1.23",
          "form": "어떠신가요 단무지랑 우엉넣는 산적..",
          "original_form": "어떠신가요 단무지랑 우엉넣는 산적..",
          "speaker_id": "2",
          "time": "20210828 09:02"
        },
        {
          "id": "MDRW2100020527.1.24",
          "form": "우엉은...",
          "original_form": "우엉은...",
          "speaker_id": "1",
          "time": "20210828 09:02"
        },
        {
          "id": "MDRW2100020527.1.25",
          "form": "우엉은 조금...",
          "original_form": "우엉은 조금...",
          "speaker_id": "1",
          "time": "20210828 09:02"
        },
        {
          "id": "MDRW2100020527.1.26",
          "form": "저는 개인적으로 불호..",
          "original_form": "저는 개인적으로 불호..",
          "speaker_id": "2",
          "time": "20210828 09:02"
        },
        {
          "id": "MDRW2100020527.1.27",
          "form": "우엉은 좀 그래요",
          "original_form": "우엉은 좀 그래요",
          "speaker_id": "1",
          "time": "20210828 09:02"
        },
        {
          "id": "MDRW2100020527.1.28",
          "form": "ㅋㅋㅋㅋ 비슷하시네요",
          "original_form": "ㅋㅋㅋㅋ 비슷하시네요",
          "speaker_id": "2",
          "time": "20210828 09:02"
        }
      ]
    }
  ]
