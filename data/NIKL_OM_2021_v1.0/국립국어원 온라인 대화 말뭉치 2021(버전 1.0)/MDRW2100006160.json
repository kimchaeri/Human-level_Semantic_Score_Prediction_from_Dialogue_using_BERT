      "utterance": [
        {
          "id": "MDRW2100006160.1.1",
          "form": "야구좋아하세요?",
          "original_form": "야구좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006160.1.2",
          "form": "네ㅋㅋ 야구 좋아하세요?",
          "original_form": "네ㅋㅋ 야구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006160.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006160.1.4",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006160.1.5",
          "form": "어느팀 응원하세요?",
          "original_form": "어느팀 응원하세요?",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006160.1.6",
          "form": "전 슥이요 ㅋㅋ",
          "original_form": "전 슥이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006160.1.7",
          "form": "전 두산이요",
          "original_form": "전 두산이요",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006160.1.8",
          "form": "오늘 우취되서 경기가 없네요",
          "original_form": "오늘 우취되서 경기가 없네요",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006160.1.9",
          "form": "여자분들 두산팬 많더라구요",
          "original_form": "여자분들 두산팬 많더라구요",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006160.1.10",
          "form": "가을에 왜이리 비가 오는지",
          "original_form": "가을에 왜이리 비가 오는지",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006160.1.11",
          "form": "야구는 맨날 취소고 ㅜㅜ",
          "original_form": "야구는 맨날 취소고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006160.1.12",
          "form": "다른팀 경기 켜놓고 보고있어요ㅋㅋ",
          "original_form": "다른팀 경기 켜놓고 보고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006160.1.13",
          "form": "한경기는 차이너무나서 노잼이네요 ㅋㅋ",
          "original_form": "한경기는 차이너무나서 노잼이네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006160.1.14",
          "form": "그래서 저도 롯데 삼성 보고 있어요ㅋㅋ",
          "original_form": "그래서 저도 롯데 삼성 보고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006160.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100006160.1.16",
          "form": "이제 야구 거의 끝자락이죠?",
          "original_form": "이제 야구 거의 끝자락이죠?",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100006160.1.17",
          "form": "작년이랑 비슷하게 끝날 것 같아요",
          "original_form": "작년이랑 비슷하게 끝날 것 같아요",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100006160.1.18",
          "form": "슥 잘나가다가",
          "original_form": "슥 잘나가다가",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100006160.1.19",
          "form": "선발 망해서",
          "original_form": "선발 망해서",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100006160.1.20",
          "form": "내리막길 걷고있어서",
          "original_form": "내리막길 걷고있어서",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100006160.1.21",
          "form": "요즘 야구 잠시 안보고 있었는데",
          "original_form": "요즘 야구 잠시 안보고 있었는데",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100006160.1.22",
          "form": "메이저리그만보고 ㅋㅋ",
          "original_form": "메이저리그만보고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100006160.1.23",
          "form": "아무래도 문승원 박종훈 수술이 컸죠ㅠ",
          "original_form": "아무래도 문승원 박종훈 수술이 컸죠ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100006160.1.24",
          "form": "스포티비 결제하셨나봐요",
          "original_form": "스포티비 결제하셨나봐요",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100006160.1.25",
          "form": "네 마자요",
          "original_form": "네 마자요",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100006160.1.26",
          "form": "외인도 한명 부상",
          "original_form": "외인도 한명 부상",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100006160.1.27",
          "form": "대체용병도 못하고",
          "original_form": "대체용병도 못하고",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100006160.1.28",
          "form": "스포티비 안해도 한경기씩 해줘요 ㅎㅎ",
          "original_form": "스포티비 안해도 한경기씩 해줘요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100006160.1.29",
          "form": "네이버에서 하이라이트도 보구요",
          "original_form": "네이버에서 하이라이트도 보구요",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100006160.1.30",
          "form": "김광현 경기 위주로 보시나요?ㅎㅎ",
          "original_form": "김광현 경기 위주로 보시나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006160.1.31",
          "form": "현진이도 봐요",
          "original_form": "현진이도 봐요",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006160.1.32",
          "form": "근데 동부라서",
          "original_form": "근데 동부라서",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006160.1.33",
          "form": "시간이 애매하더라구요",
          "original_form": "시간이 애매하더라구요",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006160.1.34",
          "form": "광현이는 내년에 돌아올거 같아요",
          "original_form": "광현이는 내년에 돌아올거 같아요",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006160.1.35",
          "form": "작년처럼 못해서",
          "original_form": "작년처럼 못해서",
          "speaker_id": "2",
          "time": "20210907 21:26"
        }
      ]
    }
