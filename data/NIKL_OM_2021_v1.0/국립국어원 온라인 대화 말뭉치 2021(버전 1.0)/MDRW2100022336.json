      "utterance": [
        {
          "id": "MDRW2100022336.1.1",
          "form": "최근에 빕스 가보셨어요?",
          "original_form": "최근에 빕스 가보셨어요?",
          "speaker_id": "2",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022336.1.2",
          "form": "빕스가서 뭐 드셔써요",
          "original_form": "빕스가서 뭐 드셔써요",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022336.1.3",
          "form": "빕스가 거긴가",
          "original_form": "빕스가 거긴가",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.4",
          "form": "저 빕스에서 이것저것 다 먹었잖아요 ㅠㅠ",
          "original_form": "저 빕스에서 이것저것 다 먹었잖아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.5",
          "form": "식전빵 죽는곳",
          "original_form": "식전빵 죽는곳",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.6",
          "form": "아닌데",
          "original_form": "아닌데",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.7",
          "form": "거긴 아웃백이에요 ㅋㅋㅋ",
          "original_form": "거긴 아웃백이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.8",
          "form": "아  그건 아웃백이ㅡㄱㆍ나",
          "original_form": "아  그건 아웃백이ㅡㄱㆍ나",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.9",
          "form": "빕스는 샐러드바 있고 스테이크 팔아요",
          "original_form": "빕스는 샐러드바 있고 스테이크 팔아요",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.10",
          "form": "아웃백이구나",
          "original_form": "아웃백이구나",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.11",
          "form": "빕스가봤어요",
          "original_form": "빕스가봤어요",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.12",
          "form": "거기서 쌀국수 먹고 뱉은거같은데",
          "original_form": "거기서 쌀국수 먹고 뱉은거같은데",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.13",
          "form": "거기 감바스에 마늘빵 완전 대박인거 있죠",
          "original_form": "거기 감바스에 마늘빵 완전 대박인거 있죠",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.14",
          "form": "ㅋㅋㅋㅋ쌀국수는...",
          "original_form": "ㅋㅋㅋㅋ쌀국수는...",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.15",
          "form": "이번엔 안먹었네요",
          "original_form": "이번엔 안먹었네요",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.16",
          "form": "ㅋㅋㅋ 그쵸",
          "original_form": "ㅋㅋㅋ 그쵸",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.17",
          "form": "감바스도 있구나",
          "original_form": "감바스도 있구나",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022336.1.18",
          "form": "나날히 발전하네요",
          "original_form": "나날히 발전하네요",
          "speaker_id": "1",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022336.1.19",
          "form": "네 새로 생겼더라구요 이번에",
          "original_form": "네 새로 생겼더라구요 이번에",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022336.1.20",
          "form": "새우가 탱글통글 했어요",
          "original_form": "새우가 탱글통글 했어요",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022336.1.21",
          "form": "안에 가리비인가 조개인가 홍합인가 뭐 들어있고",
          "original_form": "안에 가리비인가 조개인가 홍합인가 뭐 들어있고",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022336.1.22",
          "form": "전 거기가서 볶음밥 많이 먹었던거같은..",
          "original_form": "전 거기가서 볶음밥 많이 먹었던거같은..",
          "speaker_id": "1",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022336.1.23",
          "form": "ㅋㅋㅋ아 볶음밥..",
          "original_form": "ㅋㅋㅋ아 볶음밥..",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022336.1.24",
          "form": "맛있었겠어요",
          "original_form": "맛있었겠어요",
          "speaker_id": "1",
          "time": "20210829 12:47"
        }
      ]
    }
