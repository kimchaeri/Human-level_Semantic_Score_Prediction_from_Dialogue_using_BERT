      "utterance": [
        {
          "id": "MDRW2100028603.1.1",
          "form": "웃기지좀말자...",
          "original_form": "웃기지좀말자...",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028603.1.2",
          "form": "쁘걸이 알아서 떴다..",
          "original_form": "쁘걸이 알아서 떴다..",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028603.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ자기 돌이 모델인데 불매하는거 진짜....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ자기 돌이 모델인데 불매하는거 진짜....",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028603.1.4",
          "form": "노 닥",
          "original_form": "노 닥",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028603.1.5",
          "form": "답",
          "original_form": "답",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028603.1.6",
          "form": "글고 진짜 팬질하는 사람들은",
          "original_form": "글고 진짜 팬질하는 사람들은",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028603.1.7",
          "form": "20대 중반이지 무슨 지들이 ㅡㅡ",
          "original_form": "20대 중반이지 무슨 지들이 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028603.1.8",
          "form": "성준이가 군대있을대 나온노랜데",
          "original_form": "성준이가 군대있을대 나온노랜데",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028603.1.9",
          "form": "무슨340대 아재들이",
          "original_form": "무슨340대 아재들이",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028603.1.10",
          "form": "지들덕에 떴다는거 웃겨",
          "original_form": "지들덕에 떴다는거 웃겨",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028603.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞어\r\n그리고 여팬들도 나이 많은 사람 있는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞어\r\n그리고 여팬들도 나이 많은 사람 있는데",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028603.1.12",
          "form": "자기들만 나이있다는 착각 쩔엉",
          "original_form": "자기들만 나이있다는 착각 쩔엉",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028603.1.13",
          "form": "아 여기도 전문직 있어.....",
          "original_form": "아 여기도 전문직 있어.....",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028603.1.14",
          "form": "뭔 학생 빠순이들이만 있는 줄 알아",
          "original_form": "뭔 학생 빠순이들이만 있는 줄 알아",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028603.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ여기도 전문직 이고 여기가 더 다양해.........",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ여기도 전문직 이고 여기가 더 다양해.........",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028603.1.16",
          "form": "진짜 고리타분 아저씨들이야",
          "original_form": "진짜 고리타분 아저씨들이야",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028603.1.17",
          "form": "여기 연령대가 훨씬 다양한데",
          "original_form": "여기 연령대가 훨씬 다양한데",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028603.1.18",
          "form": "도시락 사기나 당한 멍청이들이",
          "original_form": "도시락 사기나 당한 멍청이들이",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028603.1.19",
          "form": "무슨 자기들이 엄청뭐라도 된ㄴ줄알아",
          "original_form": "무슨 자기들이 엄청뭐라도 된ㄴ줄알아",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028603.1.20",
          "form": "이 대단한 내가 처음으로 아이돌 덕질을 하고",
          "original_form": "이 대단한 내가 처음으로 아이돌 덕질을 하고",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028603.1.21",
          "form": "그게 너희야",
          "original_form": "그게 너희야",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028603.1.22",
          "form": "이거에 취한 아저씨들..",
          "original_form": "이거에 취한 아저씨들..",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028603.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028603.1.24",
          "form": "아진짜 ㄹㅇ",
          "original_form": "아진짜 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028603.1.25",
          "form": "그말이딱이네",
          "original_form": "그말이딱이네",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028603.1.26",
          "form": "사람들이 웃기다 재밌다 해주니",
          "original_form": "사람들이 웃기다 재밌다 해주니",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028603.1.27",
          "form": "진짜 뭐라도된ㄴ줄 알고",
          "original_form": "진짜 뭐라도된ㄴ줄 알고",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028603.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞어... 진짜 걍 아저씨 1,2면서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞어... 진짜 걍 아저씨 1,2면서",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028603.1.29",
          "form": "착각 심해",
          "original_form": "착각 심해",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028603.1.30",
          "form": "쁘걸 노래는 애초에",
          "original_form": "쁘걸 노래는 애초에",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028603.1.31",
          "form": "케톡에서도 숨듣명이었음....",
          "original_form": "케톡에서도 숨듣명이었음....",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028603.1.32",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028603.1.33",
          "form": "원래 알음알음 꼽히던 노래였는데",
          "original_form": "원래 알음알음 꼽히던 노래였는데",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028603.1.34",
          "form": "그리고 예비역들이 좋아해줬던건 놀라운일이지만",
          "original_form": "그리고 예비역들이 좋아해줬던건 놀라운일이지만",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028603.1.35",
          "form": "그이후는 340대로 자리잡고",
          "original_form": "그이후는 340대로 자리잡고",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028603.1.36",
          "form": "분위기망침",
          "original_form": "분위기망침",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028603.1.37",
          "form": "덕질대상이 중요한게아니라",
          "original_form": "덕질대상이 중요한게아니라",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028603.1.38",
          "form": "덕질하는 \"내\"가 중요한거",
          "original_form": "덕질하는 \"내\"가 중요한거",
