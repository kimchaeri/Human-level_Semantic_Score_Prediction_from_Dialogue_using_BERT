      "utterance": [
        {
          "id": "MDRW2100026557.1.1",
          "form": "오 쇼핑이라 저 지금 가방 보고 있어요",
          "original_form": "오 쇼핑이라 저 지금 가방 보고 있어요",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026557.1.2",
          "form": "오 어떤 브랜드요",
          "original_form": "오 어떤 브랜드요",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026557.1.3",
          "form": "잘 몰라서 그냥 적당히 검색만 해보고 있는데 어렵네요ㅋㅋㅋ",
          "original_form": "잘 몰라서 그냥 적당히 검색만 해보고 있는데 어렵네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026557.1.4",
          "form": "미니백 생각중인데 사이즈가 죄다 괜찮나 싶어서ㅋㅋ",
          "original_form": "미니백 생각중인데 사이즈가 죄다 괜찮나 싶어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026557.1.5",
          "form": "오 방금 검색해봤는데 귀엽네요",
          "original_form": "오 방금 검색해봤는데 귀엽네요",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026557.1.6",
          "form": "맞아요 큰 가방 있어도 결국은 미니백 아님 에코백",
          "original_form": "맞아요 큰 가방 있어도 결국은 미니백 아님 에코백",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026557.1.7",
          "form": "와 맞아요 저 요새 에코백만 들고다님",
          "original_form": "와 맞아요 저 요새 에코백만 들고다님",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026557.1.8",
          "form": "코시국이라",
          "original_form": "코시국이라",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026557.1.9",
          "form": "캐쥬얼이 대세예요",
          "original_form": "캐쥬얼이 대세예요",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026557.1.10",
          "form": "전 노트북 들고 나가서 카페서 노는 거 좋아하는데",
          "original_form": "전 노트북 들고 나가서 카페서 노는 거 좋아하는데",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026557.1.11",
          "form": "큰 가방.. 가죽 이런 거 쓰면 너무 무거워서ㅋㅋㅋ",
          "original_form": "큰 가방.. 가죽 이런 거 쓰면 너무 무거워서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026557.1.12",
          "form": "저는 노트북 일키로도 안되는데",
          "original_form": "저는 노트북 일키로도 안되는데",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026557.1.13",
          "form": "요샌 딱 맞는 에코백이 최고라고 생각합니다",
          "original_form": "요샌 딱 맞는 에코백이 최고라고 생각합니다",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026557.1.14",
          "form": "드는 걸 싫어해서",
          "original_form": "드는 걸 싫어해서",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026557.1.15",
          "form": "걍 핸펀만해요",
          "original_form": "걍 핸펀만해요",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026557.1.16",
          "form": "근데 미니백 사이즈면",
          "original_form": "근데 미니백 사이즈면",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026557.1.17",
          "form": "에코백도 안되는게 텀블러처럼 또 여러개 가지고 있게돼요",
          "original_form": "에코백도 안되는게 텀블러처럼 또 여러개 가지고 있게돼요",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026557.1.18",
          "form": "어느정도 물건이 들어가나요?",
          "original_form": "어느정도 물건이 들어가나요?",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026557.1.19",
          "form": "폰 지갑 보조배터리는 다 들어가려나",
          "original_form": "폰 지갑 보조배터리는 다 들어가려나",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026557.1.20",
          "form": "지갑 없구 폰 핸펀충전기 끝 ㅋㅋ",
          "original_form": "지갑 없구 폰 핸펀충전기 끝 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026557.1.21",
          "form": "회사가지고 가실 가방 찾으세요?",
          "original_form": "회사가지고 가실 가방 찾으세요?",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026557.1.22",
          "form": "아뇨 그냥 가볍게 다닐 느낌??",
          "original_form": "아뇨 그냥 가볍게 다닐 느낌??",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026557.1.23",
          "form": "큰가방은 충분히 많아서ㅠㅜ 어렵네요",
          "original_form": "큰가방은 충분히 많아서ㅠㅜ 어렵네요",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026557.1.24",
          "form": "가격대는요?",
          "original_form": "가격대는요?",
          "speaker_id": "1",
          "time": "20210901 23:07"
        }
      ]
    }
