      "utterance": [
        {
          "id": "MDRW2100019187.1.1",
          "form": "아하! 전 22살이에욬ㅋㅋ",
          "original_form": "아하! 전 22살이에욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019187.1.2",
          "form": "아잌ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아잌ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019187.1.3",
          "form": "2살차이군여!!!",
          "original_form": "2살차이군여!!!",
          "speaker_id": "2",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019187.1.4",
          "form": "ㅋㅋㅋㅋㅋ 그러네요 ㅋㅋ 혹시요새 자주보시는 드라마나 예능 있나요??",
          "original_form": "ㅋㅋㅋㅋㅋ 그러네요 ㅋㅋ 혹시요새 자주보시는 드라마나 예능 있나요??",
          "speaker_id": "1",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100019187.1.5",
          "form": "슬의생보구있구",
          "original_form": "슬의생보구있구",
          "speaker_id": "2",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100019187.1.6",
          "form": "블랙독이요!!!",
          "original_form": "블랙독이요!!!",
          "speaker_id": "2",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100019187.1.7",
          "form": "대탈출은 본방사수고!",
          "original_form": "대탈출은 본방사수고!",
          "speaker_id": "2",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100019187.1.8",
          "form": "아하! 슬의생 주변에서 다들 보는것같네요 정말 ㅋㅋ",
          "original_form": "아하! 슬의생 주변에서 다들 보는것같네요 정말 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100019187.1.9",
          "form": "대탈출 재밌나요? 볼까말까 고민중이에요",
          "original_form": "대탈출 재밌나요? 볼까말까 고민중이에요",
          "speaker_id": "1",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100019187.1.10",
          "form": "음...대탈출 재밌어여!!!",
          "original_form": "음...대탈출 재밌어여!!!",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019187.1.11",
          "form": "저 세트장에거",
          "original_form": "저 세트장에거",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019187.1.12",
          "form": "방탈출 하고싶다",
          "original_form": "방탈출 하고싶다",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019187.1.13",
          "form": "이런느낌",
          "original_form": "이런느낌",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019187.1.14",
          "form": "아하! 오늘부터 정주행각인데요?ㅋㅋㅋ",
          "original_form": "아하! 오늘부터 정주행각인데요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100019187.1.15",
          "form": "시즌1부터봐야겧죠?",
          "original_form": "시즌1부터봐야겧죠?",
          "speaker_id": "1",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100019187.1.16",
          "form": "아낭 물론이죠!!!!!!!!!!!!!!!",
          "original_form": "아낭 물론이죠!!!!!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019187.1.17",
          "form": "오케이 접수완료!",
          "original_form": "오케이 접수완료!",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019187.1.18",
          "form": "아 좋죠좋죠!!!!",
          "original_form": "아 좋죠좋죠!!!!",
          "speaker_id": "2",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019187.1.19",
          "form": "곧 개강인게 안믿겨여 ㅠㅠ",
          "original_form": "곧 개강인게 안믿겨여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019187.1.20",
          "form": "그니깐요 ㅠㅠㅠ",
          "original_form": "그니깐요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019187.1.21",
          "form": "혹시 요번학기 대면인가요?",
          "original_form": "혹시 요번학기 대면인가요?",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019187.1.22",
          "form": "음...저는",
          "original_form": "음...저는",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019187.1.23",
          "form": "혼합이여!",
          "original_form": "혼합이여!",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019187.1.24",
          "form": "전공은 어떨게 되세여?!",
          "original_form": "전공은 어떨게 되세여?!",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019187.1.25",
          "form": "아 그렇구나..",
          "original_form": "아 그렇구나..",
          "speaker_id": "1",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019187.1.26",
          "form": "고민하다 이거 픽했어요 ㅋㅋ",
          "original_form": "고민하다 이거 픽했어요 ㅋㅋ",
          "speaker_id": "1",
