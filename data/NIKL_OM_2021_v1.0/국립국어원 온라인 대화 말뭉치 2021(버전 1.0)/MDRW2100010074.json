      "utterance": [
        {
          "id": "MDRW2100010074.1.1",
          "form": "가족들이랑 함께 사시나요?",
          "original_form": "가족들이랑 함께 사시나요?",
          "speaker_id": "1",
          "time": "20210823 14:09"
        },
        {
          "id": "MDRW2100010074.1.2",
          "form": "와이프랑 둘이 살아요 ㅎㅎ",
          "original_form": "와이프랑 둘이 살아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100010074.1.3",
          "form": "아 ㅎㅎ 저는 아직 결혼전이라 부모님과 함께 살고 있어요",
          "original_form": "아 ㅎㅎ 저는 아직 결혼전이라 부모님과 함께 살고 있어요",
          "speaker_id": "1",
          "time": "20210823 16:41"
        },
        {
          "id": "MDRW2100010074.1.4",
          "form": "ㅋㅋ그때가 그리울거에요",
          "original_form": "ㅋㅋ그때가 그리울거에요",
          "speaker_id": "2",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010074.1.5",
          "form": "집안일은 도우시나요",
          "original_form": "집안일은 도우시나요",
          "speaker_id": "2",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010074.1.6",
          "form": "도우려고 마음만 늘 먹어요......ㅎㅎ",
          "original_form": "도우려고 마음만 늘 먹어요......ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 17:29"
        },
        {
          "id": "MDRW2100010074.1.7",
          "form": "마음이 중요하죠!",
          "original_form": "마음이 중요하죠!",
          "speaker_id": "2",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010074.1.8",
          "form": "이제 슬슬 독립을 해야하나 생각중인데",
          "original_form": "이제 슬슬 독립을 해야하나 생각중인데",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010074.1.9",
          "form": "역시 금전적인 부분이 제일 걸리는거 같아요 ㅎㅎ",
          "original_form": "역시 금전적인 부분이 제일 걸리는거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010074.1.10",
          "form": "어후 집나오면 돈입니다",
          "original_form": "어후 집나오면 돈입니다",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010074.1.11",
          "form": "다들 그러더라고요 ㅋㅋㅋ",
          "original_form": "다들 그러더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010074.1.12",
          "form": "최대한 붙어있으라고요",
          "original_form": "최대한 붙어있으라고요",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010074.1.13",
          "form": "네 결혼전까지는 굳이!",
          "original_form": "네 결혼전까지는 굳이!",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010074.1.14",
          "form": "그래도 마음 한켠에는 늘 혼자 사는 삶을 꿈꾸게 되는거 같아요ㅎㅎ",
          "original_form": "그래도 마음 한켠에는 늘 혼자 사는 삶을 꿈꾸게 되는거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 09:53"
        },
        {
          "id": "MDRW2100010074.1.15",
          "form": "ㅋㅋ자취 매력있긴하죠",
          "original_form": "ㅋㅋ자취 매력있긴하죠",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100010074.1.16",
          "form": "자취하는 친구들이 막 좋다고 하니까 더 끌려요 ㅎㅎ",
          "original_form": "자취하는 친구들이 막 좋다고 하니까 더 끌려요 ㅎㅎ",
          "speaker_id": "1",
