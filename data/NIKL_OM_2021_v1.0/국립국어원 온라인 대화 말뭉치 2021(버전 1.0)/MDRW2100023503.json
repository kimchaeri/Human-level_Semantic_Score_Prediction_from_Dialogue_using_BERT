      "utterance": [
        {
          "id": "MDRW2100023503.1.1",
          "form": "오늘 날씨 선선하네요! 어제처럼!!",
          "original_form": "오늘 날씨 선선하네요! 어제처럼!!",
          "speaker_id": "2",
          "time": "20210830 10:18"
        },
        {
          "id": "MDRW2100023503.1.2",
          "form": "근데 나가면 덥겠죠ㅜㅜ",
          "original_form": "근데 나가면 덥겠죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 10:18"
        },
        {
          "id": "MDRW2100023503.1.3",
          "form": "흑 맞아요",
          "original_form": "흑 맞아요",
          "speaker_id": "1",
          "time": "20210830 10:22"
        },
        {
          "id": "MDRW2100023503.1.4",
          "form": "지하철은 엄청 추웠어요!",
          "original_form": "지하철은 엄청 추웠어요!",
          "speaker_id": "1",
          "time": "20210830 10:22"
        },
        {
          "id": "MDRW2100023503.1.5",
          "form": "겉옷 입고 다니니까 또 덥고...",
          "original_form": "겉옷 입고 다니니까 또 덥고...",
          "speaker_id": "1",
          "time": "20210830 10:22"
        },
        {
          "id": "MDRW2100023503.1.6",
          "form": "그리고 아까 출근길에 빗방울이 떨어지더라구요?!",
          "original_form": "그리고 아까 출근길에 빗방울이 떨어지더라구요?!",
          "speaker_id": "1",
          "time": "20210830 10:22"
        },
        {
          "id": "MDRW2100023503.1.7",
          "form": "그렇군요ㅜㅜ",
          "original_form": "그렇군요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100023503.1.8",
          "form": "오늘 긴바지는 포기해야겠네요ㅜㅜ",
          "original_form": "오늘 긴바지는 포기해야겠네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100023503.1.9",
          "form": "실내에서 입을 긴팔 정도만 챙겨야겠어요",
          "original_form": "실내에서 입을 긴팔 정도만 챙겨야겠어요",
          "speaker_id": "2",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100023503.1.10",
          "form": "네에 그리고 왠지 모르게 습하답니다...",
          "original_form": "네에 그리고 왠지 모르게 습하답니다...",
          "speaker_id": "1",
          "time": "20210830 10:49"
        },
        {
          "id": "MDRW2100023503.1.11",
          "form": "오늘 외출하시나요?",
          "original_form": "오늘 외출하시나요?",
          "speaker_id": "1",
          "time": "20210830 10:49"
        },
        {
          "id": "MDRW2100023503.1.12",
          "form": "네 오늘 점심 먹으러요! ㅋㅋㅋㅋㅋ",
          "original_form": "네 오늘 점심 먹으러요! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023503.1.13",
          "form": "중요한 스케줄이에요...",
          "original_form": "중요한 스케줄이에요...",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023503.1.14",
          "form": "오늘 너무 습하고 덥네요!!",
          "original_form": "오늘 너무 습하고 덥네요!!",
          "speaker_id": "2",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023503.1.15",
          "form": "은근히 너무 더워요ㅜㅜ",
          "original_form": "은근히 너무 더워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023503.1.16",
          "form": "우와우와",
          "original_form": "우와우와",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023503.1.17",
          "form": "오늘 친구 만나시나요?",
          "original_form": "오늘 친구 만나시나요?",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023503.1.18",
          "form": "그러면 우산 챙겨가세요!!!!",
          "original_form": "그러면 우산 챙겨가세요!!!!",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023503.1.19",
          "form": "언제든 비가 와도 이상하지 않은 날씨예요!!",
          "original_form": "언제든 비가 와도 이상하지 않은 날씨예요!!",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023503.1.20",
          "form": "네! 점심 먹으러 가서 저녁 때 돌아왔어요...",
          "original_form": "네! 점심 먹으러 가서 저녁 때 돌아왔어요...",
          "speaker_id": "2",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023503.1.21",
          "form": "시간가는 줄 몰랐네요! ㅋㅋㅋ",
          "original_form": "시간가는 줄 몰랐네요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023503.1.22",
          "form": "우산 챙겨갔는데 비가 안왔네요ㅜㅜ",
          "original_form": "우산 챙겨갔는데 비가 안왔네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023503.1.23",
          "form": "역시 요즘 날씨는 종잡을 수가 없어요",
          "original_form": "역시 요즘 날씨는 종잡을 수가 없어요",
          "speaker_id": "2",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023503.1.24",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100023503.1.25",
          "form": "아 그 동네에는 비가 왔나요?",
          "original_form": "아 그 동네에는 비가 왔나요?",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100023503.1.26",
          "form": "제가 오늘 비 피해서 잘 다녔나봐요...",
          "original_form": "제가 오늘 비 피해서 잘 다녔나봐요...",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100023503.1.27",
          "form": "비 안왔어요!! 근데 하루종일 올거같은 날씨이긴 했어요ㅜㅜ",
          "original_form": "비 안왔어요!! 근데 하루종일 올거같은 날씨이긴 했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100023503.1.28",
          "form": "내일부터는 비가 계속 온다네요!!",
          "original_form": "내일부터는 비가 계속 온다네요!!",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100023503.1.29",
          "form": "우산 꼭 챙기세요~",
          "original_form": "우산 꼭 챙기세요~",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100023503.1.30",
          "form": "헉 일기예보 감사해요",
          "original_form": "헉 일기예보 감사해요",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100023503.1.31",
          "form": "내일은 장화신고 가야겠어요!!!",
          "original_form": "내일은 장화신고 가야겠어요!!!",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100023503.1.32",
          "form": "장우산도 들고 가야지요~",
          "original_form": "장우산도 들고 가야지요~",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100023503.1.33",
          "form": "아하 장화 부러워요!",
          "original_form": "아하 장화 부러워요!",
          "speaker_id": "2",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100023503.1.34",
          "form": "저는 큰 장우산은 샀어요!",
          "original_form": "저는 큰 장우산은 샀어요!",
          "speaker_id": "2",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100023503.1.35",
          "form": "우산살 70cm짜리인데 가방까지 넉넉히 가려줘서 좋아요!",
          "original_form": "우산살 70cm짜리인데 가방까지 넉넉히 가려줘서 좋아요!",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100023503.1.36",
          "form": "우와 70cm 라니",
          "original_form": "우와 70cm 라니",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100023503.1.37",
          "form": "우산 살 때 길이까지 재본 적이 없어서",
          "original_form": "우산 살 때 길이까지 재본 적이 없어서",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100023503.1.38",
          "form": "감은 안오지만",
          "original_form": "감은 안오지만",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100023503.1.39",
          "form": "가방을 가려준다니 정말 든든하겠어요!!!",
          "original_form": "가방을 가려준다니 정말 든든하겠어요!!!",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100023503.1.40",
          "form": "맞아요! 보통은 60일거에요!",
          "original_form": "맞아요! 보통은 60일거에요!",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100023503.1.41",
          "form": "골프우산이 70-80하는 것 같아요",
          "original_form": "골프우산이 70-80하는 것 같아요",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100023503.1.42",
          "form": "우산이 아무리 좋아도 비가 안오는게... 제일 편하긴 해요 ㅜㅜ",
          "original_form": "우산이 아무리 좋아도 비가 안오는게... 제일 편하긴 해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100023503.1.43",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100023503.1.44",
          "form": "그리고 비 와서 저기압이면",
          "original_form": "그리고 비 와서 저기압이면",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100023503.1.45",
          "form": "사람 기분도 이상하게 축 쳐지더라구요 ㅠㅠ",
          "original_form": "사람 기분도 이상하게 축 쳐지더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100023503.1.46",
          "form": "맞아요 날씨 영향받고 싶지 않은데 그냥 불가능해요ㅜㅜ",
          "original_form": "맞아요 날씨 영향받고 싶지 않은데 그냥 불가능해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 09:37"
        },
        {
          "id": "MDRW2100023503.1.47",
          "form": "여기는 오늘 안개가 짙어요!",
          "original_form": "여기는 오늘 안개가 짙어요!",
          "speaker_id": "2",
          "time": "20210831 09:38"
        },
        {
          "id": "MDRW2100023503.1.48",
          "form": "비가 온다고 했는데 우산 챙기셨나요??",
          "original_form": "비가 온다고 했는데 우산 챙기셨나요??",
          "speaker_id": "2",
          "time": "20210831 09:38"
        },
        {
          "id": "MDRW2100023503.1.49",
          "form": "오오 그러게요 오늘은 날이 참 흐리네요ㅠ",
          "original_form": "오오 그러게요 오늘은 날이 참 흐리네요ㅠ",
          "speaker_id": "1",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100023503.1.50",
          "form": "네네 우산 챙겼어요!!",
          "original_form": "네네 우산 챙겼어요!!",
          "speaker_id": "1",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100023503.1.51",
          "form": "근데 가는 길엔 비가 안와서",
          "original_form": "근데 가는 길엔 비가 안와서",
          "speaker_id": "1",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100023503.1.52",
          "form": "그냥 3단 우산 챙겼답니다!",
          "original_form": "그냥 3단 우산 챙겼답니다!",
          "speaker_id": "1",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100023503.1.53",
          "form": "대신 장화를 신었어요~",
          "original_form": "대신 장화를 신었어요~",
          "speaker_id": "1",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100023503.1.54",
          "form": "name2 계신 곳은 비가 오나요?",
          "original_form": "&name2& 계신 곳은 비가 오나요?",
          "speaker_id": "1",
          "time": "20210831 09:48"
        },
        {
