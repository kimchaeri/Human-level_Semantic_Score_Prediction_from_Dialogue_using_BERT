      "utterance": [
        {
          "id": "MDRW2100042580.1.1",
          "form": "아니 애초에",
          "original_form": "아니 애초에",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042580.1.2",
          "form": "요새는 스킨 내나보던데",
          "original_form": "요새는 스킨 내나보던데",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042580.1.3",
          "form": "이거 망해가는 겜 특 아닌가",
          "original_form": "이거 망해가는 겜 특 아닌가",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042580.1.4",
          "form": "업데이트 일정 묻는 질문에다가",
          "original_form": "업데이트 일정 묻는 질문에다가",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042580.1.5",
          "form": "언제인지는 모르지만 인력이 구해지는대로 할 예정입니다~",
          "original_form": "언제인지는 모르지만 인력이 구해지는대로 할 예정입니다~",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042580.1.6",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋ어쩌잔거지",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋ어쩌잔거지",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042580.1.7",
          "form": "이런 소리 해대는 회사가 어딨음;;;",
          "original_form": "이런 소리 해대는 회사가 어딨음;;;",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042580.1.8",
          "form": "아 ㅎ",
          "original_form": "아 ㅎ",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042580.1.9",
          "form": "얘네 신작내서",
          "original_form": "얘네 신작내서",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042580.1.10",
          "form": "너무 도비이즈프리 밈에",
          "original_form": "너무 도비이즈프리 밈에",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042580.1.11",
          "form": "인력 그쪽으로 뺀거같다 그러더라",
          "original_form": "인력 그쪽으로 뺀거같다 그러더라",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042580.1.12",
          "form": "뇌가 절여진것이",
          "original_form": "뇌가 절여진것이",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042580.1.13",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042580.1.14",
          "form": "그리고 인겜에서 대놓고 땅땅 사귀는 커플이 아닌데",
          "original_form": "그리고 인겜에서 대놓고 땅땅 사귀는 커플이 아닌데",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042580.1.15",
          "form": "동인에서 판다고",
          "original_form": "동인에서 판다고",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042580.1.16",
          "form": "ㅇㅇㅋㅋㅋㄱㅋㅋ동인 캐해 따라가기",
          "original_form": "ㅇㅇㅋㅋㅋㄱㅋㅋ동인 캐해 따라가기",
          "speaker_id": "2",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042580.1.17",
          "form": "스킨에 아예 엮여서 나오는게",
          "original_form": "스킨에 아예 엮여서 나오는게",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042580.1.18",
          "form": "이번에 헬가 스킨 사면 걔 손 나온다며",
          "original_form": "이번에 헬가 스킨 사면 걔 손 나온다며",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042580.1.19",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042580.1.20",
          "form": "애초에 색깔별로 다 다르게 사게 했던겜이",
          "original_form": "애초에 색깔별로 다 다르게 사게 했던겜이",
          "speaker_id": "2",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042580.1.21",
          "form": "스킨까지 냈어",
          "original_form": "스킨까지 냈어",
          "speaker_id": "2",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042580.1.22",
          "form": "쿠키런 킹덤도",
          "original_form": "쿠키런 킹덤도",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042580.1.23",
          "form": "꽤 많이 망한듯?",
          "original_form": "꽤 많이 망한듯?",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042580.1.24",
          "form": "로오히에 비하면 킹덤은 양반이지",
          "original_form": "로오히에 비하면 킹덤은 양반이지",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042580.1.25",
          "form": "거긴 적어도 스킨 끼우는 맛은 있잖가",
          "original_form": "거긴 적어도 스킨 끼우는 맛은 있잖가",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042580.1.26",
          "form": "잖아",
          "original_form": "잖아",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042580.1.27",
          "form": "ㅇㅇ그건 그래",
          "original_form": "ㅇㅇ그건 그래",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042580.1.28",
          "form": "나 진짜",
          "original_form": "나 진짜",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042580.1.29",
          "form": "할로윈 스킨에",
          "original_form": "할로윈 스킨에",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042580.1.30",
          "form": "혼 팔뻔했잖아",
          "original_form": "혼 팔뻔했잖아",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042580.1.31",
          "form": "로오히는 솔직히...",
          "original_form": "로오히는 솔직히...",
          "speaker_id": "1",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042580.1.32",
          "form": "ㄹㅇ디자이너가 모든걸 캐리하는 겜",
          "original_form": "ㄹㅇ디자이너가 모든걸 캐리하는 겜",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042580.1.33",
          "form": "굳이 돈주고 살 디자인이 아님",
          "original_form": "굳이 돈주고 살 디자인이 아님",
          "speaker_id": "1",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042580.1.34",
          "form": "아니 그 색깔놀이",
          "original_form": "아니 그 색깔놀이",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042580.1.35",
          "form": "뭐냐고",
          "original_form": "뭐냐고",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042580.1.36",
          "form": "쿠킹덤보다도 섬세함이 떨어져",
          "original_form": "쿠킹덤보다도 섬세함이 떨어져",
          "speaker_id": "1",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042580.1.37",
          "form": "ㅇㅇ왜 굳이 쓸디로 했는지도",
          "original_form": "ㅇㅇ왜 굳이 쓸디로 했는지도",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042580.1.38",
          "form": "모르겠어",
          "original_form": "모르겠어",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042580.1.39",
          "form": "뭔가 쓰리디로 움직이는것도",
          "original_form": "뭔가 쓰리디로 움직이는것도",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042580.1.40",
          "form": "아니고",
          "original_form": "아니고",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042580.1.41",
          "form": "걍 턴제 알피지겜이",
          "original_form": "걍 턴제 알피지겜이",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042580.1.42",
          "form": "솔직히 그게 쓸디인지도 모르겠음",
          "original_form": "솔직히 그게 쓸디인지도 모르겠음",
          "speaker_id": "1",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042580.1.43",
          "form": "거의 마비 느낌이던데",
          "original_form": "거의 마비 느낌이던데",
          "speaker_id": "1",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042580.1.44",
          "form": "차라리 투디로 해서 디자인이나 화려하게 하지",
          "original_form": "차라리 투디로 해서 디자인이나 화려하게 하지",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042580.1.45",
          "form": "쿠키런 진짜",
          "original_form": "쿠키런 진짜",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042580.1.46",
          "form": "한때는 사랑했는데",
          "original_form": "한때는 사랑했는데",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042580.1.47",
          "form": "대표들 인터뷰가 내 맘을 빠르게 식혀줬지...",
          "original_form": "대표들 인터뷰가 내 맘을 빠르게 식혀줬지...",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042580.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 22",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 22",
          "speaker_id": "1",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100042580.1.49",
          "form": "특히 그 시스터즈 어쩌고",
          "original_form": "특히 그 시스터즈 어쩌고",
          "speaker_id": "1",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100042580.1.50",
          "form": "ㄹㅇ 헛소리",
          "original_form": "ㄹㅇ 헛소리",
          "speaker_id": "1",
          "time": "20210906 23:09"
        },
