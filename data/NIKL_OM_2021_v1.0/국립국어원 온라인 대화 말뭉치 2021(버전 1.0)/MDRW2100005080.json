      "utterance": [
        {
          "id": "MDRW2100005080.1.1",
          "form": "개강하기 싫다~",
          "original_form": "개강하기 싫다~",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005080.1.2",
          "form": "나도~",
          "original_form": "나도~",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005080.1.3",
          "form": "이번에 수강 인원 제한 저번보다 더 한정적이던데",
          "original_form": "이번에 수강 인원 제한 저번보다 더 한정적이던데",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005080.1.4",
          "form": "어떻게든 비대면 안 하려고 하는 거 같음",
          "original_form": "어떻게든 비대면 안 하려고 하는 거 같음",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005080.1.5",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005080.1.6",
          "form": "왜 대면을 고집하지",
          "original_form": "왜 대면을 고집하지",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005080.1.7",
          "form": "이해할 수 없음",
          "original_form": "이해할 수 없음",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005080.1.8",
          "form": "보상 안 하려고",
          "original_form": "보상 안 하려고",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005080.1.9",
          "form": "그런 거 같지 않아?",
          "original_form": "그런 거 같지 않아?",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005080.1.10",
          "form": "아 그치",
          "original_form": "아 그치",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005080.1.11",
          "form": "똥운대",
          "original_form": "똥운대",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005080.1.12",
          "form": "돈 없어서",
          "original_form": "돈 없어서",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005080.1.13",
          "form": "그지",
          "original_form": "그지",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005080.1.14",
          "form": "깽깽이",
          "original_form": "깽깽이",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005080.1.15",
          "form": "작년에 전부 비대면하고",
          "original_form": "작년에 전부 비대면하고",
          "speaker_id": "2",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005080.1.16",
          "form": "10만원 보상했다며",
          "original_form": "10만원 보상했다며",
          "speaker_id": "2",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005080.1.17",
          "form": "그거 안 하려고 발악하는 거 같음",
          "original_form": "그거 안 하려고 발악하는 거 같음",
          "speaker_id": "2",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005080.1.18",
          "form": "10만원 실화임 진짜 ㅋㅋ",
          "original_form": "10만원 실화임 진짜 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005080.1.19",
          "form": "얼마 주지도 않으면서",
          "original_form": "얼마 주지도 않으면서",
          "speaker_id": "1",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005080.1.20",
          "form": "그거 안 주려고 발악을 하네",
          "original_form": "그거 안 주려고 발악을 하네",
          "speaker_id": "1",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005080.1.21",
          "form": "슬퍼",
          "original_form": "슬퍼",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005080.1.22",
          "form": "자본주의 괴물들",
          "original_form": "자본주의 괴물들",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005080.1.23",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005080.1.24",
          "form": "그지",
          "original_form": "그지",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005080.1.25",
          "form": "깽깽이",
          "original_form": "깽깽이",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005080.1.26",
          "form": "이번에 확진자나 안 나왔으면 좋겠어",
          "original_form": "이번에 확진자나 안 나왔으면 좋겠어",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005080.1.27",
          "form": "저번 학기에 너무 빈번하게 나와서",
          "original_form": "저번 학기에 너무 빈번하게 나와서",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005080.1.28",
          "form": "맞아... 메일테러 받음",
          "original_form": "맞아... 메일테러 받음",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005080.1.29",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005080.1.30",
          "form": "괜히 무리해서 대면만 안 했으면 좋겠음",
          "original_form": "괜히 무리해서 대면만 안 했으면 좋겠음",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005080.1.31",
          "form": "변이가 너무 무서워",
          "original_form": "변이가 너무 무서워",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005080.1.32",
          "form": "그르니까",
          "original_form": "그르니까",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005080.1.33",
          "form": "그리고 대면하기 싫어",
          "original_form": "그리고 대면하기 싫어",
