      "utterance": [
        {
          "id": "MDRW2100018233.1.1",
          "form": "치과는 왜 가셨어요?",
          "original_form": "치과는 왜 가셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018233.1.2",
          "form": "갑자기 이가 찌릿해서요 ㅠ",
          "original_form": "갑자기 이가 찌릿해서요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018233.1.3",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018233.1.4",
          "form": "뭐래요?",
          "original_form": "뭐래요?",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018233.1.5",
          "form": "아무 문제 없대요",
          "original_form": "아무 문제 없대요",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018233.1.6",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018233.1.7",
          "form": "네 근데 전 넘 아프거든요 ㅠ",
          "original_form": "네 근데 전 넘 아프거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018233.1.8",
          "form": "근데 치과에선 멀쩡해서 ㅜㅜ",
          "original_form": "근데 치과에선 멀쩡해서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018233.1.9",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018233.1.10",
          "form": "병원가먼 안 아파져요",
          "original_form": "병원가먼 안 아파져요",
          "speaker_id": "2",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018233.1.11",
          "form": "네 맞아요!@",
          "original_form": "네 맞아요!@",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018233.1.12",
          "form": "넘 아파서 바로 달려갔는데",
          "original_form": "넘 아파서 바로 달려갔는데",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018233.1.13",
          "form": "황당..",
          "original_form": "황당..",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018233.1.14",
          "form": "네 그래도 심한거 아니라 다행이네요",
          "original_form": "네 그래도 심한거 아니라 다행이네요",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018233.1.15",
          "form": "네 그냥 잇몸치료만 받았어요",
          "original_form": "네 그냥 잇몸치료만 받았어요",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018233.1.16",
          "form": "잇몸치료는 얼마정도 해요?",
          "original_form": "잇몸치료는 얼마정도 해요?",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018233.1.17",
          "form": "저 오늘 19000원 정도 냈어요",
          "original_form": "저 오늘 19000원 정도 냈어요",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018233.1.18",
          "form": "비싸진 않군요",
          "original_form": "비싸진 않군요",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018233.1.19",
          "form": "네 근데 거의 3달에한번 정도 하는데",
          "original_form": "네 근데 거의 3달에한번 정도 하는데",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018233.1.20",
          "form": "2번씩 받아야해서",
          "original_form": "2번씩 받아야해서",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018233.1.21",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018233.1.22",
          "form": "그럼 은근 부담되겠어요",
          "original_form": "그럼 은근 부담되겠어요",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018233.1.23",
          "form": "네 ㅠㅠ 이빨 튼튼한 사람 부러워요",
          "original_form": "네 ㅠㅠ 이빨 튼튼한 사람 부러워요",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018233.1.24",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018233.1.25",
          "form": "오복중의 하나 ㅎㄹ",
          "original_form": "오복중의 하나 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018233.1.26",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018233.1.27",
          "form": "네 ㅠㅠ 전 원래 이가 약해서 ㅜㅜ",
          "original_form": "네 ㅠㅠ 전 원래 이가 약해서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018233.1.28",
          "form": "저도 임플란트 했어요 ㅎㅎ",
          "original_form": "저도 임플란트 했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018233.1.29",
          "form": "저 귀뚫은지",
          "original_form": "저 귀뚫은지",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018233.1.30",
          "form": "몇달 됐는데",
          "original_form": "몇달 됐는데",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018233.1.31",
          "form": "아직도 피나요 ㅠㅠ",
          "original_form": "아직도 피나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:46"
        }
      ]
