      "utterance": [
        {
          "id": "MDRW2100000723.1.1",
          "form": "혹시 이번에 플립3 구매하셨어요?",
          "original_form": "혹시 이번에 플립3 구매하셨어요?",
          "speaker_id": "1",
          "time": "20210819 18:44"
        },
        {
          "id": "MDRW2100000723.1.2",
          "form": "아니요! 구매하셨나요.",
          "original_form": "아니요! 구매하셨나요.",
          "speaker_id": "2",
          "time": "20210819 18:44"
        },
        {
          "id": "MDRW2100000723.1.3",
          "form": "요즘 사람들 그거 많이 구매하던데 이유가 있나요?",
          "original_form": "요즘 사람들 그거 많이 구매하던데 이유가 있나요?",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100000723.1.4",
          "form": "아니요 ㅎㅎ 너무비싸서 ...\r\n그냥 워치4 샀어요 ㅎ",
          "original_form": "아니요 ㅎㅎ 너무비싸서 ...\r\n그냥 워치4 샀어요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100000723.1.5",
          "form": "좋다고 하던데요 화면이 커서인지....",
          "original_form": "좋다고 하던데요 화면이 커서인지....",
          "speaker_id": "1",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100000723.1.6",
          "form": "그러게요 화면이 크다고 다 좋은건 아닌것같은데ㅠ",
          "original_form": "그러게요 화면이 크다고 다 좋은건 아닌것같은데ㅠ",
          "speaker_id": "2",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000723.1.7",
          "form": "그리고 가격이 넘  비싸요",
          "original_form": "그리고 가격이 넘  비싸요",
          "speaker_id": "2",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000723.1.8",
          "form": "글쵸 가격이 너무 사악하더라구요 ㅜㅠ",
          "original_form": "글쵸 가격이 너무 사악하더라구요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000723.1.9",
          "form": "친구도 사려고했는데 품절이라서 못샀다고 하더라구요",
          "original_form": "친구도 사려고했는데 품절이라서 못샀다고 하더라구요",
          "speaker_id": "2",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000723.1.10",
          "form": "지인분은 예약하셨다고 엄청 기대하고 계시더라구요",
          "original_form": "지인분은 예약하셨다고 엄청 기대하고 계시더라구요",
          "speaker_id": "1",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000723.1.11",
          "form": "구매할생각이 없었는데 괜히 궁금하네요",
          "original_form": "구매할생각이 없었는데 괜히 궁금하네요",
          "speaker_id": "2",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000723.1.12",
          "form": "마케팅을 잘 했나봐요",
          "original_form": "마케팅을 잘 했나봐요",
          "speaker_id": "2",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000723.1.13",
          "form": "저도 모르고있다가 지인분이 예약하신다고해서 봤다가 가격이야기듣고 헉....이가격에는 안살것같아요",
          "original_form": "저도 모르고있다가 지인분이 예약하신다고해서 봤다가 가격이야기듣고 헉....이가격에는 안살것같아요",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100000723.1.14",
          "form": "그쵸?... 너무...사악해요ㅠ",
          "original_form": "그쵸?... 너무...사악해요ㅠ",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000723.1.15",
          "form": "핸드폰 비싸도 오래는 못쓰겠더락 요",
          "original_form": "핸드폰 비싸도 오래는 못쓰겠더락 요",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000723.1.16",
          "form": "그냥 얼리어답터 과시욕 같기도했어요",
          "original_form": "그냥 얼리어답터 과시욕 같기도했어요",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000723.1.17",
          "form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋ 그것도 아예 아니라고는 못할것같아요",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋ 그것도 아예 아니라고는 못할것같아요",
          "speaker_id": "2",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000723.1.18",
          "form": "싸던 비싸던 사용기간은 비슷할듯해서\r\n저는 가성비를 따지기 때문에요 ㅎ",
