      "utterance": [
        {
          "id": "MDRW2100029522.1.1",
          "form": "아마스빈좋아요",
          "original_form": "아마스빈좋아요",
          "speaker_id": "2",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100029522.1.2",
          "form": "전과일관련ㅇ요",
          "original_form": "전과일관련ㅇ요",
          "speaker_id": "2",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100029522.1.3",
          "form": "고소하고 배부른 타로맛",
          "original_form": "고소하고 배부른 타로맛",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100029522.1.4",
          "form": "사하라나 딸기요거트",
          "original_form": "사하라나 딸기요거트",
          "speaker_id": "2",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100029522.1.5",
          "form": "펄도 넣나요. 젤리나",
          "original_form": "펄도 넣나요. 젤리나",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100029522.1.6",
          "form": "타이오카펄이 좋아요",
          "original_form": "타이오카펄이 좋아요",
          "speaker_id": "2",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100029522.1.7",
          "form": "음식택배가 왔어요",
          "original_form": "음식택배가 왔어요",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100029522.1.8",
          "form": "남은 한알까지 쏙쏙빼먹어요",
          "original_form": "남은 한알까지 쏙쏙빼먹어요",
          "speaker_id": "2",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029522.1.9",
          "form": "닭발이 왔어요 데헷",
          "original_form": "닭발이 왔어요 데헷",
          "speaker_id": "1",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029522.1.10",
          "form": "뭐시켰어요",
          "original_form": "뭐시켰어요",
          "speaker_id": "2",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029522.1.11",
          "form": "앗 부럽네요",
          "original_form": "앗 부럽네요",
          "speaker_id": "2",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029522.1.12",
          "form": "무뼈인가요",
          "original_form": "무뼈인가요",
          "speaker_id": "2",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029522.1.13",
          "form": "우체국쇼핑몰에서 주문했어요",
          "original_form": "우체국쇼핑몰에서 주문했어요",
          "speaker_id": "1",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029522.1.14",
          "form": "무뼈이지요",
          "original_form": "무뼈이지요",
          "speaker_id": "1",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029522.1.15",
          "form": "역시무뼈",
          "original_form": "역시무뼈",
          "speaker_id": "2",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029522.1.16",
          "form": "매콤하면 좋겠네요",
          "original_form": "매콤하면 좋겠네요",
          "speaker_id": "1",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029522.1.17",
          "form": "진리죠",
          "original_form": "진리죠",
          "speaker_id": "2",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029522.1.18",
          "form": "국물인가요",
          "original_form": "국물인가요",
          "speaker_id": "2",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029522.1.19",
          "form": "감자샌드위치랑 먹어야죠",
          "original_form": "감자샌드위치랑 먹어야죠",
