      "utterance": [
        {
          "id": "MDRW2100034497.1.1",
          "form": "아 ㅋㅋㅋㅋ그렇군요 추천감사해용ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ그렇군요 추천감사해용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034497.1.2",
          "form": "꼭 드셔보세요 ㅋㅋㅋ",
          "original_form": "꼭 드셔보세요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034497.1.3",
          "form": "안그래두 마카롱 안먹은지 오래되서!",
          "original_form": "안그래두 마카롱 안먹은지 오래되서!",
          "speaker_id": "2",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034497.1.4",
          "form": "아하 !!",
          "original_form": "아하 !!",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034497.1.5",
          "form": "네넹 그럴게욬ㅋㅋㅋㅋ",
          "original_form": "네넹 그럴게욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034497.1.6",
          "form": "원래 좋아하세요?",
          "original_form": "원래 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034497.1.7",
          "form": "넵ㅋㅋㅋ",
          "original_form": "넵ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034497.1.8",
          "form": "네 좋아해요! 근데 자주 먹지는 않아욬ㅋㅋㅋㅋ",
          "original_form": "네 좋아해요! 근데 자주 먹지는 않아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100034497.1.9",
          "form": "살쪄서 ㅠㅠ",
          "original_form": "살쪄서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100034497.1.10",
          "form": "맛있는건 왤케 살찔가요",
          "original_form": "맛있는건 왤케 살찔가요",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100034497.1.11",
          "form": "넘 슬퍼요",
          "original_form": "넘 슬퍼요",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100034497.1.12",
          "form": "맛난건 다살쪄요",
          "original_form": "맛난건 다살쪄요",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100034497.1.13",
          "form": "옼ㅌㅋㅋㅋ 통했네요",
          "original_form": "옼ㅌㅋㅋㅋ 통했네요",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100034497.1.14",
          "form": "사실 적게 먹으면되기는하는데...",
          "original_form": "사실 적게 먹으면되기는하는데...",
          "speaker_id": "2",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100034497.1.15",
          "form": "자제가 힘드네욬ㅋㅋㅋ ㅠㅜ",
          "original_form": "자제가 힘드네욬ㅋㅋㅋ ㅠㅜ",
          "speaker_id": "2",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100034497.1.16",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100034497.1.17",
          "form": "전 사실 식단중인데 이틀하고 때려치고 반복이에요 ㅠㅠ",
          "original_form": "전 사실 식단중인데 이틀하고 때려치고 반복이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 13:54"
        },
        {
          "id": "MDRW2100034497.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 그래요.. 9월부터 새롭게 식단하려구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도 그래요.. 9월부터 새롭게 식단하려구요",
          "speaker_id": "2",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100034497.1.19",
          "form": "냉동실에 늘 닭가슴살 있어요 ㅋㅋㅋㅋ",
          "original_form": "냉동실에 늘 닭가슴살 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100034497.1.20",
          "form": "와 늘 있다니 꾸준히 관리하시나여? ㅌㅋㅋㅋ",
          "original_form": "와 늘 있다니 꾸준히 관리하시나여? ㅌㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100034497.1.21",
          "form": "ㅋㅋㅋㅋㅋ관리하는게 맞는지 모르겟네요....ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ관리하는게 맞는지 모르겟네요....ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034497.1.22",
          "form": "살 많이 빼야되서 그냥 쟁여놔요 ㅠㅠㅠ",
          "original_form": "살 많이 빼야되서 그냥 쟁여놔요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100034497.1.23",
          "form": "저두여 ㅜㅜ 요새 다들 찌는 추세인거같아요 더",
          "original_form": "저두여 ㅜㅜ 요새 다들 찌는 추세인거같아요 더",
