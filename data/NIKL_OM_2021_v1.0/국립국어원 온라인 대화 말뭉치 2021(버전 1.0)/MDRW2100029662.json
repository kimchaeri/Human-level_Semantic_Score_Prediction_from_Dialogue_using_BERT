      "utterance": [
        {
          "id": "MDRW2100029662.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029662.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029662.1.3",
          "form": "가족들이랑 살고 계신가요?",
          "original_form": "가족들이랑 살고 계신가요?",
          "speaker_id": "1",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029662.1.4",
          "form": "네 같이 살아요",
          "original_form": "네 같이 살아요",
          "speaker_id": "2",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029662.1.5",
          "form": "집안일 하세요?",
          "original_form": "집안일 하세요?",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029662.1.6",
          "form": "당연히 해요ㅋㅋㅋㅋ",
          "original_form": "당연히 해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029662.1.7",
          "form": "아핳ㅎㅎ 뭐가 젤 싫으세요?",
          "original_form": "아핳ㅎㅎ 뭐가 젤 싫으세요?",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029662.1.8",
          "form": "전 다 괜찮은데 설거지가 너무 싫어요",
          "original_form": "전 다 괜찮은데 설거지가 너무 싫어요",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029662.1.9",
          "form": "앗 저도 설거지 싫어요ㅋㅋㅋ그리고 바닥 닦는 것도 싫어해요ㅋㅋㅋ",
          "original_form": "앗 저도 설거지 싫어요ㅋㅋㅋ그리고 바닥 닦는 것도 싫어해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100029662.1.10",
          "form": "아ㅜㅠ 바닥 닦는거 귀찮죠...",
          "original_form": "아ㅜㅠ 바닥 닦는거 귀찮죠...",
          "speaker_id": "1",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100029662.1.11",
          "form": "스팀청소기 사용하시면 좀 편하긴 한 것 같아요",
          "original_form": "스팀청소기 사용하시면 좀 편하긴 한 것 같아요",
          "speaker_id": "1",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100029662.1.12",
          "form": "스팀 청소기 편해요? 매장에서 써보니까 넘 무거워서ㅠㅠ",
          "original_form": "스팀 청소기 편해요? 매장에서 써보니까 넘 무거워서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100029662.1.13",
          "form": "몇 년 전 얘기긴 하지만요",
          "original_form": "몇 년 전 얘기긴 하지만요",
          "speaker_id": "2",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100029662.1.14",
          "form": "아 저희집꺼는 그렇게 비싸고 좋은게 아니라서 그런지",
          "original_form": "아 저희집꺼는 그렇게 비싸고 좋은게 아니라서 그런지",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100029662.1.15",
          "form": "오히려 가볍고 쓰기 편했어요ㅋㅋㅋㅋ",
          "original_form": "오히려 가볍고 쓰기 편했어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100029662.1.16",
          "form": "아 진짜요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 진짜요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100029662.1.17",
          "form": "비싼게 더 가벼운게 아니에요?",
          "original_form": "비싼게 더 가벼운게 아니에요?",
          "speaker_id": "2",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100029662.1.18",
          "form": "음 산지 엄청 오래돼서 요즘은 더 다를수있는데",
          "original_form": "음 산지 엄청 오래돼서 요즘은 더 다를수있는데",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100029662.1.19",
          "form": "제가 살 때는 기능많은게 좀 무거웠어요!",
          "original_form": "제가 살 때는 기능많은게 좀 무거웠어요!",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100029662.1.20",
          "form": "몬가 전자제품은... 돈 줄 수록 가볍다고 생각했는데....",
          "original_form": "몬가 전자제품은... 돈 줄 수록 가볍다고 생각했는데....",
          "speaker_id": "2",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100029662.1.21",
          "form": "요즘은 더 좋고 가볍게 나오지 않을까요?ㅎㅎㅎ",
          "original_form": "요즘은 더 좋고 가볍게 나오지 않을까요?ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100029662.1.22",
          "form": "노트북 그램이라던가... 그런거 보면...",
          "original_form": "노트북 그램이라던가... 그런거 보면...",
          "speaker_id": "2",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100029662.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:36"
        }
      ]
