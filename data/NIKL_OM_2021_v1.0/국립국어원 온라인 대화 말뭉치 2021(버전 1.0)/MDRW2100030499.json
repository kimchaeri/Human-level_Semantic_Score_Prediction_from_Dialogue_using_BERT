      "utterance": [
        {
          "id": "MDRW2100030499.1.1",
          "form": "놀러가고 싶어어ㅓㅓ",
          "original_form": "놀러가고 싶어어ㅓㅓ",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100030499.1.2",
          "form": "나도오오 으엉\r\nㅋㅋㅋㅋㅋ",
          "original_form": "나도오오 으엉\r\nㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100030499.1.3",
          "form": "언제 놀러갈 수 있어ㅜㅜㅜ",
          "original_form": "언제 놀러갈 수 있어ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100030499.1.4",
          "form": "그러니까ㅜㅜㅜ 행복회로 그만 돌리고 그만 여행가고 싶은데요 ㅜㅜ",
          "original_form": "그러니까ㅜㅜㅜ 행복회로 그만 돌리고 그만 여행가고 싶은데요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100030499.1.5",
          "form": "즈이 가야될 곳 많은거 아시져...",
          "original_form": "즈이 가야될 곳 많은거 아시져...",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100030499.1.6",
          "form": "파투난 강릉도 가야되고..",
          "original_form": "파투난 강릉도 가야되고..",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100030499.1.7",
          "form": "대구<- new! 가 막 생겼고",
          "original_form": "대구<- new! 가 막 생겼고",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100030499.1.8",
          "form": "연남동도 가야하나",
          "original_form": "연남동도 가야하나",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100030499.1.9",
          "form": "뭐가 그렇게 많앗지 ㅋㅋㅋ",
          "original_form": "뭐가 그렇게 많앗지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100030499.1.10",
          "form": "강릉? 강릉 뭐였지",
          "original_form": "강릉? 강릉 뭐였지",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100030499.1.11",
          "form": "강릉이었나",
          "original_form": "강릉이었나",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100030499.1.12",
          "form": "우리 작년 겨울에",
          "original_form": "우리 작년 겨울에",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100030499.1.13",
          "form": "가기로 했다가",
          "original_form": "가기로 했다가",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100030499.1.14",
          "form": "터져버린 그것",
          "original_form": "터져버린 그것",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100030499.1.15",
          "form": "뭐가 터지긴햇는데",
          "original_form": "뭐가 터지긴햇는데",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100030499.1.16",
          "form": "기억 안난다 ㅋㅋㅋ",
          "original_form": "기억 안난다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100030499.1.17",
          "form": "지금은 대구나 연남동 가야할거 같은걸 ㅋㅋㅋ",
          "original_form": "지금은 대구나 연남동 가야할거 같은걸 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100030499.1.18",
          "form": "달달구리 먹으러",
          "original_form": "달달구리 먹으러",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100030499.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100030499.1.20",
          "form": "어디든 가긴 가야할것 같은ㅋㅋㅋ",
          "original_form": "어디든 가긴 가야할것 같은ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100030499.1.21",
          "form": "일단 알아논 가게들 잘 기억해놓고.. 흑흑",
          "original_form": "일단 알아논 가게들 잘 기억해놓고.. 흑흑",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100030499.1.22",
          "form": "언제갈수잇는데 ㅠㅜ",
          "original_form": "언제갈수잇는데 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100030499.1.23",
          "form": "티가 잘 메모해놨을거라고 난 믿는다..",
          "original_form": "티가 잘 메모해놨을거라고 난 믿는다..",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100030499.1.24",
          "form": "나돜ㅋ",
          "original_form": "나돜ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100030499.1.25",
          "form": "믿는다 친구야",
          "original_form": "믿는다 친구야",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100030499.1.26",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100030499.1.27",
          "form": "이제 안해놨으면 못가는거여~",
          "original_form": "이제 안해놨으면 못가는거여~",
