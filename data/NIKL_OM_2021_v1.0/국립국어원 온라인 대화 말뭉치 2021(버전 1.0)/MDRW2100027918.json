      "utterance": [
        {
          "id": "MDRW2100027918.1.1",
          "form": "최근에 어디여행다녀온곳있으세요?",
          "original_form": "최근에 어디여행다녀온곳있으세요?",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027918.1.2",
          "form": "최근에 여수갔었어요",
          "original_form": "최근에 여수갔었어요",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100027918.1.3",
          "form": "여수요즘많이가더라구요ㅋㅋㅋ",
          "original_form": "여수요즘많이가더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:47"
        },
        {
          "id": "MDRW2100027918.1.4",
          "form": "풀빌라가셨나여ㅋㅋㅋ",
          "original_form": "풀빌라가셨나여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:47"
        },
        {
          "id": "MDRW2100027918.1.5",
          "form": "아뇨 ㅎ돈없어서 그냥 일반룸 갔어요",
          "original_form": "아뇨 ㅎ돈없어서 그냥 일반룸 갔어요",
          "speaker_id": "1",
          "time": "20210903 13:48"
        },
        {
          "id": "MDRW2100027918.1.6",
          "form": "비싸요",
          "original_form": "비싸요",
          "speaker_id": "1",
          "time": "20210903 13:48"
        },
        {
          "id": "MDRW2100027918.1.7",
          "form": "ㅋㅋㅋ 비싸죠 저도예전에갔었는데 하룻밤에 돈백씩하더라구요",
          "original_form": "ㅋㅋㅋ 비싸죠 저도예전에갔었는데 하룻밤에 돈백씩하더라구요",
          "speaker_id": "2",
          "time": "20210903 13:49"
        },
        {
          "id": "MDRW2100027918.1.8",
          "form": "뭔가돈아까움",
          "original_form": "뭔가돈아까움",
          "speaker_id": "2",
          "time": "20210903 13:49"
        },
        {
          "id": "MDRW2100027918.1.9",
          "form": "예쓰",
          "original_form": "예쓰",
          "speaker_id": "1",
          "time": "20210903 13:49"
        },
        {
          "id": "MDRW2100027918.1.10",
          "form": "너무비싸요",
          "original_form": "너무비싸요",
          "speaker_id": "1",
          "time": "20210903 13:49"
        },
        {
          "id": "MDRW2100027918.1.11",
          "form": "그냥 룸 잡고 수영장서 노는것도 재밌어요",
          "original_form": "그냥 룸 잡고 수영장서 노는것도 재밌어요",
          "speaker_id": "1",
          "time": "20210903 13:50"
        },
        {
          "id": "MDRW2100027918.1.12",
          "form": "ㅋㅋㅋ그래서이번에저는풀빌라를예약했어요",
          "original_form": "ㅋㅋㅋ그래서이번에저는풀빌라를예약했어요",
          "speaker_id": "2",
          "time": "20210903 13:51"
        },
        {
          "id": "MDRW2100027918.1.13",
          "form": "비수기이라조금더싸긴하지만 그래도비싸더라구요ㅋㅋㅋ",
          "original_form": "비수기이라조금더싸긴하지만 그래도비싸더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:51"
        },
        {
          "id": "MDRW2100027918.1.14",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210903 13:51"
        },
        {
          "id": "MDRW2100027918.1.15",
          "form": "좋은분이랑 가시나봐요",
          "original_form": "좋은분이랑 가시나봐요",
          "speaker_id": "1",
          "time": "20210903 13:52"
        },
        {
          "id": "MDRW2100027918.1.16",
          "form": "있어요 사람이랑가요ㅋㅋㅋ",
          "original_form": "있어요 사람이랑가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:59"
        },
        {
          "id": "MDRW2100027918.1.17",
          "form": "정말 착하신분이네요",
          "original_form": "정말 착하신분이네요",
          "speaker_id": "1",
          "time": "20210903 13:59"
        },
        {
          "id": "MDRW2100027918.1.18",
          "form": "바베큐는안해먹기로하고 컵라면먹기로했쪄",
          "original_form": "바베큐는안해먹기로하고 컵라면먹기로했쪄",
          "speaker_id": "2",
          "time": "20210903 13:59"
        },
        {
          "id": "MDRW2100027918.1.19",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210903 13:59"
        },
        {
          "id": "MDRW2100027918.1.20",
          "form": "물놀이에는 컵라면이죠",
          "original_form": "물놀이에는 컵라면이죠",
          "speaker_id": "1",
          "time": "20210903 13:59"
        },
        {
          "id": "MDRW2100027918.1.21",
          "form": "온수 하셩ㅎ나요",
          "original_form": "온수 하셩ㅎ나요",
          "speaker_id": "1",
          "time": "20210903 13:59"
        },
        {
          "id": "MDRW2100027918.1.22",
          "form": "그렇져 물속에서 계속안나올꺼니까요",
