      "utterance": [
        {
          "id": "MDRW2100014977.1.1",
          "form": "저도 사실 주제 잘모르겠어서 ㅋㅋㅋ 카페는 주로 어디가세요?",
          "original_form": "저도 사실 주제 잘모르겠어서 ㅋㅋㅋ 카페는 주로 어디가세요?",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014977.1.2",
          "form": "음 스타벅스는 주로 쿠폰쓰러 가는것같구",
          "original_form": "음 스타벅스는 주로 쿠폰쓰러 가는것같구",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014977.1.3",
          "form": "투썸을 자주가요!",
          "original_form": "투썸을 자주가요!",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014977.1.4",
          "form": "디저트 맛집이라 ㅎㅎ",
          "original_form": "디저트 맛집이라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014977.1.5",
          "form": "오 뭐 좋아하세요???",
          "original_form": "오 뭐 좋아하세요???",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014977.1.6",
          "form": "투썸에 티라미수가 젤 마싯아요!!",
          "original_form": "투썸에 티라미수가 젤 마싯아요!!",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014977.1.7",
          "form": "제가 옛날에 투썸에서 알바했던적도 있어서",
          "original_form": "제가 옛날에 투썸에서 알바했던적도 있어서",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014977.1.8",
          "form": "애정가는곳이기도 하고 ㅎㅎ",
          "original_form": "애정가는곳이기도 하고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014977.1.9",
          "form": "티라미수 맛있죠 ㅋㅋㅋ",
          "original_form": "티라미수 맛있죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014977.1.10",
          "form": "알바라니 음료는 뭐가맛있어요??",
          "original_form": "알바라니 음료는 뭐가맛있어요??",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014977.1.11",
          "form": "로얄밀크티가",
          "original_form": "로얄밀크티가",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014977.1.12",
          "form": "마싯고 잘팔리기도 했어요!!",
          "original_form": "마싯고 잘팔리기도 했어요!!",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014977.1.13",
          "form": "아하 요즘 투썸 리뉴얼 싹 됐더라고요",
          "original_form": "아하 요즘 투썸 리뉴얼 싹 됐더라고요",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014977.1.14",
          "form": "밀크티 꼭먹어야겟네요",
          "original_form": "밀크티 꼭먹어야겟네요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014977.1.15",
          "form": "헉 정말요?",
          "original_form": "헉 정말요?",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100014977.1.16",
          "form": "요즘 약속을 안잡다보니",
          "original_form": "요즘 약속을 안잡다보니",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100014977.1.17",
          "form": "투썸 안간지도",
          "original_form": "투썸 안간지도",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100014977.1.18",
          "form": "오래됐네용",
          "original_form": "오래됐네용",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100014977.1.19",
          "form": "투썸 그 아이스크림음료도 맛나요",
          "original_form": "투썸 그 아이스크림음료도 맛나요",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100014977.1.20",
          "form": "어떤건가요?",
          "original_form": "어떤건가요?",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100014977.1.21",
          "form": "아이스크림있고 밑에 라떼 커피?",
          "original_form": "아이스크림있고 밑에 라떼 커피?",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100014977.1.22",
          "form": "젤유명한메뉴요!!",
          "original_form": "젤유명한메뉴요!!",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100014977.1.23",
          "form": "그런게있었나용",
          "original_form": "그런게있었나용",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100014977.1.24",
          "form": "왜 기억이안나지.. ㅎㅎ",
          "original_form": "왜 기억이안나지.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100014977.1.25",
          "form": "아 ㅋㅋㅋ암튼 그거맛나요",
          "original_form": "아 ㅋㅋㅋ암튼 그거맛나요",
