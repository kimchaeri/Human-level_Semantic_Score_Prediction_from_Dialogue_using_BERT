      "utterance": [
        {
          "id": "MDRW2100032190.1.1",
          "form": "그렇군요 청소도 충분히 힘든 일이랍니다",
          "original_form": "그렇군요 청소도 충분히 힘든 일이랍니다",
          "speaker_id": "1",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032190.1.2",
          "form": "남자 친구 사귄지는 얼마나 되었지요",
          "original_form": "남자 친구 사귄지는 얼마나 되었지요",
          "speaker_id": "2",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032190.1.3",
          "form": "남편분이 옆에 계시나요",
          "original_form": "남편분이 옆에 계시나요",
          "speaker_id": "1",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032190.1.4",
          "form": "저는 5년 정도 사귄 것 같네요",
          "original_form": "저는 5년 정도 사귄 것 같네요",
          "speaker_id": "1",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032190.1.5",
          "form": "남편은 건너편에 있고 저는 쇼파에 누워 있어요",
          "original_form": "남편은 건너편에 있고 저는 쇼파에 누워 있어요",
          "speaker_id": "2",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032190.1.6",
          "form": "처음에 만났을 때는 두근거림이 아주 심했는데 요즘에는 만나면 편안함이 더 크게 느껴지네요",
          "original_form": "처음에 만났을 때는 두근거림이 아주 심했는데 요즘에는 만나면 편안함이 더 크게 느껴지네요",
          "speaker_id": "1",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032190.1.7",
          "form": "시간이 지나면 두근거림이 많이 사라지는 건 어쩔 수 없나 봐요",
          "original_form": "시간이 지나면 두근거림이 많이 사라지는 건 어쩔 수 없나 봐요",
          "speaker_id": "1",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032190.1.8",
          "form": "장거리 연애라 보다 짧게 느껴져겼어요",
          "original_form": "장거리 연애라 보다 짧게 느껴져겼어요",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032190.1.9",
          "form": "남편분 보면 가슴이 두근두근 하시나요",
          "original_form": "남편분 보면 가슴이 두근두근 하시나요",
          "speaker_id": "1",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032190.1.10",
          "form": "두근거림이 무엇인가요 먹는 건가요",
          "original_form": "두근거림이 무엇인가요 먹는 건가요",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032190.1.11",
          "form": "장거리 연애 어서 만났을 때 더 애틋하고 그런 좋은 점이 있었죠",
          "original_form": "장거리 연애 어서 만났을 때 더 애틋하고 그런 좋은 점이 있었죠",
          "speaker_id": "1",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032190.1.12",
          "form": "쇼핑할 때 가슴이 두근거리는 것을 말하는 건가요",
          "original_form": "쇼핑할 때 가슴이 두근거리는 것을 말하는 건가요",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032190.1.13",
          "form": "먹는 거 같긴 해요 두근거림 일하는 것은",
          "original_form": "먹는 거 같긴 해요 두근거림 일하는 것은",
          "speaker_id": "1",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032190.1.14",
          "form": "남편 보고는 두근두근 하지 않나요",
          "original_form": "남편 보고는 두근두근 하지 않나요",
          "speaker_id": "1",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032190.1.15",
          "form": "저도 장거리 연애 하기 때문에 오래 만났어요",
          "original_form": "저도 장거리 연애 하기 때문에 오래 만났어요",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032190.1.16",
          "form": "다시 한번 물어 보고 싶네요먹는 건가요",
          "original_form": "다시 한번 물어 보고 싶네요먹는 건가요",
          "speaker_id": "2",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032190.1.17",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032190.1.18",
          "form": "키는 어느 정도 차이가 나시죠",
          "original_form": "키는 어느 정도 차이가 나시죠",
          "speaker_id": "1",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032190.1.19",
          "form": "없으면 허전한게 남편이 지요",
          "original_form": "없으면 허전한게 남편이 지요",
          "speaker_id": "2",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032190.1.20",
          "form": "제 남친은 키가 작아서요",
          "original_form": "제 남친은 키가 작아서요",
          "speaker_id": "1",
          "time": "20210905 13:55"
        }
      ]
