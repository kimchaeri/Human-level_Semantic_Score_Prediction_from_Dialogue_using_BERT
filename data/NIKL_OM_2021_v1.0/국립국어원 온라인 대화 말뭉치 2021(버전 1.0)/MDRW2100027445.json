      "utterance": [
        {
          "id": "MDRW2100027445.1.1",
          "form": "네.... 솔직히 미쳤나 했습니다.",
          "original_form": "네.... 솔직히 미쳤나 했습니다.",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027445.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아니 그 내기니가",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아니 그 내기니가",
          "speaker_id": "1",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027445.1.3",
          "form": "해리포터 내기니까지",
          "original_form": "해리포터 내기니까지",
          "speaker_id": "1",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027445.1.4",
          "form": "이어지는 거잖아요?",
          "original_form": "이어지는 거잖아요?",
          "speaker_id": "1",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027445.1.5",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027445.1.6",
          "form": "누가봐도 나중에 덧붙인 설정이잖아요",
          "original_form": "누가봐도 나중에 덧붙인 설정이잖아요",
          "speaker_id": "2",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027445.1.7",
          "form": "그것도 칼에 동강나서 끝난거 다 아는 판에",
          "original_form": "그것도 칼에 동강나서 끝난거 다 아는 판에",
          "speaker_id": "2",
          "time": "20210902 12:39"
        },
        {
          "id": "MDRW2100027445.1.8",
          "form": "수현배우 나온다해서 엄청 기대했는데",
          "original_form": "수현배우 나온다해서 엄청 기대했는데",
          "speaker_id": "1",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027445.1.9",
          "form": "생각보다 나오지도 않고",
          "original_form": "생각보다 나오지도 않고",
          "speaker_id": "1",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027445.1.10",
          "form": "내기니라니...",
          "original_form": "내기니라니...",
          "speaker_id": "1",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027445.1.11",
          "form": "내기니!!!",
          "original_form": "내기니!!!",
          "speaker_id": "1",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027445.1.12",
          "form": "하하하하하하하",
          "original_form": "하하하하하하하",
          "speaker_id": "2",
          "time": "20210902 12:46"
        },
        {
          "id": "MDRW2100027445.1.13",
          "form": "당시에 진짜....",
          "original_form": "당시에 진짜....",
          "speaker_id": "2",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100027445.1.14",
          "form": "조니뎁 옹호까지 겹쳐서",
          "original_form": "조니뎁 옹호까지 겹쳐서",
          "speaker_id": "2",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100027445.1.15",
          "form": "롤링 머리에 총맞았냐는 소리 많이 나왔죠",
          "original_form": "롤링 머리에 총맞았냐는 소리 많이 나왔죠",
          "speaker_id": "2",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100027445.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ진심",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ진심",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027445.1.17",
          "form": "심심하면",
          "original_form": "심심하면",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027445.1.18",
          "form": "해리포터 스핀오프 단편집이라도 내주지....",
          "original_form": "해리포터 스핀오프 단편집이라도 내주지....",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027445.1.19",
          "form": "아니면 짧게 에피소드라도ㅜㅜㅜㅜ",
          "original_form": "아니면 짧게 에피소드라도ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027445.1.20",
          "form": "헤르미온느랑 론 얘기 좀..ㅡ",
          "original_form": "헤르미온느랑 론 얘기 좀..ㅡ",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027445.1.21",
          "form": "하... 제 최애 헤르미온느",
          "original_form": "하... 제 최애 헤르미온느",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027445.1.22",
          "form": "나름 후일담으로 마법부장관 됐다고 해서 위안을 삼아요...",
          "original_form": "나름 후일담으로 마법부장관 됐다고 해서 위안을 삼아요...",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027445.1.23",
          "form": "그린델완드 그만두고",
          "original_form": "그린델완드 그만두고",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027445.1.24",
          "form": "헤르미온느 단독 주연 영화나",
          "original_form": "헤르미온느 단독 주연 영화나",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027445.1.25",
          "form": "써!!!!",
          "original_form": "써!!!!",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027445.1.26",
          "form": "롤링!!!!",
          "original_form": "롤링!!!!",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027445.1.27",
          "form": "맞습니다!!!!",
          "original_form": "맞습니다!!!!",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027445.1.28",
          "form": "근데 그 3총사 롤링이랑 거의 손절했다잖아요...",
          "original_form": "근데 그 3총사 롤링이랑 거의 손절했다잖아요...",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027445.1.29",
          "form": "이제와 쓴다고 출연을 할까요..?",
          "original_form": "이제와 쓴다고 출연을 할까요..?",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027445.1.30",
          "form": "음.... 상영은 안하더라도ㅜㅜ",
          "original_form": "음.... 상영은 안하더라도ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 07:15"
        },
        {
          "id": "MDRW2100027445.1.31",
          "form": "책으로라도 읽고 싶은..",
          "original_form": "책으로라도 읽고 싶은..",
          "speaker_id": "1",
          "time": "20210903 07:15"
        },
        {
          "id": "MDRW2100027445.1.32",
          "form": "저도 그건 읽고싶네요.",
          "original_form": "저도 그건 읽고싶네요.",
          "speaker_id": "2",
          "time": "20210903 08:33"
        },
        {
          "id": "MDRW2100027445.1.33",
          "form": "이야기는 기깔나게 쓰죠, 롤링이",
          "original_form": "이야기는 기깔나게 쓰죠, 롤링이",
          "speaker_id": "2",
          "time": "20210903 08:34"
        },
        {
          "id": "MDRW2100027445.1.34",
          "form": "해리포터는 진짜 명작인데",
          "original_form": "해리포터는 진짜 명작인데",
          "speaker_id": "1",
          "time": "20210903 13:16"
        },
        {
          "id": "MDRW2100027445.1.35",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 13:16"
        },
        {
          "id": "MDRW2100027445.1.36",
          "form": "롤링 인더 딥...ㅋㅋㅋ규ㅠㅠㅠㅠ",
