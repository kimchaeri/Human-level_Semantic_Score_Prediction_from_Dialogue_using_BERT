      "utterance": [
        {
          "id": "MDRW2100014605.1.1",
          "form": "최근에 보신 영화가 뭔가요?",
          "original_form": "최근에 보신 영화가 뭔가요?",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014605.1.2",
          "form": "싱크홀이요",
          "original_form": "싱크홀이요",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014605.1.3",
          "form": "앗 저도 볼까싶었는데, 재밌나요?",
          "original_form": "앗 저도 볼까싶었는데, 재밌나요?",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014605.1.4",
          "form": "가볍게보기좋아요 저는웃으면서봤네요",
          "original_form": "가볍게보기좋아요 저는웃으면서봤네요",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014605.1.5",
          "form": "저는 최근에 인질 봤어요 황정민 주연 영화요",
          "original_form": "저는 최근에 인질 봤어요 황정민 주연 영화요",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014605.1.6",
          "form": "그것도 재밌다면서요?ㅎ",
          "original_form": "그것도 재밌다면서요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014605.1.7",
          "form": "극중이름도 황정민인거 같더라구요",
          "original_form": "극중이름도 황정민인거 같더라구요",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014605.1.8",
          "form": "네 본인역을 본인이 연기해요",
          "original_form": "네 본인역을 본인이 연기해요",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014605.1.9",
          "form": "코믹인가요?",
          "original_form": "코믹인가요?",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014605.1.10",
          "form": "아뇨 스릴러일 것 같네요",
          "original_form": "아뇨 스릴러일 것 같네요",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014605.1.11",
          "form": "시간표가 안맞아서 못봤는데 궁금하네요",
          "original_form": "시간표가 안맞아서 못봤는데 궁금하네요",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014605.1.12",
          "form": "꽤 하드해요",
          "original_form": "꽤 하드해요",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014605.1.13",
          "form": "오..그렇군요",
          "original_form": "오..그렇군요",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014605.1.14",
          "form": "폭력수위가 좀 있어요",
          "original_form": "폭력수위가 좀 있어요",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014605.1.15",
          "form": "여름에 킬링타임용으로 재밌게 볼만은 했어요",
          "original_form": "여름에 킬링타임용으로 재밌게 볼만은 했어요",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014605.1.16",
          "form": "아하 그렇군요 시간나면 보러가야겠네요",
          "original_form": "아하 그렇군요 시간나면 보러가야겠네요",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014605.1.17",
          "form": "네 재밌으실 거예요",
          "original_form": "네 재밌으실 거예요",
          "speaker_id": "1",
          "time": "20210825 20:17"
        }
      ]
