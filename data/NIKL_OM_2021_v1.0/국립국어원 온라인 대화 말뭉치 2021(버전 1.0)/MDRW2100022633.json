      "utterance": [
        {
          "id": "MDRW2100022633.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022633.1.2",
          "form": "전 매년 여행을 다녀요!",
          "original_form": "전 매년 여행을 다녀요!",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022633.1.3",
          "form": "근데 작년이랑 올해는 코로나 때문에",
          "original_form": "근데 작년이랑 올해는 코로나 때문에",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022633.1.4",
          "form": "저도 여행 좋아하는데 요즘 4단계라 좀 힘드네요",
          "original_form": "저도 여행 좋아하는데 요즘 4단계라 좀 힘드네요",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022633.1.5",
          "form": "맞아요 코로나가 여행 좋아하는 사람들은 힘드네요",
          "original_form": "맞아요 코로나가 여행 좋아하는 사람들은 힘드네요",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022633.1.6",
          "form": "가장 최근에 간게 언제이신가요?",
          "original_form": "가장 최근에 간게 언제이신가요?",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022633.1.7",
          "form": "최근에 당일로 8월 대부도 다녀온게 끝이네요",
          "original_form": "최근에 당일로 8월 대부도 다녀온게 끝이네요",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022633.1.8",
          "form": "멀리 갈 수가 없어서",
          "original_form": "멀리 갈 수가 없어서",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022633.1.9",
          "form": "휴가는 다녀오셨어요?",
          "original_form": "휴가는 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022633.1.10",
          "form": "저는 휴가때 집에만 콕 박혀 있었네요..",
          "original_form": "저는 휴가때 집에만 콕 박혀 있었네요..",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022633.1.11",
          "form": "아직 백신을 맞지 않아서",
          "original_form": "아직 백신을 맞지 않아서",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022633.1.12",
          "form": "왠지 모르게 조심하게 돼요.",
          "original_form": "왠지 모르게 조심하게 돼요.",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022633.1.13",
          "form": "대부도는 휴가때 가셨나요?",
          "original_form": "대부도는 휴가때 가셨나요?",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022633.1.14",
          "form": "저도 아직 안맞았어요... 그래서 사람 최대한 없는 곳만 골라다니게 되더라고요",
          "original_form": "저도 아직 안맞았어요... 그래서 사람 최대한 없는 곳만 골라다니게 되더라고요",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022633.1.15",
          "form": "네 휴가 때 다녀왔습니다. 3시간 머물렀나.. 돌아다니지도 못했네요",
          "original_form": "네 휴가 때 다녀왔습니다. 3시간 머물렀나.. 돌아다니지도 못했네요",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022633.1.16",
          "form": "4단계라 해수욕장도 폐장했더라고요",
          "original_form": "4단계라 해수욕장도 폐장했더라고요",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022633.1.17",
          "form": "얼른 코로나 풀려야 할텐데",
          "original_form": "얼른 코로나 풀려야 할텐데",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022633.1.18",
          "form": "지금 친구들이랑 계획은 대충 해놓고",
          "original_form": "지금 친구들이랑 계획은 대충 해놓고",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022633.1.19",
          "form": "집합금지때문에 진전이 없네요 ㅎㅎ",
          "original_form": "집합금지때문에 진전이 없네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022633.1.20",
          "form": "그러게요~ 짧고 굵게 가는 줄 알았는데 계속 진행형이네요",
          "original_form": "그러게요~ 짧고 굵게 가는 줄 알았는데 계속 진행형이네요",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022633.1.21",
          "form": "내년에는 잠잠해져야 여행을 갈텐데..",
          "original_form": "내년에는 잠잠해져야 여행을 갈텐데..",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022633.1.22",
          "form": "아는 관광 명소 있으신가요?",
          "original_form": "아는 관광 명소 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022633.1.23",
          "form": "저는 제주도 금능해수욕장이 그렇게 좋더라고요~ 관광명소라기보단 휴양하기 좋은 곳이죠",
          "original_form": "저는 제주도 금능해수욕장이 그렇게 좋더라고요~ 관광명소라기보단 휴양하기 좋은 곳이죠",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022633.1.24",
          "form": "유명하긴 협재가 유명한데 옆에 있는 조용한 금능해변이 더 좋습니다",
          "original_form": "유명하긴 협재가 유명한데 옆에 있는 조용한 금능해변이 더 좋습니다",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022633.1.25",
          "form": "오오 협재해수욕장 유튜브에서 봤었는데 금능 해변이라는곳도 있었군요 ㅎㅎ",
          "original_form": "오오 협재해수욕장 유튜브에서 봤었는데 금능 해변이라는곳도 있었군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022633.1.26",
          "form": "지인들과 놀기엔 조용한곳이 최고긴 하죠!",
          "original_form": "지인들과 놀기엔 조용한곳이 최고긴 하죠!",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022633.1.27",
          "form": "금능해변 주변에 식당가나 여행 먹거리 괜찮은가요,",
          "original_form": "금능해변 주변에 식당가나 여행 먹거리 괜찮은가요,",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022633.1.28",
          "form": "맞아요. 물도 깨끗하고 수심도 깊지 않아 가족과 조용히 보내기 좋은 곳이었습니다.",
          "original_form": "맞아요. 물도 깨끗하고 수심도 깊지 않아 가족과 조용히 보내기 좋은 곳이었습니다.",
          "speaker_id": "1",
          "time": "20210829 16:52"
        }
      ]
    }
