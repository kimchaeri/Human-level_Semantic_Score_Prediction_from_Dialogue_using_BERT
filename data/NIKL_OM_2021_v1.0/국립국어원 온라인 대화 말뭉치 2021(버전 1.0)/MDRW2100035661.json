      "utterance": [
        {
          "id": "MDRW2100035661.1.1",
          "form": "아니여",
          "original_form": "아니여",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035661.1.2",
          "form": "타자속도 엄청 빠르심",
          "original_form": "타자속도 엄청 빠르심",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035661.1.3",
          "form": "핸드폰으로 쳐요ㅋㅋㅋ",
          "original_form": "핸드폰으로 쳐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035661.1.4",
          "form": "그 자판처럼 생긴 쿼티 키보드라 그래용",
          "original_form": "그 자판처럼 생긴 쿼티 키보드라 그래용",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035661.1.5",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035661.1.6",
          "form": "저는 천지인쓰는 옛날사람..",
          "original_form": "저는 천지인쓰는 옛날사람..",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035661.1.7",
          "form": "아아ㅠㅠ 블루투스로 무슨 키보드 연결해서 쓰시면",
          "original_form": "아아ㅠㅠ 블루투스로 무슨 키보드 연결해서 쓰시면",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035661.1.8",
          "form": "낼부터 비오면",
          "original_form": "낼부터 비오면",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035661.1.9",
          "form": "손 덜 아프실거에용",
          "original_form": "손 덜 아프실거에용",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035661.1.10",
          "form": "낼부터 이번주 내내 정말 비가 계속 와요ㅋㅋㅋㅋㅋ",
          "original_form": "낼부터 이번주 내내 정말 비가 계속 와요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035661.1.11",
          "form": "그냥 현관에 우산 쌓아뒀어요ㅠㅠㅠ",
          "original_form": "그냥 현관에 우산 쌓아뒀어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035661.1.12",
          "form": "ㅋㅋ꿀팁감사함다 비오는거 잠깐오는거나 좋지",
          "original_form": "ㅋㅋ꿀팁감사함다 비오는거 잠깐오는거나 좋지",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035661.1.13",
          "form": "내내오면 습한데요ㅜ",
          "original_form": "내내오면 습한데요ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035661.1.14",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035661.1.15",
          "form": "저 반곱슬이라 비오면 습해져서",
          "original_form": "저 반곱슬이라 비오면 습해져서",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035661.1.16",
          "form": "머리 엄청 꼬불거리거든요",
          "original_form": "머리 엄청 꼬불거리거든요",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035661.1.17",
          "form": "이러고 가을은 스치고 바로겨울오는게 아닌가싶고ㅜ",
          "original_form": "이러고 가을은 스치고 바로겨울오는게 아닌가싶고ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035661.1.18",
          "form": "저도 그래욪반곱슬ㅋㅋ",
          "original_form": "저도 그래욪반곱슬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035661.1.19",
          "form": "대충 포기하고 다니긴 하는데요 그래도 신경쓰여요",
          "original_form": "대충 포기하고 다니긴 하는데요 그래도 신경쓰여요",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035661.1.20",
          "form": "요즘 계절이 중간이 없는것같아요ㅋㅋㅋ 극단적",
          "original_form": "요즘 계절이 중간이 없는것같아요ㅋㅋㅋ 극단적",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035661.1.21",
          "form": "저두...ㅠㅠ",
          "original_form": "저두...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035661.1.22",
          "form": "차라리 그래서 저는 겨울을 더 좋아해요",
          "original_form": "차라리 그래서 저는 겨울을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035661.1.23",
          "form": "겨울은 그래도 건조하니까 머리가 곱슬거리진 않아서ㅋㅋㅋ",
          "original_form": "겨울은 그래도 건조하니까 머리가 곱슬거리진 않아서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035661.1.24",
          "form": "ㅋㅋㅋ 그래도 요즘 겨울은 넘춥자나요!ㅜ",
          "original_form": "ㅋㅋㅋ 그래도 요즘 겨울은 넘춥자나요!ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035661.1.25",
          "form": "전 이상하게 겨울이 좋더라구요",
          "original_form": "전 이상하게 겨울이 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035661.1.26",
          "form": "얼어버리겠어요ㅋㅋ",
          "original_form": "얼어버리겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035661.1.27",
          "form": "그냥 한해의 끝 새해의 시작이 있어서 그런지",
          "original_form": "그냥 한해의 끝 새해의 시작이 있어서 그런지",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035661.1.28",
          "form": "설레고",
          "original_form": "설레고",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035661.1.29",
          "form": "분위기가 좋으신건가요",
          "original_form": "분위기가 좋으신건가요",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035661.1.30",
          "form": "겨울에 나는 딸기도 넘 맛있구",
          "original_form": "겨울에 나는 딸기도 넘 맛있구",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035661.1.31",
          "form": "저는 붕어빵이요",
          "original_form": "저는 붕어빵이요",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035661.1.32",
          "form": "아아 붕어빵! 겨울의 묘미죠",
          "original_form": "아아 붕어빵! 겨울의 묘미죠",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035661.1.33",
          "form": "슈붕vs팥붕",
          "original_form": "슈붕vs팥붕",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035661.1.34",
          "form": "저는 팥이요!ㅋㅋㅋ",
          "original_form": "저는 팥이요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035661.1.35",
          "form": "ㅋㅋ저두요",
          "original_form": "ㅋㅋ저두요",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035661.1.36",
          "form": "팥두개 슈하나 조합을 좋아합니다",
          "original_form": "팥두개 슈하나 조합을 좋아합니다",
