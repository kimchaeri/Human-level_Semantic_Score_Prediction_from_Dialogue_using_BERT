      "utterance": [
        {
          "id": "MDRW2100042727.1.1",
          "form": "아우터로 어떤거 즐겨입으세용",
          "original_form": "아우터로 어떤거 즐겨입으세용",
          "speaker_id": "1",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100042727.1.2",
          "form": "저는 자켓 같은거 주로 입어요 ㅋㅋㅋ",
          "original_form": "저는 자켓 같은거 주로 입어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:24"
        },
        {
          "id": "MDRW2100042727.1.3",
          "form": "가디건은 뭔가 어색하더라구요",
          "original_form": "가디건은 뭔가 어색하더라구요",
          "speaker_id": "2",
          "time": "20210907 08:24"
        },
        {
          "id": "MDRW2100042727.1.4",
          "form": "야상도 좋아하시나용",
          "original_form": "야상도 좋아하시나용",
          "speaker_id": "1",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100042727.1.5",
          "form": "야상은 부모임이 자주 사오셔서 많아가지구 ㅋㅋ 자주 입게 돼요",
          "original_form": "야상은 부모임이 자주 사오셔서 많아가지구 ㅋㅋ 자주 입게 돼요",
          "speaker_id": "2",
          "time": "20210907 10:17"
        },
        {
          "id": "MDRW2100042727.1.6",
          "form": "야상 편하고 멋스러운거같아용",
          "original_form": "야상 편하고 멋스러운거같아용",
          "speaker_id": "1",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100042727.1.7",
          "form": "편한옷 주로 입으시나여?",
          "original_form": "편한옷 주로 입으시나여?",
          "speaker_id": "2",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042727.1.8",
          "form": "네넹 주로 와이드한거 입어용 ㅎㅎ",
          "original_form": "네넹 주로 와이드한거 입어용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042727.1.9",
          "form": "아하 저도 그런거 좋긴한데 키가 작아서ㅠ",
          "original_form": "아하 저도 그런거 좋긴한데 키가 작아서ㅠ",
          "speaker_id": "2",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042727.1.10",
          "form": "집에서만 입어요 ㅋㅋ",
          "original_form": "집에서만 입어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042727.1.11",
          "form": "키가 어떻게 되세여?",
          "original_form": "키가 어떻게 되세여?",
          "speaker_id": "1",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042727.1.12",
          "form": "저 154요 ㅋㅋ큐ㅠㅠ",
          "original_form": "저 154요 ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042727.1.13",
          "form": "ㅋㅋㅋ기여워용 저는 좀큰편이라 작은분들 옷입고 신발신는게 귀여워요",
          "original_form": "ㅋㅋㅋ기여워용 저는 좀큰편이라 작은분들 옷입고 신발신는게 귀여워요",
          "speaker_id": "1",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042727.1.14",
          "form": "ㅜㅜ 저는 큰 친구들이 너무 부럽더라구요...",
          "original_form": "ㅜㅜ 저는 큰 친구들이 너무 부럽더라구요...",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042727.1.15",
          "form": "요즘 운동화도 크게 나와서 사이즈 찾기 어렵더라구욬ㅋ큐",
          "original_form": "요즘 운동화도 크게 나와서 사이즈 찾기 어렵더라구욬ㅋ큐",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042727.1.16",
          "form": "발사이즈는 몇이세요?",
          "original_form": "발사이즈는 몇이세요?",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100042727.1.17",
          "form": "220이예요! ㅋㅋ",
          "original_form": "220이예요! ㅋㅋ",
