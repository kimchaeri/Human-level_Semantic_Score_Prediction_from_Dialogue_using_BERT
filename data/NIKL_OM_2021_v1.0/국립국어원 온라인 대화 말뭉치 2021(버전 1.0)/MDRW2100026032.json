      "utterance": [
        {
          "id": "MDRW2100026032.1.1",
          "form": "비가오다 안오다가하네요",
          "original_form": "비가오다 안오다가하네요",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100026032.1.2",
          "form": "네네 비가 왔다갔다해요",
          "original_form": "네네 비가 왔다갔다해요",
          "speaker_id": "1",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100026032.1.3",
          "form": "지금은 비가오고있어요ㄱ",
          "original_form": "지금은 비가오고있어요ㄱ",
          "speaker_id": "1",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100026032.1.4",
          "form": "네 여기도요",
          "original_form": "네 여기도요",
          "speaker_id": "2",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100026032.1.5",
          "form": "이제 진짜 여름이 지났네요",
          "original_form": "이제 진짜 여름이 지났네요",
          "speaker_id": "2",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100026032.1.6",
          "form": "맞아요. 너무 가을이 확 온 느낌이에요.ㅋ",
          "original_form": "맞아요. 너무 가을이 확 온 느낌이에요.ㅋ",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100026032.1.7",
          "form": "옷은 꺼내셨어요?",
          "original_form": "옷은 꺼내셨어요?",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100026032.1.8",
          "form": "네네 쫌 꺼냈어요ㅋㅋ사실옷이 많지 않아서",
          "original_form": "네네 쫌 꺼냈어요ㅋㅋ사실옷이 많지 않아서",
          "speaker_id": "2",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100026032.1.9",
          "form": "아 그러시구나 ㅋㅋ저는 애들옷이 많아서요ㅋ",
          "original_form": "아 그러시구나 ㅋㅋ저는 애들옷이 많아서요ㅋ",
          "speaker_id": "1",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100026032.1.10",
          "form": "매번 계절 바뀔때마다",
          "original_form": "매번 계절 바뀔때마다",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026032.1.11",
          "form": "옷 다 세탁했는데",
          "original_form": "옷 다 세탁했는데",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026032.1.12",
          "form": "올해는그냥 꺼내입네요ㅋ",
          "original_form": "올해는그냥 꺼내입네요ㅋ",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026032.1.13",
          "form": "아 그쵸 내옷 말고도 남편옷 애들옷 다 챙겨야하네요",
          "original_form": "아 그쵸 내옷 말고도 남편옷 애들옷 다 챙겨야하네요",
          "speaker_id": "2",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100026032.1.14",
          "form": "옷 정리하면서도 많이 버리세요?",
          "original_form": "옷 정리하면서도 많이 버리세요?",
          "speaker_id": "2",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100026032.1.15",
          "form": "보면입는옷만입어요ㅠ",
          "original_form": "보면입는옷만입어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100026032.1.16",
          "form": "네네 버리는것보다는",
          "original_form": "네네 버리는것보다는",
          "speaker_id": "1",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026032.1.17",
          "form": "모아놨다가",
          "original_form": "모아놨다가",
          "speaker_id": "1",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026032.1.18",
          "form": "지인애들한테줘요ㅋ",
          "original_form": "지인애들한테줘요ㅋ",
          "speaker_id": "1",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026032.1.19",
          "form": "아~ 그쵸 그게 짱이죠",
          "original_form": "아~ 그쵸 그게 짱이죠",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026032.1.20",
          "form": "예전에는 옷 신경많이썼는데",
          "original_form": "예전에는 옷 신경많이썼는데",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026032.1.21",
          "form": "지금은 그냥 편한게 최고인것같아요",
          "original_form": "지금은 그냥 편한게 최고인것같아요",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026032.1.22",
          "form": "네네 저도요.ㅋㅋ사실 편한옷 너무찾게되네요",
          "original_form": "네네 저도요.ㅋㅋ사실 편한옷 너무찾게되네요",
          "speaker_id": "1",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100026032.1.23",
          "form": "이쁜옷은 왜이리 다 불편한지 모르겠어요",
          "original_form": "이쁜옷은 왜이리 다 불편한지 모르겠어요",
          "speaker_id": "2",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100026032.1.24",
          "form": "겨울에는 롱패딩으로 겨울나기해야죠ㅋㅋ",
          "original_form": "겨울에는 롱패딩으로 겨울나기해야죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100026032.1.25",
          "form": "네 맞아요, ㅋㅋ",
          "original_form": "네 맞아요, ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:17"
        },
        {
          "id": "MDRW2100026032.1.26",
          "form": "롱패딩은 두개만있어두되니  좋아요ㅋ",
          "original_form": "롱패딩은 두개만있어두되니  좋아요ㅋ",
          "speaker_id": "1",
          "time": "20210901 20:17"
        },
        {
          "id": "MDRW2100026032.1.27",
          "form": "맞아요ㅋㅋ진짜 겨울에 코트입고댕기기 추워요",
          "original_form": "맞아요ㅋㅋ진짜 겨울에 코트입고댕기기 추워요",
          "speaker_id": "2",
          "time": "20210901 20:17"
        }
      ]
    }
