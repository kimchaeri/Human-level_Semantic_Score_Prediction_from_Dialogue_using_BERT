      "utterance": [
        {
          "id": "MDRW2100031802.1.1",
          "form": "같이 사는 반려동물 있어?",
          "original_form": "같이 사는 반려동물 있어?",
          "speaker_id": "1",
          "time": "20210905 00:48"
        },
        {
          "id": "MDRW2100031802.1.2",
          "form": "아니 너는 동물키우니?",
          "original_form": "아니 너는 동물키우니?",
          "speaker_id": "2",
          "time": "20210905 00:49"
        },
        {
          "id": "MDRW2100031802.1.3",
          "form": "응 나는 고양이 한 마리ㅋㅋ",
          "original_form": "응 나는 고양이 한 마리ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:50"
        },
        {
          "id": "MDRW2100031802.1.4",
          "form": "오 어떤종이야??",
          "original_form": "오 어떤종이야??",
          "speaker_id": "2",
          "time": "20210905 00:51"
        },
        {
          "id": "MDRW2100031802.1.5",
          "form": "코리안숏헤어ㅋㅋ 길고양이야",
          "original_form": "코리안숏헤어ㅋㅋ 길고양이야",
          "speaker_id": "1",
          "time": "20210905 00:51"
        },
        {
          "id": "MDRW2100031802.1.6",
          "form": "나도 고양이 좋아해!! 최고는 길고양이라고 생각해",
          "original_form": "나도 고양이 좋아해!! 최고는 길고양이라고 생각해",
          "speaker_id": "2",
          "time": "20210905 00:53"
        },
        {
          "id": "MDRW2100031802.1.7",
          "form": "그치ㅋㅋ 요즘은 품종묘 키우는 사람 거의 못 봤어",
          "original_form": "그치ㅋㅋ 요즘은 품종묘 키우는 사람 거의 못 봤어",
          "speaker_id": "1",
          "time": "20210905 00:54"
        },
        {
          "id": "MDRW2100031802.1.8",
          "form": "근데 강아지는 고양이에 비해 품종견 키우는 게 보통이더라.",
          "original_form": "근데 강아지는 고양이에 비해 품종견 키우는 게 보통이더라.",
          "speaker_id": "1",
          "time": "20210905 00:55"
        },
        {
          "id": "MDRW2100031802.1.9",
          "form": "내친구는 품종묘 길고양이 다 키우는데 짱 커",
          "original_form": "내친구는 품종묘 길고양이 다 키우는데 짱 커",
          "speaker_id": "2",
          "time": "20210905 00:55"
        },
        {
          "id": "MDRW2100031802.1.10",
          "form": "크기가 대따커",
          "original_form": "크기가 대따커",
          "speaker_id": "2",
          "time": "20210905 00:55"
        },
        {
          "id": "MDRW2100031802.1.11",
          "form": "정말 컸나 보구나",
          "original_form": "정말 컸나 보구나",
          "speaker_id": "1",
          "time": "20210905 00:59"
        },
        {
          "id": "MDRW2100031802.1.12",
          "form": "넌 동물 알러지 없나보네?",
          "original_form": "넌 동물 알러지 없나보네?",
          "speaker_id": "1",
          "time": "20210905 00:59"
        },
        {
          "id": "MDRW2100031802.1.13",
          "form": "응응 없어 꽃가우알러지만있어",
          "original_form": "응응 없어 꽃가우알러지만있어",
          "speaker_id": "2",
          "time": "20210905 01:00"
        },
        {
          "id": "MDRW2100031802.1.14",
          "form": "친구네고양이 터키쉬앙고라라 했는데 아무래도 아닌거같아",
          "original_form": "친구네고양이 터키쉬앙고라라 했는데 아무래도 아닌거같아",
          "speaker_id": "2",
          "time": "20210905 01:00"
        },
        {
          "id": "MDRW2100031802.1.15",
          "form": "수의사쌤도 아닌거같다했데 ㅋㅋㅋㅋ",
          "original_form": "수의사쌤도 아닌거같다했데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:00"
        },
        {
          "id": "MDRW2100031802.1.16",
          "form": "와 축복 받았네ㅋㅋㅋ 요즘 털알러지 없는 사람 드물던데",
          "original_form": "와 축복 받았네ㅋㅋㅋ 요즘 털알러지 없는 사람 드물던데",
          "speaker_id": "1",
          "time": "20210905 01:01"
        },
        {
          "id": "MDRW2100031802.1.17",
          "form": "친구 고양이 진짜 웃기다ㅠㅠ",
          "original_form": "친구 고양이 진짜 웃기다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 01:01"
        },
        {
          "id": "MDRW2100031802.1.18",
          "form": "메인쿤이 섞인거 같다고 ㅋㅋㅋㅋㅋ",
          "original_form": "메인쿤이 섞인거 같다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:01"
        },
        {
          "id": "MDRW2100031802.1.19",
          "form": "친구 집에 동물 털 장난 아니겠다.",
          "original_form": "친구 집에 동물 털 장난 아니겠다.",
          "speaker_id": "1",
          "time": "20210905 01:02"
        },
        {
          "id": "MDRW2100031802.1.20",
          "form": "아이가 상당히 커 ㅋㅋㅋ생긴것도 메인쿤같이 생겼어 ㅋㅋㅋ어릴때는 안그랬는데 ㅋㅋㅋ",
          "original_form": "아이가 상당히 커 ㅋㅋㅋ생긴것도 메인쿤같이 생겼어 ㅋㅋㅋ어릴때는 안그랬는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:02"
        },
        {
          "id": "MDRW2100031802.1.21",
          "form": "그집 고양이 네마리나 키워...",
          "original_form": "그집 고양이 네마리나 키워...",
          "speaker_id": "2",
          "time": "20210905 01:02"
        },
        {
          "id": "MDRW2100031802.1.22",
          "form": "와...... 청소기 하루 열 번 밀어야겠다",
          "original_form": "와...... 청소기 하루 열 번 밀어야겠다",
          "speaker_id": "1",
          "time": "20210905 01:02"
        },
        {
          "id": "MDRW2100031802.1.23",
          "form": "마당엔 길고양이들이 밥먹으러 스무마리는오는거같애 ㅋㅋㅋㅋ",
          "original_form": "마당엔 길고양이들이 밥먹으러 스무마리는오는거같애 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:03"
        },
        {
          "id": "MDRW2100031802.1.24",
          "form": "고양이 키우는 친구가 길고양이도 챙기는구나",
          "original_form": "고양이 키우는 친구가 길고양이도 챙기는구나",
          "speaker_id": "1",
          "time": "20210905 01:03"
        }
      ]
    }
