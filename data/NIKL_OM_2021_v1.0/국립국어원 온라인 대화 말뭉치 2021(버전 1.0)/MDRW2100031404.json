      "utterance": [
        {
          "id": "MDRW2100031404.1.1",
          "form": "화장품 오래된거 쓰면 안돼요 ㅋ",
          "original_form": "화장품 오래된거 쓰면 안돼요 ㅋ",
          "speaker_id": "1",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031404.1.2",
          "form": "당연한거가지고",
          "original_form": "당연한거가지고",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031404.1.3",
          "form": "피부 안좋아진다",
          "original_form": "피부 안좋아진다",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031404.1.4",
          "form": "맞아요 그니까 집에있는 클렌징오일도 버려요 ㅋ",
          "original_form": "맞아요 그니까 집에있는 클렌징오일도 버려요 ㅋ",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031404.1.5",
          "form": "그거 오래 안되지 않았어?",
          "original_form": "그거 오래 안되지 않았어?",
          "speaker_id": "2",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031404.1.6",
          "form": "작년에 샀던가... 근데",
          "original_form": "작년에 샀던가... 근데",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031404.1.7",
          "form": "피부에 뭐 나는거같아요 바르니까",
          "original_form": "피부에 뭐 나는거같아요 바르니까",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031404.1.8",
          "form": "두드러기 나면 안좋은데",
          "original_form": "두드러기 나면 안좋은데",
          "speaker_id": "2",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031404.1.9",
          "form": "그니까 버리는게 좋겠어요",
          "original_form": "그니까 버리는게 좋겠어요",
          "speaker_id": "1",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031404.1.10",
          "form": "저 틴트도 새로 사야할거같은데",
          "original_form": "저 틴트도 새로 사야할거같은데",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031404.1.11",
          "form": "그거 너무 빨갛더라",
          "original_form": "그거 너무 빨갛더라",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031404.1.12",
          "form": "사진 보셨죠 ㅠ 완전 안맞음",
          "original_form": "사진 보셨죠 ㅠ 완전 안맞음",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031404.1.13",
          "form": "쥐잡아먹은줄",
          "original_form": "쥐잡아먹은줄",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031404.1.14",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031404.1.15",
          "form": "그절도예요?? ㅋㅋㅋㅋ 그정돈아니었던거같은데",
          "original_form": "그절도예요?? ㅋㅋㅋㅋ 그정돈아니었던거같은데",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031404.1.16",
          "form": "심했어",
          "original_form": "심했어",
          "speaker_id": "2",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031404.1.17",
          "form": "엄마두 화장 빨갛게 하시면서",
          "original_form": "엄마두 화장 빨갛게 하시면서",
          "speaker_id": "1",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100031404.1.18",
          "form": "그정도는 아니야 엄마는 잘하지",
          "original_form": "그정도는 아니야 엄마는 잘하지",
          "speaker_id": "2",
          "time": "20210904 21:26"
        },
        {
          "id": "MDRW2100031404.1.19",
          "form": "좋은데 코디가 가끔 별로세요",
          "original_form": "좋은데 코디가 가끔 별로세요",
          "speaker_id": "1",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100031404.1.20",
          "form": "같이 보러가서 골라줘야지",
          "original_form": "같이 보러가서 골라줘야지",
          "speaker_id": "2",
          "time": "20210904 21:28"
        },
        {
          "id": "MDRW2100031404.1.21",
          "form": "근데 제가 골라두 별로 ㅠ 맘에 안들어하시는거같은데...",
          "original_form": "근데 제가 골라두 별로 ㅠ 맘에 안들어하시는거같은데...",
          "speaker_id": "1",
          "time": "20210904 21:28"
        },
        {
          "id": "MDRW2100031404.1.22",
          "form": "그래도 마음에 들어",
          "original_form": "그래도 마음에 들어",
          "speaker_id": "2",
          "time": "20210904 21:29"
        }
