      "utterance": [
        {
          "id": "MDRW2100029316.1.1",
          "form": "이게뭔가요",
          "original_form": "이게뭔가요",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100029316.1.2",
          "form": "온라인에서 운동 가르쳐주는 거 아닐까요?",
          "original_form": "온라인에서 운동 가르쳐주는 거 아닐까요?",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100029316.1.3",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100029316.1.4",
          "form": "어떤거같나요",
          "original_form": "어떤거같나요",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100029316.1.5",
          "form": "운동을 좀 할 줄 아는 사람에겐 좋은거같고 아니면 조금 운동적으로 센스있는 사람도 좋은거같아요",
          "original_form": "운동을 좀 할 줄 아는 사람에겐 좋은거같고 아니면 조금 운동적으로 센스있는 사람도 좋은거같아요",
          "speaker_id": "1",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100029316.1.6",
          "form": "개초보에겐 자세잡기가 어려운거같아요",
          "original_form": "개초보에겐 자세잡기가 어려운거같아요",
          "speaker_id": "1",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100029316.1.7",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100029316.1.8",
          "form": "알기쉬워진건 좋지만 부상위험이있네요",
          "original_form": "알기쉬워진건 좋지만 부상위험이있네요",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100029316.1.9",
          "form": "그래도 누워있는 요가 자세가 젤 좋아요",
          "original_form": "그래도 누워있는 요가 자세가 젤 좋아요",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100029316.1.10",
          "form": "그쵸 ㅠ",
          "original_form": "그쵸 ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100029316.1.11",
          "form": "부상위험이 크죠",
          "original_form": "부상위험이 크죠",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100029316.1.12",
          "form": "남탓도 못함 ㅠ",
          "original_form": "남탓도 못함 ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100029316.1.13",
          "form": "배우고싶은 자세가있나요",
          "original_form": "배우고싶은 자세가있나요",
          "speaker_id": "2",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100029316.1.14",
          "form": "엄",
          "original_form": "엄",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100029316.1.15",
          "form": "스쿼트를 제대로해보고싶긴해요",
          "original_form": "스쿼트를 제대로해보고싶긴해요",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100029316.1.16",
          "form": "플랭크랑",
          "original_form": "플랭크랑",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100029316.1.17",
          "form": "님은여?",
          "original_form": "님은여?",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100029316.1.18",
          "form": "아하 저도 스쿼트는 궁금하네요",
          "original_form": "아하 저도 스쿼트는 궁금하네요",
          "speaker_id": "2",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100029316.1.19",
          "form": "플랭크는 거울보면서하니까 그래도 좀 나은거같더라구요",
          "original_form": "플랭크는 거울보면서하니까 그래도 좀 나은거같더라구요",
          "speaker_id": "2",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100029316.1.20",
          "form": "팔이안아파야한다는데",
          "original_form": "팔이안아파야한다는데",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100029316.1.21",
          "form": "그게안되더라구요",
          "original_form": "그게안되더라구요",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100029316.1.22",
          "form": "아하 다음에 한번 해보세요 궁금하네요",
          "original_form": "아하 다음에 한번 해보세요 궁금하네요",
          "speaker_id": "2",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100029316.1.23",
          "form": "아.....어디서해요?",
          "original_form": "아.....어디서해요?",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100029316.1.24",
          "form": "댁에서요ㅎㅎ 궁금하네요",
          "original_form": "댁에서요ㅎㅎ 궁금하네요",
          "speaker_id": "2",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100029316.1.25",
          "form": "ㅋㅋㅋㅋ아네",
          "original_form": "ㅋㅋㅋㅋ아네",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100029316.1.26",
          "form": "자주 따라하나요?",
          "original_form": "자주 따라하나요?",
          "speaker_id": "2",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100029316.1.27",
          "form": "못하는데....어떻게....",
          "original_form": "못하는데....어떻게....",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100029316.1.28",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210903 20:58"
        }
      ]
    }
  ]
