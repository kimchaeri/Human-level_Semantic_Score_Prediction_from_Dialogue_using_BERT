      "utterance": [
        {
          "id": "MDRW2100027526.1.1",
          "form": "보셨나요? 해리포터",
          "original_form": "보셨나요? 해리포터",
          "speaker_id": "1",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027526.1.2",
          "form": "사실 끝까지 못봤지만",
          "original_form": "사실 끝까지 못봤지만",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027526.1.3",
          "form": "내용은 전부 알고있습니다",
          "original_form": "내용은 전부 알고있습니다",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027526.1.4",
          "form": "책은 혼혈왕자까지 읽었고",
          "original_form": "책은 혼혈왕자까지 읽었고",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027526.1.5",
          "form": "오 이번에 20주년이라",
          "original_form": "오 이번에 20주년이라",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027526.1.6",
          "form": "재개봉한데요",
          "original_form": "재개봉한데요",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027526.1.7",
          "form": "영화는 4편까지만 봤어요",
          "original_form": "영화는 4편까지만 봤어요",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027526.1.8",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027526.1.9",
          "form": "극장에서 볼 수 있는건가요",
          "original_form": "극장에서 볼 수 있는건가요",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027526.1.10",
          "form": "내심 아쉬웠는데 보러가야겠네요!",
          "original_form": "내심 아쉬웠는데 보러가야겠네요!",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027526.1.11",
          "form": "다른거도 하는진 모르겠는데 마법사의돌? 1편 하더라구요",
          "original_form": "다른거도 하는진 모르겠는데 마법사의돌? 1편 하더라구요",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027526.1.12",
          "form": "샹치 개봉하던데 재밌을까요 마블",
          "original_form": "샹치 개봉하던데 재밌을까요 마블",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027526.1.13",
          "form": "마법사의 돌 정말 좋아해서 EA사에서 만든 게임도 했었는데요",
          "original_form": "마법사의 돌 정말 좋아해서 EA사에서 만든 게임도 했었는데요",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027526.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027526.1.15",
          "form": "샹치 보고 온 친구가 조금 유치하다고는 하더라고요",
          "original_form": "샹치 보고 온 친구가 조금 유치하다고는 하더라고요",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027526.1.16",
          "form": "그게임을 아시다니",
          "original_form": "그게임을 아시다니",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027526.1.17",
          "form": "저 해리포터 게임 거의다했어요",
          "original_form": "저 해리포터 게임 거의다했어요",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027526.1.18",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027526.1.19",
          "form": "3편까지는 했었어요!",
          "original_form": "3편까지는 했었어요!",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027526.1.20",
          "form": "계단 움직이고 피씨로 하는 그거 맞죠",
          "original_form": "계단 움직이고 피씨로 하는 그거 맞죠",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027526.1.21",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027526.1.22",
          "form": "저도 그정도 까지 한거같은데",
          "original_form": "저도 그정도 까지 한거같은데",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027526.1.23",
          "form": "콘솔버전은 또 좀 달라요",
          "original_form": "콘솔버전은 또 좀 달라요",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027526.1.24",
          "form": "자주보는 유투브는 뭐 있으신가요 ㅎㅎ",
          "original_form": "자주보는 유투브는 뭐 있으신가요 ㅎㅎ",
