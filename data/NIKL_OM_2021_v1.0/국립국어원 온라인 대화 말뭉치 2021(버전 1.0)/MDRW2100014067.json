      "utterance": [
        {
          "id": "MDRW2100014067.1.1",
          "form": "운동하시는 거 있으세요?",
          "original_form": "운동하시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014067.1.2",
          "form": "아니요ㅠㅠ저는 그냥 딱히 없고 걷기 해요 유산소",
          "original_form": "아니요ㅠㅠ저는 그냥 딱히 없고 걷기 해요 유산소",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014067.1.3",
          "form": "혹시 하시는 운동 있으세요?",
          "original_form": "혹시 하시는 운동 있으세요?",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014067.1.4",
          "form": "저도 걷기운동ㅋㅋㅋ",
          "original_form": "저도 걷기운동ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014067.1.5",
          "form": "딱히 하는 건 없어요",
          "original_form": "딱히 하는 건 없어요",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014067.1.6",
          "form": "저랑 같으시군요ㅋㅋㅋㅋ 움직이는걸 별로 안좋아해요ㅠ 귀찮아서",
          "original_form": "저랑 같으시군요ㅋㅋㅋㅋ 움직이는걸 별로 안좋아해요ㅠ 귀찮아서",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014067.1.7",
          "form": "집에서 쉴때는 거의 누워있는거 같아요ㅋㅋ",
          "original_form": "집에서 쉴때는 거의 누워있는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014067.1.8",
          "form": "다이어트 해 본 적은 없으시겠네요?",
          "original_form": "다이어트 해 본 적은 없으시겠네요?",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014067.1.9",
          "form": "아휴 당연 있죠 다이어트는!",
          "original_form": "아휴 당연 있죠 다이어트는!",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014067.1.10",
          "form": "ㅋㅋㅋㅋㅋ다이어트 할때는 거의 식이로 했었어요",
          "original_form": "ㅋㅋㅋㅋㅋ다이어트 할때는 거의 식이로 했었어요",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014067.1.11",
          "form": "매번 실패였지만 ㅠ",
          "original_form": "매번 실패였지만 ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014067.1.12",
          "form": "다이어트 하신적 있으세요?",
          "original_form": "다이어트 하신적 있으세요?",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014067.1.13",
          "form": "아뇨 저는 말로만 해요",
          "original_form": "아뇨 저는 말로만 해요",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014067.1.14",
          "form": "ㅋㅋㅋㅋㄱㅋㄱㅋ 홈트를 해야하는데 안 하니깐",
          "original_form": "ㅋㅋㅋㅋㄱㅋㄱㅋ 홈트를 해야하는데 안 하니깐",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014067.1.15",
          "form": "해야죠..",
          "original_form": "해야죠..",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014067.1.16",
          "form": "ㅠㅠ홈트가 젤 하기좋을것같은데",
          "original_form": "ㅠㅠ홈트가 젤 하기좋을것같은데",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014067.1.17",
          "form": "영상보고 따라하고 이런게",
          "original_form": "영상보고 따라하고 이런게",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014067.1.18",
          "form": "잘 하고 있는건지도 모르겠고ㅠ 그렇더라고요",
          "original_form": "잘 하고 있는건지도 모르겠고ㅠ 그렇더라고요",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014067.1.19",
          "form": "맞아요 그럼 헬스를 가서 pt를 받아야 되나 싶고",
          "original_form": "맞아요 그럼 헬스를 가서 pt를 받아야 되나 싶고",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014067.1.20",
          "form": "그럼 또 돈이 걱정이고",
          "original_form": "그럼 또 돈이 걱정이고",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014067.1.21",
          "form": "맞아요 맞아욬ㅋㅋㅋㅋㅋㅋ 굳이 돈 내가며 해야되나 싶고",
          "original_form": "맞아요 맞아욬ㅋㅋㅋㅋㅋㅋ 굳이 돈 내가며 해야되나 싶고",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014067.1.22",
          "form": "근데 다이어트 아니더라도 건강 생각하면 운동해야하긴 하는데ㅜㅠ 귀차니즘이 다 이겨버리네요",
          "original_form": "근데 다이어트 아니더라도 건강 생각하면 운동해야하긴 하는데ㅜㅠ 귀차니즘이 다 이겨버리네요",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014067.1.23",
          "form": "맞아요 건강 생각하면 해야 되는데..ㅈ",
          "original_form": "맞아요 건강 생각하면 해야 되는데..ㅈ",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014067.1.24",
          "form": "홈트라도 해야겠어요",
          "original_form": "홈트라도 해야겠어요",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014067.1.25",
          "form": "ㅠ답 너무늦었죠 죄송해요",
          "original_form": "ㅠ답 너무늦었죠 죄송해요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100014067.1.26",
          "form": "홈트 저도 한번 해볼까봐요",
          "original_form": "홈트 저도 한번 해볼까봐요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100014067.1.27",
          "form": "아뇨 괜찮아요ㅋㅋㅋㅋ 저도 중간에 없어지고 그래요",
          "original_form": "아뇨 괜찮아요ㅋㅋㅋㅋ 저도 중간에 없어지고 그래요",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100014067.1.28",
          "form": "땅크부부인가? 그 분들 거 좋대요",
          "original_form": "땅크부부인가? 그 분들 거 좋대요",
