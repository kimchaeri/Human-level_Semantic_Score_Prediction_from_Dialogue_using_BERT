      "utterance": [
        {
          "id": "MDRW2100036236.1.1",
          "form": "ㅋㅋㅋㅋ명예의 전당 있음 제 차지라구욧",
          "original_form": "ㅋㅋㅋㅋ명예의 전당 있음 제 차지라구욧",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100036236.1.2",
          "form": "ㅋㅋㅋㅋㅋ근데 위 작은 친구들은 보면",
          "original_form": "ㅋㅋㅋㅋㅋ근데 위 작은 친구들은 보면",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100036236.1.3",
          "form": "딱 배부르면 그만 먹더라구요",
          "original_form": "딱 배부르면 그만 먹더라구요",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100036236.1.4",
          "form": "전.. 끝까지 달리는데 ^^",
          "original_form": "전.. 끝까지 달리는데 ^^",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100036236.1.5",
          "form": "저도.. 숟가락이 절 안놔주던데요",
          "original_form": "저도.. 숟가락이 절 안놔주던데요",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100036236.1.6",
          "form": "ㅋㅋㅋㅋ아 맞아요",
          "original_form": "ㅋㅋㅋㅋ아 맞아요",
          "speaker_id": "1",
          "time": "20210830 19:54"
        },
        {
          "id": "MDRW2100036236.1.7",
          "form": "제가 안놓는게 아니라 ^^",
          "original_form": "제가 안놓는게 아니라 ^^",
          "speaker_id": "1",
          "time": "20210830 19:54"
        },
        {
          "id": "MDRW2100036236.1.8",
          "form": "또 좋아하는거 있으세요? 피자말구 ㅎㅎ",
          "original_form": "또 좋아하는거 있으세요? 피자말구 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:55"
        },
        {
          "id": "MDRW2100036236.1.9",
          "form": "그쵸 걔들이 그런거라구욧",
          "original_form": "그쵸 걔들이 그런거라구욧",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036236.1.10",
          "form": "고기여!!",
          "original_form": "고기여!!",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036236.1.11",
          "form": "특히 갈비용!",
          "original_form": "특히 갈비용!",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036236.1.12",
          "form": "아 갈비 하",
          "original_form": "아 갈비 하",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036236.1.13",
          "form": "갈비는 사랑입니다",
          "original_form": "갈비는 사랑입니다",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100036236.1.14",
          "form": "제가 돈만 많았다면 갈빗집을 인수했을텐데",
          "original_form": "제가 돈만 많았다면 갈빗집을 인수했을텐데",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100036236.1.15",
          "form": "하 그니까요",
          "original_form": "하 그니까요",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100036236.1.16",
          "form": "제가 먹는게 더 많아 적자가 나더라도 먹고싶을정도에요",
          "original_form": "제가 먹는게 더 많아 적자가 나더라도 먹고싶을정도에요",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100036236.1.17",
          "form": "하 적자나도 가보자구요",
          "original_form": "하 적자나도 가보자구요",
          "speaker_id": "1",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100036236.1.18",
          "form": "아 혹시",
          "original_form": "아 혹시",
          "speaker_id": "1",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100036236.1.19",
          "form": "갈비에 비냉드십니까 물냉드십니까",
          "original_form": "갈비에 비냉드십니까 물냉드십니까",
          "speaker_id": "1",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100036236.1.20",
          "form": "갈비는 끝 가야죠",
          "original_form": "갈비는 끝 가야죠",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036236.1.21",
          "form": "저 같이가는 사람이랑 반갈합니다",
          "original_form": "저 같이가는 사람이랑 반갈합니다",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036236.1.22",
          "form": "식후 냉면 안좋아하구요",
          "original_form": "식후 냉면 안좋아하구요",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036236.1.23",
          "form": "본식에 집중하시는 스탈이시군요??!",
          "original_form": "본식에 집중하시는 스탈이시군요??!",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100036236.1.24",
          "form": "넵ㅋㅋㅋ 저희 동네만 그런지 몰라도 후식 진짜 애기용보다 못해요ㅜㅜ name1님은 어떤거 드세요?",
          "original_form": "넵ㅋㅋㅋ 저희 동네만 그런지 몰라도 후식 진짜 애기용보다 못해요ㅜㅜ &name1&님은 어떤거 드세요?",
          "speaker_id": "2",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100036236.1.25",
          "form": "전 고기만 맛있다면 가리지 않지만",
          "original_form": "전 고기만 맛있다면 가리지 않지만",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100036236.1.26",
          "form": "생갈비엔 비냉",
          "original_form": "생갈비엔 비냉",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100036236.1.27",
          "form": "양념갈비는 물냉 먹어요 ㅎㅎ",
          "original_form": "양념갈비는 물냉 먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100036236.1.28",
