      "utterance": [
        {
          "id": "MDRW2100030207.1.1",
          "form": "동생은 9월 되면 고향에 내려와?",
          "original_form": "동생은 9월 되면 고향에 내려와?",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030207.1.2",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100030207.1.3",
          "form": "우리 소식을 업데이트 할 필요가 있구먼",
          "original_form": "우리 소식을 업데이트 할 필요가 있구먼",
          "speaker_id": "2",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100030207.1.4",
          "form": "지금 동생은 8월 말부터 내려가 있었음",
          "original_form": "지금 동생은 8월 말부터 내려가 있었음",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030207.1.5",
          "form": "막내가 서울로 가면서",
          "original_form": "막내가 서울로 가면서",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030207.1.6",
          "form": "name3가 공간이 생겨서ㅋㅋ",
          "original_form": "&name3&가 공간이 생겨서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030207.1.7",
          "form": "방금 집에서 옷 싸서 삼치로 내려갔음",
          "original_form": "방금 집에서 옷 싸서 삼치로 내려갔음",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030207.1.8",
          "form": "동생이랑 같이",
          "original_form": "동생이랑 같이",
          "speaker_id": "1",
          "time": "20210904 10:53"
        },
        {
          "id": "MDRW2100030207.1.9",
          "form": "피자먹었다길래",
          "original_form": "피자먹었다길래",
          "speaker_id": "1",
          "time": "20210904 10:53"
        },
        {
          "id": "MDRW2100030207.1.10",
          "form": "아 그건 어제 저녁",
          "original_form": "아 그건 어제 저녁",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100030207.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100030207.1.12",
          "form": "아 짐싸러 올라갔던 거야?",
          "original_form": "아 짐싸러 올라갔던 거야?",
          "speaker_id": "1",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100030207.1.13",
          "form": "야스야스 다음주 출근인데",
          "original_form": "야스야스 다음주 출근인데",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100030207.1.14",
          "form": "옷이 없었음",
          "original_form": "옷이 없었음",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100030207.1.15",
          "form": "그리고 진주에 일이 많아서",
          "original_form": "그리고 진주에 일이 많아서",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100030207.1.16",
          "form": "사랑니도 빼러 올라오고",
          "original_form": "사랑니도 빼러 올라오고",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100030207.1.17",
          "form": "이것저것 많았음",
          "original_form": "이것저것 많았음",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100030207.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210906 16:10"
        },
        {
          "id": "MDRW2100030207.1.19",
          "form": "이제 그럼 혼자 사는 거야?",
          "original_form": "이제 그럼 혼자 사는 거야?",
          "speaker_id": "1",
          "time": "20210906 16:10"
        },
        {
          "id": "MDRW2100030207.1.20",
          "form": "웅웅 그럴 듯...?",
          "original_form": "웅웅 그럴 듯...?",
          "speaker_id": "2",
          "time": "20210906 16:13"
        },
        {
          "id": "MDRW2100030207.1.21",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210906 16:13"
        },
        {
          "id": "MDRW2100030207.1.22",
          "form": "오늘 잘 쉬고 있냐!",
          "original_form": "오늘 잘 쉬고 있냐!",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100030207.1.23",
          "form": "응 부모님이랑 같이 외식했어",
          "original_form": "응 부모님이랑 같이 외식했어",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100030207.1.24",
          "form": "우앙 외식 자주하는 편인가!",
          "original_form": "우앙 외식 자주하는 편인가!",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100030207.1.25",
          "form": "아니 가뭄에 콩나듯이 ㅋㅋ",
          "original_form": "아니 가뭄에 콩나듯이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100030207.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100030207.1.27",
          "form": "가족들과 어떤 음식을 먹었는가",
          "original_form": "가족들과 어떤 음식을 먹었는가",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
