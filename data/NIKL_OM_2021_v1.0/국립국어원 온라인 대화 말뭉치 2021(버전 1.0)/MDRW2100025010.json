      "utterance": [
        {
          "id": "MDRW2100025010.1.1",
          "form": "운동 자주 하시나요?",
          "original_form": "운동 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100025010.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100025010.1.3",
          "form": "꾸준히 해야하는데 힘들더라구요",
          "original_form": "꾸준히 해야하는데 힘들더라구요",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025010.1.4",
          "form": "그쵸 ㅠㅠ 저도 체력때문에 해야하는데 힘들더라구요",
          "original_form": "그쵸 ㅠㅠ 저도 체력때문에 해야하는데 힘들더라구요",
          "speaker_id": "1",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025010.1.5",
          "form": "좋아하는 운동이 있으신가요?",
          "original_form": "좋아하는 운동이 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100025010.1.6",
          "form": "딱히 좋아하는 운동은 없어요",
          "original_form": "딱히 좋아하는 운동은 없어요",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025010.1.7",
          "form": "운동 여러가지 해보지않아서",
          "original_form": "운동 여러가지 해보지않아서",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025010.1.8",
          "form": "근데 수영은 배워보고싶더라구요",
          "original_form": "근데 수영은 배워보고싶더라구요",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025010.1.9",
          "form": "수영 재미있고 관절에 무리가 안가서 저는 너무 추천해요",
          "original_form": "수영 재미있고 관절에 무리가 안가서 저는 너무 추천해요",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025010.1.10",
          "form": "일단 땀이 안나서 ㅎㅎ",
          "original_form": "일단 땀이 안나서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025010.1.11",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100025010.1.12",
          "form": "수영 전신 운동도 되고 제가 물을 좋아해서",
          "original_form": "수영 전신 운동도 되고 제가 물을 좋아해서",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100025010.1.13",
          "form": "딱 즐기면서 운동하기 좋을것 같더라구요",
          "original_form": "딱 즐기면서 운동하기 좋을것 같더라구요",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100025010.1.14",
          "form": "name1님 수영 하셨나요?",
          "original_form": "&name1&님 수영 하셨나요?",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025010.1.15",
          "form": "네!! 어릴적에 배우고 성인되서 자유수영 종종 다녔어요!",
          "original_form": "네!! 어릴적에 배우고 성인되서 자유수영 종종 다녔어요!",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100025010.1.16",
          "form": "진짜 전신운동 되는데 땀이 안나서 너무 좋아요",
          "original_form": "진짜 전신운동 되는데 땀이 안나서 너무 좋아요",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100025010.1.17",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100025010.1.18",
          "form": "전 할수있는거라곤",
          "original_form": "전 할수있는거라곤",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100025010.1.19",
          "form": "물에 둥둥 뜨는것뿐이러",
          "original_form": "물에 둥둥 뜨는것뿐이러",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100025010.1.20",
          "form": "살짝 걱정되긴하네요ㅋㅋㅋ",
          "original_form": "살짝 걱정되긴하네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100025010.1.21",
          "form": "근데 지금은 코로나라 엄두도 못내요ㅜ",
          "original_form": "근데 지금은 코로나라 엄두도 못내요ㅜ",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100025010.1.22",
          "form": "ㅎㅎ 강습 들으시면 충분히 하실 수 있을 거에요!",
          "original_form": "ㅎㅎ 강습 들으시면 충분히 하실 수 있을 거에요!",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100025010.1.23",
          "form": "그쵸 ㅠㅠㅠ 저도 수영장 못 간지 2년 넘어가는 것 같아요",
          "original_form": "그쵸 ㅠㅠㅠ 저도 수영장 못 간지 2년 넘어가는 것 같아요",
          "speaker_id": "1",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100025010.1.24",
          "form": "헐 벌써 2년",
          "original_form": "헐 벌써 2년",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025010.1.25",
          "form": "진짜 코로나 오래가네요",
          "original_form": "진짜 코로나 오래가네요",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025010.1.26",
          "form": "효과 좋다니 코로나 끝나면 해볼게요!",
          "original_form": "효과 좋다니 코로나 끝나면 해볼게요!",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025010.1.27",
          "form": "그리고 서핑도 하고싶어요",
          "original_form": "그리고 서핑도 하고싶어요",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025010.1.28",
          "form": "되게 재밌어보이더라구요",
          "original_form": "되게 재밌어보이더라구요",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100025010.1.29",
          "form": "저도 서핑 해보고 싶어요!!",
          "original_form": "저도 서핑 해보고 싶어요!!",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100025010.1.30",
          "form": "너무 재밌어 보이고 바다 예쁜 곳 가서 하면 힐링될 거 같더라구요",
          "original_form": "너무 재밌어 보이고 바다 예쁜 곳 가서 하면 힐링될 거 같더라구요",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100025010.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025010.1.32",
          "form": "여름 더운데",
          "original_form": "여름 더운데",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025010.1.33",
          "form": "시원한 물속에서",
          "original_form": "시원한 물속에서",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025010.1.34",
          "form": "제대로 힐링",
          "original_form": "제대로 힐링",
          "speaker_id": "2",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025010.1.35",
          "form": "그쵸 요새 서핑 가르쳐주는 곳도 늘어서",
          "original_form": "그쵸 요새 서핑 가르쳐주는 곳도 늘어서",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100025010.1.36",
          "form": "시도할만 한 것 같아요!",
          "original_form": "시도할만 한 것 같아요!",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100025010.1.37",
          "form": "아 기대되네요 진짜",
          "original_form": "아 기대되네요 진짜",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100025010.1.38",
          "form": "코로나 끝나면 바로 시도!!",
          "original_form": "코로나 끝나면 바로 시도!!",
