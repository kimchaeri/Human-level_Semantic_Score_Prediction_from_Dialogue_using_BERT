      "utterance": [
        {
          "id": "MDRW2100007419.1.1",
          "form": "쇼핑좋아하시나요",
          "original_form": "쇼핑좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007419.1.2",
          "form": "육아용품 핫딜 뜨면 자주해요",
          "original_form": "육아용품 핫딜 뜨면 자주해요",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007419.1.3",
          "form": "저도 핫딜뜨면 자주했는데",
          "original_form": "저도 핫딜뜨면 자주했는데",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007419.1.4",
          "form": "이젠 자제해요 ㅎ",
          "original_form": "이젠 자제해요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007419.1.5",
          "form": "맘스홀릭베이비 아시죠?",
          "original_form": "맘스홀릭베이비 아시죠?",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007419.1.6",
          "form": "네 알아요",
          "original_form": "네 알아요",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007419.1.7",
          "form": "거기에 각종 핫딜 뜨면 알려주더라고요",
          "original_form": "거기에 각종 핫딜 뜨면 알려주더라고요",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007419.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007419.1.9",
          "form": "너무 충동소비하게 해서 잘안가요 ㅎ",
          "original_form": "너무 충동소비하게 해서 잘안가요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007419.1.10",
          "form": "쿠팡 위메프 이런거 어디 이용하시나요",
          "original_form": "쿠팡 위메프 이런거 어디 이용하시나요",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007419.1.11",
          "form": "육아용품은 한철이라 맘카페에서 싸게사서 쓰고 말았어요",
          "original_form": "육아용품은 한철이라 맘카페에서 싸게사서 쓰고 말았어요",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007419.1.12",
          "form": "저는 네이버 이용해요",
          "original_form": "저는 네이버 이용해요",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007419.1.13",
          "form": "쿠팡위메프는 정해놓고쓰진않고\r\n싸게 나온게 있다하면가고 아님 잘안가요",
          "original_form": "쿠팡위메프는 정해놓고쓰진않고\r\n싸게 나온게 있다하면가고 아님 잘안가요",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007419.1.14",
          "form": "네이버페이 쓰시나봐요",
          "original_form": "네이버페이 쓰시나봐요",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007419.1.15",
          "form": "적립 진짜 많이해줘서 자주이용해요",
          "original_form": "적립 진짜 많이해줘서 자주이용해요",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007419.1.16",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007419.1.17",
          "form": "멤버쉽해놨어요",
          "original_form": "멤버쉽해놨어요",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007419.1.18",
          "form": "저도 그거 고민했는데",
          "original_form": "저도 그거 고민했는데",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007419.1.19",
          "form": "어차피 최저가검색도 여기서하니 그냥 네이버에서 사요",
          "original_form": "어차피 최저가검색도 여기서하니 그냥 네이버에서 사요",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007419.1.20",
          "form": "하면 적립이 눈에 띄게 다른가요",
          "original_form": "하면 적립이 눈에 띄게 다른가요",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007419.1.21",
          "form": "현금 충전해서 결제하면 차이가 많이나요",
          "original_form": "현금 충전해서 결제하면 차이가 많이나요",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007419.1.22",
          "form": "그거아니면 카드로만 결제하면 큰차이가 없지싶어요",
          "original_form": "그거아니면 카드로만 결제하면 큰차이가 없지싶어요",
          "speaker_id": "2",
          "time": "20210822 02:12"
        }
      ]
