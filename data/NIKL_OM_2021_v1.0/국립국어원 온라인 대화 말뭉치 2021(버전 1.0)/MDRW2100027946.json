        {
          "id": "MDRW2100027946.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027946.1.2",
          "form": "좋아합ㄴ디ㅏ!!",
          "original_form": "좋아합ㄴ디ㅏ!!",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027946.1.3",
          "form": "전 필라테스랑 수영 좋아해요",
          "original_form": "전 필라테스랑 수영 좋아해요",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027946.1.4",
          "form": "아 저도 필라테스 좋아해요!",
          "original_form": "아 저도 필라테스 좋아해요!",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027946.1.5",
          "form": "기구하시나요?",
          "original_form": "기구하시나요?",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027946.1.6",
          "form": "네네!! 그동안 단체로 하다가 저번달부터 1대1 해보고 있어요",
          "original_form": "네네!! 그동안 단체로 하다가 저번달부터 1대1 해보고 있어요",
          "speaker_id": "1",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027946.1.7",
          "form": "진짜 몸 라인이 달라지는 느낌!!!",
          "original_form": "진짜 몸 라인이 달라지는 느낌!!!",
          "speaker_id": "2",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100027946.1.8",
          "form": "아 근데 식이도 병행해야 하는데..",
          "original_form": "아 근데 식이도 병행해야 하는데..",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100027946.1.9",
          "form": "식이를 안해서 망했어요",
          "original_form": "식이를 안해서 망했어요",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100027946.1.10",
          "form": "건강하려고 운동하죠! 그래도 키커진 느낌 들지 않나요?",
          "original_form": "건강하려고 운동하죠! 그래도 키커진 느낌 들지 않나요?",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100027946.1.11",
          "form": "맞아요 ㅎㅎㅎ 몸이 구부정하다가 좀 펴진 느낌이에요 ㅋㅋ",
          "original_form": "맞아요 ㅎㅎㅎ 몸이 구부정하다가 좀 펴진 느낌이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100027946.1.12",
          "form": "근데 안하면 바로 원상복구된다는점이ㅜㅠ",
          "original_form": "근데 안하면 바로 원상복구된다는점이ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100027946.1.13",
          "form": "ㅋㅋㅋㅋ운동 진짜 죽을때까지 해야 하나봐요",
          "original_form": "ㅋㅋㅋㅋ운동 진짜 죽을때까지 해야 하나봐요",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100027946.1.14",
          "form": "너무 약해요 근육녀석 한번 생겼음 그대로 있을것이지",
          "original_form": "너무 약해요 근육녀석 한번 생겼음 그대로 있을것이지",
          "speaker_id": "2",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100027946.1.15",
          "form": "단백질 안먹으면 금방 사라지고...",
          "original_form": "단백질 안먹으면 금방 사라지고...",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100027946.1.16",
          "form": "ㅋㅋ 전 요즘에 유투브로 홈트했어요",
          "original_form": "ㅋㅋ 전 요즘에 유투브로 홈트했어요",
