      "utterance": [
        {
          "id": "MDRW2100009167.1.1",
          "form": "네 저는 요기요씁니다",
          "original_form": "네 저는 요기요씁니다",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009167.1.2",
          "form": "저도 요기요 사용중이에요.",
          "original_form": "저도 요기요 사용중이에요.",
          "speaker_id": "1",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100009167.1.3",
          "form": "배달음식 뭐 자주드시나요?",
          "original_form": "배달음식 뭐 자주드시나요?",
          "speaker_id": "2",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100009167.1.4",
          "form": "피자 자주 배달해먹어요.",
          "original_form": "피자 자주 배달해먹어요.",
          "speaker_id": "1",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100009167.1.5",
          "form": "우와 맛있겠어요",
          "original_form": "우와 맛있겠어요",
          "speaker_id": "2",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100009167.1.6",
          "form": "매콤한피자맛있겠다",
          "original_form": "매콤한피자맛있겠다",
          "speaker_id": "2",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100009167.1.7",
          "form": "피자에 갈링디핑소스찍어먹어보셧나요",
          "original_form": "피자에 갈링디핑소스찍어먹어보셧나요",
          "speaker_id": "2",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100009167.1.8",
          "form": "피자에는 갈릭디핑소스죠",
          "original_form": "피자에는 갈릭디핑소스죠",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009167.1.9",
          "form": "어디 피자좋아하시나요?",
          "original_form": "어디 피자좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100009167.1.10",
          "form": "저는 피자헛이요",
          "original_form": "저는 피자헛이요",
          "speaker_id": "2",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100009167.1.11",
          "form": "피자헛이 참 짭쪼름하고 맛있죠",
          "original_form": "피자헛이 참 짭쪼름하고 맛있죠",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009167.1.12",
          "form": "피자헛은 샐러드바인데",
          "original_form": "피자헛은 샐러드바인데",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009167.1.13",
          "form": "그쵸 안간지너무오래되었어요",
          "original_form": "그쵸 안간지너무오래되었어요",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009167.1.14",
          "form": "요즘피자헛이잘안보이는거같아요",
          "original_form": "요즘피자헛이잘안보이는거같아요",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009167.1.15",
          "form": "다행히 저희동네에는 피자헛있어요",
          "original_form": "다행히 저희동네에는 피자헛있어요",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009167.1.16",
          "form": "듣기로는 피자만팔고 샐러드바는 안하더라고요",
          "original_form": "듣기로는 피자만팔고 샐러드바는 안하더라고요",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009167.1.17",
          "form": "코로나때문인가봐요",
          "original_form": "코로나때문인가봐요",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009167.1.18",
          "form": "그럼갈필요가없음",
          "original_form": "그럼갈필요가없음",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009167.1.19",
          "form": "맞아요 피자만 먹을거면 배달하죠",
          "original_form": "맞아요 피자만 먹을거면 배달하죠",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009167.1.20",
          "form": "네 근데 가서먹으면 더맛잇긴해요",
          "original_form": "네 근데 가서먹으면 더맛잇긴해요",
          "speaker_id": "2",
          "time": "20210822 22:24"
        }
      ]
