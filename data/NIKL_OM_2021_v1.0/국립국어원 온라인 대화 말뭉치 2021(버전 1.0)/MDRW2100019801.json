      "utterance": [
        {
          "id": "MDRW2100019801.1.1",
          "form": "넷플릭스 영화 추천하시는거 있으신가요?",
          "original_form": "넷플릭스 영화 추천하시는거 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019801.1.2",
          "form": "기묘한이야기 추천해요",
          "original_form": "기묘한이야기 추천해요",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019801.1.3",
          "form": "오 감사합니당",
          "original_form": "오 감사합니당",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100019801.1.4",
          "form": "지금 하나 볼까 했거든요",
          "original_form": "지금 하나 볼까 했거든요",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100019801.1.5",
          "form": "킹덤은 보셧겟죠 ㅎㅎ",
          "original_form": "킹덤은 보셧겟죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100019801.1.6",
          "form": "저두 하나 추천해주세요",
          "original_form": "저두 하나 추천해주세요",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100019801.1.7",
          "form": "앗 킹덤은 아직 안봤어욥ㅠ",
          "original_form": "앗 킹덤은 아직 안봤어욥ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019801.1.8",
          "form": "저는 키싱부스요ㅋㅋ",
          "original_form": "저는 키싱부스요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100019801.1.9",
          "form": "하이틴 정석느낌이 좋네요",
          "original_form": "하이틴 정석느낌이 좋네요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100019801.1.10",
          "form": "아ㅋㅋ키싱부스",
          "original_form": "아ㅋㅋ키싱부스",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100019801.1.11",
          "form": "전좀오글거렷어요ㅋㅋㅋㅋ",
          "original_form": "전좀오글거렷어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100019801.1.12",
          "form": "풋풋한 느낌",
          "original_form": "풋풋한 느낌",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100019801.1.13",
          "form": "ㅋㅋㅋ그런면이 있긴하죠ㅋㅋ",
          "original_form": "ㅋㅋㅋ그런면이 있긴하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100019801.1.14",
          "form": "그런게 엄청 땡길때가 있더라구요",
          "original_form": "그런게 엄청 땡길때가 있더라구요",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100019801.1.15",
          "form": "브리저튼 도 추천!",
          "original_form": "브리저튼 도 추천!",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100019801.1.16",
          "form": "고전하이틴이라 생각하고",
          "original_form": "고전하이틴이라 생각하고",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100019801.1.17",
          "form": "땡길때 보세용",
          "original_form": "땡길때 보세용",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100019801.1.18",
          "form": "오옹 완전 많이 아시네용ㄱ",
          "original_form": "오옹 완전 많이 아시네용ㄱ",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100019801.1.19",
          "form": "감사합니닷",
          "original_form": "감사합니닷",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100019801.1.20",
          "form": "넷플 왓챠 엄청 봣엇죵",
          "original_form": "넷플 왓챠 엄청 봣엇죵",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100019801.1.21",
          "form": "요즘 즐겨보시는 티비프로그램 잇으신가요",
          "original_form": "요즘 즐겨보시는 티비프로그램 잇으신가요",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100019801.1.22",
          "form": "크으 저는 대만드라마 보고 있어요ㅋㅋ",
          "original_form": "크으 저는 대만드라마 보고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100019801.1.23",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100019801.1.24",
          "form": "뭔가요?",
          "original_form": "뭔가요?",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100019801.1.25",
          "form": "장난스런키스가 다시 생각나서 정주행중이에요!",
          "original_form": "장난스런키스가 다시 생각나서 정주행중이에요!",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100019801.1.26",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100019801.1.27",
          "form": "오글거리지만 그맛에 보고 있습니다ㅋㅋㅋ",
          "original_form": "오글거리지만 그맛에 보고 있습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100019801.1.28",
          "form": "장키ㅋㅋㅋ 그시절 우리가 좋아했던 소녀",
          "original_form": "장키ㅋㅋㅋ 그시절 우리가 좋아했던 소녀",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100019801.1.29",
          "form": "이거도재밋어용",
          "original_form": "이거도재밋어용",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100019801.1.30",
          "form": "대만하이틴",
          "original_form": "대만하이틴",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100019801.1.31",
          "form": "크으 맞아요!",
          "original_form": "크으 맞아요!",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100019801.1.32",
          "form": "재밋져ㅜㅜ",
          "original_form": "재밋져ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 23:52"
        }
      ]
    }
