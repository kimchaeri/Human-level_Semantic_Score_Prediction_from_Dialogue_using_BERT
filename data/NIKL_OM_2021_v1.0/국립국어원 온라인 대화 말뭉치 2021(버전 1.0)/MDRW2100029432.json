        {
          "id": "MDRW2100029432.1.1",
          "form": "결혼은 하셨나요",
          "original_form": "결혼은 하셨나요",
          "speaker_id": "1",
          "time": "20210903 07:52"
        },
        {
          "id": "MDRW2100029432.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210903 07:54"
        },
        {
          "id": "MDRW2100029432.1.3",
          "form": "결혼하셨어요,,",
          "original_form": "결혼하셨어요,,",
          "speaker_id": "2",
          "time": "20210903 07:54"
        },
        {
          "id": "MDRW2100029432.1.4",
          "form": "네 기혼자입니다",
          "original_form": "네 기혼자입니다",
          "speaker_id": "1",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100029432.1.5",
          "form": "몇년차세요?",
          "original_form": "몇년차세요?",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100029432.1.6",
          "form": "전 오래되었네요\n24년이 되었네요",
          "original_form": "전 오래되었네요\n24년이 되었네요",
          "speaker_id": "1",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100029432.1.7",
          "form": "어머 이제 곧 25주년 맞으시겠네요",
          "original_form": "어머 이제 곧 25주년 맞으시겠네요",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100029432.1.8",
          "form": "미리 축하드립니다",
          "original_form": "미리 축하드립니다",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100029432.1.9",
          "form": "네 12월달 이예요",
          "original_form": "네 12월달 이예요",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100029432.1.10",
          "form": "남편분과 집안일 분담해서 하는 편이세요?",
          "original_form": "남편분과 집안일 분담해서 하는 편이세요?",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100029432.1.11",
          "form": "네 젠 요리 남편은 빨래 청소는 공동 으로해요",
          "original_form": "네 젠 요리 남편은 빨래 청소는 공동 으로해요",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100029432.1.12",
          "form": "집안일 자주 하시나요 ?",
          "original_form": "집안일 자주 하시나요 ?",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100029432.1.13",
          "form": "네 근데 해도해도 귀찮고 싫기만 하네요",
          "original_form": "네 근데 해도해도 귀찮고 싫기만 하네요",
          "speaker_id": "2",
          "time": "20210904 08:31"
        },
        {
          "id": "MDRW2100029432.1.14",
          "form": "언제쯤 로봇들이 나와서 대신해주는 세상이 오려나ㅜㅜ",
          "original_form": "언제쯤 로봇들이 나와서 대신해주는 세상이 오려나ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 08:31"
        },
        {
          "id": "MDRW2100029432.1.15",
          "form": "요즘 로봇들의 영역이 점점 커지고 있죠",
          "original_form": "요즘 로봇들의 영역이 점점 커지고 있죠",
          "speaker_id": "1",
          "time": "20210904 08:32"
        },
        {
          "id": "MDRW2100029432.1.16",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210904 08:32"
        },
        {
          "id": "MDRW2100029432.1.17",
          "form": "집안일도 빨리 대신해줌 좋겠어요",
          "original_form": "집안일도 빨리 대신해줌 좋겠어요",
          "speaker_id": "2",
          "time": "20210904 08:32"
        },
        {
          "id": "MDRW2100029432.1.18",
          "form": "구석구석 청소 잘하는 로봇이 필요해요",
          "original_form": "구석구석 청소 잘하는 로봇이 필요해요",
          "speaker_id": "1",
          "time": "20210904 08:34"
        },
        {
          "id": "MDRW2100029432.1.19",
          "form": "맞아요 사람손처럼 꼼꼼 깨끗이 해주는!!",
          "original_form": "맞아요 사람손처럼 꼼꼼 깨끗이 해주는!!",
