      "utterance": [
        {
          "id": "MDRW2100021352.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100021352.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100021352.1.3",
          "form": "가고싶은 여행지 있나요?",
          "original_form": "가고싶은 여행지 있나요?",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021352.1.4",
          "form": "가보고 싶은곳은 많아요!",
          "original_form": "가보고 싶은곳은 많아요!",
          "speaker_id": "2",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021352.1.5",
          "form": "저도 많아요ㅎㅎ",
          "original_form": "저도 많아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100021352.1.6",
          "form": "어디 가보고 싶으세요?",
          "original_form": "어디 가보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100021352.1.7",
          "form": "유럽이요",
          "original_form": "유럽이요",
          "speaker_id": "1",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100021352.1.8",
          "form": "오!  저도 해외는 유럽 가보고 싶어요",
          "original_form": "오!  저도 해외는 유럽 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100021352.1.9",
          "form": "유럽 어디 젤 가고싶으세요?",
          "original_form": "유럽 어디 젤 가고싶으세요?",
          "speaker_id": "1",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100021352.1.10",
          "form": "음 일단 프랑스 파리 가보고 싶어요\r\n어릴때부터 그냥 가보고 싶던 곳 중 하나라서ㅎㅎ",
          "original_form": "음 일단 프랑스 파리 가보고 싶어요\r\n어릴때부터 그냥 가보고 싶던 곳 중 하나라서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100021352.1.11",
          "form": "저도 파리 가보고 싶어요ㅎ +이탈리아도요",
          "original_form": "저도 파리 가보고 싶어요ㅎ +이탈리아도요",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100021352.1.12",
          "form": "이탈리아도 좋죠!",
          "original_form": "이탈리아도 좋죠!",
          "speaker_id": "2",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100021352.1.13",
          "form": "시간도 돈도 여유있어서 조금 여유롭게 기간 잡아서 다녀오면 넘 좋을거 같아요",
          "original_form": "시간도 돈도 여유있어서 조금 여유롭게 기간 잡아서 다녀오면 넘 좋을거 같아요",
          "speaker_id": "2",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100021352.1.14",
          "form": "그쵸 유럽은 한달씩 여행간다는데 정말 재밌을듯해요",
          "original_form": "그쵸 유럽은 한달씩 여행간다는데 정말 재밌을듯해요",
          "speaker_id": "1",
          "time": "20210828 18:19"
        }
      ]
