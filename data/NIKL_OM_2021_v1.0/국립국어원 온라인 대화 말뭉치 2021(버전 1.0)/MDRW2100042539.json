      "utterance": [
        {
          "id": "MDRW2100042539.1.1",
          "form": "저는 겨울 간식이 좋네요",
          "original_form": "저는 겨울 간식이 좋네요",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042539.1.2",
          "form": "계절은 가을이 낫고 ㅋㅋㅋ",
          "original_form": "계절은 가을이 낫고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042539.1.3",
          "form": "오 겨울간식은 귤 좋아하세요?",
          "original_form": "오 겨울간식은 귤 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042539.1.4",
          "form": "팥붕이여!!@",
          "original_form": "팥붕이여!!@",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042539.1.5",
          "form": "ㅋㅋㅋ 호빵도 좋아요",
          "original_form": "ㅋㅋㅋ 호빵도 좋아요",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042539.1.6",
          "form": "아니 이런 제가 붕어빵을 잊었다니",
          "original_form": "아니 이런 제가 붕어빵을 잊었다니",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042539.1.7",
          "form": "호빵은 피자!!",
          "original_form": "호빵은 피자!!",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042539.1.8",
          "form": "팥붕파이신가요",
          "original_form": "팥붕파이신가요",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042539.1.9",
          "form": "전.. 슈붕파입니다..",
          "original_form": "전.. 슈붕파입니다..",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042539.1.10",
          "form": "네 팥붕이여!!",
          "original_form": "네 팥붕이여!!",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042539.1.11",
          "form": "슈붕 또한 맛있죠",
          "original_form": "슈붕 또한 맛있죠",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.12",
          "form": "슈붕방 팥붕반",
          "original_form": "슈붕방 팥붕반",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.13",
          "form": "제 픽입니댜",
          "original_form": "제 픽입니댜",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.14",
          "form": "그거져",
          "original_form": "그거져",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.15",
          "form": "꼭 추운 겨울에 호호불며 먹어줘야해여",
          "original_form": "꼭 추운 겨울에 호호불며 먹어줘야해여",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.16",
          "form": "맛있져 ㅠㅠ",
          "original_form": "맛있져 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.17",
          "form": "오즘은 타코야끼 파는 곳도 많아서ㅠ좋아요",
          "original_form": "오즘은 타코야끼 파는 곳도 많아서ㅠ좋아요",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.18",
          "form": "저 오뎅도",
          "original_form": "저 오뎅도",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.19",
          "form": "진짜 겨울에 최고",
          "original_form": "진짜 겨울에 최고",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.20",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.21",
          "form": "타코야끼 맛있어요 ㅠㅠ",
          "original_form": "타코야끼 맛있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.22",
          "form": "오뎅 진짜 저 사랑해요",
          "original_form": "오뎅 진짜 저 사랑해요",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.23",
          "form": "계절에 딱",
          "original_form": "계절에 딱",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042539.1.24",
          "form": "겨울이져",
          "original_form": "겨울이져",
          "speaker_id": "2",
