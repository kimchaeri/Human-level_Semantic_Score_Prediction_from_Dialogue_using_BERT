      "utterance": [
        {
          "id": "MDRW2100021260.1.1",
          "form": "아니 안해",
          "original_form": "아니 안해",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021260.1.2",
          "form": "링피트가 사고싶어서 캐봤어",
          "original_form": "링피트가 사고싶어서 캐봤어",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021260.1.3",
          "form": "물론 ㅇ난 티비도 스위치도 거실장도 업지만",
          "original_form": "물론 ㅇ난 티비도 스위치도 거실장도 업지만",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021260.1.4",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021260.1.5",
          "form": "스위치가없는데 링피트를샀다고?",
          "original_form": "스위치가없는데 링피트를샀다고?",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021260.1.6",
          "form": "아 캐봣어=말해봣어 구나",
          "original_form": "아 캐봣어=말해봣어 구나",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021260.1.7",
          "form": "사고 싶엇다고!",
          "original_form": "사고 싶엇다고!",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021260.1.8",
          "form": "ㅌㅋㅋㅋㅋㅋ왜 사투리를 못알아듯어",
          "original_form": "ㅌㅋㅋㅋㅋㅋ왜 사투리를 못알아듯어",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021260.1.9",
          "form": "나도링피트사고싶은디",
          "original_form": "나도링피트사고싶은디",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.10",
          "form": "울 애긩 경상ㅇ도 여자잔아",
          "original_form": "울 애긩 경상ㅇ도 여자잔아",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.11",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.12",
          "form": "작년2월유행이었나?",
          "original_form": "작년2월유행이었나?",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.13",
          "form": "중고급처매물보면",
          "original_form": "중고급처매물보면",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.14",
          "form": "싸게 이용하다가",
          "original_form": "싸게 이용하다가",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.15",
          "form": "한두달쯤있다가 운동질리면 링피트팔고",
          "original_form": "한두달쯤있다가 운동질리면 링피트팔고",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.16",
          "form": "오 진짜?",
          "original_form": "오 진짜?",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.17",
          "form": "당근마켓에서",
          "original_form": "당근마켓에서",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.18",
          "form": "2달에 2만원정도면",
          "original_form": "2달에 2만원정도면",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.19",
          "form": "나중에 구해바야것노",
          "original_form": "나중에 구해바야것노",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.20",
          "form": "쓸수있을거같은데",
          "original_form": "쓸수있을거같은데",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.21",
          "form": "두달이나 써놓고 2만원 빼서",
          "original_form": "두달이나 써놓고 2만원 빼서",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.22",
          "form": "파는거가",
          "original_form": "파는거가",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.23",
          "form": "근데 그거 티비화면에연결해야하는거아니가?",
          "original_form": "근데 그거 티비화면에연결해야하는거아니가?",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.24",
          "form": "양심 ㅈㅑ기했네",
          "original_form": "양심 ㅈㅑ기했네",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.25",
          "form": "그치 중고로",
          "original_form": "그치 중고로",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021260.1.26",
          "form": "ㅇㅇ티비에",
          "original_form": "ㅇㅇ티비에",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021260.1.27",
          "form": "너네집에티비잇나?",
          "original_form": "너네집에티비잇나?",
          "speaker_id": "1",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021260.1.28",
          "form": "운동할수잇는충분한공간확보와",
          "original_form": "운동할수잇는충분한공간확보와",
          "speaker_id": "1",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021260.1.29",
          "form": "티비 없어...",
          "original_form": "티비 없어...",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021260.1.30",
          "form": "공간...? 없어...",
          "original_form": "공간...? 없어...",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021260.1.31",
          "form": "하지만 갖고싶을순 있는거잖아",
          "original_form": "하지만 갖고싶을순 있는거잖아",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021260.1.32",
          "form": "티비가없는데어케링피트를해",
          "original_form": "티비가없는데어케링피트를해",
          "speaker_id": "1",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021260.1.33",
          "form": "모든걸 현실성을 염두하고 소원해야하는 것은 아니야",
          "original_form": "모든걸 현실성을 염두하고 소원해야하는 것은 아니야",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021260.1.34",
          "form": "난링피트못하는이유 아빠방침대에 티비달려잇음",
          "original_form": "난링피트못하는이유 아빠방침대에 티비달려잇음",
          "speaker_id": "1",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021260.1.35",
          "form": "폰화면으론 운동안되?",
          "original_form": "폰화면으론 운동안되?",
          "speaker_id": "1",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021260.1.36",
          "form": "거실에는 업서?",
          "original_form": "거실에는 업서?",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021260.1.37",
          "form": "음... 폰 화면은.. 쫌...",
          "original_form": "음... 폰 화면은.. 쫌...",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021260.1.38",
          "form": "하고싶은 맘이 안들어",
          "original_form": "하고싶은 맘이 안들어",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021260.1.39",
          "form": "거실에잇는거 방금이너무안돼서 넣엇어",
          "original_form": "거실에잇는거 방금이너무안돼서 넣엇어",
          "speaker_id": "1",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021260.1.40",
          "form": "아이패드로하면될거같은데",
          "original_form": "아이패드로하면될거같은데",
          "speaker_id": "1",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021260.1.41",
          "form": "애긔야 아이패드 언니 줘",
          "original_form": "애긔야 아이패드 언니 줘",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021260.1.42",
          "form": "운동 하게",
          "original_form": "운동 하게{emoji:♡}",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021260.1.43",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021260.1.44",
          "form": "우리는사고흐름이 비슷하다",
          "original_form": "우리는사고흐름이 비슷하다",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.45",
          "form": "왜 웃어",
          "original_form": "왜 웃어",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.46",
          "form": "줘",
          "original_form": "줘",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.47",
          "form": "내거보고친거아니지?",
          "original_form": "내거보고친거아니지?",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.48",
          "form": "아닌데",
          "original_form": "아닌데",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.49",
          "form": "비슷한 말 햇네 ㅋㅋ",
          "original_form": "비슷한 말 햇네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.50",
          "form": "이게 상대방 말이 좀 늦게 딜레이돼서올라와",
          "original_form": "이게 상대방 말이 좀 늦게 딜레이돼서올라와",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.51",
          "form": "톡이랑은 좀 다른듯",
          "original_form": "톡이랑은 좀 다른듯",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.52",
          "form": "야야 운동스터디할래?",
          "original_form": "야야 운동스터디할래?",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.53",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.54",
          "form": "오히려 좋네",
          "original_form": "오히려 좋네",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.55",
          "form": "그거 하면",
          "original_form": "그거 하면",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.56",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.57",
          "form": "걍 내 돈 뺏어먹겟단",
          "original_form": "걍 내 돈 뺏어먹겟단",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.58",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.59",
          "form": "얘기랑 같은말로 들려",
          "original_form": "얘기랑 같은말로 들려",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.60",
          "form": "그..그..왜 우리",
          "original_form": "그..그..왜 우리",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.61",
          "form": "너는 돈을소중히여기니까",
          "original_form": "너는 돈을소중히여기니까",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.62",
          "form": "운동하겠지",
          "original_form": "운동하겠지",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.63",
          "form": "고딩때 떡뽁기 스터디?",
          "original_form": "고딩때 떡뽁기 스터디?",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.64",
          "form": "10년전얘기하지마",
          "original_form": "10년전얘기하지마",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021260.1.65",
          "form": "그것도 결국 나의 돈을 빼먹는 스터디엿자나ㅡㅡ",
          "original_form": "그것도 결국 나의 돈을 빼먹는 스터디엿자나ㅡㅡ",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100021260.1.66",
          "form": "왜냐면 내가 젤 불성실햇으이까ㅡㅡ",
          "original_form": "왜냐면 내가 젤 불성실햇으이까ㅡㅡ",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100021260.1.67",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ난 돈이 소중하니까 시작을 안할테야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ난 돈이 소중하니까 시작을 안할테야",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
