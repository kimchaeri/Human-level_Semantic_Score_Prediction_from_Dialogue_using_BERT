      "utterance": [
        {
          "id": "MDRW2100027728.1.1",
          "form": "공연보는거 좋아하시겠어요?",
          "original_form": "공연보는거 좋아하시겠어요?",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100027728.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100027728.1.3",
          "form": "근데 요즘엔 ...",
          "original_form": "근데 요즘엔 ...",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100027728.1.4",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100027728.1.5",
          "form": "저도 가고싶네요 콘서트",
          "original_form": "저도 가고싶네요 콘서트",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100027728.1.6",
          "form": "마지막 콘은 어디세요?",
          "original_form": "마지막 콘은 어디세요?",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100027728.1.7",
          "form": "성시경 콘서트요",
          "original_form": "성시경 콘서트요",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100027728.1.8",
          "form": "연말에 했던",
          "original_form": "연말에 했던",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100027728.1.9",
          "form": "우와~ 가보고 싶었던 콘!",
          "original_form": "우와~ 가보고 싶었던 콘!",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100027728.1.10",
          "form": "연말에도 야외에서 하나요? 아니죠?",
          "original_form": "연말에도 야외에서 하나요? 아니죠?",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100027728.1.11",
          "form": "네 연말엔 밖에서 안하고",
          "original_form": "네 연말엔 밖에서 안하고",
          "speaker_id": "2",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100027728.1.12",
          "form": "실내에서",
          "original_form": "실내에서",
          "speaker_id": "2",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100027728.1.13",
          "form": "아~ 실내에서 하는구나",
          "original_form": "아~ 실내에서 하는구나",
          "speaker_id": "1",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100027728.1.14",
          "form": "고대생임에도 연대야외음악당에서 콘하기로 유명하잖아요",
          "original_form": "고대생임에도 연대야외음악당에서 콘하기로 유명하잖아요",
          "speaker_id": "1",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100027728.1.15",
          "form": "네 야외에서 했어요",
          "original_form": "네 야외에서 했어요",
          "speaker_id": "2",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100027728.1.16",
          "form": "노천극장",
          "original_form": "노천극장",
          "speaker_id": "2",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100027728.1.17",
          "form": "그건 한번 가보고 싶어요",
          "original_form": "그건 한번 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100027728.1.18",
          "form": "콘서트 야외가 더 안전할거 같은데",
          "original_form": "콘서트 야외가 더 안전할거 같은데",
          "speaker_id": "2",
          "time": "20210903 14:45"
        },
        {
          "id": "MDRW2100027728.1.19",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "2",
          "time": "20210903 14:45"
        },
        {
          "id": "MDRW2100027728.1.20",
          "form": "요즘요?",
          "original_form": "요즘요?",
          "speaker_id": "1",
          "time": "20210903 15:17"
        },
        {
          "id": "MDRW2100027728.1.21",
          "form": "야외여도 통제안되고 입장까지 사람들 몰리고",
          "original_form": "야외여도 통제안되고 입장까지 사람들 몰리고",
          "speaker_id": "1",
          "time": "20210903 15:17"
        },
        {
          "id": "MDRW2100027728.1.22",
          "form": "글쎄요",
          "original_form": "글쎄요",
          "speaker_id": "1",
          "time": "20210903 15:17"
        },
        {
          "id": "MDRW2100027728.1.23",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100027728.1.24",
          "form": "에휴 코로나ㅜㅜ",
          "original_form": "에휴 코로나ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 20:29"
        }
      ]
