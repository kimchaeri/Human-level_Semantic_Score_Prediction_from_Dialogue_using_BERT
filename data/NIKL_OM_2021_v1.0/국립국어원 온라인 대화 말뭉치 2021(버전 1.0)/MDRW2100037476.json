      "utterance": [
        {
          "id": "MDRW2100037476.1.1",
          "form": "어제 저녁 드셨나요?",
          "original_form": "어제 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 00:42"
        },
        {
          "id": "MDRW2100037476.1.2",
          "form": "네 저녁 먹었어요",
          "original_form": "네 저녁 먹었어요",
          "speaker_id": "2",
          "time": "20210901 00:45"
        },
        {
          "id": "MDRW2100037476.1.3",
          "form": "어떤거 드셨나요?",
          "original_form": "어떤거 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 00:48"
        },
        {
          "id": "MDRW2100037476.1.4",
          "form": "밥이랑 계란말이에 간장 참기름 비벼먹었어요",
          "original_form": "밥이랑 계란말이에 간장 참기름 비벼먹었어요",
          "speaker_id": "2",
          "time": "20210901 00:50"
        },
        {
          "id": "MDRW2100037476.1.5",
          "form": "아 안먹은지 오래됐는데 오랜만에 먹고싶네요",
          "original_form": "아 안먹은지 오래됐는데 오랜만에 먹고싶네요",
          "speaker_id": "1",
          "time": "20210901 00:52"
        },
        {
          "id": "MDRW2100037476.1.6",
          "form": "배달음식 자주 드시나요?",
          "original_form": "배달음식 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210901 00:53"
        },
        {
          "id": "MDRW2100037476.1.7",
          "form": "자주는 안 먹고 엄마가 밥하기 힘들 때 먹어요",
          "original_form": "자주는 안 먹고 엄마가 밥하기 힘들 때 먹어요",
          "speaker_id": "2",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100037476.1.8",
          "form": "주로 어떤거 드시나요?",
          "original_form": "주로 어떤거 드시나요?",
          "speaker_id": "1",
          "time": "20210901 01:04"
        },
        {
          "id": "MDRW2100037476.1.9",
          "form": "피자랑 햄버거 자주 먹어요",
          "original_form": "피자랑 햄버거 자주 먹어요",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100037476.1.10",
          "form": "저녁 때 뭐 드셨어요?",
          "original_form": "저녁 때 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210901 01:13"
        },
        {
          "id": "MDRW2100037476.1.11",
          "form": "저는 오늘 피자 먹었어요!",
          "original_form": "저는 오늘 피자 먹었어요!",
          "speaker_id": "1",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100037476.1.12",
          "form": "피자 좋아하시나요?",
          "original_form": "피자 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100037476.1.13",
          "form": "네 피자 좋아요 어디 피자 좋아하세요?",
          "original_form": "네 피자 좋아요 어디 피자 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 01:27"
        },
        {
          "id": "MDRW2100037476.1.14",
          "form": "저는 피자마루 치킨 좋아해요!",
          "original_form": "저는 피자마루 치킨 좋아해요!",
          "speaker_id": "1",
          "time": "20210901 03:04"
        },
        {
          "id": "MDRW2100037476.1.15",
          "form": "어디 피자 좋아하세요?",
          "original_form": "어디 피자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 03:04"
        },
        {
          "id": "MDRW2100037476.1.16",
          "form": "파파존스 피자 좋아합니다",
          "original_form": "파파존스 피자 좋아합니다",
