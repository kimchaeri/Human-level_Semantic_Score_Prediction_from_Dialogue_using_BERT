      "utterance": [
        {
          "id": "MDRW2100006291.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006291.1.2",
          "form": "바려동물키우시나요?",
          "original_form": "바려동물키우시나요?",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006291.1.3",
          "form": "저는 따로 키우진 않아요",
          "original_form": "저는 따로 키우진 않아요",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006291.1.4",
          "form": "키우고 싶어도 집이 좁아서...",
          "original_form": "키우고 싶어도 집이 좁아서...",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006291.1.5",
          "form": "전 옛날에 구피",
          "original_form": "전 옛날에 구피",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006291.1.6",
          "form": "물고기요",
          "original_form": "물고기요",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006291.1.7",
          "form": "물고기랑 새키워봤네요..",
          "original_form": "물고기랑 새키워봤네요..",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006291.1.8",
          "form": "오 신기하네요",
          "original_form": "오 신기하네요",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006291.1.9",
          "form": "새요?",
          "original_form": "새요?",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006291.1.10",
          "form": "어떤 종류 인가요?",
          "original_form": "어떤 종류 인가요?",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006291.1.11",
          "form": "새는 일찍일어나서 시끄럽더군요",
          "original_form": "새는 일찍일어나서 시끄럽더군요",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006291.1.12",
          "form": "종은ㅇ기억이 잘안나네요",
          "original_form": "종은ㅇ기억이 잘안나네요",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006291.1.13",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006291.1.14",
          "form": "속담처럼 새는 부지런 한 가봐요",
          "original_form": "속담처럼 새는 부지런 한 가봐요",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006291.1.15",
          "form": "구피는 번식력이짱~~",
          "original_form": "구피는 번식력이짱~~",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006291.1.16",
          "form": "네..맞아요",
          "original_form": "네..맞아요",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006291.1.17",
          "form": "정말요 저는 물고기를 안키워봐서",
          "original_form": "정말요 저는 물고기를 안키워봐서",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006291.1.18",
          "form": "몰랐네요",
          "original_form": "몰랐네요",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006291.1.19",
          "form": "부지런해요",
          "original_form": "부지런해요",
