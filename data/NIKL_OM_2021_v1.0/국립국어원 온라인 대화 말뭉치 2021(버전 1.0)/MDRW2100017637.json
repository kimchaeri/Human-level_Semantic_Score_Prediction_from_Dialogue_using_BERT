      "utterance": [
        {
          "id": "MDRW2100017637.1.1",
          "form": "아뇨 입맛이 없어서",
          "original_form": "아뇨 입맛이 없어서",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017637.1.2",
          "form": "영양소 골고루 잘 먹어야할텐데",
          "original_form": "영양소 골고루 잘 먹어야할텐데",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017637.1.3",
          "form": "저는 케이크 아이스크림 과일 이런것만 좋아해요",
          "original_form": "저는 케이크 아이스크림 과일 이런것만 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017637.1.4",
          "form": "달콤한 맛을 좋아하는구나",
          "original_form": "달콤한 맛을 좋아하는구나",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017637.1.5",
          "form": "그런 편인듯해요",
          "original_form": "그런 편인듯해요",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017637.1.6",
          "form": "흑임자죽도 설탕 왕창 넣어 먹어요",
          "original_form": "흑임자죽도 설탕 왕창 넣어 먹어요",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017637.1.7",
          "form": "흑임자가 몸에 참 좋은건데",
          "original_form": "흑임자가 몸에 참 좋은건데",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017637.1.8",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017637.1.9",
          "form": "설탕말고 꿀 넣어요",
          "original_form": "설탕말고 꿀 넣어요",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017637.1.10",
          "form": "근데 꿀은 특유의 향이 있어서",
          "original_form": "근데 꿀은 특유의 향이 있어서",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017637.1.11",
          "form": "그럼 올리고당 같은 거는 어때요",
          "original_form": "그럼 올리고당 같은 거는 어때요",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017637.1.12",
          "form": "설탕이 몸에 많이 안좋은가요?",
          "original_form": "설탕이 몸에 많이 안좋은가요?",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017637.1.13",
          "form": "그럼 되도록 안 먹어야죠",
          "original_form": "그럼 되도록 안 먹어야죠",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017637.1.14",
          "form": "근데 유명레시피보면 설탕이 다 꼭 들어가던데",
          "original_form": "근데 유명레시피보면 설탕이 다 꼭 들어가던데",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017637.1.15",
          "form": "그게 요즘 문제에요",
          "original_form": "그게 요즘 문제에요",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017637.1.16",
          "form": "과자 음료수 그런거 다 설탕덩어리라 되도록 안 먹어야해요",
          "original_form": "과자 음료수 그런거 다 설탕덩어리라 되도록 안 먹어야해요",
          "speaker_id": "1",
          "time": "20210826 20:33"
        }
