      "utterance": [
        {
          "id": "MDRW2100043408.1.1",
          "form": "앞으로 어떻게 될진 모르겠지만 잘 됐으면좋겠어요",
          "original_form": "앞으로 어떻게 될진 모르겠지만 잘 됐으면좋겠어요",
          "speaker_id": "1",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043408.1.2",
          "form": "name2님도요!!",
          "original_form": "&name2&님도요!!",
          "speaker_id": "1",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043408.1.3",
          "form": "예..저도 화이팅입니닼ㅋㅋㅋㅋ",
          "original_form": "예..저도 화이팅입니닼ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043408.1.4",
          "form": "name2님 하시는 운동 있나요?",
          "original_form": "&name2&님 하시는 운동 있나요?",
          "speaker_id": "1",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043408.1.5",
          "form": "오늘 과식했는데",
          "original_form": "오늘 과식했는데",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043408.1.6",
          "form": "지금 산책나가는거 어떤거같나요?",
          "original_form": "지금 산책나가는거 어떤거같나요?",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043408.1.7",
          "form": "비 그쳤어요??",
          "original_form": "비 그쳤어요??",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043408.1.8",
          "form": "살짝 쌀쌀할거 같긴한데 잘 챙겨입고 나가면 시원할 거 같네요 오늘 뭔가 거리가 조용해서 좋은 거 같아요!",
          "original_form": "살짝 쌀쌀할거 같긴한데 잘 챙겨입고 나가면 시원할 거 같네요 오늘 뭔가 거리가 조용해서 좋은 거 같아요!",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043408.1.9",
          "form": "그런거같아요!!!",
          "original_form": "그런거같아요!!!",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043408.1.10",
          "form": "저도 지금 야식먹는중이라 ㅋㅋ 이따 자전거좀 돌려야겠어요..",
          "original_form": "저도 지금 야식먹는중이라 ㅋㅋ 이따 자전거좀 돌려야겠어요..",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043408.1.11",
          "form": "저는 게을러서",
          "original_form": "저는 게을러서",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100043408.1.12",
          "form": "누워서 유튜브만보다가 나갈시간을 놓쳤답니다",
          "original_form": "누워서 유튜브만보다가 나갈시간을 놓쳤답니다",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100043408.1.13",
          "form": "저도 자전거 안타고 누워서 폰보고 있답니다",
          "original_form": "저도 자전거 안타고 누워서 폰보고 있답니다",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043408.1.14",
          "form": "우리 놀수있을때 놀아요!!!",
          "original_form": "우리 놀수있을때 놀아요!!!",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043408.1.15",
          "form": "맞습니다.. 이 여유로움~",
          "original_form": "맞습니다.. 이 여유로움~",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100043408.1.16",
          "form": "숙면은 잘 취하시나요!",
          "original_form": "숙면은 잘 취하시나요!",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100043408.1.17",
          "form": "요즘엔 잘 못자는것같아요ㅠㅠ",
          "original_form": "요즘엔 잘 못자는것같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100043408.1.18",
          "form": "아이구 잠드는게 어려우신 건가요? 아니면 자주깨는??",
          "original_form": "아이구 잠드는게 어려우신 건가요? 아니면 자주깨는??",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100043408.1.19",
          "form": "자주깨요!!",
          "original_form": "자주깨요!!",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100043408.1.20",
          "form": "잔걱정 많은시기엔",
          "original_form": "잔걱정 많은시기엔",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100043408.1.21",
          "form": "자주 깨더라고요",
          "original_form": "자주 깨더라고요",
