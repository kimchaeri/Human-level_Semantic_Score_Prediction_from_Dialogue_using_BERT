      "utterance": [
        {
          "id": "MDRW2100020723.1.1",
          "form": "종교있으신가요?",
          "original_form": "종교있으신가요?",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020723.1.2",
          "form": "종교는 없어요",
          "original_form": "종교는 없어요",
          "speaker_id": "1",
          "time": "20210829 10:40"
        },
        {
          "id": "MDRW2100020723.1.3",
          "form": "지금은....",
          "original_form": "지금은....",
          "speaker_id": "1",
          "time": "20210829 10:40"
        },
        {
          "id": "MDRW2100020723.1.4",
          "form": "원래는 종교가 있으셨나요?",
          "original_form": "원래는 종교가 있으셨나요?",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100020723.1.5",
          "form": "기독교였어요",
          "original_form": "기독교였어요",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100020723.1.6",
          "form": "지금은 왜 안믿으세요?",
          "original_form": "지금은 왜 안믿으세요?",
          "speaker_id": "2",
          "time": "20210829 23:11"
        },
        {
          "id": "MDRW2100020723.1.7",
          "form": "잘안가니깐요 바빠서요",
          "original_form": "잘안가니깐요 바빠서요",
          "speaker_id": "1",
          "time": "20210830 09:13"
        },
        {
          "id": "MDRW2100020723.1.8",
          "form": "그리고 지금 이상황에서 더더욱 할수가없죠",
          "original_form": "그리고 지금 이상황에서 더더욱 할수가없죠",
          "speaker_id": "1",
          "time": "20210830 09:14"
        },
        {
          "id": "MDRW2100020723.1.9",
          "form": "종교 활동은 안가도 믿음은 가지고 계신건가요??",
          "original_form": "종교 활동은 안가도 믿음은 가지고 계신건가요??",
          "speaker_id": "2",
          "time": "20210830 10:01"
        },
        {
          "id": "MDRW2100020723.1.10",
          "form": "제자신을믿어요",
          "original_form": "제자신을믿어요",
          "speaker_id": "1",
          "time": "20210830 10:17"
        },
        {
          "id": "MDRW2100020723.1.11",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "1",
          "time": "20210830 11:48"
        },
        {
          "id": "MDRW2100020723.1.12",
          "form": "저두 제 자신을 믿어요 ㅋㅋ 종교는 가져본적이 없어요",
          "original_form": "저두 제 자신을 믿어요 ㅋㅋ 종교는 가져본적이 없어요",
          "speaker_id": "2",
          "time": "20210830 11:56"
        },
        {
          "id": "MDRW2100020723.1.13",
          "form": "ㅋㅋㅋㄱ자기자신",
          "original_form": "ㅋㅋㅋㄱ자기자신",
          "speaker_id": "1",
          "time": "20210830 15:31"
        },
        {
          "id": "MDRW2100020723.1.14",
          "form": "코로나 전에는 종교행사 참여 많이 하셨나요?",
          "original_form": "코로나 전에는 종교행사 참여 많이 하셨나요?",
          "speaker_id": "2",
          "time": "20210830 18:19"
        },
        {
          "id": "MDRW2100020723.1.15",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100020723.1.16",
          "form": "친구따라 같이 가면",
          "original_form": "친구따라 같이 가면",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100020723.1.17",
          "form": "사은품과 먹을꺼 준다길래",
          "original_form": "사은품과 먹을꺼 준다길래",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100020723.1.18",
          "form": "목적달성을 위해 따라갔어요",
          "original_form": "목적달성을 위해 따라갔어요",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100020723.1.19",
          "form": "저도 달란트잔치때 교회 많이갔던것같아요 ㅋㅋ",
          "original_form": "저도 달란트잔치때 교회 많이갔던것같아요 ㅋㅋ",
          "speaker_id": "2",
