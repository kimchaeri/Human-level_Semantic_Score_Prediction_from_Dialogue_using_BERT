      "utterance": [
        {
          "id": "MDRW2100013509.1.1",
          "form": "반려동물 기르세요?",
          "original_form": "반려동물 기르세요?",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013509.1.2",
          "form": "애완동물을 좋아해요",
          "original_form": "애완동물을 좋아해요",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013509.1.3",
          "form": "아 기르시진 않구요? 저랑 똑같네요",
          "original_form": "아 기르시진 않구요? 저랑 똑같네요",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013509.1.4",
          "form": "네 그냥 좋아만하고 있어요",
          "original_form": "네 그냥 좋아만하고 있어요",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013509.1.5",
          "form": "고양이 강아지 아니면 다른 동물 중 어떤동물 좋아하세요",
          "original_form": "고양이 강아지 아니면 다른 동물 중 어떤동물 좋아하세요",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013509.1.6",
          "form": "저는 고양이 좋아해요",
          "original_form": "저는 고양이 좋아해요",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013509.1.7",
          "form": "앗 저도요~~~! 그래서 유튜브로 자주 봐요 고영이들",
          "original_form": "앗 저도요~~~! 그래서 유튜브로 자주 봐요 고영이들",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013509.1.8",
          "form": "고양이는 너무 귀엽죠ㅋㅋㅋ",
          "original_form": "고양이는 너무 귀엽죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013509.1.9",
          "form": "맞아요ㅋㅋㅋ생긴것도 행동도 넘 귀여워요",
          "original_form": "맞아요ㅋㅋㅋ생긴것도 행동도 넘 귀여워요",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013509.1.10",
          "form": "고양이 키워보고싶어욬ㅋㅋㅋ",
          "original_form": "고양이 키워보고싶어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013509.1.11",
          "form": "길냥이와 시간 보내보시거나 고영이카페같은곳 가보셨나요?",
          "original_form": "길냥이와 시간 보내보시거나 고영이카페같은곳 가보셨나요?",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013509.1.12",
          "form": "친구집에 고양이 키워서",
          "original_form": "친구집에 고양이 키워서",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013509.1.13",
          "form": "오오 키우는 친구가 있군요",
          "original_form": "오오 키우는 친구가 있군요",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013509.1.14",
          "form": "잘놀아줘요",
          "original_form": "잘놀아줘요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013509.1.15",
          "form": "전 키우는 친구가 없어서 아쉬워요ㅠ",
          "original_form": "전 키우는 친구가 없어서 아쉬워요ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:36"
        }
      ]
