      "utterance": [
        {
          "id": "MDRW2100045057.1.1",
          "form": "햄버거 좋아하세요?",
          "original_form": "햄버거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045057.1.2",
          "form": "네 오늘도먹었어요 ㅋㅋㅋㅋ",
          "original_form": "네 오늘도먹었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045057.1.3",
          "form": "오 어디꺼 드셨나요",
          "original_form": "오 어디꺼 드셨나요",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045057.1.4",
          "form": "버거킹이여!!",
          "original_form": "버거킹이여!!",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045057.1.5",
          "form": "버거킹 맛있죠 저 와퍼 넘 좋아해요!",
          "original_form": "버거킹 맛있죠 저 와퍼 넘 좋아해요!",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045057.1.6",
          "form": "뭐드셨어요!",
          "original_form": "뭐드셨어요!",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045057.1.7",
          "form": "저 오늘 아빠상어랑 콰트로치즈와퍼여 ㅎㅎ",
          "original_form": "저 오늘 아빠상어랑 콰트로치즈와퍼여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045057.1.8",
          "form": "name2님뭐젤좋아하세요",
          "original_form": "&name2&님뭐젤좋아하세요",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045057.1.9",
          "form": "저도 콰트로랑 새우와퍼요!!",
          "original_form": "저도 콰트로랑 새우와퍼요!!",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045057.1.10",
          "form": "아빠상어 맛나나요?",
          "original_form": "아빠상어 맛나나요?",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045057.1.11",
          "form": "크 그두개가  젤맛있는것같아요 저도",
          "original_form": "크 그두개가  젤맛있는것같아요 저도",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045057.1.12",
          "form": "맞아요 와퍼가 짱이에요..",
          "original_form": "맞아요 와퍼가 짱이에요..",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045057.1.13",
          "form": "요즘 버거킹 행사 자주 해서 넘 좋아요",
          "original_form": "요즘 버거킹 행사 자주 해서 넘 좋아요",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045057.1.14",
          "form": "맞아요 ㅎㅎ오늘 skt반값이었고 요기요도 행사했었고",
          "original_form": "맞아요 ㅎㅎ오늘 skt반값이었고 요기요도 행사했었고",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045057.1.15",
          "form": "그쵸ㅎㅎ 할인받아서 먹으면 참 좋아요",
          "original_form": "그쵸ㅎㅎ 할인받아서 먹으면 참 좋아요",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045057.1.16",
          "form": "네 오늘도 사실먹을생각은없었는데 너무싸서 ㅎㅎㄹ",
          "original_form": "네 오늘도 사실먹을생각은없었는데 너무싸서 ㅎㅎㄹ",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045057.1.17",
          "form": "행사하몈 진짜 눈길이 가지않나요... 버거킹 지나갈때마다 살까말까 고민해요 ...ㅋㅋ",
          "original_form": "행사하몈 진짜 눈길이 가지않나요... 버거킹 지나갈때마다 살까말까 고민해요 ...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045057.1.18",
          "form": "네 ㅋㅋㅋㅋㅋ앗 행사하네 그럼먹어야지 하고 그대로들어가여",
          "original_form": "네 ㅋㅋㅋㅋㅋ앗 행사하네 그럼먹어야지 하고 그대로들어가여",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045057.1.19",
          "form": "진짜루요 .. 저는 버거킹은 감자말고 버거만 2개먹는 조합 참 좋아해요...",
          "original_form": "진짜루요 .. 저는 버거킹은 감자말고 버거만 2개먹는 조합 참 좋아해요...",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045057.1.20",
