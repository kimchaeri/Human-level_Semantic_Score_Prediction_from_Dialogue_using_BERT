      "utterance": [
        {
          "id": "MDRW2100026722.1.1",
          "form": "본격적으로 트위터 얘기나 해보자고 제안",
          "original_form": "본격적으로 트위터 얘기나 해보자고 제안",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026722.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026722.1.3",
          "form": "트위터 계정 거의ㅜ10년돼가는거같아서",
          "original_form": "트위터 계정 거의ㅜ10년돼가는거같아서",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026722.1.4",
          "form": "계이하고싶은데 진짜 너무 귀찮아거 20년은 더쓸거같음",
          "original_form": "계이하고싶은데 진짜 너무 귀찮아거 20년은 더쓸거같음",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026722.1.5",
          "form": "님도 20년까지 나랑같이 트위터해야함",
          "original_form": "님도 20년까지 나랑같이 트위터해야함",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026722.1.6",
          "form": "아 계이 귀찮은 거 킹정",
          "original_form": "아 계이 귀찮은 거 킹정",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026722.1.7",
          "form": "지우고 싶은 트윗 너무 많고 계정 세탁하고 싶은제",
          "original_form": "지우고 싶은 트윗 너무 많고 계정 세탁하고 싶은제",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026722.1.8",
          "form": "트청도 안 먹히고",
          "original_form": "트청도 안 먹히고",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026722.1.9",
          "form": "계이하려하면 트친들이 다 버릴ㄹ 거 같아가지구",
          "original_form": "계이하려하면 트친들이 다 버릴ㄹ 거 같아가지구",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026722.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ마저ㅠㅠㅠ트청안먹어 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ마저ㅠㅠㅠ트청안먹어 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026722.1.11",
          "form": "나도 한 20년 쓸듯...",
          "original_form": "나도 한 20년 쓸듯...",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026722.1.12",
          "form": "영원히 함께하자...",
          "original_form": "영원히 함께하자...",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026722.1.13",
          "form": "사실 트친들 어디까지 같이 가야할지도 모르겟어 제가 감히 제 새 계정에 데려가도되는걸까요 이런ㄴ늒김",
          "original_form": "사실 트친들 어디까지 같이 가야할지도 모르겟어 제가 감히 제 새 계정에 데려가도되는걸까요 이런ㄴ늒김",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026722.1.14",
          "form": "그니까 영원히 함께해....",
          "original_form": "그니까 영원히 함께해....",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026722.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ아 인정인정 너무 공감돼.....",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 인정인정 너무 공감돼.....",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026722.1.16",
          "form": "마자 나를 견뎌",
          "original_form": "마자 나를 견뎌",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026722.1.17",
          "form": "나라는 감옥속에 갇혀",
          "original_form": "나라는 감옥속에 갇혀",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026722.1.18",
          "form": "아이돌을 좋아하든 뭘 좋아하든...",
          "original_form": "아이돌을 좋아하든 뭘 좋아하든...",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026722.1.19",
          "form": "이렇게 점점 뻔뻔해진다",
          "original_form": "이렇게 점점 뻔뻔해진다",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026722.1.20",
          "form": "맞아내가 남돌을 좋아하든 뭘하든 견디세요",
          "original_form": "맞아내가 남돌을 좋아하든 뭘하든 견디세요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026722.1.21",
          "form": "그냥 저를 탐라에 꼬옥 심어두시면됩니다..",
          "original_form": "그냥 저를 탐라에 꼬옥 심어두시면됩니다..",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026722.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ트위터 점점 이상해져서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ트위터 점점 이상해져서",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026722.1.23",
          "form": "뭔가.장르계 파기도 무섭구",
          "original_form": "뭔가.장르계 파기도 무섭구",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026722.1.24",
          "form": "마자.. 특히 좀 큰 장르면 더 ......",
          "original_form": "마자.. 특히 좀 큰 장르면 더 ......",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026722.1.25",
          "form": "글구 트친소같은거 안한지너무오래돼서",
          "original_form": "글구 트친소같은거 안한지너무오래돼서",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026722.1.26",
          "form": "장르계만들어도 트친못만들고혼자놀듯",
          "original_form": "장르계만들어도 트친못만들고혼자놀듯",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026722.1.27",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026722.1.28",
          "form": "장르계 만들어도 거기 못 섞일 거 같아",
          "original_form": "장르계 만들어도 거기 못 섞일 거 같아",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026722.1.29",
          "form": "너무 어린 친구들도 많구...",
          "original_form": "너무 어린 친구들도 많구...",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026722.1.30",
          "form": "ㅁㅏ저...",
          "original_form": "ㅁㅏ저...",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026722.1.31",
          "form": "글구 플텍에서 벗어나고싶지않은..",
          "original_form": "글구 플텍에서 벗어나고싶지않은..",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026722.1.32",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026722.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026722.1.34",
          "form": "나 살 수 없어",
          "original_form": "나 살 수 없어",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026722.1.35",
          "form": "플텍없인 못살아 정말 못살아",
          "original_form": "플텍없인 못살아 정말 못살아",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026722.1.36",
          "form": "나도...",
          "original_form": "나도...",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026722.1.37",
          "form": "물고기가 물 밖에서 살 수 있어??",
          "original_form": "물고기가 물 밖에서 살 수 있어??",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026722.1.38",
          "form": "거의 그 급",
          "original_form": "거의 그 급",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026722.1.39",
          "form": "흐 ㅜㅜㅜ",
          "original_form": "흐 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026722.1.40",
          "form": "저번에 그 못죽판에",
          "original_form": "저번에 그 못죽판에",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.41",
          "form": "코스관련 일터진거",
          "original_form": "코스관련 일터진거",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.42",
          "form": "오 그랬어?",
          "original_form": "오 그랬어?",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.43",
          "form": "내 트친이랑 트친의트친분 일이었거든",
          "original_form": "내 트친이랑 트친의트친분 일이었거든",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.44",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.45",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.46",
          "form": "헐........너무무서워",
          "original_form": "헐........너무무서워",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.47",
          "form": "근데 그걸로 트친이 플텍걸엇다가",
          "original_form": "근데 그걸로 트친이 플텍걸엇다가",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.48",
          "form": "너무 갑갑해서",
          "original_form": "너무 갑갑해서",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.49",
          "form": "플텍푼다는거 듣고",
          "original_form": "플텍푼다는거 듣고",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.50",
          "form": "플텍안거는사람은저렇구나 싶엇음...",
          "original_form": "플텍안거는사람은저렇구나 싶엇음...",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.51",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026722.1.52",
          "form": "나랑 다른 세계에서 사시는데 거의",
          "original_form": "나랑 다른 세계에서 사시는데 거의",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026722.1.53",
          "form": "그니까요...",
          "original_form": "그니까요...",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026722.1.54",
          "form": "안전망이 아니라....족쇄구나 그분들이겐",
          "original_form": "안전망이 아니라....족쇄구나 그분들이겐",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026722.1.55",
          "form": "그런가봐.........",
          "original_form": "그런가봐.........",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026722.1.56",
          "form": "나는 아늑한 플텍인데",
          "original_form": "나는 아늑한 플텍인데",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026722.1.57",
          "form": "나두 완죤 아늑",
          "original_form": "나두 완죤 아늑",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026722.1.58",
          "form": "마이 스위트 홈",
          "original_form": "마이 스위트 홈",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026722.1.59",
          "form": "그리고 요즘 트친들 점점 트위터 안 들어와서",
          "original_form": "그리고 요즘 트친들 점점 트위터 안 들어와서",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026722.1.60",
          "form": "약간 쓸쓸",
          "original_form": "약간 쓸쓸",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026722.1.61",
          "form": "다들 현생이 바쁜거지",
          "original_form": "다들 현생이 바쁜거지",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
