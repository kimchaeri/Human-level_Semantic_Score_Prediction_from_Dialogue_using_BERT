      "utterance": [
        {
          "id": "MDRW2100028810.1.1",
          "form": "짤은 어디서 구하세요?",
          "original_form": "짤은 어디서 구하세요?",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028810.1.2",
          "form": "그냥 커뮤니티서 보이면 받죠 ㅎ",
          "original_form": "그냥 커뮤니티서 보이면 받죠 ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028810.1.3",
          "form": "제리는 플립버전도 나왔더라구요",
          "original_form": "제리는 플립버전도 나왔더라구요",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028810.1.4",
          "form": "주 활동무대가 어디신가요?",
          "original_form": "주 활동무대가 어디신가요?",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028810.1.5",
          "form": "주 활동..이라기엔 짤은 수집하지만 활동은 안합니다",
          "original_form": "주 활동..이라기엔 짤은 수집하지만 활동은 안합니다",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028810.1.6",
          "form": "유령이쥬",
          "original_form": "유령이쥬",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028810.1.7",
          "form": "그나마 더쿠라는 사이트?",
          "original_form": "그나마 더쿠라는 사이트?",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028810.1.8",
          "form": "사실 커뮤 잘 안해서요ㅎㅎ",
          "original_form": "사실 커뮤 잘 안해서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028810.1.9",
          "form": "웃긴 글 모음집 좋아해서 그런거 찾아다니다 봄 모여요!",
          "original_form": "웃긴 글 모음집 좋아해서 그런거 찾아다니다 봄 모여요!",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028810.1.10",
          "form": "더쿠?",
          "original_form": "더쿠?",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028810.1.11",
          "form": "저 아는곳이 없고",
          "original_form": "저 아는곳이 없고",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028810.1.12",
          "form": "회사 정책이",
          "original_form": "회사 정책이",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028810.1.13",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028810.1.14",
          "form": "국정원에서 감시해서",
          "original_form": "국정원에서 감시해서",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028810.1.15",
          "form": "볼대가 없어요",
          "original_form": "볼대가 없어요",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028810.1.16",
          "form": "보배드림도 이상하로",
          "original_form": "보배드림도 이상하로",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028810.1.17",
          "form": "이상하고",
          "original_form": "이상하고",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028810.1.18",
          "form": "부동산카페만 보고있습니다 ㅋㅋㅋ",
          "original_form": "부동산카페만 보고있습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028810.1.19",
          "form": "노잼",
          "original_form": "노잼",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028810.1.20",
          "form": "대체 어디를 다니시는 거죠ㅋㅋ",
          "original_form": "대체 어디를 다니시는 거죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028810.1.21",
          "form": "노잼하니 잼먹고 싶네요",
          "original_form": "노잼하니 잼먹고 싶네요",
          "speaker_id": "1",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028810.1.22",
          "form": "잼 노래가 생각나네요",
          "original_form": "잼 노래가 생각나네요",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028810.1.23",
          "form": "예전에 엄마가 딸기를 끓여서 잼 만들어주셧는데",
          "original_form": "예전에 엄마가 딸기를 끓여서 잼 만들어주셧는데",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028810.1.24",
          "form": "잼을 다룬 노래도 있나요?",
          "original_form": "잼을 다룬 노래도 있나요?",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028810.1.25",
          "form": "가수 잼",
          "original_form": "가수 잼",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028810.1.26",
          "form": "오오 금손이시다",
          "original_form": "오오 금손이시다",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028810.1.27",
          "form": "난 멈추지않는다",
          "original_form": "난 멈추지않는다",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028810.1.28",
          "form": "금손이십니다 ㅋㅋ",
          "original_form": "금손이십니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028810.1.29",
          "form": "잘모르실겁니다 우리회사 잘몰라용",
          "original_form": "잘모르실겁니다 우리회사 잘몰라용",
          "speaker_id": "2",
          "time": "20210902 22:12"
        }
      ]
    }
