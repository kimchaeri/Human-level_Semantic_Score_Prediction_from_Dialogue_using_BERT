      "utterance": [
        {
          "id": "MDRW2100015328.1.1",
          "form": "이번에 같이 스티커 쇼핑했잖아요",
          "original_form": "이번에 같이 스티커 쇼핑했잖아요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015328.1.2",
          "form": "맘에드나요?",
          "original_form": "맘에드나요?",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015328.1.3",
          "form": "거울이 너무 귀여워요",
          "original_form": "거울이 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015328.1.4",
          "form": "아침마다 기분좋아여",
          "original_form": "아침마다 기분좋아여",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015328.1.5",
          "form": "만족합니까",
          "original_form": "만족합니까",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015328.1.6",
          "form": "세안머리띠도 잘 구입하신것 같아요",
          "original_form": "세안머리띠도 잘 구입하신것 같아요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015328.1.7",
          "form": "나도요 거울 귀여워서 좋아요",
          "original_form": "나도요 거울 귀여워서 좋아요",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015328.1.8",
          "form": "세안머리띠 정말 좋아요 강추",
          "original_form": "세안머리띠 정말 좋아요 강추",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015328.1.9",
          "form": "담엔 거울도 더 큰걸 사야겠어요",
          "original_form": "담엔 거울도 더 큰걸 사야겠어요",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015328.1.10",
          "form": "하나 사면 다른것에 만족할 수 없는 운명",
          "original_form": "하나 사면 다른것에 만족할 수 없는 운명",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015328.1.11",
          "form": "앜ㅋㅋ 쇼핑이 쇼핑을 부르죠",
          "original_form": "앜ㅋㅋ 쇼핑이 쇼핑을 부르죠",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015328.1.12",
          "form": "맘에들면 맘에둘어서 쇼핑을 부르고",
          "original_form": "맘에들면 맘에둘어서 쇼핑을 부르고",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015328.1.13",
          "form": "맘에안들면 만회해야해서 쇼핑을 불러요",
          "original_form": "맘에안들면 만회해야해서 쇼핑을 불러요",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015328.1.14",
          "form": "ㅋㅋㅋㅋ와 명언이다",
          "original_form": "ㅋㅋㅋㅋ와 명언이다",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015328.1.15",
          "form": "가슴에 새겨야겠네요",
          "original_form": "가슴에 새겨야겠네요",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015328.1.16",
          "form": "새기면 안돼요 여러뷴!!!",
          "original_form": "새기면 안돼요 여러뷴!!!",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015328.1.17",
          "form": "쇼핑은 사라져야합니다 여러분 ㅜㅜㅜ",
          "original_form": "쇼핑은 사라져야합니다 여러분 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015328.1.18",
          "form": "이미나는 흥청망청 인생",
          "original_form": "이미나는 흥청망청 인생",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015328.1.19",
          "form": "돈을 모아야합니다ㅠㅠㅠ",
          "original_form": "돈을 모아야합니다ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015328.1.20",
          "form": "최근에 무엇을 사셨나요?",
          "original_form": "최근에 무엇을 사셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015328.1.21",
          "form": "최근에 가을에 신을 구두 두켤레",
          "original_form": "최근에 가을에 신을 구두 두켤레",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015328.1.22",
          "form": "어머어머 벌써 사셨군요!",
          "original_form": "어머어머 벌써 사셨군요!",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015328.1.23",
          "form": "다음에 보여주십쇼",
          "original_form": "다음에 보여주십쇼",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015328.1.24",
          "form": "예 운동화 인생에서 좀 벗어나",
          "original_form": "예 운동화 인생에서 좀 벗어나",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015328.1.25",
          "form": "멋진 언니가 되어야겠다는 생각에 사봤어요",
          "original_form": "멋진 언니가 되어야겠다는 생각에 사봤어요",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015328.1.26",
          "form": "우와좋아요",
          "original_form": "우와좋아요",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015328.1.27",
          "form": "저도 오픈되길 기다리는 로퍼가있답니다!",
          "original_form": "저도 오픈되길 기다리는 로퍼가있답니다!",
          "speaker_id": "1",
          "time": "20210825 22:48"
        }
      ]
    }
