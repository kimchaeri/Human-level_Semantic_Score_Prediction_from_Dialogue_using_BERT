      "utterance": [
        {
          "id": "MDRW2100043726.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043726.1.2",
          "form": "베라에 모찌 드셔보셨어요???",
          "original_form": "베라에 모찌 드셔보셨어요???",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043726.1.3",
          "form": "오ㅋ 저 보이시나요?ㅋㅋ",
          "original_form": "오ㅋ 저 보이시나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043726.1.4",
          "form": "네 오늘은 치즈에 토마토 오랫만에 먹엇네요 ㅎ",
          "original_form": "네 오늘은 치즈에 토마토 오랫만에 먹엇네요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043726.1.5",
          "form": "방금 크림치즈 하나먹었어요ㅋ",
          "original_form": "방금 크림치즈 하나먹었어요ㅋ",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043726.1.6",
          "form": "드시고 계세요? ㅎ",
          "original_form": "드시고 계세요? ㅎ",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043726.1.7",
          "form": "두개 다찾아와서",
          "original_form": "두개 다찾아와서",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043726.1.8",
          "form": "크림치즈 맛나요",
          "original_form": "크림치즈 맛나요",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043726.1.9",
          "form": "오~~맛이 크림치즈예요?",
          "original_form": "오~~맛이 크림치즈예요?",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043726.1.10",
          "form": "인절미도 있는거 같던데",
          "original_form": "인절미도 있는거 같던데",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043726.1.11",
          "form": "네네 ㅎ 감귤이랑 두개가져왔어요",
          "original_form": "네네 ㅎ 감귤이랑 두개가져왔어요",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043726.1.12",
          "form": "감귤이랑 크림치즈?",
          "original_form": "감귤이랑 크림치즈?",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043726.1.13",
          "form": "인절미는 예상되는 맛이라",
          "original_form": "인절미는 예상되는 맛이라",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043726.1.14",
          "form": "크림치즈 맛나겠어요",
          "original_form": "크림치즈 맛나겠어요",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043726.1.15",
          "form": "감귤은 아직 안먹어봐서ㅎ",
          "original_form": "감귤은 아직 안먹어봐서ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043726.1.16",
          "form": "글킨하죠 인절미 맛 ㅋㅋ",
          "original_form": "글킨하죠 인절미 맛 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:08"
