      "utterance": [
        {
          "id": "MDRW2100044514.1.1",
          "form": "그래서 요즘은 밀키트 해먹는 편이예여!",
          "original_form": "그래서 요즘은 밀키트 해먹는 편이예여!",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044514.1.2",
          "form": "오오 밀키트 어떤거용??",
          "original_form": "오오 밀키트 어떤거용??",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044514.1.3",
          "form": "그냥 쿠팡프레시로 ㅎㅎ",
          "original_form": "그냥 쿠팡프레시로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044514.1.4",
          "form": "저번에 고츠잡채 사봤는데 맛있었어요",
          "original_form": "저번에 고츠잡채 사봤는데 맛있었어요",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044514.1.5",
          "form": "우아 고추잡채!!",
          "original_form": "우아 고추잡채!!",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044514.1.6",
          "form": "쿠팡이면 배송 빨리오는거죠.?",
          "original_form": "쿠팡이면 배송 빨리오는거죠.?",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044514.1.7",
          "form": "네 로켓배송이요!",
          "original_form": "네 로켓배송이요!",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044514.1.8",
          "form": "오 저도 밀키드 사야겟어요 !!",
          "original_form": "오 저도 밀키드 사야겟어요 !!",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044514.1.9",
          "form": "간단하죠?,",
          "original_form": "간단하죠?,",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044514.1.10",
          "form": "네네 ㅋㅋ거기 다 준비된 재료들이고",
          "original_form": "네네 ㅋㅋ거기 다 준비된 재료들이고",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044514.1.11",
          "form": "레시피도 있어서 어렵지 않아요",
          "original_form": "레시피도 있어서 어렵지 않아요",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044514.1.12",
          "form": "우앙 ㅠㅠ 밀키트 종류 다양하드라구요!!!",
          "original_form": "우앙 ㅠㅠ 밀키트 종류 다양하드라구요!!!",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044514.1.13",
          "form": "맞아요 ㅋㅋ한번 도전해보세여!",
          "original_form": "맞아요 ㅋㅋ한번 도전해보세여!",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044514.1.14",
          "form": "편해요 진짜",
          "original_form": "편해요 진짜",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044514.1.15",
          "form": "마트 가면 진짜 냉동식품 잘나와여 ㅋㅋㅋ",
          "original_form": "마트 가면 진짜 냉동식품 잘나와여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044514.1.16",
          "form": "ㅋㅋㅋ주로 어떤 냉동식품 사세요",
          "original_form": "ㅋㅋㅋ주로 어떤 냉동식품 사세요",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044514.1.17",
          "form": "저는 만두나 볶음밥이요..!",
          "original_form": "저는 만두나 볶음밥이요..!",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044514.1.18",
          "form": "요즘 떡도 잘나오드라구요 ㅋㅋ",
          "original_form": "요즘 떡도 잘나오드라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044514.1.19",
          "form": "오 굳지 않는 떡 요런거 사봤는데",
          "original_form": "오 굳지 않는 떡 요런거 사봤는데",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044514.1.20",
          "form": "맛있엇어요",
          "original_form": "맛있엇어요",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044514.1.21",
          "form": "굳지않는떡 진짜 신기해요 ㅋㅋ",
          "original_form": "굳지않는떡 진짜 신기해요 ㅋㅋ",
