      "utterance": [
        {
          "id": "MDRW2100039723.1.1",
          "form": "연애중이신가요?",
          "original_form": "연애중이신가요?",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039723.1.2",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039723.1.3",
          "form": "얼마나 되셨어요?",
          "original_form": "얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039723.1.4",
          "form": "2년정도된거같아요!",
          "original_form": "2년정도된거같아요!",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039723.1.5",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039723.1.6",
          "form": "아뇨ㅎㅎ",
          "original_form": "아뇨ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039723.1.7",
          "form": "소개팅으로 만나셨나요?",
          "original_form": "소개팅으로 만나셨나요?",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039723.1.8",
          "form": "네~ 소개로 만나긴했어요 ㅎ",
          "original_form": "네~ 소개로 만나긴했어요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039723.1.9",
          "form": "그래도 인연이시네요!",
          "original_form": "그래도 인연이시네요!",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039723.1.10",
          "form": "어찌보니 ~생각도못했는ㄷㅔ",
          "original_form": "어찌보니 ~생각도못했는ㄷㅔ",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039723.1.11",
          "form": "ㅋㅋ 결혼 전제로 만나세요?",
          "original_form": "ㅋㅋ 결혼 전제로 만나세요?",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039723.1.12",
          "form": "그러긴해요 ㅋㅋ",
          "original_form": "그러긴해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039723.1.13",
          "form": "ㅋㅋ 제 친구는 적령기에 세달만나고 결혼했어요ㅎㅎ",
          "original_form": "ㅋㅋ 제 친구는 적령기에 세달만나고 결혼했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039723.1.14",
          "form": "진짜 인연이면 기간은 상관없는거같아요",
          "original_form": "진짜 인연이면 기간은 상관없는거같아요",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039723.1.15",
          "form": "맞아요 결혼 5년찬데 아직또 싸운적 없대요",
          "original_form": "맞아요 결혼 5년찬데 아직또 싸운적 없대요",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039723.1.16",
          "form": "와 잘맞으시나봐요ㅠㅠ",
          "original_form": "와 잘맞으시나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039723.1.17",
          "form": "주위 친구들 다 잘살아요 오히려 일찍 20대 결혼한 친구가 트러블이 있더라고요",
          "original_form": "주위 친구들 다 잘살아요 오히려 일찍 20대 결혼한 친구가 트러블이 있더라고요",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100039723.1.18",
          "form": "너무일찍하는건 안좋은거같아요;; 전 개인적으로",
          "original_form": "너무일찍하는건 안좋은거같아요;; 전 개인적으로",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039723.1.19",
          "form": "맞아요 진짜 나이때매 성급할 필요도 없고요 인연이 생길때 그때!",
          "original_form": "맞아요 진짜 나이때매 성급할 필요도 없고요 인연이 생길때 그때!",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039723.1.20",
          "form": "전 아이생각이없어소 ~사실 결혼에대해 별생각이없어여ㅠ",
          "original_form": "전 아이생각이없어소 ~사실 결혼에대해 별생각이없어여ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100039723.1.21",
          "form": "ㅋㅋ 연애만 즐겨도 좋아요!",
          "original_form": "ㅋㅋ 연애만 즐겨도 좋아요!",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039723.1.22",
          "form": "전 연애조차 안하지만 ㅋㅋ",
          "original_form": "전 연애조차 안하지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039723.1.23",
          "form": "좋은분 만나실꺼에요!",
          "original_form": "좋은분 만나실꺼에요!",
          "speaker_id": "2",
          "time": "20210903 23:44"
        }
      ]
    }
  ]
