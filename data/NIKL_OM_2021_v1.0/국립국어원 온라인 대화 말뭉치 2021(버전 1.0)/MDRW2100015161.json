      "utterance": [
        {
          "id": "MDRW2100015161.1.1",
          "form": "쇼핑 자주하세요?",
          "original_form": "쇼핑 자주하세요?",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015161.1.2",
          "form": "저는 가끔이요!",
          "original_form": "저는 가끔이요!",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015161.1.3",
          "form": "주로 어떤거 사세요?",
          "original_form": "주로 어떤거 사세요?",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015161.1.4",
          "form": "저는 그때 그때 너무 달라서 최근에 산건 휴대폰 액정이고ㅋㅋㅋ",
          "original_form": "저는 그때 그때 너무 달라서 최근에 산건 휴대폰 액정이고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015161.1.5",
          "form": "name1님은 최근에 뭐사셨나요?",
          "original_form": "&name1&님은 최근에 뭐사셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015161.1.6",
          "form": "저는 운동화요!",
          "original_form": "저는 운동화요!",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015161.1.7",
          "form": "어제 배송와서 신나게 택배 뜯엇습니다",
          "original_form": "어제 배송와서 신나게 택배 뜯엇습니다",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015161.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015161.1.9",
          "form": "아 진짜 운동화",
          "original_form": "아 진짜 운동화",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015161.1.10",
          "form": "저 같은거 2년째신는중 ...",
          "original_form": "저 같은거 2년째신는중 ...",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015161.1.11",
          "form": "물론 여러개..!",
          "original_form": "물론 여러개..!",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015161.1.12",
          "form": "ㅋㅋㅋㅋ저도 그래요!",
          "original_form": "ㅋㅋㅋㅋ저도 그래요!",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015161.1.13",
          "form": "특히 다른신발보다 더 여러해 신게되는 것 같아요",
          "original_form": "특히 다른신발보다 더 여러해 신게되는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015161.1.14",
          "form": "저는 요즘 기다리는 상품이 있답니다ㅎㅎㅎㅎ",
          "original_form": "저는 요즘 기다리는 상품이 있답니다ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015161.1.15",
          "form": "앗 어떤거예요??ㅎㅎ",
          "original_form": "앗 어떤거예요??ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015161.1.16",
          "form": "티파니가 왓츠인마이백에서 소개해준",
          "original_form": "티파니가 왓츠인마이백에서 소개해준",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.17",
          "form": "손소독제가있는데",
          "original_form": "손소독제가있는데",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.18",
          "form": "찾아보니까 생각보다 저렴한거예요!",
          "original_form": "찾아보니까 생각보다 저렴한거예요!",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.19",
          "form": "헐헐",
          "original_form": "헐헐",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.20",
          "form": "6500원!",
          "original_form": "6500원!",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.21",
          "form": "저그거 뭔지 알아요!",
          "original_form": "저그거 뭔지 알아요!",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.23",
          "form": "손소독제업체는정말",
          "original_form": "손소독제업체는정말",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.24",
          "form": "심지어 할인받아서",
          "original_form": "심지어 할인받아서",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.25",
          "form": "3500원에샀어요",
          "original_form": "3500원에샀어요",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.26",
          "form": "티파니한테 절해야해요ㅠㅠ",
          "original_form": "티파니한테 절해야해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.27",
          "form": "무배",
          "original_form": "무배{emoji:♡}{emoji:♡}",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015161.1.28",
          "form": "대박대박 그건 무조건 사야해요",
          "original_form": "대박대박 그건 무조건 사야해요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015161.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ 마자요 그래서 제 친구도 사줬어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 마자요 그래서 제 친구도 사줬어요",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015161.1.30",
          "form": "어제 배송했다는데",
          "original_form": "어제 배송했다는데",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015161.1.31",
          "form": "ㅋㅋㅋㅋ요새 소소한 선물로 넘 좋은구 같아요",
          "original_form": "ㅋㅋㅋㅋ요새 소소한 선물로 넘 좋은구 같아요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015161.1.32",
          "form": "아직안와서 두근두근하면서 기다리는중이에요",
          "original_form": "아직안와서 두근두근하면서 기다리는중이에요",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015161.1.33",
          "form": "역시 택배는 늘 짜릿해요!",
          "original_form": "역시 택배는 늘 짜릿해요!",
