      "utterance": [
        {
          "id": "MDRW2100013012.1.1",
          "form": "오늘 날씨는 어떠신가요?",
          "original_form": "오늘 날씨는 어떠신가요?",
          "speaker_id": "2",
          "time": "20210825 11:19"
        },
        {
          "id": "MDRW2100013012.1.2",
          "form": "좀 흐린편인거 같아요",
          "original_form": "좀 흐린편인거 같아요",
          "speaker_id": "1",
          "time": "20210825 11:19"
        },
        {
          "id": "MDRW2100013012.1.3",
          "form": "오늘 소나기가 올까요?",
          "original_form": "오늘 소나기가 올까요?",
          "speaker_id": "1",
          "time": "20210825 11:20"
        },
        {
          "id": "MDRW2100013012.1.4",
          "form": "글쎄요..여기도 흐리기만 하네요",
          "original_form": "글쎄요..여기도 흐리기만 하네요",
          "speaker_id": "2",
          "time": "20210825 11:20"
        },
        {
          "id": "MDRW2100013012.1.5",
          "form": "어떤 날씨 좋아하세요?",
          "original_form": "어떤 날씨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 11:20"
        },
        {
          "id": "MDRW2100013012.1.6",
          "form": "전 가을같이 약간 선선하고 쾌적한 날씨요",
          "original_form": "전 가을같이 약간 선선하고 쾌적한 날씨요",
          "speaker_id": "1",
          "time": "20210825 11:21"
        },
        {
          "id": "MDRW2100013012.1.7",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 11:21"
        },
        {
          "id": "MDRW2100013012.1.8",
          "form": "전 초여름 좋아해요~산뜻한 날씨요~",
          "original_form": "전 초여름 좋아해요~산뜻한 날씨요~",
          "speaker_id": "2",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100013012.1.9",
          "form": "초여름 좋아하세요?",
          "original_form": "초여름 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100013012.1.10",
          "form": "네 초여름 약간 파릇파릇하고 평화로운 느낌이 좋아요",
          "original_form": "네 초여름 약간 파릇파릇하고 평화로운 느낌이 좋아요",
          "speaker_id": "1",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013012.1.11",
          "form": "저도 초록초록한 느낌 좋아해요",
          "original_form": "저도 초록초록한 느낌 좋아해요",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100013012.1.12",
          "form": "어떤 날씨를 싫어하세요?",
          "original_form": "어떤 날씨를 싫어하세요?",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100013012.1.13",
          "form": "저는 습해서 가만 있어도 땀차고 끈적적한 날씩 극혐해요",
          "original_form": "저는 습해서 가만 있어도 땀차고 끈적적한 날씩 극혐해요",
          "speaker_id": "1",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100013012.1.14",
          "form": "여름을 싫어하시겠어요~전 스산한 날이요ㅜ",
          "original_form": "여름을 싫어하시겠어요~전 스산한 날이요ㅜ",
          "speaker_id": "2",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100013012.1.15",
          "form": "스산한 날이면 요즘같은 날도 그런가요?",
          "original_form": "스산한 날이면 요즘같은 날도 그런가요?",
          "speaker_id": "1",
          "time": "20210825 12:42"
        },
        {
          "id": "MDRW2100013012.1.16",
          "form": "요즘은 아니고 초겨울이요~으슬으슬 추운 ㅜ",
          "original_form": "요즘은 아니고 초겨울이요~으슬으슬 추운 ㅜ",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100013012.1.17",
          "form": "싫어하는 계절 있으세요?",
          "original_form": "싫어하는 계절 있으세요?",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100013012.1.18",
          "form": "그렇군요 전 그냥 겨울이 빨리 깜깜해지고 추워서 별로 안좋아해요",
          "original_form": "그렇군요 전 그냥 겨울이 빨리 깜깜해지고 추워서 별로 안좋아해요",
          "speaker_id": "1",
          "time": "20210825 12:44"
        },
        {
          "id": "MDRW2100013012.1.19",
          "form": "저두요ㅜ나이도 먹는것 같고ㅜ춥기도 춥고 우울해요",
          "original_form": "저두요ㅜ나이도 먹는것 같고ㅜ춥기도 춥고 우울해요",
          "speaker_id": "2",
          "time": "20210825 12:45"
        }
      ]
