      "utterance": [
        {
          "id": "MDRW2100000831.1.1",
          "form": "저는 알바만 하고 있어요",
          "original_form": "저는 알바만 하고 있어요",
          "speaker_id": "2",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000831.1.2",
          "form": "직장 꿀팁 있나요? ㅋㅋㅋㅎㅋ",
          "original_form": "직장 꿀팁 있나요? ㅋㅋㅋㅎㅋ",
          "speaker_id": "2",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000831.1.3",
          "form": "그냥 물 흘러가듯 ... 스며드는게 좋은 거 같아요",
          "original_form": "그냥 물 흘러가듯 ... 스며드는게 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210819 19:52"
        },
        {
          "id": "MDRW2100000831.1.4",
          "form": "흐아.. 어렵네요..",
          "original_form": "흐아.. 어렵네요..",
          "speaker_id": "2",
          "time": "20210819 19:52"
        },
        {
          "id": "MDRW2100000831.1.5",
          "form": "역시 사회생활인가요..",
          "original_form": "역시 사회생활인가요..",
          "speaker_id": "2",
          "time": "20210819 19:52"
        },
        {
          "id": "MDRW2100000831.1.6",
          "form": "네.. 싫은 것도 해야하는 슬픔...",
          "original_form": "네.. 싫은 것도 해야하는 슬픔...",
          "speaker_id": "1",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000831.1.7",
          "form": "이상한 상사를 만나면 정말 최악입니다",
          "original_form": "이상한 상사를 만나면 정말 최악입니다",
          "speaker_id": "1",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000831.1.8",
          "form": "으아..",
          "original_form": "으아..",
          "speaker_id": "2",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000831.1.9",
          "form": "군인도 직업이겠죠...?",
          "original_form": "군인도 직업이겠죠...?",
          "speaker_id": "2",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000831.1.10",
          "form": "내년에 군인이 됩니다.. 이상한 선임 만나지 않으면 자밌다는대",
          "original_form": "내년에 군인이 됩니다.. 이상한 선임 만나지 않으면 자밌다는대",
          "speaker_id": "2",
          "time": "20210819 19:54"
        },
        {
          "id": "MDRW2100000831.1.11",
          "form": "제발...!!",
          "original_form": "제발...!!",
          "speaker_id": "2",
          "time": "20210819 19:54"
        },
        {
          "id": "MDRW2100000831.1.12",
          "form": "네 직업군인이니까요!!",
          "original_form": "네 직업군인이니까요!!",
          "speaker_id": "1",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000831.1.13",
          "form": "되게 멋있네요 직업군인이라니!!!",
          "original_form": "되게 멋있네요 직업군인이라니!!!",
          "speaker_id": "1",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000831.1.14",
          "form": "아 물론 저는 직업군이이 아니다만.. ㅋㅋㅋㅎㅋ",
          "original_form": "아 물론 저는 직업군이이 아니다만.. ㅋㅋㅋㅎㅋ",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000831.1.15",
          "form": "저는 못합니다",
          "original_form": "저는 못합니다",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000831.1.16",
          "form": "전 민간인이 되고 싶어요",
          "original_form": "전 민간인이 되고 싶어요",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000831.1.17",
          "form": "아하 군대를 가시는건가요??!??!?",
          "original_form": "아하 군대를 가시는건가요??!??!?",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000831.1.18",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000831.1.19",
          "form": "끌려가는거죠..",
          "original_form": "끌려가는거죠..",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000831.1.20",
          "form": "군대.....나름 괜찮을거에요",
          "original_form": "군대.....나름 괜찮을거에요",
          "speaker_id": "1",
          "time": "20210819 20:03"
        },
        {
          "id": "MDRW2100000831.1.21",
          "form": "좋은 선임만 만나면 스트레스도 없을텐데..!",
          "original_form": "좋은 선임만 만나면 스트레스도 없을텐데..!",
          "speaker_id": "1",
          "time": "20210819 20:03"
        },
        {
          "id": "MDRW2100000831.1.22",
          "form": "그쵸 그쵸!",
          "original_form": "그쵸 그쵸!",
          "speaker_id": "2",
          "time": "20210819 20:07"
        },
        {
          "id": "MDRW2100000831.1.23",
          "form": "짜피 내년에 가는거라.. 내년의 저한테 맞길려구요",
          "original_form": "짜피 내년에 가는거라.. 내년의 저한테 맞길려구요",
          "speaker_id": "2",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000831.1.24",
          "form": "그럼 아르바이트도 많이 해보셨어요?",
          "original_form": "그럼 아르바이트도 많이 해보셨어요?",
          "speaker_id": "2",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000831.1.25",
          "form": "네 엄청 많이는 아니지만 해봤어요!!",
          "original_form": "네 엄청 많이는 아니지만 해봤어요!!",
          "speaker_id": "1",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000831.1.26",
          "form": "아르바이트 뭐 해보셨어요?",
          "original_form": "아르바이트 뭐 해보셨어요?",
          "speaker_id": "1",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000831.1.27",
          "form": "저는 상하차랑.. 그 대형 마트 알바",
          "original_form": "저는 상하차랑.. 그 대형 마트 알바",
          "speaker_id": "2",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000831.1.28",
          "form": "하고 있어요!",
          "original_form": "하고 있어요!",
          "speaker_id": "2",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000831.1.29",
          "form": "방학에만 할려구요!",
          "original_form": "방학에만 할려구요!",
          "speaker_id": "2",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000831.1.30",
          "form": "그게 돈버는데 그렇게 좋다고 하더라구요!!!",
          "original_form": "그게 돈버는데 그렇게 좋다고 하더라구요!!!",
          "speaker_id": "1",
          "time": "20210819 20:13"
        }
      ]
