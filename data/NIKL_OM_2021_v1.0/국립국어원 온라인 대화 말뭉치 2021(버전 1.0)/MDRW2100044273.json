        {
          "id": "MDRW2100044273.1.1",
          "form": "님 일단 선수엿어요",
          "original_form": "님 일단 선수엿어요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044273.1.2",
          "form": "어떻겟어요",
          "original_form": "어떻겟어요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044273.1.3",
          "form": "와.. 검도선수요?",
          "original_form": "와.. 검도선수요?",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044273.1.4",
          "form": "슬렌더에 전완근 굳",
          "original_form": "슬렌더에 전완근 굳",
          "speaker_id": "2",
          "time": "20210907 22:07"
