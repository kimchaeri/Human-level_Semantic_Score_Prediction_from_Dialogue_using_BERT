          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044273.1.16",
          "form": "어떤 상이에여",
          "original_form": "어떤 상이에여",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044273.1.17",
          "form": "누구닮앗어요?",
          "original_form": "누구닮앗어요?",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044273.1.18",
          "form": "그건 없어요 약간 날카롭고순둥해요",
          "original_form": "그건 없어요 약간 날카롭고순둥해요",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044273.1.19",
          "form": "날카롭고 순둥해요",
          "original_form": "날카롭고 순둥해요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044273.1.20",
          "form": "피부는요?",
