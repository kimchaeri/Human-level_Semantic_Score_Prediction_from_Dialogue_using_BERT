          "original_form": "검도복 완전 미쳣",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044273.1.8",
          "form": "네 선수요 프로",
          "original_form": "네 선수요 프로",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044273.1.9",
          "form": "미쳤다..ㅠㅠ",
          "original_form": "미쳤다..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044273.1.10",
          "form": "안구공유좀요",
          "original_form": "안구공유좀요",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044273.1.11",
          "form": "간지철철",
          "original_form": "간지철철",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044273.1.12",
          "form": "그러고싶구요",
          "original_form": "그러고싶구요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
