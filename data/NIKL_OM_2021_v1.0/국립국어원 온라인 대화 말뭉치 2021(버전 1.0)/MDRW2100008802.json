      "utterance": [
        {
          "id": "MDRW2100008802.1.1",
          "form": "제일 좋았던 여행지가 어디야?",
          "original_form": "제일 좋았던 여행지가 어디야?",
          "speaker_id": "1",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008802.1.2",
          "form": "나 부산!!",
          "original_form": "나 부산!!",
          "speaker_id": "2",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008802.1.3",
          "form": "나 부산 사람인데ㅋㅋㅋㅋㅋ 부산 좋지!!",
          "original_form": "나 부산 사람인데ㅋㅋㅋㅋㅋ 부산 좋지!!",
          "speaker_id": "1",
          "time": "20210822 18:49"
        },
        {
          "id": "MDRW2100008802.1.4",
          "form": "바다 좋아해?",
          "original_form": "바다 좋아해?",
          "speaker_id": "1",
          "time": "20210822 18:49"
        },
        {
          "id": "MDRW2100008802.1.5",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "2",
          "time": "20210822 18:54"
        },
        {
          "id": "MDRW2100008802.1.6",
          "form": "응 바다 좋아해",
          "original_form": "응 바다 좋아해",
          "speaker_id": "2",
          "time": "20210822 18:54"
        },
        {
          "id": "MDRW2100008802.1.7",
          "form": "너는 매일 봐서 감흥 없지",
          "original_form": "너는 매일 봐서 감흥 없지",
          "speaker_id": "2",
          "time": "20210822 18:54"
        },
        {
          "id": "MDRW2100008802.1.8",
          "form": "다들 그러더라.. 매일 바다보러 가지는 않아",
          "original_form": "다들 그러더라.. 매일 바다보러 가지는 않아",
          "speaker_id": "1",
          "time": "20210822 18:55"
        },
        {
          "id": "MDRW2100008802.1.9",
          "form": "집이 해운대는 아니니까",
          "original_form": "집이 해운대는 아니니까",
          "speaker_id": "1",
          "time": "20210822 18:55"
        },
        {
          "id": "MDRW2100008802.1.10",
          "form": "나도 바다 볼때마다 신나..!",
          "original_form": "나도 바다 볼때마다 신나..!",
          "speaker_id": "1",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100008802.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇구나",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008802.1.12",
          "form": "나 거기 갔었어 오륙도",
          "original_form": "나 거기 갔었어 오륙도",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008802.1.13",
          "form": "오 나는 안 가봤는데",
          "original_form": "오 나는 안 가봤는데",
          "speaker_id": "1",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008802.1.14",
          "form": "어땠어? 좋았을 거 같아ㅋㅋㅋㅋ",
          "original_form": "어땠어? 좋았을 거 같아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:55"
        },
        {
          "id": "MDRW2100008802.1.15",
          "form": "좋았으 산책로 따라 걸었는데 진짜 나무들이 푸르더라고",
          "original_form": "좋았으 산책로 따라 걸었는데 진짜 나무들이 푸르더라고",
          "speaker_id": "2",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100008802.1.16",
          "form": "산책하는거 좋아하는데 가봐야겠다",
          "original_form": "산책하는거 좋아하는데 가봐야겠다",
          "speaker_id": "1",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100008802.1.17",
          "form": "좋은 생각이야",
          "original_form": "좋은 생각이야",
          "speaker_id": "2",
          "time": "20210824 20:30"
        },
        {
          "id": "MDRW2100008802.1.18",
          "form": "부산에서 기념품 사 본 적 있어?",
          "original_form": "부산에서 기념품 사 본 적 있어?",
          "speaker_id": "2",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100008802.1.19",
          "form": "음 어릴 때 아쿠아리움 갔을 때 사본 거 같아",
          "original_form": "음 어릴 때 아쿠아리움 갔을 때 사본 거 같아",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100008802.1.20",
          "form": "오오 나는 제주도 갔을 때ㅋㅋ",
          "original_form": "오오 나는 제주도 갔을 때ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100008802.1.21",
          "form": "제주도에서 뭐 샀어?",
          "original_form": "제주도에서 뭐 샀어?",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100008802.1.22",
          "form": "곰인형 샀었어 테디베어",
          "original_form": "곰인형 샀었어 테디베어",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100008802.1.23",
          "form": "아 제주도에 테디베어 박물관이 있었나?",
          "original_form": "아 제주도에 테디베어 박물관이 있었나?",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100008802.1.24",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100008802.1.25",
          "form": "엄청 어릴 때 가봤는데 기억이 잘 안 난다",
          "original_form": "엄청 어릴 때 가봤는데 기억이 잘 안 난다",
          "speaker_id": "1",
          "time": "20210825 22:17"
        }
      ]
    }
