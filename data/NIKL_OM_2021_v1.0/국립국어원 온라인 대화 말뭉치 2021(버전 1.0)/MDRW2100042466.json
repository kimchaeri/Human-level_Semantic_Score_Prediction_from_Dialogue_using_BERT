      "utterance": [
        {
          "id": "MDRW2100042466.1.1",
          "form": "name3한테 비가 너무 많이 와서",
          "original_form": "&name3&한테 비가 너무 많이 와서",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.2",
          "form": "장화 사줄까 했더니",
          "original_form": "장화 사줄까 했더니",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.3",
          "form": "얘기합니다",
          "original_form": "얘기합니다",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.4",
          "form": "그런 거 안 신는다고 하던데ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "그런 거 안 신는다고 하던데ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.6",
          "form": "아직도 치마",
          "original_form": "아직도 치마",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.7",
          "form": "남자애들은 안  신 떤데",
          "original_form": "남자애들은 안  신 떤데",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.8",
          "form": "얘기를 하나",
          "original_form": "얘기를 하나",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.9",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.10",
          "form": "딸래미 들은 아직도",
          "original_form": "딸래미 들은 아직도",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.11",
          "form": "괴롭네요",
          "original_form": "괴롭네요",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.12",
          "form": "학교에 긴 치마 입고 가면 안 되나??",
          "original_form": "학교에 긴 치마 입고 가면 안 되나??",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042466.1.13",
          "form": "잊고 뭐가는 거는 상관없는 거 같은데",
          "original_form": "잊고 뭐가는 거는 상관없는 거 같은데",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042466.1.14",
          "form": "선생님이 될 수 있으면 은 편한 옷이랑 운동화 신겨 보내라 하더라구요",
          "original_form": "선생님이 될 수 있으면 은 편한 옷이랑 운동화 신겨 보내라 하더라구요",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042466.1.15",
          "form": "날씨 좋으면 은 운동장에서 뛰어논다고",
          "original_form": "날씨 좋으면 은 운동장에서 뛰어논다고",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042466.1.16",
          "form": "보면 또 긴치마 입는 애들만 입어요",
          "original_form": "보면 또 긴치마 입는 애들만 입어요",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042466.1.17",
          "form": "아 운동장??",
          "original_form": "아 운동장??",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042466.1.18",
          "form": "우리는 운동장 딱 한 번 나갔던 거 같던데",
          "original_form": "우리는 운동장 딱 한 번 나갔던 거 같던데",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042466.1.19",
          "form": "우리는 운동장 자주 나가던데?",
          "original_form": "우리는 운동장 자주 나가던데?",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042466.1.20",
          "form": "그래 치.마도 입는 애들만 입.지",
          "original_form": "그래 치.마도 입는 애들만 입.지",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042466.1.21",
          "form": "부럽노",
          "original_form": "부럽노",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042466.1.22",
          "form": "뭐 물 총도 만 들어가 놀고",
          "original_form": "뭐 물 총도 만 들어가 놀고",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042466.1.23",
          "form": "운동장 도 자주 나가고",
          "original_form": "운동장 도 자주 나가고",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042466.1.24",
          "form": "오 좋네",
          "original_form": "오 좋네",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042466.1.25",
          "form": "물총이라니",
          "original_form": "물총이라니",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042466.1.26",
          "form": "그런 거 할 때는 사진 다 찍어 주거든요",
          "original_form": "그런 거 할 때는 사진 다 찍어 주거든요",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042466.1.27",
          "form": "우리선생님은 뭐",
          "original_form": "우리선생님은 뭐",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042466.1.28",
          "form": "그건 진짜 좋아요",
          "original_form": "그건 진짜 좋아요",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042466.1.29",
          "form": "달리기할때\r\n한번 나갔던데",
          "original_form": "달리기할때\r\n한번 나갔던데",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042466.1.30",
          "form": "오 사진까지",
          "original_form": "오 사진까지",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042466.1.31",
          "form": "사진도 찍어준다고??",
          "original_form": "사진도 찍어준다고??",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042466.1.32",
          "form": "너무 좋은데",
          "original_form": "너무 좋은데",
          "speaker_id": "2",
          "time": "20210906 22:22"
        }
      ]
