      "utterance": [
        {
          "id": "MDRW2100031741.1.1",
          "form": "정말 사랑합니다",
          "original_form": "정말 사랑합니다",
          "speaker_id": "1",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031741.1.2",
          "form": "집안일 중에 제일 싫은게 뭐예요",
          "original_form": "집안일 중에 제일 싫은게 뭐예요",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031741.1.3",
          "form": "저는 설거지를 바로바로 안 하는 편이에요",
          "original_form": "저는 설거지를 바로바로 안 하는 편이에요",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031741.1.4",
          "form": "저는 설거지만 하는 편이에요",
          "original_form": "저는 설거지만 하는 편이에요",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031741.1.5",
          "form": "제사 지내고 나서 하는 설거지는 헬리랍니다",
          "original_form": "제사 지내고 나서 하는 설거지는 헬리랍니다",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031741.1.6",
          "form": "화장실 청소가 제일 싫어요",
          "original_form": "화장실 청소가 제일 싫어요",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031741.1.7",
          "form": "맞아요 저도 제주도가 제일 제사를 해서",
          "original_form": "맞아요 저도 제주도가 제일 제사를 해서",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031741.1.8",
          "form": "아 맞죠 그거 인정입니다",
          "original_form": "아 맞죠 그거 인정입니다",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031741.1.9",
          "form": "그릇만 많이 나오죠",
          "original_form": "그릇만 많이 나오죠",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031741.1.10",
          "form": "그런데 저는 설거지를 미뤘다 한 번에 하는 편이에요",
          "original_form": "그런데 저는 설거지를 미뤘다 한 번에 하는 편이에요",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031741.1.11",
          "form": "제일 귀찮아요",
          "original_form": "제일 귀찮아요",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031741.1.12",
          "form": "설겆이 하루에 한 번만 해도 되긴 하죠",
          "original_form": "설겆이 하루에 한 번만 해도 되긴 하죠",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031741.1.13",
          "form": "1년에 제사 몇 번 하나요",
          "original_form": "1년에 제사 몇 번 하나요",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031741.1.14",
          "form": "그런데 싱크대가 작 아요",
          "original_form": "그런데 싱크대가 작 아요",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031741.1.15",
          "form": "두 번 있어요",
          "original_form": "두 번 있어요",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031741.1.16",
          "form": "싱크대 작으면 설거지할때 힘든데",
          "original_form": "싱크대 작으면 설거지할때 힘든데",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031741.1.17",
          "form": "엄마가 다 하시고 저는 전만 부쳐요",
          "original_form": "엄마가 다 하시고 저는 전만 부쳐요",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031741.1.18",
          "form": "설날 추석 때 빼고 두 번인가요 포함 두 번인가요",
          "original_form": "설날 추석 때 빼고 두 번인가요 포함 두 번인가요",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031741.1.19",
          "form": "이제 곧 추석이 다가오는데 또 전에 부치 겠네요",
          "original_form": "이제 곧 추석이 다가오는데 또 전에 부치 겠네요",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031741.1.20",
          "form": "그때는 차례를 지내지요",
          "original_form": "그때는 차례를 지내지요",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031741.1.21",
          "form": "추석에 부산에 있어요",
          "original_form": "추석에 부산에 있어요",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031741.1.22",
          "form": "전 사촌 동생들이 저보다 요리를 잘해서 사촌 동생들이 많이 하는 편이에요",
          "original_form": "전 사촌 동생들이 저보다 요리를 잘해서 사촌 동생들이 많이 하는 편이에요",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031741.1.23",
          "form": "굉장히 간소하게 해서 2시간이면 준비가 끝나요",
          "original_form": "굉장히 간소하게 해서 2시간이면 준비가 끝나요",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031741.1.24",
          "form": "별일 없으면 한번 보고 맛있는 거 먹으러 갑시다",
          "original_form": "별일 없으면 한번 보고 맛있는 거 먹으러 갑시다",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031741.1.25",
          "form": "전도 상에 올릴만큼만 하거든요",
          "original_form": "전도 상에 올릴만큼만 하거든요",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031741.1.26",
          "form": "예전에는 신경 써서 많이 음식을 했던 기억이 있는데 요즘에는 우리가 먹을 것 정도만 간편하게 하는 편이에요",
          "original_form": "예전에는 신경 써서 많이 음식을 했던 기억이 있는데 요즘에는 우리가 먹을 것 정도만 간편하게 하는 편이에요",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031741.1.27",
          "form": "네 간단하게 해도 괜찮더라구요",
          "original_form": "네 간단하게 해도 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031741.1.28",
          "form": "고모가 쥐포 튀김을 좋아하시거든요 쥐포튀김 드셔보셨나요 존맛탱입니다",
          "original_form": "고모가 쥐포 튀김을 좋아하시거든요 쥐포튀김 드셔보셨나요 존맛탱입니다",
          "speaker_id": "1",
          "time": "20210904 23:59"
        }
      ]
