      "utterance": [
        {
          "id": "MDRW2100027058.1.1",
          "form": "와인이 알코올향이 비교적 덜나지 않아요?",
          "original_form": "와인이 알코올향이 비교적 덜나지 않아요?",
          "speaker_id": "2",
          "time": "20210902 11:51"
        },
        {
          "id": "MDRW2100027058.1.2",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210902 15:24"
        },
        {
          "id": "MDRW2100027058.1.3",
          "form": "점심 뭐드셨나요?",
          "original_form": "점심 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210902 15:24"
        },
        {
          "id": "MDRW2100027058.1.4",
          "form": "저는 라면이요!",
          "original_form": "저는 라면이요!",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100027058.1.5",
          "form": "님은 점심 뭐드셨어요?",
          "original_form": "님은 점심 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100027058.1.6",
          "form": "베이글이랑 초코우유요 ㅎ",
          "original_form": "베이글이랑 초코우유요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100027058.1.7",
          "form": "초코우유 좋아해요?",
          "original_form": "초코우유 좋아해요?",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100027058.1.8",
          "form": "네! 정확히는 다크초코아이스 ㅋㅋㅋ 알바하는 데에서 음료 만들어 먹었어요",
          "original_form": "네! 정확히는 다크초코아이스 ㅋㅋㅋ 알바하는 데에서 음료 만들어 먹었어요",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100027058.1.9",
          "form": "흰우유는 안마셔요 ㅎ",
          "original_form": "흰우유는 안마셔요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100027058.1.10",
          "form": "흰우유 잘 드세요?",
          "original_form": "흰우유 잘 드세요?",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100027058.1.11",
          "form": "우와 언제나 초코는 옳아요",
          "original_form": "우와 언제나 초코는 옳아요",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100027058.1.12",
          "form": "저는 흰우유 좋아는 하는데",
          "original_form": "저는 흰우유 좋아는 하는데",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100027058.1.13",
          "form": "소화를 잘 못시켜서 힘드네요ㅠ",
          "original_form": "소화를 잘 못시켜서 힘드네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100027058.1.14",
          "form": "저두요ㅠ 라떼마시면 화장실 직행이에요 ㅋㅋㅋ",
          "original_form": "저두요ㅠ 라떼마시면 화장실 직행이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100027058.1.15",
          "form": "저도 라떼마시면 하루가 힘들어요ㅋㅋㅠ",
          "original_form": "저도 라떼마시면 하루가 힘들어요ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100027058.1.16",
          "form": "두유로 먹어보셨어요?",
          "original_form": "두유로 먹어보셨어요?",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100027058.1.17",
          "form": "네네! 두유 옵션되는 스벅 커피빈만 가요ㅎ",
          "original_form": "네네! 두유 옵션되는 스벅 커피빈만 가요ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100027058.1.18",
          "form": "오 두유로 먹으면 좀 나은가요??",
          "original_form": "오 두유로 먹으면 좀 나은가요??",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100027058.1.19",
          "form": "네 두유는 배 안아프더라구요",
          "original_form": "네 두유는 배 안아프더라구요",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100027058.1.20",
          "form": "근데 좀 비린맛이나서 시럽 꼭 넣어요",
          "original_form": "근데 좀 비린맛이나서 시럽 꼭 넣어요",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100027058.1.21",
          "form": "시럽넣으면 두유 특유의 냄새가 좀 덜 나나요?",
          "original_form": "시럽넣으면 두유 특유의 냄새가 좀 덜 나나요?",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100027058.1.22",
          "form": "네 바닐라라떼랑 비슷한 맛 나요 ㅎㅎ",
          "original_form": "네 바닐라라떼랑 비슷한 맛 나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:53"
        }
      ]
