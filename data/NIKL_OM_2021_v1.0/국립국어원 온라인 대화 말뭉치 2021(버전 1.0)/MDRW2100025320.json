        {
          "id": "MDRW2100025320.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210901 09:19"
        },
        {
          "id": "MDRW2100025320.1.2",
          "form": "아침은 걸렀어요!",
          "original_form": "아침은 걸렀어요!",
          "speaker_id": "2",
          "time": "20210901 09:26"
        },
        {
          "id": "MDRW2100025320.1.3",
          "form": "5분이라도 더 자고 싶은 마음에",
          "original_form": "5분이라도 더 자고 싶은 마음에",
          "speaker_id": "2",
          "time": "20210901 09:26"
        },
        {
          "id": "MDRW2100025320.1.4",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210901 09:26"
        },
        {
          "id": "MDRW2100025320.1.5",
          "form": "저도 시리얼",
          "original_form": "저도 시리얼",
          "speaker_id": "1",
          "time": "20210901 09:26"
        },
        {
          "id": "MDRW2100025320.1.6",
          "form": "오 시리얼도 맛있죠",
          "original_form": "오 시리얼도 맛있죠",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025320.1.7",
          "form": "저는 간식 뒤져보고있어요",
          "original_form": "저는 간식 뒤져보고있어요",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025320.1.8",
          "form": "밤만쥬 있네요",
          "original_form": "밤만쥬 있네요",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025320.1.9",
          "form": "오 맛나겠네녀",
          "original_form": "오 맛나겠네녀",
          "speaker_id": "1",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025320.1.10",
          "form": "그쵸 맛있네요",
          "original_form": "그쵸 맛있네요",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025320.1.11",
          "form": "당땡겼는데",
          "original_form": "당땡겼는데",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025320.1.12",
          "form": "점심은 뭐 드실건가요~?",
          "original_form": "점심은 뭐 드실건가요~?",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025320.1.13",
          "form": "버거요",
          "original_form": "버거요",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025320.1.14",
          "form": "헉 프랜차이즈인가요?",
          "original_form": "헉 프랜차이즈인가요?",
          "speaker_id": "2",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025320.1.15",
          "form": "맥도요",
          "original_form": "맥도요",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025320.1.16",
          "form": "맥도 뭐드세요",
          "original_form": "맥도 뭐드세요",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100025320.1.17",
          "form": "상하이요",
          "original_form": "상하이요",
          "speaker_id": "1",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025320.1.18",
          "form": "아 상하이 맛있죠 저는 1955 주로 먹는것같아요",
          "original_form": "아 상하이 맛있죠 저는 1955 주로 먹는것같아요",
