      "utterance": [
        {
          "id": "MDRW2100034154.1.1",
          "form": "남자 아이돌은 안좋아하시나요?",
          "original_form": "남자 아이돌은 안좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100034154.1.2",
          "form": "저는 사실 어렸을 때부터 엑소 좋아했어서요..ㅋㅋㅋㅋ",
          "original_form": "저는 사실 어렸을 때부터 엑소 좋아했어서요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100034154.1.3",
          "form": "헐 엑소 오랜만이네욬ㅋㅋ",
          "original_form": "헐 엑소 오랜만이네욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100034154.1.4",
          "form": "진짜 으르렁때 안좋아하는 애들이 없었는데 ㅋㅋ",
          "original_form": "진짜 으르렁때 안좋아하는 애들이 없었는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100034154.1.5",
          "form": "그쵸 ㅋㅋㅋㅋ 진짜 혁명이었는데 어느새 다 군대가버렸네요 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋ 진짜 혁명이었는데 어느새 다 군대가버렸네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100034154.1.6",
          "form": "그렇네요ㅋㅋ 갔다가 제대한 사람도 있고 ,,",
          "original_form": "그렇네요ㅋㅋ 갔다가 제대한 사람도 있고 ,,",
          "speaker_id": "1",
          "time": "20210827 18:43"
        },
        {
          "id": "MDRW2100034154.1.7",
          "form": "맞아요 시간이 넘 빨라요 ㅋㅋ",
          "original_form": "맞아요 시간이 넘 빨라요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100034154.1.8",
          "form": "누굴 젤 좋아하셨어요?",
          "original_form": "누굴 젤 좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100034154.1.9",
          "form": "좋아하시는 연예인 있으세요?",
          "original_form": "좋아하시는 연예인 있으세요?",
          "speaker_id": "2",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100034154.1.10",
          "form": "계속 바뀌었는데 지금은 수호요 ㅋㅋㅋ",
          "original_form": "계속 바뀌었는데 지금은 수호요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100034154.1.11",
          "form": "수호 진짜 스엠상의 표본이잖아욬ㅋㅋ",
          "original_form": "수호 진짜 스엠상의 표본이잖아욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100034154.1.12",
          "form": "맞아요 완전 상견례 프리패스상 ㅋㅋㅋㅋ",
          "original_form": "맞아요 완전 상견례 프리패스상 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100034154.1.13",
          "form": "맞아요 ㅋㅋ 약간 실물이 더 잘생겼을거같은",
          "original_form": "맞아요 ㅋㅋ 약간 실물이 더 잘생겼을거같은",
          "speaker_id": "1",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100034154.1.14",
          "form": "얼굴이에요 ㅎㅎ",
          "original_form": "얼굴이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100034154.1.15",
          "form": "맞아요 ㅋㅋㅋㅋ 빛이나게 생긴..",
          "original_form": "맞아요 ㅋㅋㅋㅋ 빛이나게 생긴..",
          "speaker_id": "2",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100034154.1.16",
          "form": "전 예전에 백현 좋아했었는데 ㅋㅋ",
          "original_form": "전 예전에 백현 좋아했었는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100034154.1.17",
          "form": "강아지상이여서요 ㅋㅋ",
          "original_form": "강아지상이여서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100034154.1.18",
          "form": "백현이 제일 인기 많죠 ㅋㅋㅋ",
