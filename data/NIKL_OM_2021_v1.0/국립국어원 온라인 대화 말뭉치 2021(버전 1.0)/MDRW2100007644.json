      "utterance": [
        {
          "id": "MDRW2100007644.1.1",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210822 08:58"
        },
        {
          "id": "MDRW2100007644.1.2",
          "form": "영양제 먹긴해요",
          "original_form": "영양제 먹긴해요",
          "speaker_id": "2",
          "time": "20210822 08:58"
        },
        {
          "id": "MDRW2100007644.1.3",
          "form": "근데 자꾸 까먹고 안먹어요",
          "original_form": "근데 자꾸 까먹고 안먹어요",
          "speaker_id": "2",
          "time": "20210822 08:58"
        },
        {
          "id": "MDRW2100007644.1.4",
          "form": "뭐 드세요?",
          "original_form": "뭐 드세요?",
          "speaker_id": "2",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007644.1.5",
          "form": "저도 영양제를 챙겨 먹지는 못해요ㅋㅋ",
          "original_form": "저도 영양제를 챙겨 먹지는 못해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007644.1.6",
          "form": "유산균은 아주 잘 챙겨먹습니다",
          "original_form": "유산균은 아주 잘 챙겨먹습니다",
          "speaker_id": "1",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007644.1.7",
          "form": "그렇죠 저도요",
          "original_form": "그렇죠 저도요",
          "speaker_id": "2",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007644.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007644.1.9",
          "form": "저도요 유산균 필수요",
          "original_form": "저도요 유산균 필수요",
          "speaker_id": "2",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007644.1.10",
          "form": "맛있게 나오더라구요 요새",
          "original_form": "맛있게 나오더라구요 요새",
          "speaker_id": "1",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007644.1.11",
          "form": "변비가 있어서요 ㅡㅡ",
          "original_form": "변비가 있어서요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007644.1.12",
          "form": "맞아요 새콤달콤해요",
          "original_form": "맞아요 새콤달콤해요",
          "speaker_id": "2",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007644.1.13",
          "form": "전 새콤한맛이 좋아요",
          "original_form": "전 새콤한맛이 좋아요",
          "speaker_id": "2",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007644.1.14",
          "form": "레모나 처럼요",
          "original_form": "레모나 처럼요",
          "speaker_id": "2",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007644.1.15",
          "form": "저도ㅋㅋㅋ어릴때ㅋㅋㅋ있었다가 먹으니까 확실히 좋이지던데요?",
          "original_form": "저도ㅋㅋㅋ어릴때ㅋㅋㅋ있었다가 먹으니까 확실히 좋이지던데요?",
          "speaker_id": "1",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007644.1.16",
          "form": "윽 전 달콤한 맛ㅋㅋㅋ :)",
          "original_form": "윽 전 달콤한 맛ㅋㅋㅋ :)",
          "speaker_id": "1",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007644.1.17",
          "form": "근데 많이 먹다가 살찌는거 아니야? 싶기도 하도라구요 종종",
          "original_form": "근데 많이 먹다가 살찌는거 아니야? 싶기도 하도라구요 종종",
          "speaker_id": "1",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007644.1.18",
          "form": "그렇죠 안먹음 안돼요 달콤도 좋아요",
          "original_form": "그렇죠 안먹음 안돼요 달콤도 좋아요",
          "speaker_id": "2",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007644.1.19",
          "form": "에이 설마요",
          "original_form": "에이 설마요",
          "speaker_id": "2",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007644.1.20",
          "form": "찔까요 ?ㅋㅋ",
          "original_form": "찔까요 ?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007644.1.21",
          "form": "그 다욧 유산균도 있던데요",
          "original_form": "그 다욧 유산균도 있던데요",
          "speaker_id": "2",
          "time": "20210822 09:01"
        },
        {
          "id": "MDRW2100007644.1.22",
          "form": "아 혹시ㅋㅋ 말 편히 하는건 어떠세요 :)",
          "original_form": "아 혹시ㅋㅋ 말 편히 하는건 어떠세요 :)",
          "speaker_id": "1",
          "time": "20210822 09:01"
        },
        {
          "id": "MDRW2100007644.1.23",
          "form": "아 그러세요 ㅎㅎ",
          "original_form": "아 그러세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 09:01"
        },
        {
          "id": "MDRW2100007644.1.24",
          "form": "아 저 말잘 못놓는데",
          "original_form": "아 저 말잘 못놓는데",
          "speaker_id": "2",
          "time": "20210822 09:02"
        },
        {
          "id": "MDRW2100007644.1.25",
          "form": "ㅋㅋㅋ할수 있어ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ할수 있어ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:02"
        },
        {
          "id": "MDRW2100007644.1.26",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210822 09:02"
        },
        {
          "id": "MDRW2100007644.1.27",
          "form": "초등학교때 연락끊긴 친구라고 치자ㅋㅋ",
          "original_form": "초등학교때 연락끊긴 친구라고 치자ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:02"
        },
        {
          "id": "MDRW2100007644.1.28",
          "form": "그러자",
          "original_form": "그러자",
          "speaker_id": "2",
          "time": "20210822 09:02"
        },
        {
          "id": "MDRW2100007644.1.29",
          "form": "존댓말은 길게 쳐야됔ㅋㅋㅋㅋ",
          "original_form": "존댓말은 길게 쳐야됔ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:02"
        },
        {
          "id": "MDRW2100007644.1.30",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210822 09:02"
        },
        {
          "id": "MDRW2100007644.1.31",
          "form": "다욧 유산균은ㅋㅋㅋ처음들어본다!",
          "original_form": "다욧 유산균은ㅋㅋㅋ처음들어본다!",
          "speaker_id": "1",
          "time": "20210822 09:02"
        },
        {
          "id": "MDRW2100007644.1.32",
          "form": "마스크 쓰면 트러블 안나?",
          "original_form": "마스크 쓰면 트러블 안나?",
          "speaker_id": "2",
          "time": "20210822 09:02"
        },
        {
          "id": "MDRW2100007644.1.33",
          "form": "완전 나지",
          "original_form": "완전 나지",
          "speaker_id": "1",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007644.1.34",
          "form": "있어 검색해봐",
          "original_form": "있어 검색해봐",
          "speaker_id": "2",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007644.1.35",
          "form": "나 건성이라 얼굴에 뭐 안나는 편인데ㅋㅋㅋ",
          "original_form": "나 건성이라 얼굴에 뭐 안나는 편인데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007644.1.36",
          "form": "너도 뾰루지 ㅜㅜ",
          "original_form": "너도 뾰루지 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007644.1.37",
          "form": "요새 들어서 뾰루지들이 인사해ㅜㅜ",
          "original_form": "요새 들어서 뾰루지들이 인사해ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007644.1.38",
          "form": "여드름도 안났었는데 뾰루지가",
          "original_form": "여드름도 안났었는데 뾰루지가",
          "speaker_id": "2",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007644.1.39",
          "form": "야 너두?",
          "original_form": "야 너두?",
          "speaker_id": "1",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007644.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007644.1.41",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007644.1.42",
          "form": "슬프네",
          "original_form": "슬프네",
          "speaker_id": "2",
          "time": "20210822 09:03"
        },
        {
          "id": "MDRW2100007644.1.43",
          "form": "되게 비슷하네ㅋㅋㅋ나랑 식습관이랑 생활 패턴도 비슷하려나",
          "original_form": "되게 비슷하네ㅋㅋㅋ나랑 식습관이랑 생활 패턴도 비슷하려나",
          "speaker_id": "1",
          "time": "20210822 09:04"
        },
        {
          "id": "MDRW2100007644.1.44",
          "form": "오차피 마스크땜시 관리도 안돼",
          "original_form": "오차피 마스크땜시 관리도 안돼",
          "speaker_id": "2",
          "time": "20210822 09:04"
        },
        {
          "id": "MDRW2100007644.1.45",
          "form": "마자ㅋㅋㅋ화장은 좀 덜해서ㅋㅋ그건 피부에 좋을듯?",
          "original_form": "마자ㅋㅋㅋ화장은 좀 덜해서ㅋㅋ그건 피부에 좋을듯?",
          "speaker_id": "1",
          "time": "20210822 09:04"
        },
        {
          "id": "MDRW2100007644.1.46",
          "form": "글쎄 그러려나",
          "original_form": "글쎄 그러려나",
          "speaker_id": "2",
          "time": "20210822 09:04"
        },
        {
          "id": "MDRW2100007644.1.47",
          "form": "그렇긴해",
          "original_form": "그렇긴해",
          "speaker_id": "2",
          "time": "20210822 09:04"
        },
        {
          "id": "MDRW2100007644.1.48",
          "form": "화장품도 화학약물이니까 아무리 순해도",
          "original_form": "화장품도 화학약물이니까 아무리 순해도",
          "speaker_id": "1",
