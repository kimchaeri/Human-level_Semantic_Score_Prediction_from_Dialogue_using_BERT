        },
        {
          "id": "MDRW2100013138.1.3",
          "form": "맛집 좋아하세요?",
          "original_form": "맛집 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100013138.1.4",
          "form": "정말 좋아해요",
          "original_form": "정말 좋아해요",
          "speaker_id": "2",
          "time": "20210826 08:11"
        },
        {
          "id": "MDRW2100013138.1.5",
          "form": "어떤메뉴요?",
          "original_form": "어떤메뉴요?",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100013138.1.6",
          "form": "파스타 피자요",
          "original_form": "파스타 피자요",
          "speaker_id": "2",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100013138.1.7",
          "form": "오 너무좋아요둘다",
          "original_form": "오 너무좋아요둘다",
          "speaker_id": "1",
          "time": "20210826 10:43"
        },
        {
          "id": "MDRW2100013138.1.8",
          "form": "혹시 로제떡볶이 드셔보셨나요",
          "original_form": "혹시 로제떡볶이 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210826 10:43"
        },
        {
          "id": "MDRW2100013138.1.9",
          "form": "네 엽떡로제요",
          "original_form": "네 엽떡로제요",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100013138.1.10",
          "form": "매운거 좋아하시는군요?",
          "original_form": "매운거 좋아하시는군요?",
          "speaker_id": "2",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100013138.1.11",
          "form": "안느끼하고 맛있더라구요",
          "original_form": "안느끼하고 맛있더라구요",
          "speaker_id": "1",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100013138.1.12",
          "form": "맛있지용 ㅎ 요즘 로제에 빠졌어요",
          "original_form": "맛있지용 ㅎ 요즘 로제에 빠졌어요",
          "speaker_id": "2",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100013138.1.13",
          "form": "배떡 드셔보셨나요",
          "original_form": "배떡 드셔보셨나요",
          "speaker_id": "1",
