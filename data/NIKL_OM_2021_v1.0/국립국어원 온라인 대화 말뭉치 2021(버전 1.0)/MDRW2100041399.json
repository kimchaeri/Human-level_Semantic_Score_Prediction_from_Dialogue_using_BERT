      "utterance": [
        {
          "id": "MDRW2100041399.1.1",
          "form": "완전 맛있었겠다 있어요 샐러드 편하죠",
          "original_form": "완전 맛있었겠다 있어요 샐러드 편하죠",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041399.1.2",
          "form": "이주에 한 번꼴로 가면 그래도 어느 정도 할인 품목은 사서 오는 편이에요",
          "original_form": "이주에 한 번꼴로 가면 그래도 어느 정도 할인 품목은 사서 오는 편이에요",
          "speaker_id": "1",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041399.1.3",
          "form": "그래서 다른  편의점 음식은 잘 모르는데",
          "original_form": "그래서 다른  편의점 음식은 잘 모르는데",
          "speaker_id": "1",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041399.1.4",
          "form": "2 + 1이 나 1 + 1 할인행사를 자주 이용 하세요",
          "original_form": "2 + 1이 나 1 + 1 할인행사를 자주 이용 하세요",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041399.1.5",
          "form": "이마트 24 편의점에 파는 물품은 물품은 잘 알아요 ㅋㅋㅋㅋ",
          "original_form": "이마트 24 편의점에 파는 물품은 물품은 잘 알아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041399.1.6",
          "form": "완전 할인 행사 위주로 삽니다",
          "original_form": "완전 할인 행사 위주로 삽니다",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041399.1.7",
          "form": "이마트 24 커피도 맛있게 괜찮아요",
          "original_form": "이마트 24 커피도 맛있게 괜찮아요",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041399.1.8",
          "form": "투플 원은 사면 하나만 가져오고 두 개는 주고 주고 옵니다 유유",
          "original_form": "투플 원은 사면 하나만 가져오고 두 개는 주고 주고 옵니다 유유",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041399.1.9",
          "form": "GS25 커피도 굉장히 맛있고요 거기는 커피 머신 비싸다고 들었거든요",
          "original_form": "GS25 커피도 굉장히 맛있고요 거기는 커피 머신 비싸다고 들었거든요",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041399.1.10",
          "form": "뭔가 슬픈",
          "original_form": "뭔가 슬픈",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041399.1.11",
          "form": "맞아요 이마트 24 커피 맛있어요 일단 신선해요",
          "original_form": "맞아요 이마트 24 커피 맛있어요 일단 신선해요",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041399.1.12",
          "form": "이야기네요",
          "original_form": "이야기네요",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041399.1.13",
          "form": "오늘은 젤리를 엄청 샀어요",
          "original_form": "오늘은 젤리를 엄청 샀어요",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041399.1.14",
          "form": "혹시 미니스톱에서 파는 기다란 콘 아이스크림 드셔보셨나요",
          "original_form": "혹시 미니스톱에서 파는 기다란 콘 아이스크림 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041399.1.15",
          "form": "미니스톱은 아이스크림 맛집입니다",
          "original_form": "미니스톱은 아이스크림 맛집입니다",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041399.1.16",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041399.1.17",
          "form": "무슨 젤리를 사셨나요",
          "original_form": "무슨 젤리를 사셨나요",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041399.1.18",
          "form": "일하는 곳 근처에 미니스톱 있는데",
          "original_form": "일하는 곳 근처에 미니스톱 있는데",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041399.1.19",
          "form": "이름도 모르는 젤리에요",
          "original_form": "이름도 모르는 젤리에요",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041399.1.20",
          "form": "곰돌이 모양인가요",
          "original_form": "곰돌이 모양인가요",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041399.1.21",
          "form": "과일 맛인가요 콜라 맛인가요",
          "original_form": "과일 맛인가요 콜라 맛인가요",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041399.1.22",
          "form": "하 리본은 아닌 거 같구요 막막 머시 밀로 우처럼",
          "original_form": "하 리본은 아닌 거 같구요 막막 머시 밀로 우처럼",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041399.1.23",
          "form": "큼직큼직하고 막 그랬어요",
          "original_form": "큼직큼직하고 막 그랬어요",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041399.1.24",
          "form": "젤리 종류가 진짜 너무 많더라구요",
          "original_form": "젤리 종류가 진짜 너무 많더라구요",
          "speaker_id": "1",
          "time": "20210905 23:04"
        }
      ]
    }
