      "utterance": [
        {
          "id": "MDRW2100038791.1.1",
          "form": "ㅋㅋ저두요 슬퍼요",
          "original_form": "ㅋㅋ저두요 슬퍼요",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100038791.1.2",
          "form": "ㅜㅜ 이사해야하는데..",
          "original_form": "ㅜㅜ 이사해야하는데..",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038791.1.3",
          "form": "월급이 줄어서 너무 슬퍼요ㅠㅠ",
          "original_form": "월급이 줄어서 너무 슬퍼요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038791.1.4",
          "form": "왜요 멋진곳인데",
          "original_form": "왜요 멋진곳인데",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038791.1.5",
          "form": "휴양지 사는데ㅜ월급준건 슬퍼요",
          "original_form": "휴양지 사는데ㅜ월급준건 슬퍼요",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038791.1.6",
          "form": "집이 오래되어서 고칠곳이 너무 많아요ㅜㅜ",
          "original_form": "집이 오래되어서 고칠곳이 너무 많아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038791.1.7",
          "form": "name1님집도 비새나요?",
          "original_form": "&name1&님집도 비새나요?",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100038791.1.8",
          "form": "그럴땐 막걸리착 걸치면 기분좋아져요",
          "original_form": "그럴땐 막걸리착 걸치면 기분좋아져요",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038791.1.9",
          "form": "아뇨.",
          "original_form": "아뇨.",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100038791.1.10",
          "form": "주인이 아직 안온건가요?",
          "original_form": "주인이 아직 안온건가요?",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100038791.1.11",
          "form": "지금가는길이에요",
          "original_form": "지금가는길이에요",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100038791.1.12",
          "form": "지금 왓는디 비가와야 알거같아유",
          "original_form": "지금 왓는디 비가와야 알거같아유",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100038791.1.13",
          "form": "아....  .",
          "original_form": "아....  .",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038791.1.14",
          "form": "수리해주긴 해야할텐데..",
          "original_form": "수리해주긴 해야할텐데..",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038791.1.15",
          "form": "해주셨겠징",
          "original_form": "해주셨겠징",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038791.1.16",
          "form": "name1님 머좀 더 드셨슈?",
          "original_form": "&name1&님 머좀 더 드셨슈?",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038791.1.17",
          "form": "나는 쥐포묵고 있지롱",
          "original_form": "나는 쥐포묵고 있지롱",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038791.1.18",
          "form": "고구마ㅜㅜ",
          "original_form": "고구마ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100038791.1.19",
          "form": "일찍자려고ㅜㅜ",
          "original_form": "일찍자려고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100038791.1.20",
          "form": "배고프다",
          "original_form": "배고프다",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100038791.1.21",
          "form": "쥐포를 몇 개나 먹고 있는 건가요",
          "original_form": "쥐포를 몇 개나 먹고 있는 건가요",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100038791.1.22",
          "form": "혼자 다 먹을 건가요",
          "original_form": "혼자 다 먹을 건가요",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100038791.1.23",
          "form": "소스는 찍어 먹고 있나요",
          "original_form": "소스는 찍어 먹고 있나요",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100038791.1.24",
          "form": "양념된거야요",
          "original_form": "양념된거야요",
          "speaker_id": "2",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100038791.1.25",
          "form": "룸메하고 마넌치  다묵어부럿소",
          "original_form": "룸메하고 마넌치  다묵어부럿소",
          "speaker_id": "2",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100038791.1.26",
          "form": "안이랬는데 밤마다 자꾸 먹어서 배나왔슈",
          "original_form": "안이랬는데 밤마다 자꾸 먹어서 배나왔슈",
          "speaker_id": "2",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100038791.1.27",
          "form": "인간적이겠네요ㅋㅋㅋ",
          "original_form": "인간적이겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100038791.1.28",
          "form": "인격이니 소중히 다뤄주세요",
          "original_form": "인격이니 소중히 다뤄주세요",
          "speaker_id": "1",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100038791.1.29",
          "form": "쥐포보다는 아귀포가 맛있는데",
          "original_form": "쥐포보다는 아귀포가 맛있는데",
          "speaker_id": "1",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100038791.1.30",
          "form": "아귀포 보이문 또 사무야겠네이",
          "original_form": "아귀포 보이문 또 사무야겠네이",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100038791.1.31",
          "form": "내가 또 한인간하제",
          "original_form": "내가 또 한인간하제",
          "speaker_id": "2",
          "time": "20210903 10:42"
        }
      ]
