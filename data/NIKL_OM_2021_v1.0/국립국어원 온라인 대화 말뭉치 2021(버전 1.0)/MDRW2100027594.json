      "utterance": [
        {
          "id": "MDRW2100027594.1.1",
          "form": "안됐다",
          "original_form": "안됐다",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027594.1.2",
          "form": "세제 직접 사오시는 편인가요",
          "original_form": "세제 직접 사오시는 편인가요",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027594.1.3",
          "form": "아뇨 어머니가 사오심",
          "original_form": "아뇨 어머니가 사오심",
          "speaker_id": "1",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027594.1.4",
          "form": "우리도",
          "original_form": "우리도",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027594.1.5",
          "form": "세제 없다더니 설거지는 어떻게 잘 끝내셨는지",
          "original_form": "세제 없다더니 설거지는 어떻게 잘 끝내셨는지",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027594.1.6",
          "form": "싱크대 밑에 봤더니 리필 있어서",
          "original_form": "싱크대 밑에 봤더니 리필 있어서",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027594.1.7",
          "form": "다행히 잘 끝냈습니다",
          "original_form": "다행히 잘 끝냈습니다",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027594.1.8",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027594.1.9",
          "form": "우리 집도 설거지 쌓여있는데",
          "original_form": "우리 집도 설거지 쌓여있는데",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027594.1.10",
          "form": "...언제 하지",
          "original_form": "...언제 하지",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027594.1.11",
          "form": "...지금?",
          "original_form": "...지금?",
          "speaker_id": "1",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027594.1.12",
          "form": "2시부터 수업임(ㅋㅋ)",
          "original_form": "2시부터 수업임(ㅋㅋ)",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027594.1.13",
          "form": "끝나고 해야지",
          "original_form": "끝나고 해야지",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027594.1.14",
          "form": "저런...",
          "original_form": "저런...",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027594.1.15",
          "form": "난 이제 설거지 다 해서 커피 마실 건데",
          "original_form": "난 이제 설거지 다 해서 커피 마실 건데",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027594.1.16",
          "form": "글쿤... 우리 집은 커피 잘 안 마셔서",
          "original_form": "글쿤... 우리 집은 커피 잘 안 마셔서",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027594.1.17",
          "form": "오히려 액상과당을 먹는 편",
          "original_form": "오히려 액상과당을 먹는 편",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027594.1.18",
          "form": "액상과당 어떤 거",
          "original_form": "액상과당 어떤 거",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027594.1.19",
          "form": "요즘은 포도맛 웰치스 쌓아두고 먹음",
          "original_form": "요즘은 포도맛 웰치스 쌓아두고 먹음",
          "speaker_id": "2",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100027594.1.20",
          "form": "맛있겠다...",
          "original_form": "맛있겠다...",
          "speaker_id": "1",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100027594.1.21",
          "form": "웰치스 최애맛이 포도맛인가요",
          "original_form": "웰치스 최애맛이 포도맛인가요",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027594.1.22",
          "form": "넹 포도맛 짱.",
          "original_form": "넹 포도맛 짱.",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027594.1.23",
          "form": "코스트코 가면 한 박스씩 사오는 듯",
          "original_form": "코스트코 가면 한 박스씩 사오는 듯",
          "speaker_id": "2",
          "time": "20210902 14:12"
        }
      ]
