      "utterance": [
        {
          "id": "MDRW2100022342.1.1",
          "form": "아웃백은 식전빵 먹으러 가고싶네요",
          "original_form": "아웃백은 식전빵 먹으러 가고싶네요",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022342.1.2",
          "form": "저도 콩고기는 별로더라구요.. 대체식품으로 많이 언급되는데.. 음..",
          "original_form": "저도 콩고기는 별로더라구요.. 대체식품으로 많이 언급되는데.. 음..",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022342.1.3",
          "form": "콩고기 저 학교다닐 적에 많이 먹었어요",
          "original_form": "콩고기 저 학교다닐 적에 많이 먹었어요",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022342.1.4",
          "form": "식전빵 좋아하시나요",
          "original_form": "식전빵 좋아하시나요",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022342.1.5",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022342.1.6",
          "form": "급십에 많이 나왔나",
          "original_form": "급십에 많이 나왔나",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022342.1.7",
          "form": "ㅋㅋㅋ네네 학교가 채식주의를 좋아해서",
          "original_form": "ㅋㅋㅋ네네 학교가 채식주의를 좋아해서",
          "speaker_id": "2",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022342.1.8",
          "form": "급식실에 콩고기 많이 내놨는데 맛있었어요",
          "original_form": "급식실에 콩고기 많이 내놨는데 맛있었어요",
          "speaker_id": "2",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022342.1.9",
          "form": "아.. 먹을거 업었겠다",
          "original_form": "아.. 먹을거 업었겠다",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022342.1.10",
          "form": "아니구나",
          "original_form": "아니구나",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022342.1.11",
          "form": "의외로 식단 맛있게 잘 짜시더라구요",
          "original_form": "의외로 식단 맛있게 잘 짜시더라구요",
          "speaker_id": "2",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022342.1.12",
          "form": "맛있었구나",
          "original_form": "맛있었구나",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022342.1.13",
          "form": "비빔밥도 전주비빔밥보다 훨씬 맛있었어요",
          "original_form": "비빔밥도 전주비빔밥보다 훨씬 맛있었어요",
          "speaker_id": "2",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022342.1.14",
          "form": "그래서 되게 독특한 메뉴 많이 먹었네요",
          "original_form": "그래서 되게 독특한 메뉴 많이 먹었네요",
          "speaker_id": "2",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022342.1.15",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022342.1.16",
          "form": "근데 그때 친구들이 다 싫어했어요",
          "original_form": "근데 그때 친구들이 다 싫어했어요",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022342.1.17",
          "form": "육류가 없다고...ㅋㅋ",
          "original_form": "육류가 없다고...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022342.1.18",
          "form": "ㅋㅋㅋ 육류없는건 그렇죠",
          "original_form": "ㅋㅋㅋ 육류없는건 그렇죠",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022342.1.19",
          "form": "어떤거 기억에 남나요",
          "original_form": "어떤거 기억에 남나요",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022342.1.20",
          "form": "저는 왜 그런거 먹고도 살쪘죠 ㅠㅠ",
          "original_form": "저는 왜 그런거 먹고도 살쪘죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022342.1.21",
          "form": "콩튀김 같은 게 기억에 남아요",
          "original_form": "콩튀김 같은 게 기억에 남아요",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022342.1.22",
          "form": "입에 맞으셨나보다",
          "original_form": "입에 맞으셨나보다",
          "speaker_id": "1",
          "time": "20210829 12:51"
        }
      ]
