      "utterance": [
        {
          "id": "MDRW2100014305.1.1",
          "form": "좋아하는 배달음식 있으신가용",
          "original_form": "좋아하는 배달음식 있으신가용",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014305.1.2",
          "form": "요즘은 마라탕이요",
          "original_form": "요즘은 마라탕이요",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014305.1.3",
          "form": "저도 마라탕 좋아해요 ㅎㅎ !",
          "original_form": "저도 마라탕 좋아해요 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014305.1.4",
          "form": "중국당면 넣어서 드시나용",
          "original_form": "중국당면 넣어서 드시나용",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014305.1.5",
          "form": "중국당면돠 숙주, 고기는 기본으로 넣어요ㅎㅎ",
          "original_form": "중국당면돠 숙주, 고기는 기본으로 넣어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014305.1.6",
          "form": "ㅎㅎ 아 맞아요 전 거기 건두부까지 기본으로 꼭 넣어요",
          "original_form": "ㅎㅎ 아 맞아요 전 거기 건두부까지 기본으로 꼭 넣어요",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014305.1.7",
          "form": "매운맛으로 하시나요?",
          "original_form": "매운맛으로 하시나요?",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014305.1.8",
          "form": "저는 순한맛이요...ㅎㅎ",
          "original_form": "저는 순한맛이요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014305.1.9",
          "form": "아아 순한맛은 안먹어봤는데",
          "original_form": "아아 순한맛은 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014305.1.10",
          "form": "좀 덜 빨간가요???",
          "original_form": "좀 덜 빨간가요???",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014305.1.11",
          "form": "빨갛기는 비슷한데 맛이 좀 덜 얼얼해요",
          "original_form": "빨갛기는 비슷한데 맛이 좀 덜 얼얼해요",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014305.1.12",
          "form": "아아 그 마라가 좀 덜 들어가나봐요",
          "original_form": "아아 그 마라가 좀 덜 들어가나봐요",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014305.1.13",
          "form": "그 마라 알갱이? 그거 싫어하는데 ㅠㅜ",
          "original_form": "그 마라 알갱이? 그거 싫어하는데 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014305.1.14",
          "form": "씹으면...힘들어지는 알갱이ㅋㅋㅋㅋ국물도 조심히 마셔야하죠ㅋㅋ",
          "original_form": "씹으면...힘들어지는 알갱이ㅋㅋㅋㅋ국물도 조심히 마셔야하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014305.1.15",
          "form": "맞아요 ㅋㅋ 집근처에 마라탕 맛집이 있는데",
          "original_form": "맞아요 ㅋㅋ 집근처에 마라탕 맛집이 있는데",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014305.1.16",
          "form": "그 알갱이가 너무 많아서 못가요 ㅠ",
          "original_form": "그 알갱이가 너무 많아서 못가요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014305.1.17",
          "form": "저는 그나마 맛이 순한 라공방 많이 가요! 거기는 좀 괜찮더라구요",
          "original_form": "저는 그나마 맛이 순한 라공방 많이 가요! 거기는 좀 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014305.1.18",
          "form": "오오 주변에 있나 찾아봐야겠어요 !",
          "original_form": "오오 주변에 있나 찾아봐야겠어요 !",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014305.1.19",
          "form": "배달도 되죠 ??",
          "original_form": "배달도 되죠 ??",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014305.1.20",
          "form": "배달도 되는 걸로 알아요!",
          "original_form": "배달도 되는 걸로 알아요!",
          "speaker_id": "2",
          "time": "20210825 19:21"
        }
      ]
