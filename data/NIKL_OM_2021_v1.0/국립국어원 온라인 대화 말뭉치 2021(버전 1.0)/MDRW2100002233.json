      "utterance": [
        {
          "id": "MDRW2100002233.1.1",
          "form": "나 요즘 예전 노래를 들어",
          "original_form": "나 요즘 예전 노래를 들어",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002233.1.2",
          "form": "샤이니의 초록비 들어봤니",
          "original_form": "샤이니의 초록비 들어봤니",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002233.1.3",
          "form": "아주 우리의 학창시절을 생각나게 하는 노래란다.",
          "original_form": "아주 우리의 학창시절을 생각나게 하는 노래란다.",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002233.1.4",
          "form": "오 아니 첨들어보는 제목인데?",
          "original_form": "오 아니 첨들어보는 제목인데?",
          "speaker_id": "1",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002233.1.5",
          "form": "내가 노래추천을 할 것이ㅣ라는 걸 어떻ㄱ게 알았니",
          "original_form": "내가 노래추천을 할 것이ㅣ라는 걸 어떻ㄱ게 알았니",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.6",
          "form": "나도 어제 처음 들어봤어",
          "original_form": "나도 어제 처음 들어봤어",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.7",
          "form": "여왕의교실이라는 드라마 오에스티더라.",
          "original_form": "여왕의교실이라는 드라마 오에스티더라.",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.8",
          "form": "가사가 멋져. 한번 들어봐,",
          "original_form": "가사가 멋져. 한번 들어봐,",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.9",
          "form": "아 오에스티구나",
          "original_form": "아 오에스티구나",
          "speaker_id": "1",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.10",
          "form": "알겠어 음악 목록에 넣어둬야지",
          "original_form": "알겠어 음악 목록에 넣어둬야지",
          "speaker_id": "1",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.11",
          "form": "뭔가 미래를 향해 나아가는 노래란다.",
          "original_form": "뭔가 미래를 향해 나아가는 노래란다.",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.12",
          "form": "나는 요즘 영화음악을 들어",
          "original_form": "나는 요즘 영화음악을 들어",
          "speaker_id": "1",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.13",
          "form": "나는 오늘 그걸 무한반복했어.",
          "original_form": "나는 오늘 그걸 무한반복했어.",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.14",
          "form": "어떤 노래를 듣니",
          "original_form": "어떤 노래를 듣니",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아주 빠졌나봐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아주 빠졌나봐",
          "speaker_id": "1",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.16",
          "form": "위대한 쇼맨 ost가 아주좋아",
          "original_form": "위대한 쇼맨 ost가 아주좋아",
          "speaker_id": "1",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.17",
          "form": "아주 신나.",
          "original_form": "아주 신나.",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.18",
          "form": "내용은 별론데 음악이 좋아",
          "original_form": "내용은 별론데 음악이 좋아",
          "speaker_id": "1",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002233.1.19",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210820 01:01"
        },
        {
          "id": "MDRW2100002233.1.20",
          "form": "너도 신나고싶구나",
          "original_form": "너도 신나고싶구나",
          "speaker_id": "2",
          "time": "20210820 01:01"
        },
        {
          "id": "MDRW2100002233.1.21",
          "form": "아무래도 신나는게 좋지",
          "original_form": "아무래도 신나는게 좋지",
          "speaker_id": "1",
          "time": "20210820 01:01"
        },
        {
          "id": "MDRW2100002233.1.22",
          "form": "그리고 노래방도 너무 가고싶어",
          "original_form": "그리고 노래방도 너무 가고싶어",
          "speaker_id": "1",
          "time": "20210820 01:01"
        },
        {
          "id": "MDRW2100002233.1.23",
          "form": "니가 하나 차리는건 어때",
          "original_form": "니가 하나 차리는건 어때",
          "speaker_id": "1",
          "time": "20210820 01:01"
        },
        {
          "id": "MDRW2100002233.1.24",
          "form": "나도... 그건 아니",
          "original_form": "나도... 그건 아니",
          "speaker_id": "2",
          "time": "20210820 01:01"
        }
      ]
    }
