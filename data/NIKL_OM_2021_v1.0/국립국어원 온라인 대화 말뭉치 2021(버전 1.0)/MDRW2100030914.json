      "utterance": [
        {
          "id": "MDRW2100030914.1.1",
          "form": "군대가보셧나요",
          "original_form": "군대가보셧나요",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100030914.1.2",
          "form": "넹! 예비역이여요",
          "original_form": "넹! 예비역이여요",
          "speaker_id": "2",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100030914.1.3",
          "form": "어땟어요 군대",
          "original_form": "어땟어요 군대",
          "speaker_id": "1",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030914.1.4",
          "form": "어떤병",
          "original_form": "어떤병",
          "speaker_id": "1",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030914.1.5",
          "form": "육군이였구",
          "original_form": "육군이였구",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030914.1.6",
          "form": "방공병과 였어요",
          "original_form": "방공병과 였어요",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030914.1.7",
          "form": "방공병과에서도 작전병이였고",
          "original_form": "방공병과에서도 작전병이였고",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030914.1.8",
          "form": "헉 방공병 ?",
          "original_form": "헉 방공병 ?",
          "speaker_id": "1",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030914.1.9",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030914.1.10",
          "form": "방공병이머죠",
          "original_form": "방공병이머죠",
          "speaker_id": "1",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030914.1.11",
          "form": "그... 육군에서 하늘 막는 역할이라구 생각하심되어요",
          "original_form": "그... 육군에서 하늘 막는 역할이라구 생각하심되어요",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030914.1.12",
          "form": "name1님은 군 경험이 있으신가욤?",
          "original_form": "&name1&님은 군 경험이 있으신가욤?",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030914.1.13",
          "form": "대박 포 쏘나요",
          "original_form": "대박 포 쏘나요",
          "speaker_id": "1",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030914.1.14",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "1",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030914.1.15",
          "form": "맞아요. 그 미사일같은것도 있고, 발칸이라고 해서 두루루루 쏘는거 있어요",
          "original_form": "맞아요. 그 미사일같은것도 있고, 발칸이라고 해서 두루루루 쏘는거 있어요",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030914.1.16",
          "form": "발칸 이라고 검색해보심 될듯",
          "original_form": "발칸 이라고 검색해보심 될듯",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030914.1.17",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "1",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030914.1.18",
          "form": "군대는 그냥 저는 작전병이라서 막 훈련을 엄청 많이 받고 그런게 아니였어서",
          "original_form": "군대는 그냥 저는 작전병이라서 막 훈련을 엄청 많이 받고 그런게 아니였어서",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030914.1.19",
          "form": "작전병은 머해요",
          "original_form": "작전병은 머해요",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030914.1.20",
          "form": "진짜 그냥 회사생활처럼 한것 같아요 ㅋㅋㅋㅋ ㅠㅠㅠ",
          "original_form": "진짜 그냥 회사생활처럼 한것 같아요 ㅋㅋㅋㅋ ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030914.1.21",
          "form": "브레인?",
          "original_form": "브레인?",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030914.1.22",
          "form": "그.....",
          "original_form": "그.....",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030914.1.23",
          "form": "전시업무랑 평시업무가 있는데",
          "original_form": "전시업무랑 평시업무가 있는데",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030914.1.24",
          "form": "지금은 많이 바뀌긴 했는데",
          "original_form": "지금은 많이 바뀌긴 했는데",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030914.1.25",
          "form": "적 비행기가 나타났으면, 그 좌표를 지도에 그려주는 역할을 해요",
          "original_form": "적 비행기가 나타났으면, 그 좌표를 지도에 그려주는 역할을 해요",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030914.1.26",
          "form": "손으로 직접 그려요 ㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "손으로 직접 그려요 ㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030914.1.27",
          "form": "요즘은 인공위성으로 다 추적해서 모니터에 표시해주더라구욤",
          "original_form": "요즘은 인공위성으로 다 추적해서 모니터에 표시해주더라구욤",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030914.1.28",
          "form": "와 꿀잼이네요",
          "original_form": "와 꿀잼이네요",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030914.1.29",
          "form": "어케선발대셧죠",
          "original_form": "어케선발대셧죠",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030914.1.30",
          "form": "앗 논산에서 차출되어서 갔어요",
          "original_form": "앗 논산에서 차출되어서 갔어요",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030914.1.31",
          "form": "랜덤이여요!",
          "original_form": "랜덤이여요!",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030914.1.32",
          "form": "운인가요",
          "original_form": "운인가요",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030914.1.33",
          "form": "운이죠! 순전히 운",
          "original_form": "운이죠! 순전히 운",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030914.1.34",
          "form": "오 럭키가이",
          "original_form": "오 럭키가이",
          "speaker_id": "1",
          "time": "20210904 14:19"
        }
      ]
    }
