      "utterance": [
        {
          "id": "MDRW2100004552.1.1",
          "form": "기기 호환은 기본적으로 다 해줘야 하는거 아니냐구...",
          "original_form": "기기 호환은 기본적으로 다 해줘야 하는거 아니냐구...",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004552.1.2",
          "form": "들여보내라!!!!",
          "original_form": "들여보내라!!!!",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004552.1.3",
          "form": "맞아 매일매일 주고 있어",
          "original_form": "맞아 매일매일 주고 있어",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004552.1.4",
          "form": "이벤트 아니면 안 돌아갔을지도 몰라",
          "original_form": "이벤트 아니면 안 돌아갔을지도 몰라",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004552.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004552.1.6",
          "form": "지금도 줌?",
          "original_form": "지금도 줌?",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004552.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004552.1.8",
          "form": "매일 하나씩 주는거라",
          "original_form": "매일 하나씩 주는거라",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004552.1.9",
          "form": "뭐 나온거 있니",
          "original_form": "뭐 나온거 있니",
          "speaker_id": "1",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004552.1.10",
          "form": "엔 안나왔나 아직?",
          "original_form": "엔 안나왔나 아직?",
          "speaker_id": "1",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004552.1.11",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004552.1.12",
          "form": "다음달 되어야 올거 같구",
          "original_form": "다음달 되어야 올거 같구",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004552.1.13",
          "form": "지금까지 수확은... 없다",
          "original_form": "지금까지 수확은... 없다",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004552.1.14",
          "form": "저러어어어언..",
          "original_form": "저러어어어언..",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004552.1.15",
          "form": "너는? 요즘 어떤 게임 해?",
          "original_form": "너는? 요즘 어떤 게임 해?",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004552.1.16",
          "form": "나는 젝스님트..",
          "original_form": "나는 젝스님트..",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004552.1.17",
          "form": "후후..",
          "original_form": "후후..",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004552.1.18",
          "form": "니네랑 하는거 말고는 딱히?",
          "original_form": "니네랑 하는거 말고는 딱히?",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004552.1.19",
          "form": "모바일 게임에서 벗어난 자유인이 됐나봐",
          "original_form": "모바일 게임에서 벗어난 자유인이 됐나봐",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004552.1.20",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004552.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 요즘 안한다는 거지!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 요즘 안한다는 거지!!",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004552.1.22",
          "form": "페그오 멀린이랑 룰밥 뽑고 안들어감",
          "original_form": "페그오 멀린이랑 룰밥 뽑고 안들어감",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004552.1.23",
          "form": "원하는거 다 얻었다는건가",
          "original_form": "원하는거 다 얻었다는건가",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004552.1.24",
          "form": "그게... 된다니",
          "original_form": "그게... 된다니",
          "speaker_id": "2",
          "time": "20210821 17:17"
        }
      ]
