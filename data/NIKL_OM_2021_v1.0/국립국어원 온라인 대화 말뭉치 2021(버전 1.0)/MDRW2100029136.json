      "utterance": [
        {
          "id": "MDRW2100029136.1.1",
          "form": "님네는 그럼 공지용 카톡방이 없나요",
          "original_form": "님네는 그럼 공지용 카톡방이 없나요",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029136.1.2",
          "form": "그건 따로 있죠",
          "original_form": "그건 따로 있죠",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029136.1.3",
          "form": "에타에서 공지 안 함",
          "original_form": "에타에서 공지 안 함",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029136.1.4",
          "form": "거긴 진짜 걍 잡담/홍보용",
          "original_form": "거긴 진짜 걍 잡담/홍보용",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029136.1.5",
          "form": "그건... 당연함",
          "original_form": "그건... 당연함",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029136.1.6",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029136.1.7",
          "form": "잡담... 뭐 함??",
          "original_form": "잡담... 뭐 함??",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029136.1.8",
          "form": "걍 전공 수업얘기나",
          "original_form": "걍 전공 수업얘기나",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029136.1.9",
          "form": "진로... 아니면 교수님...",
          "original_form": "진로... 아니면 교수님...",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029136.1.10",
          "form": "왕신기",
          "original_form": "왕신기",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029136.1.11",
          "form": "어떤 수업이 낫냐 이런 글도 많고",
          "original_form": "어떤 수업이 낫냐 이런 글도 많고",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029136.1.12",
          "form": "우리도 그런 거 있었음 좋겠다...",
          "original_form": "우리도 그런 거 있었음 좋겠다...",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029136.1.13",
          "form": "이쪽 진로로 가려면 어떻게 하는 게 좋아요? 하는 db글도 자주 보이는",
          "original_form": "이쪽 진로로 가려면 어떻게 하는 게 좋아요? 하는 db글도 자주 보이는",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029136.1.14",
          "form": "요즘은 정정 기간이라... 관련 글들 많고...",
          "original_form": "요즘은 정정 기간이라... 관련 글들 많고...",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029136.1.15",
          "form": "우리는 그냥 그런 것들 아는 선배한테만 물어보는데",
          "original_form": "우리는 그냥 그런 것들 아는 선배한테만 물어보는데",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029136.1.16",
          "form": "이런 얘기를 익명으로 하지 않는단 말이야",
          "original_form": "이런 얘기를 익명으로 하지 않는단 말이야",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029136.1.17",
          "form": "신기하네",
          "original_form": "신기하네",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029136.1.18",
          "form": "아는 선배나... 학생회장이나",
          "original_form": "아는 선배나... 학생회장이나",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029136.1.19",
          "form": "우린 분위기가 그래서 더 그럴지도",
          "original_form": "우린 분위기가 그래서 더 그럴지도",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029136.1.20",
          "form": "족보도 아는 선배 통해서 안 하고 커뮤니티에서 공유하잖아요",
          "original_form": "족보도 아는 선배 통해서 안 하고 커뮤니티에서 공유하잖아요",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029136.1.21",
          "form": "우린... 족보가 없으니까",
          "original_form": "우린... 족보가 없으니까",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029136.1.22",
          "form": "님네 다른 과는 그럴걸",
          "original_form": "님네 다른 과는 그럴걸",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029136.1.23",
          "form": "교수님 취향에 대한 정보는 선배한테 묻긴 함",
          "original_form": "교수님 취향에 대한 정보는 선배한테 묻긴 함",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029136.1.24",
          "form": "그니까 그런 db가",
          "original_form": "그니까 그런 db가",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029136.1.25",
          "form": "우리는 인맥으로 하는 걸 별로 안 좋아해서",
          "original_form": "우리는 인맥으로 하는 걸 별로 안 좋아해서",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029136.1.26",
          "form": "교양 족보는 커뮤니티에서 구하긴 하지",
          "original_form": "교양 족보는 커뮤니티에서 구하긴 하지",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029136.1.27",
          "form": "걍 다 쌓아둠 커뮤니티에",
          "original_form": "걍 다 쌓아둠 커뮤니티에",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029136.1.28",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029136.1.29",
          "form": "커뮤니티도... 서로 연락해서 구하는 게 아니라",
          "original_form": "커뮤니티도... 서로 연락해서 구하는 게 아니라",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029136.1.30",
          "form": "그냥 게시판 같은 게 있음",
          "original_form": "그냥 게시판 같은 게 있음",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029136.1.31",
          "form": "족보도",
          "original_form": "족보도",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029136.1.32",
          "form": "her..",
          "original_form": "her..",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029136.1.33",
          "form": "근데 뭐... 취향 같은 건 크게 중요한 요소는 아니라서",
          "original_form": "근데 뭐... 취향 같은 건 크게 중요한 요소는 아니라서",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029136.1.34",
          "form": "꼭 구할 필요가 없긴 하지",
          "original_form": "꼭 구할 필요가 없긴 하지",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029136.1.35",
          "form": "필요성을 못 느껴서 그런 걸지도 모름",
          "original_form": "필요성을 못 느껴서 그런 걸지도 모름",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029136.1.36",
          "form": "걍... 우리학교 분위기임",
          "original_form": "걍... 우리학교 분위기임",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029136.1.37",
          "form": "다른 학교들은 원래 그래요",
          "original_form": "다른 학교들은 원래 그래요",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029136.1.38",
          "form": "기본적으로 선후배 관계가 나쁘지 않은 분위기라 그럴 수도 있고",
          "original_form": "기본적으로 선후배 관계가 나쁘지 않은 분위기라 그럴 수도 있고",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029136.1.39",
          "form": "나도 우리학교가 특이하다고 선배들한테 얘기 많이 들었음",
          "original_form": "나도 우리학교가 특이하다고 선배들한테 얘기 많이 들었음",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029136.1.40",
          "form": "그렇군......",
          "original_form": "그렇군......",
          "speaker_id": "1",
