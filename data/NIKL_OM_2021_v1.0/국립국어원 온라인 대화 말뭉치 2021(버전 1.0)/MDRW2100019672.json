      "utterance": [
        {
          "id": "MDRW2100019672.1.1",
          "form": "누나 이거에진심이시군요!ㅋㅋㅋ",
          "original_form": "누나 이거에진심이시군요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100019672.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ 아 혹시 그만하고 싶으신가요??!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아 혹시 그만하고 싶으신가요??!!",
          "speaker_id": "1",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019672.1.3",
          "form": "저는 빨리 해치우고 싶어서 ㅎㅎ..",
          "original_form": "저는 빨리 해치우고 싶어서 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019672.1.4",
          "form": "아니아니저는",
          "original_form": "아니아니저는",
          "speaker_id": "2",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019672.1.5",
          "form": "누나랑 연락하는거 조아여!@@",
          "original_form": "누나랑 연락하는거 조아여!@@",
          "speaker_id": "2",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019672.1.6",
          "form": "사실 친구가하자고해서",
          "original_form": "사실 친구가하자고해서",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019672.1.7",
          "form": "설치했다가",
          "original_form": "설치했다가",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019672.1.8",
          "form": "우연히 누나같은 좋은분만나서",
          "original_form": "우연히 누나같은 좋은분만나서",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019672.1.9",
          "form": "너무좋아여",
          "original_form": "너무좋아여",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019672.1.10",
          "form": "저 누나랑 친구1명말고는",
          "original_form": "저 누나랑 친구1명말고는",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019672.1.11",
          "form": "연락아무도안하거든요!",
          "original_form": "연락아무도안하거든요!",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019672.1.12",
          "form": "앜ㅋㅋ 저는 친구랑 할려했는데",
          "original_form": "앜ㅋㅋ 저는 친구랑 할려했는데",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019672.1.13",
          "form": "아이폰이라서..씁쓸",
          "original_form": "아이폰이라서..씁쓸",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019672.1.14",
          "form": "앗,,,,ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗,,,,ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019672.1.15",
          "form": "전 진짜 영광이에여",
          "original_form": "전 진짜 영광이에여",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019672.1.16",
          "form": "누나만나거",
          "original_form": "누나만나거",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019672.1.17",
          "form": "영광 ㅋㅋㅋㅋ",
          "original_form": "영광 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019672.1.18",
          "form": "황송하옵니다,,",
          "original_form": "황송하옵니다,,",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019672.1.19",
          "form": "보는 유튜버있나요??",
          "original_form": "보는 유튜버있나요??",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019672.1.20",
          "form": "음..저는 보통",
          "original_form": "음..저는 보통",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019672.1.21",
          "form": "게임영상이나 예능봐서",
          "original_form": "게임영상이나 예능봐서",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019672.1.22",
          "form": "유튜버는 없는거같아여!!",
          "original_form": "유튜버는 없는거같아여!!",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019672.1.23",
          "form": "아 먹방같은거는 안보시나봐요??",
          "original_form": "아 먹방같은거는 안보시나봐요??",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019672.1.24",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019672.1.25",
          "form": "그냥 알고리즘뜨는거아니면",
          "original_form": "그냥 알고리즘뜨는거아니면",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019672.1.26",
          "form": "알고리즘 ㅋㅋㅋ 무섭뎌",
          "original_form": "알고리즘 ㅋㅋㅋ 무섭뎌",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019672.1.27",
          "form": "무섭죠 ㅋㅋㅋㅋ",
          "original_form": "무섭죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019672.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아여",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019672.1.29",
          "form": "어떨게보면 이런 랜덤챗팅?",
          "original_form": "어떨게보면 이런 랜덤챗팅?",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019672.1.30",
          "form": "이럳거 처음인데",
          "original_form": "이럳거 처음인데",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019672.1.31",
          "form": "좋은사람만나서 너무 좋아여",
          "original_form": "좋은사람만나서 너무 좋아여",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019672.1.32",
          "form": "연락을이어가고싶은분은 처음이랄까",
          "original_form": "연락을이어가고싶은분은 처음이랄까",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100019672.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100019672.1.34",
          "form": "아 ㅋㅋㅋㅋㅋ 맞아여 평소엔 잘 안하죠",
          "original_form": "아 ㅋㅋㅋㅋㅋ 맞아여 평소엔 잘 안하죠",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019672.1.35",
          "form": "티빙에서 다른거는 안보세요?",
          "original_form": "티빙에서 다른거는 안보세요?",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019672.1.36",
          "form": "네네 드라마보구",
          "original_form": "네네 드라마보구",
          "speaker_id": "2",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019672.1.37",
          "form": "대탈출보고",
          "original_form": "대탈출보고",
          "speaker_id": "2",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019672.1.38",
          "form": "그러합니다!",
          "original_form": "그러합니다!",
          "speaker_id": "2",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019672.1.39",
          "form": "오호 티빙에는",
          "original_form": "오호 티빙에는",
