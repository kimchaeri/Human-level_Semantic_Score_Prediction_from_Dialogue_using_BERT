      "utterance": [
        {
          "id": "MDRW2100022991.1.1",
          "form": "토마토도 아니랍니다",
          "original_form": "토마토도 아니랍니다",
          "speaker_id": "2",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100022991.1.2",
          "form": "식물이고 과일이 아니예요",
          "original_form": "식물이고 과일이 아니예요",
          "speaker_id": "2",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100022991.1.3",
          "form": "무슨 색이에요? ㅋㅋㅋㅋㅋ",
          "original_form": "무슨 색이에요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100022991.1.4",
          "form": "앗 너무 큰 힌트인데",
          "original_form": "앗 너무 큰 힌트인데",
          "speaker_id": "2",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100022991.1.5",
          "form": "흙색이예요",
          "original_form": "흙색이예요",
          "speaker_id": "2",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100022991.1.6",
          "form": "흙색 식물이요???",
          "original_form": "흙색 식물이요???",
          "speaker_id": "1",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100022991.1.7",
          "form": "땅콩?",
          "original_form": "땅콩?",
          "speaker_id": "1",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100022991.1.8",
          "form": "땡~~~~~아닙니다",
          "original_form": "땡~~~~~아닙니다",
          "speaker_id": "2",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100022991.1.9",
          "form": "저의 위장은 땅콩으로 차지 않습니다",
          "original_form": "저의 위장은 땅콩으로 차지 않습니다",
          "speaker_id": "2",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100022991.1.10",
          "form": "식물입니다",
          "original_form": "식물입니다",
          "speaker_id": "2",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022991.1.11",
          "form": "땅콩도 식물인데요... 우씨",
          "original_form": "땅콩도 식물인데요... 우씨",
          "speaker_id": "1",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022991.1.12",
          "form": "뭐지?",
          "original_form": "뭐지?",
          "speaker_id": "1",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022991.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ식물은 맞지만",
          "original_form": "ㅋㅋㅋㅋㅋㅋ식물은 맞지만",
          "speaker_id": "2",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022991.1.14",
          "form": "땅콩은 너무 땅콩만해서 배가 안 차요",
          "original_form": "땅콩은 너무 땅콩만해서 배가 안 차요",
          "speaker_id": "2",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022991.1.15",
          "form": "아나ㅋㅋㅋ 크기가 어느 정도인가요",
          "original_form": "아나ㅋㅋㅋ 크기가 어느 정도인가요",
          "speaker_id": "1",
          "time": "20210829 20:10"
        },
        {
          "id": "MDRW2100022991.1.16",
          "form": "땅콩답지요",
          "original_form": "땅콩답지요",
          "speaker_id": "2",
          "time": "20210829 20:10"
        },
        {
          "id": "MDRW2100022991.1.17",
          "form": "아 크기는",
          "original_form": "아 크기는",
          "speaker_id": "2",
          "time": "20210829 20:10"
        },
        {
          "id": "MDRW2100022991.1.18",
          "form": "다양하지만 보통 시장에서 사면 한 주먹정도 해요",
          "original_form": "다양하지만 보통 시장에서 사면 한 주먹정도 해요",
          "speaker_id": "2",
          "time": "20210829 20:10"
        },
        {
          "id": "MDRW2100022991.1.19",
          "form": "크기가 다양하다고요...",
          "original_form": "크기가 다양하다고요...",
          "speaker_id": "1",
          "time": "20210829 20:10"
        },
        {
          "id": "MDRW2100022991.1.20",
          "form": "어떻게 조리해서 드셨어요?",
          "original_form": "어떻게 조리해서 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022991.1.21",
          "form": "냄비에 쪄서 먹었답니다",
          "original_form": "냄비에 쪄서 먹었답니다",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022991.1.22",
          "form": "아 정답 감자",
          "original_form": "아 정답 감자",
          "speaker_id": "1",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100022991.1.23",
          "form": "감자 쪄드샸군요!",
          "original_form": "감자 쪄드샸군요!",
          "speaker_id": "1",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100022991.1.24",
          "form": "와 딩동댕동댕",
          "original_form": "와 딩동댕동댕",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100022991.1.25",
          "form": "정답입니다 천재해일",
          "original_form": "정답입니다 천재해일",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100022991.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100022991.1.27",
          "form": "예~~~~ 감자 아직 제철이죠ㅋㅋㅋㅋ",
          "original_form": "예~~~~ 감자 아직 제철이죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100022991.1.28",
          "form": "초성힌트까지 안 쓰시고 맞추셨네요!",
          "original_form": "초성힌트까지 안 쓰시고 맞추셨네요!",
          "speaker_id": "2",
          "time": "20210829 20:13"
        }
      ]
    }
