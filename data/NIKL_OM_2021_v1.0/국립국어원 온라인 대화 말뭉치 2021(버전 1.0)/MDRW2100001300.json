      "utterance": [
        {
          "id": "MDRW2100001300.2.1",
          "form": "저녁 뭐먹지?",
          "original_form": "저녁 뭐먹지?",
          "speaker_id": "1",
          "time": "20210517 15:58"
        },
        {
          "id": "MDRW2100001300.2.2",
          "form": "맛있는거 ㅎㅎ \r\n미역국 많이많이 해줘",
          "original_form": "맛있는거 ㅎㅎ \r\n미역국 많이많이 해줘",
          "speaker_id": "2",
          "time": "20210517 15:59"
        },
        {
          "id": "MDRW2100001300.2.3",
          "form": "욕심은 ㅜ\r\n다먹지도 않으면서",
          "original_form": "욕심은 ㅜ\r\n다먹지도 않으면서",
          "speaker_id": "1",
          "time": "20210517 15:59"
        },
        {
          "id": "MDRW2100001300.2.4",
          "form": "비 그쳐야하는데 너무 온다",
          "original_form": "비 그쳐야하는데 너무 온다",
          "speaker_id": "2",
          "time": "20210517 15:59"
        },
        {
          "id": "MDRW2100001300.2.5",
          "form": "그러게 싫다\r\n더워지려고 그러나 음",
          "original_form": "그러게 싫다\r\n더워지려고 그러나 음",
          "speaker_id": "1",
          "time": "20210517 16:00"
        },
        {
          "id": "MDRW2100001300.2.6",
          "form": "그렇겠지,\r\n비가 와야 농사가 잘되지, 안그래도 힘들자나",
          "original_form": "그렇겠지,\r\n비가 와야 농사가 잘되지, 안그래도 힘들자나",
          "speaker_id": "2",
          "time": "20210517 16:00"
        },
        {
          "id": "MDRW2100001300.2.7",
          "form": "갔다왔습니다",
          "original_form": "갔다왔습니다",
          "speaker_id": "1",
          "time": "20210517 16:55"
        },
        {
          "id": "MDRW2100001300.2.8",
          "form": "네네 장모님은",
          "original_form": "네네 장모님은",
          "speaker_id": "2",
          "time": "20210517 16:56"
        },
        {
          "id": "MDRW2100001300.2.9",
          "form": "늦게 온다던데 \r\n없어",
          "original_form": "늦게 온다던데 \r\n없어",
          "speaker_id": "1",
          "time": "20210517 16:56"
        },
        {
          "id": "MDRW2100001300.2.10",
          "form": "음 저녁메뉴는 뭐야",
          "original_form": "음 저녁메뉴는 뭐야",
          "speaker_id": "2",
          "time": "20210517 16:58"
        },
        {
          "id": "MDRW2100001300.2.11",
          "form": "고추잡채\r\n과자 주워 먹었더니 입맛이 없다",
          "original_form": "고추잡채\r\n과자 주워 먹었더니 입맛이 없다",
          "speaker_id": "1",
          "time": "20210517 16:58"
        },
        {
          "id": "MDRW2100001300.2.12",
          "form": "맛있겠다\r\n굿굿 좋아",
          "original_form": "맛있겠다\r\n굿굿 좋아",
          "speaker_id": "2",
          "time": "20210517 17:02"
        },
        {
          "id": "MDRW2100001300.2.13",
          "form": "밀키트지요.\r\n요즘은 잘 나오자나요.ㅋ",
          "original_form": "밀키트지요.\r\n요즘은 잘 나오자나요.ㅋ",
          "speaker_id": "1",
          "time": "20210517 17:03"
        },
        {
          "id": "MDRW2100001300.2.14",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "1",
