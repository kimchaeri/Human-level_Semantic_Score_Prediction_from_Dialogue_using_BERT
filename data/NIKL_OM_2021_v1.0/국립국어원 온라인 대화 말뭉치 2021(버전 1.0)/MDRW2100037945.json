      "utterance": [
        {
          "id": "MDRW2100037945.1.1",
          "form": "매운 음식은 잘 드세요?",
          "original_form": "매운 음식은 잘 드세요?",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100037945.1.2",
          "form": "매운음식은..... 잘 먹지 못하나 있으면 먹어욬ㅋㅋㅋㅋㅋ",
          "original_form": "매운음식은..... 잘 먹지 못하나 있으면 먹어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100037945.1.3",
          "form": "치즈를 넣어서 먹거나 음료의 도움을 받습니다 ㅋㅋㅋ",
          "original_form": "치즈를 넣어서 먹거나 음료의 도움을 받습니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100037945.1.4",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100037945.1.5",
          "form": "매움이 목적인 음식은 안좋아하는데",
          "original_form": "매움이 목적인 음식은 안좋아하는데",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100037945.1.6",
          "form": "엽떡 4단계 같은거요",
          "original_form": "엽떡 4단계 같은거요",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100037945.1.7",
          "form": "근데 매콤한 맛은 좋아해요",
          "original_form": "근데 매콤한 맛은 좋아해요",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100037945.1.8",
          "form": "해물찜, 낙지볶음,닭갈비, 낙곱새 뭐 이런정도의 매운맛?",
          "original_form": "해물찜, 낙지볶음,닭갈비, 낙곱새 뭐 이런정도의 매운맛?",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100037945.1.9",
          "form": "그정도는 저도 먹을수 있어요!! ㅎㅎ 저도 엽떡은 안먹어봤어요",
          "original_form": "그정도는 저도 먹을수 있어요!! ㅎㅎ 저도 엽떡은 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100037945.1.10",
          "form": "불닭볶음면 정도까진 맛있게 먹었는데 그 이상은 못먹겠더라구요ㅠㅠ",
          "original_form": "불닭볶음면 정도까진 맛있게 먹었는데 그 이상은 못먹겠더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100037945.1.11",
          "form": "불닭볶음면 전 소스 반만넣어서 먹어요",
          "original_form": "불닭볶음면 전 소스 반만넣어서 먹어요",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100037945.1.12",
          "form": "저더여... ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저더여... ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100037945.1.13",
          "form": "참치마요 삼김같은것도 꼭 있어야하구요",
          "original_form": "참치마요 삼김같은것도 꼭 있어야하구요",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100037945.1.14",
          "form": "아님 물 많이..",
          "original_form": "아님 물 많이..",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100037945.1.15",
          "form": "불닭볶음탕도 드셔보셨어요?",
          "original_form": "불닭볶음탕도 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100037945.1.16",
          "form": "이건 더 맵더라구요!!",
          "original_form": "이건 더 맵더라구요!!",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100037945.1.17",
          "form": "ㅋㅋㅋ 그래서 까르보불닭을 더 좋아해요",
          "original_form": "ㅋㅋㅋ 그래서 까르보불닭을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100037945.1.18",
          "form": "아뇨 볶음탕은 안먹어봤어요",
          "original_form": "아뇨 볶음탕은 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100037945.1.19",
          "form": "사실 까르보 나온뒤로 오리지날 불닭은 안먹었어요 ㅋㅋ",
          "original_form": "사실 까르보 나온뒤로 오리지날 불닭은 안먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100037945.1.20",
          "form": "아아 ㅋㅋ  불닭소스맛 국물이 있어서 더 매웠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아아 ㅋㅋ  불닭소스맛 국물이 있어서 더 매웠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100037945.1.21",
          "form": "치즈 잔뜩 뿌려먹었도니 나름 괜찮더라구오!",
          "original_form": "치즈 잔뜩 뿌려먹었도니 나름 괜찮더라구오!",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100037945.1.22",
          "form": "으악! 더 맵다니 전의가 꺾입니다ㅋㅋㅋㅋ",
          "original_form": "으악! 더 맵다니 전의가 꺾입니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100037945.1.23",
          "form": "ㅋㅋㅋㅋ매운걸 뜨거운 국물로 먹으니까 아무래도 그렇더라구요ㅠㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ매운걸 뜨거운 국물로 먹으니까 아무래도 그렇더라구요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100037945.1.24",
          "form": "근데 맵찔이 저도 나름 맛있게 먹었어요",
          "original_form": "근데 맵찔이 저도 나름 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100037945.1.25",
          "form": "하, 한번 도전해 볼까요?",
          "original_form": "하, 한번 도전해 볼까요?",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100037945.1.26",
          "form": "마요네즈 뿌려먹어야하나..?",
          "original_form": "마요네즈 뿌려먹어야하나..?",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100037945.1.27",
          "form": "그러시거나 치즈 듬뿍이욤ㅁ",
          "original_form": "그러시거나 치즈 듬뿍이욤ㅁ",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100037945.1.28",
          "form": "저 진짜 매운거 잘 안먹는편인데 제가 먹은정도면 괜찮아여",
