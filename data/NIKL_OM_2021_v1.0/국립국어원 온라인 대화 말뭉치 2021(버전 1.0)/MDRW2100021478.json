        },
        {
          "id": "MDRW2100021478.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021478.1.3",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021478.1.4",
          "form": "전 별로 못 가봤어요 ㅠㅠ 좋아하세요?",
          "original_form": "전 별로 못 가봤어요 ㅠㅠ 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021478.1.5",
          "form": "가끔가는 여행은 기분전환되서 좋아해요",
          "original_form": "가끔가는 여행은 기분전환되서 좋아해요",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021478.1.6",
          "form": "길게는 못가본것같아요",
          "original_form": "길게는 못가본것같아요",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021478.1.7",
          "form": "학생일때는 돈이없었고 일하니깐 시간내기 힘들어요",
          "original_form": "학생일때는 돈이없었고 일하니깐 시간내기 힘들어요",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021478.1.8",
          "form": "맞아요 비용이 많이 들더라구요",
          "original_form": "맞아요 비용이 많이 들더라구요",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021478.1.9",
          "form": "어디가 제일 좋으셨어요?",
          "original_form": "어디가 제일 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021478.1.10",
          "form": "지금 딱 떠오르는 곳은 필리핀이요",
          "original_form": "지금 딱 떠오르는 곳은 필리핀이요",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100021478.1.11",
          "form": "필리필 많이 가더라구요!",
          "original_form": "필리필 많이 가더라구요!",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021478.1.12",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021478.1.13",
          "form": "원헌드레드아일랜드였나? 거기서 엄청 높고 긴 로프 바다위에서 타고 내려갔을때 진짜 신났어요",
          "original_form": "원헌드레드아일랜드였나? 거기서 엄청 높고 긴 로프 바다위에서 타고 내려갔을때 진짜 신났어요",
          "speaker_id": "1",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021478.1.14",
          "form": "와 신기하네요",
          "original_form": "와 신기하네요",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021478.1.15",
          "form": "그런 스포츠? 즐기세요?",
          "original_form": "그런 스포츠? 즐기세요?",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021478.1.16",
          "form": "근데 여행은 무엇보다 누구랑 가는지가 제일 중요해요",
          "original_form": "근데 여행은 무엇보다 누구랑 가는지가 제일 중요해요",
          "speaker_id": "1",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021478.1.17",
          "form": "아 공감합니다",
          "original_form": "아 공감합니다",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021478.1.18",
          "form": "목숨이 왔다리갔다리 안하는 스포츠 즐기는정도는 돼요",
          "original_form": "목숨이 왔다리갔다리 안하는 스포츠 즐기는정도는 돼요",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021478.1.19",
          "form": "최근에 어디 가신데있어요?",
          "original_form": "최근에 어디 가신데있어요?",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021478.1.20",
          "form": "목숨 왔다리 ㅋㅋㅋ 무서운데요 ㅌㅋㅋ",
          "original_form": "목숨 왔다리 ㅋㅋㅋ 무서운데요 ㅌㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021478.1.21",
          "form": "저는 여행은 아니고 호캉스만 다녀왔어요",
          "original_form": "저는 여행은 아니고 호캉스만 다녀왔어요",
          "speaker_id": "2",
          "time": "20210828 19:35"
        }
      ]
