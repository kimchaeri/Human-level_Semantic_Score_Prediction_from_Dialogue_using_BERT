      "utterance": [
        {
          "id": "MDRW2100029113.1.1",
          "form": "나중에 여행 가능해지면 가보고싶은 곳 있으신가요?",
          "original_form": "나중에 여행 가능해지면 가보고싶은 곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029113.1.2",
          "form": "저는 베트남가고 싶어요",
          "original_form": "저는 베트남가고 싶어요",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029113.1.3",
          "form": "한번도 가본적이 없어서",
          "original_form": "한번도 가본적이 없어서",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029113.1.4",
          "form": "베트남 좋죠",
          "original_form": "베트남 좋죠",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029113.1.5",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029113.1.6",
          "form": "저는 이탈리아요",
          "original_form": "저는 이탈리아요",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029113.1.7",
          "form": "관광지들이랑 바티칸 가보고싶어요",
          "original_form": "관광지들이랑 바티칸 가보고싶어요",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029113.1.8",
          "form": "이탈리아 낭만적이다",
          "original_form": "이탈리아 낭만적이다",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029113.1.9",
          "form": "저는 가본적 없는데",
          "original_form": "저는 가본적 없는데",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029113.1.10",
          "form": "아내가 이탈리아 못가봤냐며 자주 놀려요",
          "original_form": "아내가 이탈리아 못가봤냐며 자주 놀려요",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029113.1.11",
          "form": "저도 이탈리아 가보고 싶네요",
          "original_form": "저도 이탈리아 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029113.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029113.1.13",
          "form": "해외여행 많이 해보셨어요?",
          "original_form": "해외여행 많이 해보셨어요?",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029113.1.14",
          "form": "전에 읽었던 책에서 주인공이 이탈리아를 갔던게 인상깊었거든요",
          "original_form": "전에 읽었던 책에서 주인공이 이탈리아를 갔던게 인상깊었거든요",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029113.1.15",
          "form": "앗 많이 안가봤어요",
          "original_form": "앗 많이 안가봤어요",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029113.1.16",
          "form": "가족여행으로 네번 다녀왔네요",
          "original_form": "가족여행으로 네번 다녀왔네요",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029113.1.17",
          "form": "어디 다녀오셨어요?",
          "original_form": "어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029113.1.18",
          "form": "어릴때 베트남이랑 앙코르와트 가봤고",
          "original_form": "어릴때 베트남이랑 앙코르와트 가봤고",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029113.1.19",
          "form": "학생때 보라카이, 청도, 태국 다녀왔어요",
          "original_form": "학생때 보라카이, 청도, 태국 다녀왔어요",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029113.1.20",
          "form": "보라카이가 인도네시아?",
          "original_form": "보라카이가 인도네시아?",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029113.1.21",
          "form": "베트남 앙코르와트는 패키지로 묶어서 다녀왔어요",
          "original_form": "베트남 앙코르와트는 패키지로 묶어서 다녀왔어요",
          "speaker_id": "2",
          "time": "20210902 23:50"
        }
