      "utterance": [
        {
          "id": "MDRW2100030277.1.1",
          "form": "재밌게 보는 프로 있으신가요",
          "original_form": "재밌게 보는 프로 있으신가요",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030277.1.2",
          "form": "요즘은 놀면뭐하니 재밌더라구요",
          "original_form": "요즘은 놀면뭐하니 재밌더라구요",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030277.1.3",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030277.1.4",
          "form": "무도유니버스",
          "original_form": "무도유니버스",
          "speaker_id": "2",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030277.1.5",
          "form": "하하랑 정준하 나오더라구요",
          "original_form": "하하랑 정준하 나오더라구요",
          "speaker_id": "1",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030277.1.6",
          "form": "ㅋㅋ정과장은 역시",
          "original_form": "ㅋㅋ정과장은 역시",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030277.1.7",
          "form": "이번주도 재밌을 것 같아요 ㅋㅋ",
          "original_form": "이번주도 재밌을 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030277.1.8",
          "form": "박명수가 보고싶어요ㅋㅋ",
          "original_form": "박명수가 보고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030277.1.9",
          "form": "그러게요. 일정이 겹치나..",
          "original_form": "그러게요. 일정이 겹치나..",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030277.1.10",
          "form": "네 다른거 한다고....ㅋㄲ",
          "original_form": "네 다른거 한다고....ㅋㄲ",
          "speaker_id": "2",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100030277.1.11",
          "form": "그래도 언젠가 한 번은 나올지도요ㅎㅎ",
          "original_form": "그래도 언젠가 한 번은 나올지도요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030277.1.12",
          "form": "그럼요 나올겁니다",
          "original_form": "그럼요 나올겁니다",
          "speaker_id": "2",
          "time": "20210904 01:12"
        },
        {
          "id": "MDRW2100030277.1.13",
          "form": "또 재밌게 보는 방송 있으신가요?",
          "original_form": "또 재밌게 보는 방송 있으신가요?",
          "speaker_id": "1",
          "time": "20210904 01:20"
        },
        {
          "id": "MDRW2100030277.1.14",
          "form": "런닝맨이오ㅋㅋ",
          "original_form": "런닝맨이오ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030277.1.15",
          "form": "저는 잘 못 봤는데 최근 반응 좋더라구요",
