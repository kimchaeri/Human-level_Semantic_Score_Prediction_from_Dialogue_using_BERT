      "utterance": [
        {
          "id": "MDRW2100039959.1.1",
          "form": "리얼피치바는 정말",
          "original_form": "리얼피치바는 정말",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039959.1.2",
          "form": "그냥 복숭아가 아니라",
          "original_form": "그냥 복숭아가 아니라",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100039959.1.3",
          "form": "고급스러운 복숭아랄까요.....?",
          "original_form": "고급스러운 복숭아랄까요.....?",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100039959.1.4",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100039959.1.5",
          "form": "이건 먹어야해요",
          "original_form": "이건 먹어야해요",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100039959.1.6",
          "form": "네,,,",
          "original_form": "네,,,",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.7",
          "form": "안그래도 민트초코 마카롱아이스크림도 사러가고 싶었는데",
          "original_form": "안그래도 민트초코 마카롱아이스크림도 사러가고 싶었는데",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.8",
          "form": "그리고 더 나아가서",
          "original_form": "그리고 더 나아가서",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.9",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.10",
          "form": "그거 진짜",
          "original_form": "그거 진짜",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.11",
          "form": "같이 사와야겠어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "같이 사와야겠어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.12",
          "form": "마카롱 아이스크림은 짖짜",
          "original_form": "마카롱 아이스크림은 짖짜",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.13",
          "form": "세계평화상 줘야해요",
          "original_form": "세계평화상 줘야해요",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.14",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.16",
          "form": "반드시 사오겠습니다...",
          "original_form": "반드시 사오겠습니다...",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.17",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.18",
          "form": "모든 맛을 다 먹어봤죠",
          "original_form": "모든 맛을 다 먹어봤죠",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.19",
          "form": "근데 이상하게",
          "original_form": "근데 이상하게",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039959.1.20",
          "form": "배스킨라빈스에서 파는 마카롱 아이스크림보다",
          "original_form": "배스킨라빈스에서 파는 마카롱 아이스크림보다",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.21",
          "form": "파바에서 파는 게 더 맛나요",
          "original_form": "파바에서 파는 게 더 맛나요",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.22",
          "form": "물론 둘 다 맛있지만",
          "original_form": "물론 둘 다 맛있지만",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.23",
          "form": "배라보다 맛있다구요?",
          "original_form": "배라보다 맛있다구요?",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.24",
          "form": "세상에....",
          "original_form": "세상에....",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.25",
          "form": "네....",
          "original_form": "네....",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.26",
          "form": "뭔가...",
          "original_form": "뭔가...",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.27",
          "form": "파바가 민초에 영혼을 바쳤나봐요",
          "original_form": "파바가 민초에 영혼을 바쳤나봐요",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.28",
          "form": "작정하고 만들었다는 느낌?",
          "original_form": "작정하고 만들었다는 느낌?",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.29",
          "form": "그거랑 파바에서 파는 마카롱 아이스크림은",
          "original_form": "그거랑 파바에서 파는 마카롱 아이스크림은",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.30",
          "form": "그냥 다 드셔주세요...",
          "original_form": "그냥 다 드셔주세요...",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ민초를 대하는 바람직한 마음가짐을 다 하셨나보네요 파바가",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ민초를 대하는 바람직한 마음가짐을 다 하셨나보네요 파바가",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039959.1.32",
          "form": "맞아욬ㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039959.1.33",
          "form": "그리고 파바에서 파는 민초 케이크는",
          "original_form": "그리고 파바에서 파는 민초 케이크는",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039959.1.34",
          "form": "조만간 달려가야겠어요 좋은 정보 진짜 감사합니다...!",
          "original_form": "조만간 달려가야겠어요 좋은 정보 진짜 감사합니다...!",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039959.1.35",
          "form": "얼려서 드시면 더 맛나요",
          "original_form": "얼려서 드시면 더 맛나요",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039959.1.36",
          "form": "앗 이럴수가 좀 더 일찍 알았다면...!",
          "original_form": "앗 이럴수가 좀 더 일찍 알았다면...!",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039959.1.37",
          "form": "민초 케이크는 먹어봤거든요!!ㅋㅋㅋㅋㅋ\r\n물론 얼리지 않아도 맛있었지만..!",
          "original_form": "민초 케이크는 먹어봤거든요!!ㅋㅋㅋㅋㅋ\r\n물론 얼리지 않아도 맛있었지만..!",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039959.1.38",
          "form": "아 그럼 좀 실망하셨겠다ㅠ",
          "original_form": "아 그럼 좀 실망하셨겠다ㅠ",
          "speaker_id": "1",
          "time": "20210903 23:41"
        }
      ]
