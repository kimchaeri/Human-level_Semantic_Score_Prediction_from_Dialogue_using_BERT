        },
        {
          "id": "MDRW2100024115.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024115.1.3",
          "form": "배달음식 좋아해요?",
          "original_form": "배달음식 좋아해요?",
          "speaker_id": "2",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024115.1.4",
          "form": "네 좋아해요 ㅎㅎ 배달음식도 좋아하고 해먹는것도 좋아해요 님은요",
          "original_form": "네 좋아해요 ㅎㅎ 배달음식도 좋아하고 해먹는것도 좋아해요 님은요",
          "speaker_id": "1",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024115.1.5",
          "form": "저도요  ㅎ",
          "original_form": "저도요  ㅎ",
          "speaker_id": "2",
          "time": "20210831 11:24"
        },
        {
          "id": "MDRW2100024115.1.6",
          "form": "오늘같은날 시켜먹기 좋겠네요 뭐시켜드실건가요",
          "original_form": "오늘같은날 시켜먹기 좋겠네요 뭐시켜드실건가요",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024115.1.7",
          "form": "버거킹 포장중이요 ㅎ",
          "original_form": "버거킹 포장중이요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024115.1.8",
          "form": "혹시 치즈와퍼?!",
          "original_form": "혹시 치즈와퍼?!",
          "speaker_id": "1",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024115.1.9",
          "form": "넹 그냥와퍼랑 치즈와퍼있어요",
          "original_form": "넹 그냥와퍼랑 치즈와퍼있어요",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024115.1.10",
          "form": "ㅎㅎ 버거킹 먹은지 오래인데 맛있겠어요 감튀는요?",
          "original_form": "ㅎㅎ 버거킹 먹은지 오래인데 맛있겠어요 감튀는요?",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024115.1.11",
          "form": "당근있죠 젤좋아해요버거킹껄로",
          "original_form": "당근있죠 젤좋아해요버거킹껄로",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024115.1.12",
          "form": "원플원이거든요",
          "original_form": "원플원이거든요",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024115.1.13",
          "form": "아 오늘 행사하는구나 ㅋㅋㅋㅋ 버거킹 사람 몰리겠어요.. 배달도 많겠는데요",
          "original_form": "아 오늘 행사하는구나 ㅋㅋㅋㅋ 버거킹 사람 몰리겠어요.. 배달도 많겠는데요",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024115.1.14",
          "form": "거의 원플원하는거같아요 감튀는",
          "original_form": "거의 원플원하는거같아요 감튀는",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024115.1.15",
          "form": "비오는날은 배달은 다 눅눅하고 안좋더라고요",
          "original_form": "비오는날은 배달은 다 눅눅하고 안좋더라고요",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024115.1.16",
          "form": "ㅎㅎ 맛있게 드세요 저는 햄버거 먹을때 사이다로먹는데 님은요?",
          "original_form": "ㅎㅎ 맛있게 드세요 저는 햄버거 먹을때 사이다로먹는데 님은요?",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024115.1.17",
          "form": "저는 환타요 ㅋ 제일 김이 안빠지더라고요",
          "original_form": "저는 환타요 ㅋ 제일 김이 안빠지더라고요",
          "speaker_id": "2",
          "time": "20210831 17:42"
        }
      ]
