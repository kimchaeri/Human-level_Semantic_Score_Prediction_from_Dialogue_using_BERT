        {
          "id": "MDRW2100031851.1.1",
          "form": "gabojago~~~!!!!",
          "original_form": "gabojago~~~!!!!",
          "speaker_id": "1",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031851.1.2",
          "form": "헉그럼 전공 뭐셨는지",
          "original_form": "헉그럼 전공 뭐셨는지",
          "speaker_id": "2",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031851.1.3",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031851.1.4",
          "form": "여쭤봐도...될까용",
          "original_form": "여쭤봐도...될까용",
          "speaker_id": "2",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031851.1.5",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031851.1.6",
          "form": "학부는 화학나노과학과",
          "original_form": "학부는 화학나노과학과",
          "speaker_id": "1",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031851.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031851.1.8",
          "form": "석사는 유기화학 전공입니다",
          "original_form": "석사는 유기화학 전공입니다",
          "speaker_id": "1",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031851.1.9",
          "form": "너무멋져요",
          "original_form": "너무멋져요",
          "speaker_id": "2",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031851.1.10",
          "form": "자연대라서",
          "original_form": "자연대라서",
          "speaker_id": "1",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031851.1.11",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031851.1.12",
          "form": "공대만큼 취업이 안되거든요",
          "original_form": "공대만큼 취업이 안되거든요",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.13",
          "form": "껄껄!",
          "original_form": "껄껄!",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.14",
          "form": "너무 멋지잖아요",
          "original_form": "너무 멋지잖아요",
          "speaker_id": "2",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.15",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.16",
          "form": "감사합니다... 제가 또 칭찬에",
          "original_form": "감사합니다... 제가 또 칭찬에",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.17",
          "form": "춤추는 돌고래라서~~~~",
          "original_form": "춤추는 돌고래라서~~~~",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.18",
          "form": "저 자연과학너무좋아하거든요",
          "original_form": "저 자연과학너무좋아하거든요",
          "speaker_id": "2",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.19",
          "form": "덩실덩실",
          "original_form": "덩실덩실",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.20",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.21",
          "form": "아니 인팁",
          "original_form": "아니 인팁",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.22",
          "form": "정말",
          "original_form": "정말",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.23",
          "form": "대단해요",
          "original_form": "대단해요",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.24",
          "form": "저는 사람이 좋은데요",
          "original_form": "저는 사람이 좋은데요",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.25",
          "form": "저 철학이랑 물리랑 지구과학좋아해서",
          "original_form": "저 철학이랑 물리랑 지구과학좋아해서",
          "speaker_id": "2",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 자연과학을 좋아하신다니 정말!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 자연과학을 좋아하신다니 정말!!!",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.27",
          "form": "철학과갈뻔했어요",
          "original_form": "철학과갈뻔했어요",
          "speaker_id": "2",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.28",
          "form": "네네네네",
          "original_form": "네네네네",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.29",
          "form": "근데 교양으로 철학듣고",
          "original_form": "근데 교양으로 철학듣고",
          "speaker_id": "2",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031851.1.30",
          "form": "이건 안된다",
          "original_form": "이건 안된다",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031851.1.31",
          "form": "이래서 냅다 응용수학을하게되었죠",
          "original_form": "이래서 냅다 응용수학을하게되었죠",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031851.1.32",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "1",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031851.1.33",
          "form": "헉 사람좋아하신다면 뭔가 마케팅쪽도 좋아하시겠어요",
          "original_form": "헉 사람좋아하신다면 뭔가 마케팅쪽도 좋아하시겠어요",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031851.1.34",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031851.1.35",
          "form": "아니근데 응용수학과",
          "original_form": "아니근데 응용수학과",
          "speaker_id": "1",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031851.1.36",
          "form": "헉넵",
          "original_form": "헉넵",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031851.1.37",
          "form": "더 멋지지않나요 막 암호 이런거 하시나",
          "original_form": "더 멋지지않나요 막 암호 이런거 하시나",
          "speaker_id": "1",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031851.1.38",
          "form": "아 마케팅도 좋죠!!!@",
          "original_form": "아 마케팅도 좋죠!!!@",
          "speaker_id": "1",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031851.1.39",
          "form": "크악역시",
          "original_form": "크악역시",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031851.1.40",
          "form": "암호이런거할줄알았는데",
          "original_form": "암호이런거할줄알았는데",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031851.1.41",
          "form": "아낰ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아낰ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 02:57"
        },
        {
          "id": "MDRW2100031851.1.42",
          "form": "아니던가요?!",
          "original_form": "아니던가요?!",
          "speaker_id": "1",
          "time": "20210905 02:57"
        },
        {
          "id": "MDRW2100031851.1.43",
          "form": "미적분학이랑 이상한 c++",
          "original_form": "미적분학이랑 이상한 c++",
          "speaker_id": "2",
          "time": "20210905 02:57"
        },
        {
          "id": "MDRW2100031851.1.44",
          "form": "이런거하던데요",
          "original_form": "이런거하던데요",
          "speaker_id": "2",
          "time": "20210905 02:57"
        },
        {
          "id": "MDRW2100031851.1.45",
