      "utterance": [
        {
          "id": "MDRW2100006701.1.1",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100006701.1.2",
          "form": "저는 키크고 종교같고 긍정적인 사람이요",
          "original_form": "저는 키크고 종교같고 긍정적인 사람이요",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100006701.1.3",
          "form": "와 다 중요하지요",
          "original_form": "와 다 중요하지요",
          "speaker_id": "1",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100006701.1.4",
          "form": "종교가 있으세요?",
          "original_form": "종교가 있으세요?",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006701.1.5",
          "form": "네 기독교요 ㅎ",
          "original_form": "네 기독교요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100006701.1.6",
          "form": "저는 무교였는데",
          "original_form": "저는 무교였는데",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100006701.1.7",
          "form": "결혼하며 종교가지니 행복해요ㅎ",
          "original_form": "결혼하며 종교가지니 행복해요ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100006701.1.8",
          "form": "오 드문경우네요 어떤 종교요?",
          "original_form": "오 드문경우네요 어떤 종교요?",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100006701.1.9",
          "form": "저도 기독교요ㅎㅎ",
          "original_form": "저도 기독교요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100006701.1.10",
          "form": "오 배우자분이 기독교셨나요",
          "original_form": "오 배우자분이 기독교셨나요",
          "speaker_id": "2",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100006701.1.11",
          "form": "네 저흰무교집안",
          "original_form": "네 저흰무교집안",
          "speaker_id": "1",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100006701.1.12",
          "form": "상대는 기독교집안",
          "original_form": "상대는 기독교집안",
          "speaker_id": "1",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100006701.1.13",
          "form": "종교 다르면 결심하기가 쉽지않으실텐데.괜찮으셨나봐요",
          "original_form": "종교 다르면 결심하기가 쉽지않으실텐데.괜찮으셨나봐요",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100006701.1.14",
          "form": "처음엔 갈등이 있었는데",
          "original_form": "처음엔 갈등이 있었는데",
          "speaker_id": "1",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100006701.1.15",
          "form": "사랑으로 극복ㅋㅋ",
          "original_form": "사랑으로 극복ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100006701.1.16",
          "form": "와 찐사랑이네요",
          "original_form": "와 찐사랑이네요",
          "speaker_id": "2",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100006701.1.17",
          "form": "말하고 보니 그렇네요ㅋㅋ",
          "original_form": "말하고 보니 그렇네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100006701.1.18",
          "form": "원래 종교가 같으셨어요?",
          "original_form": "원래 종교가 같으셨어요?",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100006701.1.19",
          "form": "네 그래서 결혼 결심하는데 수월했지요",
          "original_form": "네 그래서 결혼 결심하는데 수월했지요",
          "speaker_id": "2",
          "time": "20210908 07:36"
        }
