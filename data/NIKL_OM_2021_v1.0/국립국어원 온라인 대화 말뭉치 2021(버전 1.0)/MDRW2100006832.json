      "utterance": [
        {
          "id": "MDRW2100006832.1.1",
          "form": "어떤 메뉴 좋아하시나여?",
          "original_form": "어떤 메뉴 좋아하시나여?",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100006832.1.2",
          "form": "피자햄버거요 20님은요",
          "original_form": "피자햄버거요 20님은요",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100006832.1.3",
          "form": "곱창 막창 이런 거 좋아해요",
          "original_form": "곱창 막창 이런 거 좋아해요",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100006832.1.4",
          "form": "치킨은요?",
          "original_form": "치킨은요?",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006832.1.5",
          "form": "치킨도 좋아하져!!",
          "original_form": "치킨도 좋아하져!!",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006832.1.6",
          "form": "치킨은 후라이드 좋아하구여",
          "original_form": "치킨은 후라이드 좋아하구여",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006832.1.7",
          "form": "어디거 드세요?",
          "original_form": "어디거 드세요?",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100006832.1.8",
          "form": "흠 여기저기 다 좋아하는데 BBQ가 제일 맛있는거 같긴해여",
          "original_form": "흠 여기저기 다 좋아하는데 BBQ가 제일 맛있는거 같긴해여",
          "speaker_id": "1",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100006832.1.9",
          "form": "동감입니다 비비큐 후라이드",
          "original_form": "동감입니다 비비큐 후라이드",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006832.1.10",
          "form": "그쳐 거기 후라이드가 뭔가 조금더 맛있는 느낌이긴해요",
          "original_form": "그쳐 거기 후라이드가 뭔가 조금더 맛있는 느낌이긴해요",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006832.1.11",
          "form": "후라이드 파세요?",
          "original_form": "후라이드 파세요?",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006832.1.12",
          "form": "네 후라이드파입니다",
          "original_form": "네 후라이드파입니다",
          "speaker_id": "2",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100006832.1.13",
          "form": "역시 치킨은 후라이드죠~ ㅋㅋ",
          "original_form": "역시 치킨은 후라이드죠~ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100006832.1.14",
          "form": "푸라닭은 드셔보셨나요",
          "original_form": "푸라닭은 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006832.1.15",
          "form": "엇 아니요~ 나중에 먹어봐야 겠네요",
          "original_form": "엇 아니요~ 나중에 먹어봐야 겠네요",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006832.1.16",
          "form": "블랙마요가 유명하더라구요",
          "original_form": "블랙마요가 유명하더라구요",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006832.1.17",
          "form": "오 알아두겠습니당~",
          "original_form": "오 알아두겠습니당~",
          "speaker_id": "1",
          "time": "20210908 10:42"
