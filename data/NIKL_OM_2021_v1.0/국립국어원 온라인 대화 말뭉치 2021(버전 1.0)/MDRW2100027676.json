      "utterance": [
        {
          "id": "MDRW2100027676.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027676.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027676.1.3",
          "form": "애가 셋이다보니 엄두도 못내요",
          "original_form": "애가 셋이다보니 엄두도 못내요",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027676.1.4",
          "form": "아이고 강아지같은 아이들을 키우시는군요",
          "original_form": "아이고 강아지같은 아이들을 키우시는군요",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027676.1.5",
          "form": "ㅎㅎ 이제는 많이 커서 강아지 같지는않고",
          "original_form": "ㅎㅎ 이제는 많이 커서 강아지 같지는않고",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027676.1.6",
          "form": "앜ㅋㅋ 꿀을 찾는 곰과같은가요",
          "original_form": "앜ㅋㅋ 꿀을 찾는 곰과같은가요",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027676.1.7",
          "form": "정답입니다!!!",
          "original_form": "정답입니다!!!",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027676.1.8",
          "form": "하핳 저는 비숑을 키우고싶은데 애완견은 키우면 돈이 많이드니까 상상으로 키워요",
          "original_form": "하핳 저는 비숑을 키우고싶은데 애완견은 키우면 돈이 많이드니까 상상으로 키워요",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027676.1.9",
          "form": "ㅎㅎ 저도 상상으로 키워야겠네요",
          "original_form": "ㅎㅎ 저도 상상으로 키워야겠네요",
          "speaker_id": "2",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027676.1.10",
          "form": "애완견은 책임감도 크게가져야하니까요...",
          "original_form": "애완견은 책임감도 크게가져야하니까요...",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027676.1.11",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027676.1.12",
          "form": "그래도!! 나중에 부자가된다면 이름을 빵이 혹은 몽이라고 지으며 키우고싶어요",
          "original_form": "그래도!! 나중에 부자가된다면 이름을 빵이 혹은 몽이라고 지으며 키우고싶어요",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027676.1.13",
          "form": "키우면 귀엽긴하죠..잘따르고",
          "original_form": "키우면 귀엽긴하죠..잘따르고",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027676.1.14",
          "form": "리트리버도 키우고싶긴한데 앞에 마당이 있어야할것 같아요 크기가 큰 아이라",
          "original_form": "리트리버도 키우고싶긴한데 앞에 마당이 있어야할것 같아요 크기가 큰 아이라",
          "speaker_id": "1",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027676.1.15",
          "form": "그렇겠네요.리트리버 아파트에서 키우시는분봤는데 완전 민폐예요",
          "original_form": "그렇겠네요.리트리버 아파트에서 키우시는분봤는데 완전 민폐예요",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027676.1.16",
          "form": "정말요? 크게 짖나요?",
          "original_form": "정말요? 크게 짖나요?",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027676.1.17",
          "form": "주변사람들이 다 뭐라고 하더군요..짖는소리 어마어마하게크고",
          "original_form": "주변사람들이 다 뭐라고 하더군요..짖는소리 어마어마하게크고",
          "speaker_id": "2",
          "time": "20210902 14:25"
        }
      ]
