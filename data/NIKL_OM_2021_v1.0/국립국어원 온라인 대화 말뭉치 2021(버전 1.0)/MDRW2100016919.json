      "utterance": [
        {
          "id": "MDRW2100016919.1.1",
          "form": "비 많이와서 너무 습하지않나요 ㅠ",
          "original_form": "비 많이와서 너무 습하지않나요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016919.1.2",
          "form": "맞아요 요며칠 너무 습하다가 그나마 오늘은 비가 안와서 덜 습한거같아요",
          "original_form": "맞아요 요며칠 너무 습하다가 그나마 오늘은 비가 안와서 덜 습한거같아요",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016919.1.3",
          "form": "그쵸 그래도 그나마 몇주전보다는 덜더운거같아서 다행이에요",
          "original_form": "그쵸 그래도 그나마 몇주전보다는 덜더운거같아서 다행이에요",
          "speaker_id": "1",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016919.1.4",
          "form": "이제 여름이 지나가고 잇나봐요 ㅋㅋㅋ",
          "original_form": "이제 여름이 지나가고 잇나봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016919.1.5",
          "form": "그러게여ㅋㅋㅋㅋ빨리 가을 왔으면 좋겟어요",
          "original_form": "그러게여ㅋㅋㅋㅋ빨리 가을 왔으면 좋겟어요",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016919.1.6",
          "form": "어느계절좋아하세요?",
          "original_form": "어느계절좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016919.1.7",
          "form": "저는 봄 제일 좋아해요",
          "original_form": "저는 봄 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016919.1.8",
          "form": "저두 봄이랑 가을 좋아해요!",
          "original_form": "저두 봄이랑 가을 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016919.1.9",
          "form": "가을도 좋아요 선선하고 맛잇는 과일 많아서",
          "original_form": "가을도 좋아요 선선하고 맛잇는 과일 많아서",
          "speaker_id": "2",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100016919.1.10",
          "form": "ㅋㅋㅋㅋ가을에 어떤과일이잇죠??",
          "original_form": "ㅋㅋㅋㅋ가을에 어떤과일이잇죠??",
          "speaker_id": "1",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016919.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ전 감말랭이를 좋아해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전 감말랭이를 좋아해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016919.1.12",
          "form": "포도도 좋아하구요",
          "original_form": "포도도 좋아하구요",
          "speaker_id": "2",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016919.1.13",
          "form": "오오 그렇군여 포도가 가을에 나오는구나",
          "original_form": "오오 그렇군여 포도가 가을에 나오는구나",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016919.1.14",
          "form": "가을옷 장만하셧어요?",
          "original_form": "가을옷 장만하셧어요?",
          "speaker_id": "1",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016919.1.15",
          "form": "아니요 ㅋㅋㅋㅋㅋ안그래도 가을옷이 없어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요 ㅋㅋㅋㅋㅋ안그래도 가을옷이 없어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016919.1.16",
          "form": "가을옷 사셧어요?",
          "original_form": "가을옷 사셧어요?",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016919.1.17",
          "form": "아니요ㅋㅋㅋㅋㅋ먼가 계절이 오면 급하게 사는편인거같아요 ㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋㅋ먼가 계절이 오면 급하게 사는편인거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016919.1.18",
          "form": "ㅋㅋㅋㅋㅋ그리고 뭔가 가을은 너무 순식간에 지나가버리는 느낌이라 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그리고 뭔가 가을은 너무 순식간에 지나가버리는 느낌이라 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016919.1.19",
          "form": "아 그건그래요ㅋㅋㅋㅋㅋ살까말까고민하는 이유이기도해요",
          "original_form": "아 그건그래요ㅋㅋㅋㅋㅋ살까말까고민하는 이유이기도해요",
