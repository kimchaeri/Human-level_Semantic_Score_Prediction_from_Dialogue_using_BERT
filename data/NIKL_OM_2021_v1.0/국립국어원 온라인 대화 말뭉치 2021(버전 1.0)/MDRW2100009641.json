      "utterance": [
        {
          "id": "MDRW2100009641.1.1",
          "form": "온라인 커뮤니티 하는거 있나요?",
          "original_form": "온라인 커뮤니티 하는거 있나요?",
          "speaker_id": "1",
          "time": "20210823 02:08"
        },
        {
          "id": "MDRW2100009641.1.2",
          "form": "저는 대학생이라 그냥 에타?정도만 하고 있어요",
          "original_form": "저는 대학생이라 그냥 에타?정도만 하고 있어요",
          "speaker_id": "2",
          "time": "20210823 02:09"
        },
        {
          "id": "MDRW2100009641.1.3",
          "form": "저는 네이버 블로그, 에타해요!",
          "original_form": "저는 네이버 블로그, 에타해요!",
          "speaker_id": "1",
          "time": "20210823 02:09"
        },
        {
          "id": "MDRW2100009641.1.4",
          "form": "저도 대학생이에 ㅛㅎㅎㅎ",
          "original_form": "저도 대학생이에 ㅛㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:09"
        },
        {
          "id": "MDRW2100009641.1.5",
          "form": "name2님 대학 에타는 화력 어느정도 인가요?ㅎㅎ",
          "original_form": "&name2&님 대학 에타는 화력 어느정도 인가요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:10"
        },
        {
          "id": "MDRW2100009641.1.6",
          "form": "뭔가 코로나 전에는 화력이 더 셌던 것 같은데 코로나 터지고 나서 사람들이 학교에 유대감을 가질 일이 거의 없어서 조금 줄은 것 같아요",
          "original_form": "뭔가 코로나 전에는 화력이 더 셌던 것 같은데 코로나 터지고 나서 사람들이 학교에 유대감을 가질 일이 거의 없어서 조금 줄은 것 같아요",
          "speaker_id": "2",
          "time": "20210823 02:12"
        },
        {
          "id": "MDRW2100009641.1.7",
          "form": "그래도 센 편인 것 같아요 ㅎㅎ",
          "original_form": "그래도 센 편인 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 02:12"
        },
        {
          "id": "MDRW2100009641.1.8",
          "form": "그렇죠 저희 에타는 사람이 작아서 그렇게 세진 않아요 ㅎㅎ",
          "original_form": "그렇죠 저희 에타는 사람이 작아서 그렇게 세진 않아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:15"
        },
        {
          "id": "MDRW2100009641.1.9",
          "form": "수강신청 때문에 조금 활발해진 것 같네요",
          "original_form": "수강신청 때문에 조금 활발해진 것 같네요",
          "speaker_id": "1",
          "time": "20210823 02:15"
        },
        {
          "id": "MDRW2100009641.1.10",
          "form": "아 수강신청날 활발하죠",
          "original_form": "아 수강신청날 활발하죠",
          "speaker_id": "2",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009641.1.11",
          "form": "그리고 수강정정기간도..",
          "original_form": "그리고 수강정정기간도..",
          "speaker_id": "2",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009641.1.12",
          "form": "에타가 있어서 그나마 학교 정보 아는 것 같아 ㅎㅎ",
          "original_form": "에타가 있어서 그나마 학교 정보 아는 것 같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009641.1.13",
          "form": "아니면 몰랐을 듯 ㅠ",
          "original_form": "아니면 몰랐을 듯 ㅠ",
          "speaker_id": "1",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009641.1.14",
          "form": "아 맞아요.. 에타에 꿀팁 전수해주시는 분들이 많으셔서..",
          "original_form": "아 맞아요.. 에타에 꿀팁 전수해주시는 분들이 많으셔서..",
          "speaker_id": "2",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009641.1.15",
          "form": "도움이 많이 되는 것 같아요",
          "original_form": "도움이 많이 되는 것 같아요",
          "speaker_id": "2",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009641.1.16",
          "form": "맞아요 속이야기도 많이 읽게 되고",
          "original_form": "맞아요 속이야기도 많이 읽게 되고",
          "speaker_id": "1",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009641.1.17",
          "form": "익명의 힘이 참 대단해요",
          "original_form": "익명의 힘이 참 대단해요",
          "speaker_id": "1",
          "time": "20210823 02:20"
        },
        {
          "id": "MDRW2100009641.1.18",
          "form": "아 맞아요 저도 힘들 때나 화날 때 익명으로 글 자주 써요..ㅋㅋㅋㅋ",
          "original_form": "아 맞아요 저도 힘들 때나 화날 때 익명으로 글 자주 써요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:22"
        },
        {
          "id": "MDRW2100009641.1.19",
          "form": "다른 게시판이 있어서..",
          "original_form": "다른 게시판이 있어서..",
          "speaker_id": "2",
          "time": "20210823 02:22"
        },
        {
          "id": "MDRW2100009641.1.20",
          "form": "앜ㅋㅋㅋㅋ 저는 쓰지는 않고",
          "original_form": "앜ㅋㅋㅋㅋ 저는 쓰지는 않고",
          "speaker_id": "1",
          "time": "20210823 02:22"
        },
        {
          "id": "MDRW2100009641.1.21",
          "form": "눈팅 자주해요",
          "original_form": "눈팅 자주해요",
          "speaker_id": "1",
          "time": "20210823 02:22"
        },
        {
          "id": "MDRW2100009641.1.22",
          "form": "아우성 게시판 이런건가요?",
          "original_form": "아우성 게시판 이런건가요?",
          "speaker_id": "1",
          "time": "20210823 02:22"
        },
        {
          "id": "MDRW2100009641.1.23",
          "form": "네네 뭐 그런 게시판이요",
          "original_form": "네네 뭐 그런 게시판이요",
          "speaker_id": "2",
          "time": "20210823 02:24"
        },
        {
          "id": "MDRW2100009641.1.24",
          "form": "저희 학교는 그런 거 없네요ㅠㅠㅠ",
          "original_form": "저희 학교는 그런 거 없네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009641.1.25",
          "form": "다른 사람들도 많이 써서 제 글도 묻히기 좋은..?",
          "original_form": "다른 사람들도 많이 써서 제 글도 묻히기 좋은..?",
          "speaker_id": "2",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009641.1.26",
          "form": "가끔 연애게시판 가요",
          "original_form": "가끔 연애게시판 가요",
          "speaker_id": "1",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009641.1.27",
          "form": "아아..",
          "original_form": "아아..",
          "speaker_id": "2",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009641.1.28",
          "form": "연애게시판에선 보통 무슨 얘기하나요?",
          "original_form": "연애게시판에선 보통 무슨 얘기하나요?",
          "speaker_id": "2",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009641.1.29",
          "form": "그냥 그런 새벽 감성글 보면 재밌어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 그런 새벽 감성글 보면 재밌어요 ㅋㅋㅋㅋㅋ",
