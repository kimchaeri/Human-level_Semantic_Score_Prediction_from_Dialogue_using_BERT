      "utterance": [
        {
          "id": "MDRW2100034594.1.1",
          "form": "옷은 요즘 뭐입어요?",
          "original_form": "옷은 요즘 뭐입어요?",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034594.1.2",
          "form": "아직 가을 옷 정리를 안 해서 여름 옷 입고 있어요ㅎㅎ 냉장고 바지에 반팔티입어요",
          "original_form": "아직 가을 옷 정리를 안 해서 여름 옷 입고 있어요ㅎㅎ 냉장고 바지에 반팔티입어요",
          "speaker_id": "1",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100034594.1.3",
          "form": "아 저도 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 저도 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100034594.1.4",
          "form": "제 여름 룩이네요",
          "original_form": "제 여름 룩이네요",
          "speaker_id": "2",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034594.1.5",
          "form": "냉장고바지 너무 시원하고 편해요~",
          "original_form": "냉장고바지 너무 시원하고 편해요~",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034594.1.6",
          "form": "맞아요 주머니 있는거 사셨나요 ㅎㅎ",
          "original_form": "맞아요 주머니 있는거 사셨나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034594.1.7",
          "form": "네! 저는 바지는 주머니 꼭 있는 걸로 사요",
          "original_form": "네! 저는 바지는 주머니 꼭 있는 걸로 사요",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034594.1.8",
          "form": "주머니 없으면 너무 불편하죠",
          "original_form": "주머니 없으면 너무 불편하죠",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034594.1.9",
          "form": "가짜 주머니 마주치면 막 짜증이 ;;",
          "original_form": "가짜 주머니 마주치면 막 짜증이 ;;",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034594.1.10",
          "form": "맞아요 화나요ㅜ 그래서 요즘엔 꼭 보고 사요",
          "original_form": "맞아요 화나요ㅜ 그래서 요즘엔 꼭 보고 사요",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034594.1.11",
          "form": "가을에는 주로 어떤 옷 입으세요?",
          "original_form": "가을에는 주로 어떤 옷 입으세요?",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034594.1.12",
          "form": "저 후드 마니아예여",
          "original_form": "저 후드 마니아예여",
          "speaker_id": "2",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100034594.1.13",
          "form": "깔별로 모아두는",
          "original_form": "깔별로 모아두는",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034594.1.14",
          "form": "후드 좋아하시는 군요!",
          "original_form": "후드 좋아하시는 군요!",
          "speaker_id": "1",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100034594.1.15",
          "form": "저는 요즘 후드보다 맨투맨이 더 좋더라고요ㅎㅎ",
          "original_form": "저는 요즘 후드보다 맨투맨이 더 좋더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100034594.1.16",
          "form": "맨투맨은 어디가 좋나요?",
          "original_form": "맨투맨은 어디가 좋나요?",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100034594.1.17",
          "form": "저는 저도 모르게 후드를 자꾸사서 ㅎ..",
          "original_form": "저는 저도 모르게 후드를 자꾸사서 ㅎ..",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100034594.1.18",
          "form": "저는 후드는 목이 좀 불편하더라고요! 그리고 모자 부분이 부피도 크고 세탁했을 때 잘 안 말라서 맨투맨을 더 사게 되더라고요ㅎㅎ",
          "original_form": "저는 후드는 목이 좀 불편하더라고요! 그리고 모자 부분이 부피도 크고 세탁했을 때 잘 안 말라서 맨투맨을 더 사게 되더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100034594.1.19",
          "form": "무슨느낌인지 알것같아요 ㅠㅠ",
          "original_form": "무슨느낌인지 알것같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100034594.1.20",
          "form": "근데 이게 또 익숙해지니까 엄청 안락하게느껴지더라고요",
          "original_form": "근데 이게 또 익숙해지니까 엄청 안락하게느껴지더라고요",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100034594.1.21",
          "form": "후드 쓰고 강의실에서 자면 꿀잠 예약이에요 ^^..",
          "original_form": "후드 쓰고 강의실에서 자면 꿀잠 예약이에요 ^^..",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
