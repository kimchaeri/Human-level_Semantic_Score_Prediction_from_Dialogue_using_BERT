      "utterance": [
        {
          "id": "MDRW2100040213.1.1",
          "form": "아니여~ 아직 안맞았어요!! 백신 접종하셨어용?",
          "original_form": "아니여~ 아직 안맞았어요!! 백신 접종하셨어용?",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100040213.1.2",
          "form": "네 저는 어제 2차접종 했어요",
          "original_form": "네 저는 어제 2차접종 했어요",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100040213.1.3",
          "form": "화이자 맞으셨나요?",
          "original_form": "화이자 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100040213.1.4",
          "form": "네 화이자",
          "original_form": "네 화이자",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100040213.1.5",
          "form": "맞고 어떠세요??",
          "original_form": "맞고 어떠세요??",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100040213.1.6",
          "form": "화이자 2차가 더 힘들대서 걱정했는데",
          "original_form": "화이자 2차가 더 힘들대서 걱정했는데",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100040213.1.7",
          "form": "확실히 1차 때보단 좀 힘든 것 같아요",
          "original_form": "확실히 1차 때보단 좀 힘든 것 같아요",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100040213.1.8",
          "form": "24시간 지나고나니 몸살기가 나타났어요",
          "original_form": "24시간 지나고나니 몸살기가 나타났어요",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100040213.1.9",
          "form": "아고ㅜㅜ",
          "original_form": "아고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 14:47"
        },
        {
          "id": "MDRW2100040213.1.10",
          "form": "1차때는 어떠셨어요??",
          "original_form": "1차때는 어떠셨어요??",
          "speaker_id": "2",
          "time": "20210904 14:47"
        },
        {
          "id": "MDRW2100040213.1.11",
          "form": "보통 접종부위가 붓고 아프다던데..",
          "original_form": "보통 접종부위가 붓고 아프다던데..",
          "speaker_id": "2",
          "time": "20210904 14:48"
        },
        {
          "id": "MDRW2100040213.1.12",
          "form": "1차때는 접종한 쪽 팔만 아팠어요",
          "original_form": "1차때는 접종한 쪽 팔만 아팠어요",
          "speaker_id": "1",
          "time": "20210904 14:48"
        },
        {
          "id": "MDRW2100040213.1.13",
          "form": "한 이틀정도",
          "original_form": "한 이틀정도",
          "speaker_id": "1",
          "time": "20210904 14:48"
        },
        {
          "id": "MDRW2100040213.1.14",
          "form": "이번에 몸살기는 어제부터 있으신거예요?",
          "original_form": "이번에 몸살기는 어제부터 있으신거예요?",
          "speaker_id": "2",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040213.1.15",
          "form": "아뇨 24시간 지난 후부터요",
          "original_form": "아뇨 24시간 지난 후부터요",
          "speaker_id": "1",
          "time": "20210904 18:34"
        },
        {
          "id": "MDRW2100040213.1.16",
          "form": "지금 고생중이신가요ㅠ",
          "original_form": "지금 고생중이신가요ㅠ",
          "speaker_id": "2",
          "time": "20210904 18:52"
        },
        {
          "id": "MDRW2100040213.1.17",
          "form": "이제 좀 나아지고 있어요~",
          "original_form": "이제 좀 나아지고 있어요~",
          "speaker_id": "1",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100040213.1.18",
          "form": "이따가 자기 전에 약 또 먹으려고요",
          "original_form": "이따가 자기 전에 약 또 먹으려고요",
          "speaker_id": "1",
          "time": "20210904 19:06"
        },
        {
          "id": "MDRW2100040213.1.19",
          "form": "다행이에요 ㅜㅜ 부작용무서워서 맞는게 좀 무서워요ㅠ",
          "original_form": "다행이에요 ㅜㅜ 부작용무서워서 맞는게 좀 무서워요ㅠ",
          "speaker_id": "2",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100040213.1.20",
          "form": "이젠 꽤 괜찮아졌어요 저는",
          "original_form": "이젠 꽤 괜찮아졌어요 저는",
          "speaker_id": "1",
          "time": "20210904 21:04"
        }
      ]
