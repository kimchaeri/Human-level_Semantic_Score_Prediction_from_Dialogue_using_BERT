      "utterance": [
        {
          "id": "MDRW2100001482.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 13:01"
        },
        {
          "id": "MDRW2100001482.1.2",
          "form": "네 안녕하세요~~",
          "original_form": "네 안녕하세요~~",
          "speaker_id": "2",
          "time": "20211015 13:01"
        },
        {
          "id": "MDRW2100001482.1.3",
          "form": "이번에 아이폰 13 나온거 보셨나요?",
          "original_form": "이번에 아이폰 13 나온거 보셨나요?",
          "speaker_id": "2",
          "time": "20211015 13:02"
        },
        {
          "id": "MDRW2100001482.1.4",
          "form": "아 네 봤어용",
          "original_form": "아 네 봤어용",
          "speaker_id": "1",
          "time": "20211015 13:03"
        },
        {
          "id": "MDRW2100001482.1.5",
          "form": "또 잘팔릴거 보니까 애플이 참 대단하다 느끼네요",
          "original_form": "또 잘팔릴거 보니까 애플이 참 대단하다 느끼네요",
          "speaker_id": "1",
          "time": "20211015 13:03"
        },
        {
          "id": "MDRW2100001482.1.6",
          "form": "저는 실제로 아는분이 쓰는거 봤는데",
          "original_form": "저는 실제로 아는분이 쓰는거 봤는데",
          "speaker_id": "2",
          "time": "20211015 13:03"
        },
        {
          "id": "MDRW2100001482.1.7",
          "form": "카메라 크기가 어후...",
          "original_form": "카메라 크기가 어후...",
          "speaker_id": "2",
          "time": "20211015 13:03"
        },
        {
          "id": "MDRW2100001482.1.8",
          "form": "진짜 좀 심각하게 크더라고요 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 좀 심각하게 크더라고요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 13:04"
        },
        {
          "id": "MDRW2100001482.1.9",
          "form": "이것도 곧 뇌이징되겠지만..?",
          "original_form": "이것도 곧 뇌이징되겠지만..?",
          "speaker_id": "2",
          "time": "20211015 13:04"
        },
        {
          "id": "MDRW2100001482.1.10",
          "form": "그러게요 애플은 참",
          "original_form": "그러게요 애플은 참",
          "speaker_id": "2",
          "time": "20211015 13:04"
        },
        {
          "id": "MDRW2100001482.1.11",
          "form": "ㅋㅋㅋ그져",
          "original_form": "ㅋㅋㅋ그져",
          "speaker_id": "1",
          "time": "20211015 13:04"
        },
        {
          "id": "MDRW2100001482.1.12",
          "form": "절대 망하지 않을듯요 ㅎ",
          "original_form": "절대 망하지 않을듯요 ㅎ",
          "speaker_id": "2",
          "time": "20211015 13:04"
        },
        {
          "id": "MDRW2100001482.1.13",
          "form": "사실 저도 프로맥스 사려고",
          "original_form": "사실 저도 프로맥스 사려고",
          "speaker_id": "2",
          "time": "20211015 13:04"
        },
        {
          "id": "MDRW2100001482.1.14",
          "form": "대기타고있어요",
          "original_form": "대기타고있어요",
          "speaker_id": "2",
          "time": "20211015 13:04"
        },
        {
          "id": "MDRW2100001482.1.15",
          "form": "키킼",
          "original_form": "키킼",
          "speaker_id": "2",
          "time": "20211015 13:04"
        },
        {
          "id": "MDRW2100001482.1.16",
          "form": "ㅋㅋㅋㅋ아 다들 애플 욕하면서",
          "original_form": "ㅋㅋㅋㅋ아 다들 애플 욕하면서",
          "speaker_id": "1",
          "time": "20211015 13:05"
        },
        {
          "id": "MDRW2100001482.1.17",
          "form": "갖고는 싶어할때",
          "original_form": "갖고는 싶어할때",
          "speaker_id": "1",
          "time": "20211015 13:05"
        },
        {
          "id": "MDRW2100001482.1.18",
          "form": "샀어야하는데 애플주식",
          "original_form": "샀어야하는데 애플주식",
          "speaker_id": "1",
          "time": "20211015 13:05"
        },
        {
          "id": "MDRW2100001482.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 웃곀ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 웃곀ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 13:05"
        },
        {
          "id": "MDRW2100001482.1.20",
          "form": "결국..",
          "original_form": "결국..",
          "speaker_id": "2",
          "time": "20211015 13:05"
        },
        {
          "id": "MDRW2100001482.1.21",
          "form": "그 때 샀어야했는데..! 로 귀결되는",
          "original_form": "그 때 샀어야했는데..! 로 귀결되는",
          "speaker_id": "2",
          "time": "20211015 13:05"
        },
        {
          "id": "MDRW2100001482.1.22",
          "form": "답은 미국주식인가..",
          "original_form": "답은 미국주식인가..",
          "speaker_id": "2",
          "time": "20211015 13:05"
        },
        {
          "id": "MDRW2100001482.1.23",
          "form": "님은 갤럭시 쓰세요?",
          "original_form": "님은 갤럭시 쓰세요?",
          "speaker_id": "2",
          "time": "20211015 13:05"
        },
        {
          "id": "MDRW2100001482.1.24",
          "form": "네 저는 갤럭시노트 써요",
          "original_form": "네 저는 갤럭시노트 써요",
          "speaker_id": "1",
          "time": "20211015 13:05"
        },
        {
          "id": "MDRW2100001482.1.25",
          "form": "삼성페이랑 기본용량 큰거때문에 못버리겠어요",
          "original_form": "삼성페이랑 기본용량 큰거때문에 못버리겠어요",
          "speaker_id": "1",
          "time": "20211015 13:06"
        },
        {
          "id": "MDRW2100001482.1.26",
          "form": "오 저도 지금 폰은 노트 10 플러스에요",
          "original_form": "오 저도 지금 폰은 노트 10 플러스에요",
          "speaker_id": "2",
          "time": "20211015 13:06"
        },
        {
          "id": "MDRW2100001482.1.27",
          "form": "아 인정 ㅎㅎ 저도 지금 폰 쓰묜서",
          "original_form": "아 인정 ㅎㅎ 저도 지금 폰 쓰묜서",
          "speaker_id": "2",
          "time": "20211015 13:06"
        },
        {
          "id": "MDRW2100001482.1.28",
          "form": "용량 걱정은 안해봤네용",
          "original_form": "용량 걱정은 안해봤네용",
          "speaker_id": "2",
          "time": "20211015 13:06"
        },
        {
          "id": "MDRW2100001482.1.29",
          "form": "오 ㅋㅋ저도 노트10인데",
          "original_form": "오 ㅋㅋ저도 노트10인데",
          "speaker_id": "1",
          "time": "20211015 13:07"
        },
        {
          "id": "MDRW2100001482.1.30",
          "form": "노트10만한게 없는듯..",
          "original_form": "노트10만한게 없는듯..",
          "speaker_id": "1",
          "time": "20211015 13:07"
        },
        {
          "id": "MDRW2100001482.1.31",
          "form": "솔직히 디자인도 이쁘고",
          "original_form": "솔직히 디자인도 이쁘고",
          "speaker_id": "2",
          "time": "20211015 13:08"
        },
        {
          "id": "MDRW2100001482.1.32",
          "form": "3년 썼는데",
          "original_form": "3년 썼는데",
          "speaker_id": "2",
          "time": "20211015 13:08"
        },
        {
          "id": "MDRW2100001482.1.33",
          "form": "2년 정도 더 쓰기 가능..",
          "original_form": "2년 정도 더 쓰기 가능..",
          "speaker_id": "2",
          "time": "20211015 13:08"
        },
        {
          "id": "MDRW2100001482.1.34",
          "form": "ㅋㅋ저두요",
          "original_form": "ㅋㅋ저두요",
          "speaker_id": "1",
          "time": "20211015 13:08"
        },
        {
          "id": "MDRW2100001482.1.35",
          "form": "오히려 폰바꾸신다는 분들 노트10이전 세대면",
          "original_form": "오히려 폰바꾸신다는 분들 노트10이전 세대면",
          "speaker_id": "1",
          "time": "20211015 13:08"
        },
        {
          "id": "MDRW2100001482.1.36",
          "form": "지금 노트10사도 한 2년쓰니까 중고로 싼거사라고",
          "original_form": "지금 노트10사도 한 2년쓰니까 중고로 싼거사라고",
          "speaker_id": "1",
          "time": "20211015 13:09"
        },
        {
          "id": "MDRW2100001482.1.37",
          "form": "추천줌",
          "original_form": "추천줌",
          "speaker_id": "1",
          "time": "20211015 13:09"
        },
        {
          "id": "MDRW2100001482.1.38",
          "form": "호오 맞아요 그게 현명한거같기도 ㅎㅎㅎㅎ\r\n",
