        {
          "id": "MDRW2100020563.1.1",
          "form": "빠바는 그걸 드셔야해요",
          "original_form": "빠바는 그걸 드셔야해요",
          "speaker_id": "2",
          "time": "20210828 09:48"
        },
        {
          "id": "MDRW2100020563.1.2",
          "form": "어떤거죠",
          "original_form": "어떤거죠",
          "speaker_id": "1",
          "time": "20210828 09:49"
        },
        {
          "id": "MDRW2100020563.1.3",
          "form": "마카롱 아이스크림...",
          "original_form": "마카롱 아이스크림...",
          "speaker_id": "2",
          "time": "20210828 09:49"
        },
        {
          "id": "MDRW2100020563.1.4",
          "form": "블루베리맛...",
          "original_form": "블루베리맛...",
          "speaker_id": "2",
          "time": "20210828 09:49"
        },
        {
          "id": "MDRW2100020563.1.5",
          "form": "와 아이스크림 맛집인거 아시는구나",
          "original_form": "와 아이스크림 맛집인거 아시는구나",
          "speaker_id": "1",
          "time": "20210828 09:49"
        },
        {
          "id": "MDRW2100020563.1.6",
          "form": "디들 간과하기 쉬운데",
          "original_form": "디들 간과하기 쉬운데",
          "speaker_id": "2",
          "time": "20210828 09:49"
        },
        {
          "id": "MDRW2100020563.1.7",
          "form": "빠바는 마카롱 아이스크림 맛집이랍니다!",
          "original_form": "빠바는 마카롱 아이스크림 맛집이랍니다!",
          "speaker_id": "2",
          "time": "20210828 09:49"
        },
        {
          "id": "MDRW2100020563.1.8",
          "form": "파바 아이스크림 맛집이라고 말하고다니는데 모르시는분 많더라구요",
          "original_form": "파바 아이스크림 맛집이라고 말하고다니는데 모르시는분 많더라구요",
          "speaker_id": "1",
          "time": "20210828 09:49"
        },
        {
          "id": "MDRW2100020563.1.9",
          "form": "역시 name1님은 알고계셨군요!",
          "original_form": "역시 &name1&님은 알고계셨군요!",
          "speaker_id": "2",
          "time": "20210828 09:49"
        },
        {
          "id": "MDRW2100020563.1.10",
          "form": "소름",
          "original_form": "소름",
          "speaker_id": "2",
          "time": "20210828 09:49"
        },
        {
          "id": "MDRW2100020563.1.11",
          "form": "그리고 샌드위치도 좋아해요",
          "original_form": "그리고 샌드위치도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 09:50"
        },
        {
          "id": "MDRW2100020563.1.12",
          "form": "비싸서 자주는 안먹어요",
          "original_form": "비싸서 자주는 안먹어요",
          "speaker_id": "2",
          "time": "20210828 09:50"
        },
        {
          "id": "MDRW2100020563.1.13",
          "form": "잠시만요 아이스크림 얘기좀 할게요 별거없지만",
          "original_form": "잠시만요 아이스크림 얘기좀 할게요 별거없지만",
          "speaker_id": "1",
          "time": "20210828 09:51"
        },
        {
          "id": "MDRW2100020563.1.14",
          "form": "처음에",
          "original_form": "처음에",
