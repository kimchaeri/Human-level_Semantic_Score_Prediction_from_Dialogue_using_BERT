      "utterance": [
        {
          "id": "MDRW2100026350.1.1",
          "form": "안녕하세요!가장 좋아하는 음식이 뭐에요?",
          "original_form": "안녕하세요!가장 좋아하는 음식이 뭐에요?",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026350.1.2",
          "form": "매운음식이나 회요",
          "original_form": "매운음식이나 회요",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026350.1.3",
          "form": "엽기떡볶이 조아하시겠어요",
          "original_form": "엽기떡볶이 조아하시겠어요",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026350.1.4",
          "form": "오 한창 많이 먹엇어요!",
          "original_form": "오 한창 많이 먹엇어요!",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026350.1.5",
          "form": "엽떡에서도 로제가 나왔던데 먹어보고싶어요",
          "original_form": "엽떡에서도 로제가 나왔던데 먹어보고싶어요",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026350.1.6",
          "form": "맛잇다는거같던데요?",
          "original_form": "맛잇다는거같던데요?",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026350.1.7",
          "form": "네 평이 좋더라구요",
          "original_form": "네 평이 좋더라구요",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026350.1.8",
          "form": "다른브랜드 로제는 먹다가 너무 느끼해서...",
          "original_form": "다른브랜드 로제는 먹다가 너무 느끼해서...",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026350.1.9",
          "form": "저도 다른데서 먹엇는데 첨에만 맛잇고..",
          "original_form": "저도 다른데서 먹엇는데 첨에만 맛잇고..",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026350.1.10",
          "form": "뒤로갈수록 핵느끼..김치가 필요한맛ㅜㅜ",
          "original_form": "뒤로갈수록 핵느끼..김치가 필요한맛ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026350.1.11",
          "form": "다남겼어요...내돈",
          "original_form": "다남겼어요...내돈",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026350.1.12",
          "form": "젤 아깝져ㅜㅜ",
          "original_form": "젤 아깝져ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026350.1.13",
          "form": "그니까용ㅠㅜ담엔 엽떡로제로 도전해볼라구여",
          "original_form": "그니까용ㅠㅜ담엔 엽떡로제로 도전해볼라구여",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026350.1.14",
          "form": "덜느끼하대요",
          "original_form": "덜느끼하대요",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026350.1.15",
          "form": "양을 1인분만 팔면 도전해보고싶네요ㅋ",
          "original_form": "양을 1인분만 팔면 도전해보고싶네요ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026350.1.16",
          "form": "하긴 그것도 문제네요..양이 넘 많고 비싼것ㅜㅜ",
          "original_form": "하긴 그것도 문제네요..양이 넘 많고 비싼것ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        }
      ]
