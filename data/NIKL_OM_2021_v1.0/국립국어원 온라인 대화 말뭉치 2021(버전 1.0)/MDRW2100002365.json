        {
          "id": "MDRW2100002365.1.1",
          "form": "요즘",
          "original_form": "요즘",
          "speaker_id": "1",
          "time": "20210820 10:29"
        },
        {
          "id": "MDRW2100002365.1.2",
          "form": "월세 때문에 돈이 안모인다",
          "original_form": "월세 때문에 돈이 안모인다",
          "speaker_id": "1",
          "time": "20210820 10:30"
        },
        {
          "id": "MDRW2100002365.1.3",
          "form": "ㄷㄷ얼마씩이야?",
          "original_form": "ㄷㄷ얼마씩이야?",
          "speaker_id": "2",
          "time": "20210820 10:30"
        },
        {
          "id": "MDRW2100002365.1.4",
          "form": "청약은 어케됐어?",
          "original_form": "청약은 어케됐어?",
          "speaker_id": "2",
          "time": "20210820 10:30"
        },
        {
          "id": "MDRW2100002365.1.5",
          "form": "55만원",
          "original_form": "55만원",
          "speaker_id": "1",
          "time": "20210820 10:34"
        },
        {
          "id": "MDRW2100002365.1.6",
          "form": "청약은 됐는데",
          "original_form": "청약은 됐는데",
          "speaker_id": "1",
          "time": "20210820 10:34"
        },
        {
          "id": "MDRW2100002365.1.7",
          "form": "2023년 5월 이후입주야",
          "original_form": "2023년 5월 이후입주야",
          "speaker_id": "1",
          "time": "20210820 10:34"
        },
        {
          "id": "MDRW2100002365.1.8",
          "form": "좋은데로 잘 됐아?",
          "original_form": "좋은데로 잘 됐아?",
          "speaker_id": "2",
          "time": "20210820 10:35"
        },
        {
          "id": "MDRW2100002365.1.9",
          "form": "신혼특약? 생애첫주택?",
          "original_form": "신혼특약? 생애첫주택?",
          "speaker_id": "2",
          "time": "20210820 10:35"
        },
        {
          "id": "MDRW2100002365.1.10",
          "form": "일반",
          "original_form": "일반",
          "speaker_id": "1",
          "time": "20210820 10:35"
        },
        {
          "id": "MDRW2100002365.1.11",
          "form": "너 청약했어?",
          "original_form": "너 청약했어?",
          "speaker_id": "1",
          "time": "20210820 10:35"
        },
        {
          "id": "MDRW2100002365.1.12",
          "form": "아니 나 세대주가 아니라 못해",
          "original_form": "아니 나 세대주가 아니라 못해",
          "speaker_id": "2",
          "time": "20210820 10:36"
        },
        {
          "id": "MDRW2100002365.1.13",
          "form": "아파트는 분리세대주가 안되수",
          "original_form": "아파트는 분리세대주가 안되수",
          "speaker_id": "2",
          "time": "20210820 10:36"
        },
        {
          "id": "MDRW2100002365.1.14",
          "form": "단독주탣 찾아보고 있어",
          "original_form": "단독주탣 찾아보고 있어",
          "speaker_id": "2",
          "time": "20210820 10:36"
        },
        {
          "id": "MDRW2100002365.1.15",
          "form": "아니 세대주가 아니면 청약을 못해?",
          "original_form": "아니 세대주가 아니면 청약을 못해?",
          "speaker_id": "1",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002365.1.16",
          "form": "어 자격이 없어",
          "original_form": "어 자격이 없어",
          "speaker_id": "2",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002365.1.17",
          "form": "아 그건 몰랐네",
          "original_form": "아 그건 몰랐네",
          "speaker_id": "1",
          "time": "20210820 10:41"
        },
        {
          "id": "MDRW2100002365.1.18",
          "form": "일하고있나",
          "original_form": "일하고있나",
          "speaker_id": "1",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002365.1.19",
          "form": "아니 과장님이 커피타줘서 마시고있엉",
          "original_form": "아니 과장님이 커피타줘서 마시고있엉",
          "speaker_id": "2",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002365.1.20",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002365.1.21",
          "form": "오후에 나간다며",
          "original_form": "오후에 나간다며",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002365.1.22",
          "form": "연장되서",
          "original_form": "연장되서",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002365.1.23",
          "form": "담주에 가기로했어",
          "original_form": "담주에 가기로했어",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002365.1.24",
          "form": "아하 굳굳",
          "original_form": "아하 굳굳",
          "speaker_id": "1",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002365.1.25",
          "form": "오늘은 몇 시 퇴근",
          "original_form": "오늘은 몇 시 퇴근",
