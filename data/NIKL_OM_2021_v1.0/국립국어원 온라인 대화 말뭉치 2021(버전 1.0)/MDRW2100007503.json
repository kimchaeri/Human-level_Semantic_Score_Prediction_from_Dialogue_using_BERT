      "utterance": [
        {
          "id": "MDRW2100007503.1.1",
          "form": "그 어린이집 좀 별로인거같아요",
          "original_form": "그 어린이집 좀 별로인거같아요",
          "speaker_id": "1",
          "time": "20210822 03:53"
        },
        {
          "id": "MDRW2100007503.1.2",
          "form": "나라님 덕분에 스트레스가 풀렸어요",
          "original_form": "나라님 덕분에 스트레스가 풀렸어요",
          "speaker_id": "2",
          "time": "20210822 03:53"
        },
        {
          "id": "MDRW2100007503.1.3",
          "form": "익명의 누구한테 이것저것",
          "original_form": "익명의 누구한테 이것저것",
          "speaker_id": "2",
          "time": "20210822 03:53"
        },
        {
          "id": "MDRW2100007503.1.4",
          "form": "못걸으니 자기들이 힘드니까 자꾸 비교하고",
          "original_form": "못걸으니 자기들이 힘드니까 자꾸 비교하고",
          "speaker_id": "1",
          "time": "20210822 03:53"
        },
        {
          "id": "MDRW2100007503.1.5",
          "form": "맞아요 힘들데요",
          "original_form": "맞아요 힘들데요",
          "speaker_id": "2",
          "time": "20210822 03:53"
        },
        {
          "id": "MDRW2100007503.1.6",
          "form": "애기 넘어질까봐 봐줘야된다고",
          "original_form": "애기 넘어질까봐 봐줘야된다고",
          "speaker_id": "2",
          "time": "20210822 03:54"
        },
        {
          "id": "MDRW2100007503.1.7",
          "form": "뭐붙잡고 일어나거든요",
          "original_form": "뭐붙잡고 일어나거든요",
          "speaker_id": "2",
          "time": "20210822 03:54"
        },
        {
          "id": "MDRW2100007503.1.8",
          "form": "얼른 옮기셔요 ㅎ",
          "original_form": "얼른 옮기셔요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 03:54"
        },
        {
          "id": "MDRW2100007503.1.9",
          "form": "대기중이예요 내년초에나 가능하데요",
          "original_form": "대기중이예요 내년초에나 가능하데요",
          "speaker_id": "2",
          "time": "20210822 03:55"
        },
        {
          "id": "MDRW2100007503.1.10",
          "form": "군립은3년기다려야된데서 국립3군데 걸었어요",
          "original_form": "군립은3년기다려야된데서 국립3군데 걸었어요",
          "speaker_id": "2",
          "time": "20210822 03:55"
        },
        {
          "id": "MDRW2100007503.1.11",
          "form": "에고 대기도 어마어마하네요",
          "original_form": "에고 대기도 어마어마하네요",
          "speaker_id": "1",
          "time": "20210822 03:55"
        },
        {
          "id": "MDRW2100007503.1.12",
          "form": "0세반은 치열해요",
          "original_form": "0세반은 치열해요",
          "speaker_id": "2",
          "time": "20210822 03:57"
        },
        {
          "id": "MDRW2100007503.1.13",
          "form": "언니네얼집은3개월도있다니ㅠㅠ",
          "original_form": "언니네얼집은3개월도있다니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:57"
        },
        {
          "id": "MDRW2100007503.1.14",
          "form": "주제가 학교네요\r\n학교다닐 기회생기면 다니실거예요?",
          "original_form": "주제가 학교네요\r\n학교다닐 기회생기면 다니실거예요?",
          "speaker_id": "2",
          "time": "20210822 03:58"
        },
        {
          "id": "MDRW2100007503.1.15",
          "form": "새로운 길이 열린다면요 ㅎㅎ",
          "original_form": "새로운 길이 열린다면요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 03:58"
        },
        {
          "id": "MDRW2100007503.1.16",
          "form": "학업스트레스는 있겠지만 머리도 굳어서 안따라주고 ㅎㅎ",
          "original_form": "학업스트레스는 있겠지만 머리도 굳어서 안따라주고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 03:59"
        },
        {
          "id": "MDRW2100007503.1.17",
          "form": "그래도 이직에 도움이 된다면 다닐것같아요",
          "original_form": "그래도 이직에 도움이 된다면 다닐것같아요",
          "speaker_id": "1",
          "time": "20210822 03:59"
        },
        {
          "id": "MDRW2100007503.1.18",
          "form": "혹시 뭐 관심있어요?",
          "original_form": "혹시 뭐 관심있어요?",
          "speaker_id": "2",
          "time": "20210822 04:01"
        },
        {
          "id": "MDRW2100007503.1.19",
          "form": "저는 뭔가 마케팅쪽",
          "original_form": "저는 뭔가 마케팅쪽",
          "speaker_id": "2",
          "time": "20210822 04:01"
        },
        {
          "id": "MDRW2100007503.1.20",
          "form": "마케팅이 말이 마케팅이지\r\n영업이라는데 맞나요",
          "original_form": "마케팅이 말이 마케팅이지\r\n영업이라는데 맞나요",
          "speaker_id": "2",
          "time": "20210822 04:02"
        },
        {
          "id": "MDRW2100007503.1.21",
          "form": "디자인하는거 좋아하고..",
          "original_form": "디자인하는거 좋아하고..",
          "speaker_id": "2",
          "time": "20210822 04:02"
        },
        {
          "id": "MDRW2100007503.1.22",
          "form": "전공이랑 관심이 너무 다르죠ㅋㅋ",
          "original_form": "전공이랑 관심이 너무 다르죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 04:02"
        },
        {
          "id": "MDRW2100007503.1.23",
          "form": "네 영업이죠 마케팅",
          "original_form": "네 영업이죠 마케팅",
          "speaker_id": "1",
          "time": "20210822 04:05"
        },
        {
          "id": "MDRW2100007503.1.24",
          "form": "잘팔리기위한거가 기본이니",
          "original_form": "잘팔리기위한거가 기본이니",
          "speaker_id": "1",
          "time": "20210822 04:05"
        },
        {
          "id": "MDRW2100007503.1.25",
          "form": "디자인하는거랑은 또 다른측면인데요?",
          "original_form": "디자인하는거랑은 또 다른측면인데요?",
          "speaker_id": "1",
          "time": "20210822 04:05"
        },
        {
          "id": "MDRW2100007503.1.26",
          "form": "관심만 가지고 도전하기에는 어리지가 않네요",
          "original_form": "관심만 가지고 도전하기에는 어리지가 않네요",
          "speaker_id": "2",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007503.1.27",
          "form": "네 로고만들고 시각디자인쪽 관심있었어요!!",
          "original_form": "네 로고만들고 시각디자인쪽 관심있었어요!!",
          "speaker_id": "2",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007503.1.28",
          "form": "와 재능이 많으시네요",
          "original_form": "와 재능이 많으시네요",
          "speaker_id": "1",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007503.1.29",
          "form": "취업힘들데서 안하긴했는데 그때했으면 재밌었을것같아요",
          "original_form": "취업힘들데서 안하긴했는데 그때했으면 재밌었을것같아요",
          "speaker_id": "2",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007503.1.30",
          "form": "외가가 다 예술해요",
          "original_form": "외가가 다 예술해요",
          "speaker_id": "2",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007503.1.31",
          "form": "손으로하는거 잘하고",
          "original_form": "손으로하는거 잘하고",
          "speaker_id": "2",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007503.1.32",
          "form": "운동못하고ㅋㅋ",
          "original_form": "운동못하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007503.1.33",
          "form": "와 그럼 쭉해보시지...",
          "original_form": "와 그럼 쭉해보시지...",
          "speaker_id": "1",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007503.1.34",
          "form": "석사말고 디자인 해보세요 ㅎㅎ",
          "original_form": "석사말고 디자인 해보세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007503.1.35",
          "form": "뭔가 애기가 생기니까 위험한거는 도전을 안하게되는거같아요",
          "original_form": "뭔가 애기가 생기니까 위험한거는 도전을 안하게되는거같아요",
          "speaker_id": "2",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007503.1.36",
          "form": "적성에 맞는거 해야 잼나죠",
          "original_form": "적성에 맞는거 해야 잼나죠",
          "speaker_id": "1",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007503.1.37",
          "form": "괜찮은 직업갖고싶고",
          "original_form": "괜찮은 직업갖고싶고",
          "speaker_id": "2",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007503.1.38",
          "form": "공무원이 짱이긴한데\r\n진득하앉아서 공부할스타일이 못되요ㅋㅋ",
          "original_form": "공무원이 짱이긴한데\r\n진득하앉아서 공부할스타일이 못되요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007503.1.39",
          "form": "ㅎㅎㅎ 공무원도 그리쉬운건 아닌거같아요",
          "original_form": "ㅎㅎㅎ 공무원도 그리쉬운건 아닌거같아요",
          "speaker_id": "1",
          "time": "20210822 04:09"
        },
        {
          "id": "MDRW2100007503.1.40",
          "form": "동생은 행정과라 공시생이예요\r\n아직 대학생인데 학교를 안가서\r\n새벽부터 밤까지 공부만하는데\r\n얘처럼 해야 합격하지 않을까싶어서 저는 아닌것같아요ㅋㅋ",
          "original_form": "동생은 행정과라 공시생이예요\r\n아직 대학생인데 학교를 안가서\r\n새벽부터 밤까지 공부만하는데\r\n얘처럼 해야 합격하지 않을까싶어서 저는 아닌것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 04:09"
        },
        {
          "id": "MDRW2100007503.1.41",
          "form": "핸드폰도없애고",
          "original_form": "핸드폰도없애고",
          "speaker_id": "2",
          "time": "20210822 04:10"
        },
        {
          "id": "MDRW2100007503.1.42",
          "form": "사촌이 공무원인데 여느 워킹맘과같더라구요",
          "original_form": "사촌이 공무원인데 여느 워킹맘과같더라구요",
          "speaker_id": "1",
          "time": "20210822 04:10"
        },
        {
          "id": "MDRW2100007503.1.43",
          "form": "아 부러워요! 절대 짤리지않는 정규직",
          "original_form": "아 부러워요! 절대 짤리지않는 정규직",
          "speaker_id": "2",
          "time": "20210822 04:10"
        },
        {
          "id": "MDRW2100007503.1.44",
          "form": "아침일찍 애들보내고 6시땡해서 일어나서 나와야 차안막힌다고",
          "original_form": "아침일찍 애들보내고 6시땡해서 일어나서 나와야 차안막힌다고",
          "speaker_id": "1",
          "time": "20210822 04:10"
        },
        {
          "id": "MDRW2100007503.1.45",
          "form": "저도 정규직이지만 대우가 ㅎㅎ",
          "original_form": "저도 정규직이지만 대우가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 04:10"
        },
        {
          "id": "MDRW2100007503.1.46",
          "form": "집에오면 7시 ㅎㅎ",
          "original_form": "집에오면 7시 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 04:10"
        },
        {
          "id": "MDRW2100007503.1.47",
          "form": "친구는 공대졸업하고 일하다가 갑자기 의대편입했는데 진짜 대단한거같아요",
          "original_form": "친구는 공대졸업하고 일하다가 갑자기 의대편입했는데 진짜 대단한거같아요",
          "speaker_id": "2",
          "time": "20210822 04:11"
        },
        {
          "id": "MDRW2100007503.1.48",
          "form": "애셋 먹이고 뒷정리하고 막내, 씻기고 ㅎㅎ",
          "original_form": "애셋 먹이고 뒷정리하고 막내, 씻기고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 04:11"
        },
        {
          "id": "MDRW2100007503.1.49",
          "form": "대단한 결심이네요",
          "original_form": "대단한 결심이네요",
          "speaker_id": "1",
          "time": "20210822 04:12"
        },
        {
          "id": "MDRW2100007503.1.50",
          "form": "이제 시작이면 늦은편이긴한데 미래생각하면 좋은직업이잖아요?",
          "original_form": "이제 시작이면 늦은편이긴한데 미래생각하면 좋은직업이잖아요?",
          "speaker_id": "2",
          "time": "20210822 04:12"
        },
        {
          "id": "MDRW2100007503.1.51",
          "form": "뭔가 저도 미래에도 좋은 직업을 갖길원해요",
          "original_form": "뭔가 저도 미래에도 좋은 직업을 갖길원해요",
          "speaker_id": "2",
          "time": "20210822 04:12"
        },
        {
          "id": "MDRW2100007503.1.52",
          "form": "나라님은 맞으신가요?",
          "original_form": "나라님은 맞으신가요?",
          "speaker_id": "2",
          "time": "20210822 04:13"
        },
        {
          "id": "MDRW2100007503.1.53",
          "form": "지금처럼 고민하시는거보니",
          "original_form": "지금처럼 고민하시는거보니",
          "speaker_id": "1",
          "time": "20210822 04:13"
        },
        {
          "id": "MDRW2100007503.1.54",
          "form": "좋은직업 찾으실듯행ㆍ",
          "original_form": "좋은직업 찾으실듯행ㆍ",
          "speaker_id": "1",
          "time": "20210822 04:13"
        },
        {
          "id": "MDRW2100007503.1.55",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210822 04:13"
        },
        {
