        },
        {
          "id": "MDRW2100009509.1.2",
          "form": "30년 넘게 살면서",
          "original_form": "30년 넘게 살면서",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009509.1.3",
          "form": "저희 엄마도 저땜에 욕이 느셨죠",
          "original_form": "저희 엄마도 저땜에 욕이 느셨죠",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009509.1.4",
          "form": "와 내가 이런 괴성을?! ㅋㅋㅋㅋㅋㅈ",
          "original_form": "와 내가 이런 괴성을?! ㅋㅋㅋㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009509.1.5",
          "form": "이새.끼 는 기본....",
          "original_form": "이새.끼 는 기본....",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009509.1.6",
          "form": "쌍욕도 하시던가요?",
          "original_form": "쌍욕도 하시던가요?",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009509.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009509.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009509.1.9",
          "form": "ㅠㅠ 갑자기 불안해지네요",
          "original_form": "ㅠㅠ 갑자기 불안해지네요",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009509.1.10",
          "form": "욕하는 헐크가 될까봐",
          "original_form": "욕하는 헐크가 될까봐",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009509.1.11",
          "form": "아직 욕은 안하시죠?",
          "original_form": "아직 욕은 안하시죠?",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009509.1.12",
          "form": "더 크면...  말 더 안듣고...",
          "original_form": "더 크면...  말 더 안듣고...",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009509.1.13",
          "form": "욕이 튀어나오는...",
          "original_form": "욕이 튀어나오는...",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009509.1.14",
          "form": "네ㅠㅠ 아직 욕은 안해요ㅋㅋ",
          "original_form": "네ㅠㅠ 아직 욕은 안해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009509.1.15",
          "form": "아니 더 크면은\r\n말을 점점 더 알아들으니\r\n말을 잘 들어야 되는거 아닌가여ㅠ",
          "original_form": "아니 더 크면은\r\n말을 점점 더 알아들으니\r\n말을 잘 들어야 되는거 아닌가여ㅠ",
          "speaker_id": "1",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009509.1.16",
          "form": "name2이님이 잘못했네ㅜㅜ",
          "original_form": "&name2&이님이 잘못했네ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009509.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009509.1.18",
          "form": "겪어보시죠",
          "original_form": "겪어보시죠",
          "speaker_id": "2",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009509.1.19",
          "form": "아들마다 다른지...",
          "original_form": "아들마다 다른지...",
          "speaker_id": "2",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009509.1.20",
          "form": "아아아아아\r\n안들린다아아아아",
          "original_form": "아아아아아\r\n안들린다아아아아",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009509.1.21",
          "form": "그거 아세요?",
          "original_form": "그거 아세요?",
          "speaker_id": "2",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009509.1.22",
          "form": "저희집은요",
          "original_form": "저희집은요",
          "speaker_id": "2",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009509.1.23",
          "form": "아들이 저 하나에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아들이 저 하나에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009509.1.24",
          "form": "제 밑에 여동생인데",
          "original_form": "제 밑에 여동생인데",
          "speaker_id": "2",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009509.1.25",
          "form": "엄마는 욕이 느셨어요.....",
          "original_form": "엄마는 욕이 느셨어요.....",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009509.1.26",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009509.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009509.1.28",
          "form": "반전이네요 ㅋㅋㅋㅋ",
          "original_form": "반전이네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009509.1.29",
          "form": "화이팅입니다",
          "original_form": "화이팅입니다",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009509.1.30",
          "form": "병주고 약주시는건가여ㅜㅜ",
          "original_form": "병주고 약주시는건가여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009509.1.31",
          "form": "응원하는거죠",
          "original_form": "응원하는거죠",
          "speaker_id": "2",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009509.1.32",
          "form": "여자여자 하시다고 하셨으니",
          "original_form": "여자여자 하시다고 하셨으니",
          "speaker_id": "2",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009509.1.33",
          "form": "욕은 안하실듯",
          "original_form": "욕은 안하실듯",
          "speaker_id": "2",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009509.1.34",
          "form": "저희 엄마는 아주 직설적이셔서",
          "original_form": "저희 엄마는 아주 직설적이셔서",
          "speaker_id": "2",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009509.1.35",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009509.1.36",
          "form": "저희 엄마도\r\n오빠랑 저 키우셨는데",
          "original_form": "저희 엄마도\r\n오빠랑 저 키우셨는데",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009509.1.37",
          "form": "세상 좋은 엄마인데\r\n아주 가끔 욕하셨던것도 같네요 ㅋㅋㅋ",
          "original_form": "세상 좋은 엄마인데\r\n아주 가끔 욕하셨던것도 같네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009509.1.38",
          "form": "결국 저도 그렇게\r\n엄마가 되어가는걸까요",
          "original_form": "결국 저도 그렇게\r\n엄마가 되어가는걸까요",
          "speaker_id": "1",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100009509.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100009509.1.40",
          "form": "그렇게 엄마를 이해하게 되는건가요",
          "original_form": "그렇게 엄마를 이해하게 되는건가요",
          "speaker_id": "2",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100009509.1.41",
          "form": "근데 확실한건\r\n엄마보다 더 좋은 엄마는\r\n될 수 없단거 같아요",
          "original_form": "근데 확실한건\r\n엄마보다 더 좋은 엄마는\r\n될 수 없단거 같아요",
          "speaker_id": "1",
          "time": "20210823 00:16"
        },
        {
          "id": "MDRW2100009509.1.42",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100009509.1.43",
          "form": "나는 엄마이지만\r\n여전히 엄마가 필요한\r\n엄마니까요 :)",
          "original_form": "나는 엄마이지만\r\n여전히 엄마가 필요한\r\n엄마니까요 :)",
