      "utterance": [
        {
          "id": "MDRW2100020585.1.1",
          "form": "안녕하세용 아침은 드셨어요?",
          "original_form": "안녕하세용 아침은 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020585.1.2",
          "form": "네 먹었어요 단호박죽 먹었어요 ㅎㅎ 아침 드셨어요??",
          "original_form": "네 먹었어요 단호박죽 먹었어요 ㅎㅎ 아침 드셨어요??",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020585.1.3",
          "form": "저는 아직 안먹었는데",
          "original_form": "저는 아직 안먹었는데",
          "speaker_id": "1",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020585.1.4",
          "form": "아점으로 빵 먹으려고",
          "original_form": "아점으로 빵 먹으려고",
          "speaker_id": "1",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020585.1.5",
          "form": "오픈까지 기다리구있어용",
          "original_form": "오픈까지 기다리구있어용",
          "speaker_id": "1",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020585.1.6",
          "form": "빵집오픈기다리시는거에요? 무슨빵 드시려구요??",
          "original_form": "빵집오픈기다리시는거에요? 무슨빵 드시려구요??",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020585.1.7",
          "form": "네네 요기요 파리바게트 할인하구있어서ㅋㅋㅋㅋ",
          "original_form": "네네 요기요 파리바게트 할인하구있어서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020585.1.8",
          "form": "샐러드랑 샌드위치요!",
          "original_form": "샐러드랑 샌드위치요!",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020585.1.9",
          "form": "요새 요기요에서 파바 이벤트 많이하더라구요 ㅋㅋㅋ 샌드위치 맛있을것같아요",
          "original_form": "요새 요기요에서 파바 이벤트 많이하더라구요 ㅋㅋㅋ 샌드위치 맛있을것같아요",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020585.1.10",
          "form": "저는 어디가는길이라서 휴게소에서 호두과자를 간식으로 사 먹었어요",
          "original_form": "저는 어디가는길이라서 휴게소에서 호두과자를 간식으로 사 먹었어요",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020585.1.11",
          "form": "우와 갓구운 호두과자 진짜 넘 맛있죠ㅠㅠ",
          "original_form": "우와 갓구운 호두과자 진짜 넘 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020585.1.12",
          "form": "맛있는거 드시다니 부러워요",
          "original_form": "맛있는거 드시다니 부러워요",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020585.1.13",
          "form": "따끈따끈하더라구요 평소에 먹던것보다 좀 더 구워졌는지 겉이 좀 바삭하고 진한갈색이었는데ㅋㅋㅋ그래도 바삭해서 맛있었어요",
          "original_form": "따끈따끈하더라구요 평소에 먹던것보다 좀 더 구워졌는지 겉이 좀 바삭하고 진한갈색이었는데ㅋㅋㅋ그래도 바삭해서 맛있었어요",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020585.1.14",
          "form": "헐ㅠㅠ 묘사를 넘 섬세히 해주셔서",
          "original_form": "헐ㅠㅠ 묘사를 넘 섬세히 해주셔서",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020585.1.15",
          "form": "지금 침 넘어가요ㅋㅋㅋ",
          "original_form": "지금 침 넘어가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020585.1.16",
          "form": "간만에 먹은거라서 너무 맛있었나봐요 방금먹은거라서 설명이 상세해졌네요 ㅋㅋㅋ",
          "original_form": "간만에 먹은거라서 너무 맛있었나봐요 방금먹은거라서 설명이 상세해졌네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020585.1.17",
          "form": "저는 동생집 가고있는데 아마 점심은 집밥먹울것같아요 ㅎㅎ",
          "original_form": "저는 동생집 가고있는데 아마 점심은 집밥먹울것같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020585.1.18",
          "form": "집밥이 최고죠ㅎㅎ 동생분이 직접 요리해주시는건가용?",
          "original_form": "집밥이 최고죠ㅎㅎ 동생분이 직접 요리해주시는건가용?",
          "speaker_id": "1",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020585.1.19",
          "form": "밥은 동생이하고 나머지는 아마 제가 할것같아요 ㅋㅋㅋ 냉장고정리도 할겸 냉장고털어먹어야하거든요",
          "original_form": "밥은 동생이하고 나머지는 아마 제가 할것같아요 ㅋㅋㅋ 냉장고정리도 할겸 냉장고털어먹어야하거든요",
          "speaker_id": "2",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020585.1.20",
          "form": "누구랑 같이 요리하면 되게 신나던데",
          "original_form": "누구랑 같이 요리하면 되게 신나던데",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020585.1.21",
          "form": "밥도 맛있구요ㅋㅋ",
          "original_form": "밥도 맛있구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:24"
        }
      ]
    }
