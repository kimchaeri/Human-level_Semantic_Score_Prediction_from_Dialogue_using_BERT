      "utterance": [
        {
          "id": "MDRW2100013546.1.1",
          "form": "날씨가 변덕이 심하네",
          "original_form": "날씨가 변덕이 심하네",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013546.1.2",
          "form": "그러게 며칠전부터 난리다",
          "original_form": "그러게 며칠전부터 난리다",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013546.1.3",
          "form": "비가 개었다가 쏟아지다가",
          "original_form": "비가 개었다가 쏟아지다가",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013546.1.4",
          "form": "또 지금 비가 내리네",
          "original_form": "또 지금 비가 내리네",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013546.1.5",
          "form": "이제 맑은 하늘 좀 보고싶당",
          "original_form": "이제 맑은 하늘 좀 보고싶당",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013546.1.6",
          "form": "날씨가 맑으면 어디라도 갈텐데",
          "original_form": "날씨가 맑으면 어디라도 갈텐데",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013546.1.7",
          "form": "그러니까 날씨가 이래서 집에만 있네",
          "original_form": "그러니까 날씨가 이래서 집에만 있네",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013546.1.8",
          "form": "가족끼리 날씨좋아지면 어디라도 가자",
          "original_form": "가족끼리 날씨좋아지면 어디라도 가자",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013546.1.9",
          "form": "ㅇㅇ알았옹~",
          "original_form": "ㅇㅇ알았옹~",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013546.1.10",
          "form": "이번에 폭우로 피해는 많이 안 입었을까",
          "original_form": "이번에 폭우로 피해는 많이 안 입었을까",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013546.1.11",
          "form": "부산이 비가 많이 왔나보던데",
          "original_form": "부산이 비가 많이 왔나보던데",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013546.1.12",
          "form": "그렇구나~ 피해 크지않음 좋겠다",
          "original_form": "그렇구나~ 피해 크지않음 좋겠다",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013546.1.13",
          "form": "이제 여름이 끝난듯",
          "original_form": "이제 여름이 끝난듯",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013546.1.14",
          "form": "그러게 날씨가 덥지가 않네",
          "original_form": "그러게 날씨가 덥지가 않네",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013546.1.15",
          "form": "선선하니 시원해서 좋아",
          "original_form": "선선하니 시원해서 좋아",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013546.1.16",
          "form": "선풍기 아직 틀긴하잖아",
          "original_form": "선풍기 아직 틀긴하잖아",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013546.1.17",
          "form": "그래도 선풍기만 틀어도 시원하니까",
          "original_form": "그래도 선풍기만 틀어도 시원하니까",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013546.1.18",
          "form": "그건그렇지 ㅎㅎ 습도가좀",
          "original_form": "그건그렇지 ㅎㅎ 습도가좀",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013546.1.19",
          "form": "습도가 어제는 높았는데 오늘은 괜찮은듯",
          "original_form": "습도가 어제는 높았는데 오늘은 괜찮은듯",
          "speaker_id": "1",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013546.1.20",
          "form": "그렇네 어제는 조금만 움직여도 땀나더니",
          "original_form": "그렇네 어제는 조금만 움직여도 땀나더니",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013546.1.21",
          "form": "오늘은 날씨가 추워졌네",
          "original_form": "오늘은 날씨가 추워졌네",
          "speaker_id": "2",
          "time": "20210901 21:32"
        }
