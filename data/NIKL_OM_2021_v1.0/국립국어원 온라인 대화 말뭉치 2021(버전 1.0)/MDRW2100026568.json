        {
          "id": "MDRW2100026568.1.1",
          "form": "유튜브 자주 보세요?",
          "original_form": "유튜브 자주 보세요?",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026568.1.2",
          "form": "네 ㅎ ㅎ 거의 매일?",
          "original_form": "네 ㅎ ㅎ 거의 매일?",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026568.1.3",
          "form": "어떤거 주로 보세요??",
          "original_form": "어떤거 주로 보세요??",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026568.1.4",
          "form": "저 슈카 자주 보고 음",
          "original_form": "저 슈카 자주 보고 음",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026568.1.5",
          "form": "사랑과전쟁이요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사랑과전쟁이요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026568.1.6",
          "form": "사랑과 전쟁이요??? 그 제가 아는 사랑과 전쟁??",
          "original_form": "사랑과 전쟁이요??? 그 제가 아는 사랑과 전쟁??",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026568.1.7",
          "form": "엄청 오래되지 않았나용ㅋㅋㅋㅋㅋ",
          "original_form": "엄청 오래되지 않았나용ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026568.1.8",
          "form": "kemi tv에서 하이라이트 해줍니다 ㅎㅎ",
          "original_form": "kemi tv에서 하이라이트 해줍니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026568.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 요즘 드라마 재밌는게 없어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 요즘 드라마 재밌는게 없어서",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026568.1.10",
          "form": "옛날게 더 재밌는거 같기도 해요!",
          "original_form": "옛날게 더 재밌는거 같기도 해요!",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026568.1.11",
          "form": "네 맞아요 ㅎㅎ",
          "original_form": "네 맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026568.1.12",
          "form": "구관이 명관이죠",
          "original_form": "구관이 명관이죠",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026568.1.13",
          "form": "저도 사랑과 전쟁만큼은 아니지만 꽤 오래된 하이킥 자주 봐요",
          "original_form": "저도 사랑과 전쟁만큼은 아니지만 꽤 오래된 하이킥 자주 봐요",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026568.1.14",
          "form": "옛날 드라마인데도 너무 재밌어요",
          "original_form": "옛날 드라마인데도 너무 재밌어요",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026568.1.15",
          "form": "그거 자주봐요 저도 ㅋㅋ",
          "original_form": "그거 자주봐요 저도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026568.1.16",
          "form": "민용이 멋짐",
          "original_form": "민용이 멋짐",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026568.1.17",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100026568.1.18",
          "form": "박해미랑 티키타카 너무 웃겨요",
          "original_form": "박해미랑 티키타카 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100026568.1.19",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100026568.1.20",
          "form": "제가 대학 처음 갔을때",
          "original_form": "제가 대학 처음 갔을때",
          "speaker_id": "2",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100026568.1.21",
