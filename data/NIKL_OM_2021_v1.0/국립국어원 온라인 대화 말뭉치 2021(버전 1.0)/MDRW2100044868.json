      "utterance": [
        {
          "id": "MDRW2100044868.1.1",
          "form": "혹시 이번 대선 공약 보셨나요",
          "original_form": "혹시 이번 대선 공약 보셨나요",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044868.1.2",
          "form": "그게 무슨 상관이죠",
          "original_form": "그게 무슨 상관이죠",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044868.1.3",
          "form": "주 4일제 가 주된 공약 이더라고요",
          "original_form": "주 4일제 가 주된 공약 이더라고요",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044868.1.4",
          "form": "반대하시나요",
          "original_form": "반대하시나요",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044868.1.5",
          "form": "네 완전요",
          "original_form": "네 완전요",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044868.1.6",
          "form": "왜죠",
          "original_form": "왜죠",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044868.1.7",
          "form": "4일째 하면서 일자리 실업률 낮춘다는 거죠",
          "original_form": "4일째 하면서 일자리 실업률 낮춘다는 거죠",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044868.1.8",
          "form": "좋은게 아닌가요",
          "original_form": "좋은게 아닌가요",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044868.1.9",
          "form": "기존 일자리를 나눠 가진다는 의미 좀",
          "original_form": "기존 일자리를 나눠 가진다는 의미 좀",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044868.1.10",
          "form": "그게 무슨 얘기죠",
          "original_form": "그게 무슨 얘기죠",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044868.1.11",
          "form": "월급이 줄어든다는",
          "original_form": "월급이 줄어든다는",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044868.1.12",
          "form": "에이 설마요",
          "original_form": "에이 설마요",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044868.1.13",
          "form": "도대체 어느 회사가 일을 적게 하는데 돈을 똑같이 줄까요",
