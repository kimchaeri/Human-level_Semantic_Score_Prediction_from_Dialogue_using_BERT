      "utterance": [
        {
          "id": "MDRW2100019090.1.1",
          "form": "혹시 반려동물 이름이 name2?",
          "original_form": "혹시 반려동물 이름이 &name2&?",
          "speaker_id": "1",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019090.1.2",
          "form": "우와맞습니다",
          "original_form": "우와맞습니다",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100019090.1.3",
          "form": "천재 name1님",
          "original_form": "천재 &name1&님",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100019090.1.4",
          "form": "ㅋㅋㅋ 그거슨 강아지 일거시여..?",
          "original_form": "ㅋㅋㅋ 그거슨 강아지 일거시여..?",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019090.1.5",
          "form": "네강아지입니다 ㅋㅋ",
          "original_form": "네강아지입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100019090.1.6",
          "form": "ㅋㅋ 하야색 말티즈? 이면 돗자리 깝니다..",
          "original_form": "ㅋㅋ 하야색 말티즈? 이면 돗자리 깝니다..",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019090.1.7",
          "form": "앗 아까워요 장모치와와 크림색 ㅋㅋ",
          "original_form": "앗 아까워요 장모치와와 크림색 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019090.1.8",
          "form": "돗자리말고 다른 일 하실 운명!!",
          "original_form": "돗자리말고 다른 일 하실 운명!!",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019090.1.9",
          "form": "ㅋㅋㅋㅋ 모애요...",
          "original_form": "ㅋㅋㅋㅋ 모애요...",
          "speaker_id": "1",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019090.1.10",
          "form": "예전엔 말티즈엿엇죠?",
          "original_form": "예전엔 말티즈엿엇죠?",
          "speaker_id": "1",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019090.1.11",
          "form": "앗 처음 키우는 강아쥐에요 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 처음 키우는 강아쥐에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019090.1.12",
          "form": "이참에 한마리 더 키우시죠.. ㅋㅋㅋ \r\n강아지 얼마나 됫어요?",
          "original_form": "이참에 한마리 더 키우시죠.. ㅋㅋㅋ \r\n강아지 얼마나 됫어요?",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019090.1.13",
          "form": "앗ㅋㅋㅋ하얀말티즈",
          "original_form": "앗ㅋㅋㅋ하얀말티즈",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019090.1.14",
          "form": "4살이에용 넘 이뻐요",
          "original_form": "4살이에용 넘 이뻐요",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019090.1.15",
          "form": "근데 치와와도 크림색이 있어요? 많이 못본듯..",
          "original_form": "근데 치와와도 크림색이 있어요? 많이 못본듯..",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019090.1.16",
          "form": "바닐라 라떼같은",
          "original_form": "바닐라 라떼같은",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019090.1.17",
          "form": "크림색이에요",
          "original_form": "크림색이에요",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019090.1.18",
          "form": "보통 브라운이 많지 않나요? 크림이고 엄청 귀옂겟당",
          "original_form": "보통 브라운이 많지 않나요? 크림이고 엄청 귀옂겟당",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019090.1.19",
          "form": "브라운도 섞여있어요 ㅋㅋ",
          "original_form": "브라운도 섞여있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019090.1.20",
          "form": "브라운 크림",
          "original_form": "브라운 크림",
          "speaker_id": "2",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019090.1.21",
          "form": "내 너무 이뻐요 제 전부 제 심장",
