      "utterance": [
        {
          "id": "MDRW2100046059.1.1",
          "form": "모사실껀가요지원금으로",
          "original_form": "모사실껀가요지원금으로",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046059.1.2",
          "form": "고민 안해봤어용",
          "original_form": "고민 안해봤어용",
          "speaker_id": "2",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046059.1.3",
          "form": "뭐사실건가여?",
          "original_form": "뭐사실건가여?",
          "speaker_id": "2",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046059.1.4",
          "form": "ㅎㅎㅎ저도아직용",
          "original_form": "ㅎㅎㅎ저도아직용",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046059.1.5",
          "form": "그러시군요~",
          "original_form": "그러시군요~",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046059.1.6",
          "form": "ㅎㅎ원래전기장판사고싶었어요",
          "original_form": "ㅎㅎ원래전기장판사고싶었어요",
          "speaker_id": "1",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046059.1.7",
          "form": "오?ㅋㅋㅋ 이번 겨울 대비인가여?",
          "original_form": "오?ㅋㅋㅋ 이번 겨울 대비인가여?",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046059.1.8",
          "form": "ㅎㅎ맞아용",
          "original_form": "ㅎㅎ맞아용",
          "speaker_id": "1",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100046059.1.9",
          "form": "추위를 잘타서 전기장판 필수에요 진짜",
          "original_form": "추위를 잘타서 전기장판 필수에요 진짜",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046059.1.10",
          "form": "저도요ㅠㅜ",
          "original_form": "저도요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046059.1.11",
          "form": "전기장판 인터넷으로 사실건가요?",
          "original_form": "전기장판 인터넷으로 사실건가요?",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046059.1.12",
          "form": "ㅎㅎ가서보고사고싶어요",
          "original_form": "ㅎㅎ가서보고사고싶어요",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046059.1.13",
          "form": "아아 오프라인~ 오프라인 쇼핑도 좋죠",
          "original_form": "아아 오프라인~ 오프라인 쇼핑도 좋죠",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046059.1.14",
          "form": "시장같은데서 팔죠?",
          "original_form": "시장같은데서 팔죠?",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046059.1.15",
          "form": "그렇죵 마트나 이런곳",
