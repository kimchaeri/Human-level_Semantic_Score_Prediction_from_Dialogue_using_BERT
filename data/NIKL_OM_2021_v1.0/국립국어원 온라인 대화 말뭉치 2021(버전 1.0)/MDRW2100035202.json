      "utterance": [
        {
          "id": "MDRW2100035202.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100035202.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100035202.1.3",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100035202.1.4",
          "form": "아침에 커피랑 빵 먹엇어요",
          "original_form": "아침에 커피랑 빵 먹엇어요",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100035202.1.5",
          "form": "간단하게 드셨네요",
          "original_form": "간단하게 드셨네요",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100035202.1.6",
          "form": "모닝커피 자주 드시나요?",
          "original_form": "모닝커피 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100035202.1.7",
          "form": "출근하자마자 아메리카노 먹게돼서",
          "original_form": "출근하자마자 아메리카노 먹게돼서",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100035202.1.8",
          "form": "습관처럼 되버렷어요ㅋㅋ",
          "original_form": "습관처럼 되버렷어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100035202.1.9",
          "form": "카페서 주로 뭐 마시나여",
          "original_form": "카페서 주로 뭐 마시나여",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100035202.1.10",
          "form": "저는 카페인을 잘 못 마셔서 티나 초코음료 마셔요!",
          "original_form": "저는 카페인을 잘 못 마셔서 티나 초코음료 마셔요!",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100035202.1.11",
          "form": "저두 저녁엔 잠못자서 절대안마셔요 ㅎ",
          "original_form": "저두 저녁엔 잠못자서 절대안마셔요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100035202.1.12",
          "form": "요즘 믹스커피 맛있게 먹는 레시피 많던데 자주 못해먹어서 좀 아쉬워요 ㅎㅎ",
          "original_form": "요즘 믹스커피 맛있게 먹는 레시피 많던데 자주 못해먹어서 좀 아쉬워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100035202.1.13",
          "form": "아 저는 살찔까봐 못먹어요ㅜㅜ",
          "original_form": "아 저는 살찔까봐 못먹어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100035202.1.14",
          "form": "진짜 맛잇지만 참습니다",
          "original_form": "진짜 맛잇지만 참습니다",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100035202.1.15",
          "form": "맞아요 제가 카페인을 잘 받아들이는 몸이었다면 살이 엄청 쪘을거에요.... ㅎㅎ",
          "original_form": "맞아요 제가 카페인을 잘 받아들이는 몸이었다면 살이 엄청 쪘을거에요.... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100035202.1.16",
          "form": "믹스커피는 정말 카페인이 필요한 날 한번씩 먹는데 너무 달고 맛있더라구요ㅠㅠ",
          "original_form": "믹스커피는 정말 카페인이 필요한 날 한번씩 먹는데 너무 달고 맛있더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100035202.1.17",
          "form": "저두여 ㅋㅋ 진짜 가끔씩 ㅋㅋ",
          "original_form": "저두여 ㅋㅋ 진짜 가끔씩 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100035202.1.18",
          "form": "점심엔 뭐 드세요",
          "original_form": "점심엔 뭐 드세요",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100035202.1.19",
          "form": "국밥 먹고싶어서 포장해오려구요!",
          "original_form": "국밥 먹고싶어서 포장해오려구요!",
          "speaker_id": "1",
          "time": "20210829 12:44"
        }
