      "utterance": [
        {
          "id": "MDRW2100009015.1.1",
          "form": "악뮤 얘기 했네요!",
          "original_form": "악뮤 얘기 했네요!",
          "speaker_id": "1",
          "time": "20210822 20:30"
        },
        {
          "id": "MDRW2100009015.1.2",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "2",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100009015.1.3",
          "form": "네 운동해요!",
          "original_form": "네 운동해요!",
          "speaker_id": "1",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100009015.1.4",
          "form": "지금도 러닝 나왔네요 ㅎㅎ",
          "original_form": "지금도 러닝 나왔네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100009015.1.5",
          "form": "러닝나오셨구나 요즘은 저녁날씨가 좋아서 더 좋으시겠다",
          "original_form": "러닝나오셨구나 요즘은 저녁날씨가 좋아서 더 좋으시겠다",
          "speaker_id": "2",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100009015.1.6",
          "form": "네 맞아요!!",
          "original_form": "네 맞아요!!",
          "speaker_id": "1",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100009015.1.7",
          "form": "저도 간만에 나왔어요",
          "original_form": "저도 간만에 나왔어요",
          "speaker_id": "1",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100009015.1.8",
          "form": "여름애는 더워서 못나오니까요",
          "original_form": "여름애는 더워서 못나오니까요",
          "speaker_id": "1",
          "time": "20210822 21:04"
        },
        {
          "id": "MDRW2100009015.1.9",
          "form": "name2님은 운동 뭐 하고계신거 있으신가요?",
          "original_form": "&name2&님은 운동 뭐 하고계신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 21:04"
        },
        {
          "id": "MDRW2100009015.1.10",
          "form": "저는 시국때문에 링피트 사서 링피트 하고있어요",
          "original_form": "저는 시국때문에 링피트 사서 링피트 하고있어요",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009015.1.11",
          "form": "아 링피트 재밌죠!",
          "original_form": "아 링피트 재밌죠!",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009015.1.12",
          "form": "저도 링피트 가끔씩해요 ㅎㅎ",
          "original_form": "저도 링피트 가끔씩해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009015.1.13",
          "form": "은근 확실하게 운동되잖아요",
          "original_form": "은근 확실하게 운동되잖아요",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009015.1.14",
          "form": "그리고 꾸준하기 쉽다고해야할까요",
          "original_form": "그리고 꾸준하기 쉽다고해야할까요",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009015.1.15",
          "form": "게임이다보니까 목표가 눈에보여서 할만해요",
          "original_form": "게임이다보니까 목표가 눈에보여서 할만해요",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009015.1.16",
          "form": "런닝은 보통 얼마나해요?",
          "original_form": "런닝은 보통 얼마나해요?",
          "speaker_id": "2",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009015.1.17",
          "form": "달리는건 런데이 켜서 30분 정도하고",
          "original_form": "달리는건 런데이 켜서 30분 정도하고",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009015.1.18",
          "form": "시간 되면 1시간 정도 더 걸어요!",
          "original_form": "시간 되면 1시간 정도 더 걸어요!",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009015.1.19",
          "form": "총 1시간 반정도?",
          "original_form": "총 1시간 반정도?",
          "speaker_id": "1",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009015.1.20",
          "form": "핼스장도 다니고 있어요",
          "original_form": "핼스장도 다니고 있어요",
          "speaker_id": "1",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009015.1.21",
          "form": "우와 건강관리 멋져요 저는 요즘 조금 일상이 빠듯해서 평일 운동 너무 힘들던데ㅠㅠ",
          "original_form": "우와 건강관리 멋져요 저는 요즘 조금 일상이 빠듯해서 평일 운동 너무 힘들던데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009015.1.22",
          "form": "건강상태가 너무 나빠진것같아서",
          "original_form": "건강상태가 너무 나빠진것같아서",
          "speaker_id": "1",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009015.1.23",
          "form": "이번 방학때 확! 시작했어요",
          "original_form": "이번 방학때 확! 시작했어요",
          "speaker_id": "1",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009015.1.24",
          "form": "뭔가 체력이 느는게 느껴져서 신기해요 ㅎㅎ",
          "original_form": "뭔가 체력이 느는게 느껴져서 신기해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009015.1.25",
          "form": "운동 조금 하다보면 체력느는게 느껴질때 제일 뿌듯하죠",
          "original_form": "운동 조금 하다보면 체력느는게 느껴질때 제일 뿌듯하죠",
          "speaker_id": "2",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100009015.1.26",
          "form": "맞아요 요즘 컨디션도 좋아서 참 좋답니다",
          "original_form": "맞아요 요즘 컨디션도 좋아서 참 좋답니다",
          "speaker_id": "1",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100009015.1.27",
          "form": "식이조절은...",
          "original_form": "식이조절은...",
          "speaker_id": "1",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100009015.1.28",
          "form": "눈물나지만요..",
          "original_form": "눈물나지만요..",
          "speaker_id": "1",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100009015.1.29",
          "form": "아앗...저두요...",
          "original_form": "아앗...저두요...",
          "speaker_id": "2",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009015.1.30",
          "form": "하다가 깔끔하게 포기!",
          "original_form": "하다가 깔끔하게 포기!",
          "speaker_id": "2",
          "time": "20210822 22:02"
        }
      ]
