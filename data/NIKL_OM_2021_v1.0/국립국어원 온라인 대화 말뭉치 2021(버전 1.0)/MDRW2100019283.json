      "utterance": [
        {
          "id": "MDRW2100019283.1.1",
          "form": "판타지소설 좋아하세요?",
          "original_form": "판타지소설 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100019283.1.2",
          "form": "네 매일 봐요",
          "original_form": "네 매일 봐요",
          "speaker_id": "2",
          "time": "20210827 17:19"
        },
        {
          "id": "MDRW2100019283.1.3",
          "form": "저도 좋아해요ㅋㅋㅋ",
          "original_form": "저도 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:19"
        },
        {
          "id": "MDRW2100019283.1.4",
          "form": "뭐보세요?",
          "original_form": "뭐보세요?",
          "speaker_id": "1",
          "time": "20210827 17:19"
        },
        {
          "id": "MDRW2100019283.1.5",
          "form": "시리즈랑 카카페서 이것 저것 보고 있어요",
          "original_form": "시리즈랑 카카페서 이것 저것 보고 있어요",
          "speaker_id": "2",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100019283.1.6",
          "form": "요새 재밌게 보는건 전생이 천재였다랑",
          "original_form": "요새 재밌게 보는건 전생이 천재였다랑",
          "speaker_id": "2",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100019283.1.7",
          "form": "백망되 읽고 있어요",
          "original_form": "백망되 읽고 있어요",
          "speaker_id": "2",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100019283.1.8",
          "form": "저두 전생이 천재였다 재밌게 읽고 있어요",
          "original_form": "저두 전생이 천재였다 재밌게 읽고 있어요",
          "speaker_id": "1",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100019283.1.9",
          "form": "오늘은 미술천재? 읽으려구요 ㅎㅎㅎ",
          "original_form": "오늘은 미술천재? 읽으려구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019283.1.10",
          "form": "그것도 추천 많던데",
          "original_form": "그것도 추천 많던데",
          "speaker_id": "2",
          "time": "20210827 17:24"
        },
        {
          "id": "MDRW2100019283.1.11",
          "form": "재밌으면 좋겠네요",
          "original_form": "재밌으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210827 17:24"
        },
        {
          "id": "MDRW2100019283.1.12",
          "form": "전 오늘 읽을꺼 골라야해요..",
          "original_form": "전 오늘 읽을꺼 골라야해요..",
          "speaker_id": "2",
          "time": "20210827 17:24"
        },
        {
          "id": "MDRW2100019283.1.13",
          "form": "네네 기대하고 있어요ㅋㅋㅋ",
          "original_form": "네네 기대하고 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:24"
        },
        {
          "id": "MDRW2100019283.1.14",
          "form": "앗.. 고르는게 제일 귀찮죠",
          "original_form": "앗.. 고르는게 제일 귀찮죠",
          "speaker_id": "1",
          "time": "20210827 17:24"
        },
        {
          "id": "MDRW2100019283.1.15",
          "form": "새로 읽을꺼 찾는게 젤 힘들어요ㅜㅜ",
          "original_form": "새로 읽을꺼 찾는게 젤 힘들어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 17:24"
        },
        {
          "id": "MDRW2100019283.1.16",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:24"
        },
        {
          "id": "MDRW2100019283.1.17",
          "form": "야구물은 한번 몰라 읽고",
          "original_form": "야구물은 한번 몰라 읽고",
          "speaker_id": "2",
          "time": "20210827 17:24"
        },
        {
          "id": "MDRW2100019283.1.18",
          "form": "함 끊으니 다시 안읽히네요..",
          "original_form": "함 끊으니 다시 안읽히네요..",
          "speaker_id": "2",
          "time": "20210827 17:25"
        },
        {
          "id": "MDRW2100019283.1.19",
          "form": "잘 읽히는 시기가 있는거 같아요ㅋㅋㅋ",
          "original_form": "잘 읽히는 시기가 있는거 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:25"
        },
        {
          "id": "MDRW2100019283.1.20",
          "form": "저는 오늘 웹툰도 한번 봐보려구요",
          "original_form": "저는 오늘 웹툰도 한번 봐보려구요",
          "speaker_id": "1",
          "time": "20210827 17:25"
        },
        {
          "id": "MDRW2100019283.1.21",
          "form": "네이버 무료웹툰 추천 글을 봤어요",
          "original_form": "네이버 무료웹툰 추천 글을 봤어요",
          "speaker_id": "1",
          "time": "20210827 17:25"
        },
        {
          "id": "MDRW2100019283.1.22",
          "form": "어 뭐 추천 받으셨어요?",
          "original_form": "어 뭐 추천 받으셨어요?",
          "speaker_id": "2",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019283.1.23",
          "form": "오 리디 비오는날 쿠폰 주네요",
          "original_form": "오 리디 비오는날 쿠폰 주네요",
          "speaker_id": "2",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019283.1.24",
          "form": "고래별이랑 동트는 로맨스 등등이요ㅎㅎㅎ",
          "original_form": "고래별이랑 동트는 로맨스 등등이요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:05"
        },
        {
          "id": "MDRW2100019283.1.25",
          "form": "오 리디 쿠폰도 주는데 한권 사야겠네요ㅎㅎㅎ",
          "original_form": "오 리디 쿠폰도 주는데 한권 사야겠네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:06"
        }
      ]
    }
  ]
