      "utterance": [
        {
          "id": "MDRW2100018184.1.1",
          "form": "ㅋㅋㅋㅋㅋ편입한학교 졸업하고",
          "original_form": "ㅋㅋㅋㅋㅋ편입한학교 졸업하고",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018184.1.2",
          "form": "취업준비하면서 아르바이트",
          "original_form": "취업준비하면서 아르바이트",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018184.1.3",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018184.1.4",
          "form": "로 대학교 계약직 근무했었거든요",
          "original_form": "로 대학교 계약직 근무했었거든요",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018184.1.5",
          "form": "헐 완전 열심히 사셨네요!",
          "original_form": "헐 완전 열심히 사셨네요!",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018184.1.6",
          "form": "취직전엔 계약직이라 좀 꺼려졌는데 \n막상 일해보니 환경도 좋고..ㅋㅋㅋㅋ",
          "original_form": "취직전엔 계약직이라 좀 꺼려졌는데 \n막상 일해보니 환경도 좋고..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018184.1.7",
          "form": "그리고 계약직 근무 은근 빡시잖아요",
          "original_form": "그리고 계약직 근무 은근 빡시잖아요",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018184.1.8",
          "form": "나만 내가 계약직이라고 생각하지않으면 딱 좋더라구요",
          "original_form": "나만 내가 계약직이라고 생각하지않으면 딱 좋더라구요",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018184.1.9",
          "form": "아 환경 좋아요??",
          "original_form": "아 환경 좋아요??",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018184.1.10",
          "form": "근데 ㅋㅋㅋ이게 편입도 편입생이라고 약간 제가 자격지심이있었고",
          "original_form": "근데 ㅋㅋㅋ이게 편입도 편입생이라고 약간 제가 자격지심이있었고",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018184.1.11",
          "form": "계약직근무할때도 알바일뿐인데 일에 정들수록",
          "original_form": "계약직근무할때도 알바일뿐인데 일에 정들수록",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018184.1.12",
          "form": "계약직이라는게 너무 힘들더라구요",
          "original_form": "계약직이라는게 너무 힘들더라구요",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018184.1.13",
          "form": "아 ㅠㅠ 그렇죠 진짜",
          "original_form": "아 ㅠㅠ 그렇죠 진짜",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018184.1.14",
          "form": "아무도 눈치주거나 무사하거나그러지않는데 저혼자 뭔가 떳떳한거같지않고",
          "original_form": "아무도 눈치주거나 무사하거나그러지않는데 저혼자 뭔가 떳떳한거같지않고",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018184.1.15",
          "form": "ㅠㅠㅠㅠ약간 그런느낌",
          "original_form": "ㅠㅠㅠㅠ약간 그런느낌",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018184.1.16",
          "form": "잠깐 일하러간건데 눌러앉아서",
          "original_form": "잠깐 일하러간건데 눌러앉아서",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018184.1.17",
          "form": "결혼할때까지 다녔어요 ㅋㅋㅋㅋ",
          "original_form": "결혼할때까지 다녔어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018184.1.18",
          "form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018184.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018184.1.20",
          "form": "그럼 엄텅",
          "original_form": "그럼 엄텅",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018184.1.21",
          "form": "애정이 남아있어ㅛ겠네요",
          "original_form": "애정이 남아있어ㅛ겠네요",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018184.1.22",
          "form": "결혼하고서 그만뒀는데",
          "original_form": "결혼하고서 그만뒀는데",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018184.1.23",
          "form": "저 챙겨쥬는교수님이",
          "original_form": "저 챙겨쥬는교수님이",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018184.1.24",
          "form": "부서옮겨줄테니 출근하라고해서",
          "original_form": "부서옮겨줄테니 출근하라고해서",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018184.1.25",
          "form": "또 다님ㅌㅋㅋㅋㅋ",
          "original_form": "또 다님ㅌㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018184.1.26",
          "form": "그러다가 임신준비한다고 퇴사했어요",
          "original_form": "그러다가 임신준비한다고 퇴사했어요",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018184.1.27",
          "form": "징하게 다녔죠 ㅋㅋㅋ",
          "original_form": "징하게 다녔죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018184.1.28",
          "form": "거기서 일하면서",
          "original_form": "거기서 일하면서",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018184.1.29",
          "form": "그학교도 편입시험이 있었눈데",
          "original_form": "그학교도 편입시험이 있었눈데",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018184.1.30",
          "form": "헐 너무 좋은 곳이네요...",
          "original_form": "헐 너무 좋은 곳이네요...",
          "speaker_id": "2",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018184.1.31",
          "form": "거기 면접관으로도 일하고",
          "original_form": "거기 면접관으로도 일하고",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018184.1.32",
          "form": "ㅜㅜㅜㅜㅜ아 진짜 ㅜㅜㅜ 기분 이상하더라구요",
          "original_form": "ㅜㅜㅜㅜㅜ아 진짜 ㅜㅜㅜ 기분 이상하더라구요",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018184.1.33",
          "form": "와.. 진짜 너무 멋져요",
          "original_form": "와.. 진짜 너무 멋져요",
          "speaker_id": "2",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018184.1.34",
          "form": "내가 쳔편입한 학교에",
          "original_form": "내가 쳔편입한 학교에",
          "speaker_id": "2",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018184.1.35",
          "form": "면접관이 되다니",
          "original_form": "면접관이 되다니",
          "speaker_id": "2",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018184.1.36",
          "form": "ㅋㅋㅋㅋ 시험볼때 앞에서 감독하는일 하는데 \n막 내가 저들중 한명이였지 ㅋㅋㅋ이러곸ㅋㅋ \n\n아 근데 전",
          "original_form": "ㅋㅋㅋㅋ 시험볼때 앞에서 감독하는일 하는데 \n막 내가 저들중 한명이였지 ㅋㅋㅋ이러곸ㅋㅋ \n\n아 근데 전",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018184.1.37",
          "form": "편입한학교말고 ㅋㅋㅋㅋㅋㅋ집근처 학교였어욬ㅋㅋㅋㅋㅋ",
          "original_form": "편입한학교말고 ㅋㅋㅋㅋㅋㅋ집근처 학교였어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018184.1.38",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그래도 편입생에서 면접관의 위치가 된게",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그래도 편입생에서 면접관의 위치가 된게",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018184.1.39",
          "form": "집근처 알바자리구하다가 ㅋㅋㅋㅋㅋ",
          "original_form": "집근처 알바자리구하다가 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018184.1.40",
          "form": "정말 멋진거같아요",
          "original_form": "정말 멋진거같아요",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018184.1.41",
          "form": "인생사 ㅋㅋㅋㅋㅋ",
          "original_form": "인생사 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018184.1.42",
          "form": "저도 내년엔... ㅎㅎ",
          "original_form": "저도 내년엔... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018184.1.43",
          "form": "꼭 편입 성공해서 그런 자리에 있어보고 깊어여",
          "original_form": "꼭 편입 성공해서 그런 자리에 있어보고 깊어여",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018184.1.44",
