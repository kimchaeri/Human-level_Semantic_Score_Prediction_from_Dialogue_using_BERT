      "utterance": [
        {
          "id": "MDRW2100041482.1.1",
          "form": "나 지금 맛있는 녀석들 보는곳",
          "original_form": "나 지금 맛있는 녀석들 보는곳",
          "speaker_id": "2",
          "time": "20210906 01:15"
        },
        {
          "id": "MDRW2100041482.1.2",
          "form": "그거 어디서 봤니",
          "original_form": "그거 어디서 봤니",
          "speaker_id": "1",
          "time": "20210906 01:17"
        },
        {
          "id": "MDRW2100041482.1.3",
          "form": "넷플릭스",
          "original_form": "넷플릭스",
          "speaker_id": "2",
          "time": "20210906 01:17"
        },
        {
          "id": "MDRW2100041482.1.4",
          "form": "나 옛날엔 밥 먹을 때 많이 봤는데",
          "original_form": "나 옛날엔 밥 먹을 때 많이 봤는데",
          "speaker_id": "1",
          "time": "20210906 01:17"
        },
        {
          "id": "MDRW2100041482.1.5",
          "form": "여전히 재미있어",
          "original_form": "여전히 재미있어",
          "speaker_id": "2",
          "time": "20210906 01:17"
        },
        {
          "id": "MDRW2100041482.1.6",
          "form": "게스트가 누구야",
          "original_form": "게스트가 누구야",
          "speaker_id": "1",
          "time": "20210906 01:17"
        },
        {
          "id": "MDRW2100041482.1.7",
          "form": "안창림",
          "original_form": "안창림",
          "speaker_id": "2",
          "time": "20210906 01:17"
        },
        {
          "id": "MDRW2100041482.1.8",
          "form": "우와 나 그 선수 진짜 좋아하는데",
          "original_form": "우와 나 그 선수 진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210906 01:17"
        },
        {
          "id": "MDRW2100041482.1.9",
          "form": "얼굴이 너무 귀엽게 생겼네",
          "original_form": "얼굴이 너무 귀엽게 생겼네",
          "speaker_id": "2",
          "time": "20210906 01:17"
        },
        {
          "id": "MDRW2100041482.1.10",
          "form": "왠지 유도 선수들이라 잘 먹을 거 같네",
          "original_form": "왠지 유도 선수들이라 잘 먹을 거 같네",
          "speaker_id": "1",
          "time": "20210906 01:18"
        },
        {
          "id": "MDRW2100041482.1.11",
          "form": "근데 막상 체중 조절 때문에 많이 못 먹나봐",
          "original_form": "근데 막상 체중 조절 때문에 많이 못 먹나봐",
          "speaker_id": "2",
          "time": "20210906 01:18"
        },
        {
          "id": "MDRW2100041482.1.12",
          "form": "저런 운동 하느라 도 힘들 텐데",
          "original_form": "저런 운동 하느라 도 힘들 텐데",
          "speaker_id": "1",
          "time": "20210906 01:18"
        },
        {
          "id": "MDRW2100041482.1.13",
          "form": "방송에서라도 많이 먹었으면 좋겠네",
          "original_form": "방송에서라도 많이 먹었으면 좋겠네",
