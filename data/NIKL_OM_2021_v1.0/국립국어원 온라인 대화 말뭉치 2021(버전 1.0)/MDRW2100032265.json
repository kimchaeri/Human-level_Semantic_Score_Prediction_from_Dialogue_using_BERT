      "utterance": [
        {
          "id": "MDRW2100032265.1.1",
          "form": "이번에 넷플릭스에서 새로 나온 디피라는 한국 드라마가 있는데",
          "original_form": "이번에 넷플릭스에서 새로 나온 디피라는 한국 드라마가 있는데",
          "speaker_id": "1",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032265.1.2",
          "form": "엄청 인기인 것 같더라구요",
          "original_form": "엄청 인기인 것 같더라구요",
          "speaker_id": "1",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032265.1.3",
          "form": "처음 들어보는데 무슨 내용인가요",
          "original_form": "처음 들어보는데 무슨 내용인가요",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032265.1.4",
          "form": "군인 나오는 내용 같아요 저도 보진 않았어요",
          "original_form": "군인 나오는 내용 같아요 저도 보진 않았어요",
          "speaker_id": "1",
          "time": "20210905 14:40"
        },
        {
          "id": "MDRW2100032265.1.5",
          "form": "등장인물은 누구예요",
          "original_form": "등장인물은 누구예요",
          "speaker_id": "2",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032265.1.6",
          "form": "어 잘 모르시는군요",
          "original_form": "어 잘 모르시는군요",
          "speaker_id": "2",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032265.1.7",
          "form": "제가 모르는 사람들이에요 그런데 그 드라마가 인기가 많아서 제작 사랑 원작사 있던 곳에 주식까지 올랐더라구요",
          "original_form": "제가 모르는 사람들이에요 그런데 그 드라마가 인기가 많아서 제작 사랑 원작사 있던 곳에 주식까지 올랐더라구요",
          "speaker_id": "1",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032265.1.8",
          "form": "그래서 들어왔습니다",
          "original_form": "그래서 들어왔습니다",
          "speaker_id": "1",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032265.1.9",
          "form": "덕분에 넷플릭스 잘 보고 있습니다",
          "original_form": "덕분에 넷플릭스 잘 보고 있습니다",
          "speaker_id": "2",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032265.1.10",
          "form": "봤습니다",
          "original_form": "봤습니다",
          "speaker_id": "1",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032265.1.11",
          "form": "쉬어 하는 거 좋아요",
          "original_form": "쉬어 하는 거 좋아요",
          "speaker_id": "1",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032265.1.12",
          "form": "감사 감사요",
          "original_form": "감사 감사요",
          "speaker_id": "2",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032265.1.13",
          "form": "혼자서 많이 찾아보진 않거든요",
          "original_form": "혼자서 많이 찾아보진 않거든요",
          "speaker_id": "1",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032265.1.14",
          "form": "그래서 남편이 다음에 식사 한 끼 하자고 하더라고요",
          "original_form": "그래서 남편이 다음에 식사 한 끼 하자고 하더라고요",
          "speaker_id": "2",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032265.1.15",
          "form": "너무 고맙다 고요",
          "original_form": "너무 고맙다 고요",
          "speaker_id": "2",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032265.1.16",
          "form": "남편분은 어떤 걸 잘 보시나요",
          "original_form": "남편분은 어떤 걸 잘 보시나요",
          "speaker_id": "1",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.17",
          "form": "영광입니다 같이 식사 한번 해요",
          "original_form": "영광입니다 같이 식사 한번 해요",
          "speaker_id": "1",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.18",
          "form": "그때 격리 할 때 킹덤 너무 잘 봤어요",
          "original_form": "그때 격리 할 때 킹덤 너무 잘 봤어요",
          "speaker_id": "2",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.19",
          "form": "아 킹덤 저 예전에 보다가 못 봤는데 재미있지요",
          "original_form": "아 킹덤 저 예전에 보다가 못 봤는데 재미있지요",
          "speaker_id": "1",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.20",
          "form": "시간이 잘 가더라고요",
          "original_form": "시간이 잘 가더라고요",
          "speaker_id": "2",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.21",
          "form": "무서운 장면은 못 본다 하시지 않으셨나요 잘 보셨나 봐요",
          "original_form": "무서운 장면은 못 본다 하시지 않으셨나요 잘 보셨나 봐요",
          "speaker_id": "1",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.22",
          "form": "이태원 클라쓰도 봤어요",
          "original_form": "이태원 클라쓰도 봤어요",
          "speaker_id": "2",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.23",
          "form": "궁금해서 볼 수밖에 없었죠",
          "original_form": "궁금해서 볼 수밖에 없었죠",
          "speaker_id": "2",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.24",
          "form": "이태원 클라쓰도 재미있지요",
          "original_form": "이태원 클라쓰도 재미있지요",
          "speaker_id": "1",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.25",
          "form": "그 정도는 볼 수 있어요",
          "original_form": "그 정도는 볼 수 있어요",
          "speaker_id": "2",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.26",
          "form": "부산행 같은 거는 볼 수 있나요",
          "original_form": "부산행 같은 거는 볼 수 있나요",
          "speaker_id": "1",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.27",
          "form": "네 역시 드라마는 몰아서 봐야 해요",
          "original_form": "네 역시 드라마는 몰아서 봐야 해요",
          "speaker_id": "2",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032265.1.28",
          "form": "부산행 재밌게 봤었거든요",
          "original_form": "부산행 재밌게 봤었거든요",
          "speaker_id": "1",
          "time": "20210905 14:43"
        }
      ]
    }
