      "utterance": [
        {
          "id": "MDRW2100007524.1.1",
          "form": "오버워치는 안한지되게 오래됐어",
          "original_form": "오버워치는 안한지되게 오래됐어",
          "speaker_id": "2",
          "time": "20210822 02:55"
        },
        {
          "id": "MDRW2100007524.1.2",
          "form": "넌어때",
          "original_form": "넌어때",
          "speaker_id": "2",
          "time": "20210822 02:55"
        },
        {
          "id": "MDRW2100007524.1.3",
          "form": "나도 되게 오래됨",
          "original_form": "나도 되게 오래됨",
          "speaker_id": "1",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007524.1.4",
          "form": "블리자드 다 쓰러져가던데",
          "original_form": "블리자드 다 쓰러져가던데",
          "speaker_id": "1",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007524.1.5",
          "form": "고소당했대ㅠ슬퍼",
          "original_form": "고소당했대ㅠ슬퍼",
          "speaker_id": "1",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007524.1.6",
          "form": "name3이랑은 무슨게임했어?",
          "original_form": "&name3&이랑은 무슨게임했어?",
          "speaker_id": "1",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007524.1.7",
          "form": "왜?? 왜 고소당해??",
          "original_form": "왜?? 왜 고소당해??",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007524.1.8",
          "form": "롤 한판햇어",
          "original_form": "롤 한판햇어",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007524.1.9",
          "form": "내가이김ㅋㅋ",
          "original_form": "내가이김ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007524.1.10",
          "form": "롤",
          "original_form": "롤",
          "speaker_id": "1",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007524.1.11",
          "form": "못하는 애들끼리의 전투",
          "original_form": "못하는 애들끼리의 전투",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007524.1.12",
          "form": "규칙이 이해가 안돼",
          "original_form": "규칙이 이해가 안돼",
          "speaker_id": "1",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007524.1.13",
          "form": "약간 그거같애",
          "original_form": "약간 그거같애",
          "speaker_id": "1",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007524.1.14",
          "form": "야구",
          "original_form": "야구",
          "speaker_id": "1",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007524.1.15",
          "form": "난 야구 규칙 몰라",
          "original_form": "난 야구 규칙 몰라",
          "speaker_id": "2",
          "time": "20210822 02:58"
        },
        {
          "id": "MDRW2100007524.1.16",
          "form": "롤은 그냥 상대방 기지 먼저 부수면 돼",
          "original_form": "롤은 그냥 상대방 기지 먼저 부수면 돼",
          "speaker_id": "2",
          "time": "20210822 02:58"
        },
        {
          "id": "MDRW2100007524.1.17",
          "form": "오 롤은 상대방 기지를",
          "original_form": "오 롤은 상대방 기지를",
          "speaker_id": "1",
          "time": "20210822 02:59"
        },
        {
          "id": "MDRW2100007524.1.18",
          "form": "근데 우리도 롤 같이 했었나",
          "original_form": "근데 우리도 롤 같이 했었나",
          "speaker_id": "1",
          "time": "20210822 02:59"
        },
        {
          "id": "MDRW2100007524.1.19",
          "form": "아닐걸...?",
          "original_form": "아닐걸...?",
          "speaker_id": "2",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007524.1.20",
          "form": "나 롤은 이때까지 name3이랑만 해봤을거야",
          "original_form": "나 롤은 이때까지 &name3&이랑만 해봤을거야",
          "speaker_id": "2",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007524.1.21",
          "form": "한 세번정도?",
          "original_form": "한 세번정도?",
          "speaker_id": "2",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007524.1.22",
          "form": "너아니면 누구랑 롤해봤지",
          "original_form": "너아니면 누구랑 롤해봤지",
          "speaker_id": "1",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007524.1.23",
          "form": "분명 누구랑 같이 했는데",
          "original_form": "분명 누구랑 같이 했는데",
          "speaker_id": "1",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007524.1.24",
          "form": "넌 대학친구가 많으니",
          "original_form": "넌 대학친구가 많으니",
          "speaker_id": "2",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007524.1.25",
          "form": "대학친구겠지",
          "original_form": "대학친구겠지",
          "speaker_id": "2",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007524.1.26",
          "form": "난 아니야 일단",
          "original_form": "난 아니야 일단",
          "speaker_id": "2",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007524.1.27",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "1",
          "time": "20210822 03:03"
        },
        {
          "id": "MDRW2100007524.1.28",
          "form": "대학친구아니레",
          "original_form": "대학친구아니레",
          "speaker_id": "1",
          "time": "20210822 03:03"
        },
        {
          "id": "MDRW2100007524.1.29",
          "form": "아닌데",
          "original_form": "아닌데",
          "speaker_id": "1",
          "time": "20210822 03:03"
        },
        {
          "id": "MDRW2100007524.1.30",
          "form": "pc.방 아무나랑 안 가는데",
          "original_form": "pc.방 아무나랑 안 가는데",
          "speaker_id": "1",
          "time": "20210822 03:04"
        },
        {
          "id": "MDRW2100007524.1.31",
          "form": "그럼 너도 name3이??",
          "original_form": "그럼 너도 &name3&이??",
          "speaker_id": "2",
          "time": "20210822 03:04"
        },
        {
          "id": "MDRW2100007524.1.32",
          "form": "너인거같애",
          "original_form": "너인거같애",
          "speaker_id": "1",
          "time": "20210822 03:04"
        },
        {
          "id": "MDRW2100007524.1.33",
          "form": "name3이 피방 가서 못하면서 롤하자그래 ㅋㅋㅋ",
          "original_form": "&name3&이 피방 가서 못하면서 롤하자그래 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:04"
        },
        {
          "id": "MDRW2100007524.1.34",
          "form": "잉... 나라고...?",
          "original_form": "잉... 나라고...?",
          "speaker_id": "2",
          "time": "20210822 03:04"
        },
        {
          "id": "MDRW2100007524.1.35",
          "form": "나랑은 오버워치를 했을거같은뎅",
