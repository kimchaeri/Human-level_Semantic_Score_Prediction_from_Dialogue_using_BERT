      "utterance": [
        {
          "id": "MDRW2100021496.1.1",
          "form": "저녁이라 그런지 시원한거같아요 날씨가! ㅋㅋㅋ",
          "original_form": "저녁이라 그런지 시원한거같아요 날씨가! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100021496.1.2",
          "form": "요즘 아침저녁으로 선선하죠 비도 내리고요",
          "original_form": "요즘 아침저녁으로 선선하죠 비도 내리고요",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021496.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021496.1.4",
          "form": "비가 오다말다 하더라구요 ㅠ",
          "original_form": "비가 오다말다 하더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021496.1.5",
          "form": "그러게요 요즘 산발적 소나기 잘 내리죠",
          "original_form": "그러게요 요즘 산발적 소나기 잘 내리죠",
          "speaker_id": "2",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100021496.1.6",
          "form": "맞아요 골치더라구요",
          "original_form": "맞아요 골치더라구요",
          "speaker_id": "1",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100021496.1.7",
          "form": "우산 챙겨서 다니세요? 앗 지금 비 와요",
          "original_form": "우산 챙겨서 다니세요? 앗 지금 비 와요",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021496.1.8",
          "form": "헉 지금요??!!",
          "original_form": "헉 지금요??!!",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021496.1.9",
          "form": "저 가방에 작은거 들고다녀요",
          "original_form": "저 가방에 작은거 들고다녀요",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021496.1.10",
          "form": "네 저희 지역은 지금 내려요",
          "original_form": "네 저희 지역은 지금 내려요",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021496.1.11",
          "form": "저두요 우산 가지고 다녀요",
          "original_form": "저두요 우산 가지고 다녀요",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100021496.1.12",
          "form": "우산을 들고다녀야 편할거같아요 몸은 불편하지만 ㅜㅜ",
          "original_form": "우산을 들고다녀야 편할거같아요 몸은 불편하지만 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100021496.1.13",
          "form": "가을장마니까 어쩔 수 없죠 거긴 날씨 어때요?",
          "original_form": "가을장마니까 어쩔 수 없죠 거긴 날씨 어때요?",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021496.1.14",
          "form": "여긴 흐리기만하고 비는 안와요 ㅋㅋㅋ",
          "original_form": "여긴 흐리기만하고 비는 안와요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021496.1.15",
          "form": "역시 소나기네요 금방 그쳤어요",
          "original_form": "역시 소나기네요 금방 그쳤어요",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021496.1.16",
          "form": "다행이네요 너무 오면 골치인데..",
          "original_form": "다행이네요 너무 오면 골치인데..",
