      "utterance": [
        {
          "id": "MDRW2100006572.1.1",
          "form": "종교 있어요?",
          "original_form": "종교 있어요?",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006572.1.2",
          "form": "저는 무교예요",
          "original_form": "저는 무교예요",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006572.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006572.1.4",
          "form": "아 아니다 저 있어요",
          "original_form": "아 아니다 저 있어요",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006572.1.5",
          "form": "종교인보면 신기해요",
          "original_form": "종교인보면 신기해요",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006572.1.6",
          "form": "뭐요",
          "original_form": "뭐요",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006572.1.7",
          "form": "플라잉스파게티교요",
          "original_form": "플라잉스파게티교요",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006572.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006572.1.9",
          "form": "아시죠?",
          "original_form": "아시죠?",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006572.1.10",
          "form": "완전 유명한데",
          "original_form": "완전 유명한데",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006572.1.11",
          "form": "전 그럼 사타니즘이요",
          "original_form": "전 그럼 사타니즘이요",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006572.1.12",
          "form": "아 그건 또 뭐람 ;",
          "original_form": "아 그건 또 뭐람 ;",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006572.1.13",
          "form": "알아요 눈알달린 스파게티",
          "original_form": "알아요 눈알달린 스파게티",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006572.1.14",
          "form": "신도가 적은 곳인가봐요",
          "original_form": "신도가 적은 곳인가봐요",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006572.1.15",
          "form": "모르세요? 완전 유명함",
          "original_form": "모르세요? 완전 유명함",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006572.1.16",
          "form": "저희 멕시코주 고소했어요",
          "original_form": "저희 멕시코주 고소했어요",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006572.1.17",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006572.1.18",
          "form": "사탄을위한 낙태를 막아서 ㅉ",
          "original_form": "사탄을위한 낙태를 막아서 ㅉ",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006572.1.19",
          "form": "역시 종교가 좀 유명해지려면",
          "original_form": "역시 종교가 좀 유명해지려면",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006572.1.20",
          "form": "종교의 자유 침해임",
          "original_form": "종교의 자유 침해임",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006572.1.21",
          "form": "딴지릉 좀 많이 걸어야죠",
          "original_form": "딴지릉 좀 많이 걸어야죠",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006572.1.22",
          "form": "사고만한게 없죠",
          "original_form": "사고만한게 없죠",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006572.1.23",
          "form": "그래서 기독교가 유명함",
          "original_form": "그래서 기독교가 유명함",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006572.1.24",
          "form": "아 그래서 사타니즘이예요??",
          "original_form": "아 그래서 사타니즘이예요??",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006572.1.25",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006572.1.26",
          "form": "아 걔들은 너무 활발해서",
          "original_form": "아 걔들은 너무 활발해서",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006572.1.27",
          "form": "좀 암세포 같고 그래요",
          "original_form": "좀 암세포 같고 그래요",
          "speaker_id": "2",
          "time": "20210908 02:37"
        }
      ]
    }
