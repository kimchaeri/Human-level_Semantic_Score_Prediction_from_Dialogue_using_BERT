      "utterance": [
        {
          "id": "MDRW2100033035.1.1",
          "form": "더 개인적으로 움직이셨겠네요!",
          "original_form": "더 개인적으로 움직이셨겠네요!",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.2",
          "form": "오오 리유저블 컵!",
          "original_form": "오오 리유저블 컵!",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.3",
          "form": "저거 근데",
          "original_form": "저거 근데",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.4",
          "form": "네네 저 처음 보는 주제들이에요 ㅎㅎㅎ",
          "original_form": "네네 저 처음 보는 주제들이에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.5",
          "form": "텀블러가 더 안좋다네요",
          "original_form": "텀블러가 더 안좋다네요",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.6",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.7",
          "form": "스테인리스도요?!",
          "original_form": "스테인리스도요?!",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.8",
          "form": "저걸 한 수천번인가써야",
          "original_form": "저걸 한 수천번인가써야",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.9",
          "form": "하긴 사람들이 너무 여러 개",
          "original_form": "하긴 사람들이 너무 여러 개",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.10",
          "form": "사서 쓰니까....",
          "original_form": "사서 쓰니까....",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.12",
          "form": "도움이되는데",
          "original_form": "도움이되는데",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033035.1.13",
          "form": "한사람이 여러개갖고있는건",
          "original_form": "한사람이 여러개갖고있는건",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.14",
          "form": "아 정말 그러겠네요ㅠ",
          "original_form": "아 정말 그러겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.15",
          "form": "오히려 더 환경에 안좋다고 ㅠ",
          "original_form": "오히려 더 환경에 안좋다고 ㅠ",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.16",
          "form": "어차피 그 텀블러를 만드는 과정에서도",
          "original_form": "어차피 그 텀블러를 만드는 과정에서도",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.17",
          "form": "환경에 안좋은 것들이 많이 나올텐데...!!!",
          "original_form": "환경에 안좋은 것들이 많이 나올텐데...!!!",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.18",
          "form": "근데 보통 안사도 선물로 들어오지 않나요",
          "original_form": "근데 보통 안사도 선물로 들어오지 않나요",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.19",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.20",
          "form": "산적없어도 집에 엄청많은데",
          "original_form": "산적없어도 집에 엄청많은데",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.21",
          "form": "아 그리고 그 뭐지",
          "original_form": "아 그리고 그 뭐지",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.22",
          "form": "마이보틀 이런 거는",
          "original_form": "마이보틀 이런 거는",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.23",
          "form": "막상 갖고다니기번거롭죠",
          "original_form": "막상 갖고다니기번거롭죠",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.24",
          "form": "좀 쓰다보면 냄새 나지 않나요?",
          "original_form": "좀 쓰다보면 냄새 나지 않나요?",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.25",
          "form": "네네 사실 저는 잘 갖고 다니기는 하는데",
          "original_form": "네네 사실 저는 잘 갖고 다니기는 하는데",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033035.1.26",
          "form": "네네 그리고환경호르몬도 걱정되고",
          "original_form": "네네 그리고환경호르몬도 걱정되고",
