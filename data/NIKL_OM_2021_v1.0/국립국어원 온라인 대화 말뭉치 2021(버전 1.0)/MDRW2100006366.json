      "utterance": [
        {
          "id": "MDRW2100006366.1.1",
          "form": "네 아이 둘이요 ㅎㅎ",
          "original_form": "네 아이 둘이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100006366.1.2",
          "form": "우와!!! 대박ㅋㅋㅋ 아들인가요 딸인가요",
          "original_form": "우와!!! 대박ㅋㅋㅋ 아들인가요 딸인가요",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006366.1.3",
          "form": "아들 둘입니다..ㅎㅎ",
          "original_form": "아들 둘입니다..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006366.1.4",
          "form": "아이고 ㅋㅋㅋㅋ고생많으십니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아이고 ㅋㅋㅋㅋ고생많으십니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006366.1.5",
          "form": "아직 뛰어다니는 애기들인가요?",
          "original_form": "아직 뛰어다니는 애기들인가요?",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006366.1.6",
          "form": "그렇습니다....ㅜ",
          "original_form": "그렇습니다....ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006366.1.7",
          "form": "에고ㅠㅠ 화이팅입니다 아버님ㅋㅋㅋ그래도 자기들끼리 잘놀죠??",
          "original_form": "에고ㅠㅠ 화이팅입니다 아버님ㅋㅋㅋ그래도 자기들끼리 잘놀죠??",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006366.1.8",
          "form": "네 감사합니다..ㅎ\r\n잘 놀고 잘 싸우죠..ㅎ",
          "original_form": "네 감사합니다..ㅎ\r\n잘 놀고 잘 싸우죠..ㅎ",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006366.1.9",
          "form": "ㅋㅋㅋㅋ몇살차이에요?",
          "original_form": "ㅋㅋㅋㅋ몇살차이에요?",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006366.1.10",
          "form": "3살 터울입니다",
          "original_form": "3살 터울입니다",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006366.1.11",
          "form": "ㅋㅋ저도 제동생이랑 3살 터울이었는데",
          "original_form": "ㅋㅋ저도 제동생이랑 3살 터울이었는데",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006366.1.12",
          "form": "입학식 졸업식 겹칠때가 가끔있었어요ㅠ",
          "original_form": "입학식 졸업식 겹칠때가 가끔있었어요ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006366.1.13",
          "form": "아 그렇죠ㅠ그게.3살터울의 큰 단점..ㅎㅎ",
          "original_form": "아 그렇죠ㅠ그게.3살터울의 큰 단점..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100006366.1.14",
          "form": "ㅋㅋㅋ엄마아빠 나눠서 온적도 있어요ㅠㅠㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ엄마아빠 나눠서 온적도 있어요ㅠㅠㅋㅋㅋ",
