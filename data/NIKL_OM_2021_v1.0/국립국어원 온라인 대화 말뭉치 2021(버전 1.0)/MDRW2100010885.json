      "utterance": [
        {
          "id": "MDRW2100010885.1.1",
          "form": "몸이 약하신편이신가요?",
          "original_form": "몸이 약하신편이신가요?",
          "speaker_id": "1",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100010885.1.2",
          "form": "건강한 편이라고 생각하는데",
          "original_form": "건강한 편이라고 생각하는데",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010885.1.3",
          "form": "스트레스에는 좀 약한 거 같아요~",
          "original_form": "스트레스에는 좀 약한 거 같아요~",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010885.1.4",
          "form": "아 스트레스가 적은 일을 하셔야겠어요",
          "original_form": "아 스트레스가 적은 일을 하셔야겠어요",
          "speaker_id": "1",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100010885.1.5",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "2",
          "time": "20210824 09:37"
        },
        {
          "id": "MDRW2100010885.1.6",
          "form": "아니면 스스로 스트레스를 잘 푸는 습관을 만들어야할텐데..",
          "original_form": "아니면 스스로 스트레스를 잘 푸는 습관을 만들어야할텐데..",
          "speaker_id": "2",
          "time": "20210824 09:37"
        },
        {
          "id": "MDRW2100010885.1.7",
          "form": "평소에 스트레스 관리 어떻게 하세요?",
          "original_form": "평소에 스트레스 관리 어떻게 하세요?",
          "speaker_id": "2",
          "time": "20210824 09:37"
        },
        {
          "id": "MDRW2100010885.1.8",
          "form": "저는 스트레스받으면 수다떠는거같아요",
          "original_form": "저는 스트레스받으면 수다떠는거같아요",
          "speaker_id": "1",
          "time": "20210824 09:43"
        },
        {
          "id": "MDRW2100010885.1.9",
          "form": "재밌는거보구",
          "original_form": "재밌는거보구",
          "speaker_id": "1",
          "time": "20210824 09:43"
        },
        {
          "id": "MDRW2100010885.1.10",
          "form": "재밌는 가 보고 맘껏 웃으면 확실히 스트레스가 날아가는 거 같긴 해요~",
          "original_form": "재밌는 가 보고 맘껏 웃으면 확실히 스트레스가 날아가는 거 같긴 해요~",
          "speaker_id": "2",
          "time": "20210824 09:55"
        },
        {
          "id": "MDRW2100010885.1.11",
          "form": "근본해결은 안되지면 한결나아요 name2님은 어떻게하시나요",
          "original_form": "근본해결은 안되지면 한결나아요 &name2&님은 어떻게하시나요",
          "speaker_id": "1",
          "time": "20210824 09:56"
        },
        {
          "id": "MDRW2100010885.1.12",
          "form": "저는 달달한 걸 먹거나 푹 자거나 자연을 보러 가거나 하는 거 같아요~",
          "original_form": "저는 달달한 걸 먹거나 푹 자거나 자연을 보러 가거나 하는 거 같아요~",
          "speaker_id": "2",
          "time": "20210824 10:18"
        },
        {
          "id": "MDRW2100010885.1.13",
          "form": "아 좋군요 코로나가 좀 걸리지만",
          "original_form": "아 좋군요 코로나가 좀 걸리지만",
          "speaker_id": "1",
          "time": "20210824 10:27"
        },
        {
          "id": "MDRW2100010885.1.14",
          "form": "달달이 저도 좋아요",
          "original_form": "달달이 저도 좋아요",
          "speaker_id": "1",
          "time": "20210824 10:27"
        },
        {
          "id": "MDRW2100010885.1.15",
          "form": "그래서 이것저것 간식 사먹는게 어느새 습관이 되가지고..ㅎㅎ",
          "original_form": "그래서 이것저것 간식 사먹는게 어느새 습관이 되가지고..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100010885.1.16",
          "form": "운동도 습관을 들여야 하는데 쉽지.않네요",
          "original_form": "운동도 습관을 들여야 하는데 쉽지.않네요",
          "speaker_id": "2",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100010885.1.17",
          "form": "저 앉아서 일하는데 서서하려구요ㅋㅋㅋ",
          "original_form": "저 앉아서 일하는데 서서하려구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100010885.1.18",
          "form": "하나씩 노력해보려구요ㅜㅜㅜ",
          "original_form": "하나씩 노력해보려구요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100010885.1.19",
          "form": "저도 하나씩 습관을 바꿔봐야겠어요~",
          "original_form": "저도 하나씩 습관을 바꿔봐야겠어요~",
          "speaker_id": "2",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100010885.1.20",
          "form": "자전거 잘타고싶어요",
          "original_form": "자전거 잘타고싶어요",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100010885.1.21",
          "form": "자전거로 출퇴해보고프네요",
          "original_form": "자전거로 출퇴해보고프네요",
