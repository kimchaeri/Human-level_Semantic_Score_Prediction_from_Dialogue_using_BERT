      "utterance": [
        {
          "id": "MDRW2100000674.1.1",
          "form": "가족관계는어떻게되세요?ㅎ",
          "original_form": "가족관계는어떻게되세요?ㅎ",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000674.1.2",
          "form": "저는 결혼해서 남편이랑 같이 살아요",
          "original_form": "저는 결혼해서 남편이랑 같이 살아요",
          "speaker_id": "2",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000674.1.3",
          "form": "저도 거기다 애둘이 있어여ㅜ",
          "original_form": "저도 거기다 애둘이 있어여ㅜ",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000674.1.4",
          "form": "아 그러시구나~!육아 힘드시죠ㅠ",
          "original_form": "아 그러시구나~!육아 힘드시죠ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000674.1.5",
          "form": "네ㅠ첫째둘째터울이 있어서 그나마나은데 역시 쉽지는 않아여ㅠ",
          "original_form": "네ㅠ첫째둘째터울이 있어서 그나마나은데 역시 쉽지는 않아여ㅠ",
          "speaker_id": "1",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000674.1.6",
          "form": "애들이 몇살이에요?",
          "original_form": "애들이 몇살이에요?",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000674.1.7",
          "form": "열살.세살이에요~",
          "original_form": "열살.세살이에요~",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000674.1.8",
          "form": "아~아직 손이 많이 가겠어요~",
          "original_form": "아~아직 손이 많이 가겠어요~",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000674.1.9",
          "form": "네세살이 특히ㅠㅠ",
          "original_form": "네세살이 특히ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000674.1.10",
          "form": "터울이 많이 나네요~ 근데 둘째가 너무 귀여울것 같아요",
          "original_form": "터울이 많이 나네요~ 근데 둘째가 너무 귀여울것 같아요",
          "speaker_id": "2",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000674.1.11",
          "form": "맞아요 딱귀여울때죠ㅎ",
          "original_form": "맞아요 딱귀여울때죠ㅎ",
          "speaker_id": "1",
          "time": "20210819 18:03"
        },
        {
          "id": "MDRW2100000674.1.12",
          "form": "집안일은 혼자 하시나요?",
          "original_form": "집안일은 혼자 하시나요?",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100000674.1.13",
          "form": "네ㅡ전업주부라 ㅠ 그래도 신랑이 많이 도와주긴해요",
          "original_form": "네ㅡ전업주부라 ㅠ 그래도 신랑이 많이 도와주긴해요",
