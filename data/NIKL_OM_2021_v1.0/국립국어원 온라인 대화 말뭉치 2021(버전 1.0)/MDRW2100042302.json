      "utterance": [
        {
          "id": "MDRW2100042302.1.1",
          "form": "일 하시죠?",
          "original_form": "일 하시죠?",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042302.1.2",
          "form": "네..일해요",
          "original_form": "네..일해요",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042302.1.3",
          "form": "저도요 이번달 너무 바쁘네요 ㅠ",
          "original_form": "저도요 이번달 너무 바쁘네요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042302.1.4",
          "form": "바쁘신일이 뭐예요?",
          "original_form": "바쁘신일이 뭐예요?",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042302.1.5",
          "form": "보고서 마감도 두개가 이번달 말이고",
          "original_form": "보고서 마감도 두개가 이번달 말이고",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042302.1.6",
          "form": "당장 금욜에도 pt 있고요",
          "original_form": "당장 금욜에도 pt 있고요",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042302.1.7",
          "form": "이번주 다음주에 출장도 나가야하고",
          "original_form": "이번주 다음주에 출장도 나가야하고",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042302.1.8",
          "form": "바쁘시네요",
          "original_form": "바쁘시네요",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042302.1.9",
          "form": "네 ㅠ",
          "original_form": "네 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042302.1.10",
          "form": "눈 뜨면 10월이었음 좋겠어요",
          "original_form": "눈 뜨면 10월이었음 좋겠어요",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042302.1.11",
          "form": "출장도가시고..부럽네요",
          "original_form": "출장도가시고..부럽네요",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042302.1.12",
          "form": "지금 할일이.산더미인데 출장을 두번이나 가려니 너무 부담스러운데요 ㅠ",
          "original_form": "지금 할일이.산더미인데 출장을 두번이나 가려니 너무 부담스러운데요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042302.1.13",
          "form": "전 출장한번도 안가봐서 가보고싶네요",
          "original_form": "전 출장한번도 안가봐서 가보고싶네요",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042302.1.14",
          "form": "놀러가는 기분으로 갈수도 있는데요. 바쁜데 나가려니 완전 스트레스예요 ㅠ",
          "original_form": "놀러가는 기분으로 갈수도 있는데요. 바쁜데 나가려니 완전 스트레스예요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 07:02"
        },
        {
          "id": "MDRW2100042302.1.15",
          "form": "그렇기도할것같아요",
          "original_form": "그렇기도할것같아요",
          "speaker_id": "1",
          "time": "20210907 07:19"
        },
        {
          "id": "MDRW2100042302.1.16",
          "form": "내일 출장가려고 오늘 늦게까지 근무했어요 ㅠ",
          "original_form": "내일 출장가려고 오늘 늦게까지 근무했어요 ㅠ",
          "speaker_id": "2",
