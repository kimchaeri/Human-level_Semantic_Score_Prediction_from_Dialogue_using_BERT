      "utterance": [
        {
          "id": "MDRW2100029168.1.1",
          "form": "넷플 고모가",
          "original_form": "넷플 고모가",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029168.1.2",
          "form": "엄청보심",
          "original_form": "엄청보심",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029168.1.3",
          "form": "하녀이야기 재밌다던데",
          "original_form": "하녀이야기 재밌다던데",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029168.1.4",
          "form": "왓챠인가 어딘가 있댔ㅇㅇㅁ",
          "original_form": "왓챠인가 어딘가 있댔ㅇㅇㅁ",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029168.1.5",
          "form": "저는 넷 플 릭스 안 써요",
          "original_form": "저는 넷 플 릭스 안 써요",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029168.1.6",
          "form": "그거 책이 원본이고",
          "original_form": "그거 책이 원본이고",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.7",
          "form": "와 차도 안 써요",
          "original_form": "와 차도 안 써요",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.8",
          "form": "글쓴이 쓴 다른책",
          "original_form": "글쓴이 쓴 다른책",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.9",
          "form": "쿠팡 플레이는 봐요",
          "original_form": "쿠팡 플레이는 봐요",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.10",
          "form": "그레이스라고 있는데",
          "original_form": "그레이스라고 있는데",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.11",
          "form": "넷플에 그레이스",
          "original_form": "넷플에 그레이스",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.12",
          "form": "있음",
          "original_form": "있음",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.13",
          "form": "매력적임",
          "original_form": "매력적임",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.14",
          "form": "재미있어요",
          "original_form": "재미있어요",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.15",
          "form": "쿠팡플레이가",
          "original_form": "쿠팡플레이가",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.16",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.17",
          "form": "쿠팡에서 나온 동영상 사이트에요",
          "original_form": "쿠팡에서 나온 동영상 사이트에요",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.18",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029168.1.19",
          "form": "영상많아?",
          "original_form": "영상많아?",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029168.1.20",
          "form": "우리 아이가 달라졌어요 그분 나오세요",
          "original_form": "우리 아이가 달라졌어요 그분 나오세요",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029168.1.21",
          "form": "건 모르겠어요",
          "original_form": "건 모르겠어요",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029168.1.22",
          "form": "티비 영화 드라마중 뭐가 많아",
          "original_form": "티비 영화 드라마중 뭐가 많아",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029168.1.23",
          "form": "이제 디즈니 플러스 곧 출시될텐데",
          "original_form": "이제 디즈니 플러스 곧 출시될텐데",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029168.1.24",
          "form": "그걸로 가끔 봐요",
          "original_form": "그걸로 가끔 봐요",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029168.1.25",
          "form": "궁금",
          "original_form": "궁금",
          "speaker_id": "2",
          "time": "20210903 00:03"
        }
      ]
