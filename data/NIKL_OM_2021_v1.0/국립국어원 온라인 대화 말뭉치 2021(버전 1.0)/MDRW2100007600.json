      "utterance": [
        {
          "id": "MDRW2100007600.1.1",
          "form": "저 당근에서 최근에",
          "original_form": "저 당근에서 최근에",
          "speaker_id": "1",
          "time": "20210822 04:04"
        },
        {
          "id": "MDRW2100007600.1.2",
          "form": "뚫어뻥 샀거든요",
          "original_form": "뚫어뻥 샀거든요",
          "speaker_id": "1",
          "time": "20210822 04:04"
        },
        {
          "id": "MDRW2100007600.1.3",
          "form": "미스터펑이라고",
          "original_form": "미스터펑이라고",
          "speaker_id": "1",
          "time": "20210822 04:04"
        },
        {
          "id": "MDRW2100007600.1.4",
          "form": "탄산실린더로 뚫는게 있어요",
          "original_form": "탄산실린더로 뚫는게 있어요",
          "speaker_id": "1",
          "time": "20210822 04:05"
        },
        {
          "id": "MDRW2100007600.1.5",
          "form": "욕조가 어떤놈이 씻었는지 막힌겁니다",
          "original_form": "욕조가 어떤놈이 씻었는지 막힌겁니다",
          "speaker_id": "1",
          "time": "20210822 04:05"
        },
        {
          "id": "MDRW2100007600.1.6",
          "form": "아무리 락스같은 걸 부어도 안뚫리길래 미스터펑이라는 뚫어뻥을 구매했죠",
          "original_form": "아무리 락스같은 걸 부어도 안뚫리길래 미스터펑이라는 뚫어뻥을 구매했죠",
          "speaker_id": "1",
          "time": "20210822 04:05"
        },
        {
          "id": "MDRW2100007600.1.7",
          "form": "공기를 총처럼 쏴서 집어넣는건데",
          "original_form": "공기를 총처럼 쏴서 집어넣는건데",
          "speaker_id": "1",
          "time": "20210822 04:05"
        },
        {
          "id": "MDRW2100007600.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007600.1.9",
          "form": "아니 그런게 있어요? 신기하다",
          "original_form": "아니 그런게 있어요? 신기하다",
          "speaker_id": "2",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007600.1.10",
          "form": "7방만에 뚫렸는데 욕조도 같이 뚫렸었답니다",
          "original_form": "7방만에 뚫렸는데 욕조도 같이 뚫렸었답니다",
          "speaker_id": "1",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007600.1.11",
          "form": "크게 혼났죠",
          "original_form": "크게 혼났죠",
          "speaker_id": "1",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007600.1.12",
          "form": "이게 버전이 여러가지 있었어요",
          "original_form": "이게 버전이 여러가지 있었어요",
          "speaker_id": "1",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007600.1.13",
          "form": "...?",
          "original_form": "...?",
          "speaker_id": "2",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007600.1.14",
          "form": "변기용",
          "original_form": "변기용",
          "speaker_id": "1",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007600.1.15",
          "form": "세면대용",
          "original_form": "세면대용",
          "speaker_id": "1",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007600.1.16",
          "form": "당근으로 뻥산비용<욕조고치는비용...?",
          "original_form": "당근으로 뻥산비용<욕조고치는비용...?",
          "speaker_id": "2",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007600.1.17",
          "form": "미스터펑이 진짜 세더라고요",
          "original_form": "미스터펑이 진짜 세더라고요",
          "speaker_id": "1",
          "time": "20210822 04:06"
        },
        {
          "id": "MDRW2100007600.1.18",
          "form": "변기용은 강추한대요",
          "original_form": "변기용은 강추한대요",
          "speaker_id": "1",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007600.1.19",
          "form": "yes",
          "original_form": "yes",
          "speaker_id": "1",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007600.1.20",
          "form": "but 잇츠 낫 마이 머니",
          "original_form": "but 잇츠 낫 마이 머니",
          "speaker_id": "1",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007600.1.21",
          "form": "마이 패런츠 쿠롸이",
          "original_form": "마이 패런츠 쿠롸이",
          "speaker_id": "1",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007600.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 ㅡㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 ㅡㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007600.1.23",
          "form": "마이 도터 검은머리짐승",
          "original_form": "마이 도터 검은머리짐승",
          "speaker_id": "1",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007600.1.24",
          "form": "전 당근사기 당햇엇어요",
          "original_form": "전 당근사기 당햇엇어요",
          "speaker_id": "2",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007600.1.25",
          "form": "헐 뭐죠",
          "original_form": "헐 뭐죠",
          "speaker_id": "1",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007600.1.26",
          "form": "나 당근사기 첨봐요",
          "original_form": "나 당근사기 첨봐요",
          "speaker_id": "1",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007600.1.27",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007600.1.28",
          "form": "약간 실습이었나요? 검찰로 가기 전 실습",
          "original_form": "약간 실습이었나요? 검찰로 가기 전 실습",
          "speaker_id": "1",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007600.1.29",
          "form": "실무 한 번 해보고자 고소장 접수",
          "original_form": "실무 한 번 해보고자 고소장 접수",
          "speaker_id": "1",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007600.1.30",
          "form": "아놔진짜상처주지마세요",
          "original_form": "아놔진짜상처주지마세요",
          "speaker_id": "2",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007600.1.31",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007600.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007600.1.33",
          "form": "진심으로 사과드립니다",
          "original_form": "진심으로 사과드립니다",
          "speaker_id": "1",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007600.1.34",
          "form": "취소.",
          "original_form": "취소.",
          "speaker_id": "1",
          "time": "20210822 04:09"
        },
        {
          "id": "MDRW2100007600.1.35",
          "form": "진짜웃기네ㅠ",
          "original_form": "진짜웃기네ㅠ",
          "speaker_id": "2",
          "time": "20210822 04:09"
        },
        {
          "id": "MDRW2100007600.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 04:09"
        },
        {
          "id": "MDRW2100007600.1.37",
          "form": "선풍기 사러 갔는데 진짜 다 멀쩡해서",
          "original_form": "선풍기 사러 갔는데 진짜 다 멀쩡해서",
          "speaker_id": "2",
          "time": "20210822 04:09"
        },
