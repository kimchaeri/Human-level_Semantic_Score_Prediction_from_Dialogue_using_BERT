      "utterance": [
        {
          "id": "MDRW2100010195.1.1",
          "form": "집안일 어떤 게 제일 힘들어?",
          "original_form": "집안일 어떤 게 제일 힘들어?",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010195.1.2",
          "form": "빨래",
          "original_form": "빨래",
          "speaker_id": "2",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010195.1.3",
          "form": "빨래 힘들지",
          "original_form": "빨래 힘들지",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010195.1.4",
          "form": "넘 어려워",
          "original_form": "넘 어려워",
          "speaker_id": "2",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010195.1.5",
          "form": "빨래가 은근히 신경쓸 게 많더라고",
          "original_form": "빨래가 은근히 신경쓸 게 많더라고",
          "speaker_id": "1",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010195.1.6",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010195.1.7",
          "form": "그럼 그나마 제일 나은 건 뭐야?",
          "original_form": "그럼 그나마 제일 나은 건 뭐야?",
          "speaker_id": "1",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010195.1.8",
          "form": "설거지",
          "original_form": "설거지",
          "speaker_id": "2",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010195.1.9",
          "form": "설거지가 그나마 만만한가?",
          "original_form": "설거지가 그나마 만만한가?",
          "speaker_id": "1",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010195.1.10",
          "form": "응 그나마?",
          "original_form": "응 그나마?",
          "speaker_id": "2",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010195.1.11",
          "form": "난 쓰레기 버리는 게 그나마 제일 나은 거 같아",
          "original_form": "난 쓰레기 버리는 게 그나마 제일 나은 거 같아",
          "speaker_id": "1",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010195.1.12",
          "form": "아 맞다 그것도 있었지",
          "original_form": "아 맞다 그것도 있었지",
          "speaker_id": "2",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010195.1.13",
          "form": "그럼 둘중에 하나를 고른다며?",
          "original_form": "그럼 둘중에 하나를 고른다며?",
          "speaker_id": "1",
          "time": "20210823 15:31"
        },
        {
          "id": "MDRW2100010195.1.14",
          "form": "둘 중에 뭐?",
          "original_form": "둘 중에 뭐?",
          "speaker_id": "2",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100010195.1.15",
          "form": "설거지랑 분리수거",
          "original_form": "설거지랑 분리수거",
          "speaker_id": "1",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100010195.1.16",
          "form": "분리수거",
          "original_form": "분리수거",
          "speaker_id": "2",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100010195.1.17",
          "form": "뭐야ㅋㅋㅋㅌ",
          "original_form": "뭐야ㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100010195.1.18",
          "form": "ㅋㅋㅋㅋ 그게 편하니까",
          "original_form": "ㅋㅋㅋㅋ 그게 편하니까",
          "speaker_id": "2",
