      "utterance": [
        {
          "id": "MDRW2100042678.1.1",
          "form": "아 오늘 코스트코 안 간다더니ㅠㅜ",
          "original_form": "아 오늘 코스트코 안 간다더니ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 01:05"
        },
        {
          "id": "MDRW2100042678.1.2",
          "form": "원래 안 간려고 했는데",
          "original_form": "원래 안 간려고 했는데",
          "speaker_id": "2",
          "time": "20210907 01:06"
        },
        {
          "id": "MDRW2100042678.1.3",
          "form": "아저씨 귀리 쉐이크가 떨어져서 간거야",
          "original_form": "아저씨 귀리 쉐이크가 떨어져서 간거야",
          "speaker_id": "2",
          "time": "20210907 01:06"
        },
        {
          "id": "MDRW2100042678.1.4",
          "form": "코스트코 가는줄 알았으면",
          "original_form": "코스트코 가는줄 알았으면",
          "speaker_id": "1",
          "time": "20210907 01:07"
        },
        {
          "id": "MDRW2100042678.1.5",
          "form": "커피빈에서 케이크 하는 건데ㅠㅜ",
          "original_form": "커피빈에서 케이크 하는 건데ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 01:08"
        },
        {
          "id": "MDRW2100042678.1.6",
          "form": "거기 차대기 힘들어ㅋㅋ",
          "original_form": "거기 차대기 힘들어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 01:11"
        },
        {
          "id": "MDRW2100042678.1.7",
          "form": "그래도 너무 아쉬움ㅠㅠㅠ",
          "original_form": "그래도 너무 아쉬움ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 01:12"
        },
        {
          "id": "MDRW2100042678.1.8",
          "form": "거기 케이크가 그렇게 먹고싶냐",
          "original_form": "거기 케이크가 그렇게 먹고싶냐",
          "speaker_id": "2",
          "time": "20210907 01:12"
        },
        {
          "id": "MDRW2100042678.1.9",
          "form": "맛있다던데 못먹어봐서 그럼",
          "original_form": "맛있다던데 못먹어봐서 그럼",
          "speaker_id": "1",
          "time": "20210907 01:12"
        },
        {
          "id": "MDRW2100042678.1.10",
          "form": "정관에 있을 때 먹지",
          "original_form": "정관에 있을 때 먹지",
          "speaker_id": "2",
          "time": "20210907 01:14"
        },
        {
          "id": "MDRW2100042678.1.11",
          "form": "그땐 커피빈 케이크 있는지도 몰라서ㅠ",
          "original_form": "그땐 커피빈 케이크 있는지도 몰라서ㅠ",
          "speaker_id": "1",
          "time": "20210907 01:14"
        },
        {
          "id": "MDRW2100042678.1.12",
          "form": "나중에 먹을기회 있겠지",
          "original_form": "나중에 먹을기회 있겠지",
          "speaker_id": "2",
          "time": "20210907 01:15"
        },
        {
          "id": "MDRW2100042678.1.13",
          "form": "아 코로나만 아니면 애들이랑",
          "original_form": "아 코로나만 아니면 애들이랑",
          "speaker_id": "1",
          "time": "20210907 01:15"
        },
        {
          "id": "MDRW2100042678.1.14",
          "form": "진작 갔다ㅠ",
          "original_form": "진작 갔다ㅠ",
          "speaker_id": "1",
          "time": "20210907 01:15"
        },
        {
          "id": "MDRW2100042678.1.15",
          "form": "요즘은 조심해야지",
          "original_form": "요즘은 조심해야지",
          "speaker_id": "2",
          "time": "20210907 01:16"
        },
        {
          "id": "MDRW2100042678.1.16",
          "form": "그래서 부산대 앞에서 결국 했잖앙..",
          "original_form": "그래서 부산대 앞에서 결국 했잖앙..",
          "speaker_id": "1",
          "time": "20210907 01:16"
