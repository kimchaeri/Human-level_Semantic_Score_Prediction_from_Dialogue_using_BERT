      "utterance": [
        {
          "id": "MDRW2100009773.1.1",
          "form": "연애는 어떠세요",
          "original_form": "연애는 어떠세요",
          "speaker_id": "1",
          "time": "20210823 09:44"
        },
        {
          "id": "MDRW2100009773.1.2",
          "form": "인연이 있다면?",
          "original_form": "인연이 있다면?",
          "speaker_id": "2",
          "time": "20210823 09:50"
        },
        {
          "id": "MDRW2100009773.1.3",
          "form": "흘러가는대로?",
          "original_form": "흘러가는대로?",
          "speaker_id": "2",
          "time": "20210823 09:50"
        },
        {
          "id": "MDRW2100009773.1.4",
          "form": "ㅎㅎ 자만추!",
          "original_form": "ㅎㅎ 자만추!",
          "speaker_id": "1",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009773.1.5",
          "form": "근데 코로나시국이라",
          "original_form": "근데 코로나시국이라",
          "speaker_id": "2",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009773.1.6",
          "form": "적극성이 필요한 것 같기도요",
          "original_form": "적극성이 필요한 것 같기도요",
          "speaker_id": "2",
          "time": "20210823 09:54"
        },
        {
          "id": "MDRW2100009773.1.7",
          "form": "맞아요ㅎㅎ 이상형은?",
          "original_form": "맞아요ㅎㅎ 이상형은?",
          "speaker_id": "1",
          "time": "20210823 09:55"
        },
        {
          "id": "MDRW2100009773.1.8",
          "form": "대화가 잘 통하는 사람?",
          "original_form": "대화가 잘 통하는 사람?",
          "speaker_id": "2",
          "time": "20210823 09:57"
        },
        {
          "id": "MDRW2100009773.1.9",
          "form": "그게 우선이긴해요",
          "original_form": "그게 우선이긴해요",
          "speaker_id": "1",
          "time": "20210823 09:57"
        },
        {
          "id": "MDRW2100009773.1.10",
          "form": "경제관념도 일치 해야 할 듯해요",
          "original_form": "경제관념도 일치 해야 할 듯해요",
          "speaker_id": "2",
          "time": "20210823 10:19"
        },
        {
          "id": "MDRW2100009773.1.11",
          "form": "맞아요 그거 크죠!",
          "original_form": "맞아요 그거 크죠!",
          "speaker_id": "1",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009773.1.12",
          "form": "결혼하셨나요",
          "original_form": "결혼하셨나요",
          "speaker_id": "2",
          "time": "20210823 12:31"
        },
        {
          "id": "MDRW2100009773.1.13",
          "form": "네 작년에 했어요ㅎㅎ",
          "original_form": "네 작년에 했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 12:57"
        },
        {
          "id": "MDRW2100009773.1.14",
          "form": "오 한참 신혼이실 때네요",
          "original_form": "오 한참 신혼이실 때네요",
          "speaker_id": "2",
          "time": "20210823 13:52"
        },
        {
          "id": "MDRW2100009773.1.15",
          "form": "네 ㅎㅎ 그쵸!",
          "original_form": "네 ㅎㅎ 그쵸!",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100009773.1.16",
          "form": "소개팅 안하세요?",
          "original_form": "소개팅 안하세요?",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100009773.1.17",
          "form": "코로나 핑계대면서 미적 대고있어요",
          "original_form": "코로나 핑계대면서 미적 대고있어요",
          "speaker_id": "2",
          "time": "20210823 14:20"
        }
      ]
