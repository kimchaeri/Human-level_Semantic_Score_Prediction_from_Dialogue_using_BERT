      "utterance": [
        {
          "id": "MDRW2100014341.1.1",
          "form": "저는 아이폰 강화유리를 샀어요",
          "original_form": "저는 아이폰 강화유리를 샀어요",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014341.1.2",
          "form": "오 강화유리 좋나요?",
          "original_form": "오 강화유리 좋나요?",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014341.1.3",
          "form": "전 일반필름보다 좋아요",
          "original_form": "전 일반필름보다 좋아요",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014341.1.4",
          "form": "혹시 상품 추천해주실 수 있냐요",
          "original_form": "혹시 상품 추천해주실 수 있냐요",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014341.1.5",
          "form": "전 신지모루 강화유리쓰는데 가성비가 좋아요",
          "original_form": "전 신지모루 강화유리쓰는데 가성비가 좋아요",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014341.1.6",
          "form": "저도 구매해볼게요",
          "original_form": "저도 구매해볼게요",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014341.1.7",
          "form": "전 이번에 생수를 샀어요",
          "original_form": "전 이번에 생수를 샀어요",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014341.1.8",
          "form": "수질부적합 기사보셨나요",
          "original_form": "수질부적합 기사보셨나요",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014341.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014341.1.10",
          "form": "그래서 구매문의에 문의했는데 괜찮은 제품이래서 다행이예요",
          "original_form": "그래서 구매문의에 문의했는데 괜찮은 제품이래서 다행이예요",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014341.1.11",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014341.1.12",
          "form": "다른 건 사신 거 없나요",
          "original_form": "다른 건 사신 거 없나요",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014341.1.13",
          "form": "순두부가 3개에 천원이어어샀어요",
          "original_form": "순두부가 3개에 천원이어어샀어요",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014341.1.14",
          "form": "저렴하게 잘 사셨어요",
          "original_form": "저렴하게 잘 사셨어요",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014341.1.15",
          "form": "쇼핑하는 거 조하하시나요",
          "original_form": "쇼핑하는 거 조하하시나요",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014341.1.16",
          "form": "아이쇼핑하는거좋앙새요",
          "original_form": "아이쇼핑하는거좋앙새요",
          "speaker_id": "2",
          "time": "20210825 18:35"
        }
