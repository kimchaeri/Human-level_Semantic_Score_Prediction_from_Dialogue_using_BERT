        },
        {
          "id": "MDRW2100008946.1.2",
          "form": "전 치킨이요",
          "original_form": "전 치킨이요",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100008946.1.3",
          "form": "배달시키셨나요?",
          "original_form": "배달시키셨나요?",
