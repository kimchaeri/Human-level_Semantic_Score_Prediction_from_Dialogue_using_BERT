        },
        {
          "id": "MDRW2100008946.1.10",
          "form": "다 못먹어요",
          "original_form": "다 못먹어요",
          "speaker_id": "1",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100008946.1.11",
          "form": "저희 집은 좀 많이 먹어서요ㅋㅋ",
