          "speaker_id": "1",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100008946.1.5",
          "form": "어떤 종류로 드셨나요?",
          "original_form": "어떤 종류로 드셨나요?",
          "speaker_id": "2",
          "time": "20210822 22:26"
