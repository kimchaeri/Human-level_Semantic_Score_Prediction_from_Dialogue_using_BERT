      "utterance": [
        {
          "id": "MDRW2100018111.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018111.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018111.1.3",
          "form": "배달 자주 시키나요",
          "original_form": "배달 자주 시키나요",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018111.1.4",
          "form": "네 너무 자주 시켜서 좀 줄이려구요 ㅠ",
          "original_form": "네 너무 자주 시켜서 좀 줄이려구요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018111.1.5",
          "form": "주로 뭐 시키세요",
          "original_form": "주로 뭐 시키세요",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018111.1.6",
          "form": "요샌 유가네닭갈비 많이 시켰어요",
          "original_form": "요샌 유가네닭갈비 많이 시켰어요",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018111.1.7",
          "form": "와 그것도 배달이 되는 줄 몰맀어요",
          "original_form": "와 그것도 배달이 되는 줄 몰맀어요",
          "speaker_id": "1",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018111.1.8",
          "form": "그럼 볶음밥은 못 먹나요",
          "original_form": "그럼 볶음밥은 못 먹나요",
          "speaker_id": "1",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018111.1.9",
          "form": "볶음밥도 배달해줘여 ㅋㅋㅋ",
          "original_form": "볶음밥도 배달해줘여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018111.1.10",
          "form": "장점이 시키면 볶음밥이랑 닭이랑 동시에 먹을 수 있어서 좋아요",
          "original_form": "장점이 시키면 볶음밥이랑 닭이랑 동시에 먹을 수 있어서 좋아요",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018111.1.11",
          "form": "대박 신박하네요",
          "original_form": "대박 신박하네요",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018111.1.12",
          "form": "볶음밥만도 배달도 돼요",
          "original_form": "볶음밥만도 배달도 돼요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018111.1.13",
          "form": "눌은 맛도 살아있나요",
          "original_form": "눌은 맛도 살아있나요",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018111.1.14",
          "form": "불맛나는 누룽지...같은.. 뭔지 아시죠",
          "original_form": "불맛나는 누룽지...같은.. 뭔지 아시죠",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018111.1.15",
          "form": "알죠 볶음밥은 눌린거 긁어먹는 맛이에요 ㅋㅋㅋ",
          "original_form": "알죠 볶음밥은 눌린거 긁어먹는 맛이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018111.1.16",
          "form": "와 ㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018111.1.17",
          "form": "치킨이나 떡볶이는",
          "original_form": "치킨이나 떡볶이는",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018111.1.18",
          "form": "어디 브랜드 좋아하세요",
          "original_form": "어디 브랜드 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018111.1.19",
          "form": "음 떡볶이는 신전이나 신참 달고마가 맛있고",
          "original_form": "음 떡볶이는 신전이나 신참 달고마가 맛있고",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018111.1.20",
          "form": "치킨은 뿌링클이요 ㅋㅋㅋ",
          "original_form": "치킨은 뿌링클이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018111.1.21",
          "form": "신참 달고나 먹어보고 싶었는데 ㅋㅋ",
          "original_form": "신참 달고나 먹어보고 싶었는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:18"
        }
      ]
