      "utterance": [
        {
          "id": "MDRW2100023882.1.1",
          "form": "네 굉장히 어려울거같다구 하더라고요",
          "original_form": "네 굉장히 어려울거같다구 하더라고요",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023882.1.2",
          "form": "작가도 바뿌고 배우들도 여간 바쁜게 아니라 ㅠ ㅠ",
          "original_form": "작가도 바뿌고 배우들도 여간 바쁜게 아니라 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023882.1.3",
          "form": "으악 하긴 킹덤 쓰는 중이니 ㅠ.^",
          "original_form": "으악 하긴 킹덤 쓰는 중이니 ㅠ.^",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023882.1.4",
          "form": "킹덤은 보셨나요?!",
          "original_form": "킹덤은 보셨나요?!",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023882.1.5",
          "form": "앗 아녀 킹덤 너무 무섭다는 말을 들어서 도전조차 하지 못해ㅛ습니다..",
          "original_form": "앗 아녀 킹덤 너무 무섭다는 말을 들어서 도전조차 하지 못해ㅛ습니다..",
          "speaker_id": "2",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023882.1.6",
          "form": "허엇 완전 재밌는데 아쉽군요",
          "original_form": "허엇 완전 재밌는데 아쉽군요",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023882.1.7",
          "form": "name2은 무서운 거 잘 못 보시나요?!",
          "original_form": "&name2&은 무서운 거 잘 못 보시나요?!",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023882.1.8",
          "form": "냉..!! 전 무서운거 잘 못봐서요..ㅠㅠ 너무 아쉬워여",
          "original_form": "냉..!! 전 무서운거 잘 못봐서요..ㅠㅠ 너무 아쉬워여",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100023882.1.9",
          "form": "볼 수 있는게 한정적인거같아요",
          "original_form": "볼 수 있는게 한정적인거같아요",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100023882.1.10",
          "form": "앗 그래도 다른 잔잔한 드라마 많으니까..!",
          "original_form": "앗 그래도 다른 잔잔한 드라마 많으니까..!",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100023882.1.11",
          "form": "응답하라 시리즈 보셨나요?",
          "original_form": "응답하라 시리즈 보셨나요?",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100023882.1.12",
          "form": "헐~!!!",
          "original_form": "헐~!!!",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100023882.1.13",
          "form": "네 저 응답하라 완전 좋아해요",
          "original_form": "네 저 응답하라 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100023882.1.14",
          "form": "응답하라 중에 최애 어떤거세요?",
          "original_form": "응답하라 중에 최애 어떤거세요?",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100023882.1.15",
          "form": "저 저 혜리 나온 거 제일 좋아해요!!!",
          "original_form": "저 저 혜리 나온 거 제일 좋아해요!!!",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100023882.1.16",
          "form": "그 서브 커플을 완전 좋아햇거든요",
          "original_form": "그 서브 커플을 완전 좋아햇거든요",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100023882.1.17",
          "form": "보라 선우!",
          "original_form": "보라 선우!",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100023882.1.18",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100023882.1.19",
          "form": "저도 보라 선우.. 사랑했어요",
          "original_form": "저도 보라 선우.. 사랑했어요",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100023882.1.20",
          "form": "어쩜 이럴수가",
          "original_form": "어쩜 이럴수가",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100023882.1.21",
          "form": "헐헐 대박!!!!",
          "original_form": "헐헐 대박!!!!",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100023882.1.22",
          "form": "name2 어남류였어요 택이었어요!",
          "original_form": "&name2& 어남류였어요 택이었어요!",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100023882.1.23",
          "form": "전 당근 어남택..",
          "original_form": "전 당근 어남택..",
          "speaker_id": "2",
          "time": "20210831 15:00"
        },
        {
          "id": "MDRW2100023882.1.24",
          "form": "택이 얼굴이 서사아님니가..",
          "original_form": "택이 얼굴이 서사아님니가..",
          "speaker_id": "2",
          "time": "20210831 15:00"
        },
        {
          "id": "MDRW2100023882.1.25",
          "form": "앗 아니...!!!!",
          "original_form": "앗 아니...!!!!",
          "speaker_id": "1",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100023882.1.26",
          "form": "저는..저는",
          "original_form": "저는..저는",
          "speaker_id": "1",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100023882.1.27",
          "form": "어남류였단 말이에욥",
          "original_form": "어남류였단 말이에욥",
          "speaker_id": "1",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100023882.1.28",
          "form": "아니 이런~!!!!",
          "original_form": "아니 이런~!!!!",
          "speaker_id": "2",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100023882.1.29",
          "form": "어남류 대부뷴 사람들이 어남류였던거 같아요 ㅎㅎ",
          "original_form": "어남류 대부뷴 사람들이 어남류였던거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100023882.1.30",
          "form": "그래서 저 마지막에 좀 충격받았어요",
          "original_form": "그래서 저 마지막에 좀 충격받았어요",
          "speaker_id": "1",
          "time": "20210831 16:14"
        },
        {
          "id": "MDRW2100023882.1.31",
          "form": "물론 택도 넘 좋아요 ㅠ.!",
          "original_form": "물론 택도 넘 좋아요 ㅠ.!",
          "speaker_id": "1",
          "time": "20210831 16:14"
        },
        {
          "id": "MDRW2100023882.1.32",
