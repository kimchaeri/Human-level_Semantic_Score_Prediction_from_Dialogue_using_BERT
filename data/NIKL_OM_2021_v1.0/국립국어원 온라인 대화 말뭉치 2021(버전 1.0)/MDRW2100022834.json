        {
          "id": "MDRW2100022834.1.1",
          "form": "반려동물은 읍지만 랜선으로 키운다",
          "original_form": "반려동물은 읍지만 랜선으로 키운다",
          "speaker_id": "1",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022834.1.2",
          "form": "난 두줄이뿐",
          "original_form": "난 두줄이뿐",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022834.1.3",
          "form": "언니네 그 사촌인가",
          "original_form": "언니네 그 사촌인가",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022834.1.4",
          "form": "기여운댕댕이잇자나",
          "original_form": "기여운댕댕이잇자나",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022834.1.5",
          "form": "그래서 랜선으로 키우징 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 랜선으로 키우징 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022834.1.6",
          "form": "근데 안본지 오래됨",
          "original_form": "근데 안본지 오래됨",
          "speaker_id": "1",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022834.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022834.1.8",
          "form": "힝ㅜㅜ",
          "original_form": "힝ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022834.1.9",
          "form": "우선 멀고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우선 멀고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022834.1.10",
          "form": "사진마니보내주새요오",
          "original_form": "사진마니보내주새요오",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022834.1.11",
          "form": "후쿠도기엽고",
          "original_form": "후쿠도기엽고",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022834.1.12",
          "form": "나 이걸로 사람들이 말 좀 걸어오드라",
          "original_form": "나 이걸로 사람들이 말 좀 걸어오드라",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022834.1.13",
          "form": "보리도기엽고",
          "original_form": "보리도기엽고",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022834.1.14",
          "form": "그렇게",
          "original_form": "그렇게",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022834.1.15",
          "form": "그러더라",
          "original_form": "그러더라",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022834.1.16",
          "form": "제출을 꽤함",
          "original_form": "제출을 꽤함",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022834.1.17",
          "form": "마자 남의집 멍멍이라 귀엽다",
          "original_form": "마자 남의집 멍멍이라 귀엽다",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022834.1.18",
          "form": "동물얘기..모가좋지",
          "original_form": "동물얘기..모가좋지",
