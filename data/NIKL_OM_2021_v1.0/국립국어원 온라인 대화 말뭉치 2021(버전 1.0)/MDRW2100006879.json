      "utterance": [
        {
          "id": "MDRW2100006879.1.1",
          "form": "당신덕이지",
          "original_form": "당신덕이지",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006879.1.2",
          "form": "당신이 우리 3모녀를 먹여살리느라 고생이 많았죠",
          "original_form": "당신이 우리 3모녀를 먹여살리느라 고생이 많았죠",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006879.1.3",
          "form": "당연한 일인것인데",
          "original_form": "당연한 일인것인데",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006879.1.4",
          "form": "그것도 못하는 사람들도 많고",
          "original_form": "그것도 못하는 사람들도 많고",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006879.1.5",
          "form": "또 하고 싶어도 능력이 없어서 못하는 경우도 있어요",
          "original_form": "또 하고 싶어도 능력이 없어서 못하는 경우도 있어요",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100006879.1.6",
          "form": "하느님이 도와주셨어",
          "original_form": "하느님이 도와주셨어",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100006879.1.7",
          "form": "정말 그런거 같아요",
          "original_form": "정말 그런거 같아요",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100006879.1.8",
          "form": "우리 네식구 계속 잘 살자고",
          "original_form": "우리 네식구 계속 잘 살자고",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100006879.1.9",
          "form": "네 우리는 환갑도 지나고 늙어가지만 딸들이 있어서 든든해요",
          "original_form": "네 우리는 환갑도 지나고 늙어가지만 딸들이 있어서 든든해요",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006879.1.10",
          "form": "착한 딸들이야",
          "original_form": "착한 딸들이야",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006879.1.11",
          "form": "부모 챙길줄 아는 딸들이라 고맙죠",
          "original_form": "부모 챙길줄 아는 딸들이라 고맙죠",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006879.1.12",
          "form": "효녀들이지",
          "original_form": "효녀들이지",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006879.1.13",
          "form": "아빠가 워낙  효자라 닮았나봐요",
          "original_form": "아빠가 워낙  효자라 닮았나봐요",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006879.1.14",
          "form": "덕분에 당신이 고생했지",
          "original_form": "덕분에 당신이 고생했지",
          "speaker_id": "2",
