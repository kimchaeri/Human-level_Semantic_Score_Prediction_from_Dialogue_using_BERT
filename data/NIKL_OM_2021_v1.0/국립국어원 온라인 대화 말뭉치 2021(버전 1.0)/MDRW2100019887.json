      "utterance": [
        {
          "id": "MDRW2100019887.1.1",
          "form": "군대 하면 생각하는게ㅎ",
          "original_form": "군대 하면 생각하는게ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019887.1.2",
          "form": "초코파이가 가장생각나요ㅎ",
          "original_form": "초코파이가 가장생각나요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019887.1.3",
          "form": "왜용왜요~",
          "original_form": "왜용왜요~",
          "speaker_id": "1",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019887.1.4",
          "form": "주말에 한번은 종교활동이라고 절, 교회, 휴식 이렇게 시간을 주거든요 ㅎ",
          "original_form": "주말에 한번은 종교활동이라고 절, 교회, 휴식 이렇게 시간을 주거든요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019887.1.5",
          "form": "그춀~~",
          "original_form": "그춀~~",
          "speaker_id": "1",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019887.1.6",
          "form": "그럼 보통 교회가면 초코파이를 줬던것같애요ㅎ",
          "original_form": "그럼 보통 교회가면 초코파이를 줬던것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019887.1.7",
          "form": "그래서 저는 무교이긴 하지만 그래도 불교에 가까운데",
          "original_form": "그래서 저는 무교이긴 하지만 그래도 불교에 가까운데",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019887.1.8",
          "form": "석가모니님을 배신하고ㅎㅎ 교회로ㅎㅎ",
          "original_form": "석가모니님을 배신하고ㅎㅎ 교회로ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019887.1.9",
          "form": "ㅋㅋㅋ 초코파이의 힘",
          "original_form": "ㅋㅋㅋ 초코파이의 힘",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019887.1.10",
          "form": "그땐ㅎ 종교보다 초코파이가 좋았어요ㅎ",
          "original_form": "그땐ㅎ 종교보다 초코파이가 좋았어요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019887.1.11",
          "form": "화생방도하고 뭐 암튼 다했네요ㅎㅎ 4주 캠프동안ㅎ",
          "original_form": "화생방도하고 뭐 암튼 다했네요ㅎㅎ 4주 캠프동안ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019887.1.12",
          "form": "당땡길때 초코파이 ~~",
          "original_form": "당땡길때 초코파이 ~~",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019887.1.13",
          "form": "ㅎㅎ 엄청 좋죠ㅎㅎ",
          "original_form": "ㅎㅎ 엄청 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019887.1.14",
          "form": "그때 암튼 담배도 피우게 해주ㅏㅆ어요ㅎ",
          "original_form": "그때 암튼 담배도 피우게 해주ㅏㅆ어요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019887.1.15",
          "form": "진짜 짧게 가도 할건 다했네요",
          "original_form": "진짜 짧게 가도 할건 다했네요",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019887.1.16",
          "form": "전 어릴때부터 안폈는데ㅎ",
          "original_form": "전 어릴때부터 안폈는데ㅎ",
