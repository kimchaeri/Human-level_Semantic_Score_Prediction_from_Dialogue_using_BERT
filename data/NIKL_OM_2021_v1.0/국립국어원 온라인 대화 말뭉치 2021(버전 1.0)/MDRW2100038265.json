        },
        {
          "id": "MDRW2100038265.1.7",
          "form": "네 저는 아이들키우고있어요ㅋ",
          "original_form": "네 저는 아이들키우고있어요ㅋ",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100038265.1.8",
          "form": "저희 애들은 다 컸어요 ㅎㅎ",
          "original_form": "저희 애들은 다 컸어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100038265.1.9",
          "form": "와 너무좋겠어요. 부럽습니다ㅋㅋ",
          "original_form": "와 너무좋겠어요. 부럽습니다ㅋㅋ",
