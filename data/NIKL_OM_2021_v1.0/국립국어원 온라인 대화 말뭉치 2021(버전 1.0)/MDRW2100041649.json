      "utterance": [
        {
          "id": "MDRW2100041649.1.1",
          "form": "그게뭔가요?",
          "original_form": "그게뭔가요?",
          "speaker_id": "2",
          "time": "20210906 13:52"
        },
        {
          "id": "MDRW2100041649.1.2",
          "form": "과자같으건데 통밀로 만든 비스켓이요",
          "original_form": "과자같으건데 통밀로 만든 비스켓이요",
          "speaker_id": "1",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100041649.1.3",
          "form": "근데 왜 토스트에요?",
          "original_form": "근데 왜 토스트에요?",
          "speaker_id": "2",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100041649.1.4",
          "form": "모양을 꼭 작은 미니식빵처럼 만들어놨어요 ㅋㅋ",
          "original_form": "모양을 꼭 작은 미니식빵처럼 만들어놨어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100041649.1.5",
          "form": "아하 ㅋㅋㅋ귀엽네요",
          "original_form": "아하 ㅋㅋㅋ귀엽네요",
          "speaker_id": "2",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100041649.1.6",
          "form": "맛도 바싹 구운 토스트식빵같아요",
          "original_form": "맛도 바싹 구운 토스트식빵같아요",
          "speaker_id": "1",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100041649.1.7",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100041649.1.8",
          "form": "네 저는 주로 크림치즈 올려 먹어요",
          "original_form": "네 저는 주로 크림치즈 올려 먹어요",
          "speaker_id": "1",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100041649.1.9",
          "form": "헉 맛있겠어요 저 크림치즈 좋아하는데",
          "original_form": "헉 맛있겠어요 저 크림치즈 좋아하는데",
          "speaker_id": "2",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100041649.1.10",
          "form": "네 베이글이나 빵에 올려먹어도 괜찮긴 한데..",
          "original_form": "네 베이글이나 빵에 올려먹어도 괜찮긴 한데..",
          "speaker_id": "1",
          "time": "20210906 13:54"
        },
        {
          "id": "MDRW2100041649.1.11",
          "form": "맛있죠 완전",
          "original_form": "맛있죠 완전",
          "speaker_id": "2",
          "time": "20210906 13:54"
        },
        {
          "id": "MDRW2100041649.1.12",
          "form": "저거는 다이어트할때 먹는데 좋아요 ㅋㅋ",
          "original_form": "저거는 다이어트할때 먹는데 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:54"
        },
        {
          "id": "MDRW2100041649.1.13",
          "form": "과잔데 먹어도 살안쪄요?",
          "original_form": "과잔데 먹어도 살안쪄요?",
          "speaker_id": "2",
          "time": "20210906 13:54"
        },
        {
          "id": "MDRW2100041649.1.14",
          "form": "너무 많이안먹고 밥대신 몇조각 먹으면요!",
          "original_form": "너무 많이안먹고 밥대신 몇조각 먹으면요!",
          "speaker_id": "1",
          "time": "20210906 13:54"
        }
