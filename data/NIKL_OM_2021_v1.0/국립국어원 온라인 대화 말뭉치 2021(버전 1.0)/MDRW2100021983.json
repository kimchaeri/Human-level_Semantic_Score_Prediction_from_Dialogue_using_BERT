      "utterance": [
        {
          "id": "MDRW2100021983.1.1",
          "form": "거기 한번도 못가봐서 가보고 싶어요",
          "original_form": "거기 한번도 못가봐서 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021983.1.2",
          "form": "아 이제 백종원 채널 나온지도 꽤 됐고",
          "original_form": "아 이제 백종원 채널 나온지도 꽤 됐고",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021983.1.3",
          "form": "최근에는 코로나 땜에 멀리 안 돌아다니느라 못가봤어요",
          "original_form": "최근에는 코로나 땜에 멀리 안 돌아다니느라 못가봤어요",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021983.1.4",
          "form": "코로나로 관광객도 줄었으니 괜찮지 않을까요ㅎ",
          "original_form": "코로나로 관광객도 줄었으니 괜찮지 않을까요ㅎ",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021983.1.5",
          "form": "그쵸ㅜㅜ",
          "original_form": "그쵸ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021983.1.6",
          "form": "여수 부산 식도락 여행으로 가고 싶은데ㅜ",
          "original_form": "여수 부산 식도락 여행으로 가고 싶은데ㅜ",
          "speaker_id": "1",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021983.1.7",
          "form": "여수 가시면 서대회 추천 드립니당",
          "original_form": "여수 가시면 서대회 추천 드립니당",
          "speaker_id": "2",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021983.1.8",
          "form": "엇 회무침 얘기하시는 건가요?",
          "original_form": "엇 회무침 얘기하시는 건가요?",
          "speaker_id": "1",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021983.1.9",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021983.1.10",
          "form": "저 진짜 좋아하거든요",
          "original_form": "저 진짜 좋아하거든요",
          "speaker_id": "2",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021983.1.11",
          "form": "그 근처가 고향이라서",
          "original_form": "그 근처가 고향이라서",
          "speaker_id": "2",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021983.1.12",
          "form": "그래서 식재료 다양하게 쓰는 음식들 잘 먹는 편이기도 한것같아요",
          "original_form": "그래서 식재료 다양하게 쓰는 음식들 잘 먹는 편이기도 한것같아요",
          "speaker_id": "2",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021983.1.13",
          "form": "와 여수 진짜 맛있는 동네죠",
          "original_form": "와 여수 진짜 맛있는 동네죠",
          "speaker_id": "1",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021983.1.14",
          "form": "자주 드시겠네요 부러워요ㅜ",
          "original_form": "자주 드시겠네요 부러워요ㅜ",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021983.1.15",
          "form": "여수는 진짜 가까워서 어릴 때부터 자주 갔어요",
          "original_form": "여수는 진짜 가까워서 어릴 때부터 자주 갔어요",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021983.1.16",
          "form": "요즘은 가까이 안 살아서 자주 못가지만...",
          "original_form": "요즘은 가까이 안 살아서 자주 못가지만...",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021983.1.17",
          "form": "갓김치 좋아하시나요",
          "original_form": "갓김치 좋아하시나요",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021983.1.18",
          "form": "제 최애김치라...",
          "original_form": "제 최애김치라...",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021983.1.19",
          "form": "갓김치 고들빼기김치 파김치 삼대장",
          "original_form": "갓김치 고들빼기김치 파김치 삼대장",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021983.1.20",
          "form": "갓김치ㅎㅎ 귀하고 없어서 못 먹죠",
          "original_form": "갓김치ㅎㅎ 귀하고 없어서 못 먹죠",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021983.1.21",
          "form": "김치잘알",
          "original_form": "김치잘알",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021983.1.22",
          "form": "저는 라면엔 무조선 갓김치",
          "original_form": "저는 라면엔 무조선 갓김치",
          "speaker_id": "2",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021983.1.23",
          "form": "서울에 제가 좋아하는 삼겹살 가게가 있었거든요",
          "original_form": "서울에 제가 좋아하는 삼겹살 가게가 있었거든요",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021983.1.24",
          "form": "고들빼기도 씁쓸하고 맛있어용ㅋㅋㅋ",
          "original_form": "고들빼기도 씁쓸하고 맛있어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021983.1.25",
          "form": "수요미식회도 나온 육통령이라고",
          "original_form": "수요미식회도 나온 육통령이라고",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021983.1.26",
          "form": "고들빼기도 취향일 것 같아요!!!",
          "original_form": "고들빼기도 취향일 것 같아요!!!",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021983.1.27",
          "form": "육통령이 갓김치  같이 주는게 그것도 유명해요",
          "original_form": "육통령이 갓김치  같이 주는게 그것도 유명해요",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021983.1.28",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021983.1.29",
          "form": "솔직히 갓김치 먹으러 갔죠ㅎㅎ",
          "original_form": "솔직히 갓김치 먹으러 갔죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021983.1.30",
          "form": "삼겹살이랑 먹어도 무조건 맛있으니까요",
          "original_form": "삼겹살이랑 먹어도 무조건 맛있으니까요",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021983.1.31",
          "form": "근데 어느날부터 안주더라고요ㅜ",
          "original_form": "근데 어느날부터 안주더라고요ㅜ",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021983.1.32",
          "form": "수요미식회서도 극찬한 갓김치인데",
          "original_form": "수요미식회서도 극찬한 갓김치인데",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021983.1.33",
          "form": "물가가 올랐나요ㅜㅜ 그후 발길 끊었어요",
          "original_form": "물가가 올랐나요ㅜㅜ 그후 발길 끊었어요",
          "speaker_id": "1",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021983.1.34",
          "form": "헐...ㅜㅜ 지금 가보려고 가까운 곳 검색중이었는데 안나온다니....",
          "original_form": "헐...ㅜㅜ 지금 가보려고 가까운 곳 검색중이었는데 안나온다니....",
          "speaker_id": "2",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021983.1.35",
          "form": "근데 여기 고기가 제대로이긴해요",
          "original_form": "근데 여기 고기가 제대로이긴해요",
          "speaker_id": "1",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021983.1.36",
          "form": "저녁에 가면 줄 서고",
          "original_form": "저녁에 가면 줄 서고",
          "speaker_id": "1",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021983.1.37",
          "form": "그래도 전 갓김치에 빈정 상해서ㅜㅜ 안 가요",
          "original_form": "그래도 전 갓김치에 빈정 상해서ㅜㅜ 안 가요",
          "speaker_id": "1",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021983.1.38",
          "form": "주던걸 뺏다니 부들부들",
          "original_form": "주던걸 뺏다니 부들부들",
          "speaker_id": "1",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021983.1.39",
          "form": "갓김치도 사먹으면 비싸겠죠...?",
          "original_form": "갓김치도 사먹으면 비싸겠죠...?",
          "speaker_id": "2",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021983.1.40",
          "form": "맞아요 왠지 줬다뱄으면 좀 그래요",
          "original_form": "맞아요 왠지 줬다뱄으면 좀 그래요",
          "speaker_id": "2",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021983.1.41",
          "form": "헉 검색하니 다시 주네요",
          "original_form": "헉 검색하니 다시 주네요",
          "speaker_id": "1",
          "time": "20210829 00:42"
        },
        {
          "id": "MDRW2100021983.1.42",
          "form": "그럼 여기 추천이요ㅎㅎㅎ",
          "original_form": "그럼 여기 추천이요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:42"
        },
        {
          "id": "MDRW2100021983.1.43",
          "form": "잠시 안 줬던건가요",
          "original_form": "잠시 안 줬던건가요",
          "speaker_id": "2",
          "time": "20210829 00:43"
        },
        {
          "id": "MDRW2100021983.1.44",
          "form": "저도 가서 먹어봐야겠어요",
          "original_form": "저도 가서 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210829 00:43"
        },
        {
          "id": "MDRW2100021983.1.45",
          "form": "그런가봐요 넹넹",
          "original_form": "그런가봐요 넹넹",
          "speaker_id": "1",
          "time": "20210829 00:43"
        }
      ]
    }
