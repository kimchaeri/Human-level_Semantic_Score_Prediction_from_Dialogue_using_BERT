      "utterance": [
        {
          "id": "MDRW2100011716.1.1",
          "form": "조이랑 크러쉬 열애설 봤니",
          "original_form": "조이랑 크러쉬 열애설 봤니",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011716.1.2",
          "form": "설이 아니지 ㅋㅋㅋ",
          "original_form": "설이 아니지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011716.1.3",
          "form": "응 귀엽드라ㅋㅋㅋ",
          "original_form": "응 귀엽드라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011716.1.4",
          "form": "귀여운것들끼리 잘 만났네 했다ㅋㅋ",
          "original_form": "귀여운것들끼리 잘 만났네 했다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011716.1.5",
          "form": "둘이 작업할때 영상 댓글에 누가 크러쉬 소개팅 나와서 뚝딱거리는거같다했는데 그 댓글에 사람들이 다들 놀라하고 있더라 ㅋㅋㅋㅋ",
          "original_form": "둘이 작업할때 영상 댓글에 누가 크러쉬 소개팅 나와서 뚝딱거리는거같다했는데 그 댓글에 사람들이 다들 놀라하고 있더라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011716.1.6",
          "form": "그 댓글 쓴 사람 돗자리 깔아야겠네ㅋㅋㅋ",
          "original_form": "그 댓글 쓴 사람 돗자리 깔아야겠네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100011716.1.7",
          "form": "그도 그럴만한게 조이가 쳐다보고 노래부르니 가사를 까먹음 ㅋㅋㅋ",
          "original_form": "그도 그럴만한게 조이가 쳐다보고 노래부르니 가사를 까먹음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:27"
        },
        {
          "id": "MDRW2100011716.1.8",
          "form": "크러쉬 ㅋㅋㅋ",
          "original_form": "크러쉬 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:27"
        },
        {
          "id": "MDRW2100011716.1.9",
          "form": "캬항ㅋㅋㅋ 귀엽다ㅋㅋㅋㅋ",
          "original_form": "캬항ㅋㅋㅋ 귀엽다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:27"
        },
        {
          "id": "MDRW2100011716.1.10",
          "form": "둘이 뭔가 잘 어울림",
          "original_form": "둘이 뭔가 잘 어울림",
          "speaker_id": "1",
          "time": "20210824 15:28"
        },
        {
          "id": "MDRW2100011716.1.11",
          "form": "같이 강아지 산책하는 사진도 떴더만",
          "original_form": "같이 강아지 산책하는 사진도 떴더만",
          "speaker_id": "1",
          "time": "20210824 15:28"
        },
        {
          "id": "MDRW2100011716.1.12",
          "form": "둘이서 같이?",
          "original_form": "둘이서 같이?",
          "speaker_id": "2",
          "time": "20210824 15:28"
        },
        {
          "id": "MDRW2100011716.1.13",
          "form": "응 각자 강아지 데리고 ㅋㅋㅋ",
          "original_form": "응 각자 강아지 데리고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011716.1.14",
          "form": "좋을때다ㅋㅋㅋ",
          "original_form": "좋을때다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011716.1.15",
          "form": "그니까 풋풋하고 보기 좋더라",
          "original_form": "그니까 풋풋하고 보기 좋더라",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011716.1.16",
          "form": "난 크러쉬 라디오스타 걸크러쉬편에 나왔을때부터 좋아했음ㅋㅋ그편 되게 웃겼어",
          "original_form": "난 크러쉬 라디오스타 걸크러쉬편에 나왔을때부터 좋아했음ㅋㅋ그편 되게 웃겼어",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011716.1.17",
          "form": "또래의 연애는 참 보기가 좋구나 ㅋㅋ",
          "original_form": "또래의 연애는 참 보기가 좋구나 ㅋㅋ",
