      "utterance": [
        {
          "id": "MDRW2100010100.1.1",
          "form": "인터넷 쇼핑할 때 어디 많이 이용해?",
          "original_form": "인터넷 쇼핑할 때 어디 많이 이용해?",
          "speaker_id": "1",
          "time": "20210823 14:22"
        },
        {
          "id": "MDRW2100010100.1.2",
          "form": "여러가진데",
          "original_form": "여러가진데",
          "speaker_id": "2",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100010100.1.3",
          "form": "제일 많이 쓰는 거 하나만 얘기해봐",
          "original_form": "제일 많이 쓰는 거 하나만 얘기해봐",
          "speaker_id": "1",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100010100.1.4",
          "form": "하이버",
          "original_form": "하이버",
          "speaker_id": "2",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100010100.1.5",
          "form": "인터넷 쇼핑몰?",
          "original_form": "인터넷 쇼핑몰?",
          "speaker_id": "1",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100010100.1.6",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100010100.1.7",
          "form": "처음들어보네",
          "original_form": "처음들어보네",
          "speaker_id": "1",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100010100.1.8",
          "form": "그래? 무슨 인터넷 쇼핑몰 이용해?",
          "original_form": "그래? 무슨 인터넷 쇼핑몰 이용해?",
          "speaker_id": "2",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100010100.1.9",
          "form": "네이버쇼핑이랑 쿠팡",
          "original_form": "네이버쇼핑이랑 쿠팡",
          "speaker_id": "1",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100010100.1.10",
          "form": "아 맞다 쿠팡도 있었지?",
          "original_form": "아 맞다 쿠팡도 있었지?",
          "speaker_id": "2",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100010100.1.11",
          "form": "응 쿠팡 요즘 진짜 많이 쓰지",
          "original_form": "응 쿠팡 요즘 진짜 많이 쓰지",
          "speaker_id": "1",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100010100.1.12",
          "form": "쿠팡이 젤 싸지 않나?",
          "original_form": "쿠팡이 젤 싸지 않나?",
          "speaker_id": "2",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100010100.1.13",
          "form": "꼭 그렇지는 않은데 배송이 빨라서",
          "original_form": "꼭 그렇지는 않은데 배송이 빨라서",
          "speaker_id": "1",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100010100.1.14",
          "form": "그게 젤 크지. 네이버 쇼핑은 어때?",
          "original_form": "그게 젤 크지. 네이버 쇼핑은 어때?",
          "speaker_id": "2",
          "time": "20210823 14:28"
        },
        {
          "id": "MDRW2100010100.1.15",
          "form": "네이버 쇼핑은 쿠팡에 없고나 비싼 거 살 때",
          "original_form": "네이버 쇼핑은 쿠팡에 없고나 비싼 거 살 때",
