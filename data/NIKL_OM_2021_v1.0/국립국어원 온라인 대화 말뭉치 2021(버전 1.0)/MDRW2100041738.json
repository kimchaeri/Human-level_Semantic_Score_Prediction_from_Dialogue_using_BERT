        {
          "id": "MDRW2100041738.1.1",
          "form": "처음에는 수술해서 된다고 소리 보내라고 해서",
          "original_form": "처음에는 수술해서 된다고 소리 보내라고 해서",
          "speaker_id": "2",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100041738.1.2",
          "form": "서류 보내라고",
          "original_form": "서류 보내라고",
          "speaker_id": "2",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100041738.1.3",
          "form": "서류도 요즘엔 돈 다 받잖아ㅡㅡ",
          "original_form": "서류도 요즘엔 돈 다 받잖아ㅡㅡ",
          "speaker_id": "2",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100041738.1.4",
          "form": "보냈더니 안 된다고..",
          "original_form": "보냈더니 안 된다고..",
          "speaker_id": "2",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100041738.1.5",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041738.1.6",
          "form": "속상하다 이",
          "original_form": "속상하다 이",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041738.1.7",
          "form": "시술이 아니구 무조건 수술인가보네",
          "original_form": "시술이 아니구 무조건 수술인가보네",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041738.1.8",
          "form": "아이고 ㅠ",
          "original_form": "아이고 ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041738.1.9",
          "form": "그런가봐ㅠ",
          "original_form": "그런가봐ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041738.1.10",
          "form": "아까비 ㅠ",
          "original_form": "아까비 ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041738.1.11",
          "form": "병원에서도",
          "original_form": "병원에서도",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041738.1.12",
          "form": "얘기 안 해줬는데",
          "original_form": "얘기 안 해줬는데",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041738.1.13",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041738.1.14",
          "form": "우리가 어떻게 아노~",
          "original_form": "우리가 어떻게 아노~",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041738.1.15",
          "form": "그러니깐은",
          "original_form": "그러니깐은",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041738.1.16",
          "form": "그래도 혹시나 모르니",
          "original_form": "그래도 혹시나 모르니",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041738.1.17",
          "form": "전화 한번 해봐야겠다",
          "original_form": "전화 한번 해봐야겠다",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041738.1.18",
          "form": "보험고객센터에 한번 더 전화해봐봐",
          "original_form": "보험고객센터에 한번 더 전화해봐봐",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041738.1.19",
          "form": "그래 그 아줌마 혹시 나 몰라서 그럴 수도 있으니ㅋㅋㅋ",
          "original_form": "그래 그 아줌마 혹시 나 몰라서 그럴 수도 있으니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041738.1.20",
          "form": "그렇지",
