      "utterance": [
        {
          "id": "MDRW2100030045.1.1",
          "form": "요즘 날씨 어떤것같아?",
          "original_form": "요즘 날씨 어떤것같아?",
          "speaker_id": "1",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030045.1.2",
          "form": "비가 자주오는데 그만큼 시원해진거같아",
          "original_form": "비가 자주오는데 그만큼 시원해진거같아",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030045.1.3",
          "form": "계속 가을날씨였음 좋겠는데 여름날씨가 돌아올까봐 걱정 돼",
          "original_form": "계속 가을날씨였음 좋겠는데 여름날씨가 돌아올까봐 걱정 돼",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100030045.1.4",
          "form": "이제 9월인데 점점 쌀쌀해지지 않을까?",
          "original_form": "이제 9월인데 점점 쌀쌀해지지 않을까?",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100030045.1.5",
          "form": "그랬으면 좋겠어 가을 옷 꺼내고싶거든",
          "original_form": "그랬으면 좋겠어 가을 옷 꺼내고싶거든",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100030045.1.6",
          "form": "꺼내! 어차피 금방 겨울옷 입어야해서 얼마 못입을걸",
          "original_form": "꺼내! 어차피 금방 겨울옷 입어야해서 얼마 못입을걸",
          "speaker_id": "2",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100030045.1.7",
          "form": "ㅠㅠ 너무 슬픈 일이야 가을 길었으면 좋겠어!\r\n겨울 너무 추워",
          "original_form": "ㅠㅠ 너무 슬픈 일이야 가을 길었으면 좋겠어!\r\n겨울 너무 추워",
          "speaker_id": "1",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100030045.1.8",
          "form": "그치만 난 겨울이 좋아",
          "original_form": "그치만 난 겨울이 좋아",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100030045.1.9",
          "form": "여름은 너무 덥거든..",
          "original_form": "여름은 너무 덥거든..",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100030045.1.10",
          "form": "유독 이번 여름이 더웠어 죽을것같았음ㅜㅠ",
          "original_form": "유독 이번 여름이 더웠어 죽을것같았음ㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100030045.1.11",
          "form": "맞아 그래서 내가 여름 햇빛에 녹아버렸지",
          "original_form": "맞아 그래서 내가 여름 햇빛에 녹아버렸지",
          "speaker_id": "2",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100030045.1.12",
          "form": "녹아버렸지만 다시 언거니?",
          "original_form": "녹아버렸지만 다시 언거니?",
          "speaker_id": "1",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100030045.1.13",
          "form": "아직 녹아있어..",
          "original_form": "아직 녹아있어..",
          "speaker_id": "2",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100030045.1.14",
          "form": "안타깝다 언제 얼어?",
          "original_form": "안타깝다 언제 얼어?",
          "speaker_id": "1",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100030045.1.15",
          "form": "겨울에!",
          "original_form": "겨울에!",
          "speaker_id": "2",
          "time": "20210903 20:10"
        }
      ]
    }
