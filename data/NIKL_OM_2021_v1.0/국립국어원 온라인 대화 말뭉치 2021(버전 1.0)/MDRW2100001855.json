      "utterance": [
        {
          "id": "MDRW2100001855.1.1",
          "form": "책많이 읽나요?",
          "original_form": "책많이 읽나요?",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001855.1.2",
          "form": "책 좋아했죠",
          "original_form": "책 좋아했죠",
          "speaker_id": "1",
          "time": "20210819 21:46"
        },
        {
          "id": "MDRW2100001855.1.3",
          "form": "책종류뭐좋아해요",
          "original_form": "책종류뭐좋아해요",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001855.1.4",
          "form": "지금은 애들 책만 많이 읽어요",
          "original_form": "지금은 애들 책만 많이 읽어요",
          "speaker_id": "1",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001855.1.5",
          "form": "난시집좋아해",
          "original_form": "난시집좋아해",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001855.1.6",
          "form": "메이지원서 샀어요",
          "original_form": "메이지원서 샀어요",
          "speaker_id": "1",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001855.1.7",
          "form": "시를읽으면마음이편해요",
          "original_form": "시를읽으면마음이편해요",
          "speaker_id": "2",
          "time": "20210819 21:49"
        },
        {
          "id": "MDRW2100001855.1.8",
          "form": "시집은 짧아요",
          "original_form": "시집은 짧아요",
          "speaker_id": "1",
          "time": "20210819 21:49"
        },
        {
          "id": "MDRW2100001855.1.9",
          "form": "함축적이예요",
          "original_form": "함축적이예요",
          "speaker_id": "2",
          "time": "20210819 21:50"
        },
        {
          "id": "MDRW2100001855.1.10",
          "form": "에릭칼 그림책 좋아요",
          "original_form": "에릭칼 그림책 좋아요",
          "speaker_id": "1",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001855.1.11",
          "form": "그래요?ㅎ 아이가 좋아해?",
          "original_form": "그래요?ㅎ 아이가 좋아해?",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001855.1.12",
          "form": "베스트셀러 그림책이에요",
          "original_form": "베스트셀러 그림책이에요",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001855.1.13",
          "form": "그림책 좋지요",
          "original_form": "그림책 좋지요",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001855.1.14",
          "form": "루시커지의 메이지도 유명한책입니다",
          "original_form": "루시커지의 메이지도 유명한책입니다",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001855.1.15",
          "form": "아네. 추천감사해요",
          "original_form": "아네. 추천감사해요",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001855.1.16",
          "form": "읽어봅시다",
          "original_form": "읽어봅시다",
          "speaker_id": "1",
          "time": "20210819 21:53"
        }
      ]
