          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006403.1.5",
          "form": "사실 나도",
          "original_form": "사실 나도",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006403.1.6",
          "form": "아 뭐야ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 뭐야ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006403.1.7",
          "form": "아니 티가 읽으라고 해섴ㅋㅋㅋㅋ",
          "original_form": "아니 티가 읽으라고 해섴ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006403.1.8",
          "form": "왠지 안땡겨용",
          "original_form": "왠지 안땡겨용",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006403.1.9",
          "form": "아 맞다 하고 봣더니",
          "original_form": "아 맞다 하고 봣더니",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006403.1.10",
          "form": "문송 3일남앗데",
          "original_form": "문송 3일남앗데",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006403.1.11",
          "form": "아 몰랑",
          "original_form": "아 몰랑",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006403.1.12",
          "form": "이럴수가",
          "original_form": "이럴수가",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100006403.1.13",
          "form": "럴슈럴슈",
          "original_form": "럴슈럴슈",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100006403.1.14",
          "form": "아이돌은 안찾아볼거같구..",
          "original_form": "아이돌은 안찾아볼거같구..",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100006403.1.15",
          "form": "문송은 보다가",
          "original_form": "문송은 보다가",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100006403.1.16",
          "form": "끊길듯 ㅠㅜ",
          "original_form": "끊길듯 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100006403.1.17",
          "form": "3일 안에 50화니깐..",
          "original_form": "3일 안에 50화니깐..",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100006403.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100006403.1.19",
          "form": "노오우ㅜㅜ",
          "original_form": "노오우ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100006403.1.20",
          "form": "지금 보고 잇는거 다 봐가면서 봐야하는데 지금ㅋㅋㅋ",
          "original_form": "지금 보고 잇는거 다 봐가면서 봐야하는데 지금ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100006403.1.21",
          "form": "일단 눈을 버리기로 햇여",
          "original_form": "일단 눈을 버리기로 햇여",
