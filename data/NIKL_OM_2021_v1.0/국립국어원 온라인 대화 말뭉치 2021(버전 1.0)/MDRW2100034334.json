      "utterance": [
        {
          "id": "MDRW2100034334.1.1",
          "form": "일단 기본이지만 급여랑 거리?ㅋㅋ",
          "original_form": "일단 기본이지만 급여랑 거리?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100034334.1.2",
          "form": "공감해요 ㅋㅋㅋ",
          "original_form": "공감해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100034334.1.3",
          "form": "일단 돈벌러가는데니까 젤 먼저 보이긴해요 중요하구",
          "original_form": "일단 돈벌러가는데니까 젤 먼저 보이긴해요 중요하구",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100034334.1.4",
          "form": "마자요ㅋㅋㅋ\r\n아무리 잘해줘도  월급적으면 힘들죠ㅡ",
          "original_form": "마자요ㅋㅋㅋ\r\n아무리 잘해줘도  월급적으면 힘들죠ㅡ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100034334.1.5",
          "form": "그리고 아무래도 분위기나 사람도 보게되죠",
          "original_form": "그리고 아무래도 분위기나 사람도 보게되죠",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100034334.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100034334.1.7",
          "form": "면접볼때 회사분위기도 잘봐야하는거같아요",
          "original_form": "면접볼때 회사분위기도 잘봐야하는거같아요",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100034334.1.8",
          "form": "그니까요 눈치가 좋아야 그런것도 잘  캐치 할 텐데",
          "original_form": "그니까요 눈치가 좋아야 그런것도 잘  캐치 할 텐데",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100034334.1.9",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100034334.1.10",
          "form": "근데 첫인상이랑 다른회사도 많더라구요ㅋㅋ",
          "original_form": "근데 첫인상이랑 다른회사도 많더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100034334.1.11",
          "form": "아그래요??",
          "original_form": "아그래요??",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100034334.1.12",
          "form": "회사 잘 알아보는거도 일이에요",
          "original_form": "회사 잘 알아보는거도 일이에요",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100034334.1.13",
          "form": "네 겉으로는 좋아보이고 복지도 좋을거같았는데",
          "original_form": "네 겉으로는 좋아보이고 복지도 좋을거같았는데",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100034334.1.14",
          "form": "더 작은회사보다 못한곳도 있더라구요",
          "original_form": "더 작은회사보다 못한곳도 있더라구요",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100034334.1.15",
          "form": "아이고 ㅜㅜ 속이 부실하네요",
          "original_form": "아이고 ㅜㅜ 속이 부실하네요",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100034334.1.16",
          "form": "그리고 약간  사장님 마인드도  중요한거같아요",
          "original_form": "그리고 약간  사장님 마인드도  중요한거같아요",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100034334.1.17",
          "form": "헛 맞아요",
          "original_form": "헛 맞아요",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100034334.1.18",
          "form": "짜잘한거에 돈 안쓰고",
          "original_form": "짜잘한거에 돈 안쓰고",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100034334.1.19",
          "form": "경영자 마인드가 중요하죠",
          "original_form": "경영자 마인드가 중요하죠",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100034334.1.20",
          "form": "그러면 직원들한테 돈쓰는것도 아까워하더라구요",
          "original_form": "그러면 직원들한테 돈쓰는것도 아까워하더라구요",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100034334.1.21",
          "form": "하 그러면 좀 그렇겠어요",
          "original_form": "하 그러면 좀 그렇겠어요",
