      "utterance": [
        {
          "id": "MDRW2100026683.1.1",
          "form": "아 아까 말 못했는데 저도 세례를 세 군데에서 받았습니다",
          "original_form": "아 아까 말 못했는데 저도 세례를 세 군데에서 받았습니다",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026683.1.2",
          "form": "군대서요?ㅎㅎ",
          "original_form": "군대서요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026683.1.3",
          "form": "전주 교 기독교 그리고 절",
          "original_form": "전주 교 기독교 그리고 절",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026683.1.4",
          "form": "내 햄버거를 준다고 하더군요",
          "original_form": "내 햄버거를 준다고 하더군요",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026683.1.5",
          "form": "양심을 다 팔았습니다",
          "original_form": "양심을 다 팔았습니다",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026683.1.6",
          "form": "오오 햄버거는 못참지ㅎ",
          "original_form": "오오 햄버거는 못참지ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026683.1.7",
          "form": "기독교가 특히 잘준다던데 맞나요",
          "original_form": "기독교가 특히 잘준다던데 맞나요",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026683.1.8",
          "form": "기독교는  불고기 버거를 줍니다",
          "original_form": "기독교는  불고기 버거를 줍니다",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026683.1.9",
          "form": "아 그 당시에 롯데리아였습니다",
          "original_form": "아 그 당시에 롯데리아였습니다",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026683.1.10",
          "form": "감자튀김도 줬어요",
          "original_form": "감자튀김도 줬어요",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026683.1.11",
          "form": "콜라도요?",
          "original_form": "콜라도요?",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026683.1.12",
          "form": "콜라는 캔 콜라",
          "original_form": "콜라는 캔 콜라",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026683.1.13",
          "form": "시원한 캠 콜라 너무 맛있었습니다",
          "original_form": "시원한 캠 콜라 너무 맛있었습니다",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026683.1.14",
          "form": "아직도 생각이 나네요 그 신성한 장면이..",
          "original_form": "아직도 생각이 나네요 그 신성한 장면이..",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026683.1.15",
          "form": "어디 햄버거를 좋아하시나요",
          "original_form": "어디 햄버거를 좋아하시나요",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026683.1.16",
          "form": "자애로운 종교군요",
          "original_form": "자애로운 종교군요",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026683.1.17",
          "form": "투비컨티뉴?",
          "original_form": "투비컨티뉴?",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026683.1.18",
          "form": "ㅎㅎ 아직이용ㅎ",
          "original_form": "ㅎㅎ 아직이용ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026683.1.19",
          "form": "버거킹 좋아합니다",
          "original_form": "버거킹 좋아합니다",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026683.1.20",
          "form": "콰치즈 사랑해요",
          "original_form": "콰치즈 사랑해요",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026683.1.21",
          "form": "님은 어디거가 취향인가요",
          "original_form": "님은 어디거가 취향인가요",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026683.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026683.1.23",
          "form": "저는 맘스터치 맞나",
          "original_form": "저는 맘스터치 맞나",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026683.1.24",
          "form": "사실은 맥도날드입니다",
          "original_form": "사실은 맥도날드입니다",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026683.1.25",
          "form": "맥도날드 무슨 버거가 맛있나요!",
          "original_form": "맥도날드 무슨 버거가 맛있나요!",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026683.1.26",
          "form": "1955 버거가 맛있습니다",
          "original_form": "1955 버거가 맛있습니다",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026683.1.27",
          "form": "버거킹 감자는 감자는 조금 두껍더라구요",
          "original_form": "버거킹 감자는 감자는 조금 두껍더라구요",
          "speaker_id": "2",
          "time": "20210901 23:40"
        }
      ]
    }
  ]
