      "utterance": [
        {
          "id": "MDRW2100020570.1.1",
          "form": "안좋아하시는구나",
          "original_form": "안좋아하시는구나",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.2",
          "form": "오케이",
          "original_form": "오케이",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.3",
          "form": "후후후 전 예측할 수 없는 사람이죠",
          "original_form": "후후후 전 예측할 수 없는 사람이죠",
          "speaker_id": "2",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.4",
          "form": "정말 그러네요",
          "original_form": "정말 그러네요",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.5",
          "form": "저의 자만이 불러온",
          "original_form": "저의 자만이 불러온",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.6",
          "form": "name1님은 좋아하시나요!",
          "original_form": "&name1&님은 좋아하시나요!",
          "speaker_id": "2",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.7",
          "form": "미스테이크..",
          "original_form": "미스테이크..",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.8",
          "form": "네 전 좋아해요",
          "original_form": "네 전 좋아해요",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.9",
          "form": "뀐아망 이름을 아시는걸 보니",
          "original_form": "뀐아망 이름을 아시는걸 보니",
          "speaker_id": "2",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.10",
          "form": "평소에 관심이 있으셨군요",
          "original_form": "평소에 관심이 있으셨군요",
          "speaker_id": "2",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.11",
          "form": "버터맛을 좋아해서",
          "original_form": "버터맛을 좋아해서",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.12",
          "form": "아 그쵸 페스츄리는 버터양으로 결정돼죠",
          "original_form": "아 그쵸 페스츄리는 버터양으로 결정돼죠",
          "speaker_id": "2",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020570.1.13",
          "form": "그 목막히는 맛 좋아하신다 그러셔서 빵중에 상미종 식빵 좋아하실거같아요",
          "original_form": "그 목막히는 맛 좋아하신다 그러셔서 빵중에 상미종 식빵 좋아하실거같아요",
          "speaker_id": "1",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020570.1.14",
          "form": "저도 그... 크로플은 좋아해요",
          "original_form": "저도 그... 크로플은 좋아해요",
          "speaker_id": "2",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020570.1.15",
          "form": "크로플 맛있어",
          "original_form": "크로플 맛있어",
          "speaker_id": "1",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020570.1.16",
          "form": "위에 아이스크림 올려드시나요",
          "original_form": "위에 아이스크림 올려드시나요",
          "speaker_id": "1",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020570.1.17",
          "form": "오오 맞아요",
          "original_form": "오오 맞아요",
          "speaker_id": "2",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020570.1.18",
          "form": "아이스크림도 좋은데 전 쌩도 좋아요",
          "original_form": "아이스크림도 좋은데 전 쌩도 좋아요",
          "speaker_id": "2",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020570.1.19",
          "form": "시럽뿌린걸 더 선호합니다",
          "original_form": "시럽뿌린걸 더 선호합니다",
          "speaker_id": "2",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020570.1.20",
          "form": "크로플 is 뭔들..",
          "original_form": "크로플 is 뭔들..",
