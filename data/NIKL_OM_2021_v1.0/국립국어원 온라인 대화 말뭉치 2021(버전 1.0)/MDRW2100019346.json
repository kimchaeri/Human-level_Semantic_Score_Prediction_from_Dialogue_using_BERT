        {
          "id": "MDRW2100019346.1.1",
          "form": "전시회 가고 싶은데",
          "original_form": "전시회 가고 싶은데",
          "speaker_id": "2",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019346.1.2",
          "form": "아 주재 계속항걸그랫나ㅋㅋㅋ쏘리",
          "original_form": "아 주재 계속항걸그랫나ㅋㅋㅋ쏘리",
          "speaker_id": "1",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019346.1.3",
          "form": "부산에는 전시회를 많이 안해서",
          "original_form": "부산에는 전시회를 많이 안해서",
          "speaker_id": "2",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019346.1.4",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019346.1.5",
          "form": "아 ㄱㅊㄱㅊ",
          "original_form": "아 ㄱㅊㄱㅊ",
          "speaker_id": "2",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019346.1.6",
          "form": "서울가야 많이 하고",
          "original_form": "서울가야 많이 하고",
          "speaker_id": "2",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019346.1.7",
          "form": "콘서트도 가보고싶은데",
          "original_form": "콘서트도 가보고싶은데",
          "speaker_id": "1",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019346.1.8",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019346.1.9",
          "form": "이제 콘서트는.....큼",
          "original_form": "이제 콘서트는.....큼",
          "speaker_id": "1",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019346.1.10",
          "form": "나 청하 콘서트 가보고 싶다",
          "original_form": "나 청하 콘서트 가보고 싶다",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019346.1.11",
          "form": "나도나도",
          "original_form": "나도나도",
          "speaker_id": "1",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019346.1.12",
          "form": "난 태연 아이유",
