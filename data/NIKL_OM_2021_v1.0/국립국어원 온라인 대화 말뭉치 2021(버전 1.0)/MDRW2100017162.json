      "utterance": [
        {
          "id": "MDRW2100017162.1.1",
          "form": "안녕하세요! 수강신청 하셨나요!?",
          "original_form": "안녕하세요! 수강신청 하셨나요!?",
          "speaker_id": "1",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017162.1.2",
          "form": "안녕하세요~ 네 했어요",
          "original_form": "안녕하세요~ 네 했어요",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017162.1.3",
          "form": "개강이 코앞이라 너무 싫네요..",
          "original_form": "개강이 코앞이라 너무 싫네요..",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017162.1.4",
          "form": "맞아요... 휴학하고 싶어요...ㅎㅎ",
          "original_form": "맞아요... 휴학하고 싶어요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017162.1.5",
          "form": "수강신청 올클 하셨나요?",
          "original_form": "수강신청 올클 하셨나요?",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017162.1.6",
          "form": "하나 못잡은게 있었는데ㅠㅠ",
          "original_form": "하나 못잡은게 있었는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017162.1.7",
          "form": "그냥.. 담에 들으려구요ㅎㅎ",
          "original_form": "그냥.. 담에 들으려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017162.1.8",
          "form": "아쉽네요ㅠㅠ 이전에 전공 과목 몇 개 들으세요?",
          "original_form": "아쉽네요ㅠㅠ 이전에 전공 과목 몇 개 들으세요?",
          "speaker_id": "1",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017162.1.9",
          "form": "3개요, 원래 교양을 더 들으려고 했는데..",
          "original_form": "3개요, 원래 교양을 더 들으려고 했는데..",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017162.1.10",
          "form": "몇개 들으시나요?",
          "original_form": "몇개 들으시나요?",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017162.1.11",
          "form": "저도 전공은 3개요! 근데 2개가 기초라 교양은 1과목...",
          "original_form": "저도 전공은 3개요! 근데 2개가 기초라 교양은 1과목...",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017162.1.12",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017162.1.13",
          "form": "수강신청 너무 어려운것 같아요",
          "original_form": "수강신청 너무 어려운것 같아요",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017162.1.14",
          "form": "맞아요ㅠ 같은 돈 내고 듣는 건데 원하는 거 듣고 싶어요ㅠㅠ",
          "original_form": "맞아요ㅠ 같은 돈 내고 듣는 건데 원하는 거 듣고 싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017162.1.15",
          "form": "몇백만원짜리 티켓팅ㅠㅠㅠ",
          "original_form": "몇백만원짜리 티켓팅ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017162.1.16",
          "form": "꿀교양은 정말 1초도 안 돼서 마감되는 거 같아요...",
          "original_form": "꿀교양은 정말 1초도 안 돼서 마감되는 거 같아요...",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017162.1.17",
          "form": "무서워서 아직 도전도 못 해봤어요",
          "original_form": "무서워서 아직 도전도 못 해봤어요",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017162.1.18",
          "form": "저 이번에 꿀교양 도전했다가 장렬히 전사했어요ㅋㅋㅋㅋ..",
          "original_form": "저 이번에 꿀교양 도전했다가 장렬히 전사했어요ㅋㅋㅋㅋ..",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017162.1.19",
          "form": "화이팅합시다..",
          "original_form": "화이팅합시다..",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
