      "utterance": [
        {
          "id": "MDRW2100008191.1.1",
          "form": "태풍온대",
          "original_form": "태풍온대",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100008191.1.2",
          "form": "지금 여기는 비옴",
          "original_form": "지금 여기는 비옴",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100008191.1.3",
          "form": "여기도 근데 별일 없이 지나갈듯",
          "original_form": "여기도 근데 별일 없이 지나갈듯",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100008191.1.4",
          "form": "난 좀 더 바람 많이 불면 좋겠다",
          "original_form": "난 좀 더 바람 많이 불면 좋겠다",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100008191.1.5",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100008191.1.6",
          "form": "그냥... 그냥",
          "original_form": "그냥... 그냥",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100008191.1.7",
          "form": "낼은 오늘보다 덥대",
          "original_form": "낼은 오늘보다 덥대",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100008191.1.8",
          "form": "지금 이 온도가 딱인데",
          "original_form": "지금 이 온도가 딱인데",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100008191.1.9",
          "form": "아쉽",
          "original_form": "아쉽",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100008191.1.10",
          "form": "25도 밑으로 내려가면 좋겠다",
          "original_form": "25도 밑으로 내려가면 좋겠다",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100008191.1.11",
          "form": "그러게 그럼 딱 가을날씬데",
          "original_form": "그러게 그럼 딱 가을날씬데",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100008191.1.12",
          "form": "오늘 낮에도 학원에서 에어컨 틀었다구",
          "original_form": "오늘 낮에도 학원에서 에어컨 틀었다구",
          "speaker_id": "1",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100008191.1.13",
          "form": "빨리 에어컨 안트는 날씨오면 조케땅",
          "original_form": "빨리 에어컨 안트는 날씨오면 조케땅",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100008191.1.14",
          "form": "마자 그렇다고 겨울이 좋은건 아니고",
          "original_form": "마자 그렇다고 겨울이 좋은건 아니고",
          "speaker_id": "1",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100008191.1.15",
          "form": "그치 그건..",
          "original_form": "그치 그건..",
          "speaker_id": "2",
