      "utterance": [
        {
          "id": "MDRW2100024224.1.1",
          "form": "소주 2병이면 주량 쌔시네요",
          "original_form": "소주 2병이면 주량 쌔시네요",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024224.1.2",
          "form": "근데 술맛은 잘 몰라요ㅎㅇ",
          "original_form": "근데 술맛은 잘 몰라요ㅎㅇ",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024224.1.3",
          "form": "오직 안주발",
          "original_form": "오직 안주발",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024224.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024224.1.5",
          "form": "저는 1병반?",
          "original_form": "저는 1병반?",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024224.1.6",
          "form": "예쁜사람 있거나 기분좋으면 3병?",
          "original_form": "예쁜사람 있거나 기분좋으면 3병?",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024224.1.7",
          "form": "님도 세시네요!",
          "original_form": "님도 세시네요!",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024224.1.8",
          "form": "아니ㅋㅋ 주량이 아니잖아욧!",
          "original_form": "아니ㅋㅋ 주량이 아니잖아욧!",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024224.1.9",
          "form": "마자욧",
          "original_form": "마자욧",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024224.1.10",
          "form": "소고기 좋아요 돼지고기 좋아요?",
          "original_form": "소고기 좋아요 돼지고기 좋아요?",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024224.1.11",
          "form": "돼지요!! 소고기는 가끔이면 족해요ㅎ",
          "original_form": "돼지요!! 소고기는 가끔이면 족해요ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024224.1.12",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024224.1.13",
          "form": "먹깨비 맞네요",
          "original_form": "먹깨비 맞네요",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024224.1.14",
          "form": "소고기먹으면 돼지됩니다",
          "original_form": "소고기먹으면 돼지됩니다",
          "speaker_id": "2",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024224.1.15",
          "form": "ㅎㅎ 띵언이네요ㅎㅎ",
          "original_form": "ㅎㅎ 띵언이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024224.1.16",
          "form": "혹시  혹시 돼지 등갈비 드셔보셨나요",
          "original_form": "혹시  혹시 돼지 등갈비 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024224.1.17",
          "form": "그럼요!",
          "original_form": "그럼요!",
          "speaker_id": "1",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100024224.1.18",
          "form": "맛집 아시나요?",
          "original_form": "맛집 아시나요?",
          "speaker_id": "1",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100024224.1.19",
          "form": "제가 불피워서 굽습니다",
          "original_form": "제가 불피워서 굽습니다",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024224.1.20",
          "form": "장갑 두장 끼고 등갈비를 일위 등갈비를 이리저리 뒤집어가며 굽습니다",
          "original_form": "장갑 두장 끼고 등갈비를 일위 등갈비를 이리저리 뒤집어가며 굽습니다",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024224.1.21",
          "form": "헉;; 정말 애완동물  한마리만 키우시죠ㅜ",
          "original_form": "헉;; 정말 애완동물  한마리만 키우시죠ㅜ",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100024224.1.22",
          "form": "재롱도 잘 피우는데",
          "original_form": "재롱도 잘 피우는데",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100024224.1.23",
          "form": "어떤 재롱인가요?",
          "original_form": "어떤 재롱인가요?",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100024224.1.24",
          "form": "일단 등갈비를 주고 말하시죵!!!",
          "original_form": "일단 등갈비를 주고 말하시죵!!!",
          "speaker_id": "1",
          "time": "20210830 23:48"
        }
      ]
