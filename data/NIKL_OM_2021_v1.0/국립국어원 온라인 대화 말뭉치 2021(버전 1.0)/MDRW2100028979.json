      "utterance": [
        {
          "id": "MDRW2100028979.1.1",
          "form": "제생각 어때요",
          "original_form": "제생각 어때요",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028979.1.2",
          "form": "너무 나이뚜 한데요...",
          "original_form": "너무 나이뚜 한데요...",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028979.1.3",
          "form": "제가 드림카라고 차를 샀는데",
          "original_form": "제가 드림카라고 차를 샀는데",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028979.1.4",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028979.1.5",
          "form": "엄마가 집에 차가\r\n두대가 뭐가 필요하냐며 ㅠㅠ",
          "original_form": "엄마가 집에 차가\r\n두대가 뭐가 필요하냐며 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028979.1.6",
          "form": "애둘 낳고도 혼났어여 엄마한테ㅋㅋㅋ",
          "original_form": "애둘 낳고도 혼났어여 엄마한테ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028979.1.7",
          "form": "휠사진 해놨다간\r\n등짝이 남아나질\r\n않을거 같은데염ㅋㅋㅋ",
          "original_form": "휠사진 해놨다간\r\n등짝이 남아나질\r\n않을거 같은데염ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028979.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028979.1.9",
          "form": "아직도 모르세요?",
          "original_form": "아직도 모르세요?",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028979.1.10",
          "form": "멀티프로필기능?",
          "original_form": "멀티프로필기능?",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028979.1.11",
          "form": "역시 나이가 먹어도\r\n미혼들이 젊게 사는듯ㅋㅋㅋ",
          "original_form": "역시 나이가 먹어도\r\n미혼들이 젊게 사는듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028979.1.12",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028979.1.13",
          "form": "name2 젊은이시네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "&name2& 젊은이시네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028979.1.14",
          "form": "이 좋은기능을",
          "original_form": "이 좋은기능을",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028979.1.15",
          "form": "쓰셔야죠",
          "original_form": "쓰셔야죠",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028979.1.16",
          "form": "이미 차는 샀고",
          "original_form": "이미 차는 샀고",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028979.1.17",
          "form": "히힣ㅋㅋ\r\n근데... 곧 남편 생일입니다",
          "original_form": "히힣ㅋㅋ\r\n근데... 곧 남편 생일입니다",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028979.1.18",
          "form": "혼나면 어쩔수없죠 ㅋㅋㅋㅋ",
          "original_form": "혼나면 어쩔수없죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028979.1.19",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028979.1.20",
          "form": "그럼 생일을 잘 챙겨주고",
          "original_form": "그럼 생일을 잘 챙겨주고",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028979.1.21",
          "form": "딜을 하시죠",
          "original_form": "딜을 하시죠",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028979.1.22",
          "form": "그리고 남편이 휠 바꾸고 싶다한거\r\n제가 놉!했었어서 제 차도\r\n안해줄거 같아여 ㅋㅋㅋ",
          "original_form": "그리고 남편이 휠 바꾸고 싶다한거\r\n제가 놉!했었어서 제 차도\r\n안해줄거 같아여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028979.1.23",
          "form": "복수는 또다른복수를 낳는다",
          "original_form": "복수는 또다른복수를 낳는다",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028979.1.24",
          "form": "그러지 말라고 하세요 ㅋㅋ",
          "original_form": "그러지 말라고 하세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028979.1.25",
          "form": "ㅠ.ㅠ ㅋㅋ",
          "original_form": "ㅠ.ㅠ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028979.1.26",
          "form": "근데 휠은 바꿔 뭐하나요",
          "original_form": "근데 휠은 바꿔 뭐하나요",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028979.1.27",
          "form": "다 부질없어요",
          "original_form": "다 부질없어요",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028979.1.28",
          "form": "갑자기 왜그러세요",
          "original_form": "갑자기 왜그러세요",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028979.1.29",
          "form": "아까 폰도 그러시더니",
          "original_form": "아까 폰도 그러시더니",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028979.1.30",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028979.1.31",
          "form": "갑자기 바꿔서 모하냐 분위기",
          "original_form": "갑자기 바꿔서 모하냐 분위기",
          "speaker_id": "2",
          "time": "20210902 23:25"
        }
      ]
