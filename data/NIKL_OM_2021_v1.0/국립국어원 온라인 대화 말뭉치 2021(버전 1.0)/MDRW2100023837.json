      "utterance": [
        {
          "id": "MDRW2100023837.1.1",
          "form": "피부관리 받아보셨나요",
          "original_form": "피부관리 받아보셨나요",
          "speaker_id": "1",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100023837.1.2",
          "form": "어렸을때? 관리받다가 지금은 안받고있습니다",
          "original_form": "어렸을때? 관리받다가 지금은 안받고있습니다",
          "speaker_id": "2",
          "time": "20210830 18:51"
        },
        {
          "id": "MDRW2100023837.1.3",
          "form": "저두요 최근엔 경험이 없어요",
          "original_form": "저두요 최근엔 경험이 없어요",
          "speaker_id": "1",
          "time": "20210830 19:07"
        },
        {
          "id": "MDRW2100023837.1.4",
          "form": "요즘 문화가잘되있어서 싸게받을수있다하더라구요",
          "original_form": "요즘 문화가잘되있어서 싸게받을수있다하더라구요",
          "speaker_id": "2",
          "time": "20210830 19:11"
        },
        {
          "id": "MDRW2100023837.1.5",
          "form": "아 그래요? 저는 기미가 심해져서 고민이예요",
          "original_form": "아 그래요? 저는 기미가 심해져서 고민이예요",
          "speaker_id": "1",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100023837.1.6",
          "form": "저두인데...푸석해지는 느낌이있어서요ㅠ",
          "original_form": "저두인데...푸석해지는 느낌이있어서요ㅠ",
          "speaker_id": "2",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100023837.1.7",
          "form": "피부과가보고싶은데 비용이나 효과가 고민이네요",
          "original_form": "피부과가보고싶은데 비용이나 효과가 고민이네요",
          "speaker_id": "1",
          "time": "20210830 19:27"
        },
        {
          "id": "MDRW2100023837.1.8",
          "form": "그게 제일 힘들긴하죠",
          "original_form": "그게 제일 힘들긴하죠",
          "speaker_id": "2",
          "time": "20210830 19:31"
        },
        {
          "id": "MDRW2100023837.1.9",
          "form": "다이어트는 해보셨나요",
          "original_form": "다이어트는 해보셨나요",
          "speaker_id": "1",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023837.1.10",
          "form": "네 해봤죠",
          "original_form": "네 해봤죠",
          "speaker_id": "2",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023837.1.11",
          "form": "지금은 복구됬지만",
          "original_form": "지금은 복구됬지만",
          "speaker_id": "2",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023837.1.12",
          "form": "어떤 방식으로 하셨나요",
          "original_form": "어떤 방식으로 하셨나요",
          "speaker_id": "1",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023837.1.13",
          "form": "식단조절은 잘못하고 그냥 무작정 운동만한거같아요",
          "original_form": "식단조절은 잘못하고 그냥 무작정 운동만한거같아요",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023837.1.14",
          "form": "와 그래도 성공하신거보면 운동량이 많았나봐요",
          "original_form": "와 그래도 성공하신거보면 운동량이 많았나봐요",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023837.1.15",
          "form": "하루 3시간정도했습니다.",
          "original_form": "하루 3시간정도했습니다.",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100023837.1.16",
          "form": "와...어떤 운동이요?",
          "original_form": "와...어떤 운동이요?",
          "speaker_id": "1",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100023837.1.17",
          "form": "뛰기 웟몸 팔굽혀펴기 이렇게 매일했습니다",
          "original_form": "뛰기 웟몸 팔굽혀펴기 이렇게 매일했습니다",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023837.1.18",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "1",
          "time": "20210831 06:07"
        }
      ]
