      "utterance": [
        {
          "id": "MDRW2100003723.1.1",
          "form": "히트 불새 달의 연인 청춘의덫요",
          "original_form": "히트 불새 달의 연인 청춘의덫요",
          "speaker_id": "2",
          "time": "20210821 11:43"
        },
        {
          "id": "MDRW2100003723.1.2",
          "form": "어떤거 생각나세요 ?",
          "original_form": "어떤거 생각나세요 ?",
          "speaker_id": "2",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003723.1.3",
          "form": "달의연인요! 보보경심 올해 다시 봤어요ㅎㅎ\r\n불새는 2021 아침드라마 덕분에 다시 보구요",
          "original_form": "달의연인요! 보보경심 올해 다시 봤어요ㅎㅎ\r\n불새는 2021 아침드라마 덕분에 다시 보구요",
          "speaker_id": "1",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100003723.1.4",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100003723.1.5",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100003723.1.6",
          "form": "똑같아요",
          "original_form": "똑같아요",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100003723.1.7",
          "form": "전 예전 불새요",
          "original_form": "전 예전 불새요",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100003723.1.8",
          "form": "요번 불새는 몰입이 안되죠?",
          "original_form": "요번 불새는 몰입이 안되죠?",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100003723.1.9",
          "form": "보보경심 아 너무 안타까웠어요 여주와 남주의 모습이 ㅡㅡ",
          "original_form": "보보경심 아 너무 안타까웠어요 여주와 남주의 모습이 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100003723.1.10",
          "form": "8왕자와 부인도 너무 안타깝고요",
          "original_form": "8왕자와 부인도 너무 안타깝고요",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100003723.1.11",
          "form": "곁의 소중함을 모르는것 같아요",
          "original_form": "곁의 소중함을 모르는것 같아요",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100003723.1.12",
          "form": "맞아요ㅠㅠ 보보경심 대만판인가?도 있더라구요",
          "original_form": "맞아요ㅠㅠ 보보경심 대만판인가?도 있더라구요",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100003723.1.13",
          "form": "요번 불새는 너무 막장중에 막장ㅋ",
          "original_form": "요번 불새는 너무 막장중에 막장ㅋ",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100003723.1.14",
          "form": "그건 몰입감 떨어져요",
          "original_form": "그건 몰입감 떨어져요",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100003723.1.15",
          "form": "남주들이 아저씨예요 ㅡㅡ",
          "original_form": "남주들이 아저씨예요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100003723.1.16",
          "form": "맞아요 막장",
          "original_form": "맞아요 막장",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100003723.1.17",
          "form": "전 작품이 품격 ㅋㅋ느껴져요",
          "original_form": "전 작품이 품격 ㅋㅋ느껴져요",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100003723.1.18",
          "form": "아ㅋㅋㅋㅋ 그초 말도안되게 해외갔다가 갑자기 사장이라니ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ 그초 말도안되게 해외갔다가 갑자기 사장이라니ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100003723.1.19",
          "form": "그러니까요 서진이 에릭 이은주가 최고고 내용도 좋아요",
          "original_form": "그러니까요 서진이 에릭 이은주가 최고고 내용도 좋아요",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100003723.1.20",
          "form": "그쵸ㅋㅋㅋ ㄱ그리고 그여주가..",
          "original_form": "그쵸ㅋㅋㅋ ㄱ그리고 그여주가..",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100003723.1.21",
          "form": "홍수아란사실에",
          "original_form": "홍수아란사실에",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100003723.1.22",
          "form": "완전 놀랐어요...",
          "original_form": "완전 놀랐어요...",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100003723.1.23",
          "form": "얼굴을 그냥 통채로 갈아 엎은듯...",
          "original_form": "얼굴을 그냥 통채로 갈아 엎은듯...",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100003723.1.24",
          "form": "저도 못알아봤어요",
          "original_form": "저도 못알아봤어요",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100003723.1.25",
          "form": "세상에 너무심해요",
          "original_form": "세상에 너무심해요",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100003723.1.26",
          "form": "이은주는 정말 자연스럽고 분위기가 좋았는데",
          "original_form": "이은주는 정말 자연스럽고 분위기가 좋았는데",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100003723.1.27",
          "form": "항상 안타까워요",
          "original_form": "항상 안타까워요",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100003723.1.28",
          "form": "아 저 청춘의덫도 또봤어요",
          "original_form": "아 저 청춘의덫도 또봤어요",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100003723.1.29",
          "form": "심은하의 모습도 좋고 전광렬은 이제보니 아저씨네요 ㅋㅋ",
          "original_form": "심은하의 모습도 좋고 전광렬은 이제보니 아저씨네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100003723.1.30",
          "form": "이종원 쓰레기였어요 ㅡㅡ",
          "original_form": "이종원 쓰레기였어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100003723.1.31",
          "form": "유호정 폭탄머리 생각나요 갈색 립스틱도",
          "original_form": "유호정 폭탄머리 생각나요 갈색 립스틱도",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100003723.1.32",
          "form": "올드한데 땡기는 재미가 있었어요 ㅎㅎ",
          "original_form": "올드한데 땡기는 재미가 있었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100003723.1.33",
          "form": "넷플릭스 이용하세요?",
          "original_form": "넷플릭스 이용하세요?",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100003723.1.34",
          "form": "전 넷플릭스랑 유튜브 주로봐요",
          "original_form": "전 넷플릭스랑 유튜브 주로봐요",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100003723.1.35",
          "form": "청춘의 덫도 진짜 옛날 드라마네요 ㅎㅎㅎ\r\n오늘은 드라마스페셜 옛날꺼 보고 있는데 사의찬미라는 거 되게 재밌네요^^\r\n저는 웨이브 넷플, 티빙 보고 있어요 ^^",
          "original_form": "청춘의 덫도 진짜 옛날 드라마네요 ㅎㅎㅎ\r\n오늘은 드라마스페셜 옛날꺼 보고 있는데 사의찬미라는 거 되게 재밌네요^^\r\n저는 웨이브 넷플, 티빙 보고 있어요 ^^",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100003723.1.36",
          "form": "오 저 아주옛날에 봤어요",
          "original_form": "오 저 아주옛날에 봤어요",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100003723.1.37",
          "form": "마지막 장면 생각나요",
          "original_form": "마지막 장면 생각나요",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100003723.1.38",
          "form": "배에서 뛰어내리는 ㅜㅜ",
          "original_form": "배에서 뛰어내리는 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100003723.1.39",
          "form": "앗 그래요?? 저는 이번에 처음 봤는데... 보고면서 울었네요",
          "original_form": "앗 그래요?? 저는 이번에 처음 봤는데... 보고면서 울었네요",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100003723.1.40",
          "form": "진짜 코드 비슷한거 같애요 !!!",
          "original_form": "진짜 코드 비슷한거 같애요 !!!",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100003723.1.41",
          "form": "네 아 노래 부르는장면도 생각나요",
          "original_form": "네 아 노래 부르는장면도 생각나요",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100003723.1.42",
          "form": "전 진짜 그 개봉할때 봤는데도 기억나요",
          "original_form": "전 진짜 그 개봉할때 봤는데도 기억나요",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100003723.1.43",
          "form": "맞아요.. 남주가 이종석인가?? 보는데 급 두근두근",
          "original_form": "맞아요.. 남주가 이종석인가?? 보는데 급 두근두근",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100003723.1.44",
          "form": "연애하는 기분이랄까 ㅏㅋㅋㅋㅋ",
          "original_form": "연애하는 기분이랄까 ㅏㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100003723.1.45",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100003723.1.46",
          "form": "전 그전 버전이요",
          "original_form": "전 그전 버전이요",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100003723.1.47",
