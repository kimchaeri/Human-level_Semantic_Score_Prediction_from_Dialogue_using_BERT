      "utterance": [
        {
          "id": "MDRW2100036458.1.1",
          "form": "그렇지 면요리가 간단",
          "original_form": "그렇지 면요리가 간단",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036458.1.2",
          "form": "응 올리브오일만 있어도 뚝딱",
          "original_form": "응 올리브오일만 있어도 뚝딱",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036458.1.3",
          "form": "거기다 마늘 있으면 더 좋고",
          "original_form": "거기다 마늘 있으면 더 좋고",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036458.1.4",
          "form": "응 맞아 기본 파스타면에다가 재료가 틀리면 다른 요리가 나오니",
          "original_form": "응 맞아 기본 파스타면에다가 재료가 틀리면 다른 요리가 나오니",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036458.1.5",
          "form": "그래 애들도 잘 먹고",
          "original_form": "그래 애들도 잘 먹고",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036458.1.6",
          "form": "맞아 난 해물파스타가 좋더라",
          "original_form": "맞아 난 해물파스타가 좋더라",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036458.1.7",
          "form": "잘못하면 비리다",
          "original_form": "잘못하면 비리다",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036458.1.8",
          "form": "해물이나 생선요리 잘못하면 비리지",
          "original_form": "해물이나 생선요리 잘못하면 비리지",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036458.1.9",
          "form": "엉 그래도 쉽긴하지",
          "original_form": "엉 그래도 쉽긴하지",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036458.1.10",
          "form": "호불호도 잘 없고",
          "original_form": "호불호도 잘 없고",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036458.1.11",
          "form": "너희 형부도  좋아한다",
          "original_form": "너희 형부도  좋아한다",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036458.1.12",
          "form": "엉 내가 좋아하는 조개 파스타 최고",
          "original_form": "엉 내가 좋아하는 조개 파스타 최고",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036458.1.13",
          "form": "조개 아니라도 명란젖도 괜찮다",
          "original_form": "조개 아니라도 명란젖도 괜찮다",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036458.1.14",
          "form": "조개 파스타 만드는데는 30분도 안걸림",
          "original_form": "조개 파스타 만드는데는 30분도 안걸림",
