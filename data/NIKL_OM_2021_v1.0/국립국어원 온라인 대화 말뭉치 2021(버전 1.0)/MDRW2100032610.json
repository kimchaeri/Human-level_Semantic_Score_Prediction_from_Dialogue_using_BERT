      "utterance": [
        {
          "id": "MDRW2100032610.1.1",
          "form": "반딧불이 넘 예쁘겠어요",
          "original_form": "반딧불이 넘 예쁘겠어요",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032610.1.2",
          "form": "자연의아름다움",
          "original_form": "자연의아름다움",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032610.1.3",
          "form": "언제까지 볼수있을지 모르니까요",
          "original_form": "언제까지 볼수있을지 모르니까요",
          "speaker_id": "2",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032610.1.4",
          "form": "바다 좋아하세요?",
          "original_form": "바다 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032610.1.5",
          "form": "네 완전요!",
          "original_form": "네 완전요!",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100032610.1.6",
          "form": "환경오염정말 ㅠ",
          "original_form": "환경오염정말 ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100032610.1.7",
          "form": "맞다 세부 가고싶다 하셨죠",
          "original_form": "맞다 세부 가고싶다 하셨죠",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100032610.1.8",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100032610.1.9",
          "form": "바다 원없이 보고싶어요",
          "original_form": "바다 원없이 보고싶어요",
          "speaker_id": "1",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032610.1.10",
          "form": "초록바다",
          "original_form": "초록바다",
          "speaker_id": "1",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032610.1.11",
          "form": "바다 좋쵸",
          "original_form": "바다 좋쵸",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032610.1.12",
          "form": "마음에 탁 트여서",
          "original_form": "마음에 탁 트여서",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032610.1.13",
          "form": "ㅎㅎ맞아요 몰디브도 가구싶고",
          "original_form": "ㅎㅎ맞아요 몰디브도 가구싶고",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032610.1.14",
          "form": "몰디브 오오",
          "original_form": "몰디브 오오",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032610.1.15",
          "form": "신행으로 유명한 곳이죠",
          "original_form": "신행으로 유명한 곳이죠",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032610.1.16",
          "form": "전 결혼생각이 없어서",
          "original_form": "전 결혼생각이 없어서",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032610.1.17",
          "form": "저도 결혼생각없어서 맘맞는 친구랑 가려구요",
          "original_form": "저도 결혼생각없어서 맘맞는 친구랑 가려구요",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032610.1.18",
          "form": "ㅋㅋㅋ부부만 가라는법이 있나요~",
          "original_form": "ㅋㅋㅋ부부만 가라는법이 있나요~",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032610.1.19",
          "form": "근데 신행으로 유명한 곳들이 주로",
          "original_form": "근데 신행으로 유명한 곳들이 주로",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032610.1.20",
          "form": "싱글여성 만세 ㅎㅎ",
          "original_form": "싱글여성 만세 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032610.1.21",
          "form": "비싼 리조트에 게하가 잘 없구",
          "original_form": "비싼 리조트에 게하가 잘 없구",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032610.1.22",
          "form": "주로 휴양이어서요",
          "original_form": "주로 휴양이어서요",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032610.1.23",
          "form": "그건그래요..",
          "original_form": "그건그래요..",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032610.1.24",
          "form": "레알 돈쓰며 쉬러가는곳...ㅎㅎ",
          "original_form": "레알 돈쓰며 쉬러가는곳...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032610.1.25",
          "form": "협재해수욕장이 약간 몰디브너낌이던데",
          "original_form": "협재해수욕장이 약간 몰디브너낌이던데",
          "speaker_id": "2",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032610.1.26",
          "form": "담에 꼭 가보셔요",
          "original_form": "담에 꼭 가보셔요",
          "speaker_id": "2",
