      "utterance": [
        {
          "id": "MDRW2100022908.1.1",
          "form": "오 어떤 느낌의 쇼파?",
          "original_form": "오 어떤 느낌의 쇼파?",
          "speaker_id": "2",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022908.1.2",
          "form": "음 너무 작은거 말고 최소 3인용? 카우치도 있는거",
          "original_form": "음 너무 작은거 말고 최소 3인용? 카우치도 있는거",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022908.1.3",
          "form": "아 좋지좋지.. 재질은??",
          "original_form": "아 좋지좋지.. 재질은??",
          "speaker_id": "2",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022908.1.4",
          "form": "요즘 아쿠아패브릭이라고 유행하더라",
          "original_form": "요즘 아쿠아패브릭이라고 유행하더라",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022908.1.5",
          "form": "오 되게 보들보들해보인다",
          "original_form": "오 되게 보들보들해보인다",
          "speaker_id": "2",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022908.1.6",
          "form": "응 근데 가죽이랑 비슷한가봐 오염 잘 안되고",
          "original_form": "응 근데 가죽이랑 비슷한가봐 오염 잘 안되고",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022908.1.7",
          "form": "나는 고양이가 있어서 가죽소파는 못쓰거든ㅠㅠ 다 뜯어놔서ㅠㅠ",
          "original_form": "나는 고양이가 있어서 가죽소파는 못쓰거든ㅠㅠ 다 뜯어놔서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022908.1.8",
          "form": "아하ㅠㅠ 그럼 천쇼파?",
          "original_form": "아하ㅠㅠ 그럼 천쇼파?",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022908.1.9",
          "form": "응 패브릭 소파 써야되는데 저거는 좀 괜찮으려나? 나도 거실 있는 집으로 이사가면 쇼파 사려구!",
          "original_form": "응 패브릭 소파 써야되는데 저거는 좀 괜찮으려나? 나도 거실 있는 집으로 이사가면 쇼파 사려구!",
          "speaker_id": "2",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022908.1.10",
          "form": "저거는 오히려 가죽이랑 비슷해서 별로일지두",
          "original_form": "저거는 오히려 가죽이랑 비슷해서 별로일지두",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022908.1.11",
          "form": "오염안되고 그러는거라",
          "original_form": "오염안되고 그러는거라",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022908.1.12",
          "form": "아 그런가.... 고양이가 뜯지만 않으면 참 좋은데...",
          "original_form": "아 그런가.... 고양이가 뜯지만 않으면 참 좋은데...",
          "speaker_id": "2",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022908.1.13",
          "form": "맞아 아니면 쇼파에 커버 씌워서 쓰면 안되나?",
          "original_form": "맞아 아니면 쇼파에 커버 씌워서 쓰면 안되나?",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022908.1.14",
          "form": "그럼 저 재질이 잘 안보이지 않을까?ㅋㅋㅋ",
          "original_form": "그럼 저 재질이 잘 안보이지 않을까?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022908.1.15",
          "form": "그치...ㅋㅋㅋㅋㅋ근데 요즘 커버도 이쁜거 많더라",
          "original_form": "그치...ㅋㅋㅋㅋㅋ근데 요즘 커버도 이쁜거 많더라",
          "speaker_id": "1",
          "time": "20210829 19:05"
        }
      ]
