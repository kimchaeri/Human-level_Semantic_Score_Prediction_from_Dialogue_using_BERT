      "utterance": [
        {
          "id": "MDRW2100004519.1.1",
          "form": "음 형제자매 있으세요?",
          "original_form": "음 형제자매 있으세요?",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004519.1.2",
          "form": "동생 한 명 있어요",
          "original_form": "동생 한 명 있어요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004519.1.3",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004519.1.4",
          "form": "저도 여동생한명이요 ㅎㅎ",
          "original_form": "저도 여동생한명이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004519.1.5",
          "form": "어릴땐 죽어라싸웠는데.. 이젠 진짜 동생이 제 베프예요 ㅋㅋㅋ",
          "original_form": "어릴땐 죽어라싸웠는데.. 이젠 진짜 동생이 제 베프예요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004519.1.6",
          "form": "아 부러워요~",
          "original_form": "아 부러워요~",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004519.1.7",
          "form": "저는 남동생인데",
          "original_form": "저는 남동생인데",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004519.1.8",
          "form": "나이차이가 꽤 있어서 어색해요",
          "original_form": "나이차이가 꽤 있어서 어색해요",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004519.1.9",
          "form": "아하 그래도 막 대들진(?) 않겠어요 ㅋㅋ",
          "original_form": "아하 그래도 막 대들진(?) 않겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004519.1.10",
          "form": "맞아요 제가 어릴 때 좀 눌러놔서ㅋㅋㅋ",
          "original_form": "맞아요 제가 어릴 때 좀 눌러놔서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004519.1.11",
          "form": "부모님이 동생한테 더 유하셔서 저라도 눌러놔야 했어요",
          "original_form": "부모님이 동생한테 더 유하셔서 저라도 눌러놔야 했어요",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004519.1.12",
          "form": "아~ 이런... 잘하셨어요",
          "original_form": "아~ 이런... 잘하셨어요",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004519.1.13",
          "form": "k장녀가 고생이많죠..ㅎ",
          "original_form": "k장녀가 고생이많죠..ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004519.1.14",
          "form": "맞아요~ 저희 정말 고생해요~",
          "original_form": "맞아요~ 저희 정말 고생해요~",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004519.1.15",
          "form": "저는 곧 자취할건데",
          "original_form": "저는 곧 자취할건데",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004519.1.16",
          "form": "가족을 떠나는게 섭섭한것보다 시원함이 크더라고욬ㅋㅋㅋ큐ㅠㅠ",
          "original_form": "가족을 떠나는게 섭섭한것보다 시원함이 크더라고욬ㅋㅋㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004519.1.17",
          "form": "얼른 나가살고싶다..ㅎㅎ",
          "original_form": "얼른 나가살고싶다..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004519.1.18",
          "form": "가족이랑 같이 살면 제약이 많죠ㅜ",
          "original_form": "가족이랑 같이 살면 제약이 많죠ㅜ",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004519.1.19",
          "form": "이해합니다",
          "original_form": "이해합니다",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004519.1.20",
          "form": "저도 얼른 돈 모아서 자취하고 싶어요",
          "original_form": "저도 얼른 돈 모아서 자취하고 싶어요",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004519.1.21",
          "form": "힘내자구요ㅎㅎ",
          "original_form": "힘내자구요ㅎㅎ",
