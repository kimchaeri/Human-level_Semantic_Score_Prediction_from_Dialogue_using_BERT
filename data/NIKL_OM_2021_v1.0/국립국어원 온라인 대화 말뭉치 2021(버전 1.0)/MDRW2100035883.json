      "utterance": [
        {
          "id": "MDRW2100035883.1.1",
          "form": "오늘 날씨는 어떤가요?",
          "original_form": "오늘 날씨는 어떤가요?",
          "speaker_id": "2",
          "time": "20210830 11:28"
        },
        {
          "id": "MDRW2100035883.1.2",
          "form": "약간 흐리꾸리하네요!",
          "original_form": "약간 흐리꾸리하네요!",
          "speaker_id": "1",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100035883.1.3",
          "form": "오! 여기두요!",
          "original_form": "오! 여기두요!",
          "speaker_id": "2",
          "time": "20210830 12:31"
        },
        {
          "id": "MDRW2100035883.1.4",
          "form": "계속 맑더니 비올것만 같아요ㅜ",
          "original_form": "계속 맑더니 비올것만 같아요ㅜ",
          "speaker_id": "2",
          "time": "20210830 12:31"
        },
        {
          "id": "MDRW2100035883.1.5",
          "form": "비가올것같죠ㅠ",
          "original_form": "비가올것같죠ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:31"
        },
        {
          "id": "MDRW2100035883.1.6",
          "form": "여기도 ㅠ",
          "original_form": "여기도 ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:31"
        },
        {
          "id": "MDRW2100035883.1.7",
          "form": "이제 진짜 곧 긴팔입을것같아요",
          "original_form": "이제 진짜 곧 긴팔입을것같아요",
          "speaker_id": "1",
          "time": "20210830 12:31"
        },
        {
          "id": "MDRW2100035883.1.8",
          "form": "ㅠㅠ싫으네요",
          "original_form": "ㅠㅠ싫으네요",
          "speaker_id": "2",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100035883.1.9",
          "form": "맞아요 또 가을 훅 지나갈까 조마조마해요ㅠ",
          "original_form": "맞아요 또 가을 훅 지나갈까 조마조마해요ㅠ",
          "speaker_id": "2",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100035883.1.10",
          "form": "ㅋㅋㄱㅋ이번 가을은 이주는 될까요?ㅠ",
          "original_form": "ㅋㅋㄱㅋ이번 가을은 이주는 될까요?ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:46"
        },
        {
          "id": "MDRW2100035883.1.11",
          "form": "흡ㅠ 그정도만 되도 좋겠어요ㅋㅋ",
          "original_form": "흡ㅠ 그정도만 되도 좋겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100035883.1.12",
          "form": "신상 한 번 입고라도 갔음 좋겠네오",
          "original_form": "신상 한 번 입고라도 갔음 좋겠네오",
          "speaker_id": "2",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100035883.1.13",
          "form": "가을이 이쁜옷이 많아서ㅠㅠ",
          "original_form": "가을이 이쁜옷이 많아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100035883.1.14",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100035883.1.15",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210830 12:54"
        },
        {
          "id": "MDRW2100035883.1.16",
          "form": "제일 짧은데 제일 예뻐요ㅋㅋㅋ",
          "original_form": "제일 짧은데 제일 예뻐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:54"
        },
        {
          "id": "MDRW2100035883.1.17",
          "form": "라이더자켓같은거 진짜",
          "original_form": "라이더자켓같은거 진짜",
          "speaker_id": "1",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100035883.1.18",
          "form": "가성비 최하",
          "original_form": "가성비 최하",
          "speaker_id": "1",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100035883.1.19",
          "form": "근데존예",
          "original_form": "근데존예",
          "speaker_id": "1",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100035883.1.20",
          "form": "아 인정이요",
          "original_form": "아 인정이요",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035883.1.21",
          "form": "라이더랑",
          "original_form": "라이더랑",
          "speaker_id": "1",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035883.1.22",
          "form": "트렌치의",
          "original_form": "트렌치의",
          "speaker_id": "1",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035883.1.23",
          "form": "계절",
          "original_form": "계절",
          "speaker_id": "1",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035883.1.24",
          "form": "한 번 입는거라도 인생샷 오케이잖아요",
          "original_form": "한 번 입는거라도 인생샷 오케이잖아요",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035883.1.25",
          "form": "크으 그쵸",
          "original_form": "크으 그쵸",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035883.1.26",
          "form": "없음 안돼죠",
          "original_form": "없음 안돼죠",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035883.1.27",
          "form": "ㅂㄱㄱㅋㄱㅋㅋㅋㅋ필수템",
          "original_form": "ㅂㄱㄱㅋㄱㅋㅋㅋㅋ필수템",
