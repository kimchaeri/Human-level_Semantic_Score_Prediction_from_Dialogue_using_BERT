      "utterance": [
        {
          "id": "MDRW2100017548.1.1",
          "form": "좋아하는 가수 있으신가요?",
          "original_form": "좋아하는 가수 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017548.1.2",
          "form": "다양하게 좋아해요!",
          "original_form": "다양하게 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017548.1.3",
          "form": "특별히 좋아하시능 가수있으신가요?",
          "original_form": "특별히 좋아하시능 가수있으신가요?",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017548.1.4",
          "form": "저도 다양해요! 박효신 아이유 그리고 엔시티",
          "original_form": "저도 다양해요! 박효신 아이유 그리고 엔시티",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017548.1.5",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017548.1.6",
          "form": "엔시티는 진짜루 좋아하는가수맞나욬ㅋㅋㅋㅋ?",
          "original_form": "엔시티는 진짜루 좋아하는가수맞나욬ㅋㅋㅋㅋ?",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017548.1.7",
          "form": "사실 엔시티가 메인입니다",
          "original_form": "사실 엔시티가 메인입니다",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017548.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017548.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ드립을 직접 들으니 새롭네욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ드립을 직접 들으니 새롭네욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017548.1.10",
          "form": "저는 잘모르긴하는데 재현?",
          "original_form": "저는 잘모르긴하는데 재현?",
          "speaker_id": "2",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017548.1.11",
          "form": "잘생겼던데요;;;;",
          "original_form": "잘생겼던데요;;;;",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017548.1.12",
          "form": "맞아요 잘생겼어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 잘생겼어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017548.1.13",
          "form": "요즘 어떤 노래 많이들으시나요?",
          "original_form": "요즘 어떤 노래 많이들으시나요?",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017548.1.14",
          "form": "다양하게 듣긴하는데 태연노래좋아서 계속들어요 ㅋㅋㅋ",
          "original_form": "다양하게 듣긴하는데 태연노래좋아서 계속들어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017548.1.15",
          "form": "위켄드인가요??",
          "original_form": "위켄드인가요??",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017548.1.16",
          "form": "월요일에들으면 그나마 기분이 나아지더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "월요일에들으면 그나마 기분이 나아지더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017548.1.17",
          "form": "아 ㅋㅋㅋㅋㅋ네 맞아요",
          "original_form": "아 ㅋㅋㅋㅋㅋ네 맞아요",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017548.1.18",
          "form": "담주에 키랑 태연 노래나오더라구요",
          "original_form": "담주에 키랑 태연 노래나오더라구요",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017548.1.19",
          "form": "기대중임니다",
          "original_form": "기대중임니다",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017548.1.20",
          "form": "오 맞아요 그렇다더라구요",
          "original_form": "오 맞아요 그렇다더라구요",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017548.1.21",
          "form": "저도기대하고있어요",
          "original_form": "저도기대하고있어요",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017548.1.22",
          "form": "ㅋㅋㅋㅋㅋ네 저는 샤이니좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋ네 저는 샤이니좋아해요",
