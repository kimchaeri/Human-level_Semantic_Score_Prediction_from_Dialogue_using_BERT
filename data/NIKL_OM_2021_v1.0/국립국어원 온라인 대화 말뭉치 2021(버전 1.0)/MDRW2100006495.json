      "utterance": [
        {
          "id": "MDRW2100006495.1.1",
          "form": "위드코로나 전환 어떻게 생각하세요",
          "original_form": "위드코로나 전환 어떻게 생각하세요",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100006495.1.2",
          "form": "이제는 코로나와 늘 함께 해야 할것 같아요",
          "original_form": "이제는 코로나와 늘 함께 해야 할것 같아요",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100006495.1.3",
          "form": "전 찬성 이예요",
          "original_form": "전 찬성 이예요",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100006495.1.4",
          "form": "그러시군요 뉴스에.빠르면 11월도 보더라구요",
          "original_form": "그러시군요 뉴스에.빠르면 11월도 보더라구요",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100006495.1.5",
          "form": "진짜로 바라지 않던 상황인데 빨리 다가 오네요",
          "original_form": "진짜로 바라지 않던 상황인데 빨리 다가 오네요",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100006495.1.6",
          "form": "일상이 다시.돌아올까요?",
          "original_form": "일상이 다시.돌아올까요?",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006495.1.7",
          "form": "코로나 이전으로 돌아가고 싶어요",
          "original_form": "코로나 이전으로 돌아가고 싶어요",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100006495.1.8",
          "form": "완벽히는 어렵다고 보더라구요 전문가들이",
          "original_form": "완벽히는 어렵다고 보더라구요 전문가들이",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100006495.1.9",
          "form": "그래서 슬퍼요",
          "original_form": "그래서 슬퍼요",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100006495.1.10",
          "form": "어떤 일상이 가장 그리우세요?",
          "original_form": "어떤 일상이 가장 그리우세요?",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100006495.1.11",
          "form": "마스크 안 쓰고 펀하게 다니는 일상이 그립네요",
          "original_form": "마스크 안 쓰고 펀하게 다니는 일상이 그립네요",
          "speaker_id": "2",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100006495.1.12",
          "form": "위드코로나는 피할수 없는 숙제인것 같네요",
          "original_form": "위드코로나는 피할수 없는 숙제인것 같네요",
          "speaker_id": "2",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100006495.1.13",
          "form": "그런가요 시기는 언제가 좋다고 생각하시나요?-",
          "original_form": "그런가요 시기는 언제가 좋다고 생각하시나요?-",
          "speaker_id": "1",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100006495.1.14",
          "form": "내년에는 가능하지 않을까요",
          "original_form": "내년에는 가능하지 않을까요",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100006495.1.15",
          "form": "그런가요 뉴스에선 10월 11월을 말하더라구요",
          "original_form": "그런가요 뉴스에선 10월 11월을 말하더라구요",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100006495.1.16",
          "form": "좀 빠르고 좀더 지나야 하지 않을까요",
          "original_form": "좀 빠르고 좀더 지나야 하지 않을까요",
          "speaker_id": "2",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100006495.1.17",
          "form": "어떻게 생각하시나요",
          "original_form": "어떻게 생각하시나요",
          "speaker_id": "2",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100006495.1.18",
          "form": "네 좀 이른듯하긴 해요",
          "original_form": "네 좀 이른듯하긴 해요",
          "speaker_id": "1",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100006495.1.19",
          "form": "뉴스를 틀어보니 위드코로나가 1번 꼭지네요",
          "original_form": "뉴스를 틀어보니 위드코로나가 1번 꼭지네요",
