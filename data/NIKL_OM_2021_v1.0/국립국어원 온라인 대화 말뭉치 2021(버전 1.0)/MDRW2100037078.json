          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100037078.1.4",
          "form": "어디 꺼?",
          "original_form": "어디 꺼?",
          "speaker_id": "1",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100037078.1.5",
          "form": "크리스피도넛이 브랸드?",
          "original_form": "크리스피도넛이 브랸드?",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037078.1.6",
          "form": "잖어",
          "original_form": "잖어",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037078.1.7",
          "form": "글레이즈 도넛이 도넛 종류 이름이구",
          "original_form": "글레이즈 도넛이 도넛 종류 이름이구",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037078.1.8",
          "form": "아 그런 거여?",
          "original_form": "아 그런 거여?",
          "speaker_id": "1",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037078.1.9",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037078.1.10",
          "form": "글레이즈드 먹고 싶다",
          "original_form": "글레이즈드 먹고 싶다",
          "speaker_id": "1",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037078.1.11",
          "form": "설탕 시럽",
          "original_form": "설탕 시럽",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037078.1.12",
          "form": "뿌려진...",
          "original_form": "뿌려진...",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037078.1.13",
          "form": "크흐..",
          "original_form": "크흐..",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037078.1.14",
