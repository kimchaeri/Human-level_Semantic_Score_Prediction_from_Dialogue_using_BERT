      "utterance": [
        {
          "id": "MDRW2100016114.1.1",
          "form": "비가그쳤네요",
          "original_form": "비가그쳤네요",
          "speaker_id": "1",
          "time": "20210826 09:52"
        },
        {
          "id": "MDRW2100016114.1.2",
          "form": "출근길에 날씨가 좋았어요 ㅎㅎ",
          "original_form": "출근길에 날씨가 좋았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:52"
        },
        {
          "id": "MDRW2100016114.1.3",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016114.1.4",
          "form": "예보보니까",
          "original_form": "예보보니까",
          "speaker_id": "2",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016114.1.5",
          "form": "내일 살짝 비소식이 있는데 정말 잠깐이네요!",
          "original_form": "내일 살짝 비소식이 있는데 정말 잠깐이네요!",
          "speaker_id": "2",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016114.1.6",
          "form": "저는 더위에 쥐약이라서 여름이 너무 힘들어요ㅜ",
          "original_form": "저는 더위에 쥐약이라서 여름이 너무 힘들어요ㅜ",
          "speaker_id": "1",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016114.1.7",
          "form": "저도요ㅠㅠㅠㅠㅠㅠ",
          "original_form": "저도요ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 10:23"
        },
        {
          "id": "MDRW2100016114.1.8",
          "form": "겨울 만세",
          "original_form": "겨울 만세",
          "speaker_id": "2",
          "time": "20210826 10:23"
        },
        {
          "id": "MDRW2100016114.1.9",
          "form": "추운 게 백배 낫습니다",
          "original_form": "추운 게 백배 낫습니다",
          "speaker_id": "2",
          "time": "20210826 10:23"
        },
        {
          "id": "MDRW2100016114.1.10",
          "form": "아ㅠㅠㅠㅠㅠ",
          "original_form": "아ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016114.1.11",
          "form": "진심",
          "original_form": "진심",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016114.1.12",
          "form": "겨울 만만세",
          "original_form": "겨울 만만세",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016114.1.13",
          "form": "껴입음되쥬~~~!!!!",
          "original_form": "껴입음되쥬~~~!!!!",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016114.1.14",
          "form": "이제 드디어 여름의 끝인거같아서 행복해욤",
          "original_form": "이제 드디어 여름의 끝인거같아서 행복해욤",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016114.1.15",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016114.1.16",
          "form": "일찍 어두워지는 건 별로인데!",
          "original_form": "일찍 어두워지는 건 별로인데!",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016114.1.17",
          "form": "그래도 겨울이 훨-씬 좋아요",
          "original_form": "그래도 겨울이 훨-씬 좋아요",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016114.1.18",
          "form": "제일 좋아하는 계절이 뭔가요?",
          "original_form": "제일 좋아하는 계절이 뭔가요?",
          "speaker_id": "1",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100016114.1.19",
          "form": "당연 겨울이지요!",
          "original_form": "당연 겨울이지요!",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016114.1.20",
          "form": "전 추운 거 좋아해요",
          "original_form": "전 추운 거 좋아해요",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016114.1.21",
          "form": "저두 더위를 많이타서\r\n겨울이 막 추웟던 적이 별로 없어요 ㅎㅎㅎ",
          "original_form": "저두 더위를 많이타서\r\n겨울이 막 추웟던 적이 별로 없어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016114.1.22",
          "form": "땀나는 거 너무 싫어요 ㅠㅠㅠㅠㅠ끈적끈적",
          "original_form": "땀나는 거 너무 싫어요 ㅠㅠㅠㅠㅠ끈적끈적",
          "speaker_id": "2",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016114.1.23",
          "form": "여름에 습도 높은 게 진짜 제일 최악같아요",
          "original_form": "여름에 습도 높은 게 진짜 제일 최악같아요",
          "speaker_id": "2",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016114.1.24",
          "form": "전 그래서 여름엔 에어컨틀어놓고 집콕해요",
          "original_form": "전 그래서 여름엔 에어컨틀어놓고 집콕해요",
          "speaker_id": "1",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016114.1.25",
          "form": "출퇴근만해도 방전되는거같아요ㅜ",
          "original_form": "출퇴근만해도 방전되는거같아요ㅜ",
          "speaker_id": "1",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016114.1.26",
          "form": "맞아요 이동할 때 진이 다 빠져서",
          "original_form": "맞아요 이동할 때 진이 다 빠져서",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016114.1.27",
          "form": "오늘은 구름 가득 흐리네요!",
          "original_form": "오늘은 구름 가득 흐리네요!",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016114.1.28",
          "form": "앗 거기는 그렇구나",
          "original_form": "앗 거기는 그렇구나",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016114.1.29",
          "form": "저는 출근길에 엄청 맑았어요 ㅎㅎ",
          "original_form": "저는 출근길에 엄청 맑았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016114.1.30",
          "form": "네! 완전 구름잔뜩이에요",
          "original_form": "네! 완전 구름잔뜩이에요",
          "speaker_id": "2",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016114.1.31",
          "form": "여긴 서울입니당",
          "original_form": "여긴 서울입니당",
          "speaker_id": "2",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016114.1.32",
          "form": "대프리카는 쨍쨍 덥네요 흑흑",
          "original_form": "대프리카는 쨍쨍 덥네요 흑흑",
          "speaker_id": "1",
          "time": "20210826 14:35"
        }
      ]
