      "utterance": [
        {
          "id": "MDRW2100019754.1.1",
          "form": "지금 일하구 계시나요??!!",
          "original_form": "지금 일하구 계시나요??!!",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019754.1.2",
          "form": "아뇨 백조된지 좀 됐답니다 ㅋㅋㅋ",
          "original_form": "아뇨 백조된지 좀 됐답니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019754.1.3",
          "form": "아하,,,",
          "original_form": "아하,,,",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019754.1.4",
          "form": "저는 내년에 졸업이라",
          "original_form": "저는 내년에 졸업이라",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019754.1.5",
          "form": "아하 ㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019754.1.6",
          "form": "학교생활 더하고싶지 않으세요?",
          "original_form": "학교생활 더하고싶지 않으세요?",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019754.1.7",
          "form": "내년에 교생나가서",
          "original_form": "내년에 교생나가서",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019754.1.8",
          "form": "음...그것도그래요!!",
          "original_form": "음...그것도그래요!!",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019754.1.9",
          "form": "재밌을거같기도하고",
          "original_form": "재밌을거같기도하고",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019754.1.10",
          "form": "와 그래요? 좋은 직업 앞두고 있네요 ㅋㅋ",
          "original_form": "와 그래요? 좋은 직업 앞두고 있네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100019754.1.11",
          "form": "후항...임용고시가 너무 좁아서",
          "original_form": "후항...임용고시가 너무 좁아서",
          "speaker_id": "1",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100019754.1.12",
          "form": "취업고민이에요 ㅠㅠ",
          "original_form": "취업고민이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100019754.1.13",
          "form": "화이팅입니다 ㅠㅠㅠ 학교다닐때가 젤 좋은거같아요",
          "original_form": "화이팅입니다 ㅠㅠㅠ 학교다닐때가 젤 좋은거같아요",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019754.1.14",
          "form": "후항 누난 무슨과였어요??",
          "original_form": "후항 누난 무슨과였어요??",
          "speaker_id": "1",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019754.1.15",
