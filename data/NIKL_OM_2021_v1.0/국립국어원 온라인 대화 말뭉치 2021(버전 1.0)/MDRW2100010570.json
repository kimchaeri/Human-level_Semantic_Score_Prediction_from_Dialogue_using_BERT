        },
        {
          "id": "MDRW2100010570.1.2",
          "form": "name2은요",
          "original_form": "&name2&은요",
          "speaker_id": "1",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010570.1.3",
          "form": "저도 그런거같아요",
          "original_form": "저도 그런거같아요",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010570.1.4",
          "form": "작년부터 미루고미루다가 그냥 식올리고있어요",
          "original_form": "작년부터 미루고미루다가 그냥 식올리고있어요",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010570.1.5",
          "form": "코로나로 더이상 미루기어렵대요",
          "original_form": "코로나로 더이상 미루기어렵대요",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010570.1.6",
          "form": "그쵸 아무래도 장기로 갈거 같으니까 ㅠㅠ",
          "original_form": "그쵸 아무래도 장기로 갈거 같으니까 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010570.1.7",
          "form": "이상형 있으세요?",
          "original_form": "이상형 있으세요?",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010570.1.8",
          "form": "4단계가 풀릴 기미가 없네요ㅎㅎ",
          "original_form": "4단계가 풀릴 기미가 없네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010570.1.9",
          "form": "저는 존경할만하구 대화가 잘통하는사람인거같아요",
          "original_form": "저는 존경할만하구 대화가 잘통하는사람인거같아요",
          "speaker_id": "2",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010570.1.10",
          "form": "name1은요",
          "original_form": "&name1&은요",
          "speaker_id": "2",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010570.1.11",
          "form": "저는 웃음코드 맞고 감정기복이 크지 않은 사람이요 ㅎㅎ",
          "original_form": "저는 웃음코드 맞고 감정기복이 크지 않은 사람이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:47"
        },
        {
          "id": "MDRW2100010570.1.12",
          "form": "역시 다들 한마음인거 같네요",
          "original_form": "역시 다들 한마음인거 같네요",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010570.1.13",
          "form": "제 주위에 결혼한분들은",
          "original_form": "제 주위에 결혼한분들은",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010570.1.14",
          "form": "남편이 아주아주 착하더라구요 ㅋㅋㅋ착한남자만 품절되나봐요",
          "original_form": "남편이 아주아주 착하더라구요 ㅋㅋㅋ착한남자만 품절되나봐요",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010570.1.15",
          "form": "ㅋㅋㅋㅋ 다 그런거죠 뭐 ㅋㅋㅋ 괜찮은 남자는 품절이 빠른 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 다 그런거죠 뭐 ㅋㅋㅋ 괜찮은 남자는 품절이 빠른 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:51"
        },
        {
          "id": "MDRW2100010570.1.16",
          "form": "ㅋㅋㅋㅋ보기드문데 어떻게  찾아냈을까나",
          "original_form": "ㅋㅋㅋㅋ보기드문데 어떻게  찾아냈을까나",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010570.1.17",
          "form": "대학때 약간 재미없는 사람이 진국인거 같아요 나중에 보면 ㅋㅋㅋㅋ",
          "original_form": "대학때 약간 재미없는 사람이 진국인거 같아요 나중에 보면 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:54"
        },
        {
          "id": "MDRW2100010570.1.18",
          "form": "오..저는 그런거 못 캐치해냈는데",
          "original_form": "오..저는 그런거 못 캐치해냈는데",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010570.1.19",
          "form": "저희 학부는 여자가 많았거든요ㅜㅜ",
          "original_form": "저희 학부는 여자가 많았거든요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010570.1.20",
          "form": "아 저도요 ㅋㅋ 10퍼가 남자라 ㅠㅠㅠ 그 안에서 만나긴 힘들었어요 ㅋㅋㅋ",
          "original_form": "아 저도요 ㅋㅋ 10퍼가 남자라 ㅠㅠㅠ 그 안에서 만나긴 힘들었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010570.1.21",
          "form": "ㅋㅋ 대학이 답이 아닌가봐요..그럼어디서 남자를..ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ 대학이 답이 아닌가봐요..그럼어디서 남자를..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100010570.1.22",
          "form": "대학때 다른 활동을 많이 하면서 만나야 했던 거 같아요 사회에 나오면 한계가 있더라고요..",
          "original_form": "대학때 다른 활동을 많이 하면서 만나야 했던 거 같아요 사회에 나오면 한계가 있더라고요..",
          "speaker_id": "1",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100010570.1.23",
