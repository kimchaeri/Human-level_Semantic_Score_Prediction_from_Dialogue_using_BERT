      "utterance": [
        {
          "id": "MDRW2100025191.1.1",
          "form": "알바하니?",
          "original_form": "알바하니?",
          "speaker_id": "2",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100025191.1.2",
          "form": "나 만화방 알바해",
          "original_form": "나 만화방 알바해",
          "speaker_id": "1",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100025191.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100025191.1.4",
          "form": "나는 편의점 알바생이야",
          "original_form": "나는 편의점 알바생이야",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100025191.1.5",
          "form": "만화방 재밌니?",
          "original_form": "만화방 재밌니?",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100025191.1.6",
          "form": "개인이 하는 곳이라 사람이 잘 안와서 좋아",
          "original_form": "개인이 하는 곳이라 사람이 잘 안와서 좋아",
          "speaker_id": "1",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100025191.1.7",
          "form": "개인시간이 많거든",
          "original_form": "개인시간이 많거든",
          "speaker_id": "1",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100025191.1.8",
          "form": "편의점 알바 힘들다던데 괜찮아?",
          "original_form": "편의점 알바 힘들다던데 괜찮아?",
          "speaker_id": "1",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100025191.1.9",
          "form": "응 나도 개인시간이 많아서 좋아",
          "original_form": "응 나도 개인시간이 많아서 좋아",
          "speaker_id": "2",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100025191.1.10",
          "form": "일하다가 만화책보기 가능?",
          "original_form": "일하다가 만화책보기 가능?",
          "speaker_id": "2",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100025191.1.11",
          "form": "응 가능해",
          "original_form": "응 가능해",
          "speaker_id": "1",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100025191.1.12",
          "form": "이것저것 많이 봤어ㅋㅋㅋ",
          "original_form": "이것저것 많이 봤어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100025191.1.13",
          "form": "시급은 최저였어?",
          "original_form": "시급은 최저였어?",
          "speaker_id": "2",
          "time": "20210901 00:32"
        },
        {
          "id": "MDRW2100025191.1.14",
          "form": "원래 최저였는데 오래 하니까 사장님이 더 챙겨주시더라!",
          "original_form": "원래 최저였는데 오래 하니까 사장님이 더 챙겨주시더라!",
          "speaker_id": "1",
          "time": "20210901 00:32"
        },
        {
          "id": "MDRW2100025191.1.15",
          "form": "좋겠다 난 그런것도 없었어",
          "original_form": "좋겠다 난 그런것도 없었어",
          "speaker_id": "2",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100025191.1.16",
          "form": "그래도 곧 그만두려고",
          "original_form": "그래도 곧 그만두려고",
          "speaker_id": "1",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100025191.1.17",
          "form": "다음엔 편의점 안할려고",
          "original_form": "다음엔 편의점 안할려고",
          "speaker_id": "2",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100025191.1.18",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100025191.1.19",
          "form": "누가 자꾸 만화방 앞에 토하고 가더라",
          "original_form": "누가 자꾸 만화방 앞에 토하고 가더라",
          "speaker_id": "1",
          "time": "20210901 00:34"
        },
        {
          "id": "MDRW2100025191.1.20",
          "form": "오우 고생이많았구나",
          "original_form": "오우 고생이많았구나",
          "speaker_id": "2",
          "time": "20210901 00:34"
        },
        {
          "id": "MDRW2100025191.1.21",
          "form": "다음엔 어디서 일하고싶니?",
          "original_form": "다음엔 어디서 일하고싶니?",
          "speaker_id": "2",
          "time": "20210901 00:34"
        },
        {
          "id": "MDRW2100025191.1.22",
          "form": "나 영화관 알바 한번 해보고 싶어",
          "original_form": "나 영화관 알바 한번 해보고 싶어",
          "speaker_id": "1",
          "time": "20210901 00:34"
        },
        {
          "id": "MDRW2100025191.1.23",
          "form": "재밌어보이지 않아?",
          "original_form": "재밌어보이지 않아?",
          "speaker_id": "1",
          "time": "20210901 00:34"
        },
        {
          "id": "MDRW2100025191.1.24",
          "form": "맞아 근데 친목이 오진대",
          "original_form": "맞아 근데 친목이 오진대",
          "speaker_id": "2",
          "time": "20210901 00:35"
        },
        {
          "id": "MDRW2100025191.1.25",
          "form": "괜찮겠어?",
          "original_form": "괜찮겠어?",
          "speaker_id": "2",
          "time": "20210901 00:35"
        },
        {
          "id": "MDRW2100025191.1.26",
          "form": "안 괜찮으면 그만두지 뭐",
          "original_form": "안 괜찮으면 그만두지 뭐",
          "speaker_id": "1",
          "time": "20210901 00:36"
        },
        {
          "id": "MDRW2100025191.1.27",
          "form": "마인드멋있다",
          "original_form": "마인드멋있다",
          "speaker_id": "2",
          "time": "20210901 00:36"
        },
        {
          "id": "MDRW2100025191.1.28",
          "form": "나도 그렇게살아야지",
          "original_form": "나도 그렇게살아야지",
          "speaker_id": "2",
          "time": "20210901 00:36"
        },
        {
          "id": "MDRW2100025191.1.29",
          "form": "평생직장도 아니고 아르바이트인데 그렇게까지 스트레스 받고 싶지 않아",
          "original_form": "평생직장도 아니고 아르바이트인데 그렇게까지 스트레스 받고 싶지 않아",
          "speaker_id": "1",
          "time": "20210901 00:36"
        }
      ]
    }
  ]
