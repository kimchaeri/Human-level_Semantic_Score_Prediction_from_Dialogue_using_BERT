      "utterance": [
        {
          "id": "MDRW2100023059.1.1",
          "form": "역시 드라이브를 해야 ㅋㅋㅋ",
          "original_form": "역시 드라이브를 해야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023059.1.2",
          "form": "손잡는건 극장도 좋은데",
          "original_form": "손잡는건 극장도 좋은데",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023059.1.3",
          "form": "덥썩",
          "original_form": "덥썩",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023059.1.4",
          "form": "넹 영화관에서도 잡았어여!",
          "original_form": "넹 영화관에서도 잡았어여!",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023059.1.5",
          "form": "이것이 30대의 진도인가",
          "original_form": "이것이 30대의 진도인가",
          "speaker_id": "2",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023059.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023059.1.7",
          "form": "이제 복습만 남았네요",
          "original_form": "이제 복습만 남았네요",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023059.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023059.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023059.1.10",
          "form": "무한복습",
          "original_form": "무한복습",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100023059.1.11",
          "form": "어우 부끄러워서 복습 못하겠다..",
          "original_form": "어우 부끄러워서 복습 못하겠다..",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100023059.1.12",
          "form": "지금 심장이 너무 쿵쾅대서",
          "original_form": "지금 심장이 너무 쿵쾅대서",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100023059.1.13",
          "form": "ㅋㅋㅋㅋㅋ지금도요?",
          "original_form": "ㅋㅋㅋㅋㅋ지금도요?",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100023059.1.14",
          "form": "캠퍼스 빠져나오다가 길 잃었어요",
          "original_form": "캠퍼스 빠져나오다가 길 잃었어요",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100023059.1.15",
          "form": "네 이게 불과 한시간도 전에 일이라ㅋㅋㅋㅋ",
          "original_form": "네 이게 불과 한시간도 전에 일이라ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100023059.1.16",
          "form": "지금 집이죠?",
          "original_form": "지금 집이죠?",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100023059.1.17",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100023059.1.18",
          "form": "아직도 두근대요?",
          "original_form": "아직도 두근대요?",
          "speaker_id": "2",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100023059.1.19",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "1",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100023059.1.20",
          "form": "어우모야 ㅋㅋㅋ",
          "original_form": "어우모야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100023059.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100023059.1.22",
          "form": "아니 진짜 1차 백신 부작용인가...",
          "original_form": "아니 진짜 1차 백신 부작용인가...",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023059.1.23",
          "form": "ㅋㅋㅋㅋ  열나요?",
          "original_form": "ㅋㅋㅋㅋ  열나요?",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023059.1.24",
          "form": "왜 이런 것일까요...",
          "original_form": "왜 이런 것일까요...",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023059.1.25",
          "form": "아니요ㅋㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023059.1.26",
          "form": "가슴이 뛰어요?",
          "original_form": "가슴이 뛰어요?",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023059.1.27",
          "form": "청춘 드라마수준인데 이거...",
          "original_form": "청춘 드라마수준인데 이거...",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023059.1.28",
          "form": "네 첫연애도 아닌데 왜이러는 걸까요",
          "original_form": "네 첫연애도 아닌데 왜이러는 걸까요",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023059.1.29",
          "form": "솔직히말해봐요",
          "original_form": "솔직히말해봐요",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023059.1.30",
          "form": "뭘욬ㅋㅋㅋㅋ",
          "original_form": "뭘욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023059.1.31",
          "form": "처음이죠?ㅋㅋㅋ",
          "original_form": "처음이죠?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023059.1.32",
          "form": "첫키스인거 같은데",
          "original_form": "첫키스인거 같은데",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023059.1.33",
          "form": "아닌데ㅋㅋㅋㅋ 그전에도 물론 설레고 떨렸겠지만은",
          "original_form": "아닌데ㅋㅋㅋㅋ 그전에도 물론 설레고 떨렸겠지만은",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023059.1.34",
          "form": "다 잊어버려서 기억이 안나네요",
          "original_form": "다 잊어버려서 기억이 안나네요",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023059.1.35",
          "form": "연애세포가 되살아났어요?",
          "original_form": "연애세포가 되살아났어요?",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023059.1.36",
          "form": "네 다 죽은 줄 알았네요 그동안",
          "original_form": "네 다 죽은 줄 알았네요 그동안",
          "speaker_id": "1",
          "time": "20210829 21:24"
        }
      ]
    }
