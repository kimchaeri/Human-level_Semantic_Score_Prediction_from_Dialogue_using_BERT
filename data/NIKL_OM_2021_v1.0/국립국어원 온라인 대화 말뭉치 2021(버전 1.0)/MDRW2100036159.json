      "utterance": [
        {
          "id": "MDRW2100036159.1.1",
          "form": "헐.......대박 무서워 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "헐.......대박 무서워 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100036159.1.2",
          "form": "그치 ㅠㅠ",
          "original_form": "그치 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100036159.1.3",
          "form": "그니깐 사모가 나도 금요일날 진짜 쥬말에",
          "original_form": "그니깐 사모가 나도 금요일날 진짜 쥬말에",
          "speaker_id": "1",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100036159.1.4",
          "form": "암것도 하지말고 쉬라고ㅠㅠ",
          "original_form": "암것도 하지말고 쉬라고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100036159.1.5",
          "form": "내일은 먼저 10시출근하기로ㅠ",
          "original_form": "내일은 먼저 10시출근하기로ㅠ",
          "speaker_id": "1",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100036159.1.6",
          "form": "시어머님이 사장님이 좋긴해 ㅎㅎ",
          "original_form": "시어머님이 사장님이 좋긴해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100036159.1.7",
          "form": "나한테 넘 미안해하심 부탁해서 ㅠ",
          "original_form": "나한테 넘 미안해하심 부탁해서 ㅠ",
          "speaker_id": "1",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100036159.1.8",
          "form": "진짜 무섭네 주위에 있으니 ㅠㅠ",
          "original_form": "진짜 무섭네 주위에 있으니 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100036159.1.9",
          "form": "일하는사람은 어쩌나싶어",
          "original_form": "일하는사람은 어쩌나싶어",
          "speaker_id": "1",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100036159.1.10",
          "form": "진짜 ㅠ 어떤회사는 2틀 휴가주고한다던데",
          "original_form": "진짜 ㅠ 어떤회사는 2틀 휴가주고한다던데",
          "speaker_id": "1",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100036159.1.11",
          "form": "그래야 평일에도 맞지 ㅠㅠ",
          "original_form": "그래야 평일에도 맞지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100036159.1.12",
          "form": "안주면 개인휴가라도 써야할듯ㅠㅠ",
          "original_form": "안주면 개인휴가라도 써야할듯ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100036159.1.13",
          "form": "그치ㅠㅠ 연차쓰는거지ㅠ",
          "original_form": "그치ㅠㅠ 연차쓰는거지ㅠ",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036159.1.14",
          "form": "난 졸지에 알바비버네 ㅋㅋㅋ",
          "original_form": "난 졸지에 알바비버네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036159.1.15",
          "form": "ㅠㅠ 얼른 괜찮아지시깋",
          "original_form": "ㅠㅠ 얼른 괜찮아지시깋",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036159.1.16",
          "form": "웅 ㅠㅠ 나도 무서웡",
          "original_form": "웅 ㅠㅠ 나도 무서웡",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036159.1.17",
          "form": "내일 10시부터 잘할수잇겟지 ㅋㅋㅋ",
          "original_form": "내일 10시부터 잘할수잇겟지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036159.1.18",
          "form": "ㅋㅋㅋ낼 비온다디 별로 안오길...",
          "original_form": "ㅋㅋㅋ낼 비온다디 별로 안오길...",
          "speaker_id": "2",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100036159.1.19",
          "form": "ㅠㅠㅠ 요새 아침에 무섭당께 ㅋㅋㅋㅋ손님이 많아",
          "original_form": "ㅠㅠㅠ 요새 아침에 무섭당께 ㅋㅋㅋㅋ손님이 많아",
          "speaker_id": "1",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100036159.1.20",
          "form": "ㅋㅋㅋ애들보내고 오는건가",
          "original_form": "ㅋㅋㅋ애들보내고 오는건가",
