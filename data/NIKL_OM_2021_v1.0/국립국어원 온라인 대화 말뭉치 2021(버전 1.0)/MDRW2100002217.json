      "utterance": [
        {
          "id": "MDRW2100002217.1.1",
          "form": "너는 종교가 있니",
          "original_form": "너는 종교가 있니",
          "speaker_id": "1",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002217.1.2",
          "form": "너는 종교가",
          "original_form": "너는 종교가",
          "speaker_id": "2",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002217.1.3",
          "form": "없어 나는 없어",
          "original_form": "없어 나는 없어",
          "speaker_id": "2",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002217.1.4",
          "form": "너는 종교가 있던가?",
          "original_form": "너는 종교가 있던가?",
          "speaker_id": "2",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002217.1.5",
          "form": "어 어떻게알았어?",
          "original_form": "어 어떻게알았어?",
          "speaker_id": "1",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002217.1.6",
          "form": "나도 없어",
          "original_form": "나도 없어",
          "speaker_id": "1",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002217.1.7",
          "form": "너는 불교쪽에 가깝지?",
          "original_form": "너는 불교쪽에 가깝지?",
          "speaker_id": "2",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002217.1.8",
          "form": "우리가 보낸 세월이",
          "original_form": "우리가 보낸 세월이",
          "speaker_id": "2",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002217.1.9",
          "form": "아무래도 굳이 따지자면",
          "original_form": "아무래도 굳이 따지자면",
          "speaker_id": "1",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002217.1.10",
          "form": "아 없어?",
          "original_form": "아 없어?",
          "speaker_id": "2",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002217.1.11",
          "form": "불교에 가깝긴하지",
          "original_form": "불교에 가깝긴하지",
          "speaker_id": "1",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002217.1.12",
          "form": "절에가면 마음이 편안해지긴해",
          "original_form": "절에가면 마음이 편안해지긴해",
          "speaker_id": "1",
          "time": "20210820 00:43"
        },
        {
          "id": "MDRW2100002217.1.13",
          "form": "오 나는 절에 가 본 적이 별로 없어.",
          "original_form": "오 나는 절에 가 본 적이 별로 없어.",
          "speaker_id": "2",
          "time": "20210820 00:43"
        },
        {
          "id": "MDRW2100002217.1.14",
          "form": "너는 어느 절에 가봤니?",
          "original_form": "너는 어느 절에 가봤니?",
          "speaker_id": "2",
          "time": "20210820 00:43"
        },
        {
          "id": "MDRW2100002217.1.15",
          "form": "아무래도 동화사가 가까우니까",
          "original_form": "아무래도 동화사가 가까우니까",
          "speaker_id": "1",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002217.1.16",
          "form": "가본적있어",
          "original_form": "가본적있어",
          "speaker_id": "1",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002217.1.17",
          "form": "아니면 불국사도 사실 절이니까",
          "original_form": "아니면 불국사도 사실 절이니까",
          "speaker_id": "1",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002217.1.18",
          "form": "나는 예전에 현장학습으로 간",
          "original_form": "나는 예전에 현장학습으로 간",
          "speaker_id": "2",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002217.1.19",
          "form": "나도 동화산데",
          "original_form": "나도 동화산데",
          "speaker_id": "2",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002217.1.20",
          "form": "너도 절에 즐겨가지는 않구나.",
          "original_form": "너도 절에 즐겨가지는 않구나.",
          "speaker_id": "2",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002217.1.21",
          "form": "나도 불국사도 가봤어.",
          "original_form": "나도 불국사도 가봤어.",
          "speaker_id": "2",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002217.1.22",
          "form": "즐겨가기엔 절이 머니까",
          "original_form": "즐겨가기엔 절이 머니까",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002217.1.23",
          "form": "템플스테이 같은거는 해보고싶지않아?",
          "original_form": "템플스테이 같은거는 해보고싶지않아?",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002217.1.24",
          "form": "그건 조금 걱정이 되네",
          "original_form": "그건 조금 걱정이 되네",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002217.1.25",
          "form": "바닥에서 자는 게 좀 힘들어서.",
          "original_form": "바닥에서 자는 게 좀 힘들어서.",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002217.1.26",
          "form": "근데 나 절밥은 먹어보고 싶어.",
          "original_form": "근데 나 절밥은 먹어보고 싶어.",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002217.1.27",
          "form": "아직 한번도 먹어본 적이 없거든.",
          "original_form": "아직 한번도 먹어본 적이 없거든.",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002217.1.28",
          "form": "나두 절밥 비빔밥",
          "original_form": "나두 절밥 비빔밥",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002217.1.29",
          "form": "절밥 먹어본 적 있니?",
          "original_form": "절밥 먹어본 적 있니?",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002217.1.30",
          "form": "근데 발우공양인가",
          "original_form": "근데 발우공양인가",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002217.1.31",
          "form": "맛있다던데.",
          "original_form": "맛있다던데.",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002217.1.32",
          "form": "그건 무서워",
          "original_form": "그건 무서워",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002217.1.33",
          "form": "그럼 애초에 밥을 조금만 받겠어.",
          "original_form": "그럼 애초에 밥을 조금만 받겠어.",
          "speaker_id": "2",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002217.1.34",
          "form": "단무지는 안먹어도 될까?",
          "original_form": "단무지는 안먹어도 될까?",
          "speaker_id": "2",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002217.1.35",
          "form": "좋은생각이다",
          "original_form": "좋은생각이다",
          "speaker_id": "1",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002217.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ어어 안받으면 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어어 안받으면 ㅠ",
          "speaker_id": "1",
          "time": "20210820 00:46"
        }
      ]
    }
