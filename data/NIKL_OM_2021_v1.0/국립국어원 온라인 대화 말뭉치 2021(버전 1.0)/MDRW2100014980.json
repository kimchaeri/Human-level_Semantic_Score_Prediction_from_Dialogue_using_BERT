      "utterance": [
        {
          "id": "MDRW2100014980.1.1",
          "form": "이번주 내내 비 오네",
          "original_form": "이번주 내내 비 오네",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014980.1.2",
          "form": "ㅠㅠ꿉꿉해서 스트레스",
          "original_form": "ㅠㅠ꿉꿉해서 스트레스",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014980.1.3",
          "form": "바람은 좀 덜불어서 괜찮은데",
          "original_form": "바람은 좀 덜불어서 괜찮은데",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014980.1.4",
          "form": "어젠가 천둥번개치고 할땐진짜..",
          "original_form": "어젠가 천둥번개치고 할땐진짜..",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014980.1.5",
          "form": "아 마저 그래도 오늘은 그냥 비만와서 다행이긴함..ㅇㅇ",
          "original_form": "아 마저 그래도 오늘은 그냥 비만와서 다행이긴함..ㅇㅇ",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014980.1.6",
          "form": "취준때문에 도서관 가던것도 비와서 안가게됨..",
          "original_form": "취준때문에 도서관 가던것도 비와서 안가게됨..",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014980.1.7",
          "form": "ㅋㅋㅋ나도..",
          "original_form": "ㅋㅋㅋ나도..",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014980.1.8",
          "form": "일자리도 없고 답답해서 외출하려고해도",
          "original_form": "일자리도 없고 답답해서 외출하려고해도",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014980.1.9",
          "form": "이시국..",
          "original_form": "이시국..",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014980.1.10",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014980.1.11",
          "form": "알바라도 하면서 준비하면 좀 나을거같은데",
          "original_form": "알바라도 하면서 준비하면 좀 나을거같은데",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014980.1.12",
          "form": "알바자리 구하는것도 힘듦ㅠ",
          "original_form": "알바자리 구하는것도 힘듦ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014980.1.13",
          "form": "그래도 곧 명절이라 좀 생기지 않으려나..",
          "original_form": "그래도 곧 명절이라 좀 생기지 않으려나..",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014980.1.14",
          "form": "아 맞네 그럴수도...",
          "original_form": "아 맞네 그럴수도...",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014980.1.15",
          "form": "요즘 워크넷보면 일자리 월급도 거의 최저 맞춰서 올려놨던데..",
          "original_form": "요즘 워크넷보면 일자리 월급도 거의 최저 맞춰서 올려놨던데..",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014980.1.16",
          "form": "알바든 일이든 빨리 내자리 하나 생겼으면...",
          "original_form": "알바든 일이든 빨리 내자리 하나 생겼으면...",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014980.1.17",
          "form": "ㅋㅋㅋㅠㅠ슬프다",
          "original_form": "ㅋㅋㅋㅠㅠ슬프다",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014980.1.18",
          "form": "요즘 사람들 스펙 다 너무좋음...",
          "original_form": "요즘 사람들 스펙 다 너무좋음...",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014980.1.19",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014980.1.20",
          "form": "지원자 스펙보면 토익부터 시작해서",
          "original_form": "지원자 스펙보면 토익부터 시작해서",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014980.1.21",
          "form": "별별 자격증다있는듯...",
          "original_form": "별별 자격증다있는듯...",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014980.1.22",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014980.1.23",
          "form": "일반회사인더 한국사 이런거도 다 따는사람들도 많고...",
          "original_form": "일반회사인더 한국사 이런거도 다 따는사람들도 많고...",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014980.1.24",
          "form": "ㅠㅠ업무랑 관련없어도 일단 한줄이라도 더 쓰는게좋으니..",
          "original_form": "ㅠㅠ업무랑 관련없어도 일단 한줄이라도 더 쓰는게좋으니..",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014980.1.25",
          "form": "취업의 길은 멀고도 험하다..",
          "original_form": "취업의 길은 멀고도 험하다..",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014980.1.26",
          "form": "ㅋㅋㅋ취업만하면 이것저것할 계획도 많이 세워놨는데...",
          "original_form": "ㅋㅋㅋ취업만하면 이것저것할 계획도 많이 세워놨는데...",
          "speaker_id": "1",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014980.1.27",
          "form": "취업이...ㅠㅠ 뭐하게?",
          "original_form": "취업이...ㅠㅠ 뭐하게?",
          "speaker_id": "2",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100014980.1.28",
          "form": "청년내일채움공제 이런 제도 좋아서 눈여겨봤었음..",
          "original_form": "청년내일채움공제 이런 제도 좋아서 눈여겨봤었음..",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100014980.1.29",
          "form": "근데 취업이...",
          "original_form": "근데 취업이...",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100014980.1.30",
          "form": "ㅋㅋㅋㅋㅠㅠ내일배움카드 이런거도 좋은데",
          "original_form": "ㅋㅋㅋㅋㅠㅠ내일배움카드 이런거도 좋은데",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014980.1.31",
          "form": "내일배움카드도 그렇고 요샌 면접비처럼 취업준비지원금?? 같은것도 준다던데",
          "original_form": "내일배움카드도 그렇고 요샌 면접비처럼 취업준비지원금?? 같은것도 준다던데",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014980.1.32",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014980.1.33",
          "form": "해당되는지 알아봐야겠네",
          "original_form": "해당되는지 알아봐야겠네",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014980.1.34",
          "form": "안그래도 취업힘든데 코로나랑 겹쳐서...",
          "original_form": "안그래도 취업힘든데 코로나랑 겹쳐서...",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014980.1.35",
          "form": "ㅠㅠㅠ취뽀...",
          "original_form": "ㅠㅠㅠ취뽀...",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014980.1.36",
          "form": "이와중에 취업하는 사람많다는게 믿기지가않음..",
          "original_form": "이와중에 취업하는 사람많다는게 믿기지가않음..",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014980.1.37",
          "form": "정부일자리사업 이런것도 잘 찾아보고",
          "original_form": "정부일자리사업 이런것도 잘 찾아보고",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014980.1.38",
          "form": "지원할수있는거 해야겠음..",
          "original_form": "지원할수있는거 해야겠음..",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014980.1.39",
          "form": "그런것도 다포함한 통계라고하니까",
          "original_form": "그런것도 다포함한 통계라고하니까",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014980.1.40",
          "form": "나도 찾아보고 취업한 사람에 속하고싶다",
          "original_form": "나도 찾아보고 취업한 사람에 속하고싶다",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014980.1.41",
          "form": "ㄱㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㄱㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014980.1.42",
          "form": "맞아 인턴이라도 할수있으면",
          "original_form": "맞아 인턴이라도 할수있으면",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014980.1.43",
          "form": "이력서에 쓰기도좋고..",
          "original_form": "이력서에 쓰기도좋고..",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014980.1.44",
          "form": "ㅠㅠ일단 공모전이라도 열심히 참여해봐야겠다",
          "original_form": "ㅠㅠ일단 공모전이라도 열심히 참여해봐야겠다",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014980.1.45",
          "form": "준비기간동안 뭐했냐고 면접가면 백퍼 물어볼듯",
          "original_form": "준비기간동안 뭐했냐고 면접가면 백퍼 물어볼듯",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014980.1.46",
          "form": "ㅠㅠㅠ스트레스",
          "original_form": "ㅠㅠㅠ스트레스",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100014980.1.47",
          "form": "좋은 면접관이 왔으면...ㅋㅋㅋㅋㄱ",
          "original_form": "좋은 면접관이 왔으면...ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100014980.1.48",
          "form": "생각만해도 면접분위기 숨막힌다",
          "original_form": "생각만해도 면접분위기 숨막힌다",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014980.1.49",
          "form": "취업준비할땐 취준스트레스 일하면 업무스트레스",
          "original_form": "취업준비할땐 취준스트레스 일하면 업무스트레스",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014980.1.50",
          "form": "ㅋㅋㅋㅋㅠㅠ그래도 업무스트레스가 차라리 나을듯",
          "original_form": "ㅋㅋㅋㅋㅠㅠ그래도 업무스트레스가 차라리 나을듯",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014980.1.51",
          "form": "ㅋㅋㅋ그것만있겠나",
          "original_form": "ㅋㅋㅋ그것만있겠나",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014980.1.52",
          "form": "같이 일하는 사람들이랑 신경전",
          "original_form": "같이 일하는 사람들이랑 신경전",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014980.1.53",
          "form": "업무로 핀잔주고...",
          "original_form": "업무로 핀잔주고...",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014980.1.54",
          "form": "그러니까 들어가기전에 스펙 빵빵하게 채우고 가야함..",
          "original_form": "그러니까 들어가기전에 스펙 빵빵하게 채우고 가야함..",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014980.1.55",
          "form": "ㅋㅋㅋㅋ우리에겐 화려한 수상경력과 자격증이 필요하다..!",
          "original_form": "ㅋㅋㅋㅋ우리에겐 화려한 수상경력과 자격증이 필요하다..!",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100014980.1.56",
