      "utterance": [
        {
          "id": "MDRW2100025684.1.1",
          "form": "오 그런 류 좋아하시는구나",
          "original_form": "오 그런 류 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025684.1.2",
          "form": "전 사실 의학드라마?는 잘 못봐요.",
          "original_form": "전 사실 의학드라마?는 잘 못봐요.",
          "speaker_id": "2",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025684.1.3",
          "form": "좀 피나오고 메스나오고 그런걸 못견디는편이에요",
          "original_form": "좀 피나오고 메스나오고 그런걸 못견디는편이에요",
          "speaker_id": "2",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025684.1.4",
          "form": "사실 그아는",
          "original_form": "사실 그아는",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025684.1.5",
          "form": "의학 반",
          "original_form": "의학 반",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025684.1.6",
          "form": "1 9금 반",
          "original_form": "1 9금 반",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025684.1.7",
          "form": "이라ㅋㅋㅋㅋㅋ",
          "original_form": "이라ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025684.1.8",
          "form": "그럼 봤던 미드는 뭐 있으세요?",
          "original_form": "그럼 봤던 미드는 뭐 있으세요?",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025684.1.9",
          "form": "좀 예전이긴 한데",
          "original_form": "좀 예전이긴 한데",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025684.1.10",
          "form": "재밌게 봤던건 빅뱅이론이요ㅋㅋㅋ",
          "original_form": "재밌게 봤던건 빅뱅이론이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100025684.1.11",
          "form": "앜ㅋㅋㅋ미드계의 조상",
          "original_form": "앜ㅋㅋㅋ미드계의 조상",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100025684.1.12",
          "form": "괴짜들이잖아요",
          "original_form": "괴짜들이잖아요",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100025684.1.13",
          "form": "그당시엔 너무 어려울거같아서 안 보다가",
          "original_form": "그당시엔 너무 어려울거같아서 안 보다가",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100025684.1.14",
          "form": "나중에 봐야지했는데 재밋나요?!",
          "original_form": "나중에 봐야지했는데 재밋나요?!",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100025684.1.15",
          "form": "뭐 어려운 소리 쏼라쏼라하는데",
          "original_form": "뭐 어려운 소리 쏼라쏼라하는데",
          "speaker_id": "2",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100025684.1.16",
          "form": "굳이 깊게 이해 안해도 돼요",
          "original_form": "굳이 깊게 이해 안해도 돼요",
          "speaker_id": "2",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100025684.1.17",
          "form": "sf물에 양자역할 어쩌구저쩌구 해도",
          "original_form": "sf물에 양자역할 어쩌구저쩌구 해도",
          "speaker_id": "2",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100025684.1.18",
          "form": "걍 뭐 좀 대단한, 현재는 없는 기술을 썼구나 정도로 넘어가면 되는것처럼요",
          "original_form": "걍 뭐 좀 대단한, 현재는 없는 기술을 썼구나 정도로 넘어가면 되는것처럼요",
          "speaker_id": "2",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100025684.1.19",
          "form": "양자역학...입니다...ㅎㅎ",
          "original_form": "양자역학...입니다...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025684.1.20",
          "form": "하하하.....",
          "original_form": "하하하.....",
          "speaker_id": "1",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025684.1.21",
          "form": "더 미뤄야겠네요ㅋㅋㅋ",
          "original_form": "더 미뤄야겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025684.1.22",
          "form": "유치하긴 해도 가십걸도 재밌게 봤어요",
          "original_form": "유치하긴 해도 가십걸도 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100025684.1.23",
          "form": "그러실필요없는데",
          "original_form": "그러실필요없는데",
          "speaker_id": "2",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100025684.1.24",
          "form": "그냥 문이과 개그를 좀 스케일업 한 느낌이에요",
          "original_form": "그냥 문이과 개그를 좀 스케일업 한 느낌이에요",
          "speaker_id": "2",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100025684.1.25",
          "form": "가십걸 전 짤?로는 많이 봤어요ㅋㅋㅋ",
          "original_form": "가십걸 전 짤?로는 많이 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100025684.1.26",
          "form": "미란다 라고 아세요?",
          "original_form": "미란다 라고 아세요?",
          "speaker_id": "1",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025684.1.27",
          "form": "영국 시트콤인데",
          "original_form": "영국 시트콤인데",
          "speaker_id": "1",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025684.1.28",
          "form": "앗 알아요.",
          "original_form": "앗 알아요.",
          "speaker_id": "2",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025684.1.29",
          "form": "그 웃기다는 분",
          "original_form": "그 웃기다는 분",
          "speaker_id": "2",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025684.1.30",
          "form": "역시 토막영상으론 봤어요.",
          "original_form": "역시 토막영상으론 봤어요.",
          "speaker_id": "2",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025684.1.31",
          "form": "아ㅎㅎ풀영상으로 봐도",
          "original_form": "아ㅎㅎ풀영상으로 봐도",
          "speaker_id": "1",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025684.1.32",
          "form": "되게 재밌어요",
          "original_form": "되게 재밌어요",
          "speaker_id": "1",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025684.1.33",
          "form": "한번 봐야지 봐야지 싶으면서도",
          "original_form": "한번 봐야지 봐야지 싶으면서도",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025684.1.34",
          "form": "그렇게 되더라구요",
          "original_form": "그렇게 되더라구요",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025684.1.35",
          "form": "그 배우님 영화 스파이에서 봤는데",
          "original_form": "그 배우님 영화 스파이에서 봤는데",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025684.1.36",
          "form": "아주 씬스틸러! ㅋㅋㅋ",
          "original_form": "아주 씬스틸러! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025684.1.37",
          "form": "진짜요??",
          "original_form": "진짜요??",
          "speaker_id": "1",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025684.1.38",
          "form": "함 봐야겠어요ㅋㅋㅋ전 미란다 시트콤에서만",
          "original_form": "함 봐야겠어요ㅋㅋㅋ전 미란다 시트콤에서만",
          "speaker_id": "1",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025684.1.39",
          "form": "봤어서",
          "original_form": "봤어서",
          "speaker_id": "1",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025684.1.40",
          "form": "분량이 많지는 않은데",
          "original_form": "분량이 많지는 않은데",
          "speaker_id": "2",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100025684.1.41",
          "form": "나오실때마다 특유의 억양? 제스처? 가 넘 재밌어요",
          "original_form": "나오실때마다 특유의 억양? 제스처? 가 넘 재밌어요",
          "speaker_id": "2",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025684.1.42",
          "form": "영화자체도 가볍고 재밌게 볼만한 내용이에요",
          "original_form": "영화자체도 가볍고 재밌게 볼만한 내용이에요",
          "speaker_id": "2",
          "time": "20210901 16:17"
        }
      ]
