      "utterance": [
        {
          "id": "MDRW2100016984.1.1",
          "form": "팝콘을팔긴하는데",
          "original_form": "팝콘을팔긴하는데",
          "speaker_id": "1",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016984.1.2",
          "form": "포장해서 집에서 먹어야한대요ㅋㅋ",
          "original_form": "포장해서 집에서 먹어야한대요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016984.1.3",
          "form": "맞아여ㅋㅋㅋ",
          "original_form": "맞아여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016984.1.4",
          "form": "너무 웃기더리구요ㅋㅋㅋ",
          "original_form": "너무 웃기더리구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016984.1.5",
          "form": "팝콘 배달하는것도 아세요?",
          "original_form": "팝콘 배달하는것도 아세요?",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016984.1.6",
          "form": "싸졌더늬",
          "original_form": "싸졌더늬",
          "speaker_id": "1",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016984.1.7",
          "form": "그런 배경ㅋㅋ",
          "original_form": "그런 배경ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016984.1.8",
          "form": "극장서 배달도 해주나요??",
          "original_form": "극장서 배달도 해주나요??",
          "speaker_id": "1",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016984.1.9",
          "form": "네ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100016984.1.10",
          "form": "이벤트로 해준건지는 모르겠지만ㅋㅋㅋ",
          "original_form": "이벤트로 해준건지는 모르겠지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100016984.1.11",
          "form": "팝콘 배달 해주더라구요ㅋㅋ",
          "original_form": "팝콘 배달 해주더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100016984.1.12",
          "form": "솔직히 영화관 팝콘 보다 맛있는 팝콘은 못찾겠더라구요ㅜ",
          "original_form": "솔직히 영화관 팝콘 보다 맛있는 팝콘은 못찾겠더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100016984.1.13",
          "form": "ㅋㅋ아 지나가다 팝콘사러 들어가야겠네요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ아 지나가다 팝콘사러 들어가야겠네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100016984.1.14",
          "form": "그 따끈하고 달달한 팝콘이 그립습니다ㅠㅜ",
          "original_form": "그 따끈하고 달달한 팝콘이 그립습니다ㅠㅜ",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100016984.1.15",
          "form": "ㅋㅋ 캬라멜아니면 오리지날파?",
          "original_form": "ㅋㅋ 캬라멜아니면 오리지날파?",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100016984.1.16",
          "form": "당연 캬라멜이죠 ㅋㅋㅋㅋ",
          "original_form": "당연 캬라멜이죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:22"
        },
        {
          "id": "MDRW2100016984.1.17",
          "form": "영화관에서만 먹을수있는ㅋㅋㅋㅋ",
          "original_form": "영화관에서만 먹을수있는ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:22"
        },
        {
          "id": "MDRW2100016984.1.18",
          "form": "그 느낌ㅎ",
          "original_form": "그 느낌ㅎ",
          "speaker_id": "2",
          "time": "20210827 09:22"
        },
        {
          "id": "MDRW2100016984.1.19",
          "form": "ㅋㅋ아 영화관 팝콘 쌀때 먹어야하는데",
          "original_form": "ㅋㅋ아 영화관 팝콘 쌀때 먹어야하는데",
          "speaker_id": "1",
          "time": "20210827 09:23"
        },
        {
          "id": "MDRW2100016984.1.20",
          "form": "안에서 못먹어서 할인판매한대요",
          "original_form": "안에서 못먹어서 할인판매한대요",
          "speaker_id": "1",
          "time": "20210827 09:23"
        },
        {
          "id": "MDRW2100016984.1.21",
          "form": "그렇게라도 팔아야하니ㅠㅜ",
          "original_form": "그렇게라도 팔아야하니ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100016984.1.22",
          "form": "아이맥스로 보신적 있으세요??",
          "original_form": "아이맥스로 보신적 있으세요??",
          "speaker_id": "2",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100016984.1.23",
          "form": "한번이요 아바타였나",
          "original_form": "한번이요 아바타였나",
          "speaker_id": "1",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100016984.1.24",
          "form": "근데 기억이 안나요",
          "original_form": "근데 기억이 안나요",
          "speaker_id": "1",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100016984.1.25",
          "form": "큰 스크린으로 보는 이유가 있더라구요ㅋㅋㅋ",
          "original_form": "큰 스크린으로 보는 이유가 있더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100016984.1.26",
          "form": "전 엑시트 봤는데...",
          "original_form": "전 엑시트 봤는데...",
          "speaker_id": "2",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100016984.1.27",
          "form": "몰입감이 ㅋㅋㅋ",
          "original_form": "몰입감이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100016984.1.28",
          "form": "긴장감 쩔었어요ㅋㅋ",
          "original_form": "긴장감 쩔었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100016984.1.29",
          "form": "아 엑시트",
          "original_form": "아 엑시트",
          "speaker_id": "1",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100016984.1.30",
          "form": "재밌게봤어요",
          "original_form": "재밌게봤어요",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100016984.1.31",
          "form": "잘만든영화에요",
          "original_form": "잘만든영화에요",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100016984.1.32",
          "form": "생각외로 너무 좋았어요!!!",
          "original_form": "생각외로 너무 좋았어요!!!",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100016984.1.33",
          "form": "저도 기대감 없이 갔었는데",
          "original_form": "저도 기대감 없이 갔었는데",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100016984.1.34",
          "form": "재난영화로 진짜 딱!!!",
          "original_form": "재난영화로 진짜 딱!!!",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100016984.1.35",
          "form": "일반적인 신파도 거의 없고ㅋㅋㅋ",
          "original_form": "일반적인 신파도 거의 없고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100016984.1.36",
          "form": "네 또 그영화나와서",
          "original_form": "네 또 그영화나와서",
          "speaker_id": "1",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100016984.1.37",
          "form": "옥상문 항시 열어놓기 제도도 바뀌었잖아요",
          "original_form": "옥상문 항시 열어놓기 제도도 바뀌었잖아요",
          "speaker_id": "1",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100016984.1.38",
          "form": "sos 수신호도 확실히 알았구요ㅋㅋㅋ",
          "original_form": "sos 수신호도 확실히 알았구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100016984.1.39",
          "form": "진짜 좋은 영화ㅋㅋㅋ",
          "original_form": "진짜 좋은 영화ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100016984.1.40",
          "form": "네 머릿속에 울리는 따따 ㅋㅋㅋ",
          "original_form": "네 머릿속에 울리는 따따 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:53"
        },
        {
          "id": "MDRW2100016984.1.41",
          "form": "잊진 않을거 같아요ㅋㅋㅋ",
          "original_form": "잊진 않을거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:06"
        },
        {
          "id": "MDRW2100016984.1.42",
          "form": "재밋도있고 현실도 좋게 변화시켜주는 한국영화 진짜 백점짜리라고 봅니다",
