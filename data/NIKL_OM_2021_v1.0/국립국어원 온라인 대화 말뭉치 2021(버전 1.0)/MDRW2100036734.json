      "utterance": [
        {
          "id": "MDRW2100036734.1.1",
          "form": "룸메 언제까지\n쉬는거!??",
          "original_form": "룸메 언제까지\n쉬는거!??",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100036734.1.2",
          "form": "룸메 출근합니다 밤일가요 ㅋㅋㅋㅋ",
          "original_form": "룸메 출근합니다 밤일가요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100036734.1.3",
          "form": "이제 일해야죠 언니~",
          "original_form": "이제 일해야죠 언니~",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100036734.1.4",
          "form": "너무 많이 쉬었네요",
          "original_form": "너무 많이 쉬었네요",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100036734.1.5",
          "form": "아하 밤일 가는구만",
          "original_form": "아하 밤일 가는구만",
          "speaker_id": "2",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100036734.1.6",
          "form": "요번 달 카드값만 600이나왔어요..",
          "original_form": "요번 달 카드값만 600이나왔어요..",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100036734.1.7",
          "form": "오토바이 타러 갈까요?",
          "original_form": "오토바이 타러 갈까요?",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100036734.1.8",
          "form": "헉 600....",
          "original_form": "헉 600....",
          "speaker_id": "2",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100036734.1.9",
          "form": "살수있나....",
          "original_form": "살수있나....",
          "speaker_id": "2",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100036734.1.10",
          "form": "그래 투잡 뛰어라",
          "original_form": "그래 투잡 뛰어라",
          "speaker_id": "2",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100036734.1.11",
          "form": "안되겠다",
          "original_form": "안되겠다",
          "speaker_id": "2",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100036734.1.12",
          "form": "나가는 건 왜 이렇게 많이 나가는지ㅠ",
          "original_form": "나가는 건 왜 이렇게 많이 나가는지ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:57"
        },
        {
          "id": "MDRW2100036734.1.13",
          "form": "저녁에 오토바이 타야 되겠어요ㅋㅋㅋㅋㅋ",
          "original_form": "저녁에 오토바이 타야 되겠어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:57"
        },
        {
          "id": "MDRW2100036734.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그래 내가 주문하고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그래 내가 주문하고",
          "speaker_id": "2",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100036734.1.15",
          "form": "꼭 안전하게 와주세요 라고 적을께 ㅋㅋㅋㄱ",
          "original_form": "꼭 안전하게 와주세요 라고 적을께 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100036734.1.16",
          "form": "아 웃겨 오다가 하나 먹어도 됩니다라고 적어줘요ㅋㅋㅋㅋㅋ",
          "original_form": "아 웃겨 오다가 하나 먹어도 됩니다라고 적어줘요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100036734.1.17",
          "form": "그건 안 된다",
          "original_form": "그건 안 된다",
          "speaker_id": "2",
          "time": "20210831 12:38"
        },
        {
          "id": "MDRW2100036734.1.18",
          "form": "먹지는 말고 조심히만 가져와",
          "original_form": "먹지는 말고 조심히만 가져와",
          "speaker_id": "2",
          "time": "20210831 12:38"
        },
        {
          "id": "MDRW2100036734.1.19",
          "form": "손끝 도 대지마라",
          "original_form": "손끝 도 대지마라",
          "speaker_id": "2",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100036734.1.20",
          "form": "노력해 볼게요ㅋㅋㅋㅋ",
          "original_form": "노력해 볼게요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100036734.1.21",
          "form": "오늘 아침 둘째 6시부터 유튜브 틀으라고",
          "original_form": "오늘 아침 둘째 6시부터 유튜브 틀으라고",
          "speaker_id": "1",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100036734.1.22",
          "form": "울고 소리 지르고",
          "original_form": "울고 소리 지르고",
          "speaker_id": "1",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100036734.1.23",
          "form": "엉덩이 맴매했어요",
          "original_form": "엉덩이 맴매했어요",
          "speaker_id": "1",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100036734.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㄱㄱㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㄱㄱㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036734.1.25",
          "form": "너거둘째도",
          "original_form": "너거둘째도",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036734.1.26",
          "form": "유투브에 정신못차리네",
          "original_form": "유투브에 정신못차리네",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036734.1.27",
          "form": "그니까요 언니",
          "original_form": "그니까요 언니",
          "speaker_id": "1",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100036734.1.28",
          "form": "새벽 6시에 말이 됩니까",
          "original_form": "새벽 6시에 말이 됩니까",
          "speaker_id": "1",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100036734.1.29",
          "form": "고집이 무집이 정말ㅠ",
          "original_form": "고집이 무집이 정말ㅠ",
          "speaker_id": "1",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100036734.1.30",
          "form": "그래 우리 투도 매일 유투브 다",
          "original_form": "그래 우리 투도 매일 유투브 다",
          "speaker_id": "2",
          "time": "20210831 13:28"
        },
        {
          "id": "MDRW2100036734.1.31",
          "form": "주말만 유튜브 볼 수 있다고 해놨더니",
          "original_form": "주말만 유튜브 볼 수 있다고 해놨더니",
          "speaker_id": "2",
          "time": "20210831 13:28"
        },
        {
          "id": "MDRW2100036734.1.32",
          "form": "내일 주말만 기다림",
          "original_form": "내일 주말만 기다림",
          "speaker_id": "2",
          "time": "20210831 13:28"
        },
        {
          "id": "MDRW2100036734.1.33",
          "form": "오늘 아침에도 일어나자마자 엄마 오늘 가면 이제 주말이야?? 라고 ㅋㅋㄱㅋㄱㄱ",
          "original_form": "오늘 아침에도 일어나자마자 엄마 오늘 가면 이제 주말이야?? 라고 ㅋㅋㄱㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210831 13:28"
        },
        {
          "id": "MDRW2100036734.1.34",
          "form": "어제 하루갔는데\nㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어제 하루갔는데\nㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:28"
        },
        {
          "id": "MDRW2100036734.1.35",
          "form": "언니 우리 첫째도 그랬어요ㅋㅋㅋㅋㅋ",
          "original_form": "언니 우리 첫째도 그랬어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:39"
        },
        {
          "id": "MDRW2100036734.1.36",
          "form": "일도 학교가??라고ㅋㅋㅋㅋ",
          "original_form": "일도 학교가??라고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:39"
        }
      ]
    }
