      "utterance": [
        {
          "id": "MDRW2100016262.1.1",
          "form": "최근에 사신 물건 있으신가요?",
          "original_form": "최근에 사신 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016262.1.2",
          "form": "어제 점프수트샀어요ㅋㅋ",
          "original_form": "어제 점프수트샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016262.1.3",
          "form": "오 점프수트 좋죠ㅋㅋㅋㅋ 옷 고민 없이 입을 때 좋아요",
          "original_form": "오 점프수트 좋죠ㅋㅋㅋㅋ 옷 고민 없이 입을 때 좋아요",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016262.1.4",
          "form": "전 최근에 와인이랑 과자 샀어요!",
          "original_form": "전 최근에 와인이랑 과자 샀어요!",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016262.1.5",
          "form": "오 와인  좋아하시나요",
          "original_form": "오 와인  좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016262.1.6",
          "form": "좋아는 하는데 많이 못사봤어요ㅋㅋㅋㅋ 맛있더라고요",
          "original_form": "좋아는 하는데 많이 못사봤어요ㅋㅋㅋㅋ 맛있더라고요",
          "speaker_id": "1",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016262.1.7",
          "form": "오홍 사신 와인 이름이 뭐에요?",
          "original_form": "오홍 사신 와인 이름이 뭐에요?",
          "speaker_id": "2",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100016262.1.8",
          "form": "베어풋 스위트레드요~!",
          "original_form": "베어풋 스위트레드요~!",
          "speaker_id": "1",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100016262.1.9",
          "form": "이거 베어풋 중에 까베르네쇼비뇽을 추천하던데 편의점에는 안들어오더라고요",
          "original_form": "이거 베어풋 중에 까베르네쇼비뇽을 추천하던데 편의점에는 안들어오더라고요",
          "speaker_id": "1",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100016262.1.10",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016262.1.11",
          "form": "산미 있는 와인 좋아하시나봐요",
          "original_form": "산미 있는 와인 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016262.1.12",
          "form": "산미는 없는 것보다는 있는 게 좋더라고요",
          "original_form": "산미는 없는 것보다는 있는 게 좋더라고요",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016262.1.13",
          "form": "너무 아이셔마냥 신 거 아니면...ㅎ",
          "original_form": "너무 아이셔마냥 신 거 아니면...ㅎ",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016262.1.14",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100016262.1.15",
          "form": "그럼 브리갈다라 발폴리첼라 드셔보세욥",
          "original_form": "그럼 브리갈다라 발폴리첼라 드셔보세욥",
          "speaker_id": "2",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100016262.1.16",
          "form": "제 최애 와인ㅎㅎ",
          "original_form": "제 최애 와인ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100016262.1.17",
          "form": "ㄷㄷ... name2 10대 아니셨나요",
          "original_form": "ㄷㄷ... &name2& 10대 아니셨나요",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100016262.1.18",
          "form": "일단 추천 받고 갑니다ㅋㅋㅋ",
          "original_form": "일단 추천 받고 갑니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100016262.1.19",
          "form": "저 올해 20이요ㅋㅋ",
          "original_form": "저 올해 20이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016262.1.20",
