      "utterance": [
        {
          "id": "MDRW2100027978.1.1",
          "form": "요즘 당기는 음식은 뭐야?",
          "original_form": "요즘 당기는 음식은 뭐야?",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027978.1.2",
          "form": "요즘 고기가 너무 땡겨 체력이 부족한가봐",
          "original_form": "요즘 고기가 너무 땡겨 체력이 부족한가봐",
          "speaker_id": "2",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027978.1.3",
          "form": "체력 딸릴땐 고기지",
          "original_form": "체력 딸릴땐 고기지",
          "speaker_id": "1",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027978.1.4",
          "form": "맞아 삼겹살 구워서 쌈싸먹고 싶다",
          "original_form": "맞아 삼겹살 구워서 쌈싸먹고 싶다",
          "speaker_id": "2",
          "time": "20210902 16:41"
        },
        {
          "id": "MDRW2100027978.1.5",
          "form": "너는 당기는게 뭐야?",
          "original_form": "너는 당기는게 뭐야?",
          "speaker_id": "2",
          "time": "20210902 16:41"
        },
        {
          "id": "MDRW2100027978.1.6",
          "form": "나는 삼겹살에 김치 구워서 먹고싶어",
          "original_form": "나는 삼겹살에 김치 구워서 먹고싶어",
          "speaker_id": "1",
          "time": "20210902 16:41"
        },
        {
          "id": "MDRW2100027978.1.7",
          "form": "좋지좋지",
          "original_form": "좋지좋지",
          "speaker_id": "2",
          "time": "20210902 16:41"
        },
        {
          "id": "MDRW2100027978.1.8",
          "form": "삼겹살에 된장찌개? 냉면?",
          "original_form": "삼겹살에 된장찌개? 냉면?",
          "speaker_id": "1",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027978.1.9",
          "form": "아 고민되네 된장찌개",
          "original_form": "아 고민되네 된장찌개",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100027978.1.10",
          "form": "나는 비빔냉면이 좋더라",
          "original_form": "나는 비빔냉면이 좋더라",
          "speaker_id": "1",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027978.1.11",
          "form": "삼겹살에 비빔면",
          "original_form": "삼겹살에 비빔면",
          "speaker_id": "2",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100027978.1.12",
          "form": "먹고싶다",
          "original_form": "먹고싶다",
          "speaker_id": "2",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100027978.1.13",
          "form": "맞아 먹고싶어 지금 단것도 먹고싶어",
          "original_form": "맞아 먹고싶어 지금 단것도 먹고싶어",
          "speaker_id": "1",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100027978.1.14",
          "form": "나는 와플 먹고 싶어",
          "original_form": "나는 와플 먹고 싶어",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100027978.1.15",
          "form": "와플에 아이스아메리카노 ㅠ",
          "original_form": "와플에 아이스아메리카노 ㅠ",
