      "utterance": [
        {
          "id": "MDRW2100006192.1.1",
          "form": "올해 부상이 좀 있었던가요?",
          "original_form": "올해 부상이 좀 있었던가요?",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100006192.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100006192.1.3",
          "form": "팔꿈치가 좀....",
          "original_form": "팔꿈치가 좀....",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100006192.1.4",
          "form": "계약 마지막해라",
          "original_form": "계약 마지막해라",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100006192.1.5",
          "form": "FA 아니고 포스팅이었던가요?",
          "original_form": "FA 아니고 포스팅이었던가요?",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006192.1.6",
          "form": "슥에서 보내준거였어요",
          "original_form": "슥에서 보내준거였어요",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006192.1.7",
          "form": "복귀는 슥이죠",
          "original_form": "복귀는 슥이죠",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006192.1.8",
          "form": "내년에는 김광현도 양현종도 돌아올 것 같네요..",
          "original_form": "내년에는 김광현도 양현종도 돌아올 것 같네요..",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006192.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006192.1.10",
          "form": "솔직히 김하성도 너무 못쳐서",
          "original_form": "솔직히 김하성도 너무 못쳐서",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006192.1.11",
          "form": "우리나라 선수들 왜케 속구를 못칠까요?",
          "original_form": "우리나라 선수들 왜케 속구를 못칠까요?",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100006192.1.12",
          "form": "투수들 공이 느려서?",
          "original_form": "투수들 공이 느려서?",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100006192.1.13",
          "form": "거긴 뭐 아직 어려서ㅋㅋ",
          "original_form": "거긴 뭐 아직 어려서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100006192.1.14",
          "form": "공 느린 건 어쩔 수 없고 스트라이크존도 넓혀야 할 것 같아요",
          "original_form": "공 느린 건 어쩔 수 없고 스트라이크존도 넓혀야 할 것 같아요",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100006192.1.15",
          "form": "김하성 타율이 거의 1할 가까워지려고 해요ㅜㅜ",
          "original_form": "김하성 타율이 거의 1할 가까워지려고 해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100006192.1.16",
          "form": "박병호도 속구 못쳐서",
          "original_form": "박병호도 속구 못쳐서",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100006192.1.17",
          "form": "돌아왔는데",
          "original_form": "돌아왔는데",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100006192.1.18",
          "form": "다른 팀 가는 게 더 좋았을련지..",
          "original_form": "다른 팀 가는 게 더 좋았을련지..",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100006192.1.19",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100006192.1.20",
          "form": "샌디에이고 라인업이 너무 빵빵함",
          "original_form": "샌디에이고 라인업이 너무 빵빵함",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100006192.1.21",
          "form": "아님 뭐 트레이드 가능성도 있구요",
          "original_form": "아님 뭐 트레이드 가능성도 있구요",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100006192.1.22",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100006192.1.23",
          "form": "광현이 7월에 엄청 잘할때",
          "original_form": "광현이 7월에 엄청 잘할때",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100006192.1.24",
          "form": "트레이드썰 나왔었는데",
          "original_form": "트레이드썰 나왔었는데",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100006192.1.25",
          "form": "결국남더라구요",
          "original_form": "결국남더라구요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100006192.1.26",
          "form": "트레이드 됐으면 부상 때문에 욕만 먹었겠네요",
          "original_form": "트레이드 됐으면 부상 때문에 욕만 먹었겠네요",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100006192.1.27",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100006192.1.28",
          "form": "델꼬왔는데",
          "original_form": "델꼬왔는데",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100006192.1.29",
          "form": "쓸모가 없으니",
          "original_form": "쓸모가 없으니",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100006192.1.30",
          "form": "윈나우 팀에서 탐낸다고 들었었는데요",
