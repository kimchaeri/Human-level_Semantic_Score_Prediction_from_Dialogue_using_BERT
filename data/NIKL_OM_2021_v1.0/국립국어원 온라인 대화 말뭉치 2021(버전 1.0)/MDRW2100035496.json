      "utterance": [
        {
          "id": "MDRW2100035496.1.1",
          "form": "저녁드셨어요~?",
          "original_form": "저녁드셨어요~?",
          "speaker_id": "2",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100035496.1.2",
          "form": "넹넹 비비고 왕만두요",
          "original_form": "넹넹 비비고 왕만두요",
          "speaker_id": "1",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100035496.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100035496.1.4",
          "form": "네네 저도 집밥먹고 복숭아먹었네요 ㅎ",
          "original_form": "네네 저도 집밥먹고 복숭아먹었네요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100035496.1.5",
          "form": "복숭아라니 맛있겠어요 물복인가요 딱복인가용ㅋㅋㅋ",
          "original_form": "복숭아라니 맛있겠어요 물복인가요 딱복인가용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100035496.1.6",
          "form": "집에 딱딱이 말랑이 다잇는데 ㅋㅋㅋ 오눌운 말랑이 줄줄 백도먹었네요 ㅎ",
          "original_form": "집에 딱딱이 말랑이 다잇는데 ㅋㅋㅋ 오눌운 말랑이 줄줄 백도먹었네요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 19:33"
        },
        {
          "id": "MDRW2100035496.1.7",
          "form": "말랑복숭아 너무 맛있겠다 ㅠㅠ 전 무화과를 먹었어요",
          "original_form": "말랑복숭아 너무 맛있겠다 ㅠㅠ 전 무화과를 먹었어요",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100035496.1.8",
          "form": "무화과도 맛있죠 올해는 아직 못먹어봤네여",
          "original_form": "무화과도 맛있죠 올해는 아직 못먹어봤네여",
          "speaker_id": "2",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100035496.1.9",
          "form": "제가 오늘 먹은거 약간 흙맛인데 무화과과 원래 좀 흙맛나니깐 괜찮았어요 ㅋㅋ",
          "original_form": "제가 오늘 먹은거 약간 흙맛인데 무화과과 원래 좀 흙맛나니깐 괜찮았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100035496.1.10",
          "form": "특유에 맛고달달 하죠 말랑말랑 좋아요 ㅎㅎ",
          "original_form": "특유에 맛고달달 하죠 말랑말랑 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100035496.1.11",
          "form": "네 그리고 계절감을 줘서 더 좋아요 ㅋㅋㅋ",
          "original_form": "네 그리고 계절감을 줘서 더 좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100035496.1.12",
          "form": "가을이구나아",
          "original_form": "가을이구나아",
          "speaker_id": "1",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100035496.1.13",
          "form": "마트가니 보이더라구요 ㅎㅎ~",
          "original_form": "마트가니 보이더라구요 ㅎㅎ~",
          "speaker_id": "2",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100035496.1.14",
          "form": "복숭아를 마트에서 사셨군요 저도 복숭아를 더 먹구 싶은데",
          "original_form": "복숭아를 마트에서 사셨군요 저도 복숭아를 더 먹구 싶은데",
          "speaker_id": "1",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100035496.1.15",
          "form": "복숭아가 잘 안보이네요 그래서 포도를 좀 사왔어요",
          "original_form": "복숭아가 잘 안보이네요 그래서 포도를 좀 사왔어요",
          "speaker_id": "1",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100035496.1.16",
          "form": "복숭아는 밭에가서 사왔어요!",
          "original_form": "복숭아는 밭에가서 사왔어요!",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100035496.1.17",
          "form": "포도는 올해 2번먹었는데 아주달더라구요!",
          "original_form": "포도는 올해 2번먹었는데 아주달더라구요!",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100035496.1.18",
          "form": "우와 그럼 더 신선하겠네요 ㅠㅠㅠㅠ!!! 완전부러워요",
          "original_form": "우와 그럼 더 신선하겠네요 ㅠㅠㅠㅠ!!! 완전부러워요",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100035496.1.19",
          "form": "마트에서 산 포도도 달긴 하지만 밭에가서 골라온 복숭아만 할까요",
          "original_form": "마트에서 산 포도도 달긴 하지만 밭에가서 골라온 복숭아만 할까요",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100035496.1.20",
          "form": "경북이라 밭이 보이는데 저도 추천받은곳에만 가서 사보긴했어요",
          "original_form": "경북이라 밭이 보이는데 저도 추천받은곳에만 가서 사보긴했어요",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100035496.1.21",
          "form": "올해 그래도 기본으로 과일맛이좋은거같아요",
          "original_form": "올해 그래도 기본으로 과일맛이좋은거같아요",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100035496.1.22",
          "form": "추천이 안전하죠 모르는데건 바로 사기 좀 그래요",
          "original_form": "추천이 안전하죠 모르는데건 바로 사기 좀 그래요",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100035496.1.23",
          "form": "비가 아주 많이 오지 않아서 물맛 과일이 적어요",
          "original_form": "비가 아주 많이 오지 않아서 물맛 과일이 적어요",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100035496.1.24",
          "form": "그쵸 올해수박도",
          "original_form": "그쵸 올해수박도",
          "speaker_id": "2",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100035496.1.25",
          "form": "다 성공해서 좋았어료",
          "original_form": "다 성공해서 좋았어료",
          "speaker_id": "2",
          "time": "20210829 20:22"
        }
      ]
    }
