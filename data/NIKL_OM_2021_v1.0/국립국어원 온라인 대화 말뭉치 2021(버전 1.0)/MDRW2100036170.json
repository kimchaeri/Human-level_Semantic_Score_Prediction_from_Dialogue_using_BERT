      "utterance": [
        {
          "id": "MDRW2100036170.1.1",
          "form": "전 마스크 뾰루지는",
          "original_form": "전 마스크 뾰루지는",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036170.1.2",
          "form": "스트라이덱스 필링패드로 샥샥",
          "original_form": "스트라이덱스 필링패드로 샥샥",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036170.1.3",
          "form": "ㅋㅋ 그거 편하고 좋죠",
          "original_form": "ㅋㅋ 그거 편하고 좋죠",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036170.1.4",
          "form": "쌱 닦아주면 상쾌하더라고요",
          "original_form": "쌱 닦아주면 상쾌하더라고요",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036170.1.5",
          "form": "이것두,, 한두번이지 매일하면 너무 자극적이라",
          "original_form": "이것두,, 한두번이지 매일하면 너무 자극적이라",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036170.1.6",
          "form": "근본적인 해결책은 아니지만",
          "original_form": "근본적인 해결책은 아니지만",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036170.1.7",
          "form": "그래도 자주써요",
          "original_form": "그래도 자주써요",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036170.1.8",
          "form": "저는 약산성 좋대서 썼다가",
          "original_form": "저는 약산성 좋대서 썼다가",
          "speaker_id": "1",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100036170.1.9",
          "form": "여드름만 더 나서 그냥 뽀독뽀독 닦이는 폼클 사서 써요ㅋㅋ",
          "original_form": "여드름만 더 나서 그냥 뽀독뽀독 닦이는 폼클 사서 써요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100036170.1.10",
          "form": "어차피 높은 습기랑 습기가 만드는 팡이랑 마스크스치는 자극이 만든거라 ㅠ",
          "original_form": "어차피 높은 습기랑 습기가 만드는 팡이랑 마스크스치는 자극이 만든거라 ㅠ",
          "speaker_id": "2",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100036170.1.11",
          "form": "클렌져로 커버가 안돼요,,",
          "original_form": "클렌져로 커버가 안돼요,,",
          "speaker_id": "2",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100036170.1.12",
          "form": "아무도 없는데가서 턱스크 하는게 더 효과는 좋,,지만",
          "original_form": "아무도 없는데가서 턱스크 하는게 더 효과는 좋,,지만",
          "speaker_id": "2",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100036170.1.13",
          "form": "그렇구나ㅠ",
          "original_form": "그렇구나ㅠ",
          "speaker_id": "1",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100036170.1.14",
          "form": "ㅋㅋㅋ그럴수는 없으니까요",
          "original_form": "ㅋㅋㅋ그럴수는 없으니까요",
          "speaker_id": "1",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100036170.1.15",
          "form": "건물안은 공조기로 공기순환하는데",
          "original_form": "건물안은 공조기로 공기순환하는데",
          "speaker_id": "2",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100036170.1.16",
          "form": "마스크 벗기가 어렵죠 ㅠㅋㅋ",
          "original_form": "마스크 벗기가 어렵죠 ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100036170.1.17",
          "form": "요즘은 마스크도 가지각색으로 이쁜거 쓰시더라고요",
          "original_form": "요즘은 마스크도 가지각색으로 이쁜거 쓰시더라고요",
          "speaker_id": "1",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100036170.1.18",
          "form": "그져 공기가 순환되니까요",
          "original_form": "그져 공기가 순환되니까요",
          "speaker_id": "1",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100036170.1.19",
          "form": "전 코로나 초기에 무서워서 비싸게 많이사놔서",
          "original_form": "전 코로나 초기에 무서워서 비싸게 많이사놔서",
          "speaker_id": "2",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100036170.1.20",
          "form": "지금 마스크를 3년은 쓰게 생겼어요",
          "original_form": "지금 마스크를 3년은 쓰게 생겼어요",
          "speaker_id": "2",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100036170.1.21",
          "form": "아 뾰루지에 좀 도움되는건 마스크를 자주 교체하는거?",
          "original_form": "아 뾰루지에 좀 도움되는건 마스크를 자주 교체하는거?",
          "speaker_id": "2",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100036170.1.22",
          "form": "ㅋㅋ그져 오래쓰면 팡이가 있으니까요",
          "original_form": "ㅋㅋ그져 오래쓰면 팡이가 있으니까요",
          "speaker_id": "1",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036170.1.23",
          "form": "하루 하나 심한날은 하루 2개 이렇게 쓰거나 혹은 교체는 안하더라도 좀 마르게 다른걸로 갈아기면 도움돼요",
          "original_form": "하루 하나 심한날은 하루 2개 이렇게 쓰거나 혹은 교체는 안하더라도 좀 마르게 다른걸로 갈아기면 도움돼요",
          "speaker_id": "2",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036170.1.24",
          "form": "저도 가끔 찝찝하면 두개쓰게되더라고요",
          "original_form": "저도 가끔 찝찝하면 두개쓰게되더라고요",
          "speaker_id": "1",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100036170.1.25",
          "form": "비싸지만ㅋ쿠",
          "original_form": "비싸지만ㅋ쿠",
          "speaker_id": "1",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100036170.1.26",
          "form": "요즘 94매스크는 개당 100원에도 나와요",
          "original_form": "요즘 94매스크는 개당 100원에도 나와요",
          "speaker_id": "2",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100036170.1.27",
          "form": "하지만 전 이미 몇백개 있어서 안살분,,",
          "original_form": "하지만 전 이미 몇백개 있어서 안살분,,",
          "speaker_id": "2",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100036170.1.28",
          "form": "ㅋㅋㅋㄱㅋ초기에 엄청사셨군요",
