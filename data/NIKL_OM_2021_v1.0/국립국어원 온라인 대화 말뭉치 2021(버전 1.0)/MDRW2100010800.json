      "utterance": [
        {
          "id": "MDRW2100010800.1.1",
          "form": "최근에 구매하신 물건 있으세요?",
          "original_form": "최근에 구매하신 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100010800.1.2",
          "form": "옷을 구매했습니다. 날씨가 바뀌어서 그런가봐요",
          "original_form": "옷을 구매했습니다. 날씨가 바뀌어서 그런가봐요",
          "speaker_id": "2",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010800.1.3",
          "form": "아 지금 딱 환절기라 그렇죠",
          "original_form": "아 지금 딱 환절기라 그렇죠",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010800.1.4",
          "form": "어떤 옷 사셔썽요?",
          "original_form": "어떤 옷 사셔썽요?",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010800.1.5",
          "form": "무릎 위까지 내려오는 바지 샀습니다. name1님은 쇼핑 좀 하셧나요",
          "original_form": "무릎 위까지 내려오는 바지 샀습니다. &name1&님은 쇼핑 좀 하셧나요",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010800.1.6",
          "form": "이제 해여죠 ㅜㅜ",
          "original_form": "이제 해여죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010800.1.7",
          "form": "전 오프라인 쇼핑 좋아해서 나가야하는데",
          "original_form": "전 오프라인 쇼핑 좋아해서 나가야하는데",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010800.1.8",
          "form": "코로나땜시 ㅜㅜ 못나가구 잇네요",
          "original_form": "코로나땜시 ㅜㅜ 못나가구 잇네요",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010800.1.9",
          "form": "좋아하는 브랜드 잇나요",
          "original_form": "좋아하는 브랜드 잇나요",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010800.1.10",
          "form": "요즘 mlb 운동화예브더라구요",
          "original_form": "요즘 mlb 운동화예브더라구요",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010800.1.11",
          "form": "이번주에 백화점 나가서 어글리슈즈 하나 사려구요!",
          "original_form": "이번주에 백화점 나가서 어글리슈즈 하나 사려구요!",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010800.1.12",
          "form": "어글리 슈즈 같으거요?",
          "original_form": "어글리 슈즈 같으거요?",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010800.1.13",
          "form": "오 역시 그거군요",
          "original_form": "오 역시 그거군요",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010800.1.14",
          "form": "좋은것 같아요",
          "original_form": "좋은것 같아요",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010800.1.15",
          "form": "어떤 신발 주로 신으세요?",
          "original_form": "어떤 신발 주로 신으세요?",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010800.1.16",
          "form": "요즘은 비와서 쪼리요 ㅋㅋ",
          "original_form": "요즘은 비와서 쪼리요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010800.1.17",
          "form": "오오 쪼리 불편라지않아요??",
          "original_form": "오오 쪼리 불편라지않아요??",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010800.1.18",
          "form": "좋은 쪼리를 선물 받아서 괜찮은것 같아요 ㅎㅎ 당근마켓도 쓰세요?",
          "original_form": "좋은 쪼리를 선물 받아서 괜찮은것 같아요 ㅎㅎ 당근마켓도 쓰세요?",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010800.1.19",
          "form": "네네 가끔써요 ㅎㅎ",
          "original_form": "네네 가끔써요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010800.1.20",
          "form": "당근에서 주로 뭐사세요?",
          "original_form": "당근에서 주로 뭐사세요?",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010800.1.21",
          "form": "애플 제품 악세사리나 기프티콘이요 ㅋㅋ",
          "original_form": "애플 제품 악세사리나 기프티콘이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010800.1.22",
          "form": "기프티콘 구매해보니까 좋더라구요",
          "original_form": "기프티콘 구매해보니까 좋더라구요",
          "speaker_id": "2",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010800.1.23",
          "form": "와 당근이 다른 업체보다 싸나요?",
          "original_form": "와 당근이 다른 업체보다 싸나요?",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010800.1.24",
          "form": "개인이 기프티콘을 파는거죠",
          "original_form": "개인이 기프티콘을 파는거죠",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010800.1.25",
          "form": "근데 보통 10프로 이상 싸요",
          "original_form": "근데 보통 10프로 이상 싸요",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010800.1.26",
          "form": "스타벅스 종종 가시나요",
          "original_form": "스타벅스 종종 가시나요",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010800.1.27",
          "form": "가끔요? 사실 커피를 안마셔서 친구만날때빼고는 잘 안가요 ㅋㅋ",
          "original_form": "가끔요? 사실 커피를 안마셔서 친구만날때빼고는 잘 안가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010800.1.28",
          "form": "스타벅스 기프티콘이 많이 올라와 있어요 ㅋㅋ",
          "original_form": "스타벅스 기프티콘이 많이 올라와 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010800.1.29",
          "form": "그래서 텀블러쿠폰 같은거 저렴하게 사면 이득이죠",
          "original_form": "그래서 텀블러쿠폰 같은거 저렴하게 사면 이득이죠",
          "speaker_id": "2",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010800.1.30",
          "form": "스타벋스 텀블러가 튼튼하고 좋긴해요 ㅋㅋㅋ",
          "original_form": "스타벋스 텀블러가 튼튼하고 좋긴해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010800.1.31",
          "form": "가령 5000원으로 텀블러쿠폰을 당근에서 사면 모든 종류, 모든 사이즈 음료를 그 가격에 마실 수 있어요",
          "original_form": "가령 5000원으로 텀블러쿠폰을 당근에서 사면 모든 종류, 모든 사이즈 음료를 그 가격에 마실 수 있어요",
          "speaker_id": "2",
          "time": "20210823 22:41"
        }
