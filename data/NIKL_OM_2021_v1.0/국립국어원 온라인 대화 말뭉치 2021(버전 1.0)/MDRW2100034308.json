      "utterance": [
        {
          "id": "MDRW2100034308.1.1",
          "form": "로제 소스가 요즘 인기더라구요",
          "original_form": "로제 소스가 요즘 인기더라구요",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034308.1.2",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034308.1.3",
          "form": "뭐좋아하세요?",
          "original_form": "뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034308.1.4",
          "form": "저 사실 로제떡볶이요 ㅋㅋㅋ",
          "original_form": "저 사실 로제떡볶이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034308.1.5",
          "form": "저랑 같네요 ㅋㅋㅋ",
          "original_form": "저랑 같네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034308.1.6",
          "form": "어디거 드세요?",
          "original_form": "어디거 드세요?",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034308.1.7",
          "form": "전 신전이 젤좋더라구요",
          "original_form": "전 신전이 젤좋더라구요",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034308.1.8",
          "form": "달아가지구 ㅋㅋㅋ",
          "original_form": "달아가지구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034308.1.9",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034308.1.10",
          "form": "오저두요!",
          "original_form": "오저두요!",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034308.1.11",
          "form": "거기가 제일 맛있어요 양도 적당하구",
          "original_form": "거기가 제일 맛있어요 양도 적당하구",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034308.1.12",
          "form": "1인분먹기",
          "original_form": "1인분먹기",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034308.1.13",
          "form": "딱이죠?",
          "original_form": "딱이죠?",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034308.1.14",
          "form": "튀김도 대박 맛나요",
          "original_form": "튀김도 대박 맛나요",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034308.1.15",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034308.1.16",
          "form": "무슨 튀김드세요?",
          "original_form": "무슨 튀김드세요?",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034308.1.17",
          "form": "전 오징어튀김 최애에요",
          "original_form": "전 오징어튀김 최애에요",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034308.1.18",
          "form": "뭐가 더 맛있어요?",
          "original_form": "뭐가 더 맛있어요?",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034308.1.19",
          "form": "저도 오징어튀김이요 ㅋㅋ",
          "original_form": "저도 오징어튀김이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034308.1.20",
          "form": "그쵸 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034308.1.21",
          "form": "신전 오징어가 유독 더 맛난거같아요 실하고",
          "original_form": "신전 오징어가 유독 더 맛난거같아요 실하고",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
