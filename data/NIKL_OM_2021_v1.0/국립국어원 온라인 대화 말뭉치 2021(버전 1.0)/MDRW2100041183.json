      "utterance": [
        {
          "id": "MDRW2100041183.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 19:41"
        },
        {
          "id": "MDRW2100041183.1.2",
          "form": "이제 밤공기는 꽤 차네요",
          "original_form": "이제 밤공기는 꽤 차네요",
          "speaker_id": "1",
          "time": "20210905 19:41"
        },
        {
          "id": "MDRW2100041183.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210905 19:42"
        },
        {
          "id": "MDRW2100041183.1.4",
          "form": "날씨 너무 좋구 하늘 너무 이뻐요",
          "original_form": "날씨 너무 좋구 하늘 너무 이뻐요",
          "speaker_id": "2",
          "time": "20210905 19:42"
        },
        {
          "id": "MDRW2100041183.1.5",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210905 19:42"
        },
        {
          "id": "MDRW2100041183.1.6",
          "form": "주말동안 날씨가 정말 맑고 좋았어요",
          "original_form": "주말동안 날씨가 정말 맑고 좋았어요",
          "speaker_id": "1",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100041183.1.7",
          "form": "딱 해지기전에 너무 이뻐요ㅜ",
          "original_form": "딱 해지기전에 너무 이뻐요ㅜ",
          "speaker_id": "2",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100041183.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100041183.1.9",
          "form": "낮에 파란 하늘도 예쁘더라고요",
          "original_form": "낮에 파란 하늘도 예쁘더라고요",
          "speaker_id": "1",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100041183.1.10",
          "form": "하늘구경만해도 시간가는게 너무 아쉬워요",
          "original_form": "하늘구경만해도 시간가는게 너무 아쉬워요",
          "speaker_id": "2",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100041183.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100041183.1.12",
          "form": "가을이 짧을 것 같아 더 아쉬워요",
          "original_form": "가을이 짧을 것 같아 더 아쉬워요",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100041183.1.13",
          "form": "맞아요ㅜㅜ 곧 추워지겠죠ㅜㅜ",
          "original_form": "맞아요ㅜㅜ 곧 추워지겠죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100041183.1.14",
          "form": "네 곧 겨울이 올듯ㅠ",
          "original_form": "네 곧 겨울이 올듯ㅠ",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041183.1.15",
          "form": "요즘 봄 가을은 정말 짧죠",
          "original_form": "요즘 봄 가을은 정말 짧죠",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041183.1.16",
          "form": "가을 너므 좋아요",
          "original_form": "가을 너므 좋아요",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100041183.1.17",
          "form": "억새랑 코스모스",
          "original_form": "억새랑 코스모스",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100041183.1.18",
          "form": "네 가을 풍경들 예쁘죠",
          "original_form": "네 가을 풍경들 예쁘죠",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100041183.1.19",
          "form": "단풍들도 그렇고",
          "original_form": "단풍들도 그렇고",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100041183.1.20",
          "form": "백신맞고는 멀리좀 가볼까싶어요ㅎㅎ",
          "original_form": "백신맞고는 멀리좀 가볼까싶어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041183.1.21",
          "form": "아 가을 나들이 계획하시나요?",
          "original_form": "아 가을 나들이 계획하시나요?",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041183.1.22",
          "form": "지금 저희 동네는 비가 와요",
          "original_form": "지금 저희 동네는 비가 와요",
          "speaker_id": "1",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100041183.1.23",
