        {
          "id": "MDRW2100004925.1.1",
          "form": "영화좋아요?",
          "original_form": "영화좋아요?",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004925.1.2",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004925.1.3",
          "form": "요즘 코로나땜에 못가서 안타까워요",
          "original_form": "요즘 코로나땜에 못가서 안타까워요",
          "speaker_id": "1",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004925.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004925.1.5",
          "form": "마지막으로 간게 기생충이였던거같아요",
          "original_form": "마지막으로 간게 기생충이였던거같아요",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100004925.1.6",
          "form": "전 극한직업",
          "original_form": "전 극한직업",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100004925.1.7",
          "form": "아...진짜 오래되신거같네요",
          "original_form": "아...진짜 오래되신거같네요",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100004925.1.8",
          "form": "넼\r\nㅂ",
          "original_form": "넼\r\nㅂ",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100004925.1.9",
          "form": "요즘 모가디슈 잼난데요",
          "original_form": "요즘 모가디슈 잼난데요",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100004925.1.10",
          "form": "아 저도 들었어요",
          "original_form": "아 저도 들었어요",
          "speaker_id": "2",
          "time": "20210822 08:40"
        },
        {
          "id": "MDRW2100004925.1.11",
          "form": "인질두요 황정민 나오는데",
          "original_form": "인질두요 황정민 나오는데",
          "speaker_id": "1",
          "time": "20210822 09:16"
        },
        {
          "id": "MDRW2100004925.1.12",
          "form": "잼나나봐요",
          "original_form": "잼나나봐요",
          "speaker_id": "1",
          "time": "20210822 09:16"
        },
        {
          "id": "MDRW2100004925.1.13",
          "form": "모가디슈도요",
          "original_form": "모가디슈도요",
          "speaker_id": "2",
          "time": "20210822 09:16"
        },
        {
          "id": "MDRW2100004925.1.14",
          "form": "네 싱크홀도 감동 코믹용으로 좋데요",
          "original_form": "네 싱크홀도 감동 코믹용으로 좋데요",
          "speaker_id": "1",
          "time": "20210822 09:17"
        },
        {
          "id": "MDRW2100004925.1.15",
          "form": "그른가요",
          "original_form": "그른가요",
          "speaker_id": "2",
