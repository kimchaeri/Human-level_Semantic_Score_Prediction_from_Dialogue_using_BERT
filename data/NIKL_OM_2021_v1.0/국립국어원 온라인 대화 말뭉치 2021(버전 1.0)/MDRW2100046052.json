        {
          "id": "MDRW2100046052.1.1",
          "form": "나 어제 이사했어",
          "original_form": "나 어제 이사했어",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046052.1.2",
          "form": "이사 간 집은 어때",
          "original_form": "이사 간 집은 어때",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046052.1.3",
          "form": "일단 넓어서 좋아",
          "original_form": "일단 넓어서 좋아",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046052.1.4",
          "form": "원래 있던 집보다 넓은가 보네요",
          "original_form": "원래 있던 집보다 넓은가 보네요",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046052.1.5",
          "form": "훨씬 넓어",
          "original_form": "훨씬 넓어",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046052.1.6",
          "form": "청소는 어떻게 했어",
          "original_form": "청소는 어떻게 했어",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046052.1.7",
          "form": "말도 마",
          "original_form": "말도 마",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046052.1.8",
          "form": "힘들었나보네",
          "original_form": "힘들었나보네",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046052.1.9",
          "form": "장난 아니었어",
          "original_form": "장난 아니었어",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046052.1.10",
          "form": "원래 다른 사람이 살다가 면 그렇지",
          "original_form": "원래 다른 사람이 살다가 면 그렇지",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046052.1.11",
          "form": "화장실 청소만을 했어",
          "original_form": "화장실 청소만을 했어",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046052.1.12",
          "form": "화장실 청소 너무 어려워",
          "original_form": "화장실 청소 너무 어려워",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
