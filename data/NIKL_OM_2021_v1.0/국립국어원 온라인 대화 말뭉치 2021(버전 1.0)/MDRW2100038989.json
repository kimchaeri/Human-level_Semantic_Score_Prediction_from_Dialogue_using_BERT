      "utterance": [
        {
          "id": "MDRW2100038989.1.1",
          "form": "우왁!!! 맛있을 수밖에 없는 조합이잖아요!!",
          "original_form": "우왁!!! 맛있을 수밖에 없는 조합이잖아요!!",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100038989.1.2",
          "form": "당잗!! 당장 먹어봐여갰어요 내일",
          "original_form": "당잗!! 당장 먹어봐여갰어요 내일",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100038989.1.3",
          "form": "하 꼭 드셔요 !!! 디저트에 추가해서 !!!",
          "original_form": "하 꼭 드셔요 !!! 디저트에 추가해서 !!!",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100038989.1.4",
          "form": "크로플에 올려먹어도 맛있을맛이라구여ㅜㅜ",
          "original_form": "크로플에 올려먹어도 맛있을맛이라구여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100038989.1.5",
          "form": "으악 내일 백신 맞고 먹을 디저트. 정해졋군요",
          "original_form": "으악 내일 백신 맞고 먹을 디저트. 정해졋군요",
          "speaker_id": "2",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100038989.1.6",
          "form": "오는 길에 사와야갯어요!",
          "original_form": "오는 길에 사와야갯어요!",
          "speaker_id": "2",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100038989.1.7",
          "form": "저도 백신은 안맞지만 사와야겠어여!",
          "original_form": "저도 백신은 안맞지만 사와야겠어여!",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100038989.1.8",
          "form": "꼭꼭 잘챙겨드셔요 ! 1차죠 ??!",
          "original_form": "꼭꼭 잘챙겨드셔요 ! 1차죠 ??!",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100038989.1.9",
          "form": "네에... 그래서 조오금 오들오들 떨고 있어요 ㅠㅠ!",
          "original_form": "네에... 그래서 조오금 오들오들 떨고 있어요 ㅠㅠ!",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100038989.1.10",
          "form": "아플까봐!",
          "original_form": "아플까봐!",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100038989.1.11",
          "form": "먹는 걸로.. 치유해야죠..",
          "original_form": "먹는 걸로.. 치유해야죠..",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100038989.1.12",
          "form": "안아파여 !!! 근데 팔은 아프니까 ㅜㅜ",
          "original_form": "안아파여 !!! 근데 팔은 아프니까 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100038989.1.13",
          "form": "한손으로 먹을 준비 하셔요 !!!",
          "original_form": "한손으로 먹을 준비 하셔요 !!!",
          "speaker_id": "1",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100038989.1.14",
          "form": "어라라... 저 밥 먹을 ㄸㅐ 한정 양손잡이거든요",
          "original_form": "어라라... 저 밥 먹을 ㄸㅐ 한정 양손잡이거든요",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100038989.1.15",
          "form": "클..낫데요",
          "original_form": "클..낫데요",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100038989.1.16",
          "form": "으아아아어ㆍ",
          "original_form": "으아아아어ㆍ",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100038989.1.17",
          "form": "저두...가끔 양손으로 먹는데",
          "original_form": "저두...가끔 양손으로 먹는데",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100038989.1.18",
          "form": "그날은 한팔 봉인이였어요...",
          "original_form": "그날은 한팔 봉인이였어요...",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100038989.1.19",
          "form": "그래도 잘먹어져요 먹을 의지가 있다면 !",
          "original_form": "그래도 잘먹어져요 먹을 의지가 있다면 !",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100038989.1.20",
          "form": "노..노력해보겠습니닷@@@ 한 이틀만 먹짱의 타이틀을..",
