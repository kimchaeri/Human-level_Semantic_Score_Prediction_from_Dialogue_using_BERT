      "utterance": [
        {
          "id": "MDRW2100031867.1.1",
          "form": "안녕하세요 배달음식 자주 드세요?",
          "original_form": "안녕하세요 배달음식 자주 드세요?",
          "speaker_id": "1",
          "time": "20210905 06:37"
        },
        {
          "id": "MDRW2100031867.1.2",
          "form": "요즘은 자주 먹는것같아요",
          "original_form": "요즘은 자주 먹는것같아요",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100031867.1.3",
          "form": "가족들 집에 있는 시간이 많으니 다 할수는 없어서요",
          "original_form": "가족들 집에 있는 시간이 많으니 다 할수는 없어서요",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100031867.1.4",
          "form": "네 코로나 오고 그렇게 되었네요",
          "original_form": "네 코로나 오고 그렇게 되었네요",
          "speaker_id": "1",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100031867.1.5",
          "form": "얼마전에 회를 포장해서 집에서 먹엇는데 가성비가 엄청 좋더라구요",
          "original_form": "얼마전에 회를 포장해서 집에서 먹엇는데 가성비가 엄청 좋더라구요",
          "speaker_id": "2",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100031867.1.6",
          "form": "저녁은 뭘드셨나요?",
          "original_form": "저녁은 뭘드셨나요?",
          "speaker_id": "2",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100031867.1.7",
          "form": "옥수수 먹었어요~",
          "original_form": "옥수수 먹었어요~",
          "speaker_id": "1",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100031867.1.8",
          "form": "너무 적게 드셨네요",
          "original_form": "너무 적게 드셨네요",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100031867.1.9",
          "form": "한개먹진 않았죠 ㅋㅋ",
          "original_form": "한개먹진 않았죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100031867.1.10",
          "form": "간식으로 먹었던 메뉴라서요.",
          "original_form": "간식으로 먹었던 메뉴라서요.",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100031867.1.11",
          "form": "초당옥수수 드셔보셨나요?",
          "original_form": "초당옥수수 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100031867.1.12",
          "form": "아뇨 안먹어봤어요",
          "original_form": "아뇨 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100031867.1.13",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "1",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100031867.1.14",
          "form": "아삭아삭하고 달아요",
          "original_form": "아삭아삭하고 달아요",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100031867.1.15",
          "form": "호불호가 있지만 저는 그맛을 좋아해요",
          "original_form": "호불호가 있지만 저는 그맛을 좋아해요",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100031867.1.16",
          "form": "지금은 철이 아니라 안나오지만 내년에는 꼭드셔보세요",
          "original_form": "지금은 철이 아니라 안나오지만 내년에는 꼭드셔보세요",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100031867.1.17",
          "form": "네 추천 감사합니다 먹어볼게요!",
          "original_form": "네 추천 감사합니다 먹어볼게요!",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100031867.1.18",
          "form": "저는 훈제오리를 김치에 볶아서 먹었답니다",
          "original_form": "저는 훈제오리를 김치에 볶아서 먹었답니다",
          "speaker_id": "2",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100031867.1.19",
          "form": "오 맛있겠어요 ㅎㅎㅎ",
          "original_form": "오 맛있겠어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:44"
        }
      ]
