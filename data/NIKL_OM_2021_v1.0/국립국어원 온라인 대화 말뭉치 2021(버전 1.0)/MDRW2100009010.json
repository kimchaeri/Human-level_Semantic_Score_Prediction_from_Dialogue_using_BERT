      "utterance": [
        {
          "id": "MDRW2100009010.1.1",
          "form": "안녕하세요 요즘 개키우는 사람들 많이 보이던데요",
          "original_form": "안녕하세요 요즘 개키우는 사람들 많이 보이던데요",
          "speaker_id": "1",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100009010.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100009010.1.3",
          "form": "저는 지금은 못키우고 있어요",
          "original_form": "저는 지금은 못키우고 있어요",
          "speaker_id": "2",
          "time": "20210822 20:34"
        },
        {
          "id": "MDRW2100009010.1.4",
          "form": "옛날엔 마당있는집에 살아서 강아지 키웠었는데 지금은 혼자 자취하느라 여건이 안되내요",
          "original_form": "옛날엔 마당있는집에 살아서 강아지 키웠었는데 지금은 혼자 자취하느라 여건이 안되내요",
          "speaker_id": "2",
          "time": "20210822 20:35"
        },
        {
          "id": "MDRW2100009010.1.5",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "1",
          "time": "20210822 20:35"
        },
        {
          "id": "MDRW2100009010.1.6",
          "form": "아무래도 그렇죠",
          "original_form": "아무래도 그렇죠",
          "speaker_id": "1",
          "time": "20210822 20:35"
        },
        {
          "id": "MDRW2100009010.1.7",
          "form": "저는 거북이만 키워봤어요",
          "original_form": "저는 거북이만 키워봤어요",
          "speaker_id": "1",
          "time": "20210822 20:35"
        },
        {
          "id": "MDRW2100009010.1.8",
          "form": "제가 살던 동네만 그랬는지 모르겠는대",
          "original_form": "제가 살던 동네만 그랬는지 모르겠는대",
          "speaker_id": "2",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100009010.1.9",
          "form": "한때 거북이 키우는게 유행한적이있었어요",
          "original_form": "한때 거북이 키우는게 유행한적이있었어요",
          "speaker_id": "2",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100009010.1.10",
          "form": "그래서 친구들이 많이 키웠었어요",
          "original_form": "그래서 친구들이 많이 키웠었어요",
          "speaker_id": "2",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100009010.1.11",
          "form": "네 혹시 오래전인가요?",
          "original_form": "네 혹시 오래전인가요?",
          "speaker_id": "1",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100009010.1.12",
          "form": "저는 아주예전에 키웠어요",
          "original_form": "저는 아주예전에 키웠어요",
          "speaker_id": "1",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100009010.1.13",
          "form": "네 많이요ㅎㅎ",
          "original_form": "네 많이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100009010.1.14",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100009010.1.15",
          "form": "다들 거북이 많이 키운거같아요",
          "original_form": "다들 거북이 많이 키운거같아요",
          "speaker_id": "1",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100009010.1.16",
          "form": "혹시 여건 되시면 키워보고 싶으신 반려동물 있으세요?",
          "original_form": "혹시 여건 되시면 키워보고 싶으신 반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100009010.1.17",
          "form": "강아지 키워보고 싶어요",
          "original_form": "강아지 키워보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100009010.1.18",
          "form": "개가 사람한테 잘하더라구요",
          "original_form": "개가 사람한테 잘하더라구요",
          "speaker_id": "1",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100009010.1.19",
          "form": "맞아요 어떻게 이렇게 나를 좋아할까 싶어요",
          "original_form": "맞아요 어떻게 이렇게 나를 좋아할까 싶어요",
          "speaker_id": "2",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100009010.1.20",
          "form": "예전에 과외하던 학생 집에 개가 있었는데",
          "original_form": "예전에 과외하던 학생 집에 개가 있었는데",
          "speaker_id": "1",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100009010.1.21",
          "form": "진짜 예뻤어요",
          "original_form": "진짜 예뻤어요",
          "speaker_id": "1",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100009010.1.22",
          "form": "어릴때 키우던 강아지는 하교할때면 어떻게 알고오는지 차다니는 골목까지 와서 반겨주고 그랬어요",
          "original_form": "어릴때 키우던 강아지는 하교할때면 어떻게 알고오는지 차다니는 골목까지 와서 반겨주고 그랬어요",
          "speaker_id": "2",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100009010.1.23",
          "form": "신기하네요 냄새를 맡고 오는걸까요",
          "original_form": "신기하네요 냄새를 맡고 오는걸까요",
          "speaker_id": "1",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100009010.1.24",
          "form": "엄마 아시는 분이 고양이를 키우다가 버렸는데요",
          "original_form": "엄마 아시는 분이 고양이를 키우다가 버렸는데요",
          "speaker_id": "1",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100009010.1.25",
          "form": "멀리 가서 버렸는데 집을 찾아왔다고 하더라구요",
          "original_form": "멀리 가서 버렸는데 집을 찾아왔다고 하더라구요",
          "speaker_id": "1",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100009010.1.26",
          "form": "동물들이 능력이 좋은가봐요",
          "original_form": "동물들이 능력이 좋은가봐요",
          "speaker_id": "1",
          "time": "20210822 20:44"
        }
      ]
    }
