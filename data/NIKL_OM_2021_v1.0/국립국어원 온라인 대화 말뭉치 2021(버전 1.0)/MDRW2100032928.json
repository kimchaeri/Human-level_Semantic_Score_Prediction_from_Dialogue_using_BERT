        {
          "id": "MDRW2100032928.1.1",
          "form": "어느 지역인가요",
          "original_form": "어느 지역인가요",
          "speaker_id": "2",
          "time": "20210906 08:26"
        },
        {
          "id": "MDRW2100032928.1.2",
          "form": "제가사는지역요",
