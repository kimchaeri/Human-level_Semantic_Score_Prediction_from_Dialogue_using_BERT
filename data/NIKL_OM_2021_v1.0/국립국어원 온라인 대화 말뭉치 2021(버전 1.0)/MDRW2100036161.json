      "utterance": [
        {
          "id": "MDRW2100036161.1.1",
          "form": "홍콩도 가보고싶네요",
          "original_form": "홍콩도 가보고싶네요",
          "speaker_id": "1",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100036161.1.2",
          "form": "친구들이 그렇게 추천을 많이 해요",
          "original_form": "친구들이 그렇게 추천을 많이 해요",
          "speaker_id": "1",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100036161.1.3",
          "form": "오오 역시",
          "original_form": "오오 역시",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100036161.1.4",
          "form": "홍콩에는 디즈니랜드도 있잖아요!",
          "original_form": "홍콩에는 디즈니랜드도 있잖아요!",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100036161.1.5",
          "form": "디저트도 많고...",
          "original_form": "디저트도 많고...",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036161.1.6",
          "form": "디즈니 랜드!! 꼭 가보고싶어요ㅠㅠ",
          "original_form": "디즈니 랜드!! 꼭 가보고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036161.1.7",
          "form": "제주위사람들은 홍콩을 한번만가지않더라구요",
          "original_form": "제주위사람들은 홍콩을 한번만가지않더라구요",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036161.1.8",
          "form": "꼭 두번세번더가요",
          "original_form": "꼭 두번세번더가요",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036161.1.9",
          "form": "와 그 정도로 좋다는 뜻이겠죠???",
          "original_form": "와 그 정도로 좋다는 뜻이겠죠???",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036161.1.10",
          "form": "너무너무 궁금해지네요...",
          "original_form": "너무너무 궁금해지네요...",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036161.1.11",
          "form": "홍콩 사람들이 좀 불친절한 것도 있다던데",
          "original_form": "홍콩 사람들이 좀 불친절한 것도 있다던데",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036161.1.12",
          "form": "name1님 친구분들은 뭐라고 하시던가요???",
          "original_form": "&name1&님 친구분들은 뭐라고 하시던가요???",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036161.1.13",
          "form": "홍콩사람들에 대한 이야기는",
          "original_form": "홍콩사람들에 대한 이야기는",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036161.1.14",
          "form": "정장쫙빼입은 남성분이",
          "original_form": "정장쫙빼입은 남성분이",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036161.1.15",
          "form": "시크하게 도로에서 구해준 이야기밖에 못들었어요",
          "original_form": "시크하게 도로에서 구해준 이야기밖에 못들었어요",
          "speaker_id": "1",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036161.1.16",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036161.1.17",
          "form": "아니 ㅋㅋㅋㅋㅋㅋㅋ 세상에",
          "original_form": "아니 ㅋㅋㅋㅋㅋㅋㅋ 세상에",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036161.1.18",
          "form": "홍콩은 어쩜 그런 에피소드도 굉장히",
          "original_form": "홍콩은 어쩜 그런 에피소드도 굉장히",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036161.1.19",
          "form": "영화 같네요....",
          "original_form": "영화 같네요....",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036161.1.20",
          "form": "딴데보다가 차에 치일뻔했는데",
          "original_form": "딴데보다가 차에 치일뻔했는데",
          "speaker_id": "1",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036161.1.21",
          "form": "평생 잊지 못할 기억이었겠는데요?!",
          "original_form": "평생 잊지 못할 기억이었겠는데요?!",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036161.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036161.1.23",
          "form": "되게 시크하게 잡아주셨대요",
          "original_form": "되게 시크하게 잡아주셨대요",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036161.1.24",
          "form": "그러고 그냥 가시더라는 소리만 들었어요",
          "original_form": "그러고 그냥 가시더라는 소리만 들었어요",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036161.1.25",
          "form": "세상에 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "세상에 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036161.1.26",
          "form": "와... .",
          "original_form": "와... .",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036161.1.27",
          "form": "진짜 영화같네요....",
          "original_form": "진짜 영화같네요....",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036161.1.28",
          "form": "아니 생각해보니",
          "original_form": "아니 생각해보니",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036161.1.29",
          "form": "제 친구 중에 지금 독일에서 유학하는 애가 있는데",
          "original_form": "제 친구 중에 지금 독일에서 유학하는 애가 있는데",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036161.1.30",
          "form": "걔 남자친구가 홍콩사람이네요",
          "original_form": "걔 남자친구가 홍콩사람이네요",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036161.1.31",
          "form": "근데 홍콩남자가 여자한테 잘해준대요",
          "original_form": "근데 홍콩남자가 여자한테 잘해준대요",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036161.1.32",
          "form": "우와 글로벌해...",
          "original_form": "우와 글로벌해...",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036161.1.33",
          "form": "한국에 비하면...?!",
          "original_form": "한국에 비하면...?!",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036161.1.34",
          "form": "그춐ㅋㅋㅋㅋ",
          "original_form": "그춐ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100036161.1.35",
          "form": "전에 영상통화 하는데",
          "original_form": "전에 영상통화 하는데",
          "speaker_id": "2",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100036161.1.36",
          "form": "옆에 누가 숨어있어서 물어봤더니",
          "original_form": "옆에 누가 숨어있어서 물어봤더니",
          "speaker_id": "2",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100036161.1.37",
          "form": "그 홍콩 남자친구더라구요,,",
          "original_form": "그 홍콩 남자친구더라구요,,",
          "speaker_id": "2",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100036161.1.38",
          "form": "독일로 유학간 한국친구의 홍콩인 남자친구",
          "original_form": "독일로 유학간 한국친구의 홍콩인 남자친구",
          "speaker_id": "1",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100036161.1.39",
          "form": "너무나 글로벌한 상황ㅋㅋㅋㅋㅋ",
          "original_form": "너무나 글로벌한 상황ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100036161.1.40",
          "form": "멀리서봐서 그런가 새롭네요",
          "original_form": "멀리서봐서 그런가 새롭네요",
          "speaker_id": "1",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036161.1.41",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036161.1.42",
          "form": "맞아요 그럴 수 있겠어요!!!!!",
          "original_form": "맞아요 그럴 수 있겠어요!!!!!",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036161.1.43",
          "form": "근데 저도 해외경험이 별로 없어서",
          "original_form": "근데 저도 해외경험이 별로 없어서",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036161.1.44",
          "form": "수학여행으로 일본 다녀온 게 전부거든요ㅠ",
          "original_form": "수학여행으로 일본 다녀온 게 전부거든요ㅠ",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036161.1.45",
          "form": "비행기 좀 타보고싶은데...",
          "original_form": "비행기 좀 타보고싶은데...",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036161.1.46",
          "form": "기회가 없네요ㅠ 코로나 때문에 더더욱 ...",
          "original_form": "기회가 없네요ㅠ 코로나 때문에 더더욱 ...",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036161.1.47",
          "form": "그죠....코로나 언제끝나요...",
          "original_form": "그죠....코로나 언제끝나요...",
          "speaker_id": "1",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100036161.1.48",
          "form": "그러게요 언제 끝나나 모르겠어요ㅠㅠ",
          "original_form": "그러게요 언제 끝나나 모르겠어요ㅠㅠ",
