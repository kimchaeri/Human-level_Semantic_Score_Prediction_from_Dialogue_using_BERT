      "utterance": [
        {
          "id": "MDRW2100029622.1.1",
          "form": "여행가고싶은 날씨네요",
          "original_form": "여행가고싶은 날씨네요",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029622.1.2",
          "form": "정말요.ㅡ",
          "original_form": "정말요.ㅡ",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029622.1.3",
          "form": "며칠동안 비만내리더니",
          "original_form": "며칠동안 비만내리더니",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029622.1.4",
          "form": "오늘은 화창하네여",
          "original_form": "오늘은 화창하네여",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029622.1.5",
          "form": "오랜만에 맑은하늘보는것같아요",
          "original_form": "오랜만에 맑은하늘보는것같아요",
          "speaker_id": "1",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029622.1.6",
          "form": "휴가때어디다녀오셨나요",
          "original_form": "휴가때어디다녀오셨나요",
          "speaker_id": "1",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029622.1.7",
          "form": "요본휴가는 방콕했어요 ㅠㅠ",
          "original_form": "요본휴가는 방콕했어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:06"
        },
        {
          "id": "MDRW2100029622.1.8",
          "form": "코로나더 심해져서",
          "original_form": "코로나더 심해져서",
          "speaker_id": "2",
          "time": "20210903 12:07"
        },
        {
          "id": "MDRW2100029622.1.9",
          "form": "어우 저도요 ㅠㅠ 그냥 본가에서 방콕",
          "original_form": "어우 저도요 ㅠㅠ 그냥 본가에서 방콕",
          "speaker_id": "1",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029622.1.10",
          "form": "울릉도가고싶었는데",
          "original_form": "울릉도가고싶었는데",
          "speaker_id": "1",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029622.1.11",
          "form": "가까운곳이라도 가고싶은데...어렵네여 ㅠㅠ",
          "original_form": "가까운곳이라도 가고싶은데...어렵네여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:13"
        },
        {
          "id": "MDRW2100029622.1.12",
          "form": "옛날에 여행좀 많이다닐걸 그랬어여 ㅠ",
          "original_form": "옛날에 여행좀 많이다닐걸 그랬어여 ㅠ",
          "speaker_id": "2",
          "time": "20210903 12:13"
        },
        {
          "id": "MDRW2100029622.1.13",
          "form": "그러게요 ㅜㅜ 더 힘내서다닐걸",
          "original_form": "그러게요 ㅜㅜ 더 힘내서다닐걸",
          "speaker_id": "1",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029622.1.14",
          "form": "괜찮아자면 어디부터가실건가요",
          "original_form": "괜찮아자면 어디부터가실건가요",
          "speaker_id": "1",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029622.1.15",
          "form": "해외여행 함 가보고싶은데...",
          "original_form": "해외여행 함 가보고싶은데...",
          "speaker_id": "2",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100029622.1.16",
          "form": "제주도도 함가보고싶어여",
          "original_form": "제주도도 함가보고싶어여",
          "speaker_id": "2",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100029622.1.17",
          "form": "어렸을때라",
          "original_form": "어렸을때라",
          "speaker_id": "2",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100029622.1.18",
          "form": "기억이 잘안나서 ㄹㅎ",
          "original_form": "기억이 잘안나서 ㄹㅎ",
          "speaker_id": "2",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100029622.1.19",
          "form": "앗 해외는 안가보셨어요~?",
          "original_form": "앗 해외는 안가보셨어요~?",
          "speaker_id": "1",
          "time": "20210903 13:17"
        },
        {
          "id": "MDRW2100029622.1.20",
          "form": "저는 몇넌전에 제주도에서 2년정도살았어요",
          "original_form": "저는 몇넌전에 제주도에서 2년정도살았어요",
          "speaker_id": "1",
          "time": "20210903 13:17"
        },
        {
          "id": "MDRW2100029622.1.21",
          "form": "넹..시간이안되서 ㅠㅠ",
          "original_form": "넹..시간이안되서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029622.1.22",
          "form": "오 제주도에서 사셨었어욤??",
          "original_form": "오 제주도에서 사셨었어욤??",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029622.1.23",
          "form": "네 여행갔다가 눌러앉았어요 ㅋㅋㅋ",
          "original_form": "네 여행갔다가 눌러앉았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:29"
        },
        {
          "id": "MDRW2100029622.1.24",
          "form": "정말 좋았나봐요 ㅎㅎ",
          "original_form": "정말 좋았나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:47"
        },
        {
          "id": "MDRW2100029622.1.25",
          "form": "행복했죠..있는 돈 다 털었디만",
          "original_form": "행복했죠..있는 돈 다 털었디만",
          "speaker_id": "1",
          "time": "20210903 13:58"
        }
      ]
    }
