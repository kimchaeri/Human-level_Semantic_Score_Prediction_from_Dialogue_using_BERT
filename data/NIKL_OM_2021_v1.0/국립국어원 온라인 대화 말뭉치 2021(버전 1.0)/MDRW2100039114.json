        },
        {
          "id": "MDRW2100039114.1.2",
          "form": "name2님,,, 새학기이신가요?!",
          "original_form": "&name2&님,,, 새학기이신가요?!",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100039114.1.3",
          "form": "뭔가 장만하고픈 이 마음,,",
          "original_form": "뭔가 장만하고픈 이 마음,,",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100039114.1.4",
          "form": "전 직장인입니더...ㅎ",
          "original_form": "전 직장인입니더...ㅎ",
          "speaker_id": "2",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100039114.1.5",
          "form": "앗 저는 직장인은 아니지만",
          "original_form": "앗 저는 직장인은 아니지만",
          "speaker_id": "1",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100039114.1.6",
          "form": "학생과 사회인의 경계선에 있는... 대학원생이라",
          "original_form": "학생과 사회인의 경계선에 있는... 대학원생이라",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039114.1.7",
          "form": "name2님 고충도 잘 알 것 같아요!!!",
          "original_form": "&name2&님 고충도 잘 알 것 같아요!!!",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039114.1.8",
          "form": "가을 기념으로 뭐 장만하신거 있으신가요?",
          "original_form": "가을 기념으로 뭐 장만하신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039114.1.9",
          "form": "컴퓨터말곤없네요!!ㅋㅋ",
          "original_form": "컴퓨터말곤없네요!!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100039114.1.10",
          "form": "대학원샹 멋지네요",
          "original_form": "대학원샹 멋지네요",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100039114.1.11",
          "form": "하하 감사합니다!",
          "original_form": "하하 감사합니다!",
          "speaker_id": "1",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100039114.1.12",
          "form": "컴퓨터와 게임을 좋아하시는 name2님,,",
          "original_form": "컴퓨터와 게임을 좋아하시는 &name2&님,,",
          "speaker_id": "1",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100039114.1.13",
          "form": "맞어요 진짜 좋나해오 ㅋㅋㅋ",
          "original_form": "맞어요 진짜 좋나해오 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:08"
        },
        {
          "id": "MDRW2100039114.1.14",
          "form": "전에 별의 커비 게임도 하셨다 했죠?!",
          "original_form": "전에 별의 커비 게임도 하셨다 했죠?!",
          "speaker_id": "1",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100039114.1.15",
          "form": "아니요!!근데 컴터로 해본적은있어요",
          "original_form": "아니요!!근데 컴터로 해본적은있어요",
          "speaker_id": "2",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100039114.1.16",
          "form": "오오 컴퓨터 게임으로도 있군요?!?!?",
          "original_form": "오오 컴퓨터 게임으로도 있군요?!?!?",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100039114.1.17",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039114.1.18",
          "form": "하긴 플레이스테이션이나 닌텐도도",
          "original_form": "하긴 플레이스테이션이나 닌텐도도",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100039114.1.19",
          "form": "처음에는 컴퓨터 게임으로 시작했으려나요?!",
          "original_form": "처음에는 컴퓨터 게임으로 시작했으려나요?!",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
