      "utterance": [
        {
          "id": "MDRW2100010762.1.1",
          "form": "어제그립톡샀어요",
          "original_form": "어제그립톡샀어요",
          "speaker_id": "1",
          "time": "20210823 21:36"
        },
        {
          "id": "MDRW2100010762.1.2",
          "form": "오오 어떤 그립톡 사셨나요?",
          "original_form": "오오 어떤 그립톡 사셨나요?",
          "speaker_id": "2",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010762.1.3",
          "form": "그립톡이라고 회사이름이 그립톡인데",
          "original_form": "그립톡이라고 회사이름이 그립톡인데",
          "speaker_id": "1",
          "time": "20210823 21:40"
        },
        {
          "id": "MDRW2100010762.1.4",
          "form": "거기서 걍 알파벳적힌거샀어요",
          "original_form": "거기서 걍 알파벳적힌거샀어요",
          "speaker_id": "1",
          "time": "20210823 21:40"
        },
        {
          "id": "MDRW2100010762.1.5",
          "form": "아하 그렇군요!",
          "original_form": "아하 그렇군요!",
          "speaker_id": "2",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100010762.1.6",
          "form": "만원이라쪼매비싸요ㅠㅜ흑",
          "original_form": "만원이라쪼매비싸요ㅠㅜ흑",
          "speaker_id": "1",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010762.1.7",
          "form": "그렇네요ㅠㅠ 대신  그만큼 오래 예쁘게 쓰면 되죠!",
          "original_form": "그렇네요ㅠㅠ 대신  그만큼 오래 예쁘게 쓰면 되죠!",
          "speaker_id": "2",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010762.1.8",
          "form": "맞아요 저 한번쓰면 망가질때까지 써서ㅋㅋ",
          "original_form": "맞아요 저 한번쓰면 망가질때까지 써서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:43"
        },
        {
          "id": "MDRW2100010762.1.9",
          "form": "지난번것도3년썼어요",
          "original_form": "지난번것도3년썼어요",
          "speaker_id": "1",
          "time": "20210823 21:43"
        },
        {
          "id": "MDRW2100010762.1.10",
          "form": "우와 오래쓰셨네요!",
          "original_form": "우와 오래쓰셨네요!",
          "speaker_id": "2",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100010762.1.11",
          "form": "부서져도 순간접착제로 붙여서 꾸역꾸역썼죠..ㅋㅋㄱ",
          "original_form": "부서져도 순간접착제로 붙여서 꾸역꾸역썼죠..ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010762.1.12",
          "form": "대단해요!  한 물건을 오랬동안 쓰는건 좋은것 같아요",
          "original_form": "대단해요!  한 물건을 오랬동안 쓰는건 좋은것 같아요",
          "speaker_id": "2",
          "time": "20210823 21:56"
        },
        {
          "id": "MDRW2100010762.1.13",
          "form": "ㅋㅋ감사합니다 나름 환경보호도 한다는 마음으로 셀프 뿌듯함을 느끼는..ㅎㅎ",
          "original_form": "ㅋㅋ감사합니다 나름 환경보호도 한다는 마음으로 셀프 뿌듯함을 느끼는..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100010762.1.14",
          "form": "중고거래 해보셨어요?",
          "original_form": "중고거래 해보셨어요?",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010762.1.15",
          "form": "네 중고책이랑 당근으로 두번정도?",
          "original_form": "네 중고책이랑 당근으로 두번정도?",
          "speaker_id": "1",
          "time": "20210824 00:54"
        },
        {
          "id": "MDRW2100010762.1.16",
          "form": "아하 그렇군요  저도 당근으로 중고거래 해봤어요",
          "original_form": "아하 그렇군요  저도 당근으로 중고거래 해봤어요",
          "speaker_id": "2",
          "time": "20210824 07:47"
        },
        {
          "id": "MDRW2100010762.1.17",
          "form": "가격을 낮춰야 팔리더라고요",
          "original_form": "가격을 낮춰야 팔리더라고요",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
