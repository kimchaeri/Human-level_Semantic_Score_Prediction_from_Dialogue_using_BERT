      "utterance": [
        {
          "id": "MDRW2100033142.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100033142.1.2",
          "form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "original_form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100033142.1.3",
          "form": "가을이오면 어느 것이 가장 기대되시나요?",
          "original_form": "가을이오면 어느 것이 가장 기대되시나요?",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100033142.1.4",
          "form": "안녕하세요~~ 저는 높고 파란 하늘이요",
          "original_form": "안녕하세요~~ 저는 높고 파란 하늘이요",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100033142.1.5",
          "form": "후손님은요?",
          "original_form": "후손님은요?",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100033142.1.6",
          "form": "저는 먹거리가 기대됩니다",
          "original_form": "저는 먹거리가 기대됩니다",
          "speaker_id": "1",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100033142.1.7",
          "form": "특히 감을 좋아하지요",
          "original_form": "특히 감을 좋아하지요",
          "speaker_id": "1",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100033142.1.8",
          "form": "헉 그러네요 감이 나오네요",
          "original_form": "헉 그러네요 감이 나오네요",
          "speaker_id": "2",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100033142.1.9",
          "form": "어떤감 좋아하세요?",
          "original_form": "어떤감 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100033142.1.10",
          "form": "단감? 홍시? 곶감?",
          "original_form": "단감? 홍시? 곶감?",
          "speaker_id": "2",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100033142.1.11",
          "form": "다 좋아합니다",
          "original_form": "다 좋아합니다",
          "speaker_id": "1",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100033142.1.12",
          "form": "그래서 즐거워요",
          "original_form": "그래서 즐거워요",
          "speaker_id": "1",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100033142.1.13",
          "form": "종류별로 먹을 수 있어서 말이지요",
          "original_form": "종류별로 먹을 수 있어서 말이지요",
          "speaker_id": "1",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100033142.1.14",
          "form": "공평하시네요ㅋㅋㅋ",
          "original_form": "공평하시네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100033142.1.15",
          "form": "전 단감이 좋아요",
          "original_form": "전 단감이 좋아요",
          "speaker_id": "2",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100033142.1.16",
          "form": "그렇군요 단감이 곧 나오겠죠",
          "original_form": "그렇군요 단감이 곧 나오겠죠",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100033142.1.17",
          "form": "요새 과일은 하우스 때문에 계절이 없다하지만",
          "original_form": "요새 과일은 하우스 때문에 계절이 없다하지만",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100033142.1.18",
          "form": "그래도 제철에 먹는 과일이 가장 맛있더군요",
          "original_form": "그래도 제철에 먹는 과일이 가장 맛있더군요",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100033142.1.19",
          "form": "가을에 또 나오는게 뭐가 있죠?밤?",
          "original_form": "가을에 또 나오는게 뭐가 있죠?밤?",
          "speaker_id": "2",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100033142.1.20",
          "form": "가을은 수확의 계절이라 과일들 많이 나오죠",
          "original_form": "가을은 수확의 계절이라 과일들 많이 나오죠",
          "speaker_id": "1",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100033142.1.21",
          "form": "사과 배 같은 거요",
          "original_form": "사과 배 같은 거요",
          "speaker_id": "1",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100033142.1.22",
          "form": "요새 사철 과일들이 나와서 이제 계절의 느낌이 안나지만 그래도 원래 과일은 가을에 많이 열리니까요",
          "original_form": "요새 사철 과일들이 나와서 이제 계절의 느낌이 안나지만 그래도 원래 과일은 가을에 많이 열리니까요",
          "speaker_id": "1",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100033142.1.23",
          "form": "그렇군요 예전에는 알았던 것 같은데 이제는 계절과 상관없이 나오니까 아예 잊고 지냈어요 덕분에 알아가네요 ㅎㅎㅎ",
          "original_form": "그렇군요 예전에는 알았던 것 같은데 이제는 계절과 상관없이 나오니까 아예 잊고 지냈어요 덕분에 알아가네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100033142.1.24",
          "form": "맞아요 요즘 하우스 재배가 많아져서",
          "original_form": "맞아요 요즘 하우스 재배가 많아져서",
          "speaker_id": "1",
          "time": "20210907 05:39"
        },
        {
          "id": "MDRW2100033142.1.25",
          "form": "또 저장 기술들을 발전해서",
          "original_form": "또 저장 기술들을 발전해서",
          "speaker_id": "1",
          "time": "20210907 05:39"
        },
        {
          "id": "MDRW2100033142.1.26",
          "form": "거의 4철 아무때나 과일을 먹을 수 있지요",
          "original_form": "거의 4철 아무때나 과일을 먹을 수 있지요",
          "speaker_id": "1",
          "time": "20210907 05:39"
        },
        {
          "id": "MDRW2100033142.1.27",
          "form": "네네 좋아하시는 과일 있으세요? 가을 제철과일 말구요",
          "original_form": "네네 좋아하시는 과일 있으세요? 가을 제철과일 말구요",
          "speaker_id": "2",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100033142.1.28",
          "form": "저는 수박을 좋아해서 여름이 기다려집니다",
          "original_form": "저는 수박을 좋아해서 여름이 기다려집니다",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100033142.1.29",
          "form": "수박을 가장 좋아하지요",
          "original_form": "수박을 가장 좋아하지요",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100033142.1.30",
          "form": "수박 달고 시원한게 머리까지 맑아지는 기분이에요ㅋㅋㅋ 여름에 땀뻘뻘흘리고 찬물로 씻자마자 나와서 수박 먹는게 제일 좋아요 ㅋㅋㅋ",
          "original_form": "수박 달고 시원한게 머리까지 맑아지는 기분이에요ㅋㅋㅋ 여름에 땀뻘뻘흘리고 찬물로 씻자마자 나와서 수박 먹는게 제일 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100033142.1.31",
          "form": "어떤때 먹는 수박이 제일 좋으세요?",
          "original_form": "어떤때 먹는 수박이 제일 좋으세요?",
          "speaker_id": "2",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100033142.1.32",
          "form": "저는 시원한 수박이면 항상 좋습니다",
          "original_form": "저는 시원한 수박이면 항상 좋습니다",
          "speaker_id": "1",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100033142.1.33",
          "form": "계곡에서 깨먹는 수박 맛있지만 작년부터는 계곡에 물놀이를 못가서요",
          "original_form": "계곡에서 깨먹는 수박 맛있지만 작년부터는 계곡에 물놀이를 못가서요",
          "speaker_id": "1",
          "time": "20210907 13:15"
        }
      ]
