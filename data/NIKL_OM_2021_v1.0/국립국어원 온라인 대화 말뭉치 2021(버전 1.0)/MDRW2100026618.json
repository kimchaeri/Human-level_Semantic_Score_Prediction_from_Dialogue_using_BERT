      "utterance": [
        {
          "id": "MDRW2100026618.1.1",
          "form": "최근에 사신거있나요?",
          "original_form": "최근에 사신거있나요?",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026618.1.2",
          "form": "저 요즘엔 에센스 생리대 뭐 이런거 샀어요",
          "original_form": "저 요즘엔 에센스 생리대 뭐 이런거 샀어요",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026618.1.3",
          "form": "아 필수품이죠",
          "original_form": "아 필수품이죠",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026618.1.4",
          "form": "콩에센스라고 롤링해서 피지 뺐는데 괜찮더라고요 보들보들",
          "original_form": "콩에센스라고 롤링해서 피지 뺐는데 괜찮더라고요 보들보들",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026618.1.5",
          "form": "오 신기하네요",
          "original_form": "오 신기하네요",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026618.1.6",
          "form": "주로 어디서 쇼핑하세요?",
          "original_form": "주로 어디서 쇼핑하세요?",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026618.1.7",
          "form": "생필품은 십일번가에서 많이 사요",
          "original_form": "생필품은 십일번가에서 많이 사요",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026618.1.8",
          "form": "역시 인쇼가 제일 싸고 좋죠 ㅎㅎ",
          "original_form": "역시 인쇼가 제일 싸고 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026618.1.9",
          "form": "저도 거의 인터넷에서 물건사요",
          "original_form": "저도 거의 인터넷에서 물건사요",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026618.1.10",
          "form": "요즘에 뭐 구입하셨어요??",
          "original_form": "요즘에 뭐 구입하셨어요??",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026618.1.11",
          "form": "최근에 닭강정이요 ㅋㅋㄱㅋ",
          "original_form": "최근에 닭강정이요 ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026618.1.12",
          "form": "오 무슨 닭강정사셨어요??",
          "original_form": "오 무슨 닭강정사셨어요??",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026618.1.13",
          "form": "갑자기 생각나는건 만석...",
          "original_form": "갑자기 생각나는건 만석...",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026618.1.14",
          "form": "그냥 동네에 있는 집이예요",
          "original_form": "그냥 동네에 있는 집이예요",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026618.1.15",
          "form": "만석 먹고싶네요ㅜ",
          "original_form": "만석 먹고싶네요ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026618.1.16",
          "form": "그거 배달도 많이 되더라고요 롯데백화점같은데에서 자주 팔기도 하고",
          "original_form": "그거 배달도 많이 되더라고요 롯데백화점같은데에서 자주 팔기도 하고",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026618.1.17",
          "form": "맞아요 롯백갈때 지하에서 파는거 봤어요",
          "original_form": "맞아요 롯백갈때 지하에서 파는거 봤어요",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026618.1.18",
          "form": "종종시켜드세요?",
          "original_form": "종종시켜드세요?",
          "speaker_id": "1",
          "time": "20210901 23:22"
        }
      ]
