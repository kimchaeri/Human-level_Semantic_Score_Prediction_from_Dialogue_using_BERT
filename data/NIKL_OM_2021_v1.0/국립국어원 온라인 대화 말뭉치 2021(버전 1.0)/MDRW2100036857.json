      "utterance": [
        {
          "id": "MDRW2100036857.1.1",
          "form": "배민에서 배달 음식 시켜 드세요?",
          "original_form": "배민에서 배달 음식 시켜 드세요?",
          "speaker_id": "1",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100036857.1.2",
          "form": "네 종종 시켜먹어요",
          "original_form": "네 종종 시켜먹어요",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100036857.1.3",
          "form": "배민 리뷰 많이 참고하시는 편이세요?",
          "original_form": "배민 리뷰 많이 참고하시는 편이세요?",
          "speaker_id": "1",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100036857.1.4",
          "form": "넵!! 저는 리뷰 꼭 봐요",
          "original_form": "넵!! 저는 리뷰 꼭 봐요",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100036857.1.5",
          "form": "참고하세요?",
          "original_form": "참고하세요?",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100036857.1.6",
          "form": "네 저도 꼼꼼히 읽어보는 편이에요",
          "original_form": "네 저도 꼼꼼히 읽어보는 편이에요",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100036857.1.7",
          "form": "리뷰문제로 말이 많긴 하지만... 제발 안없어졌으면 좋겠어요",
          "original_form": "리뷰문제로 말이 많긴 하지만... 제발 안없어졌으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100036857.1.8",
          "form": "위생이나 맛 같은걸 알 수 있는건 리뷰 뿐이잖아요ㅠ",
          "original_form": "위생이나 맛 같은걸 알 수 있는건 리뷰 뿐이잖아요ㅠ",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100036857.1.9",
          "form": "네 읽다보면 진짜 맛있어서 쓴집이랑 영혼빠진 집 티가 나요",
          "original_form": "네 읽다보면 진짜 맛있어서 쓴집이랑 영혼빠진 집 티가 나요",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100036857.1.10",
          "form": "리뷰음식먹고쓴 리뷰랑요",
          "original_form": "리뷰음식먹고쓴 리뷰랑요",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100036857.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100036857.1.12",
          "form": "글고 리뷰에서 인성보이는건 사장도 마찬가지라..",
          "original_form": "글고 리뷰에서 인성보이는건 사장도 마찬가지라..",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100036857.1.13",
          "form": "손님 갑질도 많지만 사장들이 이상한 곳들도 리뷰에서 걸리잖아요",
          "original_form": "손님 갑질도 많지만 사장들이 이상한 곳들도 리뷰에서 걸리잖아요",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100036857.1.14",
          "form": "그죠 수정해서 잘했음 좋겠어요",
          "original_form": "그죠 수정해서 잘했음 좋겠어요",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100036857.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100036857.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100036857.1.17",
          "form": "리뷰는 계속 사용했으면 좋겠지만",
          "original_form": "리뷰는 계속 사용했으면 좋겠지만",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100036857.1.18",
          "form": "개선이 필요한건 분명해요",
          "original_form": "개선이 필요한건 분명해요",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100036857.1.19",
          "form": "저도 그리 생각합니다",
          "original_form": "저도 그리 생각합니다",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100036857.1.20",
          "form": "백퍼동의해요!",
          "original_form": "백퍼동의해요!",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100036857.1.21",
          "form": "어떤거 자주 시켜드세요?",
          "original_form": "어떤거 자주 시켜드세요?",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100036857.1.22",
          "form": "피자자주먹어요",
          "original_form": "피자자주먹어요",
