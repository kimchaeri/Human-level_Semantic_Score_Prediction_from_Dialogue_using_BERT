        {
          "id": "MDRW2100027297.1.1",
          "form": "사탕 좋아하나요",
          "original_form": "사탕 좋아하나요",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027297.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027297.1.3",
          "form": "젤리는요",
          "original_form": "젤리는요",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027297.1.4",
          "form": "싫어요",
          "original_form": "싫어요",
          "speaker_id": "1",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027297.1.5",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027297.1.6",
          "form": "선호하지는 않아요",
          "original_form": "선호하지는 않아요",
          "speaker_id": "1",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027297.1.7",
          "form": "연극이네",
          "original_form": "연극이네",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027297.1.8",
          "form": "캣츠보셨나요",
          "original_form": "캣츠보셨나요",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027297.1.9",
          "form": "아뇨ㅜㅜ",
          "original_form": "아뇨ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027297.1.10",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027297.1.11",
          "form": "못봤어요",
          "original_form": "못봤어요",
          "speaker_id": "2",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027297.1.12",
          "form": "뮤지컬 온라인으로 봤는데",
          "original_form": "뮤지컬 온라인으로 봤는데",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027297.1.13",
          "form": "보러가고싶어요",
          "original_form": "보러가고싶어요",
          "speaker_id": "2",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027297.1.14",
          "form": "산만하더라구요",
          "original_form": "산만하더라구요",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027297.1.15",
          "form": "레베카보고 싶어요",
          "original_form": "레베카보고 싶어요",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027297.1.16",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027297.1.17",
          "form": "못봤어요",
          "original_form": "못봤어요",
          "speaker_id": "2",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027297.1.18",
          "form": "노래만 들어봐서ㅋ",
          "original_form": "노래만 들어봐서ㅋ",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027297.1.19",
          "form": "노래는 너튜브로 봤어요",
          "original_form": "노래는 너튜브로 봤어요",
          "speaker_id": "2",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027297.1.20",
          "form": "미니티켓은 왜 뮤지컬은 안될까요",
          "original_form": "미니티켓은 왜 뮤지컬은 안될까요",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027297.1.21",
          "form": "공연으로 만족해야하나봐요",
          "original_form": "공연으로 만족해야하나봐요",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027297.1.22",
          "form": "계약을 못따오ㅏ서 그런걸까요",
          "original_form": "계약을 못따오ㅏ서 그런걸까요",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027297.1.23",
          "form": "연극말고 뮤지컬도 보고싶어요",
          "original_form": "연극말고 뮤지컬도 보고싶어요",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027297.1.24",
          "form": "전에 재즈 공연 봤는데",
          "original_form": "전에 재즈 공연 봤는데",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027297.1.25",
          "form": "좋더라구요",
          "original_form": "좋더라구요",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027297.1.26",
          "form": "연극도  많이 안올라와서 아쉽..",
          "original_form": "연극도  많이 안올라와서 아쉽..",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027297.1.27",
          "form": "뭐보셨어요?",
          "original_form": "뭐보셨어요?",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027297.1.28",
          "form": "했던거만해서",
          "original_form": "했던거만해서",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027297.1.29",
          "form": "이름을 모르겠어요",
