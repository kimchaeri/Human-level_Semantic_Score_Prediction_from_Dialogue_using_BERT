      "utterance": [
        {
          "id": "MDRW2100034430.1.1",
          "form": "모가디슈 봤어요",
          "original_form": "모가디슈 봤어요",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100034430.1.2",
          "form": "모가디슈 재밋다고 하던데!!",
          "original_form": "모가디슈 재밋다고 하던데!!",
          "speaker_id": "2",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100034430.1.3",
          "form": "네 또 아프간에서 최근에 미라클작전 한 얘기 보니까 더 실감이 나더라구요",
          "original_form": "네 또 아프간에서 최근에 미라클작전 한 얘기 보니까 더 실감이 나더라구요",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100034430.1.4",
          "form": "맞아요 진짜 영화보다 더한 현실 ㅠㅠ",
          "original_form": "맞아요 진짜 영화보다 더한 현실 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100034430.1.5",
          "form": "그니까요 모가디슈도 실화바탕이라곤 해도 실제 사건이 더 영화같았고 ㅋㅋ",
          "original_form": "그니까요 모가디슈도 실화바탕이라곤 해도 실제 사건이 더 영화같았고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100034430.1.6",
          "form": "영화 개봉 타이밍도 진짜 대박이엇던것같아요 모가디슈는",
          "original_form": "영화 개봉 타이밍도 진짜 대박이엇던것같아요 모가디슈는",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100034430.1.7",
          "form": "그러게요 진짜 다들 한번 더 관심갖게 되고 ㅋㅋㅋ",
          "original_form": "그러게요 진짜 다들 한번 더 관심갖게 되고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100034430.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ전 마블좋아하는데 블랙위도우 아직 못봐서 슬픔",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전 마블좋아하는데 블랙위도우 아직 못봐서 슬픔",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100034430.1.9",
          "form": "아 저도여 보러가야지 생각만하고있다가 마음먹으니 내려갔더라구요 ㅠㅠ",
          "original_form": "아 저도여 보러가야지 생각만하고있다가 마음먹으니 내려갔더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100034430.1.10",
          "form": "name1님도 마블 좋아하세요?",
          "original_form": "&name1&님도 마블 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100034430.1.11",
          "form": "네 정주행 했어요ㅎㅎ",
          "original_form": "네 정주행 했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100034430.1.12",
          "form": "누굴 제일 좋아하세요?",
          "original_form": "누굴 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100034430.1.13",
          "form": "전 사실 아이언맨이랑 토르 젤 좋아했는데",
          "original_form": "전 사실 아이언맨이랑 토르 젤 좋아했는데",
          "speaker_id": "1",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100034430.1.14",
          "form": "아이언맨 은퇴했으니 스파이더맨으로 ㅋㅋㅋ",
          "original_form": "아이언맨 은퇴했으니 스파이더맨으로 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100034430.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ아이언맨이랑 토르 좋죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아이언맨이랑 토르 좋죠",
          "speaker_id": "2",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100034430.1.16",
          "form": "저는 가오갤 시리즈를 제일 좋아해요",
          "original_form": "저는 가오갤 시리즈를 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100034430.1.17",
          "form": "아 맞다 가오갤도 진짜 재밌었어요",
          "original_form": "아 맞다 가오갤도 진짜 재밌었어요",
          "speaker_id": "1",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100034430.1.18",
          "form": "가오갤이랑 토르 좋아하는데",
          "original_form": "가오갤이랑 토르 좋아하는데",
          "speaker_id": "2",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100034430.1.19",
          "form": "매력있어요ㅋㅋㅋㅋ 음악도 좋고",
          "original_form": "매력있어요ㅋㅋㅋㅋ 음악도 좋고",
          "speaker_id": "1",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100034430.1.20",
          "form": "둘다 어벤져스에서 너무 ㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "original_form": "둘다 어벤져스에서 너무 ㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100034430.1.21",
          "form": "너무 범 지구적이라 ㅋㅋㅋㅋㅋ",
          "original_form": "너무 범 지구적이라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100034430.1.22",
          "form": "망가져서 ㅋㅋㅋㅋㅋㅋ슬펏어요",
          "original_form": "망가져서 ㅋㅋㅋㅋㅋㅋ슬펏어요",
          "speaker_id": "2",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100034430.1.23",
          "form": "너무 백치미있곸ㅋㅋㅋ",
          "original_form": "너무 백치미있곸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100034430.1.24",
          "form": "새로운 시리즈 얼른 나왔으면 하네요",
          "original_form": "새로운 시리즈 얼른 나왔으면 하네요",
          "speaker_id": "1",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100034430.1.25",
          "form": "토르 라그나로크에서 토르가 얼마나 멋잇엇는데 ㅠㅠ",
          "original_form": "토르 라그나로크에서 토르가 얼마나 멋잇엇는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100034430.1.26",
          "form": "맞아요 새로운 시리즈 보고싶어요",
          "original_form": "맞아요 새로운 시리즈 보고싶어요",
          "speaker_id": "2",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100034430.1.27",
          "form": "스파이더맨도 좋아해요",
          "original_form": "스파이더맨도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100034430.1.28",
          "form": "스파이더맨 시리즈의 빌런 배우들도 좋아해서 더 좋앗던것같아요",
          "original_form": "스파이더맨 시리즈의 빌런 배우들도 좋아해서 더 좋앗던것같아요",
          "speaker_id": "2",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100034430.1.29",
          "form": "그니까요 다들 토르의 매력을 잘 몰라주더라구여ㅋㅋㅋㅋ",
          "original_form": "그니까요 다들 토르의 매력을 잘 몰라주더라구여ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:07"
        },
        {
          "id": "MDRW2100034430.1.30",
          "form": "스파이더맨은 워낙 인기 시리즈라 재미는 늘 보장되는 거 같아오 ㅋㅋㅋ",
          "original_form": "스파이더맨은 워낙 인기 시리즈라 재미는 늘 보장되는 거 같아오 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:08"
        },
        {
          "id": "MDRW2100034430.1.31",
          "form": "전 원래 토르에 그닥 감흥이 없다가",
          "original_form": "전 원래 토르에 그닥 감흥이 없다가",
          "speaker_id": "2",
          "time": "20210828 12:09"
        },
        {
          "id": "MDRW2100034430.1.32",
          "form": "라그나로크보고 제대로 빠졋엇거든요",
          "original_form": "라그나로크보고 제대로 빠졋엇거든요",
          "speaker_id": "2",
          "time": "20210828 12:09"
        },
        {
          "id": "MDRW2100034430.1.33",
          "form": "그런데 어벤져스에서 그렇게...ㅠㅠ",
          "original_form": "그런데 어벤져스에서 그렇게...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 12:10"
        },
        {
          "id": "MDRW2100034430.1.34",
          "form": "맞아요 워낙 인기캐릭터라 마블에서도 각잡고 만들어주는 느낌도 잇고",
          "original_form": "맞아요 워낙 인기캐릭터라 마블에서도 각잡고 만들어주는 느낌도 잇고",
          "speaker_id": "2",
          "time": "20210828 12:10"
        },
        {
          "id": "MDRW2100034430.1.35",
          "form": "ㅋㅋㅋㅋ어벤져스는 ㅠㅠㅋㅋㅋ 아이언맨 캡아를 위한 시리즈나 다름없어서 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ어벤져스는 ㅠㅠㅋㅋㅋ 아이언맨 캡아를 위한 시리즈나 다름없어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100034430.1.36",
          "form": "그런감이 없잖아 있었죠 ㅋㅋㅋㅋㅋ",
          "original_form": "그런감이 없잖아 있었죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100034430.1.37",
          "form": "그래도 어벤져스 시리즈도 좋아해요",
          "original_form": "그래도 어벤져스 시리즈도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100034430.1.38",
          "form": "맞아요 진짜 엔드게임 때 너무 아쉽고",
          "original_form": "맞아요 진짜 엔드게임 때 너무 아쉽고",
          "speaker_id": "1",
          "time": "20210828 12:21"
        },
        {
          "id": "MDRW2100034430.1.39",
          "form": "앞으로도 또 이런 시리즈 나올까 궁금하네요",
          "original_form": "앞으로도 또 이런 시리즈 나올까 궁금하네요",
          "speaker_id": "1",
          "time": "20210828 12:21"
