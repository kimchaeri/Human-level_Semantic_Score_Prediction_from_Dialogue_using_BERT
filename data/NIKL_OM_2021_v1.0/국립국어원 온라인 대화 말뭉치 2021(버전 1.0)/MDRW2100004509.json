      "utterance": [
        {
          "id": "MDRW2100004509.1.1",
          "form": "보니까 태풍 온다고 하더라고요",
          "original_form": "보니까 태풍 온다고 하더라고요",
          "speaker_id": "2",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004509.1.2",
          "form": "내 무서워요",
          "original_form": "내 무서워요",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004509.1.3",
          "form": "부산이나 경남쪽 지금 장난아니라고 하더라고요",
          "original_form": "부산이나 경남쪽 지금 장난아니라고 하더라고요",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004509.1.4",
          "form": "비바람 엄청 분다고 하던데",
          "original_form": "비바람 엄청 분다고 하던데",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004509.1.5",
          "form": "피해가 적어야 할텐데 말이죠",
          "original_form": "피해가 적어야 할텐데 말이죠",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004509.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004509.1.7",
          "form": "저는 경북지역이라 비만 조금 오고 말지만",
          "original_form": "저는 경북지역이라 비만 조금 오고 말지만",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004509.1.8",
          "form": "그쪽은 거의 직격탄을 맞을거라",
          "original_form": "그쪽은 거의 직격탄을 맞을거라",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004509.1.9",
          "form": "걱정되네요",
          "original_form": "걱정되네요",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004509.1.10",
          "form": "물폭탄 걱정이네요",
          "original_form": "물폭탄 걱정이네요",
          "speaker_id": "1",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004509.1.11",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004509.1.12",
          "form": "태풍오면",
          "original_form": "태풍오면",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004509.1.13",
          "form": "물폭탄 피해가 만만치 않을텐데",
          "original_form": "물폭탄 피해가 만만치 않을텐데",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004509.1.14",
          "form": "태풍이 왜 안오나 싶었네요 ㅠㅠ",
          "original_form": "태풍이 왜 안오나 싶었네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004509.1.15",
          "form": "안오면 좋겠다요",
          "original_form": "안오면 좋겠다요",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004509.1.16",
          "form": "안오면 좋죠 피해도 없으니",
          "original_form": "안오면 좋죠 피해도 없으니",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004509.1.17",
          "form": "ㅠㅠ 안타까울 따름입니다",
          "original_form": "ㅠㅠ 안타까울 따름입니다",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004509.1.18",
          "form": "네 모두에게 피해없이 지나갔으면",
          "original_form": "네 모두에게 피해없이 지나갔으면",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004509.1.19",
          "form": "그렇죠 부디 아무 피해 없길 ㅠㅠ",
          "original_form": "그렇죠 부디 아무 피해 없길 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004509.1.20",
          "form": "인명 피해는 특히더 그렇고요",
          "original_form": "인명 피해는 특히더 그렇고요",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004509.1.21",
          "form": "안전이  최우선",
          "original_form": "안전이  최우선",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004509.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004509.1.23",
          "form": "무조건 안전이 최우선",
          "original_form": "무조건 안전이 최우선",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004509.1.24",
          "form": "이런 태풍같은 기상이변엔 역시 안전을 우선으로 해야죠",
          "original_form": "이런 태풍같은 기상이변엔 역시 안전을 우선으로 해야죠",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004509.1.25",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210821 17:03"
        }
