      "utterance": [
        {
          "id": "MDRW2100007284.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007284.1.2",
          "form": "안녕하세여!",
          "original_form": "안녕하세여!",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007284.1.3",
          "form": "대학생은 곧 개강 시즌인데 name2님은 좀 어떠신가요?",
          "original_form": "대학생은 곧 개강 시즌인데 &name2&님은 좀 어떠신가요?",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007284.1.4",
          "form": "저도 개강 시즌이랍니다",
          "original_form": "저도 개강 시즌이랍니다",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007284.1.5",
          "form": "저번주에 수강신청 했어요",
          "original_form": "저번주에 수강신청 했어요",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007284.1.6",
          "form": "어떠셨나요?",
          "original_form": "어떠셨나요?",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007284.1.7",
          "form": "저는 다행이도 다 잡았어요ㅎㅎ!",
          "original_form": "저는 다행이도 다 잡았어요ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007284.1.8",
          "form": "저도 올클 했답니다!",
          "original_form": "저도 올클 했답니다!",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007284.1.9",
          "form": "와~! 이번에 그럼 몇학년이신지 여쭤봐도 될까요?",
          "original_form": "와~! 이번에 그럼 몇학년이신지 여쭤봐도 될까요?",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007284.1.10",
          "form": "전 이번년도에 3학년이에요~",
          "original_form": "전 이번년도에 3학년이에요~",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007284.1.11",
          "form": "ㅋㅋㅋㄱㅋ ㄱㄱㄱㄱ 그럼요~!",
          "original_form": "ㅋㅋㅋㄱㅋ ㄱㄱㄱㄱ 그럼요~!",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007284.1.12",
          "form": "저는 3학년 2학기예요",
          "original_form": "저는 3학년 2학기예요",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007284.1.13",
          "form": "지금은 인턴 나와 있답니다!",
          "original_form": "지금은 인턴 나와 있답니다!",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007284.1.14",
          "form": "몇 학번 이세요?",
          "original_form": "몇 학번 이세요?",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007284.1.15",
          "form": "어머낙!!!!!",
          "original_form": "어머낙!!!!!",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007284.1.16",
          "form": "저 19학번이에요!",
          "original_form": "저 19학번이에요!",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007284.1.17",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007284.1.18",
          "form": "저도 19학번 이랍니다!",
          "original_form": "저도 19학번 이랍니다!",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007284.1.19",
          "form": "엄머!!",
          "original_form": "엄머!!",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007284.1.20",
          "form": "와우!",
          "original_form": "와우!",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007284.1.21",
          "form": "휴학  한번 하셨었어요!",
          "original_form": "휴학  한번 하셨었어요!",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007284.1.22",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007284.1.23",
          "form": "아뇨! 저는 계속 런~ 하고 있어요. 어서 졸업하구 대학원이나 취업 할 생각이에요~",
          "original_form": "아뇨! 저는 계속 런~ 하고 있어요. 어서 졸업하구 대학원이나 취업 할 생각이에요~",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007284.1.24",
          "form": "대학원으로 가야하는 과예요?",
          "original_form": "대학원으로 가야하는 과예요?",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007284.1.25",
          "form": "설마 심리?",
          "original_form": "설마 심리?",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007284.1.26",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007284.1.27",
          "form": "맞아요... 심리학과!",
          "original_form": "맞아요... 심리학과!",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007284.1.28",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007284.1.29",
          "form": "저도요.......",
          "original_form": "저도요.......",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007284.1.30",
          "form": "저는 정확히는  산업심리예요",
          "original_form": "저는 정확히는  산업심리예요",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007284.1.31",
          "form": "엄머!!!!!!!!!!!",
          "original_form": "엄머!!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007284.1.32",
          "form": "저는 심리학과 인데 그 중에 아동심리쪽으로 가려고 진로 잡아뒀어요~",
          "original_form": "저는 심리학과 인데 그 중에 아동심리쪽으로 가려고 진로 잡아뒀어요~",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007284.1.33",
          "form": "우와 부러워요",
          "original_form": "우와 부러워요",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007284.1.34",
          "form": "저는 아직까지 진로를 못정해서",
          "original_form": "저는 아직까지 진로를 못정해서",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007284.1.35",
          "form": "정하려고 인턴을 나가고 있어요",
          "original_form": "정하려고 인턴을 나가고 있어요",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007284.1.36",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007284.1.37",
          "form": "그래도 산업 심리면 정말 전망 좋고 선택지도 넓네요~",
          "original_form": "그래도 산업 심리면 정말 전망 좋고 선택지도 넓네요~",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007284.1.38",
          "form": "대기업 붙으시길 기원해요",
          "original_form": "대기업 붙으시길 기원해요",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007284.1.39",
          "form": "하 .... 감사해요",
          "original_form": "하 .... 감사해요",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007284.1.40",
          "form": "심리가 재미있고 전망 좋다고 하지만 생각보다 쉽지 않네요",
          "original_form": "심리가 재미있고 전망 좋다고 하지만 생각보다 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007284.1.41",
          "form": "아동 심리는 재이있어요?",
          "original_form": "아동 심리는 재이있어요?",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007284.1.42",
          "form": "아동심리... 발달 배우는게 쉽지가 않은 것 같아요",
          "original_form": "아동심리... 발달 배우는게 쉽지가 않은 것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007284.1.43",
          "form": "좡왜와동 봉사 갈 땐 엄청 힘들어요ㅜㅜ",
          "original_form": "좡왜와동 봉사 갈 땐 엄청 힘들어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007284.1.44",
          "form": "아 실습도 나가 보셨어요?",
          "original_form": "아 실습도 나가 보셨어요?",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007284.1.45",
          "form": "종강 후에 따로 봉사잡아서 다녀왔어요!",
          "original_form": "종강 후에 따로 봉사잡아서 다녀왔어요!",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007284.1.46",
          "form": "봉사에서 무슨일 하셨어요?",
          "original_form": "봉사에서 무슨일 하셨어요?",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007284.1.47",
          "form": "여러 좡왜 가진 아이들하고 이런저런 활동했어요!",
          "original_form": "여러 좡왜 가진 아이들하고 이런저런 활동했어요!",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007284.1.48",
          "form": "공으로 색 맞추면서 색이름 말하기 같은 걸 했던 것 같아요",
          "original_form": "공으로 색 맞추면서 색이름 말하기 같은 걸 했던 것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:36"
        }
      ]
    }
