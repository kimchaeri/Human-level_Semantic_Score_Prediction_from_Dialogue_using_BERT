      "utterance": [
        {
          "id": "MDRW2100016041.1.1",
          "form": "크림치즈 어떤거 사요",
          "original_form": "크림치즈 어떤거 사요",
          "speaker_id": "1",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100016041.1.2",
          "form": "이름이 기억이 안나네요",
          "original_form": "이름이 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100016041.1.3",
          "form": "아.. 사드시나요?",
          "original_form": "아.. 사드시나요?",
          "speaker_id": "1",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100016041.1.4",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100016041.1.5",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100016041.1.6",
          "form": "전 돈이 없어서 거의 사먹지 못합니다",
          "original_form": "전 돈이 없어서 거의 사먹지 못합니다",
          "speaker_id": "2",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100016041.1.7",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100016041.1.8",
          "form": "필라델피아가 가장유명하긴해요",
          "original_form": "필라델피아가 가장유명하긴해요",
          "speaker_id": "1",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100016041.1.9",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100016041.1.10",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100016041.1.11",
          "form": "필라델피아 크림치즈같아요",
          "original_form": "필라델피아 크림치즈같아요",
          "speaker_id": "2",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016041.1.12",
          "form": "네 그거 맞아요",
          "original_form": "네 그거 맞아요",
          "speaker_id": "1",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016041.1.13",
          "form": "그때 50프로 했었죠",
          "original_form": "그때 50프로 했었죠",
          "speaker_id": "2",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016041.1.14",
          "form": "나도 알았으면 좋았을텐데",
          "original_form": "나도 알았으면 좋았을텐데",
          "speaker_id": "1",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016041.1.15",
          "form": "저 이마트에서 일하거든요",
          "original_form": "저 이마트에서 일하거든요",
          "speaker_id": "2",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016041.1.16",
          "form": "와인클럽을 이제야 알다니",
          "original_form": "와인클럽을 이제야 알다니",
          "speaker_id": "1",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016041.1.17",
          "form": "헉 이마트요?",
          "original_form": "헉 이마트요?",
          "speaker_id": "1",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016041.1.18",
          "form": "그래서 쓱데이때는 무조건 저렴하게 사요",
          "original_form": "그래서 쓱데이때는 무조건 저렴하게 사요",
          "speaker_id": "2",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016041.1.19",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016041.1.20",
          "form": "뭐가 가장 싸던가요",
          "original_form": "뭐가 가장 싸던가요",
          "speaker_id": "1",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016041.1.21",
          "form": "협력회사 직원이에요",
          "original_form": "협력회사 직원이에요",
          "speaker_id": "2",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016041.1.22",
          "form": "쓱데이는",
          "original_form": "쓱데이는",
          "speaker_id": "1",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016041.1.23",
          "form": "과자죠",
