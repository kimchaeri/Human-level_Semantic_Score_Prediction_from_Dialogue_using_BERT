      "utterance": [
        {
          "id": "MDRW2100008877.1.1",
          "form": "가족이 있니?",
          "original_form": "가족이 있니?",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008877.1.2",
          "form": "탕여나지",
          "original_form": "탕여나지",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008877.1.3",
          "form": "누가제일좋아",
          "original_form": "누가제일좋아",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008877.1.4",
          "form": "가족구성이 어케 되니",
          "original_form": "가족구성이 어케 되니",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008877.1.5",
          "form": "엄마 언니 나",
          "original_form": "엄마 언니 나",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008877.1.6",
          "form": "넌 누구랑 제일친해",
          "original_form": "넌 누구랑 제일친해",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008877.1.7",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008877.1.8",
          "form": "개웃겨",
          "original_form": "개웃겨",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008877.1.9",
          "form": "남동ㅊ",
          "original_form": "남동ㅊ",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008877.1.10",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008877.1.11",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008877.1.12",
          "form": "나중에 누구랑 살고싶니",
          "original_form": "나중에 누구랑 살고싶니",
          "speaker_id": "1",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008877.1.13",
          "form": "엄마랑 동생이랑은 친해",
          "original_form": "엄마랑 동생이랑은 친해",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008877.1.14",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008877.1.15",
          "form": "혼저살고싶음",
          "original_form": "혼저살고싶음",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008877.1.16",
          "form": "공용공간이 있는 동거정도는 괜찮은데",
          "original_form": "공용공간이 있는 동거정도는 괜찮은데",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008877.1.17",
          "form": "친구랑 사는것도?",
          "original_form": "친구랑 사는것도?",
          "speaker_id": "1",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008877.1.18",
          "form": "각자 공간 있고 주방 화장실만 공유하는 방식은 괜찮음",
          "original_form": "각자 공간 있고 주방 화장실만 공유하는 방식은 괜찮음",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008877.1.19",
          "form": "근데 웬만하면",
          "original_form": "근데 웬만하면",
          "speaker_id": "2",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008877.1.20",
          "form": "가까이 살고 혼자 사는게 낫지",
          "original_form": "가까이 살고 혼자 사는게 낫지",
          "speaker_id": "2",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008877.1.21",
          "form": "그건맞아",
          "original_form": "그건맞아",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008877.1.22",
          "form": "그치만 집안일혼자 가능?",
          "original_form": "그치만 집안일혼자 가능?",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008877.1.23",
          "form": "혼자 사는 정도면 가능이지",
          "original_form": "혼자 사는 정도면 가능이지",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008877.1.24",
          "form": "나 혼자 산다면",
          "original_form": "나 혼자 산다면",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008877.1.25",
          "form": "애들이랑 돈모아서",
          "original_form": "애들이랑 돈모아서",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008877.1.26",
          "form": "로봇청소기 해줘",
          "original_form": "로봇청소기 해줘",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008877.1.27",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008877.1.28",
          "form": "노력해볼게",
          "original_form": "노력해볼게",
          "speaker_id": "1",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008877.1.29",
          "form": "집안일 뭐가 제일싫어",
          "original_form": "집안일 뭐가 제일싫어",
          "speaker_id": "1",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008877.1.30",
          "form": "설거지,,",
          "original_form": "설거지,,",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008877.1.31",
          "form": "그래서 돈 많이 벌어서",
          "original_form": "그래서 돈 많이 벌어서",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008877.1.32",
          "form": "식세기 사야함",
          "original_form": "식세기 사야함",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008877.1.33",
          "form": "넌 뭐가 싫니",
          "original_form": "넌 뭐가 싫니",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008877.1.34",
          "form": "바닥청소",
          "original_form": "바닥청소",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008877.1.35",
          "form": "설거지는 그나마 뿌듯",
          "original_form": "설거지는 그나마 뿌듯",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008877.1.36",
          "form": "설거지 너무 허리아픔",
          "original_form": "설거지 너무 허리아픔",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008877.1.37",
          "form": "그 설거지할때 물 튀는거 방지해주는 가드 앎?",
          "original_form": "그 설거지할때 물 튀는거 방지해주는 가드 앎?",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008877.1.38",
          "form": "모름",
          "original_form": "모름",
          "speaker_id": "1",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008877.1.39",
          "form": "난 고무장갑 작은것좀 나왔으면",
          "original_form": "난 고무장갑 작은것좀 나왔으면",
          "speaker_id": "1",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008877.1.40",
          "form": "고무장갑 s도 커?",
          "original_form": "고무장갑 s도 커?",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008877.1.41",
          "form": "자취 시작하면",
          "original_form": "자취 시작하면",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008877.1.42",
          "form": "물 튀김 방지 가드 사줘야지 그거 개좋음",
          "original_form": "물 튀김 방지 가드 사줘야지 그거 개좋음",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008877.1.43",
          "form": "난 건조기 사줘",
          "original_form": "난 건조기 사줘",
          "speaker_id": "1",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008877.1.44",
          "form": "s짱 큼그래서 안끼고 함",
          "original_form": "s짱 큼그래서 안끼고 함",
          "speaker_id": "1",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008877.1.45",
          "form": "네...?",
          "original_form": "네...?",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008877.1.46",
          "form": "s가 크다고..?",
          "original_form": "s가 크다고..?",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008877.1.47",
