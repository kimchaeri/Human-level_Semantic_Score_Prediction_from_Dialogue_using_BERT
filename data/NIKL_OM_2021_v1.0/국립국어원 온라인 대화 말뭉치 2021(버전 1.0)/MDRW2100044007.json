      "utterance": [
        {
          "id": "MDRW2100044007.1.1",
          "form": "다 재우고 쉬나",
          "original_form": "다 재우고 쉬나",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044007.1.2",
          "form": "우리가족은다자",
          "original_form": "우리가족은다자",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044007.1.3",
          "form": "집안일끝?",
          "original_form": "집안일끝?",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044007.1.4",
          "form": "집안일도끝이야",
          "original_form": "집안일도끝이야",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044007.1.5",
          "form": "오 퇴근이네",
          "original_form": "오 퇴근이네",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044007.1.6",
          "form": "오늘은주방마감이다",
          "original_form": "오늘은주방마감이다",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044007.1.7",
          "form": "집에서 퇴근하고싶다",
          "original_form": "집에서 퇴근하고싶다",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044007.1.8",
          "form": "밥해먹이는건좋은데힘들다",
          "original_form": "밥해먹이는건좋은데힘들다",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044007.1.9",
          "form": "나는 언제 집안일퇴근하나",
          "original_form": "나는 언제 집안일퇴근하나",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044007.1.10",
          "form": "집에서쉬고싶다",
          "original_form": "집에서쉬고싶다",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044007.1.11",
          "form": "육아가끝이없다",
          "original_form": "육아가끝이없다",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044007.1.12",
          "form": "육아는종점이없지",
          "original_form": "육아는종점이없지",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044007.1.13",
          "form": "도움받고싶다",
