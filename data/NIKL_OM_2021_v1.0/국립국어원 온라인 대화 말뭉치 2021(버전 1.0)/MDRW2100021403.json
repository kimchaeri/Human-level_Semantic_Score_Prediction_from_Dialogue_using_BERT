      "utterance": [
        {
          "id": "MDRW2100021403.1.1",
          "form": "안녕하세요~ 독서 좋아하시나요?",
          "original_form": "안녕하세요~ 독서 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100021403.1.2",
          "form": "아뇨 잘 읽진 않아요ㅠ",
          "original_form": "아뇨 잘 읽진 않아요ㅠ",
          "speaker_id": "2",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100021403.1.3",
          "form": "독서 좋아하세요??",
          "original_form": "독서 좋아하세요??",
          "speaker_id": "2",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100021403.1.4",
          "form": "사실 저도...ㅎㅎ 그래도 시간 내서 읽으려고 하는 편이에요",
          "original_form": "사실 저도...ㅎㅎ 그래도 시간 내서 읽으려고 하는 편이에요",
          "speaker_id": "1",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100021403.1.5",
          "form": "어릴 때 더 잘 읽었던 거 같아요 ㅋㅋㅋ",
          "original_form": "어릴 때 더 잘 읽었던 거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100021403.1.6",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100021403.1.7",
          "form": "어떤장르좋아하세요",
          "original_form": "어떤장르좋아하세요",
          "speaker_id": "2",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100021403.1.8",
          "form": "저는 실용적인? 지식을 얻는? 책이 좋더라구요",
          "original_form": "저는 실용적인? 지식을 얻는? 책이 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100021403.1.9",
          "form": "여기서도 드러나는 가성비적 마인드...",
          "original_form": "여기서도 드러나는 가성비적 마인드...",
          "speaker_id": "1",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100021403.1.10",
          "form": "앜ㅋㅋㅋㅋㅋㅋ자기개발을 좋아하시는군요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ자기개발을 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100021403.1.11",
          "form": "맞아요 맞아요ㅋㅋㅋㅋ 인생 좀 알차게 살고 싶은 욕심이 있어요",
          "original_form": "맞아요 맞아요ㅋㅋㅋㅋ 인생 좀 알차게 살고 싶은 욕심이 있어요",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100021403.1.12",
          "form": "문학을 읽어야 좀 풍요롭게 살 텐데요ㅋㅋㅋㅋ",
          "original_form": "문학을 읽어야 좀 풍요롭게 살 텐데요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100021403.1.13",
          "form": "저도 책좀 읽어야하는데ㅠ",
          "original_form": "저도 책좀 읽어야하는데ㅠ",
          "speaker_id": "2",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100021403.1.14",
          "form": "마지막으로읽으신책은 어떤건가요",
          "original_form": "마지막으로읽으신책은 어떤건가요",
          "speaker_id": "2",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100021403.1.15",
          "form": "저 제로 금리 시대 어쩌고 하는 책 꾸역꾸역 읽고 있었는데",
          "original_form": "저 제로 금리 시대 어쩌고 하는 책 꾸역꾸역 읽고 있었는데",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021403.1.16",
          "form": "금리가 올랐어요 ㅋㅋ 그래서 이제 안 읽으려고요",
          "original_form": "금리가 올랐어요 ㅋㅋ 그래서 이제 안 읽으려고요",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021403.1.17",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100021403.1.18",
          "form": "시대를 역행하는 책이네요 ㅋㅋㅋ",
          "original_form": "시대를 역행하는 책이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100021403.1.19",
          "form": "넼ㅋㅋㅋㅋ 어려운데 괜히 읽었어요",
          "original_form": "넼ㅋㅋㅋㅋ 어려운데 괜히 읽었어요",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021403.1.20",
          "form": "name2은 최근에 읽은 책 있으세요?",
          "original_form": "&name2&은 최근에 읽은 책 있으세요?",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021403.1.21",
          "form": "아뇨ㅋㅋ",
          "original_form": "아뇨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021403.1.22",
          "form": "나는까칠하게살기로했다 를 읽으려고 빌렸는데",
          "original_form": "나는까칠하게살기로했다 를 읽으려고 빌렸는데",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021403.1.23",
          "form": "만지만 쌓이다가 반납햇어요 ㅋㅋㅋㅋ",
          "original_form": "만지만 쌓이다가 반납햇어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021403.1.24",
          "form": "아 그건 에세이 책 같은 건가요?",
          "original_form": "아 그건 에세이 책 같은 건가요?",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021403.1.25",
          "form": "약간 그런 느낌이에요",
