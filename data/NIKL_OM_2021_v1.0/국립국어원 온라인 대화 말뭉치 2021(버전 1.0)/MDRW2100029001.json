      "utterance": [
        {
          "id": "MDRW2100029001.1.1",
          "form": "아,,, 말도안되ㅋ",
          "original_form": "아,,, 말도안되ㅋ",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029001.1.2",
          "form": "힘내시고!!",
          "original_form": "힘내시고!!",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029001.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029001.1.4",
          "form": "저야 몸에서 곧 사리가....나올걸요....",
          "original_form": "저야 몸에서 곧 사리가....나올걸요....",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029001.1.5",
          "form": "제 기준에서 최선을 다하고 있고요...\n상대방이 느낄지 모르겠지만ㅋㅋ",
          "original_form": "제 기준에서 최선을 다하고 있고요...\n상대방이 느낄지 모르겠지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029001.1.6",
          "form": "ㅋ 동의합니다 사리나오죠",
          "original_form": "ㅋ 동의합니다 사리나오죠",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029001.1.7",
          "form": "제가 섭섭한건 다 참고 말안하는데",
          "original_form": "제가 섭섭한건 다 참고 말안하는데",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029001.1.8",
          "form": "자기 불만은 한번씩 펑 터트리면...",
          "original_form": "자기 불만은 한번씩 펑 터트리면...",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029001.1.9",
          "form": "와...미쳐요 진짜\n집을 나갈 수도 없고ㅋㅋ",
          "original_form": "와...미쳐요 진짜\n집을 나갈 수도 없고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029001.1.10",
          "form": "나가고싶죠  저도ㅋ",
          "original_form": "나가고싶죠  저도ㅋ",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029001.1.11",
          "form": "결론은 패배 울면끝",
          "original_form": "결론은 패배 울면끝",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029001.1.12",
          "form": "애들있어서 참지요",
          "original_form": "애들있어서 참지요",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029001.1.13",
          "form": "애가 크면 싸우기도 힘들고 울기도 눈치보입니다",
          "original_form": "애가 크면 싸우기도 힘들고 울기도 눈치보입니다",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029001.1.14",
          "form": "이제 주무실 시간입니다ㅎ",
          "original_form": "이제 주무실 시간입니다ㅎ",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029001.1.15",
          "form": "내일 또 대화나누시죠 항상 감사드립니다^^",
          "original_form": "내일 또 대화나누시죠 항상 감사드립니다^^",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029001.1.16",
          "form": "저녁에 해주세요",
          "original_form": "저녁에 해주세요",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029001.1.17",
          "form": "네 몇시쯤?",
          "original_form": "네 몇시쯤?",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029001.1.18",
          "form": "아 불금이군요ㅋ",
          "original_form": "아 불금이군요ㅋ",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029001.1.19",
          "form": "전 애들재우고 9시 이후",
          "original_form": "전 애들재우고 9시 이후",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029001.1.20",
          "form": "넵 ㅎ 내일뵈요!",
          "original_form": "넵 ㅎ 내일뵈요!",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029001.1.21",
          "form": "역시 아이들은 일찍자는군요",
          "original_form": "역시 아이들은 일찍자는군요",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029001.1.22",
          "form": "저희집은 늦게자는....",
          "original_form": "저희집은 늦게자는....",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029001.1.23",
          "form": "재워야죠^^ 내일시작",
          "original_form": "재워야죠^^ 내일시작",
