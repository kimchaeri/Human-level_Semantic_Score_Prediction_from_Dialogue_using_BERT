      "utterance": [
        {
          "id": "MDRW2100032631.1.1",
          "form": "ㅎㅎㅎ겨울필수품이죵",
          "original_form": "ㅎㅎㅎ겨울필수품이죵",
          "speaker_id": "2",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032631.1.2",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032631.1.3",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032631.1.4",
          "form": "고민이있어요",
          "original_form": "고민이있어요",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032631.1.5",
          "form": "어떤고민이요?",
          "original_form": "어떤고민이요?",
          "speaker_id": "2",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032631.1.6",
          "form": "제가 라텍스 매트리스를 써서",
          "original_form": "제가 라텍스 매트리스를 써서",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032631.1.7",
          "form": "혹시 전기장판 쓰다 불타는게아닐까ㅠ싶고",
          "original_form": "혹시 전기장판 쓰다 불타는게아닐까ㅠ싶고",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032631.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032631.1.9",
          "form": "너무갔나요",
          "original_form": "너무갔나요",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032631.1.10",
          "form": "어머 혹시",
          "original_form": "어머 혹시",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032631.1.11",
          "form": "탄사람이있나요?",
          "original_form": "탄사람이있나요?",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032631.1.12",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032631.1.13",
          "form": "라텍스가 발화점이 낮거든요",
          "original_form": "라텍스가 발화점이 낮거든요",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032631.1.14",
          "form": "그래서 혹시나 해서요ㅎㅎ",
          "original_form": "그래서 혹시나 해서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032631.1.15",
          "form": "ㅎㅎ저도걱정되네요",
          "original_form": "ㅎㅎ저도걱정되네요",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032631.1.16",
          "form": "중고거래해보신적있으세요?",
          "original_form": "중고거래해보신적있으세요?",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032631.1.17",
          "form": "네! 당근 자주써요",
          "original_form": "네! 당근 자주써요",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032631.1.18",
          "form": "당근 써보셨나요?",
          "original_form": "당근 써보셨나요?",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032631.1.19",
          "form": "딱한번올렸는데",
          "original_form": "딱한번올렸는데",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032631.1.20",
          "form": "아무도안사서 주륵ㅎㅎㅎ",
          "original_form": "아무도안사서 주륵ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032631.1.21",
          "form": "아앗ㅋㅋㅋㅋ",
          "original_form": "아앗ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032631.1.22",
          "form": "그죠 은근 팔긴 힘들어요",
          "original_form": "그죠 은근 팔긴 힘들어요",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032631.1.23",
          "form": "헤헤 팔아보신적있으시죠?",
          "original_form": "헤헤 팔아보신적있으시죠?",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032631.1.24",
          "form": "넹 저는 팔아봤어요",
          "original_form": "넹 저는 팔아봤어요",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032631.1.25",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032631.1.26",
          "form": "경력자",
          "original_form": "경력자",
