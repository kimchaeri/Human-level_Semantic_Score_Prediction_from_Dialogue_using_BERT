      "utterance": [
        {
          "id": "MDRW2100020412.1.1",
          "form": "종교가 있으신가요",
          "original_form": "종교가 있으신가요",
          "speaker_id": "1",
          "time": "20210828 03:00"
        },
        {
          "id": "MDRW2100020412.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210828 03:00"
        },
        {
          "id": "MDRW2100020412.1.3",
          "form": "모태신앙 때문에 천주교이긴 한데",
          "original_form": "모태신앙 때문에 천주교이긴 한데",
          "speaker_id": "2",
          "time": "20210828 03:00"
        },
        {
          "id": "MDRW2100020412.1.4",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 03:00"
        },
        {
          "id": "MDRW2100020412.1.5",
          "form": "종교 자체에 대해선 회의적입니다.",
          "original_form": "종교 자체에 대해선 회의적입니다.",
          "speaker_id": "2",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020412.1.6",
          "form": "앗 저는 모태신앙은 아니고 종교가 없었지만",
          "original_form": "앗 저는 모태신앙은 아니고 종교가 없었지만",
          "speaker_id": "1",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020412.1.7",
          "form": "종교에 대해서 회의적입니다",
          "original_form": "종교에 대해서 회의적입니다",
          "speaker_id": "1",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020412.1.8",
          "form": "대화가 잘 되겠어요!",
          "original_form": "대화가 잘 되겠어요!",
          "speaker_id": "1",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020412.1.9",
          "form": "그러게요!",
          "original_form": "그러게요!",
          "speaker_id": "2",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020412.1.10",
          "form": "반갑습니다~!",
          "original_form": "반갑습니다~!",
          "speaker_id": "2",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020412.1.11",
          "form": "종교에 대해서 회의적인 이유가 있으실까요? 저는",
          "original_form": "종교에 대해서 회의적인 이유가 있으실까요? 저는",
          "speaker_id": "2",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020412.1.12",
          "form": "조물주 ㅋㅋㅋㅋ 는 있다고 생각은 드는데",
          "original_form": "조물주 ㅋㅋㅋㅋ 는 있다고 생각은 드는데",
          "speaker_id": "2",
          "time": "20210828 03:02"
        },
        {
          "id": "MDRW2100020412.1.13",
          "form": "인간들이 신을 만들고 모시는(?) 행위가",
          "original_form": "인간들이 신을 만들고 모시는(?) 행위가",
          "speaker_id": "2",
          "time": "20210828 03:02"
        },
        {
          "id": "MDRW2100020412.1.14",
          "form": "모순적이라고 느껴서요!",
          "original_form": "모순적이라고 느껴서요!",
          "speaker_id": "2",
          "time": "20210828 03:02"
        },
        {
          "id": "MDRW2100020412.1.15",
          "form": "저도 그렇게 생각해요!",
          "original_form": "저도 그렇게 생각해요!",
          "speaker_id": "1",
          "time": "20210828 03:02"
        },
        {
          "id": "MDRW2100020412.1.16",
          "form": "성경이건 코란이건 어떤 종교건 간에",
          "original_form": "성경이건 코란이건 어떤 종교건 간에",
          "speaker_id": "1",
          "time": "20210828 03:03"
        },
        {
          "id": "MDRW2100020412.1.17",
          "form": "율법이나 규칙은 결국 인간이 만들어낸거잖아요",
          "original_form": "율법이나 규칙은 결국 인간이 만들어낸거잖아요",
          "speaker_id": "1",
          "time": "20210828 03:03"
        },
        {
          "id": "MDRW2100020412.1.18",
          "form": "결국 좀 더 많은 사람이 믿냐 아니냐 그 차이지",
          "original_form": "결국 좀 더 많은 사람이 믿냐 아니냐 그 차이지",
          "speaker_id": "1",
          "time": "20210828 03:03"
        },
        {
          "id": "MDRW2100020412.1.19",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210828 03:03"
        },
        {
          "id": "MDRW2100020412.1.20",
          "form": "기독교랑 사이비종교의 차이점을 모르겟어요 ㅎㅎㅎㅎ",
          "original_form": "기독교랑 사이비종교의 차이점을 모르겟어요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 03:03"
        },
        {
          "id": "MDRW2100020412.1.21",
          "form": "그리고 그 뭐더라... 종교 관련 다큐멘터리 봤는데",
          "original_form": "그리고 그 뭐더라... 종교 관련 다큐멘터리 봤는데",
          "speaker_id": "2",
          "time": "20210828 03:03"
        },
        {
          "id": "MDRW2100020412.1.22",
          "form": "그쵸 그냥 머릿수 차이죠",
          "original_form": "그쵸 그냥 머릿수 차이죠",
          "speaker_id": "2",
          "time": "20210828 03:03"
        },
        {
          "id": "MDRW2100020412.1.23",
          "form": "신을 믿게하고... 자기들만의 규칙이 잇고...",
          "original_form": "신을 믿게하고... 자기들만의 규칙이 잇고...",
          "speaker_id": "1",
          "time": "20210828 03:03"
        },
        {
          "id": "MDRW2100020412.1.24",
          "form": "돈을 중요시하고 ㅎㅎㅎㅎ",
          "original_form": "돈을 중요시하고 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.25",
          "form": "종교의 공통점이",
          "original_form": "종교의 공통점이",
          "speaker_id": "2",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.26",
          "form": "큰 강 주변에서",
          "original_form": "큰 강 주변에서",
          "speaker_id": "2",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.27",
          "form": "기독교나 신천지나",
          "original_form": "기독교나 신천지나",
          "speaker_id": "1",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.28",
          "form": "생겨났다..",
          "original_form": "생겨났다..",
          "speaker_id": "2",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.29",
          "form": "똑같은거 아닌가 해서요!",
          "original_form": "똑같은거 아닌가 해서요!",
          "speaker_id": "1",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.30",
          "form": "맞아요 그냥 뭐랄까",
          "original_form": "맞아요 그냥 뭐랄까",
          "speaker_id": "2",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.31",
          "form": "대놓고 요구하느냐 안하느냐 차이?",
          "original_form": "대놓고 요구하느냐 안하느냐 차이?",
          "speaker_id": "2",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.32",
          "form": "ㅋㅋㅋㅋㅋ아 정말요?",
          "original_form": "ㅋㅋㅋㅋㅋ아 정말요?",
          "speaker_id": "1",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.33",
          "form": "왜 다들 큰 강 주변에서 생겨난걸까요?",
          "original_form": "왜 다들 큰 강 주변에서 생겨난걸까요?",
          "speaker_id": "1",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.34",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.35",
          "form": "아 맞아요 좀 더 강하게 어필하느냐 덜 하느냐",
          "original_form": "아 맞아요 좀 더 강하게 어필하느냐 덜 하느냐",
          "speaker_id": "1",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.36",
          "form": "그정도의 차이지",
          "original_form": "그정도의 차이지",
          "speaker_id": "1",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.37",
          "form": "결국 본질은 돈인거같아요 ㅎㅎ",
          "original_form": "결국 본질은 돈인거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.38",
          "form": "그 뭐지... 인간들이",
          "original_form": "그 뭐지... 인간들이",
          "speaker_id": "2",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020412.1.39",
          "form": "문명을 일궈낸 곳이",
          "original_form": "문명을 일궈낸 곳이",
          "speaker_id": "2",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.40",
          "form": "다 강 주변이어서",
          "original_form": "다 강 주변이어서",
          "speaker_id": "2",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.41",
          "form": "오 일리 있네요",
          "original_form": "오 일리 있네요",
          "speaker_id": "1",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.42",
          "form": "자연스레 종교도 생겨났다 하더라구요. 윗사람들이 아래사람들을 통치하려면",
          "original_form": "자연스레 종교도 생겨났다 하더라구요. 윗사람들이 아래사람들을 통치하려면",
          "speaker_id": "2",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.43",
          "form": "먹고살만하니까",
          "original_form": "먹고살만하니까",
          "speaker_id": "1",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.44",
          "form": "질서가 있어야하는데",
          "original_form": "질서가 있어야하는데",
          "speaker_id": "2",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.45",
          "form": "가장 좋은 방법이 눈에 보이지 않는",
          "original_form": "가장 좋은 방법이 눈에 보이지 않는",
          "speaker_id": "2",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.46",
          "form": "이제 슬슬 통제하기 시작한거죠",
          "original_form": "이제 슬슬 통제하기 시작한거죠",
          "speaker_id": "1",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.47",
          "form": "절대자가",
          "original_form": "절대자가",
          "speaker_id": "2",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.48",
          "form": "윗사람을 선택한거다",
          "original_form": "윗사람을 선택한거다",
          "speaker_id": "2",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.49",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.50",
          "form": "질서를 위해 만들어졌다고 생각해요",
          "original_form": "질서를 위해 만들어졌다고 생각해요",
          "speaker_id": "2",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.51",
          "form": "우리가 당연하다고 생각는",
          "original_form": "우리가 당연하다고 생각는",
          "speaker_id": "2",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020412.1.52",
          "form": "ㅋㅋㅋㅋ진짜루 맞는말같아요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ진짜루 맞는말같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 03:06"
        },
        {
          "id": "MDRW2100020412.1.53",
          "form": "대부분의 것들이 질서 유지를 위한거죠",
          "original_form": "대부분의 것들이 질서 유지를 위한거죠",
          "speaker_id": "2",
          "time": "20210828 03:06"
        },
        {
          "id": "MDRW2100020412.1.54",
          "form": "먹고살만하고",
          "original_form": "먹고살만하고",
          "speaker_id": "1",
          "time": "20210828 03:06"
        },
        {
          "id": "MDRW2100020412.1.55",
          "form": "가부장제도 그렇고 종교도 그렇고",
          "original_form": "가부장제도 그렇고 종교도 그렇고",
          "speaker_id": "2",
          "time": "20210828 03:06"
        },
        {
          "id": "MDRW2100020412.1.56",
          "form": "재산이 생기니까 자연스럽게",
          "original_form": "재산이 생기니까 자연스럽게",
          "speaker_id": "1",
          "time": "20210828 03:06"
        },
        {
          "id": "MDRW2100020412.1.57",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 03:06"
        },
        {
          "id": "MDRW2100020412.1.58",
          "form": "부의 차등이 이뤄지는데",
          "original_form": "부의 차등이 이뤄지는데",
          "speaker_id": "1",
          "time": "20210828 03:06"
        },
        {
          "id": "MDRW2100020412.1.59",
          "form": "하층민일수록",
          "original_form": "하층민일수록",
          "speaker_id": "1",
          "time": "20210828 03:06"
        },
        {
          "id": "MDRW2100020412.1.60",
          "form": "반발하기쉬우니까",
          "original_form": "반발하기쉬우니까",
          "speaker_id": "1",
          "time": "20210828 03:06"
        },
        {
          "id": "MDRW2100020412.1.61",
          "form": "그걸 이제 보이지않는 신으로",
          "original_form": "그걸 이제 보이지않는 신으로",
          "speaker_id": "1",
          "time": "20210828 03:06"
        },
        {
          "id": "MDRW2100020412.1.62",
          "form": "맞아요 저도 학부생일 때",
          "original_form": "맞아요 저도 학부생일 때",
          "speaker_id": "2",
          "time": "20210828 03:06"
        },
        {
          "id": "MDRW2100020412.1.63",
          "form": "정당헝을 부여ㅕ하나바요",
          "original_form": "정당헝을 부여ㅕ하나바요",
          "speaker_id": "1",
          "time": "20210828 03:06"
        },
        {
