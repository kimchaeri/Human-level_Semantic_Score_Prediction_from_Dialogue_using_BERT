        },
        {
          "id": "MDRW2100007473.1.32",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "1",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007473.1.33",
          "form": "티켓팅은 항상 어려워요ㅠ",
          "original_form": "티켓팅은 항상 어려워요ㅠ",
