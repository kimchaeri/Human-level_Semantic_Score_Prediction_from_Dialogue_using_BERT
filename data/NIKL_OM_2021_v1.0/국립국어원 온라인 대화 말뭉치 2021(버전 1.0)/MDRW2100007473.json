      "utterance": [
        {
          "id": "MDRW2100007473.1.1",
          "form": "웃는남자",
          "original_form": "웃는남자",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007473.1.2",
          "form": "좋아하시죠",
          "original_form": "좋아하시죠",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007473.1.3",
          "form": "예??",
          "original_form": "예??",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007473.1.4",
          "form": "잘생기면 좋아합니다",
          "original_form": "잘생기면 좋아합니다",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007473.1.5",
          "form": "아 뮤지컬이요??",
          "original_form": "아 뮤지컬이요??",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007473.1.6",
          "form": "좋아하쥬",
          "original_form": "좋아하쥬",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007473.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007473.1.8",
          "form": "혹시 기억에 남는 배우가 있나요?",
          "original_form": "혹시 기억에 남는 배우가 있나요?",
          "speaker_id": "2",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007473.1.9",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007473.1.10",
          "form": "수호여",
          "original_form": "수호여",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007473.1.11",
          "form": "저 근데 세수 좀 하고올게요",
          "original_form": "저 근데 세수 좀 하고올게요",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007473.1.12",
          "form": "찝찝..",
          "original_form": "찝찝..",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007473.1.13",
          "form": "수호 잘생겼죠",
          "original_form": "수호 잘생겼죠",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007473.1.14",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007473.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007473.1.16",
          "form": "뮤지컬 좋아하세여??",
          "original_form": "뮤지컬 좋아하세여??",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007473.1.17",
          "form": "저는 뮤지컬보다",
          "original_form": "저는 뮤지컬보다",
          "speaker_id": "2",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007473.1.18",
          "form": "뮤비 좋아해요",
          "original_form": "뮤비 좋아해요",
          "speaker_id": "2",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007473.1.19",
          "form": "코로나때문에 자유롭게 못보는게 넘 아쉽네여ㅠ",
          "original_form": "코로나때문에 자유롭게 못보는게 넘 아쉽네여ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007473.1.20",
          "form": "영화요??",
          "original_form": "영화요??",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007473.1.21",
          "form": "아님",
          "original_form": "아님",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007473.1.22",
          "form": "뮤직 비디오??",
          "original_form": "뮤직 비디오??",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007473.1.23",
          "form": "뮤직비디오요",
          "original_form": "뮤직비디오요",
          "speaker_id": "2",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007473.1.24",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007473.1.25",
          "form": "좋아하는가수가 있으세여??",
          "original_form": "좋아하는가수가 있으세여??",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007473.1.26",
          "form": "저는 아이유 좋아해요",
          "original_form": "저는 아이유 좋아해요",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007473.1.27",
          "form": "아이유 노래좋죠",
          "original_form": "아이유 노래좋죠",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007473.1.28",
          "form": "콘서트 가보심??",
          "original_form": "콘서트 가보심??",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007473.1.29",
          "form": "콘서트 티켓팅 너무 빡세서 못갔어요",
          "original_form": "콘서트 티켓팅 너무 빡세서 못갔어요",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007473.1.30",
          "form": "혹시 콘서트 가보셨나요?",
          "original_form": "혹시 콘서트 가보셨나요?",
