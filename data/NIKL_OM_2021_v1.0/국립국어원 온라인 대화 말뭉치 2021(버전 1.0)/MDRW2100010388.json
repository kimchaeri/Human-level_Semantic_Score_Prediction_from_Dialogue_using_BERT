      "utterance": [
        {
          "id": "MDRW2100010388.1.1",
          "form": "직관하러도 가시나요?",
          "original_form": "직관하러도 가시나요?",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010388.1.2",
          "form": "어우 해외를",
          "original_form": "어우 해외를",
          "speaker_id": "2",
          "time": "20210823 17:37"
        },
        {
          "id": "MDRW2100010388.1.3",
          "form": "갈돈은 없답니다ㅠ",
          "original_form": "갈돈은 없답니다ㅠ",
          "speaker_id": "2",
          "time": "20210823 17:37"
        },
        {
          "id": "MDRW2100010388.1.4",
          "form": "티켓경쟁도",
          "original_form": "티켓경쟁도",
          "speaker_id": "2",
          "time": "20210823 17:37"
        },
        {
          "id": "MDRW2100010388.1.5",
          "form": "상당할걸요",
          "original_form": "상당할걸요",
          "speaker_id": "2",
          "time": "20210823 17:38"
        },
        {
          "id": "MDRW2100010388.1.6",
          "form": "국내  축구장은 가보셨어요?",
          "original_form": "국내  축구장은 가보셨어요?",
          "speaker_id": "1",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010388.1.7",
          "form": "그정도까진아니고",
          "original_form": "그정도까진아니고",
          "speaker_id": "2",
          "time": "20210823 17:46"
        },
        {
          "id": "MDRW2100010388.1.8",
          "form": "집에서 보는걸좋아한답니다ㅎㅎㅎ",
          "original_form": "집에서 보는걸좋아한답니다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:46"
        },
        {
          "id": "MDRW2100010388.1.9",
          "form": "아ㅎ 전딱한번 가봤어요",
          "original_form": "아ㅎ 전딱한번 가봤어요",
          "speaker_id": "1",
          "time": "20210823 17:46"
        },
        {
          "id": "MDRW2100010388.1.10",
          "form": "축구장 야구장",
          "original_form": "축구장 야구장",
          "speaker_id": "1",
          "time": "20210823 17:47"
        },
        {
          "id": "MDRW2100010388.1.11",
          "form": "야구 좋아하시나요?",
          "original_form": "야구 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 17:49"
        },
        {
          "id": "MDRW2100010388.1.12",
          "form": "티비로보는게 더편하고 좋긴해요 잘보이고ㅋ",
          "original_form": "티비로보는게 더편하고 좋긴해요 잘보이고ㅋ",
          "speaker_id": "1",
          "time": "20210823 17:49"
        },
        {
          "id": "MDRW2100010388.1.13",
          "form": "중딩때 농구좋아하구 그이후론 잘안봐요",
          "original_form": "중딩때 농구좋아하구 그이후론 잘안봐요",
          "speaker_id": "1",
          "time": "20210823 17:49"
        },
        {
          "id": "MDRW2100010388.1.14",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010388.1.15",
          "form": "올림픽은 잘보긴했어요",
          "original_form": "올림픽은 잘보긴했어요",
          "speaker_id": "2",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010388.1.16",
          "form": "유일하게  빠져보는게 올림픽 월드컵이요",
          "original_form": "유일하게  빠져보는게 올림픽 월드컵이요",
          "speaker_id": "1",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010388.1.17",
          "form": "맞아요이번올림픽",
          "original_form": "맞아요이번올림픽",
          "speaker_id": "2",
          "time": "20210824 10:37"
        },
        {
          "id": "MDRW2100010388.1.18",
          "form": "너무재밌었죠",
          "original_form": "너무재밌었죠",
          "speaker_id": "2",
          "time": "20210824 10:37"
        },
        {
          "id": "MDRW2100010388.1.19",
          "form": "양궁 펜싱 선수들 넘잘해요",
          "original_form": "양궁 펜싱 선수들 넘잘해요",
          "speaker_id": "1",
          "time": "20210824 10:37"
        },
        {
          "id": "MDRW2100010388.1.20",
          "form": "양궁은",
          "original_form": "양궁은",
          "speaker_id": "2",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100010388.1.21",
          "form": "쿵야라고 별명도있더라고요",
          "original_form": "쿵야라고 별명도있더라고요",
          "speaker_id": "2",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100010388.1.22",
          "form": "김제덕선수 귀엽던데요ㅎ",
          "original_form": "김제덕선수 귀엽던데요ㅎ",
          "speaker_id": "1",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100010388.1.23",
          "form": "경기장 가서 봐도 잼날것같아요",
          "original_form": "경기장 가서 봐도 잼날것같아요",
          "speaker_id": "1",
