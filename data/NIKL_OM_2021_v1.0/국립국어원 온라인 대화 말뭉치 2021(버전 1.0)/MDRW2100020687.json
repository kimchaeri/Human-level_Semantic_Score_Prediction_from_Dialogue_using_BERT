      "utterance": [
        {
          "id": "MDRW2100020687.1.1",
          "form": "고기는 어떤걸 좋아하세요",
          "original_form": "고기는 어떤걸 좋아하세요",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020687.1.2",
          "form": "아 웃기다ㅋㅋㅋㅋㅋ 그래요 이름이 뭐가중요해 맛을 알면된거지",
          "original_form": "아 웃기다ㅋㅋㅋㅋㅋ 그래요 이름이 뭐가중요해 맛을 알면된거지",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020687.1.3",
          "form": "저 소돼지닭 다 좋아해요",
          "original_form": "저 소돼지닭 다 좋아해요",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020687.1.4",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020687.1.5",
          "form": "어우 저두요",
          "original_form": "어우 저두요",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020687.1.6",
          "form": "돼지는",
          "original_form": "돼지는",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020687.1.7",
          "form": "딱 그 세가지만 좋아해요",
          "original_form": "딱 그 세가지만 좋아해요",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020687.1.8",
          "form": "아무래도 삼겹살",
          "original_form": "아무래도 삼겹살",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020687.1.9",
          "form": "네 삼겹살이 디폴트죠",
          "original_form": "네 삼겹살이 디폴트죠",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020687.1.10",
          "form": "양고기 좀 지나갈 때 마다 냄새나서 먹어볼 생각도 안해봤어요",
          "original_form": "양고기 좀 지나갈 때 마다 냄새나서 먹어볼 생각도 안해봤어요",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020687.1.11",
          "form": "삼겹살 드실 때 쌈 싸드시나요",
          "original_form": "삼겹살 드실 때 쌈 싸드시나요",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020687.1.12",
          "form": "네 저 쌈 완전 좋아해요",
          "original_form": "네 저 쌈 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020687.1.13",
          "form": "무쌈 필수",
          "original_form": "무쌈 필수",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020687.1.14",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020687.1.15",
          "form": "쌈무진짜좋아",
          "original_form": "쌈무진짜좋아",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020687.1.16",
          "form": "새콤달콤 없어서는 안되는 필수품",
          "original_form": "새콤달콤 없어서는 안되는 필수품",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020687.1.17",
          "form": "마늘은 드시나요",
          "original_form": "마늘은 드시나요",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020687.1.18",
          "form": "혹시 마늘",
          "original_form": "혹시 마늘",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020687.1.19",
          "form": "ㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020687.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020687.1.21",
          "form": "저 먹어욬ㅋㅋㅋㅋ",
          "original_form": "저 먹어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020687.1.22",
          "form": "혹시 구워드세요",
          "original_form": "혹시 구워드세요",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020687.1.23",
          "form": "구워드시나요 생으로 드시나요",
          "original_form": "구워드시나요 생으로 드시나요",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020687.1.24",
          "form": "저는 둘다 좋아하는데",
          "original_form": "저는 둘다 좋아하는데",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020687.1.25",
          "form": "전 구운거 좋아해요",
          "original_form": "전 구운거 좋아해요",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020687.1.26",
          "form": "슬라이스된 마늘도 좋아합니다",
          "original_form": "슬라이스된 마늘도 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020687.1.27",
          "form": "통마늘 즐기시는구나",
          "original_form": "통마늘 즐기시는구나",
          "speaker_id": "1",
