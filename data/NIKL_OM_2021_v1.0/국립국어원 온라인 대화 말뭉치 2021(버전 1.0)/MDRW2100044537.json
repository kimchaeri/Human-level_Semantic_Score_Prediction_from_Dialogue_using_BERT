      "utterance": [
        {
          "id": "MDRW2100044537.1.1",
          "form": "혹시 피자도 좋아하세요?",
          "original_form": "혹시 피자도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044537.1.2",
          "form": "피자도 너무 좋아하죠~",
          "original_form": "피자도 너무 좋아하죠~",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044537.1.3",
          "form": "초딩 입맛이라~ㅎ",
          "original_form": "초딩 입맛이라~ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044537.1.4",
          "form": "최애 브랜드 있나요?",
          "original_form": "최애 브랜드 있나요?",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044537.1.5",
          "form": "집 근처에 피자헛 있어서 그냥 피자헛 이용해요~",
          "original_form": "집 근처에 피자헛 있어서 그냥 피자헛 이용해요~",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044537.1.6",
          "form": "아하 저는 피자샵을 좋아해요 ㅎ",
          "original_form": "아하 저는 피자샵을 좋아해요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044537.1.7",
          "form": "피자 종류는 어떤거 좋아하세요?",
          "original_form": "피자 종류는 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044537.1.8",
          "form": "저는 주로 감자올라간걸 좋아해요 피자샵 치즈후라이 맛있더라고요",
          "original_form": "저는 주로 감자올라간걸 좋아해요 피자샵 치즈후라이 맛있더라고요",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044537.1.9",
          "form": "저는 고구마 좋아해요~\r\n피자헛 메뉴중에는 갈릭버터쉬림프를 제일 좋아해요",
          "original_form": "저는 고구마 좋아해요~\r\n피자헛 메뉴중에는 갈릭버터쉬림프를 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044537.1.10",
          "form": "그거 맛았더라고요 갈릭버터쉬림프ㅎㅎ 그 고구마 좋아하시면 피자샵 스위트 고구마 드샤보세요 맛있더라고요 ㅎㅎ",
          "original_form": "그거 맛았더라고요 갈릭버터쉬림프ㅎㅎ 그 고구마 좋아하시면 피자샵 스위트 고구마 드샤보세요 맛있더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044537.1.11",
          "form": "피자샵이 반올림 이야기 하시는 거죠? ㅎ",
          "original_form": "피자샵이 반올림 이야기 하시는 거죠? ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044537.1.12",
          "form": "네ㅎㅎ 드셔보셨어요?",
          "original_form": "네ㅎㅎ 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044537.1.13",
          "form": "아이유 땜에 알아요~ㅎ",
          "original_form": "아이유 땜에 알아요~ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044537.1.14",
          "form": "우리는 그냥 반올림이라 호칭해서 순간 헷갈렸어요~ㅋ",
          "original_form": "우리는 그냥 반올림이라 호칭해서 순간 헷갈렸어요~ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044537.1.15",
          "form": "아하 ㅎㅎ 저도 제 딸이 포토카드 때문에 먹기 시작했거등요",
          "original_form": "아하 ㅎㅎ 저도 제 딸이 포토카드 때문에 먹기 시작했거등요",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044537.1.16",
          "form": "아하 부르는 이름이 달랐군요",
          "original_form": "아하 부르는 이름이 달랐군요",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044537.1.17",
          "form": "이렇게 해서 맛있는 피자 정보도 알고 갑니다~ㅎ",
          "original_form": "이렇게 해서 맛있는 피자 정보도 알고 갑니다~ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044537.1.18",
          "form": "ㅎㅎ 입맛에 맞으시명 좋겠네요",
          "original_form": "ㅎㅎ 입맛에 맞으시명 좋겠네요",
          "speaker_id": "2",
          "time": "20210907 23:21"
        }
