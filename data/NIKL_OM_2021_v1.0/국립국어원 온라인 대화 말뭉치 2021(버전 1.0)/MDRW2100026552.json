      "utterance": [
        {
          "id": "MDRW2100026552.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026552.1.2",
          "form": "뭐해요?",
          "original_form": "뭐해요?",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026552.1.3",
          "form": "안녕하세요~ 저는 그냥 누워있어요ㅋㅋ",
          "original_form": "안녕하세요~ 저는 그냥 누워있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026552.1.4",
          "form": "아~ㅎㅎ 혹시 학생이세요?",
          "original_form": "아~ㅎㅎ 혹시 학생이세요?",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026552.1.5",
          "form": "네 학생입니다!",
          "original_form": "네 학생입니다!",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026552.1.6",
          "form": "개강 하셨겠네요?",
          "original_form": "개강 하셨겠네요?",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026552.1.7",
          "form": "저는 31일에 개강했어요~",
          "original_form": "저는 31일에 개강했어요~",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026552.1.8",
          "form": "네... 힘들어 죽을거같아요...........",
          "original_form": "네... 힘들어 죽을거같아요...........",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026552.1.9",
          "form": "학교 대면이세요??",
          "original_form": "학교 대면이세요??",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026552.1.10",
          "form": "저는 비대면이에요",
          "original_form": "저는 비대면이에요",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026552.1.11",
          "form": "시험은 대면입니당",
          "original_form": "시험은 대면입니당",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026552.1.12",
          "form": "전 비대면 너무 좋아요",
          "original_form": "전 비대면 너무 좋아요",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026552.1.13",
          "form": "비대면 만족하세요??",
          "original_form": "비대면 만족하세요??",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026552.1.14",
          "form": "저도 코로롱은 싫긴하지만 비대면은 솔직히 만족해요 ㅎㅋ",
          "original_form": "저도 코로롱은 싫긴하지만 비대면은 솔직히 만족해요 ㅎㅋ",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026552.1.15",
          "form": "수강신청은 잘 하셨어요??",
          "original_form": "수강신청은 잘 하셨어요??",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026552.1.16",
          "form": "맞아요 수업시작 1분전에 일어나서 들을수 있는게 제일 좋은거같아요ㅋㅋㅋㅋ",
          "original_form": "맞아요 수업시작 1분전에 일어나서 들을수 있는게 제일 좋은거같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026552.1.17",
          "form": "저는 학교에서 정해져서 선택권이 없었어요...ㅜ",
          "original_form": "저는 학교에서 정해져서 선택권이 없었어요...ㅜ",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026552.1.18",
          "form": "올클 하셨나요???",
          "original_form": "올클 하셨나요???",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026552.1.19",
          "form": "넵!! 올클입니다",
          "original_form": "넵!! 올클입니다",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026552.1.20",
          "form": "솔직히 넘 쉬운거같아요 헤헤",
          "original_form": "솔직히 넘 쉬운거같아요 헤헤",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026552.1.21",
          "form": "헉 완전 금손이시네요!",
          "original_form": "헉 완전 금손이시네요!",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026552.1.22",
          "form": "전 선착순으로 수강신청했던 학기에",
          "original_form": "전 선착순으로 수강신청했던 학기에",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026552.1.23",
          "form": "매번 실패했던거같은데...",
          "original_form": "매번 실패했던거같은데...",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026552.1.24",
          "form": "근데 수강신청이 참 변수가 많아서",
          "original_form": "근데 수강신청이 참 변수가 많아서",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026552.1.25",
          "form": "본인이 잘해도 튕기거나 하는경우가 많도라고용 ㅜㅜ",
          "original_form": "본인이 잘해도 튕기거나 하는경우가 많도라고용 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026552.1.26",
          "form": "저는 운이 좋았던거 같습니당",
          "original_form": "저는 운이 좋았던거 같습니당",
          "speaker_id": "1",
