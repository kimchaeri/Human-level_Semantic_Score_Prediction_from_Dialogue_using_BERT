      "utterance": [
        {
          "id": "MDRW2100044383.1.1",
          "form": "동물 조아하세요!?",
          "original_form": "동물 조아하세요!?",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044383.1.2",
          "form": "저는 요즘에는 토끼기 끌리더라구요!",
          "original_form": "저는 요즘에는 토끼기 끌리더라구요!",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044383.1.3",
          "form": "귀여운 깡패ㅜ",
          "original_form": "귀여운 깡패ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044383.1.4",
          "form": "토끼 넘귀엽ㅜㅜ잘때 발라당 누워서 자는거 치명적이에요",
          "original_form": "토끼 넘귀엽ㅜㅜ잘때 발라당 누워서 자는거 치명적이에요",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044383.1.5",
          "form": "아 맞아요ㅠㅜㅜㅠㅜ",
          "original_form": "아 맞아요ㅠㅜㅜㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044383.1.6",
          "form": "발라당",
          "original_form": "발라당",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044383.1.7",
          "form": "잘때는 완전 순딩이죠",
          "original_form": "잘때는 완전 순딩이죠",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044383.1.8",
          "form": "어릴때 당근있어요라는 만화책 보면서 참 키우고싶었어요ㅋㅋ",
          "original_form": "어릴때 당근있어요라는 만화책 보면서 참 키우고싶었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044383.1.9",
          "form": "오오 저는 첨듣는 책이예요",
          "original_form": "오오 저는 첨듣는 책이예요",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044383.1.10",
          "form": "좋아하는 동물 있으신가요?",
          "original_form": "좋아하는 동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044383.1.11",
          "form": "저는 동물은 거의 다 좋아해요ㅠㅠ",
          "original_form": "저는 동물은 거의 다 좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044383.1.12",
          "form": "강아지파 고양이파 어느쪽이세요ㅎㅎ",
          "original_form": "강아지파 고양이파 어느쪽이세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044383.1.13",
          "form": "저는 고양이요!",
          "original_form": "저는 고양이요!",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044383.1.14",
          "form": "개는 좀 무서워요ㅠㅠ물려봐가지구",
          "original_form": "개는 좀 무서워요ㅠㅠ물려봐가지구",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044383.1.15",
          "form": "아이구 큰개였나요?ㅠㅠ",
          "original_form": "아이구 큰개였나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044383.1.16",
          "form": "네네 어렸을때 강아지 쫓아가다 어미한테ㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "네네 어렸을때 강아지 쫓아가다 어미한테ㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044383.1.17",
          "form": "옷만 물리고 다치진않았는데 그때 이후로 강아지도 멀리서만 보게되더라구요ㅠㅠ",
          "original_form": "옷만 물리고 다치진않았는데 그때 이후로 강아지도 멀리서만 보게되더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044383.1.18",
          "form": "큰일날뻔했네요ㅜㅠㅠ",
          "original_form": "큰일날뻔했네요ㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044383.1.19",
          "form": "고양이는 키워보셨어요?",
          "original_form": "고양이는 키워보셨어요?",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044383.1.20",
          "form": "아뇨ㅠㅠ 가족이 고양이 알레르기가 심해가지구ㅠ",
          "original_form": "아뇨ㅠㅠ 가족이 고양이 알레르기가 심해가지구ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044383.1.21",
          "form": "반려동물 키워보셨나요?",
          "original_form": "반려동물 키워보셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044383.1.22",
          "form": "저도 냥이 알러지 있어요ㅠ대신 강아지 키웠었어요ㅎㅎ",
          "original_form": "저도 냥이 알러지 있어요ㅠ대신 강아지 키웠었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044383.1.23",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044383.1.24",
          "form": "이름이 뭐였나요?",
