      "utterance": [
        {
          "id": "MDRW2100045395.1.1",
          "form": "요즘 엔씨 게임 아세요??",
          "original_form": "요즘 엔씨 게임 아세요??",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045395.1.2",
          "form": "블소2인가 새로나왓다고 들었어요",
          "original_form": "블소2인가 새로나왓다고 들었어요",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045395.1.3",
          "form": "맞아요 그거",
          "original_form": "맞아요 그거",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045395.1.4",
          "form": "그 게임이",
          "original_form": "그 게임이",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045395.1.5",
          "form": "그 게임이 왜요??",
          "original_form": "그 게임이 왜요??",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045395.1.6",
          "form": "완전 게임인척 하는 도박장이래요",
          "original_form": "완전 게임인척 하는 도박장이래요",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045395.1.7",
          "form": "아;; 엔씨가 엔씨했네요",
          "original_form": "아;; 엔씨가 엔씨했네요",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045395.1.8",
          "form": "그쵸??",
          "original_form": "그쵸??",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045395.1.9",
          "form": "진짜 대형 게임사라는게 하는게 다 똑같아서 완전 별로에요",
          "original_form": "진짜 대형 게임사라는게 하는게 다 똑같아서 완전 별로에요",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045395.1.10",
          "form": "맞아요 좀 신박한거 내주지 ㅋㅋㅋ 과금 구조만 되물림하고",
          "original_form": "맞아요 좀 신박한거 내주지 ㅋㅋㅋ 과금 구조만 되물림하고",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045395.1.11",
          "form": "인정입니다 ;; 과금 구조 진짜 별로,;;",
          "original_form": "인정입니다 ;; 과금 구조 진짜 별로,;;",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045395.1.12",
          "form": "그런데 그 과금구조를 또 다른 게임에서 따라하니까",
          "original_form": "그런데 그 과금구조를 또 다른 게임에서 따라하니까",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045395.1.13",
          "form": "점점 한국겜 망겜 되는 것 같아요",
          "original_form": "점점 한국겜 망겜 되는 것 같아요",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045395.1.14",
          "form": "맞아요 ;; 과금구조 잘 만드는건 알겠는데 게임성이 너무 묻혀요",
          "original_form": "맞아요 ;; 과금구조 잘 만드는건 알겠는데 게임성이 너무 묻혀요",
          "speaker_id": "2",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045395.1.15",
          "form": "또 그런 구조를 기존 유저들이 엄청 좋아하니까..ㅎ",
          "original_form": "또 그런 구조를 기존 유저들이 엄청 좋아하니까..ㅎ",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045395.1.16",
          "form": "안 망하는것 같아요",
