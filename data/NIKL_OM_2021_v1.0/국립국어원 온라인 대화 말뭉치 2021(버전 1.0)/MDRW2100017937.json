      "utterance": [
        {
          "id": "MDRW2100017937.1.1",
          "form": "운동 즐겨 하시나요?",
          "original_form": "운동 즐겨 하시나요?",
          "speaker_id": "2",
          "time": "20210830 01:07"
        },
        {
          "id": "MDRW2100017937.1.2",
          "form": "네.. 자주하고잇어요",
          "original_form": "네.. 자주하고잇어요",
          "speaker_id": "1",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100017937.1.3",
          "form": "어떤거 하세요?",
          "original_form": "어떤거 하세요?",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100017937.1.4",
          "form": "등산 런닝 홈트요..",
          "original_form": "등산 런닝 홈트요..",
          "speaker_id": "1",
          "time": "20210830 15:50"
        },
        {
          "id": "MDRW2100017937.1.5",
          "form": "우와 부지런하시네요",
          "original_form": "우와 부지런하시네요",
          "speaker_id": "2",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100017937.1.6",
          "form": "살기위해 하는거죠 뭐... ㅋㅋㅋㅋ",
          "original_form": "살기위해 하는거죠 뭐... ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:52"
        },
        {
          "id": "MDRW2100017937.1.7",
          "form": "전 ㅇㅏ직 정신이 덜 차려져서 ㅜㅜ",
          "original_form": "전 ㅇㅏ직 정신이 덜 차려져서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 17:16"
        },
        {
          "id": "MDRW2100017937.1.8",
          "form": "건강하려고 하는건데.. 건강하심 안하셔도 될거갘아요.. ㅋㅋㅋ",
          "original_form": "건강하려고 하는건데.. 건강하심 안하셔도 될거갘아요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:23"
        },
        {
          "id": "MDRW2100017937.1.9",
          "form": "ㅎㅎㅎㅎ 점점체력도 떨으지고ㅜㅜ",
          "original_form": "ㅎㅎㅎㅎ 점점체력도 떨으지고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100017937.1.10",
          "form": "뭐 운동하세요?",
          "original_form": "뭐 운동하세요?",
          "speaker_id": "1",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100017937.1.11",
          "form": "안해요 요즘은 ㅜ",
          "original_form": "안해요 요즘은 ㅜ",
          "speaker_id": "2",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100017937.1.12",
          "form": "아... 부럽... 다이어트 안해도 되는 몸..",
          "original_form": "아... 부럽... 다이어트 안해도 되는 몸..",
