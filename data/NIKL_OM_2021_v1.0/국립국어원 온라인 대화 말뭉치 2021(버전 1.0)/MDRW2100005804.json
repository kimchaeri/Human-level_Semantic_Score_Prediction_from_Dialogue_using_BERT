      "utterance": [
        {
          "id": "MDRW2100005804.1.1",
          "form": "갤럭시씀 아이폰씀?",
          "original_form": "갤럭시씀 아이폰씀?",
          "speaker_id": "1",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100005804.1.2",
          "form": "플립3이랑 폴드3",
          "original_form": "플립3이랑 폴드3",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100005804.1.3",
          "form": "난 갤럭시",
          "original_form": "난 갤럭시",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100005804.1.4",
          "form": "니도 갤락씨",
          "original_form": "니도 갤락씨",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100005804.1.5",
          "form": "일단 친구들이 폴드를 더 많이사는편임",
          "original_form": "일단 친구들이 폴드를 더 많이사는편임",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100005804.1.6",
          "form": "헐? 진짜로?",
          "original_form": "헐? 진짜로?",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100005804.1.7",
          "form": "폴드 개비싼데",
          "original_form": "폴드 개비싼데",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100005804.1.8",
          "form": "플립의 장점은 일단 가격이",
          "original_form": "플립의 장점은 일단 가격이",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100005804.1.9",
          "form": "싸다는점 이지만",
          "original_form": "싸다는점 이지만",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100005804.1.10",
          "form": "폴더폰 느낌이라 비선호 하더라고",
          "original_form": "폴더폰 느낌이라 비선호 하더라고",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100005804.1.11",
          "form": "싸지는않지만",
          "original_form": "싸지는않지만",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100005804.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100005804.1.13",
          "form": "영화나 폰게임 좋아하는 사람들이",
          "original_form": "영화나 폰게임 좋아하는 사람들이",
          "speaker_id": "2",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100005804.1.14",
          "form": "폴더 약간 아저씨폰이란 이미진뎈ㅋㅋㅋㅋ",
          "original_form": "폴더 약간 아저씨폰이란 이미진뎈ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100005804.1.15",
          "form": "폴드를 더 돟아해",
          "original_form": "폴드를 더 돟아해",
          "speaker_id": "2",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100005804.1.16",
          "form": "폴더가아니라 폴드",
          "original_form": "폴더가아니라 폴드",
          "speaker_id": "1",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100005804.1.17",
          "form": "하긴... 리겜하긴 좋겟다",
          "original_form": "하긴... 리겜하긴 좋겟다",
          "speaker_id": "1",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100005804.1.18",
          "form": "보러간적잇음?",
          "original_form": "보러간적잇음?",
          "speaker_id": "1",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100005804.1.19",
          "form": "솔직히 들고다니기에는 핸드폰 두개라서",
          "original_form": "솔직히 들고다니기에는 핸드폰 두개라서",
          "speaker_id": "2",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100005804.1.20",
          "form": "부담 장난아님",
          "original_form": "부담 장난아님",
          "speaker_id": "2",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100005804.1.21",
          "form": "ㄹㅇㅋㅋㅋㅋㅋ",
          "original_form": "ㄹㅇㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100005804.1.22",
          "form": "그리고 플립3이 그래도 최신기종치고는",
          "original_form": "그리고 플립3이 그래도 최신기종치고는",
          "speaker_id": "2",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100005804.1.23",
          "form": "싼편 이라는 것 같아서",
          "original_form": "싼편 이라는 것 같아서",
          "speaker_id": "2",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100005804.1.24",
          "form": "마저 120이니까",
          "original_form": "마저 120이니까",
          "speaker_id": "1",
          "time": "20210907 12:57"
        },
        {
          "id": "MDRW2100005804.1.25",
          "form": "다른거 새로나오면 할인 안 받아도 요즘은 한 사람 월급이니 어후",
          "original_form": "다른거 새로나오면 할인 안 받아도 요즘은 한 사람 월급이니 어후",
          "speaker_id": "2",
          "time": "20210907 12:57"
        },
        {
          "id": "MDRW2100005804.1.26",
          "form": "마저마저",
          "original_form": "마저마저",
          "speaker_id": "1",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100005804.1.27",
          "form": "너는 바꾼다면 폴드?",
          "original_form": "너는 바꾼다면 폴드?",
          "speaker_id": "1",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100005804.1.28",
          "form": "폴드 장점은\n1. 큰 화면\n2. 듀얼 스크린 가능\n3. 멀티 플레이 가능 이니까",
          "original_form": "폴드 장점은\n1. 큰 화면\n2. 듀얼 스크린 가능\n3. 멀티 플레이 가능 이니까",
          "speaker_id": "2",
          "time": "20210907 12:59"
        },
        {
          "id": "MDRW2100005804.1.29",
          "form": "플립 장점은\n1. 가벼움\n2. 세로부피\n3. 신기종 대비 가격대",
          "original_form": "플립 장점은\n1. 가벼움\n2. 세로부피\n3. 신기종 대비 가격대",
          "speaker_id": "2",
          "time": "20210907 12:59"
        },
        {
          "id": "MDRW2100005804.1.30",
          "form": "이정도니까",
          "original_form": "이정도니까",
          "speaker_id": "2",
          "time": "20210907 12:59"
        },
        {
          "id": "MDRW2100005804.1.31",
          "form": "난 둘 다 싫은편임ㅋㅋ",
          "original_form": "난 둘 다 싫은편임ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:59"
        },
        {
          "id": "MDRW2100005804.1.32",
          "form": "ㄱㅇㄱ 역시 바형이 좋다?",
          "original_form": "ㄱㅇㄱ 역시 바형이 좋다?",
          "speaker_id": "1",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100005804.1.33",
          "form": "난 보니까 인기많을만하더라",
          "original_form": "난 보니까 인기많을만하더라",
          "speaker_id": "1",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100005804.1.34",
          "form": "인기 많을만한 요소는 은근 많음",
          "original_form": "인기 많을만한 요소는 은근 많음",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100005804.1.35",
          "form": "하지만 나는 오히려 갤럭시 21이 플립이나 폴드보다 취향",
          "original_form": "하지만 나는 오히려 갤럭시 21이 플립이나 폴드보다 취향",
          "speaker_id": "2",
          "time": "20210907 13:08"
        }
      ]
