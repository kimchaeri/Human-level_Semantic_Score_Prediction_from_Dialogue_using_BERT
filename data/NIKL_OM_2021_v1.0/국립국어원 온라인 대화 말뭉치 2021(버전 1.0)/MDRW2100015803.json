      "utterance": [
        {
          "id": "MDRW2100015803.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015803.1.2",
          "form": "아니요 님은요?",
          "original_form": "아니요 님은요?",
          "speaker_id": "1",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015803.1.3",
          "form": "안키워요 키우고싶어요 ㅋㅋ",
          "original_form": "안키워요 키우고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015803.1.4",
          "form": "저도요ㅋㅋ",
          "original_form": "저도요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015803.1.5",
          "form": "키운적은 한번도 없으신가요",
          "original_form": "키운적은 한번도 없으신가요",
          "speaker_id": "1",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015803.1.6",
          "form": "넹 ㅜㅜ그냥 초딩때 햄스터?",
          "original_form": "넹 ㅜㅜ그냥 초딩때 햄스터?",
          "speaker_id": "2",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015803.1.7",
          "form": "강아지..고양이는 못키워봤네용 ㅠ",
          "original_form": "강아지..고양이는 못키워봤네용 ㅠ",
          "speaker_id": "2",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015803.1.8",
          "form": "저도 햄스터 키워봤어요",
          "original_form": "저도 햄스터 키워봤어요",
          "speaker_id": "1",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015803.1.9",
          "form": "ㅋㅋㅋ 초딩들의 반려동물",
          "original_form": "ㅋㅋㅋ 초딩들의 반려동물",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015803.1.10",
          "form": "그땐 잘 몰라서 잘 못키웠던것 같아요 전 ㅠ",
          "original_form": "그땐 잘 몰라서 잘 못키웠던것 같아요 전 ㅠ",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015803.1.11",
          "form": "저도 본의아니게 일찍 죽였네요 ㅜ",
          "original_form": "저도 본의아니게 일찍 죽였네요 ㅜ",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015803.1.12",
          "form": "ㅋㅋㅋ 크면서 책임감도 커지고",
          "original_form": "ㅋㅋㅋ 크면서 책임감도 커지고",
          "speaker_id": "2",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015803.1.13",
          "form": "병아리 키워보셨나요?",
          "original_form": "병아리 키워보셨나요?",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015803.1.14",
          "form": "헉 네네 그러고보닠 ㅋㅋ",
          "original_form": "헉 네네 그러고보닠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015803.1.15",
          "form": "그건 닭될때까지키운거같은데",
          "original_form": "그건 닭될때까지키운거같은데",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015803.1.16",
          "form": "어느새 없었죠.............",
          "original_form": "어느새 없었죠.............",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015803.1.17",
