        {
          "id": "MDRW2100003508.1.1",
          "form": "sns에서 올라온 유명한 카페야?",
          "original_form": "sns에서 올라온 유명한 카페야?",
          "speaker_id": "2",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003508.1.2",
          "form": "그것까진 모르겠는데 밖에 테라스가 있더라고",
          "original_form": "그것까진 모르겠는데 밖에 테라스가 있더라고",
          "speaker_id": "1",
          "time": "20210821 00:06"
        },
        {
          "id": "MDRW2100003508.1.3",
          "form": "와플도 정말 맛있대",
          "original_form": "와플도 정말 맛있대",
          "speaker_id": "1",
          "time": "20210821 00:06"
        },
        {
          "id": "MDRW2100003508.1.4",
          "form": "다음에 나랑도 가자!",
          "original_form": "다음에 나랑도 가자!",
          "speaker_id": "2",
          "time": "20210821 00:09"
        },
        {
          "id": "MDRW2100003508.1.5",
          "form": "그 카페는 위치가 어디야?",
          "original_form": "그 카페는 위치가 어디야?",
          "speaker_id": "2",
          "time": "20210821 00:10"
        },
        {
          "id": "MDRW2100003508.1.6",
          "form": "나도모르는데",
          "original_form": "나도모르는데",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100003508.1.7",
          "form": "그렇구나..,  ㅎㅎ",
          "original_form": "그렇구나..,  ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100003508.1.8",
          "form": "오늘 점심은 뭘 먹을까?",
          "original_form": "오늘 점심은 뭘 먹을까?",
          "speaker_id": "2",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100003508.1.9",
          "form": "비빔밥 먹자!",
          "original_form": "비빔밥 먹자!",
          "speaker_id": "1",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100003508.1.10",
          "form": "비빔밥 좋지!",
          "original_form": "비빔밥 좋지!",
          "speaker_id": "2",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100003508.1.11",
          "form": "저녁은 뭐먹을래?",
          "original_form": "저녁은 뭐먹을래?",
          "speaker_id": "2",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100003508.1.12",
          "form": "나는 편의점 가서ㅜ라면 사먹으려고",
          "original_form": "나는 편의점 가서ㅜ라면 사먹으려고",
          "speaker_id": "1",
          "time": "20210822 18:13"
        },
        {
          "id": "MDRW2100003508.1.13",
          "form": "편의점 가는구나",
          "original_form": "편의점 가는구나",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100003508.1.14",
          "form": "과자같은건 안사?",
          "original_form": "과자같은건 안사?",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100003508.1.15",
          "form": "과자는 안사구 빵이랑 라면만 샀어",
          "original_form": "과자는 안사구 빵이랑 라면만 샀어",
          "speaker_id": "1",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100003508.1.16",
          "form": "라면은 어떤거 샀어?",
          "original_form": "라면은 어떤거 샀어?",
          "speaker_id": "2",
          "time": "20210822 19:55"
        },
        {
          "id": "MDRW2100003508.1.17",
          "form": "신라면 샀어! 두개 먹었더니 배부르다^^",
          "original_form": "신라면 샀어! 두개 먹었더니 배부르다^^",
