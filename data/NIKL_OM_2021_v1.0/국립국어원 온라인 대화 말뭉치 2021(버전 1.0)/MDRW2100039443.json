      "utterance": [
        {
          "id": "MDRW2100039443.1.1",
          "form": "점심 뭐먹었어?",
          "original_form": "점심 뭐먹었어?",
          "speaker_id": "1",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039443.1.2",
          "form": "점심은 국밥!",
          "original_form": "점심은 국밥!",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039443.1.3",
          "form": "뭐 먹었어?",
          "original_form": "뭐 먹었어?",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039443.1.4",
          "form": "난 시간없어서 점심은 건너뛰고 지금 카레먹는중",
          "original_form": "난 시간없어서 점심은 건너뛰고 지금 카레먹는중",
          "speaker_id": "1",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039443.1.5",
          "form": "집에서? 아님 밖에서",
          "original_form": "집에서? 아님 밖에서",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100039443.1.6",
          "form": "집!",
          "original_form": "집!",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100039443.1.7",
          "form": "근데 고기떨어져서 고기없는카레 먹는중",
          "original_form": "근데 고기떨어져서 고기없는카레 먹는중",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100039443.1.8",
          "form": "헉..... ㄷㄷ",
          "original_form": "헉..... ㄷㄷ",
          "speaker_id": "2",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039443.1.9",
          "form": "고기없는카레 에반데",
          "original_form": "고기없는카레 에반데",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039443.1.10",
          "form": "낼은 점심도 먹어...",
          "original_form": "낼은 점심도 먹어...",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039443.1.11",
          "form": "낼은 토욜이니까 먹을수있어",
          "original_form": "낼은 토욜이니까 먹을수있어",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039443.1.12",
          "form": "늦잠땜에(..)아침은 안먹을지도 모르지만",
          "original_form": "늦잠땜에(..)아침은 안먹을지도 모르지만",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039443.1.13",
          "form": "ㅋㅋㅋ 오늘은 왜 못먹었어",
          "original_form": "ㅋㅋㅋ 오늘은 왜 못먹었어",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039443.1.14",
          "form": "숙제하느라ㅎ,,,,",
          "original_form": "숙제하느라ㅎ,,,,",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039443.1.15",
          "form": "오늘 못먹은대신 내일은 맛있는거먹어야지",
          "original_form": "오늘 못먹은대신 내일은 맛있는거먹어야지",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039443.1.16",
          "form": "에구... 그랭 맛난거 사먹어",
          "original_form": "에구... 그랭 맛난거 사먹어",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039443.1.17",
          "form": "저녁 먹었니",
          "original_form": "저녁 먹었니",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039443.1.18",
          "form": "난 지금 학원끝나서 부침개먹는중",
          "original_form": "난 지금 학원끝나서 부침개먹는중",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039443.1.19",
          "form": "ㅋㅋ 먹었지",
          "original_form": "ㅋㅋ 먹었지",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039443.1.20",
          "form": "부침개 맛있겠다",
          "original_form": "부침개 맛있겠다",
