      "utterance": [
        {
          "id": "MDRW2100019084.1.1",
          "form": "여행 좋아해요?",
          "original_form": "여행 좋아해요?",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019084.1.2",
          "form": "네 국내해외 다좋아해요 ㅎㅎ",
          "original_form": "네 국내해외 다좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019084.1.3",
          "form": "오오 여행 많이 다니시나봐요??",
          "original_form": "오오 여행 많이 다니시나봐요??",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019084.1.4",
          "form": "코로롱전엔 여행다니는게 취미였어요 ㅋㅋㅋ",
          "original_form": "코로롱전엔 여행다니는게 취미였어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019084.1.5",
          "form": "와! 재밌었겠어요",
          "original_form": "와! 재밌었겠어요",
          "speaker_id": "2",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100019084.1.6",
          "form": "해외는 어디 가보셨어요?",
          "original_form": "해외는 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100019084.1.7",
          "form": "해외는 일본 중국 스페인 미국 이렇게 가봣어요~",
          "original_form": "해외는 일본 중국 스페인 미국 이렇게 가봣어요~",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019084.1.8",
          "form": "다양하게 가보셨네요",
          "original_form": "다양하게 가보셨네요",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019084.1.9",
          "form": "나중에 코로나 끝나면 꼭 일본 여행 가고 싶어요",
          "original_form": "나중에 코로나 끝나면 꼭 일본 여행 가고 싶어요",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019084.1.10",
          "form": "일본 너무좋죠 ㅠ 전 일본엘 혼자도 가봤어요ㅎㅎ",
          "original_form": "일본 너무좋죠 ㅠ 전 일본엘 혼자도 가봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019084.1.11",
          "form": "일본 자주 가보셨나요?!",
          "original_form": "일본 자주 가보셨나요?!",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019084.1.12",
          "form": "네 ㅋㅋ 친구가 일본에 살기도 하고 해서 코로롱 터지기전엔 자주갔어요",
          "original_form": "네 ㅋㅋ 친구가 일본에 살기도 하고 해서 코로롱 터지기전엔 자주갔어요",
          "speaker_id": "1",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019084.1.13",
          "form": "ㅠㅠㅠ 부러워요 편하게 여행할 수 있었을 것같아요",
          "original_form": "ㅠㅠㅠ 부러워요 편하게 여행할 수 있었을 것같아요",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100019084.1.14",
          "form": "맞아요 ㅋㅋ 여행은 혼자보단 친구랑가는게 재밌긴해요",
          "original_form": "맞아요 ㅋㅋ 여행은 혼자보단 친구랑가는게 재밌긴해요",
          "speaker_id": "1",
          "time": "20210827 15:43"
        }
