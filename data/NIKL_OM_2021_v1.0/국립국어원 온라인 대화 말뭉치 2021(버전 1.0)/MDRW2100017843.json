      "utterance": [
        {
          "id": "MDRW2100017843.1.1",
          "form": "티켓팅 해본 적 있어요?",
          "original_form": "티켓팅 해본 적 있어요?",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017843.1.2",
          "form": "네 친구도와주면서 해봤어요 ㅋㅋㅋ",
          "original_form": "네 친구도와주면서 해봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017843.1.3",
          "form": "어떤 티켓이였나요?",
          "original_form": "어떤 티켓이였나요?",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017843.1.4",
          "form": "드림콘서트였던거같아요 ㅋㅋㅋ",
          "original_form": "드림콘서트였던거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017843.1.5",
          "form": "드림콘서트가 뭐에요?",
          "original_form": "드림콘서트가 뭐에요?",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017843.1.6",
          "form": "아이돌들 전부 모여서 나오는 콘서트에요 ㅋㅋㅋ",
          "original_form": "아이돌들 전부 모여서 나오는 콘서트에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017843.1.7",
          "form": "와 대박이다 경쟁이 치열했겠어요",
          "original_form": "와 대박이다 경쟁이 치열했겠어요",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017843.1.8",
          "form": "성공했어요 티켓팅?",
          "original_form": "성공했어요 티켓팅?",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017843.1.9",
          "form": "네 다행히도 성공했어요 ㅋㅋㅋ",
          "original_form": "네 다행히도 성공했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017843.1.10",
          "form": "대단하네요~ 성공은 누가 했어요?",
          "original_form": "대단하네요~ 성공은 누가 했어요?",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017843.1.11",
          "form": "친구란 저랑 둘다 성공했어요 ㅋㅋㅋ",
          "original_form": "친구란 저랑 둘다 성공했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017843.1.12",
          "form": "공연장이 커서 할만하더라구요",
          "original_form": "공연장이 커서 할만하더라구요",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017843.1.13",
          "form": "헐.. 되게 기분 좋았겠어요!",
          "original_form": "헐.. 되게 기분 좋았겠어요!",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017843.1.14",
          "form": "콘서트에서 비매너인 사람 본 적 있어요?",
          "original_form": "콘서트에서 비매너인 사람 본 적 있어요?",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017843.1.15",
          "form": "네 ㅠㅠ 공연 안 보고 핸드폰 잠깐도 아니고 계속 하는 사람들이 있어요",
          "original_form": "네 ㅠㅠ 공연 안 보고 핸드폰 잠깐도 아니고 계속 하는 사람들이 있어요",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017843.1.16",
          "form": "헐 맞아 그런사람 진짜 있어요",
          "original_form": "헐 맞아 그런사람 진짜 있어요",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017843.1.17",
          "form": "도대체 왜 그런걸까요?ㅜㅜ 노이해..",
          "original_form": "도대체 왜 그런걸까요?ㅜㅜ 노이해..",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017843.1.18",
          "form": "진짜 비매너에요 ㅠㅠ",
          "original_form": "진짜 비매너에요 ㅠㅠ",
          "speaker_id": "2",
