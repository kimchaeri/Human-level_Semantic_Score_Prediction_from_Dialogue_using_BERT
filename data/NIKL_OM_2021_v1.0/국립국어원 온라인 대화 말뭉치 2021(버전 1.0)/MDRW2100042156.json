      "utterance": [
        {
          "id": "MDRW2100042156.1.1",
          "form": "연애나 결혼하셨나요?",
          "original_form": "연애나 결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100042156.1.2",
          "form": "저는 중매결혼이요",
          "original_form": "저는 중매결혼이요",
          "speaker_id": "1",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100042156.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100042156.1.4",
          "form": "저 아직 미혼이라ㅋㅋ",
          "original_form": "저 아직 미혼이라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042156.1.5",
          "form": "그랬군요",
          "original_form": "그랬군요",
          "speaker_id": "1",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042156.1.6",
          "form": "그럼 미혼인건가요 비혼인건가요",
          "original_form": "그럼 미혼인건가요 비혼인건가요",
          "speaker_id": "1",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042156.1.7",
          "form": "요새는 비혼이란 단어도 생겼더라구요",
          "original_form": "요새는 비혼이란 단어도 생겼더라구요",
          "speaker_id": "1",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042156.1.8",
          "form": "전 사실.. 비혼에.. 가깝네요ㅋㅋ",
          "original_form": "전 사실.. 비혼에.. 가깝네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042156.1.9",
          "form": "그래요.. 사실 꼭 결혼해야할 필요는 없죠",
          "original_form": "그래요.. 사실 꼭 결혼해야할 필요는 없죠",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042156.1.10",
          "form": "하고 싶으면 하는 거지만",
          "original_form": "하고 싶으면 하는 거지만",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042156.1.11",
          "form": "하기 싫은데 사회적 눈치때문에 할 필요는 없다고 봅니다",
          "original_form": "하기 싫은데 사회적 눈치때문에 할 필요는 없다고 봅니다",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042156.1.12",
          "form": "아ㅜㅜ",
          "original_form": "아ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042156.1.13",
          "form": "감동이에요ㅜ",
          "original_form": "감동이에요ㅜ",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042156.1.14",
          "form": "결혼때문에 항상 엄마 한숨쉬시거든요ㅜㅠ",
          "original_form": "결혼때문에 항상 엄마 한숨쉬시거든요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042156.1.15",
          "form": "아이고 스트레스 받겠네요",
          "original_form": "아이고 스트레스 받겠네요",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042156.1.16",
          "form": "엄마의 마음이려니 하세요",
          "original_form": "엄마의 마음이려니 하세요",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042156.1.17",
          "form": "결혼 잘못하면 엄마가 더  한숨쉬실 거에요",
          "original_form": "결혼 잘못하면 엄마가 더  한숨쉬실 거에요",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042156.1.18",
          "form": "그리고 지금은 비혼이어도 진짜 마음에 드는 사람이 나중에 나타날수 있으니까",
          "original_form": "그리고 지금은 비혼이어도 진짜 마음에 드는 사람이 나중에 나타날수 있으니까",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042156.1.19",
          "form": "제가 항상 그렇게 말씀드려요 혹뗄라다 혹붙인다고ㅋㅋ",
          "original_form": "제가 항상 그렇게 말씀드려요 혹뗄라다 혹붙인다고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042156.1.20",
          "form": "좋은 사람있음 가죠... ㅋㅋ",
          "original_form": "좋은 사람있음 가죠... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042156.1.21",
          "form": "ㅈㅏ녀분들 다 보내셨나요?",
          "original_form": "ㅈㅏ녀분들 다 보내셨나요?",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042156.1.22",
          "form": "둘째만 갔어요",
          "original_form": "둘째만 갔어요",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042156.1.23",
          "form": "첫째는 아마도 비혼인듯 싶습니다 ㅎ",
          "original_form": "첫째는 아마도 비혼인듯 싶습니다 ㅎ",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042156.1.24",
          "form": "동지들이 늘어나면 저는 좋은데ㅎㅎ 저도 남동생 얼른 갔으면 좋겠어요",
          "original_form": "동지들이 늘어나면 저는 좋은데ㅎㅎ 저도 남동생 얼른 갔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042156.1.25",
          "form": "우리 첫째도 동생부터 보내라고 하더라구요",
          "original_form": "우리 첫째도 동생부터 보내라고 하더라구요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042156.1.26",
          "form": "둘째는 본인이 빨리 가고 싶어해서 갔어요",
          "original_form": "둘째는 본인이 빨리 가고 싶어해서 갔어요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042156.1.27",
          "form": "저흰 언니도 안가고ㅋㅋ 부모님이 걱정하실만하죠ㅎㅎ",
          "original_form": "저흰 언니도 안가고ㅋㅋ 부모님이 걱정하실만하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:30"
        }
      ]
