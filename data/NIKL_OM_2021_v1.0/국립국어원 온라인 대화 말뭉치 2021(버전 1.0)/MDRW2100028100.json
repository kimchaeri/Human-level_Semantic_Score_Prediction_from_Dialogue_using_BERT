      "utterance": [
        {
          "id": "MDRW2100028100.1.1",
          "form": "사찰하니까",
          "original_form": "사찰하니까",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028100.1.2",
          "form": "절밥이 생각나네요",
          "original_form": "절밥이 생각나네요",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028100.1.3",
          "form": "해마다",
          "original_form": "해마다",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028100.1.4",
          "form": "부처님오신날엔",
          "original_form": "부처님오신날엔",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028100.1.5",
          "form": "ㅎㅎ 절밥이 글케 맛나다던데",
          "original_form": "ㅎㅎ 절밥이 글케 맛나다던데",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028100.1.6",
          "form": "고향집 옆에 절에 가서",
          "original_form": "고향집 옆에 절에 가서",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028100.1.7",
          "form": "건강식 절밥을 먹곤했습니다",
          "original_form": "건강식 절밥을 먹곤했습니다",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028100.1.8",
          "form": "절에 가면 정신도 건강해지는것 같고",
          "original_form": "절에 가면 정신도 건강해지는것 같고",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028100.1.9",
          "form": "눈도 맑아지고",
          "original_form": "눈도 맑아지고",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028100.1.10",
          "form": "호흡도 편해지고",
          "original_form": "호흡도 편해지고",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028100.1.11",
          "form": "짱입니다",
          "original_form": "짱입니다",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028100.1.12",
          "form": "헉 좋겠다",
          "original_form": "헉 좋겠다",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028100.1.13",
          "form": "머리도 시원하이 깍아야되나 싶기도 하구요",
          "original_form": "머리도 시원하이 깍아야되나 싶기도 하구요",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028100.1.14",
          "form": "저 절밥 먹으러 템플스테이 고민중이잖아요ㅎㅎ",
          "original_form": "저 절밥 먹으러 템플스테이 고민중이잖아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028100.1.15",
          "form": "들어가시게요?",
          "original_form": "들어가시게요?",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028100.1.16",
          "form": "ㅋㅋㅋㄹㄹㄹ",
          "original_form": "ㅋㅋㅋㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028100.1.17",
          "form": "아녀 스님처럼은 못살..",
          "original_form": "아녀 스님처럼은 못살..",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028100.1.18",
          "form": "결혼 되는 종파도 있다니",
          "original_form": "결혼 되는 종파도 있다니",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028100.1.19",
          "form": "아 진짜요,",
          "original_form": "아 진짜요,",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028100.1.20",
          "form": "권해봅니당ㅎㅎ",
          "original_form": "권해봅니당ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028100.1.21",
          "form": "결혼은 안해도 되고",
          "original_form": "결혼은 안해도 되고",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028100.1.22",
          "form": "연애만 하면 좋겠습니다 ㅋㅋ",
          "original_form": "연애만 하면 좋겠습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028100.1.23",
          "form": "ㅋㅋ 여신과 천사만 보시더니 배가 부르셨군요ㅎ",
          "original_form": "ㅋㅋ 여신과 천사만 보시더니 배가 부르셨군요ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028100.1.24",
          "form": "절밥 피가 맑아지는 느낌은 들던가요!",
          "original_form": "절밥 피가 맑아지는 느낌은 들던가요!",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028100.1.25",
          "form": "몸속 독소가 스스슥 빠져나갑니다",
          "original_form": "몸속 독소가 스스슥 빠져나갑니다",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028100.1.26",
          "form": "ㅋㅋㅋ 자동 다이어트군요!!",
          "original_form": "ㅋㅋㅋ 자동 다이어트군요!!",
          "speaker_id": "1",
          "time": "20210902 17:40"
        }
      ]
    }
