      "utterance": [
        {
          "id": "MDRW2100012491.1.1",
          "form": "엄마 오늘 뭐했어?",
          "original_form": "엄마 오늘 뭐했어?",
          "speaker_id": "2",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100012491.1.2",
          "form": "오늘 집안일 했지",
          "original_form": "오늘 집안일 했지",
          "speaker_id": "1",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100012491.1.3",
          "form": "무슨 집안일?",
          "original_form": "무슨 집안일?",
          "speaker_id": "2",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100012491.1.4",
          "form": "설거지랑 빨래",
          "original_form": "설거지랑 빨래",
          "speaker_id": "1",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100012491.1.5",
          "form": "설거지 며칠만에 한거야?",
          "original_form": "설거지 며칠만에 한거야?",
          "speaker_id": "2",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100012491.1.6",
          "form": "3일만에 했지",
          "original_form": "3일만에 했지",
          "speaker_id": "1",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100012491.1.7",
          "form": "빨래할때 내가 새로 산 옷 빨았어?",
          "original_form": "빨래할때 내가 새로 산 옷 빨았어?",
          "speaker_id": "2",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100012491.1.8",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100012491.1.9",
          "form": "오늘은 잠옷만 빨았어",
          "original_form": "오늘은 잠옷만 빨았어",
          "speaker_id": "1",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100012491.1.10",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100012491.1.11",
          "form": "한번에 모아서 해야지",
          "original_form": "한번에 모아서 해야지",
          "speaker_id": "1",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100012491.1.12",
          "form": "오늘은 잠옷만 빠는날이야?",
          "original_form": "오늘은 잠옷만 빠는날이야?",
          "speaker_id": "2",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100012491.1.13",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100012491.1.14",
          "form": "그럼 새 옷은 언제해?",
          "original_form": "그럼 새 옷은 언제해?",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012491.1.15",
          "form": "이번주 내로 해줄게",
          "original_form": "이번주 내로 해줄게",
          "speaker_id": "1",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012491.1.16",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012491.1.17",
          "form": "내일 분리수거할때 같이 가",
          "original_form": "내일 분리수거할때 같이 가",
          "speaker_id": "1",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012491.1.18",
          "form": "그래 이번에도 많아?",
          "original_form": "그래 이번에도 많아?",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012491.1.19",
          "form": "이번에 우리 둘이 가도 2번은가야해",
          "original_form": "이번에 우리 둘이 가도 2번은가야해",
          "speaker_id": "1",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012491.1.20",
          "form": "아정말?",
          "original_form": "아정말?",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012491.1.21",
          "form": "왜이리 많아?",
          "original_form": "왜이리 많아?",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012491.1.22",
          "form": "이번주에 플라스틱이 많이 나왔어",
          "original_form": "이번주에 플라스틱이 많이 나왔어",
          "speaker_id": "1",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012491.1.23",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012491.1.24",
          "form": "이번주는 생수를 마셨잖아",
          "original_form": "이번주는 생수를 마셨잖아",
          "speaker_id": "1",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012491.1.25",
          "form": "아그렇구나",
          "original_form": "아그렇구나",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012491.1.26",
          "form": "그래 내일 같이가",
          "original_form": "그래 내일 같이가",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012491.1.27",
          "form": "오냐",
          "original_form": "오냐",
          "speaker_id": "1",
          "time": "20210824 22:34"
        }
      ]
