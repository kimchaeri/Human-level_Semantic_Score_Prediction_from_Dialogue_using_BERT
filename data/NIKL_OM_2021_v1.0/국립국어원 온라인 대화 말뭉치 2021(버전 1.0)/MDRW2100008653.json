      "utterance": [
        {
          "id": "MDRW2100008653.1.1",
          "form": "혹시 재택근무 하세요?",
          "original_form": "혹시 재택근무 하세요?",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008653.1.2",
          "form": "네 집에서 부업삼아?",
          "original_form": "네 집에서 부업삼아?",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008653.1.3",
          "form": "아 원래 재택근무 하셨군요",
          "original_form": "아 원래 재택근무 하셨군요",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008653.1.4",
          "form": "네 님은요?",
          "original_form": "네 님은요?",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008653.1.5",
          "form": "저는 코로나로 재택근무 하다가",
          "original_form": "저는 코로나로 재택근무 하다가",
          "speaker_id": "1",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008653.1.6",
          "form": "요즘엔 다시 일 나가고 있어요",
          "original_form": "요즘엔 다시 일 나가고 있어요",
          "speaker_id": "1",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008653.1.7",
          "form": "어떠세요?",
          "original_form": "어떠세요?",
          "speaker_id": "2",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008653.1.8",
          "form": "재택근무 할 때는 아무래도 좀 더 편했던 것 같아요",
          "original_form": "재택근무 할 때는 아무래도 좀 더 편했던 것 같아요",
          "speaker_id": "1",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008653.1.9",
          "form": "어떤 면에서??",
          "original_form": "어떤 면에서??",
          "speaker_id": "2",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008653.1.10",
          "form": "일단은 지옥같은 출퇴근길이 없어지니까 ㅎㅎ",
          "original_form": "일단은 지옥같은 출퇴근길이 없어지니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008653.1.11",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "2",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008653.1.12",
          "form": "네 밥도 집밥 먹을 수 있고 ㅎㅎ",
          "original_form": "네 밥도 집밥 먹을 수 있고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008653.1.13",
          "form": "밥값도 좀 절약되겠네요",
          "original_form": "밥값도 좀 절약되겠네요",
          "speaker_id": "2",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008653.1.14",
          "form": "네 편한 차림으로 일할 수 있고 ㅎㅎ",
          "original_form": "네 편한 차림으로 일할 수 있고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008653.1.15",
          "form": "네 아쉽겠어요",
          "original_form": "네 아쉽겠어요",
          "speaker_id": "2",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008653.1.16",
          "form": "저는 좀 많이 아쉽더라구요",
          "original_form": "저는 좀 많이 아쉽더라구요",
          "speaker_id": "1",
          "time": "20210822 17:14"
        }
