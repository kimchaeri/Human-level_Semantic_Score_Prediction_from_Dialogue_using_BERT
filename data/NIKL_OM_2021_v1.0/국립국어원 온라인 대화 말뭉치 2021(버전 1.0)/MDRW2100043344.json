      "utterance": [
        {
          "id": "MDRW2100043344.1.1",
          "form": "아 최근에 올영세일 ㅠㅠ",
          "original_form": "아 최근에 올영세일 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043344.1.2",
          "form": "좀 질렀어요 무료배송이라",
          "original_form": "좀 질렀어요 무료배송이라",
          "speaker_id": "2",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043344.1.3",
          "form": "올영!! 제 친구도 그거 지원금 받음 ㅓ ㄴ 사려고",
          "original_form": "올영!! 제 친구도 그거 지원금 받음 ㅓ ㄴ 사려고",
          "speaker_id": "1",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043344.1.4",
          "form": "벼르고있던데 ㅋㅋㅋㅋ",
          "original_form": "벼르고있던데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043344.1.5",
          "form": "세일 내일까지인데",
          "original_form": "세일 내일까지인데",
          "speaker_id": "2",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043344.1.6",
          "form": "주민등록 마지막자리가 잘 나왔나보네여",
          "original_form": "주민등록 마지막자리가 잘 나왔나보네여",
          "speaker_id": "2",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100043344.1.7",
          "form": "네 오늘 신청날이었어요",
          "original_form": "네 오늘 신청날이었어요",
          "speaker_id": "1",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100043344.1.8",
          "form": "하루만에 나온다고 했담서 낼 사겠다고 하도라구요 ㅋㅋㅋㅋ",
          "original_form": "하루만에 나온다고 했담서 낼 사겠다고 하도라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100043344.1.9",
          "form": "ㅇ이야",
          "original_form": "ㅇ이야",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100043344.1.10",
          "form": "전 8이라 수욜인데",
          "original_form": "전 8이라 수욜인데",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100043344.1.11",
          "form": "내일 하시는군여",
          "original_form": "내일 하시는군여",
          "speaker_id": "1",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043344.1.12",
          "form": "카드사 지금 전산 마비되고 난립니다 이거땜에 ㅋㅋㅋㅋ",
          "original_form": "카드사 지금 전산 마비되고 난립니다 이거땜에 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043344.1.13",
          "form": "홈페이지가 아예 들어가지지가 않는대요",
          "original_form": "홈페이지가 아예 들어가지지가 않는대요",
          "speaker_id": "1",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043344.1.14",
          "form": "ㄱ그거 머머 살수있는거예요 궁금",
          "original_form": "ㄱ그거 머머 살수있는거예요 궁금",
          "speaker_id": "2",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043344.1.15",
          "form": "거의 다 살수있을거요? 근데 대형마트 뭐 애플삼성같은 대형 전자기기",
          "original_form": "거의 다 살수있을거요? 근데 대형마트 뭐 애플삼성같은 대형 전자기기",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043344.1.16",
          "form": "배달 온라인쇼핑",
          "original_form": "배달 온라인쇼핑",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043344.1.17",
          "form": "이런거 빼고 거의 다 될거예요!",
          "original_form": "이런거 빼고 거의 다 될거예요!",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043344.1.18",
          "form": "쇼핑계획 있으세요?",
          "original_form": "쇼핑계획 있으세요?",
          "speaker_id": "2",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043344.1.19",
          "form": "저는 지원금받으면 학원 끊으려구요",
          "original_form": "저는 지원금받으면 학원 끊으려구요",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043344.1.20",
          "form": "학원비가 훨비싸서.. 제돈 더 들어가지만..",
          "original_form": "학원비가 훨비싸서.. 제돈 더 들어가지만..",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043344.1.21",
          "form": "그것도 좋네요",
          "original_form": "그것도 좋네요",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100043344.1.22",
          "form": "갖고 싶은 물건은 따로 없으신가요",
          "original_form": "갖고 싶은 물건은 따로 없으신가요",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043344.1.23",
          "form": "저 지금은 폰이요..",
          "original_form": "저 지금은 폰이요..",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043344.1.24",
          "form": "성능 좋은 새폰...",
          "original_form": "성능 좋은 새폰...",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043344.1.25",
          "form": "아 ㅎㅎ 네네 저두요",
          "original_form": "아 ㅎㅎ 네네 저두요",
          "speaker_id": "2",
          "time": "20210907 18:30"
        }
      ]
    }
  ]
