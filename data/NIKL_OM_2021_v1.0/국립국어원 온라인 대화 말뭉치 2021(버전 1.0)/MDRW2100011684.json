      "utterance": [
        {
          "id": "MDRW2100011684.1.1",
          "form": "저는 아쿠아슈즈 샀는데 신을일이 거의 없었네요ㅜㅜㅋㅋ",
          "original_form": "저는 아쿠아슈즈 샀는데 신을일이 거의 없었네요ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011684.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011684.1.3",
          "form": "어디놀러가서 신어야겠죠?",
          "original_form": "어디놀러가서 신어야겠죠?",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011684.1.4",
          "form": "근데 놀러가지도 않구...음 완전 새것ㅎㅎ",
          "original_form": "근데 놀러가지도 않구...음 완전 새것ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011684.1.5",
          "form": "이러다가 나중에 당근마켓같이 중고거래로...?",
          "original_form": "이러다가 나중에 당근마켓같이 중고거래로...?",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011684.1.6",
          "form": "그럴거같아요ㅎㅎ 아까워라ㅠㅠ",
          "original_form": "그럴거같아요ㅎㅎ 아까워라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011684.1.7",
          "form": "또 구입하신거 있으세요?",
          "original_form": "또 구입하신거 있으세요?",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011684.1.8",
          "form": "저는 최근에 에어팟 왼쪽 한개만 샀어요",
          "original_form": "저는 최근에 에어팟 왼쪽 한개만 샀어요",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011684.1.9",
          "form": "강아지가 제가 잠깐 자리비운사이에ㅠ",
          "original_form": "강아지가 제가 잠깐 자리비운사이에ㅠ",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011684.1.10",
          "form": "씹고 맛보고 즐기고..",
          "original_form": "씹고 맛보고 즐기고..",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011684.1.11",
          "form": "다행히 한쪽만 그래서 중고거래",
          "original_form": "다행히 한쪽만 그래서 중고거래",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011684.1.12",
          "form": "쿨거래했습니다ㅋㅋ",
          "original_form": "쿨거래했습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011684.1.13",
          "form": "저는 에어팟 세탁기에 같이 돌리고 버즈구경하고 있어요ㅜㅜㅋㅋㅋ",
          "original_form": "저는 에어팟 세탁기에 같이 돌리고 버즈구경하고 있어요ㅜㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011684.1.14",
          "form": "아앗ㅠㅠㅠ",
          "original_form": "아앗ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011684.1.15",
          "form": "이참에 새로 바꾸는것도..ㅎㅎ",
          "original_form": "이참에 새로 바꾸는것도..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011684.1.16",
          "form": "이번에 삼성에서 플립3나왔는데",
          "original_form": "이번에 삼성에서 플립3나왔는데",
          "speaker_id": "2",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011684.1.17",
          "form": "혹시 보셨어요?",
          "original_form": "혹시 보셨어요?",
          "speaker_id": "2",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011684.1.18",
          "form": "오 맞아요 이쁘던데ㅎㅎ",
          "original_form": "오 맞아요 이쁘던데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011684.1.19",
          "form": "다른것보다 색깔이ㅋㅋㅋ",
          "original_form": "다른것보다 색깔이ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011684.1.20",
          "form": "저도 가서 봤는데",
          "original_form": "저도 가서 봤는데",
          "speaker_id": "2",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011684.1.21",
          "form": "어후 못고르겠더라구요",
          "original_form": "어후 못고르겠더라구요",
          "speaker_id": "2",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011684.1.22",
          "form": "크림이랑 라벤더",
          "original_form": "크림이랑 라벤더",
          "speaker_id": "2",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011684.1.23",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011684.1.24",
          "form": "그림 실물 어때요? 궁금해요ㅋㅋ",
          "original_form": "그림 실물 어때요? 궁금해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011684.1.25",
          "form": "그린도 되게 고급져요ㅠㅠ",
          "original_form": "그린도 되게 고급져요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011684.1.26",
          "form": "근데 크림vs라벤더",
          "original_form": "근데 크림vs라벤더",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011684.1.27",
          "form": "다들 그거위주로 보더라구요",
          "original_form": "다들 그거위주로 보더라구요",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011684.1.28",
          "form": "이번에 진짜 색깔이..하나씩 다갖구 싶어요ㅋㅋ",
          "original_form": "이번에 진짜 색깔이..하나씩 다갖구 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011684.1.29",
          "form": "장난감같기도하구ㅋㅋ근데 디자인도 괜찮구 기능도 좋으니까",
          "original_form": "장난감같기도하구ㅋㅋ근데 디자인도 괜찮구 기능도 좋으니까",
          "speaker_id": "2",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011684.1.30",
          "form": "다들 많이 구매할거같아요",
          "original_form": "다들 많이 구매할거같아요",
          "speaker_id": "2",
