      "utterance": [
        {
          "id": "MDRW2100039888.1.1",
          "form": "오마이갓 교촌 허니콤보도 아시는",
          "original_form": "오마이갓 교촌 허니콤보도 아시는",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039888.1.2",
          "form": "배우신 분이라고요?",
          "original_form": "배우신 분이라고요?",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039888.1.3",
          "form": "저 기절해요 그거랑 떡볶이 조합",
          "original_form": "저 기절해요 그거랑 떡볶이 조합",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039888.1.4",
          "form": "짱인 거 아시죠",
          "original_form": "짱인 거 아시죠",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039888.1.5",
          "form": "헐.. 네.. 엽떡+허니콤보",
          "original_form": "헐.. 네.. 엽떡+허니콤보",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039888.1.6",
          "form": "진짜 어디내놔도 이기는 조합 아닌가요 ㅠㅠ",
          "original_form": "진짜 어디내놔도 이기는 조합 아닌가요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039888.1.7",
          "form": "갑자기 너무 먹고싶어요",
          "original_form": "갑자기 너무 먹고싶어요",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039888.1.8",
          "form": "저도요... 저 야식 안 먹으려고 양치까지 했는데...",
          "original_form": "저도요... 저 야식 안 먹으려고 양치까지 했는데...",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039888.1.9",
          "form": "^.^ 이거이거 배민을 켜고 싶게 만드네요..",
          "original_form": "^.^ 이거이거 배민을 켜고 싶게 만드네요..",
          "speaker_id": "1",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039888.1.10",
          "form": "내일.. 먹을 계획있으신가요..",
          "original_form": "내일.. 먹을 계획있으신가요..",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039888.1.11",
          "form": "당장 먹고 싶지만 숙녀답게 참고",
          "original_form": "당장 먹고 싶지만 숙녀답게 참고",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039888.1.12",
          "form": "내일 열자마자 시켜먹도록 하겟습니다.",
          "original_form": "내일 열자마자 시켜먹도록 하겟습니다.",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039888.1.13",
          "form": "물론 엽떡은 혼자 못 먹으니가... 편의점 떡볶이로..",
          "original_form": "물론 엽떡은 혼자 못 먹으니가... 편의점 떡볶이로..",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039888.1.14",
          "form": "공감합니다.. 전 언니랑 동생이랑 같이 사는데 엽떡에 허니콤보 먹으먄 딱이에요",
          "original_form": "공감합니다.. 전 언니랑 동생이랑 같이 사는데 엽떡에 허니콤보 먹으먄 딱이에요",
          "speaker_id": "2",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039888.1.15",
          "form": "뭐 먹을땐 역시 최고의 메이트",
          "original_form": "뭐 먹을땐 역시 최고의 메이트",
          "speaker_id": "2",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039888.1.16",
          "form": "우왁!!!! 너무 부러워요 ㅠㅠ !!!!!!!!!!!",
          "original_form": "우왁!!!! 너무 부러워요 ㅠㅠ !!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039888.1.17",
          "form": "저는 집에서 떡볶이 좋아하는게 저뿐이라 -.-",
          "original_form": "저는 집에서 떡볶이 좋아하는게 저뿐이라 -.-",
          "speaker_id": "1",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039888.1.18",
          "form": "항상 거절당합니다요",
          "original_form": "항상 거절당합니다요",
          "speaker_id": "1",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039888.1.19",
          "form": "저ㅠ아주 놀랍게도 떡볶이 안좋아합니다..",
          "original_form": "저ㅠ아주 놀랍게도 떡볶이 안좋아합니다..",
          "speaker_id": "2",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039888.1.20",
          "form": "흐지만 허니콤보가있다면 엽떡은 소스 아닌가요",
          "original_form": "흐지만 허니콤보가있다면 엽떡은 소스 아닌가요",
          "speaker_id": "2",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039888.1.21",
          "form": "아잇!!!! 그것도 맞죠!!!!! 소스 짱인정합니다",
          "original_form": "아잇!!!! 그것도 맞죠!!!!! 소스 짱인정합니다",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039888.1.22",
          "form": "핫 저는 떡볶이가 완전 소울푸드걸랑요..",
          "original_form": "핫 저는 떡볶이가 완전 소울푸드걸랑요..",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039888.1.23",
          "form": "아니 근데 전.. 또 즉석떡볶이는 좋아합니다..",
          "original_form": "아니 근데 전.. 또 즉석떡볶이는 좋아합니다..",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039888.1.24",
          "form": "떡볶이 안좋아한다는 말.. 취소할게요..",
          "original_form": "떡볶이 안좋아한다는 말.. 취소할게요..",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039888.1.25",
          "form": "떡볶이 브랜드 중에 어디 가장 좋아하세요?",
          "original_form": "떡볶이 브랜드 중에 어디 가장 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039888.1.26",
          "form": "저는 태리로제 떡볶이랑 응떡이요!",
          "original_form": "저는 태리로제 떡볶이랑 응떡이요!",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039888.1.27",
          "form": "보면.. 보면 달려들어요",
          "original_form": "보면.. 보면 달려들어요",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039888.1.28",
          "form": "태리로제요? 처음들어요!!",
          "original_form": "태리로제요? 처음들어요!!",
          "speaker_id": "2",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039888.1.29",
          "form": "저는 응떡 로제 먹어봫는데 맛있어서 놀랐어요!!",
          "original_form": "저는 응떡 로제 먹어봫는데 맛있어서 놀랐어요!!",
          "speaker_id": "2",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039888.1.30",
          "form": "제가 장담하는데! 응떡 로제의 3배라고 생각합니닷!!",
          "original_form": "제가 장담하는데! 응떡 로제의 3배라고 생각합니닷!!",
