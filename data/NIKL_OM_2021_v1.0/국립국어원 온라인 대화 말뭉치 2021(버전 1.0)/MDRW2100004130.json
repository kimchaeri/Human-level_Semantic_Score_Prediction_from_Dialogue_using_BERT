      "utterance": [
        {
          "id": "MDRW2100004130.1.1",
          "form": "쇼핑?",
          "original_form": "쇼핑?",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004130.1.2",
          "form": "나는 최근에 후드티를 샀어",
          "original_form": "나는 최근에 후드티를 샀어",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004130.1.3",
          "form": "오 예쁘겠다",
          "original_form": "오 예쁘겠다",
          "speaker_id": "2",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004130.1.4",
          "form": "나는 방금 엄마랑 마라탕 시켜먹었어",
          "original_form": "나는 방금 엄마랑 마라탕 시켜먹었어",
          "speaker_id": "2",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004130.1.5",
          "form": "마라탕 좋지",
          "original_form": "마라탕 좋지",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004130.1.6",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004130.1.7",
          "form": "엄청 오래전에 시켰던 인형이",
          "original_form": "엄청 오래전에 시켰던 인형이",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004130.1.8",
          "form": "배송터미널에 도착했다는 소식을 들었어",
          "original_form": "배송터미널에 도착했다는 소식을 들었어",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004130.1.9",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004130.1.10",
          "form": "좋은 소식이네",
          "original_form": "좋은 소식이네",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004130.1.11",
          "form": "빨리 오면 좋겠다",
          "original_form": "빨리 오면 좋겠다",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004130.1.12",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004130.1.13",
          "form": "나도 인형을 샀는데",
          "original_form": "나도 인형을 샀는데",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004130.1.14",
          "form": "인형 옷 만들어주고싶어서 천을 알아보고 있어",
          "original_form": "인형 옷 만들어주고싶어서 천을 알아보고 있어",
          "speaker_id": "1",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004130.1.15",
          "form": "우와 너 손재주가 좋은거 보다",
          "original_form": "우와 너 손재주가 좋은거 보다",
          "speaker_id": "2",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004130.1.16",
          "form": "약간 좋아",
          "original_form": "약간 좋아",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004130.1.17",
          "form": "사실은 옷을 사주기엔 돈이 별로 없오소",
          "original_form": "사실은 옷을 사주기엔 돈이 별로 없오소",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004130.1.18",
          "form": "천 사서 직접 만들어주려고",
          "original_form": "천 사서 직접 만들어주려고",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004130.1.19",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004130.1.20",
          "form": "그럴수 있죠",
          "original_form": "그럴수 있죠",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004130.1.21",
          "form": "그런옷은 유독 비싸니까",
          "original_form": "그런옷은 유독 비싸니까",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004130.1.22",
          "form": "성공하면 너에게도 나눠줄게",
          "original_form": "성공하면 너에게도 나눠줄게",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004130.1.23",
          "form": "우와 정말?",
          "original_form": "우와 정말?",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004130.1.24",
          "form": "고마워ㅠㅠ",
          "original_form": "고마워ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004130.1.25",
          "form": "멜빵 바지랑 망토중에 뭐가 더 귀여울까?",
          "original_form": "멜빵 바지랑 망토중에 뭐가 더 귀여울까?",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004130.1.26",
          "form": "멜빵 바지가 더 귀여울것 같아",
          "original_form": "멜빵 바지가 더 귀여울것 같아",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004130.1.27",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "1",
          "time": "20210821 15:16"
        }
      ]
