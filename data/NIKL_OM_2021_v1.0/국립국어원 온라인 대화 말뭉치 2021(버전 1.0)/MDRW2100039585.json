      "utterance": [
        {
          "id": "MDRW2100039585.1.1",
          "form": "김볶먹었어 ㅋㅋㅋ",
          "original_form": "김볶먹었어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039585.1.2",
          "form": "너때문에 아침부터",
          "original_form": "너때문에 아침부터",
          "speaker_id": "1",
          "time": "20210904 07:39"
        },
        {
          "id": "MDRW2100039585.1.3",
          "form": "김볶했다",
          "original_form": "김볶했다",
          "speaker_id": "1",
          "time": "20210904 07:40"
        },
        {
          "id": "MDRW2100039585.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ 아침에 김볶 넘어가?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아침에 김볶 넘어가?",
          "speaker_id": "2",
          "time": "20210904 08:22"
        },
        {
          "id": "MDRW2100039585.1.5",
          "form": "5시",
          "original_form": "5시",
          "speaker_id": "1",
          "time": "20210904 10:14"
        },
        {
          "id": "MDRW2100039585.1.6",
          "form": "반",
          "original_form": "반",
          "speaker_id": "1",
          "time": "20210904 10:14"
        },
        {
          "id": "MDRW2100039585.1.7",
          "form": "기상했음",
          "original_form": "기상했음",
          "speaker_id": "1",
          "time": "20210904 10:14"
        },
        {
          "id": "MDRW2100039585.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:14"
        },
        {
          "id": "MDRW2100039585.1.9",
          "form": "와 ㄷㄷㄷ 바른생활",
          "original_form": "와 ㄷㄷㄷ 바른생활",
          "speaker_id": "2",
          "time": "20210904 10:17"
        },
        {
          "id": "MDRW2100039585.1.10",
          "form": "아침에 글케 일찍 일나서 머해? 집안일?",
          "original_form": "아침에 글케 일찍 일나서 머해? 집안일?",
          "speaker_id": "2",
          "time": "20210904 10:17"
        },
        {
          "id": "MDRW2100039585.1.11",
          "form": "폰..하는데요",
          "original_form": "폰..하는데요",
          "speaker_id": "1",
          "time": "20210906 08:15"
        },
        {
          "id": "MDRW2100039585.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 최고의 집안일이지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 최고의 집안일이지",
          "speaker_id": "2",
          "time": "20210906 08:52"
        },
        {
          "id": "MDRW2100039585.1.13",
          "form": "ㅋㅋㅋㅋ진심 누워서 폰하다가",
          "original_form": "ㅋㅋㅋㅋ진심 누워서 폰하다가",
          "speaker_id": "1",
          "time": "20210906 09:06"
        },
        {
          "id": "MDRW2100039585.1.14",
          "form": "밥먹고 다시 누워서 폰하고",
          "original_form": "밥먹고 다시 누워서 폰하고",
          "speaker_id": "1",
          "time": "20210906 09:06"
        },
        {
          "id": "MDRW2100039585.1.15",
          "form": "주말이 맨날 이렇게 흐른다",
          "original_form": "주말이 맨날 이렇게 흐른다",
          "speaker_id": "1",
          "time": "20210906 09:06"
        },
        {
          "id": "MDRW2100039585.1.16",
          "form": "평일에는 주말되면 이거해야지 저거해야지 의욕넘치다가",
          "original_form": "평일에는 주말되면 이거해야지 저거해야지 의욕넘치다가",
          "speaker_id": "2",
          "time": "20210906 09:07"
        },
        {
          "id": "MDRW2100039585.1.17",
          "form": "주말만 되면 겨우겨우 해야하는 집안일만 하고",
          "original_form": "주말만 되면 겨우겨우 해야하는 집안일만 하고",
          "speaker_id": "2",
          "time": "20210906 09:07"
        },
        {
          "id": "MDRW2100039585.1.18",
          "form": "흐지부지 되지 않냐 ㅋㅋㅋㅋ",
          "original_form": "흐지부지 되지 않냐 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:07"
        },
        {
          "id": "MDRW2100039585.1.19",
          "form": "ㅋㅋㅋ너 왜",
          "original_form": "ㅋㅋㅋ너 왜",
          "speaker_id": "1",
          "time": "20210906 09:07"
        },
        {
          "id": "MDRW2100039585.1.20",
          "form": "나 사찰해..",
          "original_form": "나 사찰해..",
          "speaker_id": "1",
          "time": "20210906 09:07"
        },
        {
          "id": "MDRW2100039585.1.21",
          "form": "잉? 아닌대",
          "original_form": "잉? 아닌대",
          "speaker_id": "2",
          "time": "20210906 09:08"
        },
        {
          "id": "MDRW2100039585.1.22",
          "form": "어제는 등산하고",
          "original_form": "어제는 등산하고",
          "speaker_id": "1",
          "time": "20210906 09:08"
        },
        {
          "id": "MDRW2100039585.1.23",
          "form": "오리고기 먹었어 엄마랑",
