      "utterance": [
        {
          "id": "MDRW2100038227.1.1",
          "form": "한국인의 빨리빨리",
          "original_form": "한국인의 빨리빨리",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038227.1.2",
          "form": "맞어 우리나라만 가능한일",
          "original_form": "맞어 우리나라만 가능한일",
          "speaker_id": "1",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038227.1.3",
          "form": "이게 장단점이 있다",
          "original_form": "이게 장단점이 있다",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038227.1.4",
          "form": "맞아 좋을땐 좋은데 한없이",
          "original_form": "맞아 좋을땐 좋은데 한없이",
          "speaker_id": "1",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038227.1.5",
          "form": "ㅋㅋ아침이 여유롭게",
          "original_form": "ㅋㅋ아침이 여유롭게",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038227.1.6",
          "form": "그러게 커피와 사과로 아침식사하네",
          "original_form": "그러게 커피와 사과로 아침식사하네",
          "speaker_id": "1",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100038227.1.7",
          "form": "저때 윤여정은 덜 늙었는데",
          "original_form": "저때 윤여정은 덜 늙었는데",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100038227.1.8",
          "form": "지금과 비교해서 살도 있으시고",
          "original_form": "지금과 비교해서 살도 있으시고",
          "speaker_id": "1",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100038227.1.9",
          "form": "유미 입은 옷 이쁘네",
          "original_form": "유미 입은 옷 이쁘네",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100038227.1.10",
          "form": "엉 에스닉 모양의 옷이다 ㅋㅋ 바지는 저런거 있는데",
          "original_form": "엉 에스닉 모양의 옷이다 ㅋㅋ 바지는 저런거 있는데",
          "speaker_id": "1",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038227.1.11",
          "form": "그림같다 풍경이",
          "original_form": "그림같다 풍경이",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038227.1.12",
          "form": "그러게 저기 나오는 폐허가 된 상가들만 아니면",
          "original_form": "그러게 저기 나오는 폐허가 된 상가들만 아니면",
          "speaker_id": "1",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038227.1.13",
          "form": "좋다 저런게 힐링이지",
          "original_form": "좋다 저런게 힐링이지",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038227.1.14",
          "form": "유미가 세트로 입으니 더 이쁜거 같애",
          "original_form": "유미가 세트로 입으니 더 이쁜거 같애",
          "speaker_id": "1",
          "time": "20210902 01:17"
        }
      ]
