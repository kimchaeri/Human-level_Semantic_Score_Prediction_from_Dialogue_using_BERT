      "utterance": [
        {
          "id": "MDRW2100009959.1.1",
          "form": "오 점심 드시나요 이제!",
          "original_form": "오 점심 드시나요 이제!",
          "speaker_id": "2",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009959.1.2",
          "form": "네 저 점심 먹었어요!",
          "original_form": "네 저 점심 먹었어요!",
          "speaker_id": "1",
          "time": "20210823 13:15"
        },
        {
          "id": "MDRW2100009959.1.3",
          "form": "뭐 드셨나요! 전 아직입니다 히히",
          "original_form": "뭐 드셨나요! 전 아직입니다 히히",
          "speaker_id": "2",
          "time": "20210823 13:25"
        },
        {
          "id": "MDRW2100009959.1.4",
          "form": "전 라면 투움바 해서 먹었어요 아직 안 드셨다면 뭐 드실 건가요?",
          "original_form": "전 라면 투움바 해서 먹었어요 아직 안 드셨다면 뭐 드실 건가요?",
          "speaker_id": "1",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100009959.1.5",
          "form": "오 투움바가 뭐지 찾아봐야겠어요",
          "original_form": "오 투움바가 뭐지 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100009959.1.6",
          "form": "전 콩나물국밥 먹습니다 히히",
          "original_form": "전 콩나물국밥 먹습니다 히히",
          "speaker_id": "2",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100009959.1.7",
          "form": "이제야먹네요",
          "original_form": "이제야먹네요",
          "speaker_id": "2",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100009959.1.8",
          "form": "헐...! 콩나물국밥은 집에서 만든 건가요?",
          "original_form": "헐...! 콩나물국밥은 집에서 만든 건가요?",
          "speaker_id": "1",
          "time": "20210823 14:52"
        },
        {
          "id": "MDRW2100009959.1.9",
          "form": "네 ㅋㅋㅋㅋㅋㅋㅋ 애인이 요리를 잘해가지구",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋㅋ 애인이 요리를 잘해가지구",
          "speaker_id": "2",
          "time": "20210823 19:38"
        },
        {
          "id": "MDRW2100009959.1.10",
          "form": "요리담당입니당",
          "original_form": "요리담당입니당",
          "speaker_id": "2",
          "time": "20210823 19:38"
        },
        {
          "id": "MDRW2100009959.1.11",
          "form": "오ㅋㅋㅋㅋ 멋지네요 저녁은 뭐 드셨어요?",
          "original_form": "오ㅋㅋㅋㅋ 멋지네요 저녁은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100009959.1.12",
          "form": "저녁은 패스입니다 ㅋㅋㅋㅋㅋ",
          "original_form": "저녁은 패스입니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100009959.1.13",
          "form": "너무 많이 먹었어요 흑훅",
          "original_form": "너무 많이 먹었어요 흑훅",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100009959.1.14",
          "form": "오ㅋㅋㅋ 맛있는 거라도 드셨나요",
          "original_form": "오ㅋㅋㅋ 맛있는 거라도 드셨나요",
          "speaker_id": "1",
          "time": "20210823 20:25"
        },
        {
          "id": "MDRW2100009959.1.15",
          "form": "콩나물국밥 먹었어요 ㅋㅋㅋㅋㅋ 근데 이제 밥 두그릇을 곁들인...",
          "original_form": "콩나물국밥 먹었어요 ㅋㅋㅋㅋㅋ 근데 이제 밥 두그릇을 곁들인...",
          "speaker_id": "2",
          "time": "20210823 21:17"
        },
        {
          "id": "MDRW2100009959.1.16",
          "form": "엇ㅋㅋㅋ 점심이랑 똑같은 거 드셨네요",
          "original_form": "엇ㅋㅋㅋ 점심이랑 똑같은 거 드셨네요",
          "speaker_id": "1",
          "time": "20210823 21:18"
        },
        {
          "id": "MDRW2100009959.1.17",
          "form": "전 콩나물국 칼칼하게 먹는 거 좋아해요ㅎㅎ",
          "original_form": "전 콩나물국 칼칼하게 먹는 거 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 21:18"
        },
        {
          "id": "MDRW2100009959.1.18",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 남은거 싹싹",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 남은거 싹싹",
          "speaker_id": "2",
          "time": "20210823 21:18"
        },
        {
          "id": "MDRW2100009959.1.19",
          "form": "저도 칼칼한거 조아해요!",
          "original_form": "저도 칼칼한거 조아해요!",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100009959.1.20",
          "form": "콩나물국은 빨간색이어야해요",
          "original_form": "콩나물국은 빨간색이어야해요",
          "speaker_id": "2",
          "time": "20210823 21:19"
        }
