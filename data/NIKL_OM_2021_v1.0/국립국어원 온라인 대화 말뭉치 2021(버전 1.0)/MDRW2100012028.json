      "utterance": [
        {
          "id": "MDRW2100012028.1.1",
          "form": "퇴근하면서 생각해야죠",
          "original_form": "퇴근하면서 생각해야죠",
          "speaker_id": "2",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100012028.1.2",
          "form": "낙곱새 먹고싶네요 오늘은",
          "original_form": "낙곱새 먹고싶네요 오늘은",
          "speaker_id": "2",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012028.1.3",
          "form": "아 낙곱새 맛있죠ㅠㅠ",
          "original_form": "아 낙곱새 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012028.1.4",
          "form": "곱이가득차있으면 더좋겠어요",
          "original_form": "곱이가득차있으면 더좋겠어요",
          "speaker_id": "1",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012028.1.5",
          "form": "그그그그",
          "original_form": "그그그그",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012028.1.6",
          "form": "갑자기 기억안나네 아",
          "original_form": "갑자기 기억안나네 아",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012028.1.7",
          "form": "감성낙곱새 이거 체인점인데 맛잇더라구요",
          "original_form": "감성낙곱새 이거 체인점인데 맛잇더라구요",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012028.1.8",
          "form": "낙곱새를 여기서만 먹어본게 함정",
          "original_form": "낙곱새를 여기서만 먹어본게 함정",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012028.1.9",
          "form": "아 맛있겟다 진짜ㅠ",
          "original_form": "아 맛있겟다 진짜ㅠ",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012028.1.10",
          "form": "저는이따운동가서 못먹어요",
          "original_form": "저는이따운동가서 못먹어요",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012028.1.11",
          "form": "저대신 많이먹어주세요,,",
          "original_form": "저대신 많이먹어주세요,,",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012028.1.12",
          "form": "오늘은 퇴근을... 언제할지 모르겄네요",
          "original_form": "오늘은 퇴근을... 언제할지 모르겄네요",
          "speaker_id": "2",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012028.1.13",
          "form": "9시엔 가고 싶은데ㅋㅋ",
          "original_form": "9시엔 가고 싶은데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012028.1.14",
          "form": "헥? 일이많은가봐요",
          "original_form": "헥? 일이많은가봐요",
          "speaker_id": "1",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012028.1.15",
          "form": "12시간근무라니",
          "original_form": "12시간근무라니",
          "speaker_id": "1",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012028.1.16",
          "form": "커피좀드세요,,왕피곤하겟다",
          "original_form": "커피좀드세요,,왕피곤하겟다",
          "speaker_id": "1",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012028.1.17",
          "form": "히유 10시반쯤 왔네요",
          "original_form": "히유 10시반쯤 왔네요",
          "speaker_id": "2",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012028.1.18",
          "form": "코로나 터지고 더 바빠졌어요",
          "original_form": "코로나 터지고 더 바빠졌어요",
          "speaker_id": "2",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012028.1.19",
          "form": "커피 좋아하긴 하는데 카페인 중독인가 싶어서 자제합니다ㅋㅋ",
          "original_form": "커피 좋아하긴 하는데 카페인 중독인가 싶어서 자제합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012028.1.20",
          "form": "저도 코로나터지고 더빠진거같아요 방역이다뭐다,,",
          "original_form": "저도 코로나터지고 더빠진거같아요 방역이다뭐다,,",
          "speaker_id": "1",
          "time": "20210825 08:15"
        },
        {
          "id": "MDRW2100012028.1.21",
          "form": "나쁜말 안썼는데,, 식사하셨나요?",
          "original_form": "나쁜말 안썼는데,, 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100012028.1.22",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100012028.1.23",
          "form": "오늘 싸이버거세트 먹었습니다 하하",
          "original_form": "오늘 싸이버거세트 먹었습니다 하하",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100012028.1.24",
          "form": "오랜만에 배에 기름칠했어요",
          "original_form": "오랜만에 배에 기름칠했어요",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100012028.1.25",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100012028.1.26",
          "form": "점심 뭐드셨어요?",
          "original_form": "점심 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100012028.1.27",
          "form": "오늘은 미트볼이랑 양념게장이랑 미역국 나왔어요",
          "original_form": "오늘은 미트볼이랑 양념게장이랑 미역국 나왔어요",
          "speaker_id": "2",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100012028.1.28",
          "form": "미역국에 게장나왓어요? 오늘 육지와 바다의 콜라보네요",
          "original_form": "미역국에 게장나왓어요? 오늘 육지와 바다의 콜라보네요",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100012028.1.29",
          "form": "언제나 바다가 우세해요",
          "original_form": "언제나 바다가 우세해요",
          "speaker_id": "2",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100012028.1.30",
          "form": "미역국에 김먹으면 엄청비리던데",
          "original_form": "미역국에 김먹으면 엄청비리던데",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100012028.1.31",
          "form": "육지가 이겼으면",
          "original_form": "육지가 이겼으면",
          "speaker_id": "2",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100012028.1.32",
          "form": "미역국에 게장 안비린가요",
          "original_form": "미역국에 게장 안비린가요",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100012028.1.33",
          "form": "김이 은근 비리더라구요? 젖으면",
          "original_form": "김이 은근 비리더라구요? 젖으면",
          "speaker_id": "2",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100012028.1.34",
          "form": "마자여 하지만 오늘아침에 김먹고왔어요 하정우처럼 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "마자여 하지만 오늘아침에 김먹고왔어요 하정우처럼 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100012028.1.35",
          "form": "양념 게장이라 그런지 심하지는 않았어요ㅋㅋ",
          "original_form": "양념 게장이라 그런지 심하지는 않았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100012028.1.36",
          "form": "김 좋아하세요? 전 있어도 그만 없어도 그만",
          "original_form": "김 좋아하세요? 전 있어도 그만 없어도 그만",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100012028.1.37",
          "form": "저 김 환장해요 매일먹어도 안질리던데",
          "original_form": "저 김 환장해요 매일먹어도 안질리던데",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100012028.1.38",
          "form": "ㅋㅋㅋㅋ제 동생이네요",
          "original_form": "ㅋㅋㅋㅋ제 동생이네요",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100012028.1.39",
          "form": "반찬 맘에 안 들면 김 꺼내오는데ㅋㅋㅋ",
          "original_form": "반찬 맘에 안 들면 김 꺼내오는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100012028.1.40",
          "form": "코스트코 김 괜찮아요 드셔보세요",
          "original_form": "코스트코 김 괜찮아요 드셔보세요",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100012028.1.41",
          "form": "아 저는 대천김인가 그거맛잇드라구여 짭잘하니",
          "original_form": "아 저는 대천김인가 그거맛잇드라구여 짭잘하니",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100012028.1.42",
          "form": "오늘 티데이 도미노피자 세일이던데 하나먹을까 고민이에여 ㅋㅋㅋ",
          "original_form": "오늘 티데이 도미노피자 세일이던데 하나먹을까 고민이에여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100012028.1.43",
          "form": "대천김도 맛있죠ㅋㅋㅋ 옛날에 삼부자김? 이거 맛있었는데",
          "original_form": "대천김도 맛있죠ㅋㅋㅋ 옛날에 삼부자김? 이거 맛있었는데",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100012028.1.44",
          "form": "아 skt 쓰시는구나 전 이번에 kt로 와가지구 못쓰네료",
          "original_form": "아 skt 쓰시는구나 전 이번에 kt로 와가지구 못쓰네료",
