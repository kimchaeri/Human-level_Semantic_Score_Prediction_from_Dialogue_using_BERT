      "utterance": [
        {
          "id": "MDRW2100005786.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100005786.1.2",
          "form": "믿는 종교 있으신가요?",
          "original_form": "믿는 종교 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100005786.1.3",
          "form": "안녕하새요~",
          "original_form": "안녕하새요~",
          "speaker_id": "2",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100005786.1.4",
          "form": "저는 불교에요",
          "original_form": "저는 불교에요",
          "speaker_id": "1",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100005786.1.5",
          "form": "저는 종교 없습니다!",
          "original_form": "저는 종교 없습니다!",
          "speaker_id": "2",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100005786.1.6",
          "form": "헉!그렇군요",
          "original_form": "헉!그렇군요",
          "speaker_id": "1",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100005786.1.7",
          "form": "종교행사는 가보신적없나요",
          "original_form": "종교행사는 가보신적없나요",
          "speaker_id": "1",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100005786.1.8",
          "form": "부처님오신날이나 햄버거주는교회같은",
          "original_form": "부처님오신날이나 햄버거주는교회같은",
          "speaker_id": "1",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100005786.1.9",
          "form": "앗 어렸을땐 부모님따라서 교회다녔어ㅛ어요!",
          "original_form": "앗 어렸을땐 부모님따라서 교회다녔어ㅛ어요!",
          "speaker_id": "2",
          "time": "20210907 12:01"
        },
        {
          "id": "MDRW2100005786.1.10",
          "form": "역시 다들 그렇게 다녀보는것같아요!",
          "original_form": "역시 다들 그렇게 다녀보는것같아요!",
          "speaker_id": "1",
          "time": "20210907 12:01"
        },
        {
          "id": "MDRW2100005786.1.11",
          "form": "왜 이제 안다니는지 물어봐도 괜찮을까요?",
          "original_form": "왜 이제 안다니는지 물어봐도 괜찮을까요?",
          "speaker_id": "1",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100005786.1.12",
          "form": "아니면 부처님 오신날먹는 비빔밥이야기할께요",
          "original_form": "아니면 부처님 오신날먹는 비빔밥이야기할께요",
          "speaker_id": "1",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100005786.1.13",
          "form": "어릴때 강압적으로 다녔던게 별로 좋은 기억이 아니었어서",
          "original_form": "어릴때 강압적으로 다녔던게 별로 좋은 기억이 아니었어서",
          "speaker_id": "2",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100005786.1.14",
          "form": "성인이 되고난 후에 안간거 같아요!!",
          "original_form": "성인이 되고난 후에 안간거 같아요!!",
          "speaker_id": "2",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100005786.1.15",
          "form": "붛교는 어떤지 물어봐도 괜찮응까요?",
          "original_form": "붛교는 어떤지 물어봐도 괜찮응까요?",
          "speaker_id": "2",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100005786.1.16",
          "form": "아하ㅠ 그쵸..항상 억지로 주말에 갔죠",
          "original_form": "아하ㅠ 그쵸..항상 억지로 주말에 갔죠",
          "speaker_id": "1",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100005786.1.17",
          "form": "불교는 그런건 없는것같아요",
          "original_form": "불교는 그런건 없는것같아요",
          "speaker_id": "1",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100005786.1.18",
          "form": "너무늦게 가거나하는게 아니면 언제가도 괜찮고",
          "original_form": "너무늦게 가거나하는게 아니면 언제가도 괜찮고",
          "speaker_id": "1",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100005786.1.19",
          "form": "그냥 절을 둘러보고나와도 괜찮고 절해도 괜찮고",
          "original_form": "그냥 절을 둘러보고나와도 괜찮고 절해도 괜찮고",
          "speaker_id": "1",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100005786.1.20",
          "form": "저는 그래서 불교를 좋아해요ㅎㅎ",
          "original_form": "저는 그래서 불교를 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100005786.1.21",
          "form": "강요받는느낌이 없는것같아서?",
          "original_form": "강요받는느낌이 없는것같아서?",
          "speaker_id": "1",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100005786.1.22",
          "form": "좋은거같아요",
          "original_form": "좋은거같아요",
          "speaker_id": "2",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100005786.1.23",
          "form": "저는 매주 일욜마다 강제로 가여하는게 너뮤 힘들어ㅛ거든요",
          "original_form": "저는 매주 일욜마다 강제로 가여하는게 너뮤 힘들어ㅛ거든요",
          "speaker_id": "2",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100005786.1.24",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100005786.1.25",
          "form": "저도 저희부모님이 교회여서",
          "original_form": "저도 저희부모님이 교회여서",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100005786.1.26",
          "form": "그랬는데 진짜 그냥 짜증만 났어요",
          "original_form": "그랬는데 진짜 그냥 짜증만 났어요",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100005786.1.27",
          "form": "오!! 특이하시네요! 부모님이 교허ㅣ다니시는데 마라님만 불교이신거군요!",
          "original_form": "오!! 특이하시네요! 부모님이 교허ㅣ다니시는데 마라님만 불교이신거군요!",
          "speaker_id": "2",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100005786.1.28",
          "form": "저 진짜 이해해요 저도 불교로 개종할까했는데",
          "original_form": "저 진짜 이해해요 저도 불교로 개종할까했는데",
          "speaker_id": "2",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100005786.1.29",
          "form": "더이상 얽매이고싶지 않아서 무교라고 하거든요",
          "original_form": "더이상 얽매이고싶지 않아서 무교라고 하거든요",
          "speaker_id": "2",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100005786.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ집에서 그래서 절갔다올때마다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ집에서 그래서 절갔다올때마다",
          "speaker_id": "1",
          "time": "20210907 12:24"
        },
        {
          "id": "MDRW2100005786.1.31",
          "form": "눈치보이긴해요",
          "original_form": "눈치보이긴해요",
          "speaker_id": "1",
          "time": "20210907 12:24"
        },
        {
          "id": "MDRW2100005786.1.32",
          "form": "하긴..무교도 괜찮다고 생각해요!",
          "original_form": "하긴..무교도 괜찮다고 생각해요!",
          "speaker_id": "1",
          "time": "20210907 12:24"
        }
      ]
