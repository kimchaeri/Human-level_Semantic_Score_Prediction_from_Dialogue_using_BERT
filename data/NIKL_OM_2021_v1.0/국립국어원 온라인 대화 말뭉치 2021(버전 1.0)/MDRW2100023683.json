      "utterance": [
        {
          "id": "MDRW2100023683.1.1",
          "form": "그래서 육해공뭐였어요",
          "original_form": "그래서 육해공뭐였어요",
          "speaker_id": "1",
          "time": "20210830 14:38"
        },
        {
          "id": "MDRW2100023683.1.2",
          "form": "맞춰보시죠",
          "original_form": "맞춰보시죠",
          "speaker_id": "2",
          "time": "20210830 14:38"
        },
        {
          "id": "MDRW2100023683.1.3",
          "form": "육군이 제일만만",
          "original_form": "육군이 제일만만",
          "speaker_id": "1",
          "time": "20210830 14:38"
        },
        {
          "id": "MDRW2100023683.1.4",
          "form": "풉",
          "original_form": "풉",
          "speaker_id": "2",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100023683.1.5",
          "form": "해군?",
          "original_form": "해군?",
          "speaker_id": "1",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100023683.1.6",
          "form": "설마 해병대?",
          "original_form": "설마 해병대?",
          "speaker_id": "1",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100023683.1.7",
          "form": "귀신도 잡는다는?",
          "original_form": "귀신도 잡는다는?",
          "speaker_id": "1",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100023683.1.8",
          "form": "제가 그렇게 만만한 사람은 아닙니다",
          "original_form": "제가 그렇게 만만한 사람은 아닙니다",
          "speaker_id": "2",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100023683.1.9",
          "form": "해군이에요",
          "original_form": "해군이에요",
          "speaker_id": "2",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100023683.1.10",
          "form": "해군제복귀여운데",
          "original_form": "해군제복귀여운데",
          "speaker_id": "1",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100023683.1.11",
          "form": "마린",
          "original_form": "마린",
          "speaker_id": "1",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100023683.1.12",
          "form": "어케아시지?",
          "original_form": "어케아시지?",
          "speaker_id": "2",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100023683.1.13",
          "form": "박보검이 해군이자나요",
          "original_form": "박보검이 해군이자나요",
          "speaker_id": "1",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100023683.1.14",
          "form": "신기하네 잘 모르던데",
          "original_form": "신기하네 잘 모르던데",
          "speaker_id": "2",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100023683.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100023683.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100023683.1.17",
          "form": "역시.. 연옌의 중요성",
          "original_form": "역시.. 연옌의 중요성",
          "speaker_id": "2",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100023683.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ보거미오빠때매 알죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ보거미오빠때매 알죠",
          "speaker_id": "1",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100023683.1.19",
          "form": "이번에 잘나온 사진가지고 말 많던데요",
          "original_form": "이번에 잘나온 사진가지고 말 많던데요",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023683.1.20",
          "form": "머리길다 머다 하면서",
          "original_form": "머리길다 머다 하면서",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023683.1.21",
          "form": "해군은 근데 원래 윗머리 긴데",
          "original_form": "해군은 근데 원래 윗머리 긴데",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023683.1.22",
          "form": "물에 빠졌을 때 머리끄댕이 잡고 건져올려서",
          "original_form": "물에 빠졌을 때 머리끄댕이 잡고 건져올려서",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023683.1.23",
          "form": "알아요 그거 물에빠지면 머리잡으려고 그런다고햇던데",
          "original_form": "알아요 그거 물에빠지면 머리잡으려고 그런다고햇던데",
          "speaker_id": "1",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023683.1.24",
          "form": "올",
          "original_form": "올",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023683.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023683.1.26",
          "form": "고수시네요?",
          "original_form": "고수시네요?",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023683.1.27",
          "form": "의왼데",
          "original_form": "의왼데",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023683.1.28",
          "form": "언제지 계룡대 군축제 햇을때 한번갓는데",
          "original_form": "언제지 계룡대 군축제 햇을때 한번갓는데",
          "speaker_id": "1",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023683.1.29",
          "form": "가서 연예인많이보고옴요",
          "original_form": "가서 연예인많이보고옴요",
          "speaker_id": "1",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023683.1.30",
          "form": "군필여고생 이런거 아니죠?",
          "original_form": "군필여고생 이런거 아니죠?",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100023683.1.31",
          "form": "계룡대. 별들의 성지라던",
          "original_form": "계룡대. 별들의 성지라던",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100023683.1.32",
          "form": "대성이랑 태양 ㅋㅋㅋㅋㅋ",
          "original_form": "대성이랑 태양 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100023683.1.33",
          "form": "ㅋㅋㅋㅋ아 둘이 콘서트",
          "original_form": "ㅋㅋㅋㅋ아 둘이 콘서트",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100023683.1.34",
          "form": "둘이콘서트 오지게하고",
          "original_form": "둘이콘서트 오지게하고",
          "speaker_id": "1",
          "time": "20210830 14:44"
        },
        {
          "id": "MDRW2100023683.1.35",
          "form": "고경표랑 누구더라 또",
          "original_form": "고경표랑 누구더라 또",
          "speaker_id": "1",
          "time": "20210830 14:44"
        },
        {
          "id": "MDRW2100023683.1.36",
          "form": "주원?도 보고",
          "original_form": "주원?도 보고",
          "speaker_id": "1",
          "time": "20210830 14:44"
        },
        {
          "id": "MDRW2100023683.1.37",
          "form": "완전 핫할때 보고오셨네요",
          "original_form": "완전 핫할때 보고오셨네요",
          "speaker_id": "2",
          "time": "20210830 14:44"
        }
      ]
