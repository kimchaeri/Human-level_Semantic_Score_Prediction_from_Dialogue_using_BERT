      "utterance": [
        {
          "id": "MDRW2100007017.1.1",
          "form": "핫딜은 스피드 ㅋㅋㅋ",
          "original_form": "핫딜은 스피드 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007017.1.2",
          "form": "그렇지. 핫딜은 스피드ㅋ",
          "original_form": "그렇지. 핫딜은 스피드ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:24"
        },
        {
          "id": "MDRW2100007017.1.3",
          "form": "조금 이따 또 상품권사야지ㅋ이벤트ㅋ",
          "original_form": "조금 이따 또 상품권사야지ㅋ이벤트ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:25"
        },
        {
          "id": "MDRW2100007017.1.4",
          "form": "응ㅋㅋ 열시가 다가온드아ㅎㅎ",
          "original_form": "응ㅋㅋ 열시가 다가온드아ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007017.1.5",
          "form": "응응 마루시계틀어놓음ㅋㅋ",
          "original_form": "응응 마루시계틀어놓음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007017.1.6",
          "form": "알람도 울렸고 ㅋㅋ",
          "original_form": "알람도 울렸고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007017.1.7",
          "form": "알람. 너무 일찍이네ㅎㅎ 나는2분전ㅋㅋ 넘일찍하면 까먹드라ㅎ 오늘도 십퍼뜨길~~~~",
          "original_form": "알람. 너무 일찍이네ㅎㅎ 나는2분전ㅋㅋ 넘일찍하면 까먹드라ㅎ 오늘도 십퍼뜨길~~~~",
          "speaker_id": "1",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007017.1.8",
          "form": "응응ㅋㅋㅋㅋㅋ나도 나도 ㅋㅋ",
          "original_form": "응응ㅋㅋㅋㅋㅋ나도 나도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007017.1.9",
          "form": "미리 해놔야지 ㅋㅋ 할수있다.ㅋ",
          "original_form": "미리 해놔야지 ㅋㅋ 할수있다.ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007017.1.10",
          "form": "쇼핑을ㅋㅋ",
          "original_form": "쇼핑을ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007017.1.11",
          "form": "선착순 구매는 어려워ㅎ",
          "original_form": "선착순 구매는 어려워ㅎ",
          "speaker_id": "1",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007017.1.12",
          "form": "성공했다.ㅋㅋ",
          "original_form": "성공했다.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007017.1.13",
          "form": "언제 누를지 고민이구나ㅋㅋ",
          "original_form": "언제 누를지 고민이구나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007017.1.14",
          "form": "너는 선착순 구매 성공했어?",
          "original_form": "너는 선착순 구매 성공했어?",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007017.1.15",
          "form": "에잇 어제십퍼나와서 오늘바로 뽑으니 천원이네",
          "original_form": "에잇 어제십퍼나와서 오늘바로 뽑으니 천원이네",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007017.1.16",
          "form": "다른사람들은 다 금방 뽑아도 좋은거 나오던데",
          "original_form": "다른사람들은 다 금방 뽑아도 좋은거 나오던데",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007017.1.17",
          "form": "나도 바로 뽑으니 1000원이더라ㅋ",
          "original_form": "나도 바로 뽑으니 1000원이더라ㅋ",
          "speaker_id": "2",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007017.1.18",
          "form": "카페 후기를 보고 누르려무나",
          "original_form": "카페 후기를 보고 누르려무나",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007017.1.19",
          "form": "오늘?",
          "original_form": "오늘?",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007017.1.20",
          "form": "어제?",
          "original_form": "어제?",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007017.1.21",
          "form": "그래서 고민하다가 ㅋㅋㅋ 지금 누를타이밍보고있어",
          "original_form": "그래서 고민하다가 ㅋㅋㅋ 지금 누를타이밍보고있어",
          "speaker_id": "2",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007017.1.22",
          "form": "어제 1000원ㅋ",
          "original_form": "어제 1000원ㅋ",
          "speaker_id": "2",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007017.1.23",
          "form": "그래도십분안에는 눌러야됨ㅋ",
          "original_form": "그래도십분안에는 눌러야됨ㅋ",
          "speaker_id": "1",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007017.1.24",
          "form": "천원이다 히히",
          "original_form": "천원이다 히히",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007017.1.25",
          "form": "쇼핑은 어려워.ㅋㅋ이벤트도 어렵고ㅋㅋ",
          "original_form": "쇼핑은 어려워.ㅋㅋ이벤트도 어렵고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007017.1.26",
          "form": "에이 왜 벌써눌러써ㅡ",
          "original_form": "에이 왜 벌써눌러써ㅡ",
          "speaker_id": "1",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007017.1.27",
          "form": "친구도 그쯤 뽑기했는데 십퍼래 우왕",
          "original_form": "친구도 그쯤 뽑기했는데 십퍼래 우왕",
          "speaker_id": "1",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007017.1.28",
          "form": "운이다 운",
          "original_form": "운이다 운",
          "speaker_id": "1",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007017.1.29",
          "form": "와 10퍼ㅋㅋ좋다 부럽다ㅋ",
          "original_form": "와 10퍼ㅋㅋ좋다 부럽다ㅋ",
          "speaker_id": "2",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100007017.1.30",
          "form": "그니까ㅎㅎ 봤는데 백만원 뽑으면 제세22만원이래",
          "original_form": "그니까ㅎㅎ 봤는데 백만원 뽑으면 제세22만원이래",
          "speaker_id": "1",
          "time": "20210822 10:08"
        },
        {
          "id": "MDRW2100007017.1.31",
          "form": "내도 좋으니 ㅋㅋ한번만 떳으면ㅋㅋ 그걸로상품권이랑 스마일 캐시 왕창 사게",
          "original_form": "내도 좋으니 ㅋㅋ한번만 떳으면ㅋㅋ 그걸로상품권이랑 스마일 캐시 왕창 사게",
          "speaker_id": "1",
          "time": "20210822 10:08"
        },
        {
          "id": "MDRW2100007017.1.32",
          "form": "그러니깐 좋다ㅋㅋㅋ",
          "original_form": "그러니깐 좋다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
