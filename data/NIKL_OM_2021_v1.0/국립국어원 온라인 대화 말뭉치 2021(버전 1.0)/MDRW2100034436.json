      "utterance": [
        {
          "id": "MDRW2100034436.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100034436.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100034436.1.3",
          "form": "오늘 아침드셧나여?",
          "original_form": "오늘 아침드셧나여?",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100034436.1.4",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100034436.1.5",
          "form": "그래서 지금 많이 배고파요 빨리 점심 먹으러구요",
          "original_form": "그래서 지금 많이 배고파요 빨리 점심 먹으러구요",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100034436.1.6",
          "form": "아하~뭐드실거에여??",
          "original_form": "아하~뭐드실거에여??",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100034436.1.7",
          "form": "빵에다가 잼 발라먹으려구요",
          "original_form": "빵에다가 잼 발라먹으려구요",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100034436.1.8",
          "form": "name1님은 점심 뭐 먹으실거에요?",
          "original_form": "&name1&님은 점심 뭐 먹으실거에요?",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100034436.1.9",
          "form": "아하! 저는 아마 찜닭먹을까 고민중이에요~",
          "original_form": "아하! 저는 아마 찜닭먹을까 고민중이에요~",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100034436.1.10",
          "form": "찜닭!!!!",
          "original_form": "찜닭!!!!",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100034436.1.11",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100034436.1.12",
          "form": "아 찜닭 갑자기 땡기네요",
          "original_form": "아 찜닭 갑자기 땡기네요",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100034436.1.13",
          "form": "닭갈비도 땡겨요",
          "original_form": "닭갈비도 땡겨요",
          "speaker_id": "2",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100034436.1.14",
          "form": "당면 추가해서 먹는 닭갈비 맛있죠",
          "original_form": "당면 추가해서 먹는 닭갈비 맛있죠",
          "speaker_id": "2",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100034436.1.15",
          "form": "맞아요 닭은 천상의요리재료에요 뭘해먹어도평타이상 ㅜㅜ",
          "original_form": "맞아요 닭은 천상의요리재료에요 뭘해먹어도평타이상 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100034436.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100034436.1.17",
          "form": "사람들이 치킨에 열광하는 이유가 있어요",
          "original_form": "사람들이 치킨에 열광하는 이유가 있어요",
          "speaker_id": "2",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100034436.1.18",
          "form": "닭가슴살도 단백질 섭취하기 좋고요",
          "original_form": "닭가슴살도 단백질 섭취하기 좋고요",
          "speaker_id": "2",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100034436.1.19",
          "form": "그쵸!!",
          "original_form": "그쵸!!",
          "speaker_id": "1",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100034436.1.20",
          "form": "생닭으로 치킨파는집이 늘었으면좋겠어요",
          "original_form": "생닭으로 치킨파는집이 늘었으면좋겠어요",
          "speaker_id": "1",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100034436.1.21",
          "form": "하긴 요새 치킨집이 많아서",
          "original_form": "하긴 요새 치킨집이 많아서",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034436.1.22",
          "form": "그런 제품이 나올것 같기도해요",
          "original_form": "그런 제품이 나올것 같기도해요",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034436.1.23",
          "form": "맞아요ㅜ 냉동닭 상태안좋은것들 넘 많아요",
          "original_form": "맞아요ㅜ 냉동닭 상태안좋은것들 넘 많아요",
