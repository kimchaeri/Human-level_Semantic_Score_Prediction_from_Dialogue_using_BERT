        {
          "id": "MDRW2100037966.1.1",
          "form": "저녁드셧어요?",
          "original_form": "저녁드셧어요?",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100037966.1.2",
          "form": "아직이요 이제 먹으려구요",
          "original_form": "아직이요 이제 먹으려구요",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100037966.1.3",
          "form": "뭐 드실거예요?",
          "original_form": "뭐 드실거예요?",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100037966.1.4",
          "form": "소시지 야채볶음이랑 반찬 이것저것 해서 먹을 계획이에요 ㅎㅎ",
          "original_form": "소시지 야채볶음이랑 반찬 이것저것 해서 먹을 계획이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100037966.1.5",
          "form": "소시지 반찬!!",
          "original_form": "소시지 반찬!!",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100037966.1.6",
          "form": "늘 보장되는 맛잇는 맛 ㅋㅋㅋㅋㅋ",
          "original_form": "늘 보장되는 맛잇는 맛 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100037966.1.7",
          "form": "ㅋㅋ 식사하셨어요?",
          "original_form": "ㅋㅋ 식사하셨어요?",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100037966.1.8",
          "form": "네 먹엇어요",
          "original_form": "네 먹엇어요",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100037966.1.9",
          "form": "전 비빔밥먹엇어요",
          "original_form": "전 비빔밥먹엇어요",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100037966.1.10",
          "form": "비빔밥도 맛있죠ㅋㅋ 전 이제 먹고 치웠어요",
          "original_form": "비빔밥도 맛있죠ㅋㅋ 전 이제 먹고 치웠어요",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100037966.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ전 아까 일찍 먹엇더니 벌써 입이 심심하네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전 아까 일찍 먹엇더니 벌써 입이 심심하네요",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100037966.1.12",
          "form": "전 일단 껌부터 씹고 있어요ㅎㅎ",
          "original_form": "전 일단 껌부터 씹고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100037966.1.13",
          "form": "전 과자먹고싶어서 손이 드릉드릉하네요 ㅋㅋㅋㅋ",
          "original_form": "전 과자먹고싶어서 손이 드릉드릉하네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100037966.1.14",
          "form": "어떤 과자요?ㅋㅋ",
          "original_form": "어떤 과자요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100037966.1.15",
          "form": "포테토칩이요 ㅋㅋㅋㅋ",
          "original_form": "포테토칩이요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100037966.1.16",
          "form": "ㅋㅋ 감자칩 맛있죠. 생각하니까 저도 침나오네요ㅋㅋ",
          "original_form": "ㅋㅋ 감자칩 맛있죠. 생각하니까 저도 침나오네요ㅋㅋ",
