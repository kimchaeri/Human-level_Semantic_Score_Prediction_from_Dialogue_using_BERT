        },
        {
          "id": "MDRW2100025917.1.3",
          "form": "가족이 몇명인가요",
          "original_form": "가족이 몇명인가요",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100025917.1.4",
          "form": "저는 결혼해서 이제 2명이네요",
          "original_form": "저는 결혼해서 이제 2명이네요",
          "speaker_id": "1",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100025917.1.5",
          "form": "동생이 3명이신가요",
          "original_form": "동생이 3명이신가요",
          "speaker_id": "1",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100025917.1.6",
          "form": "아니요 동생은2명입니다",
          "original_form": "아니요 동생은2명입니다",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100025917.1.7",
          "form": "저포함 3명입니다",
          "original_form": "저포함 3명입니다",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100025917.1.8",
          "form": "아 그러네요 맞이세요?",
          "original_form": "아 그러네요 맞이세요?",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100025917.1.9",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100025917.1.10",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100025917.1.11",
          "form": "저도 남동생 하나 있습니다",
          "original_form": "저도 남동생 하나 있습니다",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100025917.1.12",
          "form": "귀염둥이 동생 말이군요",
          "original_form": "귀염둥이 동생 말이군요",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100025917.1.13",
          "form": "대학 잘 다니고 있나요",
          "original_form": "대학 잘 다니고 있나요",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100025917.1.14",
          "form": "이제 졸업반입니다 ㅋ",
          "original_form": "이제 졸업반입니다 ㅋ",
          "speaker_id": "1",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100025917.1.15",
          "form": "대학원준비한다네요",
          "original_form": "대학원준비한다네요",
          "speaker_id": "1",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100025917.1.16",
          "form": "멋지네요",
          "original_form": "멋지네요",
          "speaker_id": "2",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100025917.1.17",
          "form": "대학원 어디가나요",
          "original_form": "대학원 어디가나요",
          "speaker_id": "2",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100025917.1.18",
          "form": "아마 경북대로 갈거같아요 ㅋㅋ",
          "original_form": "아마 경북대로 갈거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100025917.1.19",
          "form": "취업이되야할켐데 말이죠",
          "original_form": "취업이되야할켐데 말이죠",
          "speaker_id": "1",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100025917.1.20",
          "form": "동생이라 더 신경이쓰이메요",
          "original_form": "동생이라 더 신경이쓰이메요",
          "speaker_id": "1",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100025917.1.21",
          "form": "당연히 동생일이라 더 신경쓰이겠네요",
          "original_form": "당연히 동생일이라 더 신경쓰이겠네요",
          "speaker_id": "2",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100025917.1.22",
          "form": "그래도 블루오션이라 다행이네요",
          "original_form": "그래도 블루오션이라 다행이네요",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100025917.1.23",
          "form": "막내동생은 어떻게됐나요",
          "original_form": "막내동생은 어떻게됐나요",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100025917.1.24",
          "form": "합격해서 다니고 있어요",
          "original_form": "합격해서 다니고 있어요",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100025917.1.25",
          "form": "밥벌이 하네요",
          "original_form": "밥벌이 하네요",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100025917.1.26",
          "form": "유와 어디합격했나요??",
          "original_form": "유와 어디합격했나요??",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100025917.1.27",
          "form": "경주요",
          "original_form": "경주요",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100025917.1.28",
          "form": "말안했었나요",
          "original_form": "말안했었나요",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100025917.1.29",
          "form": "공무원아요?",
          "original_form": "공무원아요?",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100025917.1.30",
