      "utterance": [
        {
          "id": "MDRW2100022194.1.1",
          "form": "연예인으로 치면요?  누구 좋아하시나요",
          "original_form": "연예인으로 치면요?  누구 좋아하시나요",
          "speaker_id": "1",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100022194.1.2",
          "form": "지금 생각해보니까 비슷한거같기도하고?",
          "original_form": "지금 생각해보니까 비슷한거같기도하고?",
          "speaker_id": "2",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022194.1.3",
          "form": "막 엄청 팬이고 그런건 아니지만",
          "original_form": "막 엄청 팬이고 그런건 아니지만",
          "speaker_id": "2",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022194.1.4",
          "form": "김지석",
          "original_form": "김지석",
          "speaker_id": "2",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022194.1.5",
          "form": "약간 지적인 이미지아닌가요",
          "original_form": "약간 지적인 이미지아닌가요",
          "speaker_id": "1",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022194.1.6",
          "form": "이승기",
          "original_form": "이승기",
          "speaker_id": "2",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022194.1.7",
          "form": "아 맞아요ㅋㅋㅋ",
          "original_form": "아 맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022194.1.8",
          "form": "근데 캐릭터때문에 그런지",
          "original_form": "근데 캐릭터때문에 그런지",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022194.1.9",
          "form": "연예인말고 현실에서는",
          "original_form": "연예인말고 현실에서는",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022194.1.10",
          "form": "기생오라비스타일을",
          "original_form": "기생오라비스타일을",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022194.1.11",
          "form": "만났어서...ㅎ",
          "original_form": "만났어서...ㅎ",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022194.1.12",
          "form": "아하..",
          "original_form": "아하..",
          "speaker_id": "1",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022194.1.13",
          "form": "말씀하신 분들 보시면",
          "original_form": "말씀하신 분들 보시면",
          "speaker_id": "1",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022194.1.14",
          "form": "지적인 이미지라 음.. 부정하신 이유를 아시겠어요",
          "original_form": "지적인 이미지라 음.. 부정하신 이유를 아시겠어요",
          "speaker_id": "1",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022194.1.15",
          "form": "요즘 즐겨보는 프로 있으세요?",
          "original_form": "요즘 즐겨보는 프로 있으세요?",
          "speaker_id": "1",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022194.1.16",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "2",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022194.1.17",
          "form": "네ㅎ",
          "original_form": "네ㅎ",
          "speaker_id": "1",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022194.1.18",
          "form": "드라마는 거의다 보고",
          "original_form": "드라마는 거의다 보고",
          "speaker_id": "2",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022194.1.19",
          "form": "식스센스 봐요ㅎ",
          "original_form": "식스센스 봐요ㅎ",
          "speaker_id": "2",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100022194.1.20",
          "form": "최근에 뭐 보셨나요",
          "original_form": "최근에 뭐 보셨나요",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100022194.1.21",
          "form": "아 저 그 남지현",
          "original_form": "아 저 그 남지현",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100022194.1.22",
          "form": "나오는편봤어요",
          "original_form": "나오는편봤어요",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100022194.1.23",
          "form": "티비돌리는데 하길래 봤는데 재밌더라구요",
          "original_form": "티비돌리는데 하길래 봤는데 재밌더라구요",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100022194.1.24",
          "form": "엊그저께 한거요ㅋㅋㅋ",
          "original_form": "엊그저께 한거요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022194.1.25",
          "form": "놀면 뭐하니도 보셨어요?",
          "original_form": "놀면 뭐하니도 보셨어요?",
          "speaker_id": "2",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022194.1.26",
          "form": "그 이번주꺼는 못보고",
          "original_form": "그 이번주꺼는 못보고",
          "speaker_id": "1",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022194.1.27",
          "form": "저번주에 탁구치는거 봤어요ㅋㅋㅋㅋ",
          "original_form": "저번주에 탁구치는거 봤어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022194.1.28",
          "form": "신유빈선수 나온거ㅋㅋㅋ",
          "original_form": "신유빈선수 나온거ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022194.1.29",
          "form": "전동탁구채들고있길래 웃겨가지고 봤어요",
          "original_form": "전동탁구채들고있길래 웃겨가지고 봤어요",
          "speaker_id": "1",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022194.1.30",
          "form": "돌아가면서",
          "original_form": "돌아가면서",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022194.1.31",
          "form": "탁구치는거 너무 웃겼어요",
          "original_form": "탁구치는거 너무 웃겼어요",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022194.1.32",
          "form": "그거 나오자마자ㅋㅋㅋ",
          "original_form": "그거 나오자마자ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022194.1.33",
          "form": "너무어이가없어서ㅋㅋㅋㅋㅋ",
          "original_form": "너무어이가없어서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:58"
        }
      ]
    }
