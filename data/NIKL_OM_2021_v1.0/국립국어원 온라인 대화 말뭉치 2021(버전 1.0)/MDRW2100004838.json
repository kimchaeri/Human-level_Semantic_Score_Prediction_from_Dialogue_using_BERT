      "utterance": [
        {
          "id": "MDRW2100004838.1.1",
          "form": "네 좋아하죠!",
          "original_form": "네 좋아하죠!",
          "speaker_id": "2",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004838.1.2",
          "form": "어디요?",
          "original_form": "어디요?",
          "speaker_id": "1",
          "time": "20210821 18:55"
        },
        {
          "id": "MDRW2100004838.1.3",
          "form": "제주도나 동해 같은 바닷가요!",
          "original_form": "제주도나 동해 같은 바닷가요!",
          "speaker_id": "2",
          "time": "20210821 18:59"
        },
        {
          "id": "MDRW2100004838.1.4",
          "form": "바다 좋죠",
          "original_form": "바다 좋죠",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004838.1.5",
          "form": "맞아요! 바다 맑은거 보면 참 마음이 편안해져요!",
          "original_form": "맞아요! 바다 맑은거 보면 참 마음이 편안해져요!",
          "speaker_id": "2",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004838.1.6",
          "form": "시원하구요",
          "original_form": "시원하구요",
          "speaker_id": "1",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004838.1.7",
          "form": "바닷바람 엄청 시원하죠!",
          "original_form": "바닷바람 엄청 시원하죠!",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004838.1.8",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004838.1.9",
          "form": "그리고 개인적으로",
          "original_form": "그리고 개인적으로",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100004838.1.10",
          "form": "트래킹도 좋아해요!",
          "original_form": "트래킹도 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100004838.1.11",
          "form": "것도 좋죠",
          "original_form": "것도 좋죠",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100004838.1.12",
          "form": "걸으면서 주변 경치 구경하는게",
          "original_form": "걸으면서 주변 경치 구경하는게",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100004838.1.13",
          "form": "낭만 넘치고 좋더라고요!",
          "original_form": "낭만 넘치고 좋더라고요!",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100004838.1.14",
          "form": "운동도 되고 구경도 하고 일석이조",
          "original_form": "운동도 되고 구경도 하고 일석이조",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100004838.1.15",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100004838.1.16",
          "form": "식물도 구경하고",
          "original_form": "식물도 구경하고",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100004838.1.17",
          "form": "지나가는 새나 동물도 구경하고",
          "original_form": "지나가는 새나 동물도 구경하고",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100004838.1.18",
          "form": "좋더라구요!",
          "original_form": "좋더라구요!",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100004838.1.19",
          "form": "네 자연이죠",
          "original_form": "네 자연이죠",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100004838.1.20",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210826 23:15"
        }
      ]
