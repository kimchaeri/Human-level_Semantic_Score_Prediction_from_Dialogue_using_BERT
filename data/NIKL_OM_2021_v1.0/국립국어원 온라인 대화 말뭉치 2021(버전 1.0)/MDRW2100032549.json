      "utterance": [
        {
          "id": "MDRW2100032549.1.1",
          "form": "저는 이제 팩 붙였어요",
          "original_form": "저는 이제 팩 붙였어요",
          "speaker_id": "1",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100032549.1.2",
          "form": "마스크팩인가요?",
          "original_form": "마스크팩인가요?",
          "speaker_id": "2",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100032549.1.3",
          "form": "네네 일리 동백오일 팩이요",
          "original_form": "네네 일리 동백오일 팩이요",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032549.1.4",
          "form": "씻고 하나 붙였어용",
          "original_form": "씻고 하나 붙였어용",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032549.1.5",
          "form": "건성이신가용ㅎㅎ",
          "original_form": "건성이신가용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032549.1.6",
          "form": "네네맞아요ㅠㅠ",
          "original_form": "네네맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032549.1.7",
          "form": "건성인데 끈적이는거 안좋아해서 더 걱정이에여",
          "original_form": "건성인데 끈적이는거 안좋아해서 더 걱정이에여",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032549.1.8",
          "form": "앗ㅠㅜ그러시구나",
          "original_form": "앗ㅠㅜ그러시구나",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032549.1.9",
          "form": "네네 그래서 간간히 팩이라도 하며 ㅋㅋ",
          "original_form": "네네 그래서 간간히 팩이라도 하며 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032549.1.10",
          "form": "음 앰플같은건 좀덜하더라고요!",
          "original_form": "음 앰플같은건 좀덜하더라고요!",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032549.1.11",
          "form": "팩도좋죠",
          "original_form": "팩도좋죠",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032549.1.12",
          "form": "오 앰플 많이쓰세오?",
          "original_form": "오 앰플 많이쓰세오?",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032549.1.13",
          "form": "음..피부가안좋고양심상뭐라도해야겠다싶을때꺼내요ㅎㅎ",
          "original_form": "음..피부가안좋고양심상뭐라도해야겠다싶을때꺼내요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032549.1.14",
          "form": "그렇군요 ㅋㅋㅋ",
          "original_form": "그렇군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032549.1.15",
          "form": "어디 브랜드 쓰세여",
          "original_form": "어디 브랜드 쓰세여",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032549.1.16",
          "form": "동인비라고",
          "original_form": "동인비라고",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032549.1.17",
          "form": "처음써보는건데",
          "original_form": "처음써보는건데",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032549.1.18",
