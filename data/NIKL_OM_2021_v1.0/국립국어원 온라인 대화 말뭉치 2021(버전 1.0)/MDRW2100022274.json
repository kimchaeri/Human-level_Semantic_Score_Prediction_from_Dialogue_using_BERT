      "utterance": [
        {
          "id": "MDRW2100022274.1.1",
          "form": "또 먹고 싶네요 밀키트",
          "original_form": "또 먹고 싶네요 밀키트",
          "speaker_id": "2",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022274.1.2",
          "form": "맞아요 완제품 전의 단계라ㅋㄱ",
          "original_form": "맞아요 완제품 전의 단계라ㅋㄱ",
          "speaker_id": "1",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022274.1.3",
          "form": "감바스 밀키트도 먹어봤어요",
          "original_form": "감바스 밀키트도 먹어봤어요",
          "speaker_id": "2",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022274.1.4",
          "form": "되게 신선하던데요?",
          "original_form": "되게 신선하던데요?",
          "speaker_id": "2",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.5",
          "form": "밀푀유는 뭐 찍어드세요",
          "original_form": "밀푀유는 뭐 찍어드세요",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.6",
          "form": "그거 원래 찍어먹어요?",
          "original_form": "그거 원래 찍어먹어요?",
          "speaker_id": "2",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.7",
          "form": "전 유자소스는 별로라",
          "original_form": "전 유자소스는 별로라",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.8",
          "form": "유자소스도 있구나",
          "original_form": "유자소스도 있구나",
          "speaker_id": "2",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.9",
          "form": "그 안에 칠리랑 유자소스 들어있어요",
          "original_form": "그 안에 칠리랑 유자소스 들어있어요",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.10",
          "form": "제 밀키트엔 소스 없었어요",
          "original_form": "제 밀키트엔 소스 없었어요",
          "speaker_id": "2",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.11",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.12",
          "form": "왜 내건 없었죠..",
          "original_form": "왜 내건 없었죠..",
          "speaker_id": "2",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.13",
          "form": "보통 들어있던데",
          "original_form": "보통 들어있던데",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.14",
          "form": "누락됐나봐요",
          "original_form": "누락됐나봐요",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.15",
          "form": "와사비간장소스 찍으면 맛있을 것 같아요",
          "original_form": "와사비간장소스 찍으면 맛있을 것 같아요",
          "speaker_id": "2",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.16",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.17",
          "form": "어..",
          "original_form": "어..",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.18",
          "form": "아닌거같아요",
          "original_form": "아닌거같아요",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.19",
          "form": "생각해보니 칠리소스 있었던 것 같아요",
          "original_form": "생각해보니 칠리소스 있었던 것 같아요",
          "speaker_id": "2",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.20",
          "form": "ㅋㅋㅋ참소스 어울리더라구요",
          "original_form": "ㅋㅋㅋ참소스 어울리더라구요",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022274.1.21",
          "form": "근데 냉장고에 이미 칠리소스 있어서 ㅋㅋㅋ",
          "original_form": "근데 냉장고에 이미 칠리소스 있어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:56"
        }
      ]
