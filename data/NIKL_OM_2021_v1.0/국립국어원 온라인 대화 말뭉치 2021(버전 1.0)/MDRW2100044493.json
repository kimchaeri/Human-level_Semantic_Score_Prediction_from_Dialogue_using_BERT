      "utterance": [
        {
          "id": "MDRW2100044493.1.1",
          "form": "간장 떨어졌는데 간장 살까",
          "original_form": "간장 떨어졌는데 간장 살까",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044493.1.2",
          "form": "간장 말고도 시 식용유도 떨어지지 않았어요",
          "original_form": "간장 말고도 시 식용유도 떨어지지 않았어요",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044493.1.3",
          "form": "식용유 말고 올리브 기림",
          "original_form": "식용유 말고 올리브 기림",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044493.1.4",
          "form": "아 올리브유",
          "original_form": "아 올리브유",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044493.1.5",
          "form": "어 그것도 다 먹어봐",
          "original_form": "어 그것도 다 먹어봐",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044493.1.6",
          "form": "그러면 간장이랑 올리브유 사면 되겠네",
          "original_form": "그러면 간장이랑 올리브유 사면 되겠네",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044493.1.7",
          "form": "지금 사게",
          "original_form": "지금 사게",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044493.1.8",
          "form": "응 카드 실적 챙기려면은 지금  주문해야 돼요",
          "original_form": "응 카드 실적 챙기려면은 지금  주문해야 돼요",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044493.1.9",
          "form": "예전에 샀던 거 보고",
          "original_form": "예전에 샀던 거 보고",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044493.1.10",
          "form": "주문해",
          "original_form": "주문해",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044493.1.11",
          "form": "알았어요 똑같은 거 사면 돼요",
          "original_form": "알았어요 똑같은 거 사면 돼요",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044493.1.12",
          "form": "응 사고 가격 알려줘",
          "original_form": "응 사고 가격 알려줘",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044493.1.13",
          "form": "저는 뭐 살 건데",
          "original_form": "저는 뭐 살 건데",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044493.1.14",
          "form": "난  핸드크림  살려고",
          "original_form": "난  핸드크림  살려고",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044493.1.15",
          "form": "저번에 산 거 다 썼어",
          "original_form": "저번에 산 거 다 썼어",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044493.1.16",
          "form": "내 다 썼어요",
          "original_form": "내 다 썼어요",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044493.1.17",
          "form": "가을 옷을 살찌 말지 고민",
          "original_form": "가을 옷을 살찌 말지 고민",
          "speaker_id": "1",
          "time": "20210907 22:56"
        }
