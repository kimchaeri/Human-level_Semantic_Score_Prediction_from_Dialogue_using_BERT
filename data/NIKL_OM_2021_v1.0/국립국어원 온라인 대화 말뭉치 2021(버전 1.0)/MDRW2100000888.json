      "utterance": [
        {
          "id": "MDRW2100000888.1.1",
          "form": "취업하셨나요?",
          "original_form": "취업하셨나요?",
          "speaker_id": "1",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000888.1.2",
          "form": "아뇨 아직 대1이라 알바중입니다!",
          "original_form": "아뇨 아직 대1이라 알바중입니다!",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000888.1.3",
          "form": "저도 알바하고싶어요",
          "original_form": "저도 알바하고싶어요",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000888.1.4",
          "form": "무직이여서요",
          "original_form": "무직이여서요",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000888.1.5",
          "form": "요즘 많이 안구해져서 ㅠㅠ",
          "original_form": "요즘 많이 안구해져서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000888.1.6",
          "form": "맞아요 그래서 일용직 나가는데 스트레스에요",
          "original_form": "맞아요 그래서 일용직 나가는데 스트레스에요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000888.1.7",
          "form": "조주기능사도 있어서 바에서 일하고싶은데",
          "original_form": "조주기능사도 있어서 바에서 일하고싶은데",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000888.1.8",
          "form": "여자만뽑아요",
          "original_form": "여자만뽑아요",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000888.1.9",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000888.1.10",
          "form": "슬프네요",
          "original_form": "슬프네요",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000888.1.11",
          "form": "저는 키즈카페 일하고 있어요",
          "original_form": "저는 키즈카페 일하고 있어요",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000888.1.12",
          "form": "부러워요 저도 저는 고양이카페 일해봤어요",
          "original_form": "부러워요 저도 저는 고양이카페 일해봤어요",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000888.1.13",
          "form": "오호 근데 저도 취직하고 싶네요 아직 멀었지만",
          "original_form": "오호 근데 저도 취직하고 싶네요 아직 멀었지만",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000888.1.14",
          "form": "대학생이시니 취직 잘되실거에요 어디 취직하고싶으세요?",
          "original_form": "대학생이시니 취직 잘되실거에요 어디 취직하고싶으세요?",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000888.1.15",
          "form": "복지관이요!",
          "original_form": "복지관이요!",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000888.1.16",
          "form": "와 어디 복지관이요?",
          "original_form": "와 어디 복지관이요?",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000888.1.17",
          "form": "아동복지나 그런쪽에 관심이 있어요!",
          "original_form": "아동복지나 그런쪽에 관심이 있어요!",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000888.1.18",
          "form": "아 그래서 키즈카페에서도 일하시는군요 우와",
          "original_form": "아 그래서 키즈카페에서도 일하시는군요 우와",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000888.1.19",
          "form": "자기소개서가 벌써 다 나왔네요",
          "original_form": "자기소개서가 벌써 다 나왔네요",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000888.1.20",
          "form": "히히 봉사도 자주하고 있어요 ㅇㅎ",
          "original_form": "히히 봉사도 자주하고 있어요 ㅇㅎ",
