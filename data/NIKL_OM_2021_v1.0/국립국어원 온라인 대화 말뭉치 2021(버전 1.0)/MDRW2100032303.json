      "utterance": [
        {
          "id": "MDRW2100032303.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100032303.1.2",
          "form": "점심 뭐 드셨나요?",
          "original_form": "점심 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100032303.1.3",
          "form": "안녕하세여ㅎㅎ",
          "original_form": "안녕하세여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100032303.1.4",
          "form": "저는 라면먹었어요",
          "original_form": "저는 라면먹었어요",
          "speaker_id": "2",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100032303.1.5",
          "form": "저는 비빔면 먹었는데ㅎㅎ",
          "original_form": "저는 비빔면 먹었는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100032303.1.6",
          "form": "무슨음식조아하시나요",
          "original_form": "무슨음식조아하시나요",
          "speaker_id": "1",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100032303.1.7",
          "form": "저는 다 좋아해여 ㅋㅋㅋ 배달음식은 치킨이요",
          "original_form": "저는 다 좋아해여 ㅋㅋㅋ 배달음식은 치킨이요",
          "speaker_id": "2",
          "time": "20210905 15:06"
        },
        {
          "id": "MDRW2100032303.1.8",
          "form": "치킨좋져",
          "original_form": "치킨좋져",
          "speaker_id": "1",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100032303.1.9",
          "form": "못먹는거 없으세요?",
          "original_form": "못먹는거 없으세요?",
          "speaker_id": "1",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100032303.1.10",
          "form": "저는 콩이랑 팥 빼고는 다 좋아해요ㅋㅋㅋ",
          "original_form": "저는 콩이랑 팥 빼고는 다 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100032303.1.11",
          "form": "못먹는거있으신가요 ㅋㅋ",
          "original_form": "못먹는거있으신가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100032303.1.12",
          "form": "콩과 팥!ㅎㅎ",
          "original_form": "콩과 팥!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100032303.1.13",
          "form": "저는 닭발 곱창 못먹어요",
          "original_form": "저는 닭발 곱창 못먹어요",
          "speaker_id": "1",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100032303.1.14",
          "form": "오오 그러시구나 제주위에도 그러신분 봤어요ㅋㅋ 대창 막창도 못드시나여??",
          "original_form": "오오 그러시구나 제주위에도 그러신분 봤어요ㅋㅋ 대창 막창도 못드시나여??",
          "speaker_id": "2",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100032303.1.15",
          "form": "네 생긴거랑 냄새때문에 못먹어요ㅋㅋ",
          "original_form": "네 생긴거랑 냄새때문에 못먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100032303.1.16",
          "form": "콩이랑 팥은 왜 못드시나요?",
          "original_form": "콩이랑 팥은 왜 못드시나요?",
          "speaker_id": "1",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100032303.1.17",
          "form": "아 어렸을때 먹고 체한뒤로는 역해서옄ㅋㅋㅋ ㅠㅠ",
          "original_form": "아 어렸을때 먹고 체한뒤로는 역해서옄ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 15:10"
        },
        {
          "id": "MDRW2100032303.1.18",
          "form": "팥빙수도 안먹어요 ㅎㄹ",
          "original_form": "팥빙수도 안먹어요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210905 15:10"
        },
        {
          "id": "MDRW2100032303.1.19",
          "form": "헉 그렇겠네요",
          "original_form": "헉 그렇겠네요",
          "speaker_id": "1",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032303.1.20",
          "form": "팥안들어간 빙수드셔야겠어요",
          "original_form": "팥안들어간 빙수드셔야겠어요",
          "speaker_id": "1",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032303.1.21",
          "form": "네네ㅋㅋㅋ 그래도 대체재가 많아서 다행이에여 ㅎㅎ",
          "original_form": "네네ㅋㅋㅋ 그래도 대체재가 많아서 다행이에여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032303.1.22",
          "form": "맞아요ㅋㅋ빙수종류 많잖아요",
          "original_form": "맞아요ㅋㅋ빙수종류 많잖아요",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032303.1.23",
          "form": "어떤거 제일 좋아하세요?",
          "original_form": "어떤거 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032303.1.24",
          "form": "저는 망빙이랑 딸빙 조아해여 ㅋㅋㅋ",
          "original_form": "저는 망빙이랑 딸빙 조아해여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032303.1.25",
          "form": "name1님은여?",
          "original_form": "&name1&님은여?",
          "speaker_id": "2",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032303.1.26",
          "form": "저도 과일다 좋아하고 치즈들어가면 더 좋아요ㅎㅎ",
          "original_form": "저도 과일다 좋아하고 치즈들어가면 더 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100032303.1.27",
          "form": "오오 치즈 맛있죠 ㅋㅋㅋ저도 과일은 딱히 안가려서 다 먹는 편이에여",
          "original_form": "오오 치즈 맛있죠 ㅋㅋㅋ저도 과일은 딱히 안가려서 다 먹는 편이에여",
