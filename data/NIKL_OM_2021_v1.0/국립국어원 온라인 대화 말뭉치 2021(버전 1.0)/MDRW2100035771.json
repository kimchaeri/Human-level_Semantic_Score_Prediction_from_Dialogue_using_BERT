        {
          "id": "MDRW2100035771.1.1",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100035771.1.2",
          "form": "최근에 뭐 사신거 없으세요??",
          "original_form": "최근에 뭐 사신거 없으세요??",
          "speaker_id": "1",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100035771.1.3",
          "form": "액정타블렛 얘기했나요?",
          "original_form": "액정타블렛 얘기했나요?",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100035771.1.4",
          "form": "네네 그림그리는거",
          "original_form": "네네 그림그리는거",
          "speaker_id": "1",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100035771.1.5",
          "form": "그게 마지막이었나요??ㅎㅎ",
          "original_form": "그게 마지막이었나요??ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100035771.1.6",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035771.1.7",
          "form": "더치커피요 성미산 좋은날 협동조합",
          "original_form": "더치커피요 성미산 좋은날 협동조합",
          "speaker_id": "2",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035771.1.8",
          "form": "담에 이거 함 드셔보세요",
          "original_form": "담에 이거 함 드셔보세요",
          "speaker_id": "2",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035771.1.9",
          "form": "오 다음엔 기억해뒀다가",
          "original_form": "오 다음엔 기억해뒀다가",
          "speaker_id": "1",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035771.1.10",
          "form": "먹어볼께용",
          "original_form": "먹어볼께용",
          "speaker_id": "1",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035771.1.11",
          "form": "요것도 봉지인가요??",
          "original_form": "요것도 봉지인가요??",
          "speaker_id": "1",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035771.1.12",
          "form": "이거 저희 사촌동생이 일하는데요",
          "original_form": "이거 저희 사촌동생이 일하는데요",
          "speaker_id": "2",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035771.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035771.1.14",
          "form": "아하!!",
          "original_form": "아하!!",
          "speaker_id": "1",
          "time": "20210830 01:19"
        },
        {
          "id": "MDRW2100035771.1.15",
          "form": "가격은 얼마정도??",
          "original_form": "가격은 얼마정도??",
          "speaker_id": "1",
          "time": "20210830 01:19"
        },
        {
          "id": "MDRW2100035771.1.16",
          "form": "40ml 12개에 13000원이요",
          "original_form": "40ml 12개에 13000원이요",
          "speaker_id": "2",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100035771.1.17",
          "form": "2만원 이상이면 무료배송이에요",
          "original_form": "2만원 이상이면 무료배송이에요",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100035771.1.18",
          "form": "제가 열박스 넘게 먹었는데요 맛이 똑같고 초콜렛향이 나요",
          "original_form": "제가 열박스 넘게 먹었는데요 맛이 똑같고 초콜렛향이 나요",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100035771.1.19",
          "form": "오 ㅎㅎ 아참 네고왕 보셔요??",
          "original_form": "오 ㅎㅎ 아참 네고왕 보셔요??",
          "speaker_id": "1",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100035771.1.20",
          "form": "이번에는 이차돌인데 카톡에서 쿠폰 받을수 있네요 ㅎㅎ",
          "original_form": "이번에는 이차돌인데 카톡에서 쿠폰 받을수 있네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100035771.1.21",
          "form": "아뇨 들어는봤어요",
          "original_form": "아뇨 들어는봤어요",
          "speaker_id": "2",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100035771.1.22",
          "form": "그건 뭐에요?",
          "original_form": "그건 뭐에요?",
          "speaker_id": "2",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100035771.1.23",
          "form": "엠씨들이 브랜드 본사가서 대표만나서 네고 하는 프로인데",
          "original_form": "엠씨들이 브랜드 본사가서 대표만나서 네고 하는 프로인데",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100035771.1.24",
          "form": "이번시즌에 배스킨도 하고 했는데 이차돌도 잇네요 ㅎㅎ",
          "original_form": "이번시즌에 배스킨도 하고 했는데 이차돌도 잇네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100035771.1.25",
          "form": "아아 그렇군요 쿠폰을 주나봐요",
          "original_form": "아아 그렇군요 쿠폰을 주나봐요",
          "speaker_id": "2",
          "time": "20210830 01:26"
        },
        {
          "id": "MDRW2100035771.1.26",
          "form": "그럼 네고왕 카톡이있는거에요?",
          "original_form": "그럼 네고왕 카톡이있는거에요?",
          "speaker_id": "2",
          "time": "20210830 01:27"
        },
        {
          "id": "MDRW2100035771.1.27",
          "form": "아니요 쿠폰은 이번에 이차돌 카톡으로",
          "original_form": "아니요 쿠폰은 이번에 이차돌 카톡으로",
          "speaker_id": "1",
          "time": "20210830 01:27"
        },
        {
          "id": "MDRW2100035771.1.28",
          "form": "카톡에거 플친 하고 받는거에요 한번 보세용 선착순",
          "original_form": "카톡에거 플친 하고 받는거에요 한번 보세용 선착순",
          "speaker_id": "1",
          "time": "20210830 01:28"
        },
        {
          "id": "MDRW2100035771.1.29",
          "form": "이차돌 가시면 괜찮은것 같아요",
          "original_form": "이차돌 가시면 괜찮은것 같아요",
          "speaker_id": "1",
          "time": "20210830 01:28"
        },
        {
          "id": "MDRW2100035771.1.30",
          "form": "아아 넵 가볼게요",
          "original_form": "아아 넵 가볼게요",
          "speaker_id": "2",
          "time": "20210830 01:28"
        }
      ]
    }
