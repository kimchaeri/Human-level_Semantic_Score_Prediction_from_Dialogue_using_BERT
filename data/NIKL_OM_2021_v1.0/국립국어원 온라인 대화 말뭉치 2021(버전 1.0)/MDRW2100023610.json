      "utterance": [
        {
          "id": "MDRW2100023610.1.1",
          "form": "뭐.. 모임 같은거 하시나요?",
          "original_form": "뭐.. 모임 같은거 하시나요?",
          "speaker_id": "1",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100023610.1.2",
          "form": "저 산악회라 등산 다니는데 코로나라 모임 못나가고 있어요!",
          "original_form": "저 산악회라 등산 다니는데 코로나라 모임 못나가고 있어요!",
          "speaker_id": "2",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100023610.1.3",
          "form": "오.. 산악회 오래하셧나요?",
          "original_form": "오.. 산악회 오래하셧나요?",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100023610.1.4",
          "form": "산 세번인가밖에 못갔어요",
          "original_form": "산 세번인가밖에 못갔어요",
          "speaker_id": "2",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100023610.1.5",
          "form": "1년 했는데",
          "original_form": "1년 했는데",
          "speaker_id": "2",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100023610.1.6",
          "form": "코로나땜시",
          "original_form": "코로나땜시",
          "speaker_id": "2",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100023610.1.7",
          "form": "산악회는 어케 가입하셧어요?",
          "original_form": "산악회는 어케 가입하셧어요?",
          "speaker_id": "1",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100023610.1.8",
          "form": "저도 등산 좋아해서.. 가입해보고 싶기도하고..",
          "original_form": "저도 등산 좋아해서.. 가입해보고 싶기도하고..",
          "speaker_id": "1",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100023610.1.9",
          "form": "지인 소개로 가입했어요 ㅎㅎ",
          "original_form": "지인 소개로 가입했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100023610.1.10",
          "form": "등산 좋아하시면 추천입니다",
          "original_form": "등산 좋아하시면 추천입니다",
          "speaker_id": "2",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100023610.1.11",
          "form": "같이 가면 혼자서는 안 갈 산도 가고 좋더라구요",
          "original_form": "같이 가면 혼자서는 안 갈 산도 가고 좋더라구요",
          "speaker_id": "2",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100023610.1.12",
          "form": "그 버스대절하는 그런건가요? \n아님 출발지에 각자?",
          "original_form": "그 버스대절하는 그런건가요? \n아님 출발지에 각자?",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100023610.1.13",
          "form": "저는 그냥 각자 모이는 것만 가봤는데",
          "original_form": "저는 그냥 각자 모이는 것만 가봤는데",
          "speaker_id": "2",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100023610.1.14",
          "form": "버스대절하는 산악회도 잇다더라구요 편할거같아요",
          "original_form": "버스대절하는 산악회도 잇다더라구요 편할거같아요",
          "speaker_id": "2",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100023610.1.15",
          "form": "보통 몇명정도 모여요?",
          "original_form": "보통 몇명정도 모여요?",
          "speaker_id": "1",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100023610.1.16",
          "form": "요즘 코로나라 안모여서 모르겠는데 예전엔",
          "original_form": "요즘 코로나라 안모여서 모르겠는데 예전엔",
          "speaker_id": "2",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100023610.1.17",
          "form": "6-7명정도 간거같아요",
          "original_form": "6-7명정도 간거같아요",
          "speaker_id": "2",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100023610.1.18",
          "form": "아.. 어디산 가보셨어용? 높은데요?",
          "original_form": "아.. 어디산 가보셨어용? 높은데요?",
          "speaker_id": "1",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100023610.1.19",
          "form": "저 한라산이요 ㅋㅋㅋㅋ",
          "original_form": "저 한라산이요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100023610.1.20",
