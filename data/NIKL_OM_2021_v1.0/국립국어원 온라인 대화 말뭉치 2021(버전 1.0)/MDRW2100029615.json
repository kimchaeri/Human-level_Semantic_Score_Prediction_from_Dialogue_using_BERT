      "utterance": [
        {
          "id": "MDRW2100029615.1.1",
          "form": "오늘은 출근할때\r\nname3가 안막던가요?",
          "original_form": "오늘은 출근할때\r\n&name3&가 안막던가요?",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029615.1.2",
          "form": "엄청 짖었어유 ㅋㅋ",
          "original_form": "엄청 짖었어유 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029615.1.3",
          "form": "오늘은 name4랑 자는날이라서",
          "original_form": "오늘은 &name4&랑 자는날이라서",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029615.1.4",
          "form": "name4랑잤는데",
          "original_form": "&name4&랑잤는데",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029615.1.5",
          "form": "name4도 저를 깨우더군요",
          "original_form": "&name4&도 저를 깨우더군요",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029615.1.6",
          "form": "ㅋㅋㅋname4랑 자는날\r\nname3랑 자는 날이\r\n정해져 있는거야?",
          "original_form": "ㅋㅋㅋ&name4&랑 자는날\r\n&name3&랑 자는 날이\r\n정해져 있는거야?",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029615.1.7",
          "form": "너 코 고는거 아니야?",
          "original_form": "너 코 고는거 아니야?",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029615.1.8",
          "form": "name3랑 name4가 왜케 깨움ㅋㅋ",
          "original_form": "&name3&랑 &name4&가 왜케 깨움ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100029615.1.9",
          "form": "알람 30분전에 맨날깨움",
          "original_form": "알람 30분전에 맨날깨움",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029615.1.10",
          "form": "name4는 명치에 올라가서 깨우고",
          "original_form": "&name4&는 명치에 올라가서 깨우고",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029615.1.11",
          "form": "name3는 귀핥아서 깨우고..ㅡ",
          "original_form": "&name3&는 귀핥아서 깨우고..ㅡ",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029615.1.12",
          "form": "...코 곤적이없는데....",
          "original_form": "...코 곤적이없는데....",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029615.1.13",
          "form": "ㅋㅋㅋㅋㅋ근데 왜깨움",
          "original_form": "ㅋㅋㅋㅋㅋ근데 왜깨움",
          "speaker_id": "1",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029615.1.14",
          "form": "이 가세요?",
          "original_form": "이 가세요?",
          "speaker_id": "1",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029615.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029615.1.16",
          "form": "아침밥아닐까여?",
          "original_form": "아침밥아닐까여?",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029615.1.17",
          "form": "아님 심심해서 ㅋㅋㅋㅋ",
          "original_form": "아님 심심해서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029615.1.18",
          "form": "집사야 해가 중천에 떴다",
          "original_form": "집사야 해가 중천에 떴다",
          "speaker_id": "1",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029615.1.19",
          "form": "밥상차려라 ㅋㅋㅋㅋ",
          "original_form": "밥상차려라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029615.1.20",
          "form": "이런건가요?   ㅋㅋㅋㅋ",
          "original_form": "이런건가요?   ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029615.1.21",
          "form": "네 맞습니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 맞습니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029615.1.22",
          "form": "그만자고 밥좀줘라",
          "original_form": "그만자고 밥좀줘라",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029615.1.23",
          "form": "애기들 밥이랑 간식 좀\r\n많이 주세염 ㅋㅋㅋ",
          "original_form": "애기들 밥이랑 간식 좀\r\n많이 주세염 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029615.1.24",
          "form": "간식비 엄청 들어가고잏어여...이미...",
          "original_form": "간식비 엄청 들어가고잏어여...이미...",
