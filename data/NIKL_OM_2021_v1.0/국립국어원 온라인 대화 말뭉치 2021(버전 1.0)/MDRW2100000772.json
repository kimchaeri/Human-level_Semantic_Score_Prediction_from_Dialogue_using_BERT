      "utterance": [
        {
          "id": "MDRW2100000772.1.1",
          "form": "최근에 구매하신물품있나요?",
          "original_form": "최근에 구매하신물품있나요?",
          "speaker_id": "2",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100000772.1.2",
          "form": "제습기 샀어요",
          "original_form": "제습기 샀어요",
          "speaker_id": "1",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100000772.1.3",
          "form": "요즘같이습할때 필수인듯요ㅜ 제습기 물금방차더라구요",
          "original_form": "요즘같이습할때 필수인듯요ㅜ 제습기 물금방차더라구요",
          "speaker_id": "2",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000772.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000772.1.5",
          "form": "근데 틀어놓으몀 너무 더워지더라구요",
          "original_form": "근데 틀어놓으몀 너무 더워지더라구요",
          "speaker_id": "1",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000772.1.6",
          "form": "ㅋㅋㅋ네 뜨끈하고 뽀송한기분이에요",
          "original_form": "ㅋㅋㅋ네 뜨끈하고 뽀송한기분이에요",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000772.1.7",
          "form": "당근마켓같은거 애용하시나요?",
          "original_form": "당근마켓같은거 애용하시나요?",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000772.1.8",
          "form": "네 해봣어요",
          "original_form": "네 해봣어요",
          "speaker_id": "1",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000772.1.9",
          "form": "이게 한번 맛들리니까 집안살림다 거덜나게되더라구여 ㅋㅋㅋㅋㅋ",
          "original_form": "이게 한번 맛들리니까 집안살림다 거덜나게되더라구여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000772.1.10",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000772.1.11",
          "form": "지역이라서 배송도 않거 바로바로",
          "original_form": "지역이라서 배송도 않거 바로바로",
          "speaker_id": "1",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000772.1.12",
          "form": "네 같은 동네주민들이니까 또 바로바로 시간될때 거래하면되고",
          "original_form": "네 같은 동네주민들이니까 또 바로바로 시간될때 거래하면되고",
          "speaker_id": "2",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000772.1.13",
          "form": "편하고 좋더라구요",
          "original_form": "편하고 좋더라구요",
          "speaker_id": "2",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000772.1.14",
          "form": "너무 편해요",
          "original_form": "너무 편해요",
          "speaker_id": "1",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000772.1.15",
          "form": "맞아요 동네분들 친해지기도하구여",
          "original_form": "맞아요 동네분들 친해지기도하구여",
          "speaker_id": "2",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000772.1.16",
          "form": "요즘들어 사고싶은 물건있나요?",
          "original_form": "요즘들어 사고싶은 물건있나요?",
          "speaker_id": "2",
          "time": "20210819 19:22"
        },
        {
