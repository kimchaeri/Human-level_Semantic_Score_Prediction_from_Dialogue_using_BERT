      "utterance": [
        {
          "id": "MDRW2100034748.1.1",
          "form": "중고거래 하시나요?",
          "original_form": "중고거래 하시나요?",
          "speaker_id": "2",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100034748.1.2",
          "form": "저는 진짜 가끔?",
          "original_form": "저는 진짜 가끔?",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100034748.1.3",
          "form": "자주 하세여?",
          "original_form": "자주 하세여?",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100034748.1.4",
          "form": "육아용품 처분할때 애용했는데 요즘은 뜸하네요",
          "original_form": "육아용품 처분할때 애용했는데 요즘은 뜸하네요",
          "speaker_id": "2",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100034748.1.5",
          "form": "오 육아용품 거래는 진짜 유용하겠네요",
          "original_form": "오 육아용품 거래는 진짜 유용하겠네요",
          "speaker_id": "1",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100034748.1.6",
          "form": "네 부피도 크고 하니 중고거래가 유용하더라구요",
          "original_form": "네 부피도 크고 하니 중고거래가 유용하더라구요",
          "speaker_id": "2",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034748.1.7",
          "form": "저는 팔아도 그냥 문구용품 사무용품 정도였어요",
          "original_form": "저는 팔아도 그냥 문구용품 사무용품 정도였어요",
          "speaker_id": "1",
          "time": "20210828 18:45"
        },
        {
          "id": "MDRW2100034748.1.8",
          "form": "아 거래는 원활했나요?",
          "original_form": "아 거래는 원활했나요?",
          "speaker_id": "2",
          "time": "20210828 18:54"
        },
        {
          "id": "MDRW2100034748.1.9",
          "form": "사실 한번 바람맞아서 ㅋㅋㅋㅋ",
          "original_form": "사실 한번 바람맞아서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:55"
        },
        {
          "id": "MDRW2100034748.1.10",
          "form": "기분나빠서 한동안 중고거래 끊었어요",
          "original_form": "기분나빠서 한동안 중고거래 끊었어요",
          "speaker_id": "1",
          "time": "20210828 18:55"
        },
        {
          "id": "MDRW2100034748.1.11",
          "form": "아 그렇죠 중고거래가 늘 마음같진 않아요",
          "original_form": "아 그렇죠 중고거래가 늘 마음같진 않아요",
          "speaker_id": "2",
          "time": "20210828 18:55"
        },
        {
          "id": "MDRW2100034748.1.12",
          "form": "20분이나 밖에서 기다렸는데...흑",
          "original_form": "20분이나 밖에서 기다렸는데...흑",
          "speaker_id": "1",
          "time": "20210828 18:55"
        },
        {
          "id": "MDRW2100034748.1.13",
          "form": "연락처 주고받는게 꺼려지기도 하지만 또 연락안되면 넘 답답하죠 ㅠ",
          "original_form": "연락처 주고받는게 꺼려지기도 하지만 또 연락안되면 넘 답답하죠 ㅠ",
          "speaker_id": "2",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100034748.1.14",
          "form": "맞아요ㅋㅋ 그래도 당근 안에 통화기능 생긴것 같더라구요?",
          "original_form": "맞아요ㅋㅋ 그래도 당근 안에 통화기능 생긴것 같더라구요?",
          "speaker_id": "1",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100034748.1.15",
          "form": "근데 제대로 해본적은 없어요!!",
          "original_form": "근데 제대로 해본적은 없어요!!",
