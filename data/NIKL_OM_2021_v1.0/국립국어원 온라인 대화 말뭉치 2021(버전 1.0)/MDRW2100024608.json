        {
          "id": "MDRW2100024608.1.1",
          "form": "여행 좋아하시나여?",
          "original_form": "여행 좋아하시나여?",
          "speaker_id": "1",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100024608.1.2",
          "form": "넵+!",
          "original_form": "넵+!",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100024608.1.3",
          "form": "여행 정말 좋아해요ㅎㅎ근데 해외여행은",
          "original_form": "여행 정말 좋아해요ㅎㅎ근데 해외여행은",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100024608.1.4",
          "form": "일본밖에 못가봤어요ㅠ",
          "original_form": "일본밖에 못가봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100024608.1.5",
          "form": "name1님은요~??",
          "original_form": "&name1&님은요~??",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100024608.1.6",
          "form": "우앗 저두요!",
          "original_form": "우앗 저두요!",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100024608.1.7",
          "form": "일본 어디 가보셨어요?",
          "original_form": "일본 어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100024608.1.8",
          "form": "도쿄만 가봤어요ㅎㅎ!!",
          "original_form": "도쿄만 가봤어요ㅎㅎ!!",
          "speaker_id": "2",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024608.1.9",
          "form": "도쿄 근교도 넘 좋았고,",
          "original_form": "도쿄 근교도 넘 좋았고,",
          "speaker_id": "2",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024608.1.10",
          "form": "도쿄 디즈니랜드 또 가고 싶어요ㅎㅎㅠㅠㅠ",
          "original_form": "도쿄 디즈니랜드 또 가고 싶어요ㅎㅎㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024608.1.11",
          "form": "name1님은 일본여행 어떠셨어요?ㅎㅎ",
          "original_form": "&name1&님은 일본여행 어떠셨어요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024608.1.12",
          "form": "우옹 저도 도쿄만 가봤는데 제가 토토로를 너무 좋아해서 상품 사러 다니느라 바빴달까요..ㅋㅋㅋ",
          "original_form": "우옹 저도 도쿄만 가봤는데 제가 토토로를 너무 좋아해서 상품 사러 다니느라 바빴달까요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024608.1.13",
          "form": "토토로 박물관 진짜 좋았어요! 사람이 너무 많았던것만 빼면요ㅠ",
          "original_form": "토토로 박물관 진짜 좋았어요! 사람이 너무 많았던것만 빼면요ㅠ",
          "speaker_id": "1",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024608.1.14",
          "form": "현지인도 진짜 많더라구요!",
          "original_form": "현지인도 진짜 많더라구요!",
          "speaker_id": "1",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024608.1.15",
          "form": "오오 토토로 박물관!!",
          "original_form": "오오 토토로 박물관!!",
          "speaker_id": "2",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024608.1.16",
          "form": "확실히",
          "original_form": "확실히",
          "speaker_id": "2",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024608.1.17",
          "form": "일본은 그런델 가야해요ㅎㅎ넘 잘해놨을 것 같아요",
          "original_form": "일본은 그런델 가야해요ㅎㅎ넘 잘해놨을 것 같아요",
          "speaker_id": "2",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024608.1.18",
          "form": "전 먹었던 것들도 다 넘 좋았어요",
          "original_form": "전 먹었던 것들도 다 넘 좋았어요",
          "speaker_id": "2",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024608.1.19",
          "form": "특별히 기억에 남는 맛집 있으신가요?",
          "original_form": "특별히 기억에 남는 맛집 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024608.1.20",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024608.1.21",
          "form": "저도 다 좋았는데 아키하바라에 돈까스 유명한집이 있어서 30분 일찍도착했는데도 줄서서 먹었는데",
          "original_form": "저도 다 좋았는데 아키하바라에 돈까스 유명한집이 있어서 30분 일찍도착했는데도 줄서서 먹었는데",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024608.1.22",
          "form": "평소에 줄서는거 싫어해서 그래도 여행이니 기다리자! 하고 기다려서 먹었는데 어떤 돈까스도 못이길정도로 맛있었어요!",
          "original_form": "평소에 줄서는거 싫어해서 그래도 여행이니 기다리자! 하고 기다려서 먹었는데 어떤 돈까스도 못이길정도로 맛있었어요!",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024608.1.23",
          "form": "오 너무 궁금한데여?",
          "original_form": "오 너무 궁금한데여?",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024608.1.24",
          "form": "돈까스.....",
          "original_form": "돈까스.....",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024608.1.25",
          "form": "ㅜㅜㅠㅠㅠ",
          "original_form": "ㅜㅜㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024608.1.26",
          "form": "전 일본에서 먹었던 우동이",
          "original_form": "전 일본에서 먹었던 우동이",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024608.1.27",
          "form": "젤 기억에 남아요 어딘진 까먹었는데",
          "original_form": "젤 기억에 남아요 어딘진 까먹었는데",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024608.1.28",
          "form": "도쿄 어딘가....의 우동집ㅜㅠㅠㅠ",
          "original_form": "도쿄 어딘가....의 우동집ㅜㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024608.1.29",
          "form": "엄청 쫄깃하고 맛있었습니닷ㅜㅠ",
          "original_form": "엄청 쫄깃하고 맛있었습니닷ㅜㅠ",
          "speaker_id": "2",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024608.1.30",
          "form": "으악 저 우동을 안먹은게 진짜 한인데!!",
          "original_form": "으악 저 우동을 안먹은게 진짜 한인데!!",
          "speaker_id": "1",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024608.1.31",
          "form": "먹으러 갈랬더니 코로나가ㅂㄷㅂㄷ",
          "original_form": "먹으러 갈랬더니 코로나가ㅂㄷㅂㄷ",
          "speaker_id": "1",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024608.1.32",
          "form": "라멘만 엄청 먹고 왔었네오ㅜ",
          "original_form": "라멘만 엄청 먹고 왔었네오ㅜ",
          "speaker_id": "1",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024608.1.33",
          "form": "엇 늦어서 죄송해요",
          "original_form": "엇 늦어서 죄송해요",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100024608.1.34",
          "form": "여행얘기 하니 넘 여행가고 싶네요..ㅎ.... 지금 개강해서 정신이 없는데",
          "original_form": "여행얘기 하니 넘 여행가고 싶네요..ㅎ.... 지금 개강해서 정신이 없는데",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100024608.1.35",
          "form": "겨울엔 꼭 어딘갈 가야겠어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "겨울엔 꼭 어딘갈 가야겠어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100024608.1.36",
          "form": "해외여행..ㅎ.ㅎ.",
          "original_form": "해외여행..ㅎ.ㅎ.",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100024608.1.37",
          "form": "아녜요! 개강하셨음 진짜 바쁘시겠어요ㅜㅜㅜ",
          "original_form": "아녜요! 개강하셨음 진짜 바쁘시겠어요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100024608.1.38",
          "form": "해외!!! 진짜 못가니 더 가고파요",
          "original_form": "해외!!! 진짜 못가니 더 가고파요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100024608.1.39",
          "form": "맞아요ㅜㅜㅠㅠ",
          "original_form": "맞아요ㅜㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100024608.1.40",
          "form": "저는 이번 올림픽보고",
          "original_form": "저는 이번 올림픽보고",
          "speaker_id": "2",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100024608.1.41",
          "form": "파리올림픽에 너무 가고 싶더라구요......",
          "original_form": "파리올림픽에 너무 가고 싶더라구요......",
          "speaker_id": "2",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100024608.1.42",
          "form": "오오 파리.. 직관으로요??",
          "original_form": "오오 파리.. 직관으로요??",
          "speaker_id": "1",
          "time": "20210902 07:09"
