      "utterance": [
        {
          "id": "MDRW2100011597.1.1",
          "form": "점심은 드셨나요?",
          "original_form": "점심은 드셨나요?",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011597.1.2",
          "form": "네 ㅎㅎ 닭가슴살 도시락 먹었어요",
          "original_form": "네 ㅎㅎ 닭가슴살 도시락 먹었어요",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011597.1.3",
          "form": "퍽퍽살 ㅎ 듣기만해도 퍽퍽ㅋ",
          "original_form": "퍽퍽살 ㅎ 듣기만해도 퍽퍽ㅋ",
          "speaker_id": "1",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011597.1.4",
          "form": "ㅋㅋ요새는 그래도 먹을만하게 나와요!",
          "original_form": "ㅋㅋ요새는 그래도 먹을만하게 나와요!",
          "speaker_id": "2",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100011597.1.5",
          "form": "허닭 괜찮던데요",
          "original_form": "허닭 괜찮던데요",
          "speaker_id": "1",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100011597.1.6",
          "form": "적립금으로 함박스테이크샀는데",
          "original_form": "적립금으로 함박스테이크샀는데",
          "speaker_id": "1",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100011597.1.7",
          "form": "오 소름....",
          "original_form": "오 소름....",
          "speaker_id": "2",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100011597.1.8",
          "form": "허닭입니다",
          "original_form": "허닭입니다",
          "speaker_id": "2",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100011597.1.9",
          "form": "허닭이 저랑 잘 맞더라고요",
          "original_form": "허닭이 저랑 잘 맞더라고요",
          "speaker_id": "2",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011597.1.10",
          "form": "아 글쿤요 ㅎ 함박 치즈퐁듀사는데 괜찮더라구요",
          "original_form": "아 글쿤요 ㅎ 함박 치즈퐁듀사는데 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011597.1.11",
          "form": "오 그건 다음 주문때 해볼께요",
          "original_form": "오 그건 다음 주문때 해볼께요",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011597.1.12",
          "form": "저녁은 뭐드세요?",
          "original_form": "저녁은 뭐드세요?",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011597.1.13",
          "form": "아직 퇴근시간이 남아서 ㅋㅋ",
          "original_form": "아직 퇴근시간이 남아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011597.1.14",
          "form": "보통 나가면서 결정해요",
          "original_form": "보통 나가면서 결정해요",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011597.1.15",
          "form": "회사에서 드시고 퇴근하시나요",
          "original_form": "회사에서 드시고 퇴근하시나요",
          "speaker_id": "2",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100011597.1.16",
          "form": "오늘은 9시 요기요 파바나 주문해서 먹을려구요",
          "original_form": "오늘은 9시 요기요 파바나 주문해서 먹을려구요",
          "speaker_id": "1",
          "time": "20210824 18:44"
        },
        {
          "id": "MDRW2100011597.1.17",
          "form": "만원어치 0원결제",
          "original_form": "만원어치 0원결제",
