        },
        {
          "id": "MDRW2100008329.1.3",
          "form": "미용실 고고 뿌염",
          "original_form": "미용실 고고 뿌염",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008329.1.4",
          "form": "잉?",
          "original_form": "잉?",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008329.1.5",
          "form": "없어졌노",
          "original_form": "없어졌노",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008329.1.6",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008329.1.7",
          "form": "다이어트 하까ㅋㅋㅋ",
          "original_form": "다이어트 하까ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008329.1.8",
          "form": "ㅋㅋㅋㅋ 왜",
          "original_form": "ㅋㅋㅋㅋ 왜",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008329.1.9",
          "form": "잉?",
          "original_form": "잉?",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008329.1.10",
          "form": "갑자기 다이어트 왜하는데요?? ㅋㅋㅋㅋㅋ",
          "original_form": "갑자기 다이어트 왜하는데요?? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008329.1.11",
          "form": "뭐가자꾸없어지노",
          "original_form": "뭐가자꾸없어지노",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008329.1.12",
          "form": "글씨가",
          "original_form": "글씨가",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008329.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008329.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 정신차리소 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 정신차리소 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.15",
          "form": "언니 지금 뿌염하러간다고??",
          "original_form": "언니 지금 뿌염하러간다고??",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.16",
          "form": "갑자기 건강해질라고",
          "original_form": "갑자기 건강해질라고",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.17",
          "form": "팔자 좋데이",
          "original_form": "팔자 좋데이",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.18",
          "form": "글씨 다있나?",
          "original_form": "글씨 다있나?",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.19",
          "form": "난 다 사라졌다ㅡㅡ",
          "original_form": "난 다 사라졌다ㅡㅡ",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.20",
          "form": "있는것도 있고",
          "original_form": "있는것도 있고",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.21",
          "form": "뭐지?",
          "original_form": "뭐지?",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.22",
          "form": "없는것도 있음 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "없는것도 있음 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.23",
          "form": "글쿤",
          "original_form": "글쿤",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.24",
          "form": "이거 제출하면 그 전꺼 다 사라짐 ㅋㅋㅋㄱㄱ",
          "original_form": "이거 제출하면 그 전꺼 다 사라짐 ㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.25",
          "form": "니 옷사라ㅋㅋ",
          "original_form": "니 옷사라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.26",
          "form": "정신 똑띠 차려야됨",
          "original_form": "정신 똑띠 차려야됨",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.27",
          "form": "왜 갑자기 내옷\r\nㅋㅋㅋㅋㅋ",
          "original_form": "왜 갑자기 내옷\r\nㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.28",
          "form": "내 거지같나 ㅋㅋㅋㅋㅋ",
          "original_form": "내 거지같나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008329.1.29",
          "form": "아무말대잔치ㅋㅋ",
          "original_form": "아무말대잔치ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008329.1.30",
          "form": "나도사고ㅋㅋ",
          "original_form": "나도사고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008329.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008329.1.32",
          "form": "아이엠 세일 할때가서",
          "original_form": "아이엠 세일 할때가서",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008329.1.33",
          "form": "사야지",
          "original_form": "사야지",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008329.1.34",
          "form": "오늘 어떤아줌마 배꼽티입었던데",
          "original_form": "오늘 어떤아줌마 배꼽티입었던데",
          "speaker_id": "1",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008329.1.35",
          "form": "지나갈때 앞에 뭐 있나 보고",
          "original_form": "지나갈때 앞에 뭐 있나 보고",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008329.1.36",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008329.1.37",
          "form": "매대상품만 살수있음",
          "original_form": "매대상품만 살수있음",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008329.1.38",
          "form": "어후 배꼽티라니",
          "original_form": "어후 배꼽티라니",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008329.1.39",
          "form": "남사시럽다 정말",
          "original_form": "남사시럽다 정말",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008329.1.40",
          "form": "아이엠좋지ㅋ",
          "original_form": "아이엠좋지ㅋ",
          "speaker_id": "1",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008329.1.41",
          "form": "왜그러노",
          "original_form": "왜그러노",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008329.1.42",
          "form": "옹",
          "original_form": "옹",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008329.1.43",
          "form": "난 안간지 좀됐군",
          "original_form": "난 안간지 좀됐군",
          "speaker_id": "1",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008329.1.44",
          "form": "세일할때만 살수있음",
          "original_form": "세일할때만 살수있음",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
