      "utterance": [
        {
          "id": "MDRW2100015008.1.1",
          "form": "그 직장인들 모여있는 곳에 위치한 닭갈비집 있는데",
          "original_form": "그 직장인들 모여있는 곳에 위치한 닭갈비집 있는데",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100015008.1.2",
          "form": "와 로컬맛집 찐이다",
          "original_form": "와 로컬맛집 찐이다",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015008.1.3",
          "form": "거기가 진짜 맛있어요",
          "original_form": "거기가 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015008.1.4",
          "form": "또 좋아하는 음식 있으신가요?",
          "original_form": "또 좋아하는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015008.1.5",
          "form": "아무래도 체인보다는 로컬이..",
          "original_form": "아무래도 체인보다는 로컬이..",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015008.1.6",
          "form": "저 피자좋아해요",
          "original_form": "저 피자좋아해요",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015008.1.7",
          "form": "피자좋아하세요",
          "original_form": "피자좋아하세요",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015008.1.8",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100015008.1.9",
          "form": "하와이안피자파이신가요 혹시",
          "original_form": "하와이안피자파이신가요 혹시",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100015008.1.10",
          "form": "하와이안 vs 슈프림",
          "original_form": "하와이안 vs 슈프림",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100015008.1.11",
          "form": "슈프림이죠",
          "original_form": "슈프림이죠",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100015008.1.12",
          "form": "하와이안좋아하세요.",
          "original_form": "하와이안좋아하세요.",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015008.1.13",
          "form": "설마",
          "original_form": "설마",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015008.1.14",
          "form": "아닐거야",
          "original_form": "아닐거야",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015008.1.15",
          "form": "예?!?!",
          "original_form": "예?!?!",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015008.1.16",
          "form": "설마",
          "original_form": "설마",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015008.1.17",
          "form": "사람이 하와이안 좋아할..수도..있지..!!!",
          "original_form": "사람이 하와이안 좋아할..수도..있지..!!!",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015008.1.18",
          "form": "새우를 버리고",
          "original_form": "새우를 버리고",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015008.1.19",
          "form": "파인애플을 선택한다고?",
          "original_form": "파인애플을 선택한다고?",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015008.1.20",
          "form": "하와이안이 더 맛있지 않나요?",
          "original_form": "하와이안이 더 맛있지 않나요?",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015008.1.21",
          "form": "잠만 차단 금지",
          "original_form": "잠만 차단 금지",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015008.1.22",
          "form": "와 탱글탱글쫀득새우를 버리고?",
          "original_form": "와 탱글탱글쫀득새우를 버리고?",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015008.1.23",
          "form": "와 우리 잠시 5센치 멀어집시다",
          "original_form": "와 우리 잠시 5센치 멀어집시다",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015008.1.24",
          "form": "쓰읍..",
          "original_form": "쓰읍..",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015008.1.25",
          "form": "페퍼로니로 화해합시다.",
          "original_form": "페퍼로니로 화해합시다.",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015008.1.26",
          "form": "좋아요 2센치 가까워집시다",
          "original_form": "좋아요 2센치 가까워집시다",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015008.1.27",
          "form": "취존할게요",
          "original_form": "취존할게요",
