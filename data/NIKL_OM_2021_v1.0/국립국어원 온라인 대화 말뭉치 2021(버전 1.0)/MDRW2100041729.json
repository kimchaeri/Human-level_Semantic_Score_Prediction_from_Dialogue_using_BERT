      "utterance": [
        {
          "id": "MDRW2100041729.1.1",
          "form": "맞아요 진짜",
          "original_form": "맞아요 진짜",
          "speaker_id": "2",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100041729.1.2",
          "form": "요기요는 가끔 파바 오천줘서 좋은거같아요",
          "original_form": "요기요는 가끔 파바 오천줘서 좋은거같아요",
          "speaker_id": "2",
          "time": "20210906 15:10"
        },
        {
          "id": "MDRW2100041729.1.3",
          "form": "맞아요 저 그거 있을때 가끔 사먹어요",
          "original_form": "맞아요 저 그거 있을때 가끔 사먹어요",
          "speaker_id": "1",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041729.1.4",
          "form": "가보면 빵 포장 되어있는거 엄청 많더라고요ㅎㅎㅎ",
          "original_form": "가보면 빵 포장 되어있는거 엄청 많더라고요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041729.1.5",
          "form": "그거 있고 점심룰렛같이 잇으면 안살수 없져",
          "original_form": "그거 있고 점심룰렛같이 잇으면 안살수 없져",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041729.1.6",
          "form": "아 저 점심룰렛은 오백원될때 많아서 이천원 되면",
          "original_form": "아 저 점심룰렛은 오백원될때 많아서 이천원 되면",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041729.1.7",
          "form": "무조건ㅋㅋㅋㅋ 시켜욬ㅋㅋㅋ",
          "original_form": "무조건ㅋㅋㅋㅋ 시켜욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041729.1.8",
          "form": "네 주문안하면 손해같고ㅋㅋ",
          "original_form": "네 주문안하면 손해같고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041729.1.9",
          "form": "ㅋㅋㅋ 안하면 빵원인데",
          "original_form": "ㅋㅋㅋ 안하면 빵원인데",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041729.1.10",
          "form": "어차피 잘 먹으니까 ㅋㅋㅋㅋ 괜찮겠죠",
          "original_form": "어차피 잘 먹으니까 ㅋㅋㅋㅋ 괜찮겠죠",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041729.1.11",
          "form": "아 전 치킨이요ㅋㅋㅋ",
          "original_form": "아 전 치킨이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100041729.1.12",
          "form": "치킨은 어떤거 좋아하세요?",
          "original_form": "치킨은 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041729.1.13",
          "form": "고.추바사삭이옄ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "고.추바사삭이옄ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041729.1.14",
          "form": "아ㅋㅋ 전 허니콤보 좋아해요",
          "original_form": "아ㅋㅋ 전 허니콤보 좋아해요",
          "speaker_id": "2",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100041729.1.15",
          "form": "넘나 맛잇어여ㅋ",
          "original_form": "넘나 맛잇어여ㅋ",
          "speaker_id": "2",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100041729.1.16",
          "form": "저희집 앞에 교촌있는데",
          "original_form": "저희집 앞에 교촌있는데",
          "speaker_id": "1",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100041729.1.17",
          "form": "포장해오면 완전 바삭",
          "original_form": "포장해오면 완전 바삭",
          "speaker_id": "1",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100041729.1.18",
          "form": "핫 좋으시겟다ㅠ",
          "original_form": "핫 좋으시겟다ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100041729.1.19",
          "form": "해서 맛있어요 근데 교촌보다 다른걸 더 좋아한다는게 문제",
          "original_form": "해서 맛있어요 근데 교촌보다 다른걸 더 좋아한다는게 문제",
          "speaker_id": "1",
          "time": "20210906 15:18"
        },
        {
