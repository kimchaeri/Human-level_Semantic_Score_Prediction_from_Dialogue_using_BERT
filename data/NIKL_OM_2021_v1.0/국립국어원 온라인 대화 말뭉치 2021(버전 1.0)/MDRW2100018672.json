      "utterance": [
        {
          "id": "MDRW2100018672.1.1",
          "form": "저도요!!!!",
          "original_form": "저도요!!!!",
          "speaker_id": "2",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100018672.1.2",
          "form": "근데 대신들어간 배우 좋아해서 언젠간 보려고 했었어요 ㅋㅋㅋㅋ",
          "original_form": "근데 대신들어간 배우 좋아해서 언젠간 보려고 했었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100018672.1.3",
          "form": "요즘 계속 재탕해서 뭐볼까 하다가 본건데",
          "original_form": "요즘 계속 재탕해서 뭐볼까 하다가 본건데",
          "speaker_id": "2",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100018672.1.4",
          "form": "러블리해요 ㅋㅋ 그 뭐지 안재홍나온드라마 거기도나왔었는데",
          "original_form": "러블리해요 ㅋㅋ 그 뭐지 안재홍나온드라마 거기도나왔었는데",
          "speaker_id": "1",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100018672.1.5",
          "form": "저를 칭찬했잖아요 이제라도 본걸",
          "original_form": "저를 칭찬했잖아요 이제라도 본걸",
          "speaker_id": "2",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100018672.1.6",
          "form": "쌈마이웨이!",
          "original_form": "쌈마이웨이!",
          "speaker_id": "2",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100018672.1.7",
          "form": "대리님ㅜㅜ이러면서 완전 귀욤신입",
          "original_form": "대리님ㅜㅜ이러면서 완전 귀욤신입",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100018672.1.8",
          "form": "아맞어맞어 거기도나왔었죠 ㅋㅋ",
          "original_form": "아맞어맞어 거기도나왔었죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100018672.1.9",
          "form": "좀 얄미운캐릭이었는데 전혀밉지않았어요 너무 러블리해서",
          "original_form": "좀 얄미운캐릭이었는데 전혀밉지않았어요 너무 러블리해서",
          "speaker_id": "1",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100018672.1.10",
          "form": "표예진님 좋아하시는구나~",
          "original_form": "표예진님 좋아하시는구나~",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100018672.1.11",
          "form": "그리고 거기도 나왔었지않나요 이나영나온건데..",
          "original_form": "그리고 거기도 나왔었지않나요 이나영나온건데..",
          "speaker_id": "1",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100018672.1.12",
          "form": "캐릭터들이 좀 약간 여사친? 그런 얄미운 캐릭이 많았죠ㅜ",
          "original_form": "캐릭터들이 좀 약간 여사친? 그런 얄미운 캐릭이 많았죠ㅜ",
          "speaker_id": "2",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100018672.1.13",
          "form": "아아...그 이종석나온 별책부록???",
          "original_form": "아아...그 이종석나온 별책부록???",
          "speaker_id": "2",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100018672.1.14",
          "form": "그건 안봤어요! 뭥가 너무 오글거릴거같아서 ㅋㅋㅋ",
          "original_form": "그건 안봤어요! 뭥가 너무 오글거릴거같아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100018672.1.15",
          "form": "헐 맞아요맞아요",
          "original_form": "헐 맞아요맞아요",
          "speaker_id": "1",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100018672.1.16",
          "form": "완전백과사전이시다 그거 오글거리는 부분은좀있는데 드라마 괜찮았어요 ㅋㅋ",
          "original_form": "완전백과사전이시다 그거 오글거리는 부분은좀있는데 드라마 괜찮았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018672.1.17",
          "form": "오글거리는것도 잘봐요 ㅋㅋㅋ",
          "original_form": "오글거리는것도 잘봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018672.1.18",
          "form": "찜해놔야겠어요ㅋㅋㅋ나중에보게",
          "original_form": "찜해놔야겠어요ㅋㅋㅋ나중에보게",
          "speaker_id": "2",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018672.1.19",
          "form": "그럼 혹시...",
          "original_form": "그럼 혹시...",
          "speaker_id": "2",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018672.1.20",
          "form": "멜로가체질 보셨나요!",
          "original_form": "멜로가체질 보셨나요!",
          "speaker_id": "2",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018672.1.21",
          "form": "아 당연하죠 ㅜㅜ ㅋㅋㅋㅋ",
          "original_form": "아 당연하죠 ㅜㅜ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018672.1.22",
          "form": "저도 어지간한거 거의 다보는편이라서 ㅋㅋㅋㅋ",
          "original_form": "저도 어지간한거 거의 다보는편이라서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100018672.1.23",
          "form": "이것도 정주행하는 리스트 중 하나예요ㅋㅋㅋㅋㅋ 어어저도요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이것도 정주행하는 리스트 중 하나예요ㅋㅋㅋㅋㅋ 어어저도요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100018672.1.24",
          "form": "막 엄청 장르타는 스타일은 아니라",
          "original_form": "막 엄청 장르타는 스타일은 아니라",
          "speaker_id": "2",
          "time": "20210827 10:35"
        }
      ]
    }
