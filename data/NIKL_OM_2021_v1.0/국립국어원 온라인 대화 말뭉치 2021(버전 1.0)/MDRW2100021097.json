      "utterance": [
        {
          "id": "MDRW2100021097.1.1",
          "form": "아 맞ㄹ어",
          "original_form": "아 맞ㄹ어",
          "speaker_id": "2",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100021097.1.2",
          "form": "고등학교때도그런말있었는데",
          "original_form": "고등학교때도그런말있었는데",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021097.1.3",
          "form": "국가에서 받은 거 횡령했네",
          "original_form": "국가에서 받은 거 횡령했네",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021097.1.4",
          "form": "급식비횡령한다고",
          "original_form": "급식비횡령한다고",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021097.1.5",
          "form": "아 ㅇㅈㅇㅈ",
          "original_form": "아 ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021097.1.6",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021097.1.7",
          "form": "진짜 얼마나 맛없으면 그런 말이 다 나오냐",
          "original_form": "진짜 얼마나 맛없으면 그런 말이 다 나오냐",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021097.1.8",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021097.1.9",
          "form": "고등학교때는",
          "original_form": "고등학교때는",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021097.1.10",
          "form": "영양사쌤한번바뀌고",
          "original_form": "영양사쌤한번바뀌고",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021097.1.11",
          "form": "맛없어지지않았음?",
          "original_form": "맛없어지지않았음?",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021097.1.12",
          "form": "어 ㅇㅈ",
          "original_form": "어 ㅇㅈ",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100021097.1.13",
          "form": "1학년초반에",
          "original_form": "1학년초반에",
          "speaker_id": "1",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100021097.1.14",
          "form": "그분은진짜괜찮았는데",
          "original_form": "그분은진짜괜찮았는데",
          "speaker_id": "1",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100021097.1.15",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100021097.1.16",
          "form": "오퍼엄청들어왔다고들음",
          "original_form": "오퍼엄청들어왔다고들음",
          "speaker_id": "1",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100021097.1.17",
          "form": "다른학교에서",
          "original_form": "다른학교에서",
          "speaker_id": "1",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100021097.1.18",
          "form": "아 ㄹㅇ?",
          "original_form": "아 ㄹㅇ?",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100021097.1.19",
          "form": "ㅅ카웃 된거임?",
          "original_form": "ㅅ카웃 된거임?",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100021097.1.20",
          "form": "ㅇㅇㅇ1학년때다른쌤이",
          "original_form": "ㅇㅇㅇ1학년때다른쌤이",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100021097.1.21",
          "form": "말씀해주심",
          "original_form": "말씀해주심",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100021097.1.22",
          "form": "아 에바야 진짜",
          "original_form": "아 에바야 진짜",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100021097.1.23",
          "form": "지금다른학교에서오퍼많이들어와서",
          "original_form": "지금다른학교에서오퍼많이들어와서",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100021097.1.24",
          "form": "다른데갈수도있다고",
          "original_form": "다른데갈수도있다고",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100021097.1.25",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100021097.1.26",
          "form": "가지 마시지.....  내가 학교 다니는 유일한 이유...",
          "original_form": "가지 마시지.....  내가 학교 다니는 유일한 이유...",
          "speaker_id": "2",
          "time": "20210828 15:53"
        }
      ]
