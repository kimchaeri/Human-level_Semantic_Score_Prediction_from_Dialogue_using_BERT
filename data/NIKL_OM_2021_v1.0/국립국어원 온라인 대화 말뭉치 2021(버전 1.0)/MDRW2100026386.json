      "utterance": [
        {
          "id": "MDRW2100026386.1.1",
          "form": "여행은 자주 가세요?",
          "original_form": "여행은 자주 가세요?",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026386.1.2",
          "form": "자주가고싶은데 자주못가는것같아요.",
          "original_form": "자주가고싶은데 자주못가는것같아요.",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026386.1.3",
          "form": "코로나도있구ㅠ",
          "original_form": "코로나도있구ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026386.1.4",
          "form": "여름휴가다녀오셨어요?",
          "original_form": "여름휴가다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026386.1.5",
          "form": "네~ 호캉스 했어요 ㅋㅋ",
          "original_form": "네~ 호캉스 했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026386.1.6",
          "form": "어디 가기에는 엄두가 안나서 호텔에서만 놀고 먹었어요!",
          "original_form": "어디 가기에는 엄두가 안나서 호텔에서만 놀고 먹었어요!",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026386.1.7",
          "form": "호캉스~~예얍!!",
          "original_form": "호캉스~~예얍!!",
          "speaker_id": "1",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026386.1.8",
          "form": "요즘엔 특히 이쁘고 좋은 숙소가 더 가고싶어요",
          "original_form": "요즘엔 특히 이쁘고 좋은 숙소가 더 가고싶어요",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026386.1.9",
          "form": "돌아다닐수없이니",
          "original_form": "돌아다닐수없이니",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026386.1.10",
          "form": "그쵸ㅠㅠ 막 해외도 못가고요...",
          "original_form": "그쵸ㅠㅠ 막 해외도 못가고요...",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026386.1.11",
          "form": "해외여행을 못가다니ㅠㅠ 흑흑",
          "original_form": "해외여행을 못가다니ㅠㅠ 흑흑",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026386.1.12",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026386.1.13",
          "form": "1년에한번은 나가줘야 하는데",
          "original_form": "1년에한번은 나가줘야 하는데",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026386.1.14",
          "form": "그낙으로 살았는데",
          "original_form": "그낙으로 살았는데",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026386.1.15",
          "form": "저도요ㅠㅠ 에고ㅠㅠ 너무 해외여행가고 싶네요",
          "original_form": "저도요ㅠㅠ 에고ㅠㅠ 너무 해외여행가고 싶네요",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026386.1.16",
          "form": "원래 호주도 가고 그러고 싶었거든요",
          "original_form": "원래 호주도 가고 그러고 싶었거든요",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026386.1.17",
          "form": "이젠.. 안되는..ㅠ",
          "original_form": "이젠.. 안되는..ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026386.1.18",
          "form": "맞다 저 여름휴가 에어비앤비로갔는데",
          "original_form": "맞다 저 여름휴가 에어비앤비로갔는데",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026386.1.19",
          "form": "저 호주 갔어요!",
          "original_form": "저 호주 갔어요!",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026386.1.20",
          "form": "신행으로",
          "original_form": "신행으로",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026386.1.21",
          "form": "근데 숙소가 사진으로봐도 이쁜곳이였는데. 들어오는시간에맞춰서 음악틀어져있고. 캔들 향 너무좋고. 방조명 다 켜져있어서 너무좋았어요",
          "original_form": "근데 숙소가 사진으로봐도 이쁜곳이였는데. 들어오는시간에맞춰서 음악틀어져있고. 캔들 향 너무좋고. 방조명 다 켜져있어서 너무좋았어요",
          "speaker_id": "1",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026386.1.22",
          "form": "우와 너무 좋았겠어요!",
          "original_form": "우와 너무 좋았겠어요!",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026386.1.23",
          "form": "그렇게 다 되어있는 곳이면 제대로 힐링 기분 날 것 같아요ㅠㅠ",
          "original_form": "그렇게 다 되어있는 곳이면 제대로 힐링 기분 날 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026386.1.24",
          "form": "호주 어때요?",
          "original_form": "호주 어때요?",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026386.1.25",
          "form": "호주는 음. . . 많이 특별하지는않았어요",
          "original_form": "호주는 음. . . 많이 특별하지는않았어요",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026386.1.26",
          "form": "그냥 도시! ? ㅎㅎ",
          "original_form": "그냥 도시! ? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026386.1.27",
          "form": "오 그렇군요! 그래도 가보고 싶어요ㅠㅠ",
          "original_form": "오 그렇군요! 그래도 가보고 싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026386.1.28",
          "form": "해외여행 제일 좋았던 곳은 어디세요?",
          "original_form": "해외여행 제일 좋았던 곳은 어디세요?",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026386.1.29",
          "form": "체코!  프라하요!",
          "original_form": "체코!  프라하요!",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026386.1.30",
          "form": "으악 프라하 저도 가봤어요 ㅋㅋㅋ 패키지로",
          "original_form": "으악 프라하 저도 가봤어요 ㅋㅋㅋ 패키지로",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026386.1.31",
          "form": "호주는 어디를 가보고싶으세요?",
          "original_form": "호주는 어디를 가보고싶으세요?",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026386.1.32",
          "form": "야경이 너무 예쁘더라구요",
          "original_form": "야경이 너무 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026386.1.33",
          "form": "오 프라하 가보셨군요",
          "original_form": "오 프라하 가보셨군요",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026386.1.34",
          "form": "호주는 그냥 자연을 즐기러?ㅋㅋ 딱히 생각해두진 않았어요!",
          "original_form": "호주는 그냥 자연을 즐기러?ㅋㅋ 딱히 생각해두진 않았어요!",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026386.1.35",
          "form": "네 프라하에서 거의 커피마시고 길거리돌아다니고 그랬던 기억이 나요 ㅋㅋㅋ",
          "original_form": "네 프라하에서 거의 커피마시고 길거리돌아다니고 그랬던 기억이 나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026386.1.36",
          "form": "저는 그냥 관광지 많이 안가구. 내집골목다니듯이 다녀보는 느낌으로 갔어요",
          "original_form": "저는 그냥 관광지 많이 안가구. 내집골목다니듯이 다녀보는 느낌으로 갔어요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026386.1.37",
          "form": "길거리돌아다니는것만해도 좋았어요",
          "original_form": "길거리돌아다니는것만해도 좋았어요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026386.1.38",
          "form": "우와 그게 좋죠~",
          "original_form": "우와 그게 좋죠~",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026386.1.39",
          "form": "진짜 제대로 즐기셨네요!",
          "original_form": "진짜 제대로 즐기셨네요!",
          "speaker_id": "2",
          "time": "20210901 22:12"
        }
      ]
    }
  ]
}
