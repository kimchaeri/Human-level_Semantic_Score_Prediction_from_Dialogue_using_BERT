        {
          "id": "MDRW2100027098.1.1",
          "form": "name1도 재탕하시나요",
          "original_form": "&name1&도 재탕하시나요",
          "speaker_id": "2",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.2",
          "form": "다먹을때까지",
          "original_form": "다먹을때까지",
          "speaker_id": "2",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.3",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.4",
          "form": "네 주전문입니다",
          "original_form": "네 주전문입니다",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.5",
          "form": "남은것 처리합니다",
          "original_form": "남은것 처리합니다",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.6",
          "form": "어떤요리를 재탕해드시나요",
          "original_form": "어떤요리를 재탕해드시나요",
          "speaker_id": "2",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.7",
          "form": "감자탕 일주일째 먹습니다",
          "original_form": "감자탕 일주일째 먹습니다",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.8",
          "form": "입에서 고기향이 납니다",
          "original_form": "입에서 고기향이 납니다",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.9",
          "form": "일주일이면 좀 많이 길군요",
          "original_form": "일주일이면 좀 많이 길군요",
          "speaker_id": "2",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.10",
          "form": "ㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.11",
          "form": "어머니가 한솥 끓이셔서",
          "original_form": "어머니가 한솥 끓이셔서",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.12",
          "form": "전에 오뎅도",
          "original_form": "전에 오뎅도",
          "speaker_id": "2",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027098.1.13",
          "form": "계속 드시지 않았나요",
          "original_form": "계속 드시지 않았나요",
          "speaker_id": "2",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027098.1.14",
          "form": "오뎅은 다릅니다",
          "original_form": "오뎅은 다릅니다",
          "speaker_id": "1",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027098.1.15",
          "form": "질리지 않지요",
          "original_form": "질리지 않지요",
          "speaker_id": "1",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027098.1.16",
          "form": "몇달 드신거같은데",
          "original_form": "몇달 드신거같은데",
          "speaker_id": "2",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027098.1.17",
          "form": "조리법을 다르게하나요",
          "original_form": "조리법을 다르게하나요",
          "speaker_id": "2",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027098.1.18",
          "form": "감자튀김도 일주일먹습니다",
          "original_form": "감자튀김도 일주일먹습니다",
          "speaker_id": "1",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027098.1.19",
          "form": "감자가 많으시군요",
          "original_form": "감자가 많으시군요",
          "speaker_id": "2",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027098.1.20",
          "form": "고구마도 맛있습니다",
          "original_form": "고구마도 맛있습니다",
          "speaker_id": "2",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027098.1.21",
          "form": "감자튀김 한 봉지가 저렴합니다",
          "original_form": "감자튀김 한 봉지가 저렴합니다",
          "speaker_id": "1",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027098.1.22",
          "form": "감자로 떠먹는 피자 해먹으면 헤븐입니다",
          "original_form": "감자로 떠먹는 피자 해먹으면 헤븐입니다",
          "speaker_id": "2",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100027098.1.23",
          "form": "고구마도 튀기면 맛있죠",
          "original_form": "고구마도 튀기면 맛있죠",
          "speaker_id": "1",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100027098.1.24",
          "form": "해서 드시나요",
          "original_form": "해서 드시나요",
          "speaker_id": "1",
          "time": "20210902 09:29"
        }
      ]
