      "utterance": [
        {
          "id": "MDRW2100021088.1.1",
          "form": "나 제주도 간다",
          "original_form": "나 제주도 간다",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100021088.1.2",
          "form": "친구들과 우도도 가고 맛집도 갈거야",
          "original_form": "친구들과 우도도 가고 맛집도 갈거야",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100021088.1.3",
          "form": "숙소는 서귀포호텔 예약했어",
          "original_form": "숙소는 서귀포호텔 예약했어",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100021088.1.4",
          "form": "보상이. 안된다는데",
          "original_form": "보상이. 안된다는데",
          "speaker_id": "1",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021088.1.5",
          "form": "무슨보상?",
          "original_form": "무슨보상?",
          "speaker_id": "2",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021088.1.6",
          "form": "제주도 가본곳중 좋았던곳 추천해줘",
          "original_form": "제주도 가본곳중 좋았던곳 추천해줘",
          "speaker_id": "1",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021088.1.7",
          "form": "글쎄",
          "original_form": "글쎄",
          "speaker_id": "2",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021088.1.8",
          "form": "제주맥주 견학추천",
          "original_form": "제주맥주 견학추천",
          "speaker_id": "2",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021088.1.9",
          "form": "미리 예약해야하고",
          "original_form": "미리 예약해야하고",
          "speaker_id": "2",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100021088.1.10",
          "form": "예약하고 가나?",
          "original_form": "예약하고 가나?",
          "speaker_id": "1",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100021088.1.11",
          "form": "지금은 가능할지 모르겠다",
          "original_form": "지금은 가능할지 모르겠다",
          "speaker_id": "2",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100021088.1.12",
          "form": "엉. 예약해야해",
          "original_form": "엉. 예약해야해",
          "speaker_id": "2",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100021088.1.13",
          "form": "아 그럴수도 있겠네",
          "original_form": "아 그럴수도 있겠네",
          "speaker_id": "1",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100021088.1.14",
          "form": "검색해보구 갈수있으면 가봐야겠다",
          "original_form": "검색해보구 갈수있으면 가봐야겠다",
          "speaker_id": "1",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100021088.1.15",
          "form": "그리고 납득이 나오는 카페도 좋았는데",
          "original_form": "그리고 납득이 나오는 카페도 좋았는데",
          "speaker_id": "2",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100021088.1.16",
          "form": "건축학개론에 나오는 서연이네집?",
          "original_form": "건축학개론에 나오는 서연이네집?",
          "speaker_id": "2",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100021088.1.17",
          "form": "서연이네카페?",
          "original_form": "서연이네카페?",
          "speaker_id": "2",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100021088.1.18",
          "form": "친구들과 같이 가는 장소는 확실히 달라",
          "original_form": "친구들과 같이 가는 장소는 확실히 달라",
          "speaker_id": "1",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100021088.1.19",
          "form": "그렇지 멤버가 어케되냐에 따라 다르지",
          "original_form": "그렇지 멤버가 어케되냐에 따라 다르지",
          "speaker_id": "2",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100021088.1.20",
          "form": "여자끼리가야 더 재밌지",
          "original_form": "여자끼리가야 더 재밌지",
          "speaker_id": "1",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100021088.1.21",
          "form": "맞아. 코드도 맞고",
          "original_form": "맞아. 코드도 맞고",
          "speaker_id": "2",
          "time": "20210828 16:48"
        }
