      "utterance": [
        {
          "id": "MDRW2100007224.1.1",
          "form": "먹는거 좋아하세요?",
          "original_form": "먹는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007224.1.2",
          "form": "너무 좋아하죠!",
          "original_form": "너무 좋아하죠!",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007224.1.3",
          "form": "매운 거 좋아하세요 혹시?",
          "original_form": "매운 거 좋아하세요 혹시?",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007224.1.4",
          "form": "넼ㅋ  한국인이라면 매운거!",
          "original_form": "넼ㅋ  한국인이라면 매운거!",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007224.1.5",
          "form": "불닭볶음면 처음 나왔을 때 너무 좋아했어요",
          "original_form": "불닭볶음면 처음 나왔을 때 너무 좋아했어요",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007224.1.6",
          "form": "저는 매운 걸 좋아하는데 잘 못 먹어서",
          "original_form": "저는 매운 걸 좋아하는데 잘 못 먹어서",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007224.1.7",
          "form": "잘 드시는 분들 너무 부럽더라구요ㅠ",
          "original_form": "잘 드시는 분들 너무 부럽더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007224.1.8",
          "form": "ㅋㅋㅋㅋㅋ 저는 옛날에는 잘먹었는데 지금은 속이 아파서 가끔씩 먹는 걸로 만족하고 있어요!",
          "original_form": "ㅋㅋㅋㅋㅋ 저는 옛날에는 잘먹었는데 지금은 속이 아파서 가끔씩 먹는 걸로 만족하고 있어요!",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007224.1.9",
          "form": "양식한식중식 중 주로 선호하는 음식 있으세요?",
          "original_form": "양식한식중식 중 주로 선호하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007224.1.10",
          "form": "저는 중식 좋아해요! 마라탕 정말 좋아했는데 요즘은 못 먹고 있네요ㅠㅠ",
          "original_form": "저는 중식 좋아해요! 마라탕 정말 좋아했는데 요즘은 못 먹고 있네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007224.1.11",
          "form": "어떤 음식 좋아하세요??",
          "original_form": "어떤 음식 좋아하세요??",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007224.1.12",
          "form": "마라탕! 저도 좋아해요",
          "original_form": "마라탕! 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007224.1.13",
          "form": "저는 요즘 한식이나 아시아음식이요",
          "original_form": "저는 요즘 한식이나 아시아음식이요",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007224.1.14",
          "form": "담백한거!",
          "original_form": "담백한거!",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007224.1.15",
          "form": "오 담백한 거 좋죠",
          "original_form": "오 담백한 거 좋죠",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007224.1.16",
          "form": "쌀국수 이런 것도 좋아하세요?",
          "original_form": "쌀국수 이런 것도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007224.1.17",
          "form": "쌀국수보단 같은 베트남 음식 중  반미 정말 좋아해요",
          "original_form": "쌀국수보단 같은 베트남 음식 중  반미 정말 좋아해요",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007224.1.18",
          "form": "반미 많이 먹어보진 않았는데",
          "original_form": "반미 많이 먹어보진 않았는데",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007224.1.19",
          "form": "되게 맛있게 먹었어요!",
          "original_form": "되게 맛있게 먹었어요!",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007224.1.20",
          "form": "저 바게뜨도 좋아해서",
          "original_form": "저 바게뜨도 좋아해서",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007224.1.21",
          "form": "입맛에 맞더라구용",
          "original_form": "입맛에 맞더라구용",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007224.1.22",
          "form": "마져요 바삭한 바게트느낌",
          "original_form": "마져요 바삭한 바게트느낌",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007224.1.23",
          "form": "ㅋㅋㅋㅋ 밤되니깐 배고프네요",
          "original_form": "ㅋㅋㅋㅋ 밤되니깐 배고프네요",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007224.1.24",
          "form": "마자여 새벽에 음식생각하니까 침나와요",
          "original_form": "마자여 새벽에 음식생각하니까 침나와요",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007224.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007224.1.26",
          "form": "까페 가는거 좋아하시나요?",
          "original_form": "까페 가는거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007224.1.27",
          "form": "카페 넘 좋아해요!",
          "original_form": "카페 넘 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007224.1.28",
          "form": "어릴 땐 단 거 먹으러 갔는데",
          "original_form": "어릴 땐 단 거 먹으러 갔는데",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007224.1.29",
          "form": "이제는 아메리카노가 최고더라구요 저는",
          "original_form": "이제는 아메리카노가 최고더라구요 저는",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007224.1.30",
          "form": "ㅋㅋㅋㅋ 아 너무 공감",
          "original_form": "ㅋㅋㅋㅋ 아 너무 공감",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007224.1.31",
          "form": "그땐 이거 왜먹지 했는데 ㅋㅋㅋㅋ",
          "original_form": "그땐 이거 왜먹지 했는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:42"
        }
      ]
