      "utterance": [
        {
          "id": "MDRW2100005940.1.1",
          "form": "안녕하세요 무슨 음식 좋아하세요",
          "original_form": "안녕하세요 무슨 음식 좋아하세요",
          "speaker_id": "1",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100005940.1.2",
          "form": "저는 싫어하는 음식이 별로없는데",
          "original_form": "저는 싫어하는 음식이 별로없는데",
          "speaker_id": "2",
          "time": "20210907 16:59"
        },
        {
          "id": "MDRW2100005940.1.3",
          "form": "든든하게 먹는걸 좋아해요",
          "original_form": "든든하게 먹는걸 좋아해요",
          "speaker_id": "2",
          "time": "20210907 16:59"
        },
        {
          "id": "MDRW2100005940.1.4",
          "form": "오 저도요 고기도 좋아하시나요",
          "original_form": "오 저도요 고기도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100005940.1.5",
          "form": "고기는 정말 종류 안 가리고 좋아해요",
          "original_form": "고기는 정말 종류 안 가리고 좋아해요",
          "speaker_id": "2",
          "time": "20210907 17:02"
        },
        {
          "id": "MDRW2100005940.1.6",
          "form": "저는 삼겹살!!! 제일좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저는 삼겹살!!! 제일좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100005940.1.7",
          "form": "삼겹살 좋죠 저 오늘 엄청 땡겨서 점심에 먹었어요",
          "original_form": "삼겹살 좋죠 저 오늘 엄청 땡겨서 점심에 먹었어요",
          "speaker_id": "2",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100005940.1.8",
          "form": "맛있죠ㅋㅋ저는 수육이 제일 좋아요",
          "original_form": "맛있죠ㅋㅋ저는 수육이 제일 좋아요",
          "speaker_id": "1",
          "time": "20210907 17:34"
        },
        {
          "id": "MDRW2100005940.1.9",
          "form": "크 점심으로",
          "original_form": "크 점심으로",
          "speaker_id": "1",
          "time": "20210907 17:34"
        },
        {
          "id": "MDRW2100005940.1.10",
          "form": "수육 좋은데 집에 안살면 해먹기가 어려워요",
          "original_form": "수육 좋은데 집에 안살면 해먹기가 어려워요",
          "speaker_id": "2",
          "time": "20210907 17:37"
        },
        {
          "id": "MDRW2100005940.1.11",
          "form": "오 어디사시는데요??",
          "original_form": "오 어디사시는데요??",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100005940.1.12",
          "form": "기숙사 살아서 취사가 안돼요",
          "original_form": "기숙사 살아서 취사가 안돼요",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100005940.1.13",
          "form": "아 세상에 그러면 하기어렵죠 ㅠㅠ 맨날 사두셔야겠다",
          "original_form": "아 세상에 그러면 하기어렵죠 ㅠㅠ 맨날 사두셔야겠다",
          "speaker_id": "1",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100005940.1.14",
          "form": "맨날 메뉴정하기전에 통장한테 물어보고요",
          "original_form": "맨날 메뉴정하기전에 통장한테 물어보고요",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100005940.1.15",
          "form": "앜ㅋㅋㅋ 통장아 괜찮니?",
          "original_form": "앜ㅋㅋㅋ 통장아 괜찮니?",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100005940.1.16",
          "form": "이제 안괜찮아서 못먹어요 ㅋㅋ",
          "original_form": "이제 안괜찮아서 못먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100005940.1.17",
          "form": "고기 자주 드세요?",
          "original_form": "고기 자주 드세요?",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100005940.1.18",
          "form": "자주먹어요ㅋㅋㅋ 제일 만만한게 고기라",
          "original_form": "자주먹어요ㅋㅋㅋ 제일 만만한게 고기라",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100005940.1.19",
          "form": "ㅋㅋ 수육이 제일 만만한가요?",
          "original_form": "ㅋㅋ 수육이 제일 만만한가요?",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100005940.1.20",
          "form": "아 아뇨 ^___ㅜ 굽는 고기가 제일 만만..",
          "original_form": "아 아뇨 ^___ㅜ 굽는 고기가 제일 만만..",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100005940.1.21",
          "form": "아 굽는거 진짜 부럽다 ㅠ",
          "original_form": "아 굽는거 진짜 부럽다 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100005940.1.22",
          "form": "ㅋㅋㅋㅋ 저도 본가갈때만 먹어요 그런뎈ㅋㅋㅋ 본가자주가서",
