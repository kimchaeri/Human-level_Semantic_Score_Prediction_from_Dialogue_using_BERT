      "utterance": [
        {
          "id": "MDRW2100006517.1.1",
          "form": "오늘 날씨 어때요",
          "original_form": "오늘 날씨 어때요",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006517.1.2",
          "form": "흐려요 비온다는 얘기도 있고요",
          "original_form": "흐려요 비온다는 얘기도 있고요",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100006517.1.3",
          "form": "여긴 아침내 흐려보이더니 또 해가 나요",
          "original_form": "여긴 아침내 흐려보이더니 또 해가 나요",
          "speaker_id": "1",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100006517.1.4",
          "form": "앗 부러워요 해 좀 쨍하게 났으면 좋겠어요",
          "original_form": "앗 부러워요 해 좀 쨍하게 났으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100006517.1.5",
          "form": "그렇죠 그래도 날씨가 변덕스러워서 습도는 높은 편이에요",
          "original_form": "그렇죠 그래도 날씨가 변덕스러워서 습도는 높은 편이에요",
          "speaker_id": "1",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100006517.1.6",
          "form": "너무 날이 눅눅해요ㅠㅠ",
          "original_form": "너무 날이 눅눅해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006517.1.7",
          "form": "제습기가 매일 열일합니다",
          "original_form": "제습기가 매일 열일합니다",
          "speaker_id": "1",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100006517.1.8",
          "form": "저희집은 제습기 없어서 그냥 습기와 친구하고 있어요",
          "original_form": "저희집은 제습기 없어서 그냥 습기와 친구하고 있어요",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100006517.1.9",
          "form": "옷장 조심하세요 ㅠㅠ",
          "original_form": "옷장 조심하세요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006517.1.10",
          "form": "옷장에 제습제도 바꿔줘야하는데ㅠㅠ",
          "original_form": "옷장에 제습제도 바꿔줘야하는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006517.1.11",
          "form": "네 지난 겨울 곰팡이 사단이 나서 제습기 엄청 돌려요 ㅠㅠ",
          "original_form": "네 지난 겨울 곰팡이 사단이 나서 제습기 엄청 돌려요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006517.1.12",
          "form": "으 ㅠㅠ 곰팡이는 어떻게 그렇게 잘 알고 피는지ㅠㅠ",
          "original_form": "으 ㅠㅠ 곰팡이는 어떻게 그렇게 잘 알고 피는지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100006517.1.13",
          "form": "정말 싫어요 ㅠ 옷 정리 많이 했네요",
