          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039450.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039450.1.11",
          "form": "사실 name2님은 affiliation 졸업자가",
          "original_form": "사실 &name2&님은 &affiliation& 졸업자가",
          "speaker_id": "1",
          "time": "20210903 18:04"
        },
        {
