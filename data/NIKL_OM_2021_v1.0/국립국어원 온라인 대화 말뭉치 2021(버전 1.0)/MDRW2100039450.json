      "utterance": [
        {
          "id": "MDRW2100039450.1.1",
          "form": "굳이 따지면 그 노래도 나 친구한테 추천 받은 건데....",
          "original_form": "굳이 따지면 그 노래도 나 친구한테 추천 받은 건데....",
          "speaker_id": "1",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039450.1.2",
          "form": "그렇구나...ㅋㅋㅋㅋ",
          "original_form": "그렇구나...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039450.1.3",
          "form": "회사에 서원대 출신도 많구",
          "original_form": "회사에 서원대 출신도 많구",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039450.1.4",
          "form": "충북여고 출신도 많다",
          "original_form": "충북여고 출신도 많다",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039450.1.5",
          "form": "다들 학교다닐때 봤을까",
          "original_form": "다들 학교다닐때 봤을까",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100039450.1.6",
          "form": "나 인사 안하고 댕겼는디",
          "original_form": "나 인사 안하고 댕겼는디",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100039450.1.7",
          "form": "같은 학년 아님 모르지 않나? 우리가 넘 무심한같ㅋㅋㅌㅋㅋ",
          "original_form": "같은 학년 아님 모르지 않나? 우리가 넘 무심한같ㅋㅋㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039450.1.8",
          "form": "다 모름",
          "original_form": "다 모름",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039450.1.9",
          "form": "자꾸 누구아냐고 물어보는데",
          "original_form": "자꾸 누구아냐고 물어보는데",
