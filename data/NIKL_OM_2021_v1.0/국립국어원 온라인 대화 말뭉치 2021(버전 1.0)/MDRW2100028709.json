      "utterance": [
        {
          "id": "MDRW2100028709.1.1",
          "form": "요거트는 요즘 그럭저럭 달게 나와서 잘 사먹어요",
          "original_form": "요거트는 요즘 그럭저럭 달게 나와서 잘 사먹어요",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028709.1.2",
          "form": "요거트도 요즘 달달하게 나와서 잘 사먹고 있어요",
          "original_form": "요거트도 요즘 달달하게 나와서 잘 사먹고 있어요",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028709.1.3",
          "form": "오늘 장볼때 깜빡 잊고 안샀네요",
          "original_form": "오늘 장볼때 깜빡 잊고 안샀네요",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028709.1.4",
          "form": "그렇군요ㅋㅋ",
          "original_form": "그렇군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028709.1.5",
          "form": "오늘 장 볼때 뭐 샀어요??",
          "original_form": "오늘 장 볼때 뭐 샀어요??",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028709.1.6",
          "form": "그냥 여러가지 야채랑 음료수도 좀 사고",
          "original_form": "그냥 여러가지 야채랑 음료수도 좀 사고",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028709.1.7",
          "form": "센베이 과자 동나기 전에 더 사왔어요",
          "original_form": "센베이 과자 동나기 전에 더 사왔어요",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028709.1.8",
          "form": "ㅋㅋㅋ 진짜 좋아하나보네요",
          "original_form": "ㅋㅋㅋ 진짜 좋아하나보네요",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028709.1.9",
          "form": "혼자 살아요? 장 보는거 보니까",
          "original_form": "혼자 살아요? 장 보는거 보니까",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028709.1.10",
          "form": "혼자 사는건 아니고 아부지랑 같이 갔다왔어요",
          "original_form": "혼자 사는건 아니고 아부지랑 같이 갔다왔어요",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028709.1.11",
          "form": "부모님 장 보는거 같이가면 재밌어요ㅋㅋ",
          "original_form": "부모님 장 보는거 같이가면 재밌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028709.1.12",
          "form": "맞아요 카트 끄는 스킬도 남다르시구",
          "original_form": "맞아요 카트 끄는 스킬도 남다르시구",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028709.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028709.1.14",
          "form": "요즘 사고 싶은거 있어요??",
          "original_form": "요즘 사고 싶은거 있어요??",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028709.1.15",
          "form": "전 엊그젠가 당근거래로 책을 사왔어요",
          "original_form": "전 엊그젠가 당근거래로 책을 사왔어요",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028709.1.16",
          "form": "따로 없어요",
          "original_form": "따로 없어요",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028709.1.17",
          "form": "어떤 책 사셨나요?",
          "original_form": "어떤 책 사셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028709.1.18",
          "form": "위대한 개츠비요",
          "original_form": "위대한 개츠비요",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028709.1.19",
          "form": "동생에 영화보고 재밌다길래ㅋㅋ",
          "original_form": "동생에 영화보고 재밌다길래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028709.1.20",
          "form": "그 디카프리오 나오는거 맞나요?",
          "original_form": "그 디카프리오 나오는거 맞나요?",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028709.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028709.1.22",
          "form": "와인잔 들고 씨익 웃던거 ㅋㅋ",
          "original_form": "와인잔 들고 씨익 웃던거 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028709.1.23",
          "form": "짤로 많이 봤어요",
          "original_form": "짤로 많이 봤어요",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028709.1.24",
          "form": "앜ㅋㅋㅋ그쵸 맞아요 유명하죠",
          "original_form": "앜ㅋㅋㅋ그쵸 맞아요 유명하죠",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028709.1.25",
          "form": "당근이 은근 유용해요 안쓰는 물건 팔기에도",
          "original_form": "당근이 은근 유용해요 안쓰는 물건 팔기에도",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028709.1.26",
          "form": "이참에 몇개 팔아볼까요",
          "original_form": "이참에 몇개 팔아볼까요",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028709.1.27",
          "form": "ㅋㅋㅋ 안쓰는 것들 함 싹 정리해요!",
          "original_form": "ㅋㅋㅋ 안쓰는 것들 함 싹 정리해요!",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028709.1.28",
          "form": "버리기는 아깝고 안쓰는 것들 싸게 팔면 훨씬 괜찮은 것 같아요",
          "original_form": "버리기는 아깝고 안쓰는 것들 싸게 팔면 훨씬 괜찮은 것 같아요",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028709.1.29",
          "form": "옷정리 한번 싹 해야겠네요",
          "original_form": "옷정리 한번 싹 해야겠네요",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028709.1.30",
          "form": "네 저도 많이 팔았어요 ㅋㅋ",
          "original_form": "네 저도 많이 팔았어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100028709.1.31",
          "form": "옷이 취향이 바뀌면 잘 안입잖아요",
          "original_form": "옷이 취향이 바뀌면 잘 안입잖아요",
          "speaker_id": "2",
          "time": "20210903 14:17"
        }
      ]
