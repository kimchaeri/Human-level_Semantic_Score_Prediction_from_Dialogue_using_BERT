      "utterance": [
        {
          "id": "MDRW2100010560.1.1",
          "form": "최근에 뭐 사셨어요?",
          "original_form": "최근에 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010560.1.2",
          "form": "소설 해외판이요ㅋ",
          "original_form": "소설 해외판이요ㅋ",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010560.1.3",
          "form": "아직 오지 않았네요",
          "original_form": "아직 오지 않았네요",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010560.1.4",
          "form": "와 외국어도 하시는구나",
          "original_form": "와 외국어도 하시는구나",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010560.1.5",
          "form": "저는 최근에 인형 샀어요",
          "original_form": "저는 최근에 인형 샀어요",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010560.1.6",
          "form": "해산물 인형 ㅋㅋㅋ",
          "original_form": "해산물 인형 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010560.1.7",
          "form": "귀여울거 같네요",
          "original_form": "귀여울거 같네요",
          "speaker_id": "1",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010560.1.8",
          "form": "저도 인형을 사보고싶어요",
          "original_form": "저도 인형을 사보고싶어요",
          "speaker_id": "1",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010560.1.9",
          "form": "요즘은 또 목이 너무 아파서",
          "original_form": "요즘은 또 목이 너무 아파서",
          "speaker_id": "2",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010560.1.10",
          "form": "베개 사려고 찾아보고있어요",
          "original_form": "베개 사려고 찾아보고있어요",
          "speaker_id": "2",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010560.1.11",
          "form": "템퍼 베개가 좋아보이던데",
          "original_form": "템퍼 베개가 좋아보이던데",
          "speaker_id": "2",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010560.1.12",
          "form": "잘 찾아보고 사면 좋을거 같네요",
          "original_form": "잘 찾아보고 사면 좋을거 같네요",
          "speaker_id": "1",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010560.1.13",
          "form": "필요한건 사는게 좋져",
          "original_form": "필요한건 사는게 좋져",
          "speaker_id": "1",
          "time": "20210823 19:47"
        },
        {
          "id": "MDRW2100010560.1.14",
          "form": "요샌 그냥 예뻐서 폰을 또 사고싶더라고요",
          "original_form": "요샌 그냥 예뻐서 폰을 또 사고싶더라고요",
          "speaker_id": "1",
          "time": "20210823 19:47"
        },
        {
          "id": "MDRW2100010560.1.15",
          "form": "어떤 거 사고싶으세요?",
          "original_form": "어떤 거 사고싶으세요?",
          "speaker_id": "2",
          "time": "20210823 19:47"
        },
        {
          "id": "MDRW2100010560.1.16",
          "form": "아이폰도 색이 예뻐서 탐나는데",
          "original_form": "아이폰도 색이 예뻐서 탐나는데",
          "speaker_id": "1",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010560.1.17",
          "form": "오 무슨색이요",
          "original_form": "오 무슨색이요",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010560.1.18",
          "form": "플립도 엄청 예쁘게 잘뽑혔더라구요",
          "original_form": "플립도 엄청 예쁘게 잘뽑혔더라구요",
          "speaker_id": "1",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010560.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010560.1.20",
          "form": "플립 이번거",
          "original_form": "플립 이번거",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010560.1.21",
          "form": "비스포크 색감에서 따왔는지",
          "original_form": "비스포크 색감에서 따왔는지",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010560.1.22",
          "form": "깔맞춤에 이쁘다라구요 ㅋㅋ",
          "original_form": "깔맞춤에 이쁘다라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010560.1.23",
          "form": "ㅋㅋㅋㅋ깔맞춤은 참을 수 없져",
          "original_form": "ㅋㅋㅋㅋ깔맞춤은 참을 수 없져",
          "speaker_id": "1",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010560.1.24",
          "form": "아 나도 폰 바꾸고싶다...",
          "original_form": "아 나도 폰 바꾸고싶다...",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010560.1.25",
          "form": "최근에 탭 s7들여서",
          "original_form": "최근에 탭 s7들여서",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010560.1.26",
          "form": "긴축재정 해야하지만요ㅠ",
          "original_form": "긴축재정 해야하지만요ㅠ",
          "speaker_id": "2",
          "time": "20210823 19:49"
        }
      ]
    }
