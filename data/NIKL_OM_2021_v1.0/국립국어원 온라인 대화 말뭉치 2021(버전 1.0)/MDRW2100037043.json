        {
          "id": "MDRW2100037043.1.2",
          "form": "전에 유행하던 달고나 라떼 해먹어본 적 있어?",
          "original_form": "전에 유행하던 달고나 라떼 해먹어본 적 있어?",
          "speaker_id": "1",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100037043.1.3",
          "form": "달고나는 먹어봤는데 그건 안 먹어 봤네",
          "original_form": "달고나는 먹어봤는데 그건 안 먹어 봤네",
          "speaker_id": "2",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100037043.1.4",
          "form": "커피에 설탕을 넣고   수천 번 젖는 거야",
          "original_form": "커피에 설탕을 넣고   수천 번 젖는 거야",
          "speaker_id": "1",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100037043.1.5",
          "form": "어깨 아프겠다",
          "original_form": "어깨 아프겠다",
          "speaker_id": "2",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100037043.1.6",
          "form": "맞아 그 재미로 하는 거야",
          "original_form": "맞아 그 재미로 하는 거야",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100037043.1.7",
          "form": "맛은 어때",
          "original_form": "맛은 어때",
          "speaker_id": "2",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100037043.1.8",
          "form": "달고나 맛이라서 진짜 맛있어",
          "original_form": "달고나 맛이라서 진짜 맛있어",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100037043.1.9",
          "form": "나도 다음에 한번 사 먹어봐야겠다",
          "original_form": "나도 다음에 한번 사 먹어봐야겠다",
          "speaker_id": "2",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100037043.1.10",
          "form": "까먹지 말고 만들어봐",
          "original_form": "까먹지 말고 만들어봐",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100037043.1.11",
          "form": "그냥 사 먹을게",
          "original_form": "그냥 사 먹을게",
          "speaker_id": "2",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100037043.1.12",
          "form": "너는 무슨 커피 좋아해",
          "original_form": "너는 무슨 커피 좋아해",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100037043.1.13",
          "form": "난 요즘 프라푸치노가 좋아",
          "original_form": "난 요즘 프라푸치노가 좋아",
          "speaker_id": "2",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100037043.1.14",
          "form": "무슨 맛을 좋아해?",
          "original_form": "무슨 맛을 좋아해?",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100037043.1.15",
          "form": "녹차 프라푸치노가 제일 맛있어",
          "original_form": "녹차 프라푸치노가 제일 맛있어",
