      "utterance": [
        {
          "id": "MDRW2100030383.1.1",
          "form": "카카오페이지에서 로맨스 소설 읽고 있어요",
          "original_form": "카카오페이지에서 로맨스 소설 읽고 있어요",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030383.1.2",
          "form": "가슴이 간질간질 흐뭇하네요",
          "original_form": "가슴이 간질간질 흐뭇하네요",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030383.1.3",
          "form": "잘 읽히겠어요",
          "original_form": "잘 읽히겠어요",
          "speaker_id": "1",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030383.1.4",
          "form": "그런데 거의 다 비슷한 종류예요",
          "original_form": "그런데 거의 다 비슷한 종류예요",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030383.1.5",
          "form": "유치하지 않나요",
          "original_form": "유치하지 않나요",
          "speaker_id": "1",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030383.1.6",
          "form": "유행하는 건 정해져 있더라고요",
          "original_form": "유행하는 건 정해져 있더라고요",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030383.1.7",
          "form": "무협지 처럼요 비슷하죠",
          "original_form": "무협지 처럼요 비슷하죠",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030383.1.8",
          "form": "유치한 맛에 보는 거죠",
          "original_form": "유치한 맛에 보는 거죠",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030383.1.9",
          "form": "무협지 많이 보셨죠",
          "original_form": "무협지 많이 보셨죠",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030383.1.10",
          "form": "간질간질 하겠네요",
          "original_form": "간질간질 하겠네요",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030383.1.11",
          "form": "엄청 봤습니다 요즘도 한 번씩 봐요",
          "original_form": "엄청 봤습니다 요즘도 한 번씩 봐요",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030383.1.12",
          "form": "주인공은 잘 생겼겠지요",
          "original_form": "주인공은 잘 생겼겠지요",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030383.1.13",
          "form": "예전엔 전통 무협지가 유행이었는데",
          "original_form": "예전엔 전통 무협지가 유행이었는데",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030383.1.14",
          "form": "요즘은 분위기가 많이 바뀌었어요",
          "original_form": "요즘은 분위기가 많이 바뀌었어요",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030383.1.15",
          "form": "판타지 소설이 생각나네요",
          "original_form": "판타지 소설이 생각나네요",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030383.1.16",
          "form": "회귀도 하고",
          "original_form": "회귀도 하고",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030383.1.17",
          "form": "그때가 그립네요",
          "original_form": "그때가 그립네요",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030383.1.18",
          "form": "게임에서 무역도 하고",
          "original_form": "게임에서 무역도 하고",
          "speaker_id": "2",
          "time": "20210904 00:24"
        }
