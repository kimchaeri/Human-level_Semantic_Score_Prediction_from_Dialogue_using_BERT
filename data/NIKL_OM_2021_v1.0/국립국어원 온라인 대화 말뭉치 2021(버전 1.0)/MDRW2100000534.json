        },
        {
          "id": "MDRW2100000534.1.2",
          "form": "나야 나 name1",
          "original_form": "나야 나 &name1&",
          "speaker_id": "1",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000534.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000534.1.4",
          "form": "놀러가고싶다",
          "original_form": "놀러가고싶다",
          "speaker_id": "1",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000534.1.5",
          "form": "요즘 코로나 심해져서 해수욕장 전부 폐쇄되었어요",
          "original_form": "요즘 코로나 심해져서 해수욕장 전부 폐쇄되었어요",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000534.1.6",
          "form": "아니 나는 해수욕장말고",
          "original_form": "아니 나는 해수욕장말고",
          "speaker_id": "1",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000534.1.7",
          "form": "논산에 가고싶어",
          "original_form": "논산에 가고싶어",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000534.1.8",
          "form": "해운대 갔다왔어요",
          "original_form": "해운대 갔다왔어요",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000534.1.9",
          "form": "언제? 요즘 해운대 막아놓지 않았나?",
          "original_form": "언제? 요즘 해운대 막아놓지 않았나?",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000534.1.10",
          "form": "예전보다 사람이 많이 없었다능~",
          "original_form": "예전보다 사람이 많이 없었다능~",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000534.1.11",
          "form": "코로나 때문에 어쩔 수 없지뭐",
          "original_form": "코로나 때문에 어쩔 수 없지뭐",
          "speaker_id": "1",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000534.1.12",
          "form": "부산 떡볶이도 먹고 왔어요 ㅋㅋ",
          "original_form": "부산 떡볶이도 먹고 왔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000534.1.13",
          "form": "부산 쌀 떡볶이 먹고싶다",
          "original_form": "부산 쌀 떡볶이 먹고싶다",
          "speaker_id": "1",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000534.1.14",
          "form": "그거 엄청 맛있는데",
          "original_form": "그거 엄청 맛있는데",
          "speaker_id": "1",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000534.1.15",
          "form": "예전엔 100만명씩 왔었는데",
          "original_form": "예전엔 100만명씩 왔었는데",
          "speaker_id": "2",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000534.1.16",
          "form": "그렇게 사람 많을 때 못가봤어",
          "original_form": "그렇게 사람 많을 때 못가봤어",
          "speaker_id": "1",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000534.1.17",
          "form": "광안리에도 사람이 별로 없어요",
          "original_form": "광안리에도 사람이 별로 없어요",
          "speaker_id": "2",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000534.1.18",
          "form": "광안리도 재미있지",
          "original_form": "광안리도 재미있지",
          "speaker_id": "1",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000534.1.19",
          "form": "다음에 같이 갑시다",
          "original_form": "다음에 같이 갑시다",
          "speaker_id": "2",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000534.1.20",
          "form": "언제 가지? 논산에 가고 싶어",
          "original_form": "언제 가지? 논산에 가고 싶어",
          "speaker_id": "1",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000534.1.21",
          "form": "요새 KTX 타면 1시간 40분 걸려요",
          "original_form": "요새 KTX 타면 1시간 40분 걸려요",
          "speaker_id": "2",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000534.1.22",
          "form": "논산에 가면 영화 드라마 촬영 셋트장 있어서 엄청 좋대",
          "original_form": "논산에 가면 영화 드라마 촬영 셋트장 있어서 엄청 좋대",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000534.1.23",
          "form": "사진도 찍고 놀면 좋은데",
          "original_form": "사진도 찍고 놀면 좋은데",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000534.1.24",
          "form": "도착하면 렌트카 빌려서 다니면 됩니다",
          "original_form": "도착하면 렌트카 빌려서 다니면 됩니다",
          "speaker_id": "2",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000534.1.25",
          "form": "논산에는 훈련소 체험 가나요?",
          "original_form": "논산에는 훈련소 체험 가나요?",
          "speaker_id": "2",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000534.1.26",
          "form": "부산도 렌트카가 되나? 서울만 있는 줄  알았네",
          "original_form": "부산도 렌트카가 되나? 서울만 있는 줄  알았네",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000534.1.27",
          "form": "아 논산훈련소 있구나 ㅠㅠ 거긴 안되겠다",
          "original_form": "아 논산훈련소 있구나 ㅠㅠ 거긴 안되겠다",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000534.1.28",
          "form": "돈주고 그거 받으러 가는 사람들도 있던데",
          "original_form": "돈주고 그거 받으러 가는 사람들도 있던데",
          "speaker_id": "2",
          "time": "20210819 16:49"
        }
      ]
