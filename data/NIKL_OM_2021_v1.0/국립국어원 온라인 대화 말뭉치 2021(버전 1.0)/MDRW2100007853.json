      "utterance": [
        {
          "id": "MDRW2100007853.1.1",
          "form": "오늘 갓지은 솥밥 맛있었어?",
          "original_form": "오늘 갓지은 솥밥 맛있었어?",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100007853.1.2",
          "form": "아 ㅋㅋㅋ 괜찮드라",
          "original_form": "아 ㅋㅋㅋ 괜찮드라",
          "speaker_id": "1",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100007853.1.3",
          "form": "나는 좀더 신김치를 좋아하긴해",
          "original_form": "나는 좀더 신김치를 좋아하긴해",
          "speaker_id": "1",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100007853.1.4",
          "form": "ㅎㅎ 집에와서 오빠가 밥달라고해서 \r\n마켓컬리 장본걸루 반찬만들었답",
          "original_form": "ㅎㅎ 집에와서 오빠가 밥달라고해서 \r\n마켓컬리 장본걸루 반찬만들었답",
          "speaker_id": "2",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100007853.1.5",
          "form": "ㅋㅋ 나는 오늘 쿠키 먹으면서 출근",
          "original_form": "ㅋㅋ 나는 오늘 쿠키 먹으면서 출근",
          "speaker_id": "1",
          "time": "20210823 07:54"
        },
        {
          "id": "MDRW2100007853.1.6",
          "form": "아몬드 박힌거랑 초코쿠키 괜찮드라",
          "original_form": "아몬드 박힌거랑 초코쿠키 괜찮드라",
          "speaker_id": "1",
          "time": "20210823 07:54"
        },
        {
          "id": "MDRW2100007853.1.7",
          "form": "ㅎㅎ응 벌써다먹었다",
          "original_form": "ㅎㅎ응 벌써다먹었다",
          "speaker_id": "2",
          "time": "20210823 08:08"
        },
        {
          "id": "MDRW2100007853.1.8",
          "form": "빠르네 나는 아직 남아잇지",
          "original_form": "빠르네 나는 아직 남아잇지",
          "speaker_id": "1",
          "time": "20210823 08:41"
        },
        {
          "id": "MDRW2100007853.1.9",
          "form": "abc쿠킹스큐디오 23일 10:45분은 어땨?",
          "original_form": "abc쿠킹스큐디오 23일 10:45분은 어땨?",
          "speaker_id": "1",
          "time": "20210823 08:56"
        },
        {
          "id": "MDRW2100007853.1.10",
          "form": "명절 다음날인데 힘들려나?",
          "original_form": "명절 다음날인데 힘들려나?",
          "speaker_id": "1",
          "time": "20210823 08:56"
        },
        {
          "id": "MDRW2100007853.1.11",
          "form": "9월4일은 뭐하니?",
          "original_form": "9월4일은 뭐하니?",
          "speaker_id": "1",
          "time": "20210823 08:59"
        },
        {
          "id": "MDRW2100007853.1.12",
          "form": "9월4일 좋다ㅎ 고고고",
          "original_form": "9월4일 좋다ㅎ 고고고",
          "speaker_id": "2",
          "time": "20210823 09:29"
        },
        {
          "id": "MDRW2100007853.1.13",
          "form": "닭꼬치 맛있겠다",
          "original_form": "닭꼬치 맛있겠다",
          "speaker_id": "2",
          "time": "20210823 09:29"
        },
        {
          "id": "MDRW2100007853.1.14",
          "form": "응 야끼도리 배우면 집에서 실습 가능할꺼 같애",
          "original_form": "응 야끼도리 배우면 집에서 실습 가능할꺼 같애",
          "speaker_id": "1",
          "time": "20210823 09:30"
        },
        {
          "id": "MDRW2100007853.1.15",
          "form": "ㅎㅎ 맛있으면 또먹고싶을테니까 재료를 살 의욕이 생기겠지",
          "original_form": "ㅎㅎ 맛있으면 또먹고싶을테니까 재료를 살 의욕이 생기겠지",
          "speaker_id": "2",
          "time": "20210823 09:31"
        },
        {
          "id": "MDRW2100007853.1.16",
          "form": "성공여부를 떠나서 그때 미리 집에 닭을 사둬야 하나 ㅋㅋㅋ",
          "original_form": "성공여부를 떠나서 그때 미리 집에 닭을 사둬야 하나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 09:32"
        },
        {
          "id": "MDRW2100007853.1.17",
          "form": "그것도 방법이네ㅋㅋ 근데 레시피 수업들어야 아는데 재료를 다 알려나",
          "original_form": "그것도 방법이네ㅋㅋ 근데 레시피 수업들어야 아는데 재료를 다 알려나",
          "speaker_id": "2",
          "time": "20210823 09:33"
        },
        {
          "id": "MDRW2100007853.1.18",
          "form": "닭이랑 대파 토마토 무슴 버섯이랑",
          "original_form": "닭이랑 대파 토마토 무슴 버섯이랑",
          "speaker_id": "1",
          "time": "20210823 09:33"
        },
        {
          "id": "MDRW2100007853.1.19",
          "form": "음..... 재료 하나 꽈리고.츄",
          "original_form": "음..... 재료 하나 꽈리고.츄",
          "speaker_id": "1",
          "time": "20210823 09:34"
        },
        {
          "id": "MDRW2100007853.1.20",
          "form": "이렇게는 가네",
          "original_form": "이렇게는 가네",
