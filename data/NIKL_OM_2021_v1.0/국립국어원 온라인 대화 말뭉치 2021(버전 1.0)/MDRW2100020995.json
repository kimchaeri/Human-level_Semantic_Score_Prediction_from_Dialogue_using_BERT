        },
        {
          "id": "MDRW2100020995.1.4",
          "form": "ㅋㅋ뭐지",
          "original_form": "ㅋㅋ뭐지",
          "speaker_id": "2",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100020995.1.5",
          "form": "ㅋㅋㅋ 근데 자꾸 살아날까봐 무서움",
          "original_form": "ㅋㅋㅋ 근데 자꾸 살아날까봐 무서움",
          "speaker_id": "1",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100020995.1.6",
          "form": "ㅋㅋㅋ아 저번에도 피나고 난리엿다고",
          "original_form": "ㅋㅋㅋ아 저번에도 피나고 난리엿다고",
          "speaker_id": "2",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100020995.1.7",
          "form": "ㅋㅋㅋㅋ 천서진도 분명 지난주에 죽는거였는데 말짱",
          "original_form": "ㅋㅋㅋㅋ 천서진도 분명 지난주에 죽는거였는데 말짱",
          "speaker_id": "1",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100020995.1.8",
          "form": "ㅋㅋㅋ왜들그러지",
