      "utterance": [
        {
          "id": "MDRW2100032252.1.1",
          "form": "스포츠 좋아하시나요",
          "original_form": "스포츠 좋아하시나요",
          "speaker_id": "2",
          "time": "20210906 12:04"
        },
        {
          "id": "MDRW2100032252.1.2",
          "form": "네 스포츠 좋아하죠?\n좋아하시나요?",
          "original_form": "네 스포츠 좋아하죠?\n좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100032252.1.3",
          "form": "보는거 좋아해용",
          "original_form": "보는거 좋아해용",
          "speaker_id": "2",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100032252.1.4",
          "form": "하는것도 좋아하세요?",
          "original_form": "하는것도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100032252.1.5",
          "form": "네 저는 하는게 더 좋더라구요",
          "original_form": "네 저는 하는게 더 좋더라구요",
          "speaker_id": "1",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100032252.1.6",
          "form": "오 어떤거 좋아하시나여",
          "original_form": "오 어떤거 좋아하시나여",
          "speaker_id": "2",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100032252.1.7",
          "form": "저는 수영도 좋아하구요\n탁구도 배웠었어요!",
          "original_form": "저는 수영도 좋아하구요\n탁구도 배웠었어요!",
          "speaker_id": "1",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100032252.1.8",
          "form": "우와.. 탁구 배워보고싶어요!",
          "original_form": "우와.. 탁구 배워보고싶어요!",
          "speaker_id": "2",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100032252.1.9",
          "form": "탁구 배워보니까 재밌더라구요",
          "original_form": "탁구 배워보니까 재밌더라구요",
          "speaker_id": "1",
          "time": "20210906 14:19"
        },
        {
          "id": "MDRW2100032252.1.10",
          "form": "점수내기 경기해도 재밌구요",
          "original_form": "점수내기 경기해도 재밌구요",
          "speaker_id": "1",
          "time": "20210906 14:20"
        },
        {
          "id": "MDRW2100032252.1.11",
          "form": "요즘 골프 탁구 테니스 많이들 하도라구욥",
          "original_form": "요즘 골프 탁구 테니스 많이들 하도라구욥",
          "speaker_id": "2",
          "time": "20210906 14:20"
        },
        {
          "id": "MDRW2100032252.1.12",
          "form": "마자요! 요새 스포츠 배우는분들이  많은거같아요",
          "original_form": "마자요! 요새 스포츠 배우는분들이  많은거같아요",
          "speaker_id": "1",
          "time": "20210906 14:22"
        },
        {
          "id": "MDRW2100032252.1.13",
          "form": "암벽등산도 해보고싶은데 아직 근력이 딸려서 못할거같네요ㅠ",
          "original_form": "암벽등산도 해보고싶은데 아직 근력이 딸려서 못할거같네요ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:23"
        },
        {
          "id": "MDRW2100032252.1.14",
          "form": "헉 .. 그건 아예 생각도 안해봣어용 ㅠㅠ",
          "original_form": "헉 .. 그건 아예 생각도 안해봣어용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100032252.1.15",
          "form": "암벽등반 하시는분들도 꽤있더라구요\n실내 암벽등반장도 많이 생기구요",
          "original_form": "암벽등반 하시는분들도 꽤있더라구요\n실내 암벽등반장도 많이 생기구요",
          "speaker_id": "1",
          "time": "20210906 15:11"
        },
