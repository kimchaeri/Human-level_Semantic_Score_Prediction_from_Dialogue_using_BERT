      "utterance": [
        {
          "id": "MDRW2100030422.1.1",
          "form": "추리소설 재미있어요",
          "original_form": "추리소설 재미있어요",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030422.1.2",
          "form": "13.67 재미있게 보고 있었는데",
          "original_form": "13.67 재미있게 보고 있었는데",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030422.1.3",
          "form": "맞아요 잘 맞추나요",
          "original_form": "맞아요 잘 맞추나요",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030422.1.4",
          "form": "저는 맨날 틀려요",
          "original_form": "저는 맨날 틀려요",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030422.1.5",
          "form": "중국 쪽 소설이어서 이름이 너무 생소해 해요",
          "original_form": "중국 쪽 소설이어서 이름이 너무 생소해 해요",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030422.1.6",
          "form": "중국 추리소설인가요",
          "original_form": "중국 추리소설인가요",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030422.1.7",
          "form": "인물 인식하기가 어려웠어요",
          "original_form": "인물 인식하기가 어려웠어요",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030422.1.8",
          "form": "특이하네요",
          "original_form": "특이하네요",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030422.1.9",
          "form": "찬호께이라는 유명한 작가인가 봐요",
          "original_form": "찬호께이라는 유명한 작가인가 봐요",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030422.1.10",
          "form": "이름이 많이 어려운가요",
          "original_form": "이름이 많이 어려운가요",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030422.1.11",
          "form": "찬호는 한국 사람 아닌가요",
          "original_form": "찬호는 한국 사람 아닌가요",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030422.1.12",
          "form": "리샤오펑이런 일은 많이 나와요",
          "original_form": "리샤오펑이런 일은 많이 나와요",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030422.1.13",
          "form": "이런이름",
          "original_form": "이런이름",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030422.1.14",
          "form": "찬호k",
          "original_form": "찬호k",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030422.1.15",
          "form": "낯설긴 하네요",
          "original_form": "낯설긴 하네요",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030422.1.16",
          "form": "들어본 적 있나요",
          "original_form": "들어본 적 있나요",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030422.1.17",
          "form": "내용은 재미있더라구요",
          "original_form": "내용은 재미있더라구요",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030422.1.18",
          "form": "중국 소설은 안 읽어봤어요",
          "original_form": "중국 소설은 안 읽어봤어요",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030422.1.19",
          "form": "삼국지 좋아해요",
          "original_form": "삼국지 좋아해요",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030422.1.20",
          "form": "인물 파악만 되면 내용은 괜찮아요",
          "original_form": "인물 파악만 되면 내용은 괜찮아요",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030422.1.21",
          "form": "삼국지에는 정말 많은 사람들이 나오잖아요",
          "original_form": "삼국지에는 정말 많은 사람들이 나오잖아요",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030422.1.22",
          "form": "저는 아직 안 읽어봤어요",
          "original_form": "저는 아직 안 읽어봤어요",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030422.1.23",
          "form": "내 스케일이 크죠",
          "original_form": "내 스케일이 크죠",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030422.1.24",
          "form": "누구를 좋아하나요",
          "original_form": "누구를 좋아하나요",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030422.1.25",
          "form": "아마 좋아할 거예요",
          "original_form": "아마 좋아할 거예요",
          "speaker_id": "2",
          "time": "20210904 00:52"
        }
