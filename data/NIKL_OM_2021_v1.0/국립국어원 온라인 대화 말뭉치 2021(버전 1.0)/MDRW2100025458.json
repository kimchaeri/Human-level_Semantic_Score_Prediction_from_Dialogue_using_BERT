      "utterance": [
        {
          "id": "MDRW2100025458.1.1",
          "form": "요즘 하시는 일은 잘되시나요",
          "original_form": "요즘 하시는 일은 잘되시나요",
          "speaker_id": "2",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025458.1.2",
          "form": "전 부업으로 주식하고 있습니다",
          "original_form": "전 부업으로 주식하고 있습니다",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025458.1.3",
          "form": "전 힘이 드네요",
          "original_form": "전 힘이 드네요",
          "speaker_id": "2",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025458.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025458.1.5",
          "form": "코로나로 일이 힘들어지니",
          "original_form": "코로나로 일이 힘들어지니",
          "speaker_id": "2",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025458.1.6",
          "form": "월급은 모르겠고",
          "original_form": "월급은 모르겠고",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025458.1.7",
          "form": "예금적금도 너무 막막하고",
          "original_form": "예금적금도 너무 막막하고",
          "speaker_id": "2",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025458.1.8",
          "form": "제 주식올랐으면 좋겠어요",
          "original_form": "제 주식올랐으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025458.1.9",
          "form": "주식을 해야 그나마 돈을 더 벌것같더라구요",
          "original_form": "주식을 해야 그나마 돈을 더 벌것같더라구요",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025458.1.10",
          "form": "하이리스크 하이리턴",
          "original_form": "하이리스크 하이리턴",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025458.1.11",
          "form": "고정월급을 가지고 있고",
          "original_form": "고정월급을 가지고 있고",
          "speaker_id": "1",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100025458.1.12",
          "form": "주식을 하는게 최고의 시나리오죠",
          "original_form": "주식을 하는게 최고의 시나리오죠",
          "speaker_id": "1",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100025458.1.13",
          "form": "그죠 맞아요",
          "original_form": "그죠 맞아요",
          "speaker_id": "2",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100025458.1.14",
          "form": "제 직업이 영업요소가 가미되어 있어서",
          "original_form": "제 직업이 영업요소가 가미되어 있어서",
          "speaker_id": "2",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100025458.1.15",
          "form": "그게 저랑 안맞아서 스트레스에요",
          "original_form": "그게 저랑 안맞아서 스트레스에요",
          "speaker_id": "2",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025458.1.16",
          "form": "name1님은 일하시는 환경이 어떠세요",
          "original_form": "&name1&님은 일하시는 환경이 어떠세요",
          "speaker_id": "2",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025458.1.17",
          "form": "저도 영업이요ㅜㅜ",
          "original_form": "저도 영업이요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025458.1.18",
          "form": "영업이 제일 힘들어요...",
          "original_form": "영업이 제일 힘들어요...",
          "speaker_id": "1",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025458.1.19",
          "form": "전 그나마 사람보고 일해요",
          "original_form": "전 그나마 사람보고 일해요",
          "speaker_id": "2",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025458.1.20",
          "form": "주위 직원들이 저랑 잘 맞아서",
          "original_form": "주위 직원들이 저랑 잘 맞아서",
          "speaker_id": "2",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025458.1.21",
          "form": "그게힘이돼요",
          "original_form": "그게힘이돼요",
          "speaker_id": "2",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025458.1.22",
          "form": "근데 스트레스도 사람때문이고",
          "original_form": "근데 스트레스도 사람때문이고",
          "speaker_id": "1",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025458.1.23",
          "form": "치료도 사람이네요",
          "original_form": "치료도 사람이네요",
          "speaker_id": "1",
          "time": "20210901 11:46"
        }
      ]
