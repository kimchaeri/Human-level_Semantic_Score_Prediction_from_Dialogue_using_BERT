      "utterance": [
        {
          "id": "MDRW2100032927.1.1",
          "form": "안녕하세요 독서 좋아하세요?",
          "original_form": "안녕하세요 독서 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 07:56"
        },
        {
          "id": "MDRW2100032927.1.2",
          "form": "네 자주 책 읽어요!",
          "original_form": "네 자주 책 읽어요!",
          "speaker_id": "2",
          "time": "20210906 08:51"
        },
        {
          "id": "MDRW2100032927.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 08:51"
        },
        {
          "id": "MDRW2100032927.1.4",
          "form": "자주는 못 읽지만 좋아해요!",
          "original_form": "자주는 못 읽지만 좋아해요!",
          "speaker_id": "1",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100032927.1.5",
          "form": "어떤 분야 좋아하세요?",
          "original_form": "어떤 분야 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100032927.1.6",
          "form": "저 인문서적을 많이 읽어요! 요즘은 프로타고라스 보고 잇어요!",
          "original_form": "저 인문서적을 많이 읽어요! 요즘은 프로타고라스 보고 잇어요!",
          "speaker_id": "2",
          "time": "20210907 07:49"
        },
        {
          "id": "MDRW2100032927.1.7",
          "form": "와 처음 들어봐요 어떤 내용이에요?",
          "original_form": "와 처음 들어봐요 어떤 내용이에요?",
          "speaker_id": "1",
          "time": "20210907 07:49"
        },
        {
          "id": "MDRW2100032927.1.8",
          "form": "소크라테스랑 프로타고라스가 말싸움하는 ㅋㅋㅋㅋ 내용이에요",
          "original_form": "소크라테스랑 프로타고라스가 말싸움하는 ㅋㅋㅋㅋ 내용이에요",
          "speaker_id": "2",
          "time": "20210907 07:51"
        },
        {
          "id": "MDRW2100032927.1.9",
          "form": "와ㅋㅋㅋㅋㅋㅋㅋㅋㅋ철학적일 것 같아요",
          "original_form": "와ㅋㅋㅋㅋㅋㅋㅋㅋㅋ철학적일 것 같아요",
          "speaker_id": "1",
          "time": "20210907 07:51"
        },
        {
          "id": "MDRW2100032927.1.10",
          "form": "추천하고 싶으신가요ㅋㅋㅋ",
          "original_form": "추천하고 싶으신가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 07:52"
        },
        {
          "id": "MDRW2100032927.1.11",
          "form": "ㅎㅎㅎㅎ 굳이 봐야하나 싶어요",
          "original_form": "ㅎㅎㅎㅎ 굳이 봐야하나 싶어요",
          "speaker_id": "2",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100032927.1.12",
          "form": "탁월함을 가르칠 수잇냐 없냐로 싸우는데 결론이 안 나거든요",
          "original_form": "탁월함을 가르칠 수잇냐 없냐로 싸우는데 결론이 안 나거든요",
          "speaker_id": "2",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100032927.1.13",
          "form": "와 결론이 안 난다니..생각하게 만드는 책인가봐요",
          "original_form": "와 결론이 안 난다니..생각하게 만드는 책인가봐요",
          "speaker_id": "1",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100032927.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 소크라테스 본인도 혼란스러워하면서 끝나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 소크라테스 본인도 혼란스러워하면서 끝나요",
          "speaker_id": "2",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100032927.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ혼란스럽네요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ혼란스럽네요...",
          "speaker_id": "1",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100032927.1.16",
          "form": "읽고 싶은 책은 바로바로 읽으시는 편이신가요?",
          "original_form": "읽고 싶은 책은 바로바로 읽으시는 편이신가요?",
          "speaker_id": "1",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100032927.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 저도 혼란스럽더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 저도 혼란스럽더라구요",
          "speaker_id": "2",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100032927.1.18",
          "form": "저 자기전에 좀 읽다가 졸려지면 자요",
          "original_form": "저 자기전에 좀 읽다가 졸려지면 자요",
          "speaker_id": "2",
          "time": "20210907 07:58"
        },
        {
          "id": "MDRW2100032927.1.19",
          "form": "잠자기 위해 책을 이용하는..",
          "original_form": "잠자기 위해 책을 이용하는..",
          "speaker_id": "2",
          "time": "20210907 07:58"
        },
        {
          "id": "MDRW2100032927.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ엄청 현명하시네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ엄청 현명하시네요",
          "speaker_id": "1",
