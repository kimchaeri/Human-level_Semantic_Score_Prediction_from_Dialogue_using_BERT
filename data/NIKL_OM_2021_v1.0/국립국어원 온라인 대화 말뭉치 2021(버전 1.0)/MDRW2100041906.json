      "utterance": [
        {
          "id": "MDRW2100041906.1.1",
          "form": "저녁 먹었는쥐.",
          "original_form": "저녁 먹었는쥐.",
          "speaker_id": "1",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041906.1.2",
          "form": "저녁 아직!!",
          "original_form": "저녁 아직!!",
          "speaker_id": "2",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041906.1.3",
          "form": "당신은 저녁을 먹었는쥐",
          "original_form": "당신은 저녁을 먹었는쥐",
          "speaker_id": "2",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041906.1.4",
          "form": "나 지금 먹는 중",
          "original_form": "나 지금 먹는 중",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041906.1.5",
          "form": "만두~",
          "original_form": "만두~",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041906.1.6",
          "form": "무슨 만두!",
          "original_form": "무슨 만두!",
          "speaker_id": "2",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041906.1.7",
          "form": "김치만두",
          "original_form": "김치만두",
          "speaker_id": "1",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041906.1.8",
          "form": "찐 거",
          "original_form": "찐 거",
          "speaker_id": "1",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041906.1.9",
          "form": "헉 맛있겠다 점심은 뭐먹었어",
          "original_form": "헉 맛있겠다 점심은 뭐먹었어",
          "speaker_id": "2",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041906.1.10",
          "form": "점심은 죽.",
          "original_form": "점심은 죽.",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041906.1.11",
          "form": "조퇴했지롱",
          "original_form": "조퇴했지롱",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041906.1.12",
          "form": "무야 아파서 죽먹었어?",
          "original_form": "무야 아파서 죽먹었어?",
          "speaker_id": "2",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100041906.1.13",
          "form": "넵ㅋ 야채죽 맛있다요",
          "original_form": "넵ㅋ 야채죽 맛있다요",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041906.1.14",
          "form": "ㅜㅜ 나는 점심 타임 놓쳐서 어제 먹고 남은 크림수프 먹었워",
          "original_form": "ㅜㅜ 나는 점심 타임 놓쳐서 어제 먹고 남은 크림수프 먹었워",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041906.1.15",
          "form": "어 ㅠㅠ 왜 놓쳤는데?",
          "original_form": "어 ㅠㅠ 왜 놓쳤는데?",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041906.1.16",
          "form": "크림수프는 맛있겠당.",
          "original_form": "크림수프는 맛있겠당.",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041906.1.17",
          "form": "이래저래 일땜에 흑흑",
          "original_form": "이래저래 일땜에 흑흑",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041906.1.18",
          "form": "너는 배달음식 뭐좋아행",
          "original_form": "너는 배달음식 뭐좋아행",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041906.1.19",
          "form": "음 난 그냥 분식",
          "original_form": "음 난 그냥 분식",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041906.1.20",
          "form": "무난쓰",
          "original_form": "무난쓰",
          "speaker_id": "1",
          "time": "20210906 17:40"
        }
      ]
