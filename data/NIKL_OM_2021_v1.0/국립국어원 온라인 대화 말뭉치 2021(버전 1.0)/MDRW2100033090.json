      "utterance": [
        {
          "id": "MDRW2100033090.1.1",
          "form": "자리보전만 하고싶은게 너무 눈에보여서",
          "original_form": "자리보전만 하고싶은게 너무 눈에보여서",
          "speaker_id": "2",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100033090.1.2",
          "form": "10월부터 적용한다는데",
          "original_form": "10월부터 적용한다는데",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100033090.1.3",
          "form": "한다는건 아니고",
          "original_form": "한다는건 아니고",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100033090.1.4",
          "form": "검토중이라는데",
          "original_form": "검토중이라는데",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100033090.1.5",
          "form": "이제 진짜 걸릴각오하고 살아야되나",
          "original_form": "이제 진짜 걸릴각오하고 살아야되나",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100033090.1.6",
          "form": "싶긴하네요",
          "original_form": "싶긴하네요",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100033090.1.7",
          "form": "아 세상에ㅠ",
          "original_form": "아 세상에ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100033090.1.8",
          "form": "그리고 아프간 난민도 들어온다고 하잖아요....?",
          "original_form": "그리고 아프간 난민도 들어온다고 하잖아요....?",
          "speaker_id": "1",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100033090.1.9",
          "form": "그 난민들은 코로나 안걸려서 오는거긴 한건지ㅠ",
          "original_form": "그 난민들은 코로나 안걸려서 오는거긴 한건지ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033090.1.10",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033090.1.11",
          "form": "진짜 무서워요",
          "original_form": "진짜 무서워요",
          "speaker_id": "2",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033090.1.12",
          "form": "오늘또 세계에서 가장빠른 접종속도라고",
          "original_form": "오늘또 세계에서 가장빠른 접종속도라고",
          "speaker_id": "2",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033090.1.13",
          "form": "마치 자신의 업적인척",
          "original_form": "마치 자신의 업적인척",
          "speaker_id": "2",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033090.1.14",
          "form": "언플했네요",
          "original_form": "언플했네요",
          "speaker_id": "2",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033090.1.15",
          "form": "이렇게 협조적인 국민들이라 본인잘못이 묻히는건데",
          "original_form": "이렇게 협조적인 국민들이라 본인잘못이 묻히는건데",
          "speaker_id": "2",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100033090.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아니진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아니진짜",
          "speaker_id": "1",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100033090.1.17",
          "form": "어쩜 그러는지 몰라요",
          "original_form": "어쩜 그러는지 몰라요",
          "speaker_id": "1",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100033090.1.18",
          "form": "k-방역 어쩌구 저쩌구 하겠죠... 보나마나...?",
          "original_form": "k-방역 어쩌구 저쩌구 하겠죠... 보나마나...?",
          "speaker_id": "1",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100033090.1.19",
          "form": "다음 정권이 잘 들어서야할텐데 인물이 없네요",
          "original_form": "다음 정권이 잘 들어서야할텐데 인물이 없네요",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033090.1.20",
          "form": "코로나 진짜...",
          "original_form": "코로나 진짜...",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033090.1.21",
          "form": "제주변은 1차맞았는데 갑자기 6주로 미뤄졌는데",
          "original_form": "제주변은 1차맞았는데 갑자기 6주로 미뤄졌는데",
          "speaker_id": "2",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033090.1.22",
          "form": "그때는 입꾹닫고 있다가",
          "original_form": "그때는 입꾹닫고 있다가",
          "speaker_id": "2",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033090.1.23",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033090.1.24",
          "form": "저러는거 너무 밉상이에요",
          "original_form": "저러는거 너무 밉상이에요",
          "speaker_id": "2",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033090.1.25",
          "form": "아 저도요!!!",
          "original_form": "아 저도요!!!",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033090.1.26",
          "form": "그러니까 말이에요ㅠ",
          "original_form": "그러니까 말이에요ㅠ",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033090.1.27",
          "form": "그리고 백신 부작용 생기면",
          "original_form": "그리고 백신 부작용 생기면",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033090.1.28",
          "form": "책임진다고 했으면서",
          "original_form": "책임진다고 했으면서",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033090.1.29",
          "form": "정작 아무것도 배상 안하지 않나요?",
          "original_form": "정작 아무것도 배상 안하지 않나요?",
          "speaker_id": "1",
          "time": "20210906 16:04"
        },
        {
          "id": "MDRW2100033090.1.30",
          "form": "사실 저희 엄마도 1차 AZ 맞고 혈전 생겨서",
          "original_form": "사실 저희 엄마도 1차 AZ 맞고 혈전 생겨서",
          "speaker_id": "1",
          "time": "20210906 16:04"
        },
        {
          "id": "MDRW2100033090.1.31",
          "form": "병원 가고 신고도 하고 그랬는데",
          "original_form": "병원 가고 신고도 하고 그랬는데",
          "speaker_id": "1",
          "time": "20210906 16:04"
        },
        {
          "id": "MDRW2100033090.1.32",
          "form": "놀랍게도 아무것도 안해주더라구요",
          "original_form": "놀랍게도 아무것도 안해주더라구요",
          "speaker_id": "1",
          "time": "20210906 16:04"
        },
        {
          "id": "MDRW2100033090.1.33",
          "form": "헉ㅠ 진짜요?",
          "original_form": "헉ㅠ 진짜요?",
          "speaker_id": "2",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100033090.1.34",
          "form": "너무 놀라셨겠어요",
          "original_form": "너무 놀라셨겠어요",
          "speaker_id": "2",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100033090.1.35",
          "form": "마트직종은 백신의무화한다던데",
          "original_form": "마트직종은 백신의무화한다던데",
          "speaker_id": "2",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100033090.1.36",
          "form": "뭘믿고 의무로 맞으란건가요",
          "original_form": "뭘믿고 의무로 맞으란건가요",
          "speaker_id": "2",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100033090.1.37",
          "form": "그러니까 말이에요",
          "original_form": "그러니까 말이에요",
          "speaker_id": "1",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100033090.1.38",
          "form": "엄마가 암환자신데",
          "original_form": "엄마가 암환자신데",
          "speaker_id": "1",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100033090.1.39",
          "form": "주치의도 그냥 자기네들이 어떻게 해줄 방법이 없다고 ...",
          "original_form": "주치의도 그냥 자기네들이 어떻게 해줄 방법이 없다고 ...",
          "speaker_id": "1",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100033090.1.40",
          "form": "그런데 위드 코로나라니ㅜ",
          "original_form": "그런데 위드 코로나라니ㅜ",
          "speaker_id": "1",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100033090.1.41",
          "form": "아 암환자시군요",
          "original_form": "아 암환자시군요",
          "speaker_id": "2",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100033090.1.42",
          "form": "우리아버지도 심장쪽 심혈관질환이라",
          "original_form": "우리아버지도 심장쪽 심혈관질환이라",
          "speaker_id": "2",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100033090.1.43",
          "form": "백신 너무 걱정이에요",
          "original_form": "백신 너무 걱정이에요",
          "speaker_id": "2",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100033090.1.44",
          "form": "지금은 좀 괜찮아지셨나요?",
          "original_form": "지금은 좀 괜찮아지셨나요?",
          "speaker_id": "2",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100033090.1.45",
          "form": "악 진짜 걱정 많으시겠어요ㅠ",
          "original_form": "악 진짜 걱정 많으시겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033090.1.46",
          "form": "네네 1차로 인한건 이제 괜찮은데",
          "original_form": "네네 1차로 인한건 이제 괜찮은데",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033090.1.47",
          "form": "며칠 전에 2차를 화이자로 맞으셨거든요ㅠ",
          "original_form": "며칠 전에 2차를 화이자로 맞으셨거든요ㅠ",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033090.1.48",
          "form": "요즘 식은땀난다고 하더라구요",
          "original_form": "요즘 식은땀난다고 하더라구요",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033090.1.49",
          "form": "아이고 당분간 잘 지켜봐야하겠네요",
          "original_form": "아이고 당분간 잘 지켜봐야하겠네요",
          "speaker_id": "2",
          "time": "20210906 16:48"
        }
      ]
