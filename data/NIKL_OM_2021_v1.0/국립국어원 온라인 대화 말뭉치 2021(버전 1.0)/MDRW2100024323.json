      "utterance": [
        {
          "id": "MDRW2100024323.1.1",
          "form": "오 그정도는 해볼만 할 것 같아요!",
          "original_form": "오 그정도는 해볼만 할 것 같아요!",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024323.1.2",
          "form": "그럼 밥은 안들어가요?",
          "original_form": "그럼 밥은 안들어가요?",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024323.1.3",
          "form": "좋아하는 음식은 지금은 마라탕이랑 김밥이 먹고 싶어요",
          "original_form": "좋아하는 음식은 지금은 마라탕이랑 김밥이 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024323.1.4",
          "form": "찐 키토는 밥 안 넣는데",
          "original_form": "찐 키토는 밥 안 넣는데",
          "speaker_id": "1",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024323.1.5",
          "form": "저는 아주얇게 넣긴해요ㅎㅎㅎ",
          "original_form": "저는 아주얇게 넣긴해요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024323.1.6",
          "form": "우왕 어떻게 마는지가 제일 궁금하네요",
          "original_form": "우왕 어떻게 마는지가 제일 궁금하네요",
          "speaker_id": "2",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024323.1.7",
          "form": "터질까?",
          "original_form": "터질까?",
          "speaker_id": "1",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024323.1.8",
          "form": "일단 말아봐?",
          "original_form": "일단 말아봐?",
          "speaker_id": "1",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024323.1.9",
          "form": "이러면서 의심하면서ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이러면서 의심하면서ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024323.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ믿음은 성공을 가져다 주나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ믿음은 성공을 가져다 주나요?",
          "speaker_id": "2",
          "time": "20210831 00:50"
        },
        {
          "id": "MDRW2100024323.1.11",
          "form": "터지면 걍 터진대로ㅋㅋㅋㅋ",
          "original_form": "터지면 걍 터진대로ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100024323.1.12",
          "form": "어차피 집에서 하는 야매김밥이라",
          "original_form": "어차피 집에서 하는 야매김밥이라",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100024323.1.13",
          "form": "ㅎㅎ그래두 맛은 그대로니까요",
          "original_form": "ㅎㅎ그래두 맛은 그대로니까요",
          "speaker_id": "2",
          "time": "20210831 01:06"
        },
        {
          "id": "MDRW2100024323.1.14",
          "form": "맛있겠어요ㅠㅠ",
          "original_form": "맛있겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 01:06"
        },
        {
          "id": "MDRW2100024323.1.15",
          "form": "지금 후식으로",
          "original_form": "지금 후식으로",
          "speaker_id": "1",
          "time": "20210831 01:07"
        },
        {
          "id": "MDRW2100024323.1.16",
          "form": "아이스크림 먹으면서 에어컨 틀었는데 천국이예요ㅎㅎㅎㅎ",
          "original_form": "아이스크림 먹으면서 에어컨 틀었는데 천국이예요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 01:07"
        },
        {
          "id": "MDRW2100024323.1.17",
          "form": "너무 부럽습니다...ㅠㅠ",
          "original_form": "너무 부럽습니다...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024323.1.18",
          "form": "요즘 아이스크림무인가게 많아져서 자주 사다놔요ㅋㅋㅋㅋ",
          "original_form": "요즘 아이스크림무인가게 많아져서 자주 사다놔요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100024323.1.19",
          "form": "아이스크림 좋아하세요?",
          "original_form": "아이스크림 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100024323.1.20",
          "form": "좋아해요!투게더 바닐라가 요즘 그렇게 맛있더라구요...ㅠㅠㅠ",
          "original_form": "좋아해요!투게더 바닐라가 요즘 그렇게 맛있더라구요...ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100024323.1.21",
          "form": "헐.. 기본이 끌릴때가 잇죠",
          "original_form": "헐.. 기본이 끌릴때가 잇죠",
          "speaker_id": "1",
          "time": "20210901 06:15"
        },
        {
          "id": "MDRW2100024323.1.22",
          "form": "저는 어제 새로 사왔어요",
          "original_form": "저는 어제 새로 사왔어요",
          "speaker_id": "1",
          "time": "20210901 06:15"
        },
        {
          "id": "MDRW2100024323.1.23",
          "form": "티코 딸기맛 새로나왔길래!",
          "original_form": "티코 딸기맛 새로나왔길래!",
          "speaker_id": "1",
          "time": "20210901 06:15"
        },
        {
          "id": "MDRW2100024323.1.24",
          "form": "우와!!대박 어떤가요??",
          "original_form": "우와!!대박 어떤가요??",
          "speaker_id": "2",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100024323.1.25",
          "form": "저 그냥 티코도 좋아하거든요",
          "original_form": "저 그냥 티코도 좋아하거든요",
          "speaker_id": "2",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100024323.1.26",
          "form": "아직 안 먹어봤어요!",
          "original_form": "아직 안 먹어봤어요!",
          "speaker_id": "1",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100024323.1.27",
          "form": "지금 먹으려고요!",
          "original_form": "지금 먹으려고요!",
          "speaker_id": "1",
          "time": "20210901 15:50"
        }
      ]
    }
