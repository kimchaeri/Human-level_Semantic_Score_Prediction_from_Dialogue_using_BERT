      "utterance": [
        {
          "id": "MDRW2100040004.1.1",
          "form": "안녕하세요 치킨 좋아하세요?",
          "original_form": "안녕하세요 치킨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100040004.1.2",
          "form": "치킨 피자 중 뭐가 좋으신가요 ㅎㅎ",
          "original_form": "치킨 피자 중 뭐가 좋으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100040004.1.3",
          "form": "치킨 사랑하죠",
          "original_form": "치킨 사랑하죠",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100040004.1.4",
          "form": "피자도 사랑하는데 고르기어렵네요..",
          "original_form": "피자도 사랑하는데 고르기어렵네요..",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100040004.1.5",
          "form": "ㅎㅎ 어느 업체 치킨 좋아하세요?",
          "original_form": "ㅎㅎ 어느 업체 치킨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100040004.1.6",
          "form": "교촌치킨 레드콤보!! 드셔보셨나여??",
          "original_form": "교촌치킨 레드콤보!! 드셔보셨나여??",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100040004.1.7",
          "form": "넹 그거맛있던데요",
          "original_form": "넹 그거맛있던데요",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100040004.1.8",
          "form": "저한테 약간 맵긴 해요",
          "original_form": "저한테 약간 맵긴 해요",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040004.1.9",
          "form": "허니콤보를 더 조아하시겠네요!",
          "original_form": "허니콤보를 더 조아하시겠네요!",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040004.1.10",
          "form": "저는 자극적인맛을 너무 좋아해서..ㅠ",
          "original_form": "저는 자극적인맛을 너무 좋아해서..ㅠ",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040004.1.11",
          "form": "저는 오리지널이 더 맛있더라고요",
          "original_form": "저는 오리지널이 더 맛있더라고요",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100040004.1.12",
          "form": "자극적인 음식을 좋아하시는 이유가 있나요?",
          "original_form": "자극적인 음식을 좋아하시는 이유가 있나요?",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100040004.1.13",
          "form": "입맛이 그렇게 변한거같아용 ㅜㅜ뭐든 짜게먹어요",
          "original_form": "입맛이 그렇게 변한거같아용 ㅜㅜ뭐든 짜게먹어요",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100040004.1.14",
          "form": "아 듣고 보니 저도 짜게 먹으면 스트레스 풀리는 거 같더라고요",
          "original_form": "아 듣고 보니 저도 짜게 먹으면 스트레스 풀리는 거 같더라고요",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100040004.1.15",
          "form": "매운 건 잘 못 먹긴해요",
          "original_form": "매운 건 잘 못 먹긴해요",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100040004.1.16",
          "form": "저도 많이 매운건 못먹어용ㅜㅜ맵찔..",
          "original_form": "저도 많이 매운건 못먹어용ㅜㅜ맵찔..",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100040004.1.17",
          "form": "갑자기 궁금한게 있는데요 맵찔이라는 말이 TV에서 유행한 말이에요?",
          "original_form": "갑자기 궁금한게 있는데요 맵찔이라는 말이 TV에서 유행한 말이에요?",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100040004.1.18",
          "form": "아뇨 그냥 어느순간 인터넷에서 많이쓰는거 본거같아요",
          "original_form": "아뇨 그냥 어느순간 인터넷에서 많이쓰는거 본거같아요",
          "speaker_id": "2",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100040004.1.19",
          "form": "네 커뮤니티에서 퍼졌나봐요",
          "original_form": "네 커뮤니티에서 퍼졌나봐요",
