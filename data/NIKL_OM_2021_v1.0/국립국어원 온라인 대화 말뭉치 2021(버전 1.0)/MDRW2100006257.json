      "utterance": [
        {
          "id": "MDRW2100006257.1.1",
          "form": "사고싶은 물건 있나요",
          "original_form": "사고싶은 물건 있나요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100006257.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006257.1.3",
          "form": "태블릿",
          "original_form": "태블릿",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006257.1.4",
          "form": "아이패드요",
          "original_form": "아이패드요",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006257.1.5",
          "form": "아이패드 얼마정도 하나요?",
          "original_form": "아이패드 얼마정도 하나요?",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006257.1.6",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006257.1.7",
          "form": "비싼건 100넘고",
          "original_form": "비싼건 100넘고",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006257.1.8",
          "form": "좀 싼건 70?",
          "original_form": "좀 싼건 70?",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006257.1.9",
          "form": "비싸죠 ㅜㅜ",
          "original_form": "비싸죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006257.1.10",
          "form": "아..노트북 가격이네요",
          "original_form": "아..노트북 가격이네요",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006257.1.11",
          "form": "아이패드는 비싸요",
          "original_form": "아이패드는 비싸요",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006257.1.12",
          "form": "갤럭시탭이 싸구",
          "original_form": "갤럭시탭이 싸구",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006257.1.13",
          "form": "갤탭은 어느정도인가요",
          "original_form": "갤탭은 어느정도인가요",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006257.1.14",
          "form": "30~60?",
          "original_form": "30~60?",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006257.1.15",
          "form": "아이패드보단 싸죠?",
          "original_form": "아이패드보단 싸죠?",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006257.1.16",
          "form": "그렇네요 반정도 차이나네요..",
          "original_form": "그렇네요 반정도 차이나네요..",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100006257.1.17",
          "form": "그래서 갤탭이 애들한테 인기가 많죠",
          "original_form": "그래서 갤탭이 애들한테 인기가 많죠",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100006257.1.18",
          "form": "그렇군요 저는 안쓰다보니 그렇게 비싼줄 몰랐네요",
          "original_form": "그렇군요 저는 안쓰다보니 그렇게 비싼줄 몰랐네요",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006257.1.19",
          "form": "태블릿이 컴퓨터값이에요",
          "original_form": "태블릿이 컴퓨터값이에요",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006257.1.20",
          "form": "이러니 부모님 등골이 휘죠 ㅋㅋ",
          "original_form": "이러니 부모님 등골이 휘죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006257.1.21",
          "form": "그런대 요즘 아이들은 또 많이들 가지고있죠",
          "original_form": "그런대 요즘 아이들은 또 많이들 가지고있죠",
          "speaker_id": "1",
          "time": "20210907 22:46"
        }
