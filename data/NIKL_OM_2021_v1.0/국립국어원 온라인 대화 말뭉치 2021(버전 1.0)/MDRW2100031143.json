      "utterance": [
        {
          "id": "MDRW2100031143.1.1",
          "form": "키우고 싶은 동물이 있나요",
          "original_form": "키우고 싶은 동물이 있나요",
          "speaker_id": "2",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031143.1.2",
          "form": "너구리를 키우고 싶어요",
          "original_form": "너구리를 키우고 싶어요",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031143.1.3",
          "form": "왜 키우고 싶어요",
          "original_form": "왜 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031143.1.4",
          "form": "너구리는 좀 특이하죠",
          "original_form": "너구리는 좀 특이하죠",
          "speaker_id": "1",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031143.1.5",
          "form": "꼬리가 무척 귀여워요",
          "original_form": "꼬리가 무척 귀여워요",
          "speaker_id": "2",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031143.1.6",
          "form": "무늬도 정말 특이하고 사랑스러워요",
          "original_form": "무늬도 정말 특이하고 사랑스러워요",
          "speaker_id": "1",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031143.1.7",
          "form": "저는 원숭이를 키우고 싶어요",
          "original_form": "저는 원숭이를 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031143.1.8",
          "form": "원숭이는 손이 많이 갈 거 같은데요",
          "original_form": "원숭이는 손이 많이 갈 거 같은데요",
          "speaker_id": "1",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031143.1.9",
          "form": "장난꾸러기 이지요 원숭이는",
          "original_form": "장난꾸러기 이지요 원숭이는",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031143.1.10",
          "form": "원숭이는 좀 사람 같아요",
          "original_form": "원숭이는 좀 사람 같아요",
          "speaker_id": "1",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031143.1.11",
          "form": "생긴 것도 사람이랑 비슷하죠",
          "original_form": "생긴 것도 사람이랑 비슷하죠",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031143.1.12",
          "form": "그런데 키울 수 있나요",
          "original_form": "그런데 키울 수 있나요",
          "speaker_id": "1",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031143.1.13",
          "form": "한국에서는 안 될 거예요",
          "original_form": "한국에서는 안 될 거예요",
