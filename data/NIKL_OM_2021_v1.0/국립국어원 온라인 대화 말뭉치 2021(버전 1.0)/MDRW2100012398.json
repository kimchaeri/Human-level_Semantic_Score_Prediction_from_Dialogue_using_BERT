      "utterance": [
        {
          "id": "MDRW2100012398.1.1",
          "form": "저녁 뭐 드셨나요?",
          "original_form": "저녁 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012398.1.2",
          "form": "치킨먹었습니다 ㅋㅋㅋ",
          "original_form": "치킨먹었습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012398.1.3",
          "form": "퇴근후 집에서 치킨",
          "original_form": "퇴근후 집에서 치킨",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012398.1.4",
          "form": "오 치킨은 진리죠",
          "original_form": "오 치킨은 진리죠",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012398.1.5",
          "form": "무슨 치킨 드셨나요?",
          "original_form": "무슨 치킨 드셨나요?",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012398.1.6",
          "form": "굽네치킨요",
          "original_form": "굽네치킨요",
          "speaker_id": "2",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100012398.1.7",
          "form": "님은 뭐드셨어요?",
          "original_form": "님은 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100012398.1.8",
          "form": "저는 콩국수 먹었어요",
          "original_form": "저는 콩국수 먹었어요",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012398.1.9",
          "form": "콩국수요?",
          "original_form": "콩국수요?",
          "speaker_id": "2",
          "time": "20210825 00:31"
        },
        {
          "id": "MDRW2100012398.1.10",
          "form": "한국적인 입맛이군요 ㅋ",
          "original_form": "한국적인 입맛이군요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 00:31"
        },
        {
          "id": "MDRW2100012398.1.11",
          "form": "ㅋㅋ 저 콩국수 엄청 좋아하거든요",
          "original_form": "ㅋㅋ 저 콩국수 엄청 좋아하거든요",
          "speaker_id": "1",
          "time": "20210825 08:27"
        },
        {
          "id": "MDRW2100012398.1.12",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210825 10:51"
        },
        {
          "id": "MDRW2100012398.1.13",
          "form": "어른입맛",
          "original_form": "어른입맛",
          "speaker_id": "2",
          "time": "20210825 10:52"
        },
        {
          "id": "MDRW2100012398.1.14",
          "form": "어렸을 때부터 먹어서 익숙한가봐요",
          "original_form": "어렸을 때부터 먹어서 익숙한가봐요",
          "speaker_id": "1",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100012398.1.15",
          "form": "name2님은 콩국수 좋아하세요?",
          "original_form": "&name2&님은 콩국수 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100012398.1.16",
          "form": "전 콩국수는 그냥 ... 있으면 먹고",
          "original_form": "전 콩국수는 그냥 ... 있으면 먹고",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100012398.1.17",
          "form": "찾아서 먹진않아요 ㅋㅋㅋ",
          "original_form": "찾아서 먹진않아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100012398.1.18",
          "form": "아하... 그럼 어떤거 좋아하세요?",
          "original_form": "아하... 그럼 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100012398.1.19",
          "form": "전 새우,게 이런것들 좋아해요",
          "original_form": "전 새우,게 이런것들 좋아해요",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100012398.1.20",
          "form": "헐 저도 좋아해요",
          "original_form": "헐 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100012398.1.21",
          "form": "ㅋㅋㅋ 새우 싫어하는사람 많이 못본거 같아요",
          "original_form": "ㅋㅋㅋ 새우 싫어하는사람 많이 못본거 같아요",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100012398.1.22",
          "form": "꽃게 너무 좋아요",
          "original_form": "꽃게 너무 좋아요",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100012398.1.23",
          "form": "대게 홍게 같은건 먹다보면 질리는데",
          "original_form": "대게 홍게 같은건 먹다보면 질리는데",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100012398.1.24",
          "form": "꽃게는 안질려요 ㅋㅋㅋ",
          "original_form": "꽃게는 안질려요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100012398.1.25",
          "form": "꽃게는 인정이죠ㅋㅋ",
          "original_form": "꽃게는 인정이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100012398.1.26",
          "form": "혹시 간장게장 좋아하세요?",
          "original_form": "혹시 간장게장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100012398.1.27",
          "form": "네 엄청",
          "original_form": "네 엄청",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100012398.1.28",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100012398.1.29",
          "form": "게장 좋아하시는군요?",
          "original_form": "게장 좋아하시는군요?",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100012398.1.30",
          "form": "네ㅎㅎ 게장에 밥 비벼먹으면 한끼 뚝딱인데",
          "original_form": "네ㅎㅎ 게장에 밥 비벼먹으면 한끼 뚝딱인데",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100012398.1.31",
          "form": "게장 맛집에 못간 지 오래됐네요ㅠ",
          "original_form": "게장 맛집에 못간 지 오래됐네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100012398.1.32",
