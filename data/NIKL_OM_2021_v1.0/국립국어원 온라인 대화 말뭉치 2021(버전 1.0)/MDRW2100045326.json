      "utterance": [
        {
          "id": "MDRW2100045326.1.1",
          "form": "저는 열라면 좋아해요 매운걸 좋아해서",
          "original_form": "저는 열라면 좋아해요 매운걸 좋아해서",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045326.1.2",
          "form": "오 그라면 가성비 좋지않나요??",
          "original_form": "오 그라면 가성비 좋지않나요??",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045326.1.3",
          "form": "네 맞아요 싼편인데 맛있어서 좋아해요",
          "original_form": "네 맞아요 싼편인데 맛있어서 좋아해요",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045326.1.4",
          "form": "계란도 넣어서 드세요?",
          "original_form": "계란도 넣어서 드세요?",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045326.1.5",
          "form": "순두부 열라면 종종 해먹어요!",
          "original_form": "순두부 열라면 종종 해먹어요!",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045326.1.6",
          "form": "계란도 나쁘진 않아요 ㅋㅋ",
          "original_form": "계란도 나쁘진 않아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045326.1.7",
          "form": "우아 순두부 넣어서 드시는더에요??",
          "original_form": "우아 순두부 넣어서 드시는더에요??",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045326.1.8",
          "form": "나름 유명한 레시피 일꺼예요",
          "original_form": "나름 유명한 레시피 일꺼예요",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045326.1.9",
          "form": "오오 저도 해바야 겟어요",
          "original_form": "오오 저도 해바야 겟어요",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045326.1.10",
          "form": "매운게 좀 순화 되나요?",
          "original_form": "매운게 좀 순화 되나요?",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045326.1.11",
          "form": "순두부 다진마늘 후추조금이요",
          "original_form": "순두부 다진마늘 후추조금이요",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045326.1.12",
          "form": "약간은요...",
          "original_form": "약간은요...",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045326.1.13",
          "form": "매운거 잘 못드시나요?",
          "original_form": "매운거 잘 못드시나요?",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045326.1.14",
          "form": "네내 ㅠㅜㅜ 속이 안좋아져거 ㅜㅜ",
          "original_form": "네내 ㅠㅜㅜ 속이 안좋아져거 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045326.1.15",
          "form": "약간 간편찌개라면 느낌 레시피예요",
          "original_form": "약간 간편찌개라면 느낌 레시피예요",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045326.1.16",
          "form": "드시고 싶으시면 스프량을 조절하시거나 조금 싱겁게 먹는방법도 있으실거같아요",
          "original_form": "드시고 싶으시면 스프량을 조절하시거나 조금 싱겁게 먹는방법도 있으실거같아요",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045326.1.17",
          "form": "오오 너뮤 좋아요 ㅠㅠ",
          "original_form": "오오 너뮤 좋아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045326.1.18",
          "form": "가끔 너무 매운음식은 후폭풍이 심한게 싫은거같아요",
          "original_form": "가끔 너무 매운음식은 후폭풍이 심한게 싫은거같아요",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045326.1.19",
          "form": "먹을땐 좋은데!",
          "original_form": "먹을땐 좋은데!",
          "speaker_id": "2",
          "time": "20210908 02:03"
        }
      ]
