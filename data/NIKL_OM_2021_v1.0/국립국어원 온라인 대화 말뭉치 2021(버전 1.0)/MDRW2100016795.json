      "utterance": [
        {
          "id": "MDRW2100016795.1.1",
          "form": "요리하는거 좋아하세요?",
          "original_form": "요리하는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100016795.1.2",
          "form": "어ㅜㅜ 저는 못합니다ㅜ",
          "original_form": "어ㅜㅜ 저는 못합니다ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100016795.1.3",
          "form": "님은 잘하시나요",
          "original_form": "님은 잘하시나요",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100016795.1.4",
          "form": "저도 못해요ㅜ",
          "original_form": "저도 못해요ㅜ",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100016795.1.5",
          "form": "쉬운거만 레시피보고 따라하는정도에요ㅎ",
          "original_form": "쉬운거만 레시피보고 따라하는정도에요ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100016795.1.6",
          "form": "장기요리가 뭔가요",
          "original_form": "장기요리가 뭔가요",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100016795.1.7",
          "form": "장기요리?그게 뭐죠?",
          "original_form": "장기요리?그게 뭐죠?",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100016795.1.8",
          "form": "아니ㅋㅋ 최근에 한 요리는 뭔가요!",
          "original_form": "아니ㅋㅋ 최근에 한 요리는 뭔가요!",
          "speaker_id": "1",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100016795.1.9",
          "form": "단호박에그슬럿이 그나마 요리라고 부를수있을듯해요ㅋㅋㅋ",
          "original_form": "단호박에그슬럿이 그나마 요리라고 부를수있을듯해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100016795.1.10",
          "form": "아니ㅋ  그나마라기엔 너무 본격적이잖아요ㅋㅋ",
          "original_form": "아니ㅋ  그나마라기엔 너무 본격적이잖아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100016795.1.11",
          "form": "엄청 간단한데 있어보여요ㅋㅋㅋ",
          "original_form": "엄청 간단한데 있어보여요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100016795.1.12",
          "form": "단호박 파내고 계란넣고 치즈넣으면 끝ㅋㅋ이름이 있어보여서ㅋㅋ",
          "original_form": "단호박 파내고 계란넣고 치즈넣으면 끝ㅋㅋ이름이 있어보여서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100016795.1.13",
          "form": "단호박 파내는것에서 아웃이에요!",
          "original_form": "단호박 파내는것에서 아웃이에요!",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100016795.1.14",
          "form": "그냥 단호박찜도 손가는데ㄷㄷㄷ",
          "original_form": "그냥 단호박찜도 손가는데ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100016795.1.15",
          "form": "ㅎㅎ사실 저도 귀찮아서 잘 안해먹어요ㅎㅎ",
          "original_form": "ㅎㅎ사실 저도 귀찮아서 잘 안해먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100016795.1.16",
          "form": "저녁은 그냥 목살 구워먹고 치웠어요ㅋㅋ",
          "original_form": "저녁은 그냥 목살 구워먹고 치웠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100016795.1.17",
          "form": "아니? 앞에 그냥이 왜 들어가죠!",
          "original_form": "아니? 앞에 그냥이 왜 들어가죠!",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100016795.1.18",
          "form": "고기님인데",
          "original_form": "고기님인데",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100016795.1.19",
          "form": "이것도 요리인가요?ㅎㅎ",
          "original_form": "이것도 요리인가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100016795.1.20",
          "form": "혹시 제가 양자로 입적해도 될까요ㅎㅎ",
          "original_form": "혹시 제가 양자로 입적해도 될까요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100016795.1.21",
          "form": "호적 파이고 싶으실수도...ㅋㅋ",
          "original_form": "호적 파이고 싶으실수도...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:14"
        }
      ]
