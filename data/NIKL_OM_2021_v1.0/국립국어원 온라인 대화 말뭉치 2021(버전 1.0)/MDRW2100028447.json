      "utterance": [
        {
          "id": "MDRW2100028447.1.1",
          "form": "요즘은 안마시려고 노력하지ㅋㅋㅋ",
          "original_form": "요즘은 안마시려고 노력하지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028447.1.2",
          "form": "위장이 박살나서..",
          "original_form": "위장이 박살나서..",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028447.1.3",
          "form": "너는 커피는 안마시잖아",
          "original_form": "너는 커피는 안마시잖아",
          "speaker_id": "2",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028447.1.4",
          "form": "아메리카노를 안마시지",
          "original_form": "아메리카노를 안마시지",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028447.1.5",
          "form": "바닐라라떼 완전 좋아함 ㅋㅋㅋㅋ",
          "original_form": "바닐라라떼 완전 좋아함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028447.1.6",
          "form": "ㅋㅋㅋㅋㅋ아니은근",
          "original_form": "ㅋㅋㅋㅋㅋ아니은근",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028447.1.7",
          "form": "카페인있는거잘먹는단말이야",
          "original_form": "카페인있는거잘먹는단말이야",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028447.1.8",
          "form": "밀크티도그렇고",
          "original_form": "밀크티도그렇고",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028447.1.9",
          "form": "바닐라라떼 무슨브랜드거 좋아해",
          "original_form": "바닐라라떼 무슨브랜드거 좋아해",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028447.1.10",
          "form": "어 딱히 브랜드 안따지고 다 잘 먹어 ㅋㅋㅋㅋ",
          "original_form": "어 딱히 브랜드 안따지고 다 잘 먹어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100028447.1.11",
          "form": "요즘엔 집에 카누 바닐라 라떼 사놓고 한 번씩 우유에 타먹음",
          "original_form": "요즘엔 집에 카누 바닐라 라떼 사놓고 한 번씩 우유에 타먹음",
          "speaker_id": "1",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100028447.1.12",
          "form": "오그거 존맛이라던데 엄마가ㅋㅋㅋ",
          "original_form": "오그거 존맛이라던데 엄마가ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028447.1.13",
          "form": "옛날에 대학동기오빠가",
          "original_form": "옛날에 대학동기오빠가",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028447.1.14",
          "form": "커피빈 바닐라라떼 개좋아했었는데",
          "original_form": "커피빈 바닐라라떼 개좋아했었는데",
          "speaker_id": "2",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028447.1.15",
          "form": "커피빈거 안먹어봄?",
          "original_form": "커피빈거 안먹어봄?",
          "speaker_id": "2",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028447.1.16",
          "form": "ㅇㅇ 안먹어봤어",
          "original_form": "ㅇㅇ 안먹어봤어",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028447.1.17",
          "form": "그러고보니 커피빈거 유명했던거 같은데",
          "original_form": "그러고보니 커피빈거 유명했던거 같은데",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028447.1.18",
          "form": "커피빈 프차가 부산에 많이 없지않나?",
          "original_form": "커피빈 프차가 부산에 많이 없지않나?",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028447.1.19",
          "form": "아니 부산만이아니고",
          "original_form": "아니 부산만이아니고",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028447.1.20",
          "form": "ㅋㅋㅋ전국에서 사라지는중..",
          "original_form": "ㅋㅋㅋ전국에서 사라지는중..",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028447.1.21",
          "form": "카페 디저트는 어디가",
          "original_form": "카페 디저트는 어디가",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028447.1.22",
          "form": "제일 좋았어?",
          "original_form": "제일 좋았어?",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028447.1.23",
          "form": "음 원래는 투썸이 케이크 최고였던 거 같은데",
          "original_form": "음 원래는 투썸이 케이크 최고였던 거 같은데",
          "speaker_id": "1",
          "time": "20210903 08:58"
        },
        {
          "id": "MDRW2100028447.1.24",
          "form": "투썸도 예전만 못한거 같어",
          "original_form": "투썸도 예전만 못한거 같어",
          "speaker_id": "1",
          "time": "20210903 08:58"
        },
        {
          "id": "MDRW2100028447.1.25",
          "form": "요새는 개인 베이커리카페도 많이 생기니까",
          "original_form": "요새는 개인 베이커리카페도 많이 생기니까",
          "speaker_id": "1",
          "time": "20210903 08:58"
        },
        {
          "id": "MDRW2100028447.1.26",
          "form": "프차보다 그런데가 나은듯..?",
          "original_form": "프차보다 그런데가 나은듯..?",
          "speaker_id": "1",
          "time": "20210903 08:59"
        },
        {
          "id": "MDRW2100028447.1.27",
          "form": "오.. 최근에",
          "original_form": "오.. 최근에",
          "speaker_id": "2",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100028447.1.28",
          "form": "먹었던거중에 젤맛있었던 메뉴 뭐야",
          "original_form": "먹었던거중에 젤맛있었던 메뉴 뭐야",
          "speaker_id": "2",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100028447.1.29",
          "form": "요즘엔...감동적인 걸 못먹어봤어",
          "original_form": "요즘엔...감동적인 걸 못먹어봤어",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100028447.1.30",
          "form": "엥 머야",
          "original_form": "엥 머야",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100028447.1.31",
          "form": "디저트류는 특별한 계기가 있거나 사람 만나야 먹으니까",
          "original_form": "디저트류는 특별한 계기가 있거나 사람 만나야 먹으니까",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100028447.1.32",
          "form": "아니 개인베이커리얘기하길래",
          "original_form": "아니 개인베이커리얘기하길래",
          "speaker_id": "2",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100028447.1.33",
          "form": "최근에 좋은데가본줄ㅋㅋㅋㅋ",
          "original_form": "최근에 좋은데가본줄ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100028447.1.34",
          "form": "아니 걍 sns 같은데 예쁜 사진 많이 올라오니까 ㅋㅋㅋㅋ",
          "original_form": "아니 걍 sns 같은데 예쁜 사진 많이 올라오니까 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100028447.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ겉모양이 맛보다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ겉모양이 맛보다",
          "speaker_id": "2",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100028447.1.36",
          "form": "중요하다는거야",
          "original_form": "중요하다는거야",
          "speaker_id": "2",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100028447.1.37",
          "form": "그건 절대 아니지",
          "original_form": "그건 절대 아니지",
          "speaker_id": "1",
          "time": "20210903 18:49"
