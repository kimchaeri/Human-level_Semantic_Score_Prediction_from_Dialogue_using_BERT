      "utterance": [
        {
          "id": "MDRW2100046397.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211021 14:44"
        },
        {
          "id": "MDRW2100046397.1.2",
          "form": "안녕하세요~ 점심은 드셨나요?",
          "original_form": "안녕하세요~ 점심은 드셨나요?",
          "speaker_id": "2",
          "time": "20211021 14:45"
        },
        {
          "id": "MDRW2100046397.1.3",
          "form": "네\r\n고씨라 name2를 선택했어요 ㅎㅎ",
          "original_form": "네\r\n고씨라 &name2&를 선택했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211021 14:45"
        },
        {
          "id": "MDRW2100046397.1.4",
          "form": "아뇨 ㅎㅎ 저도 고가입니다",
          "original_form": "아뇨 ㅎㅎ 저도 고가입니다",
          "speaker_id": "2",
          "time": "20211021 14:46"
        },
        {
          "id": "MDRW2100046397.1.5",
          "form": "ㅋㅋ 재미나네요",
          "original_form": "ㅋㅋ 재미나네요",
          "speaker_id": "1",
          "time": "20211021 14:47"
        },
        {
          "id": "MDRW2100046397.1.6",
          "form": "요즘 유행하는 드라마나 영화 보신 것 있으신가요?",
          "original_form": "요즘 유행하는 드라마나 영화 보신 것 있으신가요?",
          "speaker_id": "2",
          "time": "20211021 14:48"
        },
        {
          "id": "MDRW2100046397.1.7",
          "form": "보이스라는 보이스피싱 영화 봤어요",
          "original_form": "보이스라는 보이스피싱 영화 봤어요",
          "speaker_id": "1",
          "time": "20211021 14:48"
        },
        {
          "id": "MDRW2100046397.1.8",
          "form": "오 어떤 내용인가요?",
          "original_form": "오 어떤 내용인가요?",
          "speaker_id": "2",
          "time": "20211021 14:49"
        },
        {
          "id": "MDRW2100046397.1.9",
          "form": "변요한이 나오는 것 같던데",
          "original_form": "변요한이 나오는 것 같던데",
          "speaker_id": "2",
          "time": "20211021 14:49"
        },
        {
          "id": "MDRW2100046397.1.10",
          "form": "네 근데 변요한 보다 보이스피싱 전문가 김무열 연기가 뛰어났어요",
          "original_form": "네 근데 변요한 보다 보이스피싱 전문가 김무열 연기가 뛰어났어요",
          "speaker_id": "1",
          "time": "20211021 14:51"
        },
        {
          "id": "MDRW2100046397.1.11",
          "form": "진짜 같았습니다",
          "original_form": "진짜 같았습니다",
          "speaker_id": "1",
          "time": "20211021 14:51"
        },
        {
          "id": "MDRW2100046397.1.12",
          "form": "와 김무열이 연기를 잘하나봐요",
          "original_form": "와 김무열이 연기를 잘하나봐요",
          "speaker_id": "2",
          "time": "20211021 14:52"
        },
        {
          "id": "MDRW2100046397.1.13",
          "form": "나오는 영화마다 평이 좋네요 ㅎㅎ",
          "original_form": "나오는 영화마다 평이 좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211021 14:52"
        },
        {
          "id": "MDRW2100046397.1.14",
          "form": "실제처럼 보이스피싱 시스템을 표현했는데...\r\n\r\n연기하는 게 정말 죽이고 싶을 정도였어요",
          "original_form": "실제처럼 보이스피싱 시스템을 표현했는데...\r\n\r\n연기하는 게 정말 죽이고 싶을 정도였어요",
          "speaker_id": "1",
          "time": "20211021 14:52"
        },
        {
          "id": "MDRW2100046397.1.15",
          "form": "ㅋㅋㅋ그정도인가요",
          "original_form": "ㅋㅋㅋ그정도인가요",
          "speaker_id": "2",
          "time": "20211021 14:53"
        },
        {
          "id": "MDRW2100046397.1.16",
          "form": "네 같이 본 일행두 그랬어요",
          "original_form": "네 같이 본 일행두 그랬어요",
          "speaker_id": "1",
          "time": "20211021 14:53"
        },
        {
          "id": "MDRW2100046397.1.17",
          "form": "저도 한번 봐야겠네요",
          "original_form": "저도 한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20211021 14:53"
        },
        {
          "id": "MDRW2100046397.1.18",
          "form": "꼭 보시길 권유드립니다",
          "original_form": "꼭 보시길 권유드립니다",
          "speaker_id": "1",
          "time": "20211021 14:54"
        },
        {
          "id": "MDRW2100046397.1.19",
          "form": "넵ㅎㅎ 영화관을 간지가 꽤 됐는데",
          "original_form": "넵ㅎㅎ 영화관을 간지가 꽤 됐는데",
          "speaker_id": "2",
          "time": "20211021 14:54"
        },
        {
          "id": "MDRW2100046397.1.20",
          "form": "이번에 그걸 봐야겠네요 추천 감사해요",
          "original_form": "이번에 그걸 봐야겠네요 추천 감사해요",
