      "utterance": [
        {
          "id": "MDRW2100006858.1.1",
          "form": "책은 사시나요?",
          "original_form": "책은 사시나요?",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006858.1.2",
          "form": "아뇨... 정가제 들어오고 비싸져서 최근엔 안샀어요ㅠ",
          "original_form": "아뇨... 정가제 들어오고 비싸져서 최근엔 안샀어요ㅠ",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006858.1.3",
          "form": "그 전엔 많이 사셨나요",
          "original_form": "그 전엔 많이 사셨나요",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006858.1.4",
          "form": "네 전에는 이거저거 많이 샀었어요!",
          "original_form": "네 전에는 이거저거 많이 샀었어요!",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006858.1.5",
          "form": "그럼 그 책들은 아직 보관중인가요",
          "original_form": "그럼 그 책들은 아직 보관중인가요",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006858.1.6",
          "form": "네!! 지금 집에는 없는데 본가에 빛 안들게해서 보관중이에요",
          "original_form": "네!! 지금 집에는 없는데 본가에 빛 안들게해서 보관중이에요",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100006858.1.7",
          "form": "책이 나중엔 짐이되서 잘 못사겠더라구요",
          "original_form": "책이 나중엔 짐이되서 잘 못사겠더라구요",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006858.1.8",
          "form": "그건 그래요ㅠㅠ",
          "original_form": "그건 그래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006858.1.9",
          "form": "종이책이 좋긴한데 짐이 되는게 아쉬워요ㅠㅠ",
          "original_form": "종이책이 좋긴한데 짐이 되는게 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006858.1.10",
          "form": "리더기도 사용하시나요",
          "original_form": "리더기도 사용하시나요",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100006858.1.11",
          "form": "아뇨 리더기를 따로 살까 했는데 플랫폼마다 되고 안되고가 있어서 말았어요ㅠㅠ",
          "original_form": "아뇨 리더기를 따로 살까 했는데 플랫폼마다 되고 안되고가 있어서 말았어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006858.1.12",
          "form": "아 그렇죠 와이프도 적응못하고 처분하더군요",
          "original_form": "아 그렇죠 와이프도 적응못하고 처분하더군요",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006858.1.13",
          "form": "그냥 태블릿으로 보는게 속편하겠더라구욬ㅋㅋㅋ",
          "original_form": "그냥 태블릿으로 보는게 속편하겠더라구욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006858.1.14",
          "form": "테블릿 있음 더 그러겠어요 반응이 늦더라구요",
          "original_form": "테블릿 있음 더 그러겠어요 반응이 늦더라구요",
          "speaker_id": "1",
          "time": "20210908 11:17"
        }
      ]
