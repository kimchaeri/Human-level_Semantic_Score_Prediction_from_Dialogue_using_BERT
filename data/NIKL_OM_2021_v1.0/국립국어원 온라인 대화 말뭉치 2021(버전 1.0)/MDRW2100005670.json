      "utterance": [
        {
          "id": "MDRW2100005670.1.1",
          "form": "요즘 읽는 책 있나요?ㅎㅎ",
          "original_form": "요즘 읽는 책 있나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005670.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005670.1.3",
          "form": "일관련책읽어요",
          "original_form": "일관련책읽어요",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005670.1.4",
          "form": "우와 어때요? 재밌어요?",
          "original_form": "우와 어때요? 재밌어요?",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005670.1.5",
          "form": "어려워요ㅠㅜ",
          "original_form": "어려워요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005670.1.6",
          "form": "재미는 있기도하고 없기도하고",
          "original_form": "재미는 있기도하고 없기도하고",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005670.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005670.1.8",
          "form": "책 이름이 뭔데요?ㅋㅋ",
          "original_form": "책 이름이 뭔데요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005670.1.9",
          "form": "스프링인액션",
          "original_form": "스프링인액션",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005670.1.10",
          "form": "이요",
          "original_form": "이요",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005670.1.11",
          "form": "처음 들어봐요..",
          "original_form": "처음 들어봐요..",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005670.1.12",
          "form": "웹 애플리케이션에 관한 책인가용?",
          "original_form": "웹 애플리케이션에 관한 책인가용?",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005670.1.13",
          "form": "네이버에 찾아봤어요ㅋㅋ",
          "original_form": "네이버에 찾아봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005670.1.14",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005670.1.15",
          "form": "하고있는일관련해서 공부하는책이에요",
          "original_form": "하고있는일관련해서 공부하는책이에요",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005670.1.16",
          "form": "요즘읽는책있나요?",
          "original_form": "요즘읽는책있나요?",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005670.1.17",
          "form": "멋져요ㅋㅋㅋ 어플리케이션 항상 어떻게 만드나 궁금했는데",
          "original_form": "멋져요ㅋㅋㅋ 어플리케이션 항상 어떻게 만드나 궁금했는데",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005670.1.18",
          "form": "'아가미'라고 아시나요?",
          "original_form": "'아가미'라고 아시나요?",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005670.1.19",
          "form": "구병모 작가님 소설이에요",
          "original_form": "구병모 작가님 소설이에요",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005670.1.20",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005670.1.21",
          "form": "저 그거 초판샀어요",
          "original_form": "저 그거 초판샀어요",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005670.1.22",
          "form": "그 작가님 소설 좋아해여",
          "original_form": "그 작가님 소설 좋아해여",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005670.1.23",
          "form": "위저드베이커리랑 방주로오세요도 봤어여",
          "original_form": "위저드베이커리랑 방주로오세요도 봤어여",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005670.1.24",
          "form": "저는 책 많이 안읽다가 요즘에 읽기 시작했어요ㅋㅋㅋ",
          "original_form": "저는 책 많이 안읽다가 요즘에 읽기 시작했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005670.1.25",
          "form": "저는 요즘 바쁘단핑계로",
          "original_form": "저는 요즘 바쁘단핑계로",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005670.1.26",
          "form": "위저드베이커리 읽고싶은데 도서관에 항상 대출중이저라구여",
          "original_form": "위저드베이커리 읽고싶은데 도서관에 항상 대출중이저라구여",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005670.1.27",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005670.1.28",
          "form": "책못본지오래됐어요",
          "original_form": "책못본지오래됐어요",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005670.1.29",
          "form": "아직도인기도서인가보네요",
          "original_form": "아직도인기도서인가보네요",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005670.1.30",
          "form": "네ㅋㅋㅋ아직도 인기도서인것 같아요",
          "original_form": "네ㅋㅋㅋ아직도 인기도서인것 같아요",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005670.1.31",
          "form": "아몬드랑 위저드베이커리는 항상 대출중, 예약중이더라구요ㅋㅋ",
          "original_form": "아몬드랑 위저드베이커리는 항상 대출중, 예약중이더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005670.1.32",
          "form": "위저드베이커이",
          "original_form": "위저드베이커이",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005670.1.33",
          "form": "강추예요",
          "original_form": "강추예요",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005670.1.34",
          "form": "오 꼭 볼게요!",
          "original_form": "오 꼭 볼게요!",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100005670.1.35",
          "form": "저도 하나 추천할게요~~",
          "original_form": "저도 하나 추천할게요~~",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100005670.1.36",
          "form": "GV빌런 고태경이요!",
          "original_form": "GV빌런 고태경이요!",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100005670.1.37",
          "form": "오 메모해둘게요",
          "original_form": "오 메모해둘게요",
          "speaker_id": "2",
          "time": "20210822 00:02"
        }
      ]
