      "utterance": [
        {
          "id": "MDRW2100002863.1.1",
          "form": "런닝맨처럼 책도 좋아하시나요?",
          "original_form": "런닝맨처럼 책도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002863.1.2",
          "form": "책은 원래 읽다가 요즘은 멈췄어요",
          "original_form": "책은 원래 읽다가 요즘은 멈췄어요",
          "speaker_id": "2",
          "time": "20210820 16:08"
        },
        {
          "id": "MDRW2100002863.1.3",
          "form": "책 좋아하시나요?",
          "original_form": "책 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002863.1.4",
          "form": "ㅎㅎ 책은 좋기보다 그냥..ㅋ",
          "original_form": "ㅎㅎ 책은 좋기보다 그냥..ㅋ",
          "speaker_id": "1",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002863.1.5",
          "form": "요즘에 책방 예쁜곳 많더라고요",
          "original_form": "요즘에 책방 예쁜곳 많더라고요",
          "speaker_id": "1",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002863.1.6",
          "form": "ㅋㅋㅋㅋㅋ 오 책방이요?",
          "original_form": "ㅋㅋㅋㅋㅋ 오 책방이요?",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002863.1.7",
          "form": "저도 책방 가보고싶네요",
          "original_form": "저도 책방 가보고싶네요",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002863.1.8",
          "form": "ㅋㅋㅋ 인스타에 작가 친구 맺으면",
          "original_form": "ㅋㅋㅋ 인스타에 작가 친구 맺으면",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002863.1.9",
          "form": "좋은 글을 많이 볼 수 있어요",
          "original_form": "좋은 글을 많이 볼 수 있어요",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002863.1.10",
          "form": "오 진짜요? 좀 알아봐야겠네요",
          "original_form": "오 진짜요? 좀 알아봐야겠네요",
          "speaker_id": "2",
          "time": "20210820 18:07"
        },
        {
          "id": "MDRW2100002863.1.11",
          "form": "좋은 글 읽는 것망으로도 아주 좋죠",
          "original_form": "좋은 글 읽는 것망으로도 아주 좋죠",
          "speaker_id": "2",
          "time": "20210820 18:07"
        },
        {
          "id": "MDRW2100002863.1.12",
          "form": "좋아하는 작가는 딱히 없지만",
          "original_form": "좋아하는 작가는 딱히 없지만",
          "speaker_id": "1",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100002863.1.13",
          "form": "코로나 시대여서 사람들 생각이 깊어진 거 같아요",
          "original_form": "코로나 시대여서 사람들 생각이 깊어진 거 같아요",
          "speaker_id": "1",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100002863.1.14",
          "form": "생각 할게 많아서 그럴까요?",
          "original_form": "생각 할게 많아서 그럴까요?",
          "speaker_id": "2",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100002863.1.15",
          "form": "아니면 할 수 있는게 독서 이런거라 그럴 수도 있어요",
          "original_form": "아니면 할 수 있는게 독서 이런거라 그럴 수도 있어요",
          "speaker_id": "2",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100002863.1.16",
          "form": "아 그렇네요ㅋㅋ",
          "original_form": "아 그렇네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100002863.1.17",
          "form": "만만한게 독서ㅋ",
          "original_form": "만만한게 독서ㅋ",
          "speaker_id": "1",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100002863.1.18",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 19:45"
        },
        {
          "id": "MDRW2100002863.1.19",
          "form": "독서 해야쥬",
          "original_form": "독서 해야쥬",
          "speaker_id": "2",
          "time": "20210820 19:45"
        },
        {
          "id": "MDRW2100002863.1.20",
          "form": "근데 스마트폰 때문에ㅋㅋㅋ 사실 좀ㅋㅋ",
          "original_form": "근데 스마트폰 때문에ㅋㅋㅋ 사실 좀ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 19:46"
        },
        {
          "id": "MDRW2100002863.1.21",
          "form": "맞아요 ㅠㅠ 그래저 전자책도 나오잖아요",
          "original_form": "맞아요 ㅠㅠ 그래저 전자책도 나오잖아요",
