      "utterance": [
        {
          "id": "MDRW2100014163.1.1",
          "form": "혹시 종교 있으세요? 전 무교라서",
          "original_form": "혹시 종교 있으세요? 전 무교라서",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014163.1.2",
          "form": "저도 무교예요",
          "original_form": "저도 무교예요",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014163.1.3",
          "form": "그래도 산에 가면 절에 가보긴 해요 구경하러",
          "original_form": "그래도 산에 가면 절에 가보긴 해요 구경하러",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014163.1.4",
          "form": "근데 그건 종교랑 전혀 상관없는 관광",
          "original_form": "근데 그건 종교랑 전혀 상관없는 관광",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014163.1.5",
          "form": "저도 그런거 좋아해서",
          "original_form": "저도 그런거 좋아해서",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014163.1.6",
          "form": "나중에 템플스테이같은거 할까생각중이에요",
          "original_form": "나중에 템플스테이같은거 할까생각중이에요",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014163.1.7",
          "form": "고즈넉한 분위기가 좋잖아요 뭔가",
          "original_form": "고즈넉한 분위기가 좋잖아요 뭔가",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014163.1.8",
          "form": "오 저도 한번 해보고싶다 생각했어요",
          "original_form": "오 저도 한번 해보고싶다 생각했어요",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014163.1.9",
          "form": "빡센거말고 스님이랑 뭐 만들기 주변구경하는",
          "original_form": "빡센거말고 스님이랑 뭐 만들기 주변구경하는",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014163.1.10",
          "form": "이시국 끝나야 그런것도 가고 그러는데 말이죠",
          "original_form": "이시국 끝나야 그런것도 가고 그러는데 말이죠",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014163.1.11",
          "form": "네네그런거",
          "original_form": "네네그런거",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014163.1.12",
          "form": "편한코스 있다더라구요",
          "original_form": "편한코스 있다더라구요",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014163.1.13",
          "form": "빡센건 못해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "빡센건 못해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014163.1.14",
          "form": "백팔배 하라고 그러면 어떡해요 ㅠㅠㅋㅋㅋ",
          "original_form": "백팔배 하라고 그러면 어떡해요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014163.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그럼 탈출각",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그럼 탈출각",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014163.1.16",
          "form": "생각만으로 도가니가 아파옵니다",
          "original_form": "생각만으로 도가니가 아파옵니다",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014163.1.17",
          "form": "그런건 중고딩때 수련회로",
          "original_form": "그런건 중고딩때 수련회로",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014163.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014163.1.19",
          "form": "충분한경험...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "충분한경험...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014163.1.20",
          "form": "그래도 정말 그렇게 주변경치 구경하는 템플스테이는 할만 하겠어요",
          "original_form": "그래도 정말 그렇게 주변경치 구경하는 템플스테이는 할만 하겠어요",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014163.1.21",
          "form": "어릴때는 성당을 다녔는데 좋은 추억으로 남겼습니다...",
          "original_form": "어릴때는 성당을 다녔는데 좋은 추억으로 남겼습니다...",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014163.1.22",
          "form": "어릴때부터 다녀서 청년부 활동도 하고 뭐 할수 있는건 다 한거 같네요",
          "original_form": "어릴때부터 다녀서 청년부 활동도 하고 뭐 할수 있는건 다 한거 같네요",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014163.1.23",
          "form": "제 옛날 친구도 어렸을때 종교활동같은거 많이했었는데",
          "original_form": "제 옛날 친구도 어렸을때 종교활동같은거 많이했었는데",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014163.1.24",
          "form": "이런저런 재밌는활동같은거 많이 해서 부러웠었어요",
          "original_form": "이런저런 재밌는활동같은거 많이 해서 부러웠었어요",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014163.1.25",
          "form": "뭐 바자회나 뭐만들기 단체여행이런것도 가고 하던데",
          "original_form": "뭐 바자회나 뭐만들기 단체여행이런것도 가고 하던데",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014163.1.26",
          "form": "집에서 가족들이 각자 바쁘면 청소년기에는 종교활동이 그부분을 조금은 채워줄 수 있더라고요",
          "original_form": "집에서 가족들이 각자 바쁘면 청소년기에는 종교활동이 그부분을 조금은 채워줄 수 있더라고요",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014163.1.27",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014163.1.28",
          "form": "연극도 하고 노래도 하고 춤도 추고",
          "original_form": "연극도 하고 노래도 하고 춤도 추고",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014163.1.29",
          "form": "오 재밌었겠어요",
          "original_form": "오 재밌었겠어요",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014163.1.30",
          "form": "좋았어요 단지 신앙이 생기지는 않더라구요",
          "original_form": "좋았어요 단지 신앙이 생기지는 않더라구요",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014163.1.31",
          "form": "그래서 그때 같이 다니던 친구들은 거의 아직도 다니고있어요",
          "original_form": "그래서 그때 같이 다니던 친구들은 거의 아직도 다니고있어요",
          "speaker_id": "2",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014163.1.32",
          "form": "저를 다시 데려가고 싶어하죠 ㅋㅋㅋ",
          "original_form": "저를 다시 데려가고 싶어하죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014163.1.33",
          "form": "아 어렵네요",
          "original_form": "아 어렵네요",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014163.1.34",
          "form": "제 친구는 부모님이 신앙심이 깊은데",
          "original_form": "제 친구는 부모님이 신앙심이 깊은데",
          "speaker_id": "1",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014163.1.35",
          "form": "친구가 완전 무교 성향이라서",
          "original_form": "친구가 완전 무교 성향이라서",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014163.1.36",
          "form": "스트레스 받고 그러더라구요",
          "original_form": "스트레스 받고 그러더라구요",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014163.1.37",
          "form": "그건 정말 그럴거 같아요 ㅠㅠ 가족내에서 괜히 좀 눈치 보일거 같고...",
          "original_form": "그건 정말 그럴거 같아요 ㅠㅠ 가족내에서 괜히 좀 눈치 보일거 같고...",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014163.1.38",
          "form": "잘 맞으면 진짜 재밌고 좋은경험 할수있는데",
          "original_form": "잘 맞으면 진짜 재밌고 좋은경험 할수있는데",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014163.1.39",
          "form": "성향이 다르면 정말힘들꺼같아요",
          "original_form": "성향이 다르면 정말힘들꺼같아요",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014163.1.40",
          "form": "비슷한 성향으로 가족이 정하지면 좋을텐데",
          "original_form": "비슷한 성향으로 가족이 정하지면 좋을텐데",
          "speaker_id": "1",
          "time": "20210825 18:08"
        }
