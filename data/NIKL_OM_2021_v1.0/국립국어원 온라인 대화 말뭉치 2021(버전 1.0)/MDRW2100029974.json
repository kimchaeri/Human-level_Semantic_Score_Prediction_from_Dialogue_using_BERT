        {
          "id": "MDRW2100029974.1.2",
          "form": "정모",
          "original_form": "정모",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029974.1.3",
          "form": "뜨든",
          "original_form": "뜨든",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029974.1.4",
          "form": "뜨든",
          "original_form": "뜨든",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029974.1.5",
          "form": "우리 정모 언제하지...",
          "original_form": "우리 정모 언제하지...",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029974.1.6",
          "form": "코로나가 끝나야 정모도 할텐데",
          "original_form": "코로나가 끝나야 정모도 할텐데",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029974.1.7",
          "form": "그니까... 작년도 못하고 올해도 못했어...",
          "original_form": "그니까... 작년도 못하고 올해도 못했어...",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029974.1.8",
          "form": "다같이 모여서 고기 구워먹는 거 완전 재밌었는데",
          "original_form": "다같이 모여서 고기 구워먹는 거 완전 재밌었는데",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029974.1.9",
          "form": "그니까....그때 완전 좋았지",
          "original_form": "그니까....그때 완전 좋았지",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.10",
          "form": "모기는 있었지만",
          "original_form": "모기는 있었지만",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.11",
          "form": "근데 올겨울에 정모하면 밖에서 고기 구워먹기는 못하겠지 ㅋㅋㅋ",
          "original_form": "근데 올겨울에 정모하면 밖에서 고기 구워먹기는 못하겠지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.12",
          "form": "식당 알아놔야해",
          "original_form": "식당 알아놔야해",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.13",
          "form": "겨울엔 안에서 꾸워먹자",
          "original_form": "겨울엔 안에서 꾸워먹자",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.14",
          "form": "안에서 꾸워먹으면 힘드러",
          "original_form": "안에서 꾸워먹으면 힘드러",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.15",
          "form": "아니면 파스타같은거 해먹어두 좋겟다",
          "original_form": "아니면 파스타같은거 해먹어두 좋겟다",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.16",
          "form": "헉 좋다",
          "original_form": "헉 좋다",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.17",
          "form": "그건그래...",
          "original_form": "그건그래...",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.18",
          "form": "주방 딸린 방으로 구해야겠내",
          "original_form": "주방 딸린 방으로 구해야겠내",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.19",
          "form": "파스타랑...리조또? 같은거 해먹자",
          "original_form": "파스타랑...리조또? 같은거 해먹자",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.20",
          "form": "조아",
          "original_form": "조아",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.21",
          "form": "조와",
          "original_form": "조와",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.22",
          "form": "릿기두 있으니까 이제",
          "original_form": "릿기두 있으니까 이제",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.23",
          "form": "맞아맞아 완전체라구",
          "original_form": "맞아맞아 완전체라구",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.24",
          "form": "좀 더 큰데로 구할 수 있을듯?",
          "original_form": "좀 더 큰데로 구할 수 있을듯?",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029974.1.25",
          "form": "구치",
          "original_form": "구치",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029974.1.26",
          "form": "닌텐도 들고 모이기(ㅋㅋㅋ",
          "original_form": "닌텐도 들고 모이기(ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029974.1.27",
          "form": "6명 가능한 곳 찾으려면 흠..",
          "original_form": "6명 가능한 곳 찾으려면 흠..",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029974.1.28",
          "form": "헐 좋다",
          "original_form": "헐 좋다",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029974.1.29",
          "form": "아니 근데 6명 다 닌텐도 있다는 게 웃겨",
          "original_form": "아니 근데 6명 다 닌텐도 있다는 게 웃겨",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029974.1.30",
          "form": "6명 가능한데면 또...한참 발품팔아야겠다",
          "original_form": "6명 가능한데면 또...한참 발품팔아야겠다",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029974.1.31",
          "form": "그니까 ㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029974.1.32",
          "form": "서울에 찾아봤는데 리조트 하나 있었어 ㅋㅋㅋ",
          "original_form": "서울에 찾아봤는데 리조트 하나 있었어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100029974.1.33",
          "form": "아무래도 안에서만 놀거면 서울이 낫지 않을가 싶어서",
          "original_form": "아무래도 안에서만 놀거면 서울이 낫지 않을가 싶어서",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100029974.1.34",
          "form": "아 진짜???",
          "original_form": "아 진짜???",
          "speaker_id": "2",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100029974.1.35",
          "form": "흠...그건 글킨한데",
          "original_form": "흠...그건 글킨한데",
          "speaker_id": "2",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100029974.1.36",
          "form": "서울은 비싸고 묵을데가 별루 없을듯...",
          "original_form": "서울은 비싸고 묵을데가 별루 없을듯...",
          "speaker_id": "2",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100029974.1.37",
          "form": "아니면 경기도로 내려가야...",
          "original_form": "아니면 경기도로 내려가야...",
          "speaker_id": "2",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100029974.1.38",
          "form": "맞아... 별로 없긴 햇어",
          "original_form": "맞아... 별로 없긴 햇어",
          "speaker_id": "1",
          "time": "20210905 11:03"
        },
        {
          "id": "MDRW2100029974.1.39",
          "form": "근데 경기도는 내가 잘 몰라가지구",
          "original_form": "근데 경기도는 내가 잘 몰라가지구",
          "speaker_id": "1",
          "time": "20210905 11:03"
        }
