      "utterance": [
        {
          "id": "MDRW2100006985.1.1",
          "form": "출근시러어",
          "original_form": "출근시러어",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006985.1.2",
          "form": "나도 싫어ㅠㅠㅠㅠㅠ",
          "original_form": "나도 싫어ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 09:14"
        },
        {
          "id": "MDRW2100006985.1.3",
          "form": "내일 월요일이네....",
          "original_form": "내일 월요일이네....",
          "speaker_id": "2",
          "time": "20210822 09:14"
        },
        {
          "id": "MDRW2100006985.1.4",
          "form": "일요일 이제 하루도 안 남았네.... ㅠㅠㅠㅠ",
          "original_form": "일요일 이제 하루도 안 남았네.... ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 09:14"
        },
        {
          "id": "MDRW2100006985.1.5",
          "form": "안돼ㅠㅠㅠㅠ",
          "original_form": "안돼ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100006985.1.6",
          "form": "왜 주말은 2일뿐이야",
          "original_form": "왜 주말은 2일뿐이야",
          "speaker_id": "1",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100006985.1.7",
          "form": "이번주에 재택 2번밖에 없어서 더 울적해...",
          "original_form": "이번주에 재택 2번밖에 없어서 더 울적해...",
          "speaker_id": "2",
          "time": "20210822 11:23"
        },
        {
          "id": "MDRW2100006985.1.8",
          "form": "재택만 5일 했으면 좋겠당",
          "original_form": "재택만 5일 했으면 좋겠당",
          "speaker_id": "2",
          "time": "20210822 11:23"
        },
        {
          "id": "MDRW2100006985.1.9",
          "form": "우린 재택이 없어ㅠㅠ슬푸당",
          "original_form": "우린 재택이 없어ㅠㅠ슬푸당",
          "speaker_id": "1",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100006985.1.10",
          "form": "작은 회사라 재택 필수가 아니더라구",
          "original_form": "작은 회사라 재택 필수가 아니더라구",
          "speaker_id": "1",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100006985.1.11",
          "form": "임원들이 상주직을 더 원하나보다!",
          "original_form": "임원들이 상주직을 더 원하나보다!",
          "speaker_id": "2",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100006985.1.12",
          "form": "우리도 처음엔 별로 재택 안 좋아하시는 것 같더니",
          "original_form": "우리도 처음엔 별로 재택 안 좋아하시는 것 같더니",
          "speaker_id": "2",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100006985.1.13",
          "form": "딱히 재택도 문제없네? 하시더니 늘려줬어 ㅋㅋㅋㅋ",
          "original_form": "딱히 재택도 문제없네? 하시더니 늘려줬어 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100006985.1.14",
          "form": "재택 좋은 점 알아주셨으면 좋겠다ㅠ",
          "original_form": "재택 좋은 점 알아주셨으면 좋겠다ㅠ",
          "speaker_id": "2",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100006985.1.15",
          "form": "ㅋㅋㅋㅋ맞아ㅠㅠ출퇴근시간만 줄어도 삶이 훨씬 쾌적해질텐데",
          "original_form": "ㅋㅋㅋㅋ맞아ㅠㅠ출퇴근시간만 줄어도 삶이 훨씬 쾌적해질텐데",
          "speaker_id": "1",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100006985.1.16",
          "form": "맞아! 재택하면 자다가도 바로 출근가능인데!",
          "original_form": "맞아! 재택하면 자다가도 바로 출근가능인데!",
          "speaker_id": "2",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100006985.1.17",
          "form": "ㅋㅋㅋㅋ마쟈",
          "original_form": "ㅋㅋㅋㅋ마쟈",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100006985.1.18",
          "form": "그래서 넘 힘들어서 1년만 채우고 그만둘라구ㅠ",
          "original_form": "그래서 넘 힘들어서 1년만 채우고 그만둘라구ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100006985.1.19",
          "form": "이직 생각하는구나!",
          "original_form": "이직 생각하는구나!",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100006985.1.20",
          "form": "재택하는 곳으로 꼭 갔으면 좋겠다 ㅎㅎ",
          "original_form": "재택하는 곳으로 꼭 갔으면 좋겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100006985.1.21",
          "form": "웅!!!아직 한 6개월은 더 일해야해ㅋㅋㅋ",
          "original_form": "웅!!!아직 한 6개월은 더 일해야해ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 08:20"
        },
        {
          "id": "MDRW2100006985.1.22",
          "form": "집 가까운 곳으로 이직 생각중이야??",
          "original_form": "집 가까운 곳으로 이직 생각중이야??",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100006985.1.23",
          "form": "응응 집 가까이에 회사 많은데 머하러 멀리까지 갔나 싶어ㅠㅠ",
          "original_form": "응응 집 가까이에 회사 많은데 머하러 멀리까지 갔나 싶어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 15:09"
        }
      ]
    }
  ]
