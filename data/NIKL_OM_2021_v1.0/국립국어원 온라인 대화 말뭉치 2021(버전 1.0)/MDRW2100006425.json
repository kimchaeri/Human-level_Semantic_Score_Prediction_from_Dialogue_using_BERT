      "utterance": [
        {
          "id": "MDRW2100006425.1.1",
          "form": "10월 말부터 위 드 코리아에",
          "original_form": "10월 말부터 위 드 코리아에",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100006425.1.2",
          "form": "저는 어렵다고 봐요",
          "original_form": "저는 어렵다고 봐요",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100006425.1.3",
          "form": "왜죠",
          "original_form": "왜죠",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100006425.1.4",
          "form": "일단 접종률이 높아야 되는데 아직은 너무 낮아요",
          "original_form": "일단 접종률이 높아야 되는데 아직은 너무 낮아요",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006425.1.5",
          "form": "그건 그렇죠",
          "original_form": "그건 그렇죠",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006425.1.6",
          "form": "그리고 확진자가 어쨌든 참 명 아래로 떨어져야 되는데",
          "original_form": "그리고 확진자가 어쨌든 참 명 아래로 떨어져야 되는데",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006425.1.7",
          "form": "거리두기를 해서 그런지 조금 줄어들지 않았나요",
          "original_form": "거리두기를 해서 그런지 조금 줄어들지 않았나요",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006425.1.8",
          "form": "계속해서 확진자가 1,000명이 넘고 있는데요",
          "original_form": "계속해서 확진자가 1,000명이 넘고 있는데요",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006425.1.9",
          "form": "그렇단 얘기는 단계장 전환 힘들다는",
          "original_form": "그렇단 얘기는 단계장 전환 힘들다는",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006425.1.10",
          "form": "조금 조심히 전 안 해야 될 거 같아요",
          "original_form": "조금 조심히 전 안 해야 될 거 같아요",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100006425.1.11",
          "form": "그건 그렇겠죠",
          "original_form": "그건 그렇겠죠",
