      "utterance": [
        {
          "id": "MDRW2100011913.1.1",
          "form": "지금 피카소전 하고있네요 보셨나요",
          "original_form": "지금 피카소전 하고있네요 보셨나요",
          "speaker_id": "2",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011913.1.2",
          "form": "아뇨 혹시 지역이 서울이세요?",
          "original_form": "아뇨 혹시 지역이 서울이세요?",
          "speaker_id": "1",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011913.1.3",
          "form": "저는 창원살아서 피카소전 이야기를 첨들어요",
          "original_form": "저는 창원살아서 피카소전 이야기를 첨들어요",
          "speaker_id": "1",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011913.1.4",
          "form": "네 일땜에 올라와있어요 전 부산 사람입니다",
          "original_form": "네 일땜에 올라와있어요 전 부산 사람입니다",
          "speaker_id": "2",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100011913.1.5",
          "form": "아아 그러시군요 ㅎㅎ",
          "original_form": "아아 그러시군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100011913.1.6",
          "form": "친구들 창원살아요",
          "original_form": "친구들 창원살아요",
          "speaker_id": "2",
          "time": "20210824 17:26"
        },
        {
          "id": "MDRW2100011913.1.7",
          "form": "괜히 반갑네요 ㅋㅋ",
          "original_form": "괜히 반갑네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:26"
        },
        {
          "id": "MDRW2100011913.1.8",
          "form": "아아 넹 ㅎㅎ",
          "original_form": "아아 넹 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:26"
        },
        {
          "id": "MDRW2100011913.1.9",
          "form": "창원친구는 고전영화좋아해서 같이 오드리햅번영화봤어요",
          "original_form": "창원친구는 고전영화좋아해서 같이 오드리햅번영화봤어요",
          "speaker_id": "2",
          "time": "20210824 17:26"
        },
        {
          "id": "MDRW2100011913.1.10",
          "form": "흑백 ㅋㅋ",
          "original_form": "흑백 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100011913.1.11",
          "form": "영화관에서 하나요??",
          "original_form": "영화관에서 하나요??",
          "speaker_id": "1",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100011913.1.12",
          "form": "그건 왓챠로봤고 그친구랑은 주전장, 김복동등등 인디영화봤어요 극장에서",
          "original_form": "그건 왓챠로봤고 그친구랑은 주전장, 김복동등등 인디영화봤어요 극장에서",
          "speaker_id": "2",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011913.1.13",
          "form": "가버나움 이런거요",
          "original_form": "가버나움 이런거요",
          "speaker_id": "2",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100011913.1.14",
          "form": "오 그러시군요 이런저런거 많이 관심있으신가봐요",
          "original_form": "오 그러시군요 이런저런거 많이 관심있으신가봐요",
          "speaker_id": "1",
          "time": "20210824 17:30"
        },
        {
          "id": "MDRW2100011913.1.15",
          "form": "관심가지면 좋은 이슈라서 봤어요",
          "original_form": "관심가지면 좋은 이슈라서 봤어요",
          "speaker_id": "2",
          "time": "20210824 17:30"
        },
        {
          "id": "MDRW2100011913.1.16",
          "form": "네 그렇게 영상화해서 보여주는게",
          "original_form": "네 그렇게 영상화해서 보여주는게",
          "speaker_id": "1",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011913.1.17",
          "form": "난민문제를 다룬영화에요 가버나움추천해용",
          "original_form": "난민문제를 다룬영화에요 가버나움추천해용",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011913.1.18",
          "form": "관심환기에 좋더라구요",
          "original_form": "관심환기에 좋더라구요",
          "speaker_id": "1",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011913.1.19",
          "form": "넵 추천 감사합니다!!",
          "original_form": "넵 추천 감사합니다!!",
          "speaker_id": "1",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011913.1.20",
          "form": "전문배우들이 아니고 스트리트 채용했는데",
          "original_form": "전문배우들이 아니고 스트리트 채용했는데",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011913.1.21",
          "form": "정말 잘해요",
          "original_form": "정말 잘해요",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011913.1.22",
          "form": "아아 네",
          "original_form": "아아 네",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011913.1.23",
          "form": "저는 영화자체는 많이 안보긴 하는데요",
          "original_form": "저는 영화자체는 많이 안보긴 하는데요",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011913.1.24",
          "form": "유명배우가 나오면 본인 이미지가 도드라져사",
          "original_form": "유명배우가 나오면 본인 이미지가 도드라져사",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011913.1.25",
          "form": "걍 모르는 듯한 예술영화가 더 그럴듯하게 느껴졌어요",
          "original_form": "걍 모르는 듯한 예술영화가 더 그럴듯하게 느껴졌어요",
          "speaker_id": "1",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100011913.1.26",
          "form": "진정성이 느껴져서 좋아요 예술영화",
          "original_form": "진정성이 느껴져서 좋아요 예술영화",
          "speaker_id": "2",
          "time": "20210824 17:33"
        }
      ]
    }
