      "utterance": [
        {
          "id": "MDRW2100003718.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003718.1.2",
          "form": "안녕하세요 넷플릭스 좋아하세요?",
          "original_form": "안녕하세요 넷플릭스 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003718.1.3",
          "form": "네 안그래도 어제 넷플에서 슬의 보다 잠들었어요",
          "original_form": "네 안그래도 어제 넷플에서 슬의 보다 잠들었어요",
          "speaker_id": "1",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003718.1.4",
          "form": "저는 이번에 첫달 가입했는데",
          "original_form": "저는 이번에 첫달 가입했는데",
          "speaker_id": "2",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.6",
          "form": "마음처럼 잘 들어가지지 않네요ㅎ휴ㅠㅠ",
          "original_form": "마음처럼 잘 들어가지지 않네요ㅎ휴ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.7",
          "form": "이제 가입하셨구나",
          "original_form": "이제 가입하셨구나",
          "speaker_id": "1",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.8",
          "form": "슬기로운 의사생활 한번 봐보세요",
          "original_form": "슬기로운 의사생활 한번 봐보세요",
          "speaker_id": "1",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.9",
          "form": "3250원이 아까울 정도예요ㅠㅠ",
          "original_form": "3250원이 아까울 정도예요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.10",
          "form": "매주 들어가시게 될거예요",
          "original_form": "매주 들어가시게 될거예요",
          "speaker_id": "1",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.11",
          "form": "그거 재밌나요?",
          "original_form": "그거 재밌나요?",
          "speaker_id": "2",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.12",
          "form": "대박 재밌어요ㅋ",
          "original_form": "대박 재밌어요ㅋ",
          "speaker_id": "1",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.13",
          "form": "우와 그렇구나 한 번 봐볼게요 고마워요!",
          "original_form": "우와 그렇구나 한 번 봐볼게요 고마워요!",
          "speaker_id": "2",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.14",
          "form": "슬의생 유명하더라구요",
          "original_form": "슬의생 유명하더라구요",
          "speaker_id": "2",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.15",
          "form": "저 넷플릭스 잘 안쓰다가 슬의 때문에 거의 매주 들어가요",
          "original_form": "저 넷플릭스 잘 안쓰다가 슬의 때문에 거의 매주 들어가요",
          "speaker_id": "1",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003718.1.16",
          "form": "요즘 넷플릭스에 빠지는 작품이 많아져서",
          "original_form": "요즘 넷플릭스에 빠지는 작품이 많아져서",
          "speaker_id": "1",
          "time": "20210821 11:10"
        },
        {
          "id": "MDRW2100003718.1.17",
          "form": "점점 별로되가는데 그나마 볼만해요ㅎ",
          "original_form": "점점 별로되가는데 그나마 볼만해요ㅎ",
          "speaker_id": "1",
          "time": "20210821 11:10"
