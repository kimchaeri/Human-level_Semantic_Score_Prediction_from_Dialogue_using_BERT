      "utterance": [
        {
          "id": "MDRW2100018148.1.1",
          "form": "엄마 컵 좀 살까",
          "original_form": "엄마 컵 좀 살까",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018148.1.2",
          "form": "컵 집에 많은데",
          "original_form": "컵 집에 많은데",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018148.1.3",
          "form": "이쁜 컵이없다..ㅎ",
          "original_form": "이쁜 컵이없다..ㅎ",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018148.1.4",
          "form": "이상한 말 하지 마라",
          "original_form": "이상한 말 하지 마라",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018148.1.5",
          "form": "왜 컵이 이뻐야 물도 마실 맛이 난다",
          "original_form": "왜 컵이 이뻐야 물도 마실 맛이 난다",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018148.1.6",
          "form": "그런게 어디 있니",
          "original_form": "그런게 어디 있니",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018148.1.7",
          "form": "여기 있다 ㅋ 유리컵 몇개 살겜 ㅎㅎ",
          "original_form": "여기 있다 ㅋ 유리컵 몇개 살겜 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018148.1.8",
          "form": "됐다 낭비다",
          "original_form": "됐다 낭비다",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018148.1.9",
          "form": "그럼 하나만 살게 내꺼로",
          "original_form": "그럼 하나만 살게 내꺼로",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018148.1.10",
          "form": "쓸데없이",
          "original_form": "쓸데없이",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018148.1.11",
          "form": "몰라! 내꺼 살거당",
          "original_form": "몰라! 내꺼 살거당",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018148.1.12",
          "form": "알아서 해라",
          "original_form": "알아서 해라",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018148.1.13",
