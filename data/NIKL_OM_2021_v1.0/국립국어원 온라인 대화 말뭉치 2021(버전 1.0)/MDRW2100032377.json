      "utterance": [
        {
          "id": "MDRW2100032377.1.1",
          "form": "주변에 제일 맛있는집이 어디예요?",
          "original_form": "주변에 제일 맛있는집이 어디예요?",
          "speaker_id": "1",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100032377.1.2",
          "form": "가게 이름이요? 주메뉴?",
          "original_form": "가게 이름이요? 주메뉴?",
          "speaker_id": "2",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100032377.1.3",
          "form": "메뉴?",
          "original_form": "메뉴?",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100032377.1.4",
          "form": "데이트코스라서 양식집이 제일 많아요",
          "original_form": "데이트코스라서 양식집이 제일 많아요",
          "speaker_id": "2",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100032377.1.5",
          "form": "집근처가 데이트코스라니 ᆢㅎㅎ",
          "original_form": "집근처가 데이트코스라니 ᆢㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032377.1.6",
          "form": "양식은 뭐 좋아하세요?",
          "original_form": "양식은 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032377.1.7",
          "form": "저 리조또 파스타 피자 다 좋아하는데",
          "original_form": "저 리조또 파스타 피자 다 좋아하는데",
          "speaker_id": "2",
          "time": "20210905 16:44"
        },
        {
          "id": "MDRW2100032377.1.8",
          "form": "다 맛있죠",
          "original_form": "다 맛있죠",
          "speaker_id": "1",
          "time": "20210905 16:44"
        },
        {
          "id": "MDRW2100032377.1.9",
          "form": "포케 괜찮더라구요, 하와이식 비빔밥",
          "original_form": "포케 괜찮더라구요, 하와이식 비빔밥",
          "speaker_id": "2",
          "time": "20210905 16:44"
        },
        {
          "id": "MDRW2100032377.1.10",
          "form": "건강한 맛",
          "original_form": "건강한 맛",
          "speaker_id": "2",
          "time": "20210905 16:44"
        },
        {
          "id": "MDRW2100032377.1.11",
          "form": "회덮밥 비슷한가요?",
          "original_form": "회덮밥 비슷한가요?",
          "speaker_id": "1",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100032377.1.12",
          "form": "참치나 연어에 채소 곁들여서 비벼먹는 거예요",
          "original_form": "참치나 연어에 채소 곁들여서 비벼먹는 거예요",
          "speaker_id": "2",
          "time": "20210905 16:46"
        },
        {
          "id": "MDRW2100032377.1.13",
          "form": "콩이나 김도 넣었던 것 같은",
          "original_form": "콩이나 김도 넣었던 것 같은",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032377.1.14",
          "form": "건강한 날것같긴하네요",
          "original_form": "건강한 날것같긴하네요",
          "speaker_id": "1",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032377.1.15",
          "form": "콩 좋아하세요?",
          "original_form": "콩 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032377.1.16",
          "form": "별로 안 좋아하는데 포케로는 먹을수 있었어요",
          "original_form": "별로 안 좋아하는데 포케로는 먹을수 있었어요",
          "speaker_id": "2",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032377.1.17",
          "form": "밥에 콩이나 팥있는거 안 좋아해요",
          "original_form": "밥에 콩이나 팥있는거 안 좋아해요",
          "speaker_id": "2",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032377.1.18",
          "form": "저도 싫어해요",
          "original_form": "저도 싫어해요",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032377.1.19",
          "form": "특히 콩밥",
          "original_form": "특히 콩밥",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032377.1.20",
          "form": "그래서 급 식 잔반없는 날 너무 힘들었어요",
          "original_form": "그래서 급 식 잔반없는 날 너무 힘들었어요",
          "speaker_id": "2",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032377.1.21",
          "form": "저도 학생때 양이 작아서 매일 힘들었어요ㅜㅜㅜ",
          "original_form": "저도 학생때 양이 작아서 매일 힘들었어요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 17:13"
        }
      ]
