      "utterance": [
        {
          "id": "MDRW2100034396.1.1",
          "form": "안녕하세용 어제 뭐 드셨어요??",
          "original_form": "안녕하세용 어제 뭐 드셨어요??",
          "speaker_id": "1",
          "time": "20210828 04:09"
        },
        {
          "id": "MDRW2100034396.1.2",
          "form": "안녕하세요 떡볶이요 님은요?",
          "original_form": "안녕하세요 떡볶이요 님은요?",
          "speaker_id": "2",
          "time": "20210828 04:11"
        },
        {
          "id": "MDRW2100034396.1.3",
          "form": "저는 비가와서 그런지 짬뽕 먹었어요!ㅋㅋ",
          "original_form": "저는 비가와서 그런지 짬뽕 먹었어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 04:11"
        },
        {
          "id": "MDRW2100034396.1.4",
          "form": "떡볶이 좋아하세요?",
          "original_form": "떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 04:11"
        },
        {
          "id": "MDRW2100034396.1.5",
          "form": "오 얼큰한거드셨네요",
          "original_form": "오 얼큰한거드셨네요",
          "speaker_id": "2",
          "time": "20210828 04:12"
        },
        {
          "id": "MDRW2100034396.1.6",
          "form": "떡볶이맛있어요",
          "original_form": "떡볶이맛있어요",
          "speaker_id": "2",
          "time": "20210828 04:12"
        },
        {
          "id": "MDRW2100034396.1.7",
          "form": "둘다 매운거네요ㅋㅋㅋ 전 로제떡볶이가 맛있더라구용",
          "original_form": "둘다 매운거네요ㅋㅋㅋ 전 로제떡볶이가 맛있더라구용",
          "speaker_id": "1",
          "time": "20210828 04:13"
        },
        {
          "id": "MDRW2100034396.1.8",
          "form": "그거먹은거 같아요 청년다방에서",
          "original_form": "그거먹은거 같아요 청년다방에서",
          "speaker_id": "2",
          "time": "20210828 04:13"
        },
        {
          "id": "MDRW2100034396.1.9",
          "form": "아 청년다방! 거기 맛있죵ㅋㅋ",
          "original_form": "아 청년다방! 거기 맛있죵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 04:13"
        },
        {
          "id": "MDRW2100034396.1.10",
          "form": "저도 먹어봤어요",
          "original_form": "저도 먹어봤어요",
          "speaker_id": "1",
          "time": "20210828 04:13"
        },
        {
          "id": "MDRW2100034396.1.11",
          "form": "네 머드셔보셨나요",
          "original_form": "네 머드셔보셨나요",
          "speaker_id": "2",
          "time": "20210828 04:14"
        },
        {
          "id": "MDRW2100034396.1.12",
          "form": "제일 대표메뉴가 차돌인것같더라구요",
          "original_form": "제일 대표메뉴가 차돌인것같더라구요",
          "speaker_id": "1",
          "time": "20210828 04:14"
        },
        {
          "id": "MDRW2100034396.1.13",
          "form": "그래서 고기들어간 떡볶이는 처음 먹어봤는데",
          "original_form": "그래서 고기들어간 떡볶이는 처음 먹어봤는데",
          "speaker_id": "1",
          "time": "20210828 04:14"
        },
        {
          "id": "MDRW2100034396.1.14",
          "form": "맛있었어요ㅋㅋ",
          "original_form": "맛있었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 04:15"
        },
        {
          "id": "MDRW2100034396.1.15",
          "form": "전오늘 차돌 박이 로제 먹었어요",
          "original_form": "전오늘 차돌 박이 로제 먹었어요",
          "speaker_id": "2",
          "time": "20210828 04:15"
        },
        {
          "id": "MDRW2100034396.1.16",
          "form": "왜나쁜말이지 이게",
          "original_form": "왜나쁜말이지 이게",
          "speaker_id": "2",
          "time": "20210828 04:16"
        },
        {
          "id": "MDRW2100034396.1.17",
          "form": "아 저한텐 나쁜말 하는거",
          "original_form": "아 저한텐 나쁜말 하는거",
          "speaker_id": "1",
          "time": "20210828 04:16"
        },
        {
          "id": "MDRW2100034396.1.18",
          "form": "안떠용!",
          "original_form": "안떠용!",
          "speaker_id": "1",
          "time": "20210828 04:16"
        },
        {
          "id": "MDRW2100034396.1.19",
          "form": "그 안내문구? 그런거",
          "original_form": "그 안내문구? 그런거",
          "speaker_id": "1",
          "time": "20210828 04:16"
        },
        {
          "id": "MDRW2100034396.1.20",
          "form": "차돌박이",
          "original_form": "차돌박이",
          "speaker_id": "1",
          "time": "20210828 04:16"
        },
        {
          "id": "MDRW2100034396.1.21",
          "form": "로제버전도 있나요??",
          "original_form": "로제버전도 있나요??",
          "speaker_id": "1",
          "time": "20210828 04:16"
        },
        {
          "id": "MDRW2100034396.1.22",
          "form": "그죠 차돌박이 맛있구 오징어도 맛있더라구요",
          "original_form": "그죠 차돌박이 맛있구 오징어도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 04:16"
        },
        {
          "id": "MDRW2100034396.1.23",
          "form": "네 새로나왓나봐요",
          "original_form": "네 새로나왓나봐요",
          "speaker_id": "2",
          "time": "20210828 04:16"
        },
        {
          "id": "MDRW2100034396.1.24",
          "form": "오호 제가 또 신상 이런건 못참아서",
          "original_form": "오호 제가 또 신상 이런건 못참아서",
          "speaker_id": "1",
          "time": "20210828 04:16"
        },
        {
          "id": "MDRW2100034396.1.25",
          "form": "감사해요ㅋㅋ",
          "original_form": "감사해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 04:16"
        }
      ]
    }
  ]
