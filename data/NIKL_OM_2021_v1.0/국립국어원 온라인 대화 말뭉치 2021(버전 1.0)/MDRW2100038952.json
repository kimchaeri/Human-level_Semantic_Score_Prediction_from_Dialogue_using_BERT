      "utterance": [
        {
          "id": "MDRW2100038952.1.1",
          "form": "내일 아침엔 오늘 동생이 남긴 치킨을 먹어봅니다",
          "original_form": "내일 아침엔 오늘 동생이 남긴 치킨을 먹어봅니다",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100038952.1.2",
          "form": "우와 맛잇겠어요 밥조금이랑 먹음 딱이겠네요",
          "original_form": "우와 맛잇겠어요 밥조금이랑 먹음 딱이겠네요",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038952.1.3",
          "form": "하지만 피티샘이 본다면 튀김옷이 탄수화물이니 치킨만 먹으라고 할것같은",
          "original_form": "하지만 피티샘이 본다면 튀김옷이 탄수화물이니 치킨만 먹으라고 할것같은",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100038952.1.4",
          "form": "뷸길한 예감이 듭니다",
          "original_form": "뷸길한 예감이 듭니다",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100038952.1.5",
          "form": "ㅠㅠ 그럼 무슨맛이죠,,",
          "original_form": "ㅠㅠ 그럼 무슨맛이죠,,",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038952.1.6",
          "form": "오늘은 샤뷰샤브 먹는 날임미다",
          "original_form": "오늘은 샤뷰샤브 먹는 날임미다",
          "speaker_id": "2",
          "time": "20210903 09:16"
        },
        {
          "id": "MDRW2100038952.1.7",
          "form": "놈 신나요",
          "original_form": "놈 신나요",
          "speaker_id": "2",
          "time": "20210903 09:16"
        },
        {
          "id": "MDRW2100038952.1.8",
          "form": "맛있게 드시고오세요!",
          "original_form": "맛있게 드시고오세요!",
          "speaker_id": "1",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100038952.1.9",
          "form": "앗 알고보니 오늘..",
          "original_form": "앗 알고보니 오늘..",
          "speaker_id": "2",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100038952.1.10",
          "form": "아니규 댜음쥬 약속이었던고 실화입니갸",
          "original_form": "아니규 댜음쥬 약속이었던고 실화입니갸",
          "speaker_id": "2",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100038952.1.11",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100038952.1.12",
          "form": "대신 점심 맛있는거 먹으러 나가요",
          "original_form": "대신 점심 맛있는거 먹으러 나가요",
          "speaker_id": "2",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100038952.1.13",
          "form": "맛점하시고 오세요!ㅋㅋ고기나 생선으로요",
          "original_form": "맛점하시고 오세요!ㅋㅋ고기나 생선으로요",
          "speaker_id": "1",
          "time": "20210903 12:49"
        },
        {
          "id": "MDRW2100038952.1.14",
          "form": "저!! 완전 파스타랑 피자",
          "original_form": "저!! 완전 파스타랑 피자",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100038952.1.15",
          "form": "많이 먹고왔슴다..",
          "original_form": "많이 먹고왔슴다..",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100038952.1.16",
          "form": "약속이 있었는데요..",
          "original_form": "약속이 있었는데요..",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100038952.1.17",
          "form": "없었습니다..",
          "original_form": "없었습니다..",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100038952.1.18",
          "form": "라고 했더니 같이 일하시는분이 거하게 먹여주셨어요",
          "original_form": "라고 했더니 같이 일하시는분이 거하게 먹여주셨어요",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100038952.1.19",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100038952.1.20",
          "form": "운이 좋았습니당",
          "original_form": "운이 좋았습니당",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100038952.1.21",
          "form": "곧 휴",
          "original_form": "곧 휴",
          "speaker_id": "1",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100038952.1.22",
          "form": "바사삭",
          "original_form": "바사삭",
          "speaker_id": "1",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100038952.1.23",
          "form": "치킨 기프티콘이 생겨서 이걸 먹을거에요",
          "original_form": "치킨 기프티콘이 생겨서 이걸 먹을거에요",
          "speaker_id": "1",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100038952.1.24",
          "form": "누가 이름을 지었는지 참 웃기죸",
          "original_form": "누가 이름을 지었는지 참 웃기죸",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100038952.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ앗 바사삭 쿠폰..!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ앗 바사삭 쿠폰..!!",
          "speaker_id": "2",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100038952.1.26",
          "form": "축하드려여!!!",
          "original_form": "축하드려여!!!",
          "speaker_id": "2",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100038952.1.27",
          "form": "저 최애치킨입니당",
          "original_form": "저 최애치킨입니당",
          "speaker_id": "2",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100038952.1.28",
          "form": "name2님도 금방 들거에요",
          "original_form": "&name2&님도 금방 들거에요",
          "speaker_id": "1",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100038952.1.29",
          "form": "그러기위해 저도 같이 열심히 얘기를 ㅋㅋㅋ",
          "original_form": "그러기위해 저도 같이 열심히 얘기를 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100038952.1.30",
          "form": "전 고블링소스가 맛있더군요",
          "original_form": "전 고블링소스가 맛있더군요",
          "speaker_id": "1",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100038952.1.31",
          "form": "오!!",
          "original_form": "오!!",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100038952.1.32",
          "form": "저도 고블링소스 좋아해요",
          "original_form": "저도 고블링소스 좋아해요",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100038952.1.33",
          "form": "완전 맛있어요",
          "original_form": "완전 맛있어요",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100038952.1.34",
          "form": "오늘 저녁은 전.. 물로 대신합니다..",
          "original_form": "오늘 저녁은 전.. 물로 대신합니다..",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100038952.1.35",
          "form": "점심에 넘 망ㅎ이 먹었나봐요..",
          "original_form": "점심에 넘 망ㅎ이 먹었나봐요..",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100038952.1.36",
          "form": "아직도 배가 빵빵해여",
          "original_form": "아직도 배가 빵빵해여",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100038952.1.37",
          "form": "물드시지마시구 늦게라도 과일이라도 드세용ㅋㅋㅋ",
