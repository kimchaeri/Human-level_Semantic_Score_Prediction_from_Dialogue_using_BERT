      "utterance": [
        {
          "id": "MDRW2100042334.1.1",
          "form": "넷플릭스 보세용??",
          "original_form": "넷플릭스 보세용??",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042334.1.2",
          "form": "앗 아뇨!",
          "original_form": "앗 아뇨!",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042334.1.3",
          "form": "넷플릭스 좋아하세요?",
          "original_form": "넷플릭스 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042334.1.4",
          "form": "저는 넷플 드라마가 재밋드라구요 ㅋㅋ키",
          "original_form": "저는 넷플 드라마가 재밋드라구요 ㅋㅋ키",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042334.1.5",
          "form": "킹덤 아세요??",
          "original_form": "킹덤 아세요??",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042334.1.6",
          "form": "아 네!! 얘기는 많이 들었어요!!",
          "original_form": "아 네!! 얘기는 많이 들었어요!!",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042334.1.7",
          "form": "킹덤재밌게보셨어요?",
          "original_form": "킹덤재밌게보셨어요?",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042334.1.8",
          "form": "네네 너무 재밋었어욬ㅋㅋ",
          "original_form": "네네 너무 재밋었어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042334.1.9",
          "form": "킹덤 볼려고 넷플 결제햇어요 ㅋㅋㅋ",
          "original_form": "킹덤 볼려고 넷플 결제햇어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042334.1.10",
          "form": "드라마 챙겨보고 있는거 있으세요?",
          "original_form": "드라마 챙겨보고 있는거 있으세요?",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042334.1.11",
          "form": "저는 펜트하우스만 보고있어요!",
          "original_form": "저는 펜트하우스만 보고있어요!",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042334.1.12",
          "form": "막 찾아보는 스타일이 아니라",
          "original_form": "막 찾아보는 스타일이 아니라",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042334.1.13",
          "form": "집에서 언니가 틀어주면 보거든여 흑흑..",
          "original_form": "집에서 언니가 틀어주면 보거든여 흑흑..",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042334.1.14",
          "form": "와 펜트하우스 !!",
          "original_form": "와 펜트하우스 !!",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042334.1.15",
          "form": "이번주가 마지막이죠??",
          "original_form": "이번주가 마지막이죠??",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042334.1.16",
          "form": "네 이번쥬가 마지막화래요..!",
          "original_form": "네 이번쥬가 마지막화래요..!",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042334.1.17",
          "form": "약간.. 후련해요 제가 다..",
          "original_form": "약간.. 후련해요 제가 다..",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042334.1.18",
          "form": "완전 지금 뇌절 직전이라..",
          "original_form": "완전 지금 뇌절 직전이라..",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042334.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ와 이제 누가 죽어도 안놀라워요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ와 이제 누가 죽어도 안놀라워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042334.1.20",
          "form": "맞아여 어케아셨어요!",
          "original_form": "맞아여 어케아셨어요!",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042334.1.21",
          "form": "name1님도 보다가 하차하셨너요?",
          "original_form": "&name1&님도 보다가 하차하셨너요?",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042334.1.22",
          "form": "저는 꾸역꾸역 보고있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 꾸역꾸역 보고있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042334.1.23",
          "form": "금요일에 볼게 없어서 ㅠㅠ",
          "original_form": "금요일에 볼게 없어서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042334.1.24",
          "form": "헐 저도 어쩔수넚이 보는거죠 뭐..",
          "original_form": "헐 저도 어쩔수넚이 보는거죠 뭐..",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042334.1.25",
          "form": "하박사랑 이지아 죽는거까지 버셨나요?",
          "original_form": "하박사랑 이지아 죽는거까지 버셨나요?",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042334.1.26",
          "form": "헉 맞아요 마지막에 이지아 죽어ㅛ죠!!!",
          "original_form": "헉 맞아요 마지막에 이지아 죽어ㅛ죠!!!",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042334.1.27",
          "form": "이지아는 다시 살아날거같아요 ... ㅋㅋㅋ",
          "original_form": "이지아는 다시 살아날거같아요 ... ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042334.1.28",
          "form": "아니 저도 그렇게 생각했는데",
          "original_form": "아니 저도 그렇게 생각했는데",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042334.1.29",
          "form": "덩생이 같이 보다가 로건리가 죽어서 천국가서 만난거 아니냐소",
          "original_form": "덩생이 같이 보다가 로건리가 죽어서 천국가서 만난거 아니냐소",
