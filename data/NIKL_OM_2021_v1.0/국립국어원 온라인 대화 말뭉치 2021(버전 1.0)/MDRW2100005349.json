        {
          "id": "MDRW2100005349.1.1",
          "form": "맞다 저때 얼굴이 지금 얼굴 ㅋㅋ",
          "original_form": "맞다 저때 얼굴이 지금 얼굴 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005349.1.2",
          "form": "백종원 봐라 예전이 더 늙어보임ㅋ",
          "original_form": "백종원 봐라 예전이 더 늙어보임ㅋ",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005349.1.3",
          "form": "엉 지금이 훨 낫더라",
          "original_form": "엉 지금이 훨 낫더라",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005349.1.4",
          "form": "그니깐 무슨 20대도 아저씨야",
          "original_form": "그니깐 무슨 20대도 아저씨야",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005349.1.5",
          "form": "소유진이 관리 좀 해주겠지",
          "original_form": "소유진이 관리 좀 해주겠지",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005349.1.6",
          "form": "처음에 소유진 돈보고 결혼 했다고 했다가 지금은 결혼할만 하다고ㅋ",
          "original_form": "처음에 소유진 돈보고 결혼 했다고 했다가 지금은 결혼할만 하다고ㅋ",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005349.1.7",
          "form": "난 차달남 보기로 함 채널고정",
          "original_form": "난 차달남 보기로 함 채널고정",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005349.1.8",
          "form": "오늘 여러편 하네",
          "original_form": "오늘 여러편 하네",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005349.1.9",
          "form": "그러게 채널은 다르다 kbs 드라마",
          "original_form": "그러게 채널은 다르다 kbs 드라마",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005349.1.10",
          "form": "백종원 광고 나옴 ㅋㅋ",
          "original_form": "백종원 광고 나옴 ㅋㅋ",
