      },
      "utterance": [
        {
          "id": "MDRW2100032275.1.1",
          "form": "아 뭐라 말해야하지",
          "original_form": "아 뭐라 말해야하지",
          "speaker_id": "1",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032275.1.2",
          "form": "비가 참 많이 내린다..",
          "original_form": "비가 참 많이 내린다..",
          "speaker_id": "1",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032275.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032275.1.4",
          "form": "뭐야 비온다고?",
          "original_form": "뭐야 비온다고?",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032275.1.5",
          "form": "응 지금 비오는데",
          "original_form": "응 지금 비오는데",
          "speaker_id": "1",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032275.1.6",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032275.1.7",
          "form": "야긴 안왔으먄 좋겟내",
          "original_form": "야긴 안왔으먄 좋겟내",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032275.1.8",
          "form": "우산없어",
          "original_form": "우산없어",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032275.1.9",
          "form": "머야 진주는 다 오는거",
          "original_form": "머야 진주는 다 오는거",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032275.1.10",
          "form": "아님?",
          "original_form": "아님?",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032275.1.11",
          "form": "나는 할모니집이야",
          "original_form": "나는 할모니집이야",
          "speaker_id": "2",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100032275.1.12",
          "form": "그리고 밖애 안 보고 잇어서 잘 몰겟다",
          "original_form": "그리고 밖애 안 보고 잇어서 잘 몰겟다",
          "speaker_id": "2",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100032275.1.13",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "1",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100032275.1.14",
          "form": "난 지금 부산 가려고 밖에 나왔어",
          "original_form": "난 지금 부산 가려고 밖에 나왔어",
          "speaker_id": "1",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100032275.1.15",
          "form": "버스 기다리는 중",
          "original_form": "버스 기다리는 중",
          "speaker_id": "1",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100032275.1.16",
          "form": "아맞네 학교가여하는구나",
          "original_form": "아맞네 학교가여하는구나",
          "speaker_id": "2",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100032275.1.17",
          "form": "화이팅",
          "original_form": "화이팅",
          "speaker_id": "2",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100032275.1.18",
          "form": "지금 비 안오면 좋앗을텐데",
          "original_form": "지금 비 안오면 좋앗을텐데",
          "speaker_id": "2",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100032275.1.19",
          "form": "구러게",
          "original_form": "구러게",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100032275.1.20",
          "form": "지금음 버스 탔다",
          "original_form": "지금음 버스 탔다",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100032275.1.21",
          "form": "버스에서 아련하게 창빆보며 비구경하기",
          "original_form": "버스에서 아련하게 창빆보며 비구경하기",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032275.1.22",
          "form": "농담이고 자연 수면음악이네 꿀잠각이네",
          "original_form": "농담이고 자연 수면음악이네 꿀잠각이네",
          "speaker_id": "2",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032275.1.23",
          "form": "아냐 나에겐 이어폰이 있어",
          "original_form": "아냐 나에겐 이어폰이 있어",
          "speaker_id": "1",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032275.1.24",
          "form": "ㅋㅋ그렇겠네",
          "original_form": "ㅋㅋ그렇겠네",
          "speaker_id": "2",
          "time": "20210905 15:00"
        },
        {
          "id": "MDRW2100032275.1.25",
          "form": "음악소리 들으면서 비구경하기",
          "original_form": "음악소리 들으면서 비구경하기",
          "speaker_id": "2",
          "time": "20210905 15:00"
        },
        {
          "id": "MDRW2100032275.1.26",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "1",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100032275.1.27",
          "form": "사실 이제 비 안내리는 듯?",
          "original_form": "사실 이제 비 안내리는 듯?",
          "speaker_id": "1",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100032275.1.28",
          "form": "글쿠만 지나가는 비엿나보네",
          "original_form": "글쿠만 지나가는 비엿나보네",
          "speaker_id": "2",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100032275.1.29",
          "form": "내리고나서가 중요하겠지만",
          "original_form": "내리고나서가 중요하겠지만",
          "speaker_id": "2",
          "time": "20210905 15:02"
        }
      ]
    }
  ]
