      "utterance": [
        {
          "id": "MDRW2100009671.1.1",
          "form": "와!! 추천",
          "original_form": "와!! 추천",
          "speaker_id": "2",
          "time": "20210823 02:51"
        },
        {
          "id": "MDRW2100009671.1.2",
          "form": "김과외 어플 설치 안하셨으면",
          "original_form": "김과외 어플 설치 안하셨으면",
          "speaker_id": "1",
          "time": "20210823 02:51"
        },
        {
          "id": "MDRW2100009671.1.3",
          "form": "김과외 어플 설치하시면 과외 구하는데 도움도 되고",
          "original_form": "김과외 어플 설치하시면 과외 구하는데 도움도 되고",
          "speaker_id": "1",
          "time": "20210823 02:51"
        },
        {
          "id": "MDRW2100009671.1.4",
          "form": "저 이번 거 김과외로 구했어요!!",
          "original_form": "저 이번 거 김과외로 구했어요!!",
          "speaker_id": "2",
          "time": "20210823 02:51"
        },
        {
          "id": "MDRW2100009671.1.5",
          "form": "거기 선생님끼리 게시판이 좋아요",
          "original_form": "거기 선생님끼리 게시판이 좋아요",
          "speaker_id": "1",
          "time": "20210823 02:51"
        },
        {
          "id": "MDRW2100009671.1.6",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "2",
          "time": "20210823 02:52"
        },
        {
          "id": "MDRW2100009671.1.7",
          "form": "네네 선생님끼리 게시판 참고하세여",
          "original_form": "네네 선생님끼리 게시판 참고하세여",
          "speaker_id": "1",
          "time": "20210823 02:52"
        },
        {
          "id": "MDRW2100009671.1.8",
          "form": "거기는 안들어가봤는데",
          "original_form": "거기는 안들어가봤는데",
          "speaker_id": "2",
          "time": "20210823 02:52"
        },
        {
          "id": "MDRW2100009671.1.9",
          "form": "자료가 다양하게 있어요",
          "original_form": "자료가 다양하게 있어요",
          "speaker_id": "1",
          "time": "20210823 02:52"
        },
        {
          "id": "MDRW2100009671.1.10",
          "form": "꼭 들어가보세요",
          "original_form": "꼭 들어가보세요",
          "speaker_id": "1",
          "time": "20210823 02:52"
        },
        {
          "id": "MDRW2100009671.1.11",
          "form": "네!! 꼭 가볼게요",
          "original_form": "네!! 꼭 가볼게요",
          "speaker_id": "2",
          "time": "20210823 02:52"
        },
        {
          "id": "MDRW2100009671.1.12",
          "form": "처음에는 막 걱정이 되지만",
          "original_form": "처음에는 막 걱정이 되지만",
          "speaker_id": "1",
          "time": "20210823 02:53"
        },
        {
          "id": "MDRW2100009671.1.13",
          "form": "곧 무념무상하게 될거에요",
          "original_form": "곧 무념무상하게 될거에요",
          "speaker_id": "1",
          "time": "20210823 02:53"
        },
        {
          "id": "MDRW2100009671.1.14",
          "form": "그렇겠죠?... 처음은 모든 게 어려운 법이라지만",
          "original_form": "그렇겠죠?... 처음은 모든 게 어려운 법이라지만",
          "speaker_id": "2",
          "time": "20210823 02:53"
        },
        {
          "id": "MDRW2100009671.1.15",
          "form": "과외는 알바보다 훨 심한 거 같어요",
          "original_form": "과외는 알바보다 훨 심한 거 같어요",
          "speaker_id": "2",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009671.1.16",
          "form": "막 걱정되시죠",
          "original_form": "막 걱정되시죠",
          "speaker_id": "1",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009671.1.17",
          "form": "네... ㅠㅠ",
          "original_form": "네... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 02:55"
        },
        {
          "id": "MDRW2100009671.1.18",
          "form": "레벨테스트도 어떻게 하나 싶고",
          "original_form": "레벨테스트도 어떻게 하나 싶고",
          "speaker_id": "2",
          "time": "20210823 02:55"
        },
        {
          "id": "MDRW2100009671.1.19",
          "form": "저는 모의고사 약간 뽑아서 주고",
          "original_form": "저는 모의고사 약간 뽑아서 주고",
          "speaker_id": "1",
          "time": "20210823 02:55"
        },
        {
          "id": "MDRW2100009671.1.20",
          "form": "지금이라도 고1 인강 들어야되나 싶어요",
          "original_form": "지금이라도 고1 인강 들어야되나 싶어요",
          "speaker_id": "2",
          "time": "20210823 02:55"
        },
        {
          "id": "MDRW2100009671.1.21",
          "form": "그냥 학부모랑 상담 해요",
          "original_form": "그냥 학부모랑 상담 해요",
          "speaker_id": "1",
          "time": "20210823 02:55"
        },
        {
          "id": "MDRW2100009671.1.22",
          "form": "학부모의 마음을 얻는게 가장 중요하답니다",
          "original_form": "학부모의 마음을 얻는게 가장 중요하답니다",
          "speaker_id": "1",
          "time": "20210823 02:55"
        },
        {
          "id": "MDRW2100009671.1.23",
          "form": "첫 수업은 주로 그런 식인가요?",
          "original_form": "첫 수업은 주로 그런 식인가요?",
          "speaker_id": "2",
          "time": "20210823 02:55"
        },
        {
          "id": "MDRW2100009671.1.24",
          "form": "ㅠㅠ... 학부모님",
          "original_form": "ㅠㅠ... 학부모님",
