      "utterance": [
        {
          "id": "MDRW2100007338.1.1",
          "form": "백신 신청하셨어요?",
          "original_form": "백신 신청하셨어요?",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007338.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007338.1.3",
          "form": "그거 맞으면 며칠 아프대서 별로맞고싶은 기분은 안들어요 ㅎㅎ..",
          "original_form": "그거 맞으면 며칠 아프대서 별로맞고싶은 기분은 안들어요 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007338.1.4",
          "form": "저도 우선 신청만했어쇼",
          "original_form": "저도 우선 신청만했어쇼",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007338.1.5",
          "form": "신청도 아예안하신거예요?",
          "original_form": "신청도 아예안하신거예요?",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007338.1.6",
          "form": "네 ㅎ..",
          "original_form": "네 ㅎ..",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007338.1.7",
          "form": "그래도 해놓고 고민해보시지ㅎ",
          "original_form": "그래도 해놓고 고민해보시지ㅎ",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007338.1.8",
          "form": "에이뭐 밖에잘 나가지도않는데",
          "original_form": "에이뭐 밖에잘 나가지도않는데",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007338.1.9",
          "form": "생각없어요 ㅋㅋㅋㅋ",
          "original_form": "생각없어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007338.1.10",
          "form": "ㅎㅎ 젊은 친구들이 많이 안맞나봐요",
          "original_form": "ㅎㅎ 젊은 친구들이 많이 안맞나봐요",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007338.1.11",
          "form": "모르겠어요 맞았다는친구도있으니까 케바케일듯",
          "original_form": "모르겠어요 맞았다는친구도있으니까 케바케일듯",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007338.1.12",
          "form": "맞은친구들은 별이상없대요?",
          "original_form": "맞은친구들은 별이상없대요?",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007338.1.13",
          "form": "1차만 맞아서그런지 괜찮다네요",
          "original_form": "1차만 맞아서그런지 괜찮다네요",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007338.1.14",
          "form": "화이자요?",
          "original_form": "화이자요?",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007338.1.15",
          "form": "그럴걸요?",
          "original_form": "그럴걸요?",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007338.1.16",
          "form": "정확히모르겠네요",
          "original_form": "정확히모르겠네요",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007338.1.17",
          "form": "나이 어린데 잔여백신으로 맞은건가요?",
          "original_form": "나이 어린데 잔여백신으로 맞은건가요?",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007338.1.18",
          "form": "글쎄요..간호사 하다가 관둔친구라",
          "original_form": "글쎄요..간호사 하다가 관둔친구라",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007338.1.19",
          "form": "아 그럼 관계자로 맞은건가보네요",
          "original_form": "아 그럼 관계자로 맞은건가보네요",
          "speaker_id": "1",
