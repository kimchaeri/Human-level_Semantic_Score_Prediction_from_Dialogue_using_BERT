        },
        {
          "id": "MDRW2100039181.1.5",
          "form": "그거샀음?",
          "original_form": "그거샀음?",
          "speaker_id": "1",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100039181.1.6",
          "form": "ㅇㅇ 그거하고 혹시",
          "original_form": "ㅇㅇ 그거하고 혹시",
          "speaker_id": "2",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100039181.1.7",
          "form": "심장에 이상 생길까봐 뭐시기 약도 삼",
          "original_form": "심장에 이상 생길까봐 뭐시기 약도 삼",
          "speaker_id": "2",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100039181.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100039181.1.9",
          "form": "심장에이상생기는것ㄷㅎ있음,?",
          "original_form": "심장에이상생기는것ㄷㅎ있음,?",
          "speaker_id": "1",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100039181.1.10",
          "form": "젊은 애들이 맞고 심장에 이상생긴경우가 있대",
          "original_form": "젊은 애들이 맞고 심장에 이상생긴경우가 있대",
          "speaker_id": "2",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100039181.1.11",
          "form": "그래서 일단 사 놓음",
          "original_form": "그래서 일단 사 놓음",
          "speaker_id": "2",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100039181.1.12",
          "form": "내 심장 소중하니까",
          "original_form": "내 심장 소중하니까",
          "speaker_id": "2",
          "time": "20210903 13:02"
