      "utterance": [
        {
          "id": "MDRW2100020780.1.1",
          "form": "요즘 ㄷ",
          "original_form": "요즘 ㄷ",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020780.1.2",
          "form": "요즘 듣는 노래 있으신가요?",
          "original_form": "요즘 듣는 노래 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020780.1.3",
          "form": "저 요즘은 슬의생 ost 위주로 듣고 있어요~~ name1님은요?",
          "original_form": "저 요즘은 슬의생 ost 위주로 듣고 있어요~~ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020780.1.4",
          "form": "전 듣던 노래만 계속 듣네요 고백 리메이크 노래 듣고있어요",
          "original_form": "전 듣던 노래만 계속 듣네요 고백 리메이크 노래 듣고있어요",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020780.1.5",
          "form": "슬의생 시즌2 보시는 거예요?",
          "original_form": "슬의생 시즌2 보시는 거예요?",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020780.1.6",
          "form": "아하 네! 슬의생 시즌2 보고있어요",
          "original_form": "아하 네! 슬의생 시즌2 보고있어요",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020780.1.7",
          "form": "슬의생 보시나요?",
          "original_form": "슬의생 보시나요?",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020780.1.8",
          "form": "저 시즌1은 진짜 재밌게 봤어요!ㅋㅋ",
          "original_form": "저 시즌1은 진짜 재밌게 봤어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020780.1.9",
          "form": "근데 시즌2는 넷플 결제를 안 해서ㅋㅋ",
          "original_form": "근데 시즌2는 넷플 결제를 안 해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020780.1.10",
          "form": "미루다 안 봐졌어요",
          "original_form": "미루다 안 봐졌어요",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020780.1.11",
          "form": "시즌2도 여전히 재밌나요?",
          "original_form": "시즌2도 여전히 재밌나요?",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020780.1.12",
          "form": "아하! 네!! 저는 재밌긴한데",
          "original_form": "아하! 네!! 저는 재밌긴한데",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020780.1.13",
          "form": "보시는 분들중에서 별로라는 분들이 생각보다 많더라고요?!?!?!",
          "original_form": "보시는 분들중에서 별로라는 분들이 생각보다 많더라고요?!?!?!",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020780.1.14",
          "form": "좀 불호가 갈리나봐요 ㅎㅎ",
          "original_form": "좀 불호가 갈리나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020780.1.15",
          "form": "앗 그래요",
          "original_form": "앗 그래요",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020780.1.16",
          "form": "최애 커플은 누구예요?ㅋㅋ",
          "original_form": "최애 커플은 누구예요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020780.1.17",
          "form": "제 최애커플은 익순준완이에요 name1님은요 있나요?!",
          "original_form": "제 최애커플은 익순준완이에요 &name1&님은요 있나요?!",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020780.1.18",
          "form": "전 겨울정원이요!ㅋㅋㅋ",
          "original_form": "전 겨울정원이요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020780.1.19",
          "form": "익순이랑 준완이 여전히 잘 만나나요?ㅋㅋ",
          "original_form": "익순이랑 준완이 여전히 잘 만나나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020780.1.20",
          "form": "그....그들은...헤어졌습니다...",
          "original_form": "그....그들은...헤어졌습니다...",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020780.1.21",
          "form": "겨울정원은 잘 만나는데 익순이와 준완이는...",
          "original_form": "겨울정원은 잘 만나는데 익순이와 준완이는...",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020780.1.22",
          "form": "ㅜㅜㅋㅋ",
          "original_form": "ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020780.1.23",
          "form": "헝",
          "original_form": "헝",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020780.1.24",
          "form": "흐엉 ㅠㅠ",
          "original_form": "흐엉 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020780.1.25",
          "form": "ㅋㅋㅋ 이게 무슨 일이에요",
          "original_form": "ㅋㅋㅋ 이게 무슨 일이에요",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020780.1.26",
          "form": "ㅋㅋㅋ 익준이랑 송화는요?",
          "original_form": "ㅋㅋㅋ 익준이랑 송화는요?",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020780.1.27",
          "form": "안 이어졌습니다.......",
          "original_form": "안 이어졌습니다.......",
          "speaker_id": "2",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100020780.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아놔 겨울정원만 잘 만나고 있네요ㅜㅜㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아놔 겨울정원만 잘 만나고 있네요ㅜㅜㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100020780.1.29",
          "form": "익준송화 결론이 났어요...?",
          "original_form": "익준송화 결론이 났어요...?",
          "speaker_id": "1",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100020780.1.30",
          "form": "애매하고 그런 것도 아니고 그냥 친구...?",
          "original_form": "애매하고 그런 것도 아니고 그냥 친구...?",
          "speaker_id": "1",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100020780.1.31",
          "form": "그게 좀 애매한게 처음엔 송화쪽에서 거절을 했거든요 그때 그 시즌1 고백에 대한 대답이요!",
          "original_form": "그게 좀 애매한게 처음엔 송화쪽에서 거절을 했거든요 그때 그 시즌1 고백에 대한 대답이요!",
          "speaker_id": "2",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020780.1.32",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020780.1.33",
          "form": "근데 중간에 친구와 연애를 하는 사람이 송화한테 무슨 말을 해줬더니 송화 마음이 좀 변한 느낌이었어요!",
          "original_form": "근데 중간에 친구와 연애를 하는 사람이 송화한테 무슨 말을 해줬더니 송화 마음이 좀 변한 느낌이었어요!",
