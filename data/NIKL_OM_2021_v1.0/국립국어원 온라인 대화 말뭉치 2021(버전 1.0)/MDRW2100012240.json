      "utterance": [
        {
          "id": "MDRW2100012240.1.1",
          "form": "너 머리스타일 바꾼다며?",
          "original_form": "너 머리스타일 바꾼다며?",
          "speaker_id": "1",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100012240.1.2",
          "form": "응응!이번주에 미용실 갈꺼야",
          "original_form": "응응!이번주에 미용실 갈꺼야",
          "speaker_id": "2",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100012240.1.3",
          "form": "파마하는거야?",
          "original_form": "파마하는거야?",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012240.1.4",
          "form": "아니 염색하려구ㅎㅎ",
          "original_form": "아니 염색하려구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012240.1.5",
          "form": "오 무슨색으로?",
          "original_form": "오 무슨색으로?",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012240.1.6",
          "form": "애쉬 카키 브라운!",
          "original_form": "애쉬 카키 브라운!",
          "speaker_id": "2",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012240.1.7",
          "form": "오잉 그게 하나의 색상이야?",
          "original_form": "오잉 그게 하나의 색상이야?",
          "speaker_id": "1",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012240.1.8",
          "form": "응 갈색인데 주황끼 없이 회색이 섞였단다",
          "original_form": "응 갈색인데 주황끼 없이 회색이 섞였단다",
          "speaker_id": "2",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012240.1.9",
          "form": "우와 신기하다. 비용은 얼마나 들어?",
          "original_form": "우와 신기하다. 비용은 얼마나 들어?",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012240.1.10",
          "form": "흠 17만원에다가 기장추가 들어갈 것 같다ㅠ",
          "original_form": "흠 17만원에다가 기장추가 들어갈 것 같다ㅠ",
          "speaker_id": "2",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012240.1.11",
          "form": "헐 엄청 비싸다.",
          "original_form": "헐 엄청 비싸다.",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012240.1.12",
          "form": "셀프로는 왜 안해?",
          "original_form": "셀프로는 왜 안해?",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012240.1.13",
          "form": "아 내가 똥손이라 손으로 하는 건 잘 못해ㅠ",
          "original_form": "아 내가 똥손이라 손으로 하는 건 잘 못해ㅠ",
          "speaker_id": "2",
          "time": "20210824 20:40"
        },
        {
          "id": "MDRW2100012240.1.14",
          "form": "기장추가는 뭐야?",
          "original_form": "기장추가는 뭐야?",
          "speaker_id": "1",
          "time": "20210824 20:40"
        },
        {
          "id": "MDRW2100012240.1.15",
          "form": "내가 머리가 길어서 추가요금이 붙는거야!",
          "original_form": "내가 머리가 길어서 추가요금이 붙는거야!",
          "speaker_id": "2",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100012240.1.16",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100012240.1.17",
          "form": "말랭이 다이어트 해?",
          "original_form": "말랭이 다이어트 해?",
          "speaker_id": "2",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100012240.1.18",
          "form": "당연히 하지. 무려 8키로나 뺏다고.",
          "original_form": "당연히 하지. 무려 8키로나 뺏다고.",
          "speaker_id": "1",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100012240.1.19",
          "form": "???8키로?대단하다!!",
          "original_form": "???8키로?대단하다!!",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100012240.1.20",
          "form": "너는 다이어트해?",
          "original_form": "너는 다이어트해?",
          "speaker_id": "1",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100012240.1.21",
          "form": "난 항상 실패해ㅜ",
          "original_form": "난 항상 실패해ㅜ",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100012240.1.22",
          "form": "왜? 먹는 것 때문에?",
          "original_form": "왜? 먹는 것 때문에?",
          "speaker_id": "1",
          "time": "20210824 20:43"
        },
        {
          "id": "MDRW2100012240.1.23",
          "form": "응응 야식을 많이 먹어서 조절이 안돼",
          "original_form": "응응 야식을 많이 먹어서 조절이 안돼",
          "speaker_id": "2",
          "time": "20210824 20:43"
        },
        {
          "id": "MDRW2100012240.1.24",
          "form": "다이어트할 때 야식은 먹으면 안돼",
          "original_form": "다이어트할 때 야식은 먹으면 안돼",
          "speaker_id": "1",
          "time": "20210824 20:43"
        }
      ]
    }
