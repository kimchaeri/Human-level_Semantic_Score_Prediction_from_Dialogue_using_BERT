      "utterance": [
        {
          "id": "MDRW2100041224.1.1",
          "form": "날씨에 이어",
          "original_form": "날씨에 이어",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041224.1.2",
          "form": "기온이낮아요 가을옷입기좋을거같아용",
          "original_form": "기온이낮아요 가을옷입기좋을거같아용",
          "speaker_id": "2",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041224.1.3",
          "form": "새로산옷들을 슬슬 개시해봐요",
          "original_form": "새로산옷들을 슬슬 개시해봐요",
          "speaker_id": "2",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041224.1.4",
          "form": "오늘 셔츠원피스 샀어용",
          "original_form": "오늘 셔츠원피스 샀어용",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041224.1.5",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041224.1.6",
          "form": "셔츠원피스짱이뿐데",
          "original_form": "셔츠원피스짱이뿐데",
          "speaker_id": "2",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041224.1.7",
          "form": "내일 새로산 로퍼 개시할거에요",
          "original_form": "내일 새로산 로퍼 개시할거에요",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041224.1.8",
          "form": "사이즈가 잘 맞게 왔네요",
          "original_form": "사이즈가 잘 맞게 왔네요",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041224.1.9",
          "form": "오호 로퍼",
          "original_form": "오호 로퍼",
          "speaker_id": "2",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041224.1.10",
          "form": "굳뜨",
          "original_form": "굳뜨",
          "speaker_id": "2",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041224.1.11",
          "form": "남자 셔츠래요 엄청 오버핏일거같아요",
          "original_form": "남자 셔츠래요 엄청 오버핏일거같아요",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041224.1.12",
          "form": "오잉 괜찮아요? 궁금",
          "original_form": "오잉 괜찮아요? 궁금",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100041224.1.13",
          "form": "허리잡아주나용",
          "original_form": "허리잡아주나용",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100041224.1.14",
          "form": "아니요 왕 큰거에요 ㅋㅋㅋ",
          "original_form": "아니요 왕 큰거에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041224.1.15",
          "form": "그거입고 롱부츠 개시 할거에요",
          "original_form": "그거입고 롱부츠 개시 할거에요",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041224.1.16",
          "form": "와ㅋㅋㅋ",
          "original_form": "와ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041224.1.17",
          "form": "시크해여",
          "original_form": "시크해여",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041224.1.18",
          "form": "또 순박할거같아요 .. ㅋㅋ",
          "original_form": "또 순박할거같아요 .. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041224.1.19",
          "form": "ㅂㄱㅋㅋㅋㄱ 예전엔 눈밑에도라이너그리고다니던때가있었는데",
          "original_form": "ㅂㄱㅋㅋㅋㄱ 예전엔 눈밑에도라이너그리고다니던때가있었는데",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041224.1.20",
          "form": "헐 name2요?",
          "original_form": "헐 &name2&요?",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100041224.1.21",
          "form": "완전 무서운 언니였네요",
          "original_form": "완전 무서운 언니였네요",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100041224.1.22",
          "form": "ㅋㅋㅋㅋㅋㄱㄱ무서운언니는무슨ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㄱ무서운언니는무슨ㅋㅋㅋ",
          "speaker_id": "2",
