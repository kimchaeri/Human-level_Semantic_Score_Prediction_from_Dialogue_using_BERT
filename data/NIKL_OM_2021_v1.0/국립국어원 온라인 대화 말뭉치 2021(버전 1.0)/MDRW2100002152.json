      "utterance": [
        {
          "id": "MDRW2100002152.1.1",
          "form": "운동하실때 옷은 어떻게 입으세요?",
          "original_form": "운동하실때 옷은 어떻게 입으세요?",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002152.1.2",
          "form": "전 그냥 편한옷 반팔 반바지요",
          "original_form": "전 그냥 편한옷 반팔 반바지요",
          "speaker_id": "2",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002152.1.3",
          "form": "운동복으로 따로 구분은안하시그여?",
          "original_form": "운동복으로 따로 구분은안하시그여?",
          "speaker_id": "1",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002152.1.4",
          "form": "전 안입는 반팔반바지들 모아서 따로 입긴하는데",
          "original_form": "전 안입는 반팔반바지들 모아서 따로 입긴하는데",
          "speaker_id": "1",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002152.1.5",
          "form": "아 운동복이죠",
          "original_form": "아 운동복이죠",
          "speaker_id": "2",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002152.1.6",
          "form": "살짝 틀려요 진짜 편한거로",
          "original_form": "살짝 틀려요 진짜 편한거로",
          "speaker_id": "2",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002152.1.7",
          "form": "요즘은 운동복 사야하나고민어서요",
          "original_form": "요즘은 운동복 사야하나고민어서요",
          "speaker_id": "1",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002152.1.8",
          "form": "기능성옷들이 입었을때 확실히 틀린지 궁금해요",
          "original_form": "기능성옷들이 입었을때 확실히 틀린지 궁금해요",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002152.1.9",
          "form": "틀리긴 한데 전 조금 요새꺼는 민망해서 피해요",
          "original_form": "틀리긴 한데 전 조금 요새꺼는 민망해서 피해요",
          "speaker_id": "2",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002152.1.10",
          "form": "안다르 같은건 꿈도 못꿔요 .",
          "original_form": "안다르 같은건 꿈도 못꿔요 .",
          "speaker_id": "2",
          "time": "20210820 00:09"
        },
        {
          "id": "MDRW2100002152.1.11",
          "form": "ㅜ탑은 확실히 잡아주는게훨편하긴한데 바지는 걍 헐렁한걸루다가",
          "original_form": "ㅜ탑은 확실히 잡아주는게훨편하긴한데 바지는 걍 헐렁한걸루다가",
          "speaker_id": "1",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002152.1.12",
          "form": "브랜드들도 엄청 뭐가 많이생겼더라구여",
          "original_form": "브랜드들도 엄청 뭐가 많이생겼더라구여",
          "speaker_id": "1",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002152.1.13",
          "form": "맞아요 비슷한데 브랜드가 달라요",
          "original_form": "맞아요 비슷한데 브랜드가 달라요",
          "speaker_id": "2",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002152.1.14",
          "form": "가격도 천지차이고 뭐가뭔지모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "가격도 천지차이고 뭐가뭔지모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002152.1.15",
          "form": "그러니까요 근데 어떤건 좀 신체구조에 안맞는 이상한것도 있더라구요",
          "original_form": "그러니까요 근데 어떤건 좀 신체구조에 안맞는 이상한것도 있더라구요",
          "speaker_id": "2",
          "time": "20210820 00:12"
        },
        {
          "id": "MDRW2100002152.1.16",
          "form": "돈비싸게주고 샀는데 그러면 진짜 눈물나요ㅜㅜ",
          "original_form": "돈비싸게주고 샀는데 그러면 진짜 눈물나요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100002152.1.17",
          "form": "네 정말 운동갔다 요상해서 봤네요",
          "original_form": "네 정말 운동갔다 요상해서 봤네요",
          "speaker_id": "2",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002152.1.18",
          "form": "흑 걍 있는걸로 운동해야겠어요",
          "original_form": "흑 걍 있는걸로 운동해야겠어요",
          "speaker_id": "1",
          "time": "20210820 00:15"
        }
