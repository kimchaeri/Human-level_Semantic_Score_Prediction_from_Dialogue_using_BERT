      "utterance": [
        {
          "id": "MDRW2100029246.1.1",
          "form": "이후 얘기는 이따가 낮에 해주세요",
          "original_form": "이후 얘기는 이따가 낮에 해주세요",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029246.1.2",
          "form": "너무 졸려서 ㅜㅜ",
          "original_form": "너무 졸려서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029246.1.3",
          "form": "안뇽히 주무세요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "안뇽히 주무세요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029246.1.4",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029246.1.5",
          "form": "뒷 이야기는 알아서 지어내세염ㅋㅋ",
          "original_form": "뒷 이야기는 알아서 지어내세염ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029246.1.6",
          "form": "한순간의 선택이 평생을 좌우한다",
          "original_form": "한순간의 선택이 평생을 좌우한다",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029246.1.7",
          "form": "왜 지어내요",
          "original_form": "왜 지어내요",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029246.1.8",
          "form": "얘기해줘요",
          "original_form": "얘기해줘요",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029246.1.9",
          "form": "예쓰맨은 후회만 남는다",
          "original_form": "예쓰맨은 후회만 남는다",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029246.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029246.1.11",
          "form": "바보다 바보",
          "original_form": "바보다 바보",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029246.1.12",
          "form": "그걸 보내네",
          "original_form": "그걸 보내네",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029246.1.13",
          "form": "쿨한척",
          "original_form": "쿨한척",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029246.1.14",
          "form": "바보라고 하지마요ㅠㅠ",
          "original_form": "바보라고 하지마요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029246.1.15",
          "form": "아 갑자기 마음이 너무 아파오네ㅠㅠ",
          "original_form": "아 갑자기 마음이 너무 아파오네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029246.1.16",
          "form": "밥은 먹고 다니나ㅜㅜ",
          "original_form": "밥은 먹고 다니나ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029246.1.17",
          "form": "ㅋㅋㅋㅋㅋ  왜 우세요",
          "original_form": "ㅋㅋㅋㅋㅋ  왜 우세요",
          "speaker_id": "2",
          "time": "20210903 05:45"
        },
        {
          "id": "MDRW2100029246.1.18",
          "form": "결혼해서 와이프가 차려주는 밥 먹고 잘지내겠죠",
          "original_form": "결혼해서 와이프가 차려주는 밥 먹고 잘지내겠죠",
          "speaker_id": "2",
          "time": "20210903 05:46"
        },
        {
          "id": "MDRW2100029246.1.19",
          "form": "ㅋㅋ했을까요 결혼?",
          "original_form": "ㅋㅋ했을까요 결혼?",
          "speaker_id": "1",
          "time": "20210903 07:34"
