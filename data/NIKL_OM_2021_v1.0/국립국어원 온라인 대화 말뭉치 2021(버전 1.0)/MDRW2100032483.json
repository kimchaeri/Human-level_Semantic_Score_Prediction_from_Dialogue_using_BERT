      "utterance": [
        {
          "id": "MDRW2100032483.1.1",
          "form": "피곤할 때 먹는 음식 있어?",
          "original_form": "피곤할 때 먹는 음식 있어?",
          "speaker_id": "1",
          "time": "20210905 19:10"
        },
        {
          "id": "MDRW2100032483.1.2",
          "form": "원기 회복에는 삼계탕이지",
          "original_form": "원기 회복에는 삼계탕이지",
          "speaker_id": "2",
          "time": "20210905 19:11"
        },
        {
          "id": "MDRW2100032483.1.3",
          "form": "삼계탕 좋지ㅎㅎ 우리 집은 삼계탕보다 오리백숙 자주 먹어",
          "original_form": "삼계탕 좋지ㅎㅎ 우리 집은 삼계탕보다 오리백숙 자주 먹어",
          "speaker_id": "1",
          "time": "20210905 19:11"
        },
        {
          "id": "MDRW2100032483.1.4",
          "form": "영계백숙 오오오오",
          "original_form": "영계백숙 오오오오",
          "speaker_id": "2",
          "time": "20210905 19:11"
        },
        {
          "id": "MDRW2100032483.1.5",
          "form": "전복이나 장어도 종종 먹고",
          "original_form": "전복이나 장어도 종종 먹고",
          "speaker_id": "1",
          "time": "20210905 19:12"
        },
        {
          "id": "MDRW2100032483.1.6",
          "form": "장어랑 복분자주 맛있지",
          "original_form": "장어랑 복분자주 맛있지",
          "speaker_id": "2",
          "time": "20210905 19:12"
        },
        {
          "id": "MDRW2100032483.1.7",
          "form": "맛있는 음식 먹으면 기분 좋아져",
          "original_form": "맛있는 음식 먹으면 기분 좋아져",
          "speaker_id": "1",
          "time": "20210905 19:12"
        },
        {
          "id": "MDRW2100032483.1.8",
          "form": "장어나 전복 먹는다고 막 기운이 나는지는 모르겠는데",
          "original_form": "장어나 전복 먹는다고 막 기운이 나는지는 모르겠는데",
          "speaker_id": "2",
          "time": "20210905 19:13"
        },
        {
          "id": "MDRW2100032483.1.9",
          "form": "ㅋㅋㅋㅋ 나도 그래. 플라시보 인가",
          "original_form": "ㅋㅋㅋㅋ 나도 그래. 플라시보 인가",
          "speaker_id": "1",
          "time": "20210905 19:13"
        },
        {
          "id": "MDRW2100032483.1.10",
          "form": "장어 꼬리는 실제도 별로 특별히 나은 효능이 없다잖아",
          "original_form": "장어 꼬리는 실제도 별로 특별히 나은 효능이 없다잖아",
          "speaker_id": "2",
          "time": "20210905 19:14"
        },
        {
          "id": "MDRW2100032483.1.11",
          "form": "피곤할 때 비타민 음료나 박카스",
          "original_form": "피곤할 때 비타민 음료나 박카스",
          "speaker_id": "1",
          "time": "20210905 19:14"
        },
        {
          "id": "MDRW2100032483.1.12",
          "form": "나는 몬스터 맛있어",
          "original_form": "나는 몬스터 맛있어",
          "speaker_id": "2",
          "time": "20210905 19:14"
        },
        {
          "id": "MDRW2100032483.1.13",
          "form": "몬스터 무슨 색?",
          "original_form": "몬스터 무슨 색?",
          "speaker_id": "1",
          "time": "20210905 19:14"
        },
        {
          "id": "MDRW2100032483.1.14",
          "form": "울트라 흰색 그거 맛있어",
          "original_form": "울트라 흰색 그거 맛있어",
          "speaker_id": "2",
          "time": "20210905 19:15"
        },
        {
          "id": "MDRW2100032483.1.15",
          "form": "부산에 맛집이 많잖아",
          "original_form": "부산에 맛집이 많잖아",
          "speaker_id": "1",
