        },
        {
          "id": "MDRW2100032558.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032558.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032558.1.4",
          "form": "막 먹었어요",
          "original_form": "막 먹었어요",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032558.1.5",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032558.1.6",
          "form": "네 저도 먹었어요",
          "original_form": "네 저도 먹었어요",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032558.1.7",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032558.1.8",
          "form": "ㅎㅎ떡볶이먹었어요",
          "original_form": "ㅎㅎ떡볶이먹었어요",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032558.1.9",
          "form": "직접 민들어서 드신건가요?",
          "original_form": "직접 민들어서 드신건가요?",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032558.1.10",
          "form": "넷플릭스보면서~ ㅎㅎㅎ 혹시구독하시나요?",
          "original_form": "넷플릭스보면서~ ㅎㅎㅎ 혹시구독하시나요?",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032558.1.11",
          "form": "앗길거리에서사왔어요",
          "original_form": "앗길거리에서사왔어요",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032558.1.12",
          "form": "그렇군요 저도 넷플릭스 구독해요",
          "original_form": "그렇군요 저도 넷플릭스 구독해요",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032558.1.13",
          "form": "주로 뭐 보시나요?",
          "original_form": "주로 뭐 보시나요?",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032558.1.14",
          "form": "앗ㅎㅎ",
          "original_form": "앗ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032558.1.15",
          "form": "저는최근에",
          "original_form": "저는최근에",
          "speaker_id": "2",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032558.1.16",
          "form": "에밀리파리에가다 이거봤어요",
          "original_form": "에밀리파리에가다 이거봤어요",
          "speaker_id": "2",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032558.1.17",
          "form": "그거 재밌나요??",
          "original_form": "그거 재밌나요??",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032558.1.18",
          "form": "저도추천받아서본건데",
          "original_form": "저도추천받아서본건데",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032558.1.19",
          "form": "여주인공너무이뻐서재밌더라고요ㅎㅎ",
          "original_form": "여주인공너무이뻐서재밌더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032558.1.20",
          "form": "그거 약간 파리 구경용 드라마라던데 맞나요?",
          "original_form": "그거 약간 파리 구경용 드라마라던데 맞나요?",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032558.1.21",
          "form": "ㅎㅎㅎ맞아요 파리의문화?가나와요",
          "original_form": "ㅎㅎㅎ맞아요 파리의문화?가나와요",
