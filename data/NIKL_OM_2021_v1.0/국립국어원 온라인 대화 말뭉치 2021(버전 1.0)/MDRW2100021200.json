      "utterance": [
        {
          "id": "MDRW2100021200.1.1",
          "form": "재택근무 좋아라시나요?",
          "original_form": "재택근무 좋아라시나요?",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021200.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021200.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021200.1.4",
          "form": "전 재택휴식 좋아해요...",
          "original_form": "전 재택휴식 좋아해요...",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021200.1.5",
          "form": "출근하는게.더 좋은세유?",
          "original_form": "출근하는게.더 좋은세유?",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021200.1.6",
          "form": "전 집에서 집중을 하기가 힘들어요",
          "original_form": "전 집에서 집중을 하기가 힘들어요",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021200.1.7",
          "form": "아.. 모든 분위기를 중요시하시나보여?",
          "original_form": "아.. 모든 분위기를 중요시하시나보여?",
          "speaker_id": "1",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021200.1.8",
          "form": "그냥 집중력이 딸려요 ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 집중력이 딸려요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021200.1.9",
          "form": "직장에서  재택근무는 시행중인가요?",
          "original_form": "직장에서  재택근무는 시행중인가요?",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021200.1.10",
          "form": "저는 아니에요",
          "original_form": "저는 아니에요",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021200.1.11",
          "form": "name1님은 재택이 더 좋은 이유가 잇나요?",
          "original_form": "&name1&님은 재택이 더 좋은 이유가 잇나요?",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021200.1.12",
          "form": "전 아침 준비하고 오가는 시간..",
          "original_form": "전 아침 준비하고 오가는 시간..",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021200.1.13",
          "form": "이것만 하루 3-4시간인데..",
          "original_form": "이것만 하루 3-4시간인데..",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021200.1.14",
          "form": "너무 아깝됴..",
          "original_form": "너무 아깝됴..",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021200.1.15",
          "form": "그것도 그렇죠",
          "original_form": "그것도 그렇죠",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021200.1.16",
          "form": "확실히 집에서 일하면 하루가 엄청 길어요",
          "original_form": "확실히 집에서 일하면 하루가 엄청 길어요",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021200.1.17",
          "form": "그럴것같아요",
          "original_form": "그럴것같아요",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021200.1.18",
          "form": "능률이 떨어지지는 않나요?",
          "original_form": "능률이 떨어지지는 않나요?",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021200.1.19",
          "form": "전.. 이어폰만 끼면.. 뭔가 세상과 차단막이 생겨서..",
          "original_form": "전.. 이어폰만 끼면.. 뭔가 세상과 차단막이 생겨서..",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021200.1.20",
          "form": "집에서도 공부도 잘 되요.. \r\n대신 이어폰 없음... 도서관에서도 집중못하고..",
          "original_form": "집에서도 공부도 잘 되요.. \r\n대신 이어폰 없음... 도서관에서도 집중못하고..",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021200.1.21",
          "form": "전 집에서 공부 드럽게 못하는 타입이엇는데 ㅋㅋㅋ",
          "original_form": "전 집에서 공부 드럽게 못하는 타입이엇는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:04"
        }
