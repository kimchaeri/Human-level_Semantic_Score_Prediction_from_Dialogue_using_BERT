      "utterance": [
        {
          "id": "MDRW2100007568.1.1",
          "form": "날씨얘기나하자",
          "original_form": "날씨얘기나하자",
          "speaker_id": "1",
          "time": "20210822 03:21"
        },
        {
          "id": "MDRW2100007568.1.2",
          "form": "요즘 비가 자주 내리더라",
          "original_form": "요즘 비가 자주 내리더라",
          "speaker_id": "1",
          "time": "20210822 03:21"
        },
        {
          "id": "MDRW2100007568.1.3",
          "form": "글자",
          "original_form": "글자",
          "speaker_id": "2",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007568.1.4",
          "form": "오늘 너무 추었어",
          "original_form": "오늘 너무 추었어",
          "speaker_id": "2",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007568.1.5",
          "form": "여름 아니고 가을 다된거같아ㅠ",
          "original_form": "여름 아니고 가을 다된거같아ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007568.1.6",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007568.1.7",
          "form": "근데 에어컨은 틀었어",
          "original_form": "근데 에어컨은 틀었어",
          "speaker_id": "1",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007568.1.8",
          "form": "전기세 내가 안내니까...",
          "original_form": "전기세 내가 안내니까...",
          "speaker_id": "1",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007568.1.9",
          "form": "이 추운데",
          "original_form": "이 추운데",
          "speaker_id": "2",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007568.1.10",
          "form": "에어컨을 틀었다구....?",
          "original_form": "에어컨을 틀었다구....?",
          "speaker_id": "2",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007568.1.11",
          "form": "안추워?",
          "original_form": "안추워?",
          "speaker_id": "2",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007568.1.12",
          "form": "에어컨 틀고 이불덮은거야ㅡ",
          "original_form": "에어컨 틀고 이불덮은거야ㅡ",
          "speaker_id": "2",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007568.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007568.1.14",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007568.1.15",
          "form": "에어컨 틀고 이불 덮으면",
          "original_form": "에어컨 틀고 이불 덮으면",
          "speaker_id": "1",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007568.1.16",
          "form": "거기가 극락이지",
          "original_form": "거기가 극락이지",
          "speaker_id": "1",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007568.1.17",
          "form": "그리구 요즘 긴팔셔츠입어",
          "original_form": "그리구 요즘 긴팔셔츠입어",
          "speaker_id": "1",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007568.1.18",
          "form": "환경을 생각해주세요!!",
          "original_form": "환경을 생각해주세요!!",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007568.1.19",
          "form": "지구가 아파하고 있다구요!!",
          "original_form": "지구가 아파하고 있다구요!!",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007568.1.20",
          "form": "지구가",
          "original_form": "지구가",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007568.1.21",
          "form": "아파요!",
          "original_form": "아파요!",
          "speaker_id": "2",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007568.1.22",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007568.1.23",
          "form": "절기가 근데 과학은 과학인가벼",
          "original_form": "절기가 근데 과학은 과학인가벼",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007568.1.24",
          "form": "참..나...??!",
          "original_form": "참..나...??!",
          "speaker_id": "2",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007568.1.25",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007568.1.26",
          "form": "입추됐다고",
          "original_form": "입추됐다고",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007568.1.27",
          "form": "시원해지는게",
          "original_form": "시원해지는게",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007568.1.28",
          "form": "헉 입추됐어?",
          "original_form": "헉 입추됐어?",
          "speaker_id": "2",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007568.1.29",
          "form": "아직 8월인데",
          "original_form": "아직 8월인데",
          "speaker_id": "2",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007568.1.30",
          "form": "벌써 입추야?",
          "original_form": "벌써 입추야?",
          "speaker_id": "2",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007568.1.31",
          "form": "입추가",
          "original_form": "입추가",
          "speaker_id": "1",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007568.1.32",
          "form": "말복보다",
          "original_form": "말복보다",
          "speaker_id": "1",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007568.1.33",
          "form": "전일걸?",
          "original_form": "전일걸?",
          "speaker_id": "1",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007568.1.34",
          "form": "말복 지났다아냐?",
          "original_form": "말복 지났다아냐?",
          "speaker_id": "2",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007568.1.35",
          "form": "입추가...벌써 지났구나ㅠ",
          "original_form": "입추가...벌써 지났구나ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007568.1.36",
          "form": "어쩐지 오늘 되게 춥더라",
          "original_form": "어쩐지 오늘 되게 춥더라",
          "speaker_id": "2",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007568.1.37",
          "form": "콧물 나",
          "original_form": "콧물 나",
          "speaker_id": "2",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007568.1.38",
          "form": "킁",
          "original_form": "킁",
          "speaker_id": "2",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007568.1.39",
          "form": "그건 아닌거같애...",
          "original_form": "그건 아닌거같애...",
          "speaker_id": "1",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007568.1.40",
          "form": "왜냐",
          "original_form": "왜냐",
          "speaker_id": "1",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007568.1.41",
          "form": "오늘 30분걸었는데",
          "original_form": "오늘 30분걸었는데",
          "speaker_id": "1",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007568.1.42",
          "form": "온몸에서땀이남",
          "original_form": "온몸에서땀이남",
          "speaker_id": "1",
          "time": "20210822 03:29"
        }
      ]
