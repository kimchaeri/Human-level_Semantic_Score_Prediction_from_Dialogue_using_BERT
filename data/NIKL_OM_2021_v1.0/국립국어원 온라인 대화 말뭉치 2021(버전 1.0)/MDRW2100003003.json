      "utterance": [
        {
          "id": "MDRW2100003003.1.1",
          "form": "싸이월드 계정가지고계시나요?",
          "original_form": "싸이월드 계정가지고계시나요?",
          "speaker_id": "1",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100003003.1.2",
          "form": "가지고는 있는데\r\n폰번호가 변경되서 이번에 복구 못했어여ㅠ",
          "original_form": "가지고는 있는데\r\n폰번호가 변경되서 이번에 복구 못했어여ㅠ",
          "speaker_id": "2",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100003003.1.3",
          "form": "과거사진들의 흔적을 못찾으시겠네요ㅎㅎ",
          "original_form": "과거사진들의 흔적을 못찾으시겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003003.1.4",
          "form": "판도라의 상자라",
          "original_form": "판도라의 상자라",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100003003.1.5",
          "form": "저는 계정 찾아도 없앨거에여ㅋㅋㅋㅋㅋ",
          "original_form": "저는 계정 찾아도 없앨거에여ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100003003.1.6",
          "form": "아 그 판도라의상자에 저도 일촌하고싶네요",
          "original_form": "아 그 판도라의상자에 저도 일촌하고싶네요",
          "speaker_id": "1",
          "time": "20210820 17:36"
        },
        {
          "id": "MDRW2100003003.1.7",
          "form": "ㅋㅋㅋ혹시 어디 분이세여?",
          "original_form": "ㅋㅋㅋ혹시 어디 분이세여?",
          "speaker_id": "2",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100003003.1.8",
          "form": "그런건 일촌하고 알려드릴께요^^",
          "original_form": "그런건 일촌하고 알려드릴께요^^",
          "speaker_id": "1",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100003003.1.9",
          "form": "제 판도라들과 같은 지역분이\r\n아니라면야 일촌 가능할거 같네여ㅋㅋ",
          "original_form": "제 판도라들과 같은 지역분이\r\n아니라면야 일촌 가능할거 같네여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100003003.1.10",
          "form": "아...네ㅋㅋㅋㅋ 비밀글로 남겨주셔도 되영",
          "original_form": "아...네ㅋㅋㅋㅋ 비밀글로 남겨주셔도 되영",
          "speaker_id": "2",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100003003.1.11",
          "form": "엄청 궁금하네요ㅎㅎ 방명록을 못써서 아쉽\r\n기능없어졌다던데",
          "original_form": "엄청 궁금하네요ㅎㅎ 방명록을 못써서 아쉽\r\n기능없어졌다던데",
          "speaker_id": "1",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100003003.1.12",
          "form": "아 방명록은 아예 사라진건가요?",
          "original_form": "아 방명록은 아예 사라진건가요?",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003003.1.13",
          "form": "아님 기존 글들은 복구되는거에여?",
          "original_form": "아님 기존 글들은 복구되는거에여?",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003003.1.14",
          "form": "아에없어진것같던데 다시생기면 들어가서 쓰는것도 귀찮을듯ㅜ",
          "original_form": "아에없어진것같던데 다시생기면 들어가서 쓰는것도 귀찮을듯ㅜ",
          "speaker_id": "1",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003003.1.15",
          "form": "그때 그 시절 어떤 얘기들을\r\n했을지 너무 궁금한데ㅋㅋㅋ",
          "original_form": "그때 그 시절 어떤 얘기들을\r\n했을지 너무 궁금한데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:47"
        },
        {
          "id": "MDRW2100003003.1.16",
          "form": "그러게요ㅋㅋ 꼭 아이디찾으시면 저 검색해주세요 일촌하게ㅋㅋ",
          "original_form": "그러게요ㅋㅋ 꼭 아이디찾으시면 저 검색해주세요 일촌하게ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003003.1.17",
          "form": "그럴게여ㅋㅋㅋ\r\n사진 다 일촌공개세요?",
          "original_form": "그럴게여ㅋㅋㅋ\r\n사진 다 일촌공개세요?",
          "speaker_id": "2",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003003.1.18",
          "form": "당당히 전체공개 이쁜척한것많습니다ㅎㅎ\r\n구경오시길ㅎㅎ",
