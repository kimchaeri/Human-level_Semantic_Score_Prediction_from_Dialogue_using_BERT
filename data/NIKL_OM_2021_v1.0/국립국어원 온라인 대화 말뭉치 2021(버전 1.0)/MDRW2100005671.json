      "utterance": [
        {
          "id": "MDRW2100005671.1.1",
          "form": "아 요즘",
          "original_form": "아 요즘",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005671.1.2",
          "form": "너무 추워요",
          "original_form": "너무 추워요",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005671.1.3",
          "form": "저만 춥나요",
          "original_form": "저만 춥나요",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005671.1.4",
          "form": "아직 8월인데?",
          "original_form": "아직 8월인데?",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005671.1.5",
          "form": "저만...추워요??",
          "original_form": "저만...추워요??",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005671.1.6",
          "form": "선풍기만 쓴다고 하지 않았어?",
          "original_form": "선풍기만 쓴다고 하지 않았어?",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005671.1.7",
          "form": "저 문열고자서",
          "original_form": "저 문열고자서",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005671.1.8",
          "form": "새벽마다 떨면서 일어나서",
          "original_form": "새벽마다 떨면서 일어나서",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005671.1.9",
          "form": "엄마가 그정도로 추우면 문닫고 자라고",
          "original_form": "엄마가 그정도로 추우면 문닫고 자라고",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005671.1.10",
          "form": "혼냄여",
          "original_form": "혼냄여",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005671.1.11",
          "form": "요즘 한동안 계속 30도 넘다가",
          "original_form": "요즘 한동안 계속 30도 넘다가",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005671.1.12",
          "form": "20도대가 되니까",
          "original_form": "20도대가 되니까",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005671.1.13",
          "form": "새벽에는 춥긴 하더라",
          "original_form": "새벽에는 춥긴 하더라",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005671.1.14",
          "form": "또르르...",
          "original_form": "또르르...",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005671.1.15",
          "form": "그리고 오늘은 심지어",
          "original_form": "그리고 오늘은 심지어",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005671.1.16",
          "form": "비도 엄청왔어요",
          "original_form": "비도 엄청왔어요",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005671.1.17",
          "form": "천둥번개치고 난리남",
          "original_form": "천둥번개치고 난리남",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005671.1.18",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005671.1.19",
          "form": "거기는 어땠어요?",
          "original_form": "거기는 어땠어요?",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005671.1.20",
          "form": "여기는 번개는 안쳤는데",
          "original_form": "여기는 번개는 안쳤는데",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005671.1.21",
          "form": "태풍 온것처럼 바람이 불었어",
          "original_form": "태풍 온것처럼 바람이 불었어",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005671.1.22",
          "form": "대문 열어놨더니 집 안 물건도 떨어지더라",
          "original_form": "대문 열어놨더니 집 안 물건도 떨어지더라",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005671.1.23",
          "form": "엥 진짜요??",
          "original_form": "엥 진짜요??",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005671.1.24",
          "form": "물건도 떨어지는건",
          "original_form": "물건도 떨어지는건",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005671.1.25",
          "form": "무섭다",
          "original_form": "무섭다",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005671.1.26",
          "form": "여기는 이제 그쳤는데 거기는 어때?",
          "original_form": "여기는 이제 그쳤는데 거기는 어때?",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005671.1.27",
          "form": "아직도 비와?",
          "original_form": "아직도 비와?",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005671.1.28",
          "form": "여긴 안와욧 이제는 ㅋㅋㅋㅋ",
          "original_form": "여긴 안와욧 이제는 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005671.1.29",
          "form": "요즘은 일기예보 안맞는 경우가 많아서",
          "original_form": "요즘은 일기예보 안맞는 경우가 많아서",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005671.1.30",
          "form": "여러모로 불편할 때가 많은거같아",
          "original_form": "여러모로 불편할 때가 많은거같아",
          "speaker_id": "2",
          "time": "20210821 23:48"
        }
      ]
    }
