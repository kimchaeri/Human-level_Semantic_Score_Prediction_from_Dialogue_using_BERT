      "utterance": [
        {
          "id": "MDRW2100023482.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100023482.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100023482.1.3",
          "form": "즐겨 보는 책일 프로그램 있으세요",
          "original_form": "즐겨 보는 책일 프로그램 있으세요",
          "speaker_id": "2",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100023482.1.4",
          "form": "티비",
          "original_form": "티비",
          "speaker_id": "2",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100023482.1.5",
          "form": "저는 드라마 좋아해서 드라마 대부분 봐요",
          "original_form": "저는 드라마 좋아해서 드라마 대부분 봐요",
          "speaker_id": "1",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100023482.1.6",
          "form": "전 애기 때문에 드라마는 못 보고",
          "original_form": "전 애기 때문에 드라마는 못 보고",
          "speaker_id": "2",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100023482.1.7",
          "form": "끊어서 볼 수 있는 예능 만 본답니다",
          "original_form": "끊어서 볼 수 있는 예능 만 본답니다",
          "speaker_id": "2",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100023482.1.8",
          "form": "어떤 드라마를 제일 좋아하세요",
          "original_form": "어떤 드라마를 제일 좋아하세요",
          "speaker_id": "2",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100023482.1.9",
          "form": "아 예능 어느거 보세요?",
          "original_form": "아 예능 어느거 보세요?",
          "speaker_id": "1",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100023482.1.10",
          "form": "저는 요즘 차태현 나오는 경찰수업 재미있게봅니다",
          "original_form": "저는 요즘 차태현 나오는 경찰수업 재미있게봅니다",
          "speaker_id": "1",
          "time": "20210830 10:37"
        },
        {
          "id": "MDRW2100023482.1.11",
          "form": "전 해방타운 즐겨 봐요~",
          "original_form": "전 해방타운 즐겨 봐요~",
          "speaker_id": "2",
          "time": "20210830 10:37"
        },
        {
          "id": "MDRW2100023482.1.12",
          "form": "요즘 경찰 수업 인기 많더라구요",
          "original_form": "요즘 경찰 수업 인기 많더라구요",
          "speaker_id": "2",
          "time": "20210830 10:37"
        },
        {
          "id": "MDRW2100023482.1.13",
          "form": "차태현 도 오랜만에 나오네요",
          "original_form": "차태현 도 오랜만에 나오네요",
          "speaker_id": "2",
          "time": "20210830 10:37"
        },
        {
          "id": "MDRW2100023482.1.14",
          "form": "아 저도 그거 가끔봐요",
          "original_form": "아 저도 그거 가끔봐요",
          "speaker_id": "1",
          "time": "20210830 10:38"
        },
        {
          "id": "MDRW2100023482.1.15",
          "form": "갈수록 재미있어요",
          "original_form": "갈수록 재미있어요",
          "speaker_id": "1",
          "time": "20210830 10:38"
        },
        {
          "id": "MDRW2100023482.1.16",
          "form": "맛있는 거 먹고 티비 프로그램 보면 정말 기분 좋죠~",
          "original_form": "맛있는 거 먹고 티비 프로그램 보면 정말 기분 좋죠~",
          "speaker_id": "2",
          "time": "20210830 10:39"
        },
        {
          "id": "MDRW2100023482.1.17",
          "form": "소소한 행복입니다",
          "original_form": "소소한 행복입니다",
          "speaker_id": "2",
          "time": "20210830 10:39"
        },
        {
          "id": "MDRW2100023482.1.18",
          "form": "맞아요 티비보면서 쉬는건 정말 좋아요",
          "original_form": "맞아요 티비보면서 쉬는건 정말 좋아요",
          "speaker_id": "1",
          "time": "20210830 10:40"
        },
        {
          "id": "MDRW2100023482.1.19",
          "form": "해방타운 보시면서 대리만족 하시나봐요",
          "original_form": "해방타운 보시면서 대리만족 하시나봐요",
          "speaker_id": "1",
          "time": "20210830 10:40"
        },
        {
          "id": "MDRW2100023482.1.20",
          "form": "내 맞아요 해방 하고 싶어요ㅋㅋㅋㅋ",
          "original_form": "내 맞아요 해방 하고 싶어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:41"
        },
        {
          "id": "MDRW2100023482.1.21",
          "form": "정말 부모님은  어떻게 키우셨나 싶네요ㅋㅋㅋ",
          "original_form": "정말 부모님은  어떻게 키우셨나 싶네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:41"
        },
        {
          "id": "MDRW2100023482.1.22",
          "form": "넷플릭스 보시나요",
          "original_form": "넷플릭스 보시나요",
          "speaker_id": "2",
          "time": "20210830 10:42"
        },
        {
          "id": "MDRW2100023482.1.23",
          "form": "그쵸ㅋㅋ",
          "original_form": "그쵸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:42"
        },
        {
          "id": "MDRW2100023482.1.24",
          "form": "넷플렉스는 미드를 위주로 봐요ㅋㅋ",
          "original_form": "넷플렉스는 미드를 위주로 봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:42"
        },
        {
          "id": "MDRW2100023482.1.25",
          "form": "넷플릭스 보실시간되세요?",
          "original_form": "넷플릭스 보실시간되세요?",
          "speaker_id": "1",
          "time": "20210830 10:42"
        },
        {
          "id": "MDRW2100023482.1.26",
          "form": "최근에 대화해 보면 넷플릭스 얘기를 엄청 많이 하더라구요",
          "original_form": "최근에 대화해 보면 넷플릭스 얘기를 엄청 많이 하더라구요",
          "speaker_id": "2",
          "time": "20210830 10:42"
        },
        {
          "id": "MDRW2100023482.1.27",
          "form": "한번 들어가  본 적도 없네요 ㅠㅠ",
          "original_form": "한번 들어가  본 적도 없네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100023482.1.28",
          "form": "그쵸 정주행하기도 쉽고 몰아서 보니깐 좋아요",
          "original_form": "그쵸 정주행하기도 쉽고 몰아서 보니깐 좋아요",
          "speaker_id": "1",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100023482.1.29",
          "form": "애기때문에 시간이 안되실거같아요",
          "original_form": "애기때문에 시간이 안되실거같아요",
          "speaker_id": "1",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100023482.1.30",
          "form": "애 재우고 봐도 되는데 ㅋㅋㅋㅋ할일이 많네요ㅠㅠ",
          "original_form": "애 재우고 봐도 되는데 ㅋㅋㅋㅋ할일이 많네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 10:45"
        }
      ]
