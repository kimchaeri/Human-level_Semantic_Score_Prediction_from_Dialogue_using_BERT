          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045355.1.2",
          "form": "나중에 마지막에",
          "original_form": "나중에 마지막에",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045355.1.3",
          "form": "이제 넌 자유야",
          "original_form": "이제 넌 자유야",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045355.1.4",
          "form": "꿈틀대는 영혼",
          "original_form": "꿈틀대는 영혼",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045355.1.5",
          "form": "날아가라",
          "original_form": "날아가라",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045355.1.6",
          "form": "하면서 이츠학이 자기한테 씌워주려던 가발을",
          "original_form": "하면서 이츠학이 자기한테 씌워주려던 가발을",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045355.1.7",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045355.1.8",
          "form": "막으면서",
          "original_form": "막으면서",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045355.1.9",
          "form": "이츠학한테 쓰라고 주거든",
          "original_form": "이츠학한테 쓰라고 주거든",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045355.1.10",
          "form": "그때 그 감정이 폭팔하는 장면이",
          "original_form": "그때 그 감정이 폭팔하는 장면이",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.11",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.12",
          "form": "혐관끼리는",
          "original_form": "혐관끼리는",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.13",
          "form": "오...!",
          "original_form": "오...!",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.14",
          "form": "이런 빌드업이라면",
          "original_form": "이런 빌드업이라면",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.16",
          "form": "여기선 그냥 아 그리워했나보네",
          "original_form": "여기선 그냥 아 그리워했나보네",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.17",
          "form": "이런느낌이 되서",
          "original_form": "이런느낌이 되서",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.18",
          "form": "조금밍숭하긴해여",
          "original_form": "조금밍숭하긴해여",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.20",
          "form": "ㅇㅇㅇ 그렇긴 하겠다",
          "original_form": "ㅇㅇㅇ 그렇긴 하겠다",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.21",
          "form": "그부분이 중요한부분이라서",
          "original_form": "그부분이 중요한부분이라서",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.22",
          "form": "나중에 터질 감정을",
          "original_form": "나중에 터질 감정을",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.23",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.24",
          "form": "미리 조금씩 땅겨쓰는 느낌이네",
          "original_form": "미리 조금씩 땅겨쓰는 느낌이네",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.25",
          "form": "그때 이츠학이 엄청 감격하면서",
          "original_form": "그때 이츠학이 엄청 감격하면서",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.26",
          "form": "나가서 갈아입고",
          "original_form": "나가서 갈아입고",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045355.1.27",
          "form": "너가 이제 메인이야 하고 물려주듯이 떠나는건데",
          "original_form": "너가 이제 메인이야 하고 물려주듯이 떠나는건데",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045355.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 조는 그부분이 드라마틱해지지 않아서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 조는 그부분이 드라마틱해지지 않아서",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045355.1.29",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045355.1.30",
          "form": "암튼 그렇게 보는 재미가 있더라고옄ㅋㅋㅋ",
          "original_form": "암튼 그렇게 보는 재미가 있더라고옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045355.1.31",
          "form": "아 그리고 실버크로스.... 있었는데 없었다는 그거...",
          "original_form": "아 그리고 실버크로스.... 있었는데 없었다는 그거...",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045355.1.32",
          "form": "민기는 있었는데요 있었습니다",
          "original_form": "민기는 있었는데요 있었습니다",
          "speaker_id": "1",
          "time": "20210908 02:04"
        }
      ]
