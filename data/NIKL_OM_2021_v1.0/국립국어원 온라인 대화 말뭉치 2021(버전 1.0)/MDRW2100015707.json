      "utterance": [
        {
          "id": "MDRW2100015707.1.1",
          "form": "최근에 구매한 물건 있나요?",
          "original_form": "최근에 구매한 물건 있나요?",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015707.1.2",
          "form": "최근에 구매한건 옷인거같아요!",
          "original_form": "최근에 구매한건 옷인거같아요!",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015707.1.3",
          "form": "운동할때 입으려도 티를 샀어요ㅎㅎ",
          "original_form": "운동할때 입으려도 티를 샀어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015707.1.4",
          "form": "아하ㅎㅎ저두 운동할때 옷이랑 매트랑 이것저것 다 샀었는데",
          "original_form": "아하ㅎㅎ저두 운동할때 옷이랑 매트랑 이것저것 다 샀었는데",
          "speaker_id": "1",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015707.1.5",
          "form": "지금은 애물단지네요ㅋㅋ",
          "original_form": "지금은 애물단지네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015707.1.6",
          "form": "ㅋㅋㅋㅋ그러셨군요",
          "original_form": "ㅋㅋㅋㅋ그러셨군요",
          "speaker_id": "2",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015707.1.7",
          "form": "저는 운동할때 쓰는 용품같은거 엄청 열심히 쓰고있어요!",
          "original_form": "저는 운동할때 쓰는 용품같은거 엄청 열심히 쓰고있어요!",
          "speaker_id": "2",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015707.1.8",
          "form": "운동 하루에 얼마나 하세요?",
          "original_form": "운동 하루에 얼마나 하세요?",
          "speaker_id": "1",
          "time": "20210826 08:21"
        },
        {
          "id": "MDRW2100015707.1.9",
          "form": "저는 하루에 1시건에서 2시간 정도하는거같아요!",
          "original_form": "저는 하루에 1시건에서 2시간 정도하는거같아요!",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100015707.1.10",
          "form": "운동을 엄청 좋아해서 보충제도 다 먹어가서 곧 사야하네요!ㅋㅋㅋ",
          "original_form": "운동을 엄청 좋아해서 보충제도 다 먹어가서 곧 사야하네요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100015707.1.11",
          "form": "오 운동 꾸준히 하시는구나",
          "original_form": "오 운동 꾸준히 하시는구나",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100015707.1.12",
          "form": "홈트로 하는거예요?",
          "original_form": "홈트로 하는거예요?",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100015707.1.13",
          "form": "네 한 1~2년정도 돼가요!",
          "original_form": "네 한 1~2년정도 돼가요!",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100015707.1.14",
          "form": "헬스장에서 하고있어용!ㅎㅎ",
          "original_form": "헬스장에서 하고있어용!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100015707.1.15",
          "form": "아하 역시 헬스장을 다녀야 좀 하게 되는 것 같아요",
          "original_form": "아하 역시 헬스장을 다녀야 좀 하게 되는 것 같아요",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100015707.1.16",
          "form": "피티받으면서 하는거예요?",
          "original_form": "피티받으면서 하는거예요?",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100015707.1.17",
          "form": "아니요! 저는 피티 안받구 혼자해요!!",
          "original_form": "아니요! 저는 피티 안받구 혼자해요!!",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100015707.1.18",
          "form": "오아 멋져요!!!",
          "original_form": "오아 멋져요!!!",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100015707.1.19",
          "form": "아이구 감사합니당ㅎㅎ",
          "original_form": "아이구 감사합니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100015707.1.20",
          "form": "제가 운동을 되게 좋아해서 그냥 즐기면서 꾸준히 하는거같아요!",
          "original_form": "제가 운동을 되게 좋아해서 그냥 즐기면서 꾸준히 하는거같아요!",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100015707.1.21",
          "form": "오오 꾸준히 하는거 좋죵",
          "original_form": "오오 꾸준히 하는거 좋죵",
