      "utterance": [
        {
          "id": "MDRW2100029796.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 13:48"
        },
        {
          "id": "MDRW2100029796.1.2",
          "form": "넵 ㅋㅋ 드셨나요",
          "original_form": "넵 ㅋㅋ 드셨나요",
          "speaker_id": "2",
          "time": "20210903 13:49"
        },
        {
          "id": "MDRW2100029796.1.3",
          "form": "저는 아직이요..ㅋㅋ",
          "original_form": "저는 아직이요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:50"
        },
        {
          "id": "MDRW2100029796.1.4",
          "form": "오 뭐드시려고요?ㅎㅎ",
          "original_form": "오 뭐드시려고요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:51"
        },
        {
          "id": "MDRW2100029796.1.5",
          "form": "아직 안정했어요 근데 아마 집에 있는 반찬이랑 먹지 않을까 싶네요!",
          "original_form": "아직 안정했어요 근데 아마 집에 있는 반찬이랑 먹지 않을까 싶네요!",
          "speaker_id": "1",
          "time": "20210903 13:53"
        },
        {
          "id": "MDRW2100029796.1.6",
          "form": "잎님 뭐드셨나요",
          "original_form": "잎님 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210903 13:53"
        },
        {
          "id": "MDRW2100029796.1.7",
          "form": "저도 집밥이요 ㅋㅋㅋ 돈까스랑 파전 김치찌개먹었어요",
          "original_form": "저도 집밥이요 ㅋㅋㅋ 돈까스랑 파전 김치찌개먹었어요",
          "speaker_id": "2",
          "time": "20210903 13:54"
        },
        {
          "id": "MDRW2100029796.1.8",
          "form": "우와 완전 든든하게 챙겨드셨네요ㅋㅋ",
          "original_form": "우와 완전 든든하게 챙겨드셨네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:56"
        },
        {
          "id": "MDRW2100029796.1.9",
          "form": "네 근데 그린티라떼랑 휘낭시에도 먹었어여ㅋㅋ",
          "original_form": "네 근데 그린티라떼랑 휘낭시에도 먹었어여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:57"
        },
        {
          "id": "MDRW2100029796.1.10",
          "form": "완전배불러요",
          "original_form": "완전배불러요",
          "speaker_id": "2",
          "time": "20210903 13:58"
        },
        {
          "id": "MDRW2100029796.1.11",
          "form": "휘낭시에가 뭐예요? 디저트 같은건가?",
          "original_form": "휘낭시에가 뭐예요? 디저트 같은건가?",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100029796.1.12",
          "form": "네맞아욬ㅋㅋ 디저트요 맛있어요",
          "original_form": "네맞아욬ㅋㅋ 디저트요 맛있어요",
          "speaker_id": "2",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100029796.1.13",
          "form": "검색해봤는데 비주얼은 익숙하네요 저 배고파짐...",
          "original_form": "검색해봤는데 비주얼은 익숙하네요 저 배고파짐...",
          "speaker_id": "1",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100029796.1.14",
          "form": "ㅋㅋㅋㅠㅠ 얼른점심드세여",
          "original_form": "ㅋㅋㅋㅠㅠ 얼른점심드세여",
          "speaker_id": "2",
          "time": "20210903 14:41"
        },
        {
          "id": "MDRW2100029796.1.15",
          "form": "사실 아직 수업중이라",
          "original_form": "사실 아직 수업중이라",
          "speaker_id": "1",
          "time": "20210903 14:43"
        },
        {
          "id": "MDRW2100029796.1.16",
          "form": "끝나고 맛나게 먹을게요",
          "original_form": "끝나고 맛나게 먹을게요",
