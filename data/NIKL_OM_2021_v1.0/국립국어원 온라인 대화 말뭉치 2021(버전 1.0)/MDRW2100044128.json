      "utterance": [
        {
          "id": "MDRW2100044128.1.1",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044128.1.2",
          "form": "지금은 없고 어릴때 키웠어요 강아지 !!",
          "original_form": "지금은 없고 어릴때 키웠어요 강아지 !!",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044128.1.3",
          "form": "오 전 병아리 어릴 때 중간까지 키웠어요",
          "original_form": "오 전 병아리 어릴 때 중간까지 키웠어요",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044128.1.4",
          "form": "학교앞에서 파는 병아리요?",
          "original_form": "학교앞에서 파는 병아리요?",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044128.1.5",
          "form": "네 그 병아리요",
          "original_form": "네 그 병아리요",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044128.1.6",
          "form": "어려서 철이 없었죠 ㅠ",
          "original_form": "어려서 철이 없었죠 ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044128.1.7",
          "form": "ㅋㅋㅋ 근데 거기서 파는 아이들 어차피 안 팔리면 죽는 애들이라네요 ㅠㅠ",
          "original_form": "ㅋㅋㅋ 근데 거기서 파는 아이들 어차피 안 팔리면 죽는 애들이라네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044128.1.8",
          "form": "ㅠㅠ 전 중간에 닭 키우는 곳으로 보냈어요",
          "original_form": "ㅠㅠ 전 중간에 닭 키우는 곳으로 보냈어요",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044128.1.9",
          "form": "아 그래도 그 아이는 좋은 환경에서 자랐겠네요",
          "original_form": "아 그래도 그 아이는 좋은 환경에서 자랐겠네요",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044128.1.10",
          "form": "그런가요? 그 뒤로는 동물 키우는 것에 신중해 졌어요",
          "original_form": "그런가요? 그 뒤로는 동물 키우는 것에 신중해 졌어요",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044128.1.11",
          "form": "실내에서 키우신 거예요?",
          "original_form": "실내에서 키우신 거예요?",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044128.1.12",
          "form": "네 실내에서요",
          "original_form": "네 실내에서요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044128.1.13",
          "form": "병아리는 모이가 따로 있죠?",
          "original_form": "병아리는 모이가 따로 있죠?",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044128.1.14",
          "form": "곡물 줬던 것 같아요 어릴 적이라서요",
          "original_form": "곡물 줬던 것 같아요 어릴 적이라서요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044128.1.15",
          "form": "참 어떤 강아지 키우셨나요?",
          "original_form": "참 어떤 강아지 키우셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044128.1.16",
          "form": "저희는 치와와랑 진돗개요!",
          "original_form": "저희는 치와와랑 진돗개요!",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044128.1.17",
          "form": "치와와는 물릴 뻔한 안 좋은 추억이 ㅠㅠ 주인 잘 못이지만요",
          "original_form": "치와와는 물릴 뻔한 안 좋은 추억이 ㅠㅠ 주인 잘 못이지만요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044128.1.18",
          "form": "치와와가 아주 성격이 드세요..",
          "original_form": "치와와가 아주 성격이 드세요..",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044128.1.19",
          "form": "우리 개는 안문다는 말을 했거든요 ㅠ 진돗개는 어때요? 똑똑하고 충성심이 강하죠?",
          "original_form": "우리 개는 안문다는 말을 했거든요 ㅠ 진돗개는 어때요? 똑똑하고 충성심이 강하죠?",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044128.1.20",
          "form": "진돗개는 엄청 영리해요 근데 고집도 세요 ㅋㅋ",
          "original_form": "진돗개는 엄청 영리해요 근데 고집도 세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:22"
        }
      ]
