      "utterance": [
        {
          "id": "MDRW2100036267.1.1",
          "form": "저녁 드셨나용~?",
          "original_form": "저녁 드셨나용~?",
          "speaker_id": "2",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100036267.1.2",
          "form": "넹!",
          "original_form": "넹!",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100036267.1.3",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100036267.1.4",
          "form": "네! 피자먹었어요~",
          "original_form": "네! 피자먹었어요~",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036267.1.5",
          "form": "저는 닭갈비먹었어요",
          "original_form": "저는 닭갈비먹었어요",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036267.1.6",
          "form": "어디 피자 먹었나요?",
          "original_form": "어디 피자 먹었나요?",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036267.1.7",
          "form": "저는 피자마루요!",
          "original_form": "저는 피자마루요!",
          "speaker_id": "2",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100036267.1.8",
          "form": "name1님은 어느 닭갈비 드셨어요?",
          "original_form": "&name1&님은 어느 닭갈비 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100036267.1.9",
          "form": "저는 이마트에서 나오는 닭갈비 먹었어요",
          "original_form": "저는 이마트에서 나오는 닭갈비 먹었어요",
          "speaker_id": "1",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100036267.1.10",
          "form": "순한맛으로 먹었어요",
          "original_form": "순한맛으로 먹었어요",
          "speaker_id": "1",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100036267.1.11",
          "form": "피자는 어떤피자드셨어요?",
          "original_form": "피자는 어떤피자드셨어요?",
          "speaker_id": "1",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100036267.1.12",
          "form": "꿀고구마 피자요!!",
          "original_form": "꿀고구마 피자요!!",
          "speaker_id": "2",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036267.1.13",
          "form": "이마트 닭갈비 맛있나요??",
          "original_form": "이마트 닭갈비 맛있나요??",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100036267.1.14",
          "form": "한끼로 먹을만해요!",
          "original_form": "한끼로 먹을만해요!",
          "speaker_id": "1",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100036267.1.15",
          "form": "볶음밥소스까지있어서",
          "original_form": "볶음밥소스까지있어서",
          "speaker_id": "1",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100036267.1.16",
          "form": "든든해요",
          "original_form": "든든해요",
          "speaker_id": "1",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100036267.1.17",
          "form": "오... 밀키트 같은건가용?",
          "original_form": "오... 밀키트 같은건가용?",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100036267.1.18",
          "form": "네 밀키트예요",
          "original_form": "네 밀키트예요",
          "speaker_id": "1",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100036267.1.19",
          "form": "이마트 좋은게 많아진 것 같네요!",
          "original_form": "이마트 좋은게 많아진 것 같네요!",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100036267.1.20",
          "form": "가끔가서 세일할때 가져오는데 하나있으면 먹을거없을때 유용해요!",
          "original_form": "가끔가서 세일할때 가져오는데 하나있으면 먹을거없을때 유용해요!",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036267.1.21",
          "form": "카레향이 좀 강하긴한데 추천해요",
          "original_form": "카레향이 좀 강하긴한데 추천해요",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036267.1.22",
          "form": "오! 좋네용~~ 가격도 적당하다 들었는데 맞나용?",
          "original_form": "오! 좋네용~~ 가격도 적당하다 들었는데 맞나용?",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036267.1.23",
          "form": "네 전 마감세일해서 한 칠천원쯤에 샀어요",
          "original_form": "네 전 마감세일해서 한 칠천원쯤에 샀어요",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036267.1.24",
          "form": "2인분이예요!",
          "original_form": "2인분이예요!",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036267.1.25",
          "form": "와ㅋㅋㅋ 한ㄲㅣ에 칠천원이라니 최고네용",
          "original_form": "와ㅋㅋㅋ 한ㄲㅣ에 칠천원이라니 최고네용",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036267.1.26",
          "form": "이마트 함 달려가봐야겠어요!",
          "original_form": "이마트 함 달려가봐야겠어요!",
          "speaker_id": "2",
          "time": "20210830 21:47"
        }
      ]
    }
