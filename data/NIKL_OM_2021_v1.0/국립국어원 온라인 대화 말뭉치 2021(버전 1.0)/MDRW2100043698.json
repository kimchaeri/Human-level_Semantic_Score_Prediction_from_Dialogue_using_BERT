      "utterance": [
        {
          "id": "MDRW2100043698.1.1",
          "form": "추천하실만한 여행지 있으신가요?",
          "original_form": "추천하실만한 여행지 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043698.1.2",
          "form": "전 캐나다??",
          "original_form": "전 캐나다??",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043698.1.3",
          "form": "굉장히 깨끗해요!",
          "original_form": "굉장히 깨끗해요!",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043698.1.4",
          "form": "캐나다 어디요?",
          "original_form": "캐나다 어디요?",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043698.1.5",
          "form": "벤쿠버랑 록키갔었어요!",
          "original_form": "벤쿠버랑 록키갔었어요!",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043698.1.6",
          "form": "근데 자연환경에 감흥이 없으시면ㅋㅋ 비추",
          "original_form": "근데 자연환경에 감흥이 없으시면ㅋㅋ 비추",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043698.1.7",
          "form": "오우~ 벤쿠버~ 거기가면 꼭 봐야 할것이 있다면요?",
          "original_form": "오우~ 벤쿠버~ 거기가면 꼭 봐야 할것이 있다면요?",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043698.1.8",
          "form": "저 뉴질랜드랑 호주 갔을때 진짜 좋았거든요",
          "original_form": "저 뉴질랜드랑 호주 갔을때 진짜 좋았거든요",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043698.1.9",
          "form": "벤쿠버끼고 보통 벤프 록키산 보는 코스 많이 가더라고요",
          "original_form": "벤쿠버끼고 보통 벤프 록키산 보는 코스 많이 가더라고요",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043698.1.10",
          "form": "오우~ 다 보셨나요??",
          "original_form": "오우~ 다 보셨나요??",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043698.1.11",
          "form": "저 워킹으로 잠깐 다녀와서ㅋㅋ 벤프랑 록키만ㅋㅋ",
          "original_form": "저 워킹으로 잠깐 다녀와서ㅋㅋ 벤프랑 록키만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043698.1.12",
          "form": "중도하차했어요ㅋㅋ",
          "original_form": "중도하차했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043698.1.13",
          "form": "왜요?? 힘들어서요?",
          "original_form": "왜요?? 힘들어서요?",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043698.1.14",
          "form": "나이많을때가서 친구들 결혼소식들으니까ㅋㅋ 조급해지더라고요ㅋㅋ",
          "original_form": "나이많을때가서 친구들 결혼소식들으니까ㅋㅋ 조급해지더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043698.1.15",
          "form": "그러니까 몸도 아프고ㅋㅋ",
          "original_form": "그러니까 몸도 아프고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043698.1.16",
          "form": "그러셨군요 ㅋㅋㅋㅋ",
          "original_form": "그러셨군요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043698.1.17",
          "form": "나이가 먹어서?",
          "original_form": "나이가 먹어서?",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043698.1.18",
          "form": "네ㅋㅋ막상 한국와서는 결혼생각 안했는데ㅋㅋ",
          "original_form": "네ㅋㅋ막상 한국와서는 결혼생각 안했는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043698.1.19",
          "form": "갑다기 록키 가셔서 그런 생각이 드신거군요 ㅋㅋㅋ",
          "original_form": "갑다기 록키 가셔서 그런 생각이 드신거군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:52"
        }
      ]
