      "utterance": [
        {
          "id": "MDRW2100047277.1.1",
          "form": "저도 그래서 홈트시작한거거든요 살도찌고 건강도 안좋아져서",
          "original_form": "저도 그래서 홈트시작한거거든요 살도찌고 건강도 안좋아져서",
          "speaker_id": "2",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100047277.1.2",
          "form": "맨날 새벽에 맛있는녀석들보다가 못참고 야식먹으니",
          "original_form": "맨날 새벽에 맛있는녀석들보다가 못참고 야식먹으니",
          "speaker_id": "1",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100047277.1.3",
          "form": "살이엄청찌더라구요ㅎㅎ",
          "original_form": "살이엄청찌더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100047277.1.4",
          "form": "요즘 케이블끊겨서 안보니 좀덜먹어요",
          "original_form": "요즘 케이블끊겨서 안보니 좀덜먹어요",
          "speaker_id": "1",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100047277.1.5",
          "form": "ㅋㅋㅋㅋㅋ맞아요 먹방이 무서워요 저도 먹방보는거 좋아했는데 끊엇어요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 먹방이 무서워요 저도 먹방보는거 좋아했는데 끊엇어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 02:53"
        },
        {
          "id": "MDRW2100047277.1.6",
          "form": "따로 드시는 영양제 있으세요?",
          "original_form": "따로 드시는 영양제 있으세요?",
          "speaker_id": "1",
          "time": "20210826 02:54"
        },
        {
          "id": "MDRW2100047277.1.7",
          "form": "종합비타민이랑 유산균이랑 오메가3 정도?",
          "original_form": "종합비타민이랑 유산균이랑 오메가3 정도?",
          "speaker_id": "2",
          "time": "20210826 02:55"
        },
        {
          "id": "MDRW2100047277.1.8",
          "form": "영양제 뭐드세요?",
          "original_form": "영양제 뭐드세요?",
          "speaker_id": "2",
          "time": "20210826 02:55"
        },
        {
          "id": "MDRW2100047277.1.9",
          "form": "다들 드시나봐요",
          "original_form": "다들 드시나봐요",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100047277.1.10",
          "form": "전 귀찮아서 안먹거든요ㅎㅎ",
          "original_form": "전 귀찮아서 안먹거든요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100047277.1.11",
          "form": "비타민사면 일주일정도 먹다가 날짜지나서 다버려요",
          "original_form": "비타민사면 일주일정도 먹다가 날짜지나서 다버려요",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100047277.1.12",
          "form": "유산균은 어디꺼드세요?",
          "original_form": "유산균은 어디꺼드세요?",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100047277.1.13",
          "form": "종근당꺼 먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "종근당꺼 먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100047277.1.14",
          "form": "락토핏이요?ㅎㅎ",
          "original_form": "락토핏이요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100047277.1.15",
          "form": "다그거드시네요",
          "original_form": "다그거드시네요",
          "speaker_id": "1",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100047277.1.16",
          "form": "네 ㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100047277.1.17",
          "form": "주황색은 한통 먹어보긴했는데",
          "original_form": "주황색은 한통 먹어보긴했는데",
          "speaker_id": "1",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100047277.1.18",
          "form": "유산균은 사람마다 맞는게 잇다고 해서 다른 유산균도 먹어봣는데",
          "original_form": "유산균은 사람마다 맞는게 잇다고 해서 다른 유산균도 먹어봣는데",
          "speaker_id": "2",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100047277.1.19",
          "form": "색깔 다양하던데 무슨차이일까요?ㅎ",
          "original_form": "색깔 다양하던데 무슨차이일까요?ㅎ",
          "speaker_id": "1",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100047277.1.20",
          "form": "그게 색깔마다 들어잇는게 달라요",
          "original_form": "그게 색깔마다 들어잇는게 달라요",
          "speaker_id": "2",
          "time": "20210826 03:03"
        },
        {
          "id": "MDRW2100047277.1.21",
          "form": "저도 정확하게는 모르지만ㅋㅋㅋ",
          "original_form": "저도 정확하게는 모르지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:03"
        },
        {
          "id": "MDRW2100047277.1.22",
          "form": "드시면 몸이 달라지는게 느껴져요?",
          "original_form": "드시면 몸이 달라지는게 느껴져요?",
          "speaker_id": "1",
          "time": "20210826 03:04"
        },
        {
          "id": "MDRW2100047277.1.23",
          "form": "화장실을 잘 가요 ㅋㅋㅋㅋㅋ",
          "original_form": "화장실을 잘 가요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:05"
        },
        {
          "id": "MDRW2100047277.1.24",
          "form": "전 그래서 다른건 안먹어도 유산균은 꼭 먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 그래서 다른건 안먹어도 유산균은 꼭 먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:06"
        },
        {
          "id": "MDRW2100047277.1.25",
          "form": "오호 변비에 효과가 확실히 있나요?",
          "original_form": "오호 변비에 효과가 확실히 있나요?",
          "speaker_id": "1",
          "time": "20210826 03:06"
        },
        {
          "id": "MDRW2100047277.1.26",
          "form": "전 별차이 못느끼겠더라구요 꾸준히 안먹어서 그런지",
          "original_form": "전 별차이 못느끼겠더라구요 꾸준히 안먹어서 그런지",
          "speaker_id": "1",
          "time": "20210826 03:06"
        },
        {
          "id": "MDRW2100047277.1.27",
          "form": "전 효과가 있더라구요",
          "original_form": "전 효과가 있더라구요",
          "speaker_id": "2",
