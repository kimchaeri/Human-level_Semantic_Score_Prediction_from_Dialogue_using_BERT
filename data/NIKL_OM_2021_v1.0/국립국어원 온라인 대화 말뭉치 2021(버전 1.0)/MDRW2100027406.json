      "utterance": [
        {
          "id": "MDRW2100027406.1.1",
          "form": "그래도 코로나 전에는 수영배웠었어요",
          "original_form": "그래도 코로나 전에는 수영배웠었어요",
          "speaker_id": "1",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100027406.1.2",
          "form": "수영좋죠.카로리소비가 많이되는 운동이죠~",
          "original_form": "수영좋죠.카로리소비가 많이되는 운동이죠~",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027406.1.3",
          "form": "접영에서 좌절했지만요ㅋ",
          "original_form": "접영에서 좌절했지만요ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027406.1.4",
          "form": "몸이 접혀지지 않더라고요",
          "original_form": "몸이 접혀지지 않더라고요",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027406.1.5",
          "form": "아~~ㅎ\n접형까지 가셨나요.",
          "original_form": "아~~ㅎ\n접형까지 가셨나요.",
          "speaker_id": "2",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100027406.1.6",
          "form": "하다가 안돼서 관뒀어요ㅋ",
          "original_form": "하다가 안돼서 관뒀어요ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100027406.1.7",
          "form": "네~ㅎ\n또 다른 운동 뭐 배우신거나 하고 계신거 있나요?",
          "original_form": "네~ㅎ\n또 다른 운동 뭐 배우신거나 하고 계신거 있나요?",
          "speaker_id": "2",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027406.1.8",
          "form": "아뇨 그거말곤 없네요\n요즘은 그냥 많이 걸으려고 해여",
          "original_form": "아뇨 그거말곤 없네요\n요즘은 그냥 많이 걸으려고 해여",
          "speaker_id": "1",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027406.1.9",
          "form": "집근처 산보 할만한 곳이 많이 있나요?",
          "original_form": "집근처 산보 할만한 곳이 많이 있나요?",
          "speaker_id": "2",
          "time": "20210902 12:39"
        },
        {
          "id": "MDRW2100027406.1.10",
          "form": "공원은 잇는데 요즘은 비가 자주와서ㅋ",
          "original_form": "공원은 잇는데 요즘은 비가 자주와서ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100027406.1.11",
          "form": "주몇회정도 걸어요?",
          "original_form": "주몇회정도 걸어요?",
          "speaker_id": "2",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027406.1.12",
          "form": "한시간정도씩 매일이요\n근데 비가 오니 좋네요ㅋㅋㅋㅋ",
          "original_form": "한시간정도씩 매일이요\n근데 비가 오니 좋네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100027406.1.13",
          "form": "일산에 지금 비가와요?",
          "original_form": "일산에 지금 비가와요?",
          "speaker_id": "2",
          "time": "20210902 12:49"
        },
        {
          "id": "MDRW2100027406.1.14",
          "form": "오늘은 날이 좋아요ㅋㅋㅋ",
          "original_form": "오늘은 날이 좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:49"
        },
        {
          "id": "MDRW2100027406.1.15",
          "form": "몇번 빠지면 하기가 싫더라고요",
          "original_form": "몇번 빠지면 하기가 싫더라고요",
          "speaker_id": "1",
          "time": "20210902 12:50"
        }
