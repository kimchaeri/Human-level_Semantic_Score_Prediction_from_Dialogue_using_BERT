      "utterance": [
        {
          "id": "MDRW2100039824.1.1",
          "form": "무슨 폐가개조한 숙박시설갔는데",
          "original_form": "무슨 폐가개조한 숙박시설갔는데",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039824.1.2",
          "form": "색다르더라규요",
          "original_form": "색다르더라규요",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039824.1.3",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039824.1.4",
          "form": "무섭겠는데요?",
          "original_form": "무섭겠는데요?",
          "speaker_id": "1",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039824.1.5",
          "form": "안은 그냥 깨끗했어요",
          "original_form": "안은 그냥 깨끗했어요",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039824.1.6",
          "form": "ㅋㅋㅋㅋ 귀신폐가는 아닌",
          "original_form": "ㅋㅋㅋㅋ 귀신폐가는 아닌",
          "speaker_id": "2",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039824.1.7",
          "form": "아 다행이네요ㅎㅎㅎ",
          "original_form": "아 다행이네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039824.1.8",
          "form": "한번쯤 체험..",
          "original_form": "한번쯤 체험..",
          "speaker_id": "1",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039824.1.9",
          "form": "음 전 무서울것 같아요ㅋㅋㅋ",
          "original_form": "음 전 무서울것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039824.1.10",
          "form": "저는 부산사는데",
          "original_form": "저는 부산사는데",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039824.1.11",
          "form": "흰여울문화마을 이쁘더라구요",
          "original_form": "흰여울문화마을 이쁘더라구요",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039824.1.12",
          "form": "엇 저도 부산살아요ㅎㅎㅎ",
          "original_form": "엇 저도 부산살아요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039824.1.13",
          "form": "심지어 영도 살아요ㅋㅋㅋㅋㅋ",
          "original_form": "심지어 영도 살아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039824.1.14",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039824.1.15",
          "form": "ㅋㅋㅋ여울길 밑어",
          "original_form": "ㅋㅋㅋ여울길 밑어",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039824.1.16",
          "form": "그럼 어디있어도 바다가 보이는가요?",
          "original_form": "그럼 어디있어도 바다가 보이는가요?",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039824.1.17",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039824.1.18",
          "form": "아 그쪽인거군요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그쪽인거군요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039824.1.19",
          "form": "충격",
          "original_form": "충격",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039824.1.20",
          "form": "저는 서면 근처라",
          "original_form": "저는 서면 근처라",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039824.1.21",
          "form": "마음먹고 바다가요",
          "original_form": "마음먹고 바다가요",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039824.1.22",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039824.1.23",
          "form": "그밑에는 안살구요 ㅋㅋㅋ",
          "original_form": "그밑에는 안살구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039824.1.24",
          "form": "거실에 누워있음 크루즈터미널이 있어서",
          "original_form": "거실에 누워있음 크루즈터미널이 있어서",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039824.1.25",
          "form": "동네가 짱이쁨",
          "original_form": "동네가 짱이쁨",
          "speaker_id": "2",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039824.1.26",
          "form": "큰배가 바다에 둥둥떠있는거 보고 이사왔어요ㅎㅎ",
          "original_form": "큰배가 바다에 둥둥떠있는거 보고 이사왔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039824.1.27",
          "form": "범일동살다가요ㅋㅋㅋ",
          "original_form": "범일동살다가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:55"
        }
      ]
