      "utterance": [
        {
          "id": "MDRW2100024126.1.1",
          "form": "출퇴근길에 차에서 라디오 듣거든요",
          "original_form": "출퇴근길에 차에서 라디오 듣거든요",
          "speaker_id": "1",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024126.1.2",
          "form": "아날로그 감성이 좋아요 ㅎㅎ",
          "original_form": "아날로그 감성이 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024126.1.3",
          "form": "야자할땐 자주들었는데",
          "original_form": "야자할땐 자주들었는데",
          "speaker_id": "2",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100024126.1.4",
          "form": "요즘은 폰으로 보는 라디오 ㅋㅋ",
          "original_form": "요즘은 폰으로 보는 라디오 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100024126.1.5",
          "form": "간혹 좋아하는 아티스트 나오면 봐요",
          "original_form": "간혹 좋아하는 아티스트 나오면 봐요",
          "speaker_id": "2",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100024126.1.6",
          "form": "아 요즘은 보는라디오죠 참 ㅋㅋ",
          "original_form": "아 요즘은 보는라디오죠 참 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100024126.1.7",
          "form": "라디오사연 당첨되신적있으세요?",
          "original_form": "라디오사연 당첨되신적있으세요?",
          "speaker_id": "2",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100024126.1.8",
          "form": "저 고3때요 ㅋㅋㅋ 벌써 십년도 더 된....",
          "original_form": "저 고3때요 ㅋㅋㅋ 벌써 십년도 더 된....",
          "speaker_id": "1",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024126.1.9",
          "form": "진짜요? 전 간혹 되더라구요 요즘은 미리 쓰는게아니라",
          "original_form": "진짜요? 전 간혹 되더라구요 요즘은 미리 쓰는게아니라",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024126.1.10",
          "form": "바로바로 채팅창에 올리면 되더라구요",
          "original_form": "바로바로 채팅창에 올리면 되더라구요",
          "speaker_id": "2",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024126.1.11",
          "form": "아 요즘은 문자로 보내자나요 ㅋㅋ",
          "original_form": "아 요즘은 문자로 보내자나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024126.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024126.1.13",
          "form": "전 문자보내도 안읽어주던데",
          "original_form": "전 문자보내도 안읽어주던데",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024126.1.14",
          "form": "쫌 우껴야지 읽어주는 것같아요",
          "original_form": "쫌 우껴야지 읽어주는 것같아요",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024126.1.15",
          "form": "요즘 스벅 커피같은거 보내주던데 읽어주질 않으니....",
          "original_form": "요즘 스벅 커피같은거 보내주던데 읽어주질 않으니....",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024126.1.16",
          "form": "몇번보내다가 포기 ㅋㅋ",
          "original_form": "몇번보내다가 포기 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024126.1.17",
          "form": "전 김치가 와서",
          "original_form": "전 김치가 와서",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024126.1.18",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024126.1.19",
          "form": "김치요?",
          "original_form": "김치요?",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024126.1.20",
          "form": "와 선물 진짜 보내주는구나",
          "original_form": "와 선물 진짜 보내주는구나",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024126.1.21",
          "form": "네 하필 김장한 다음 날 오더라구요",
          "original_form": "네 하필 김장한 다음 날 오더라구요",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100024126.1.22",
          "form": "헐 타이밍이....",
          "original_form": "헐 타이밍이....",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024126.1.23",
          "form": "그래도 뭔가 받으셨네요",
          "original_form": "그래도 뭔가 받으셨네요",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024126.1.24",
          "form": "네 좀 골치여서 다른사람에게 선물했지만요 ㅋㅋ",
          "original_form": "네 좀 골치여서 다른사람에게 선물했지만요 ㅋㅋ",
          "speaker_id": "2",
