      "utterance": [
        {
          "id": "MDRW2100040830.1.1",
          "form": "아침식사 하셨어요?ㅎㅎ",
          "original_form": "아침식사 하셨어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100040830.1.2",
          "form": "네 먹었어요  name1님도 드셨나요?",
          "original_form": "네 먹었어요  &name1&님도 드셨나요?",
          "speaker_id": "2",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100040830.1.3",
          "form": "네 저도 방금 아침식사했어요",
          "original_form": "네 저도 방금 아침식사했어요",
          "speaker_id": "1",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100040830.1.4",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 09:17"
        },
        {
          "id": "MDRW2100040830.1.5",
          "form": "그냥 집밥이요 아 어제 사온 잡채가 맛이 괜찮더라구요",
          "original_form": "그냥 집밥이요 아 어제 사온 잡채가 맛이 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040830.1.6",
          "form": "오 잡채 맛있겠네요",
          "original_form": "오 잡채 맛있겠네요",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040830.1.7",
          "form": "저도 잡채 좋아해요ㅎㅎ",
          "original_form": "저도 잡채 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040830.1.8",
          "form": "오 잡채 좋아하시는군요 전 잡채 입맛에 맞으면 잘 먹어요",
          "original_form": "오 잡채 좋아하시는군요 전 잡채 입맛에 맞으면 잘 먹어요",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100040830.1.9",
          "form": "느끼할 때도 있더라구요 그럴 때는 매운 잡채 생각나요",
          "original_form": "느끼할 때도 있더라구요 그럴 때는 매운 잡채 생각나요",
          "speaker_id": "2",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100040830.1.10",
          "form": "아 느끼하면 힘들죠",
          "original_form": "아 느끼하면 힘들죠",
          "speaker_id": "1",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100040830.1.11",
          "form": "매운 잡채도 좋아요! 고추잡채",
          "original_form": "매운 잡채도 좋아요! 고추잡채",
          "speaker_id": "1",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100040830.1.12",
          "form": "매운 것 잘 드세요?  전 입은 잘 먹는데 소화력이 떨어졌네요",
          "original_form": "매운 것 잘 드세요?  전 입은 잘 먹는데 소화력이 떨어졌네요",
          "speaker_id": "2",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100040830.1.13",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100040830.1.14",
          "form": "매운음식 먹으면 속이 불편하기도 하고",
          "original_form": "매운음식 먹으면 속이 불편하기도 하고",
          "speaker_id": "1",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100040830.1.15",
          "form": "그래서 요즘은 자제해요",
          "original_form": "그래서 요즘은 자제해요",
          "speaker_id": "1",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100040830.1.16",
          "form": "저도 줄였어요ㅜ 그래도 매운 음식이 맛있긴 해요",
          "original_form": "저도 줄였어요ㅜ 그래도 매운 음식이 맛있긴 해요",
          "speaker_id": "2",
          "time": "20210905 09:29"
        },
        {
          "id": "MDRW2100040830.1.17",
          "form": "네 매운음식 당길 때가 있죠",
          "original_form": "네 매운음식 당길 때가 있죠",
          "speaker_id": "1",
          "time": "20210905 09:30"
        },
        {
          "id": "MDRW2100040830.1.18",
          "form": "건강식도 맛나면 좋겠어요 ㅠ",
          "original_form": "건강식도 맛나면 좋겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 09:32"
        },
        {
          "id": "MDRW2100040830.1.19",
          "form": "요즘은 건강식도 맛있게 나오긴 하더라고요",
          "original_form": "요즘은 건강식도 맛있게 나오긴 하더라고요",
          "speaker_id": "1",
          "time": "20210905 09:33"
        },
        {
          "id": "MDRW2100040830.1.20",
          "form": "저녁 드셨나요?ㅎㅎ",
          "original_form": "저녁 드셨나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 19:42"
        }
      ]
