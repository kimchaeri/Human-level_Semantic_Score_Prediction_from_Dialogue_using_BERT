      "utterance": [
        {
          "id": "MDRW2100016043.1.1",
          "form": "전 잘 잃어버리는 편이라ㅡ",
          "original_form": "전 잘 잃어버리는 편이라ㅡ",
          "speaker_id": "2",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016043.1.2",
          "form": "작은 것은 좀 걱정되어요",
          "original_form": "작은 것은 좀 걱정되어요",
          "speaker_id": "2",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016043.1.3",
          "form": "그렇긴해요",
          "original_form": "그렇긴해요",
          "speaker_id": "1",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016043.1.4",
          "form": "저도 막 안경 맨날 찾으러 다녀요 ㅋㅋ",
          "original_form": "저도 막 안경 맨날 찾으러 다녀요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016043.1.5",
          "form": "뭐 든 저도 그래서 좀 걱정인데",
          "original_form": "뭐 든 저도 그래서 좀 걱정인데",
          "speaker_id": "1",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016043.1.6",
          "form": "통화시 ㅋㅋ 볼이 눌려져서 막 앱이 실행되고 이래서 ㅋㅋ",
          "original_form": "통화시 ㅋㅋ 볼이 눌려져서 막 앱이 실행되고 이래서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016043.1.7",
          "form": "편하게 통화 음악 듣고 싶어요 ㅎㅎ",
          "original_form": "편하게 통화 음악 듣고 싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016043.1.8",
          "form": "워치도 탐나고요",
          "original_form": "워치도 탐나고요",
          "speaker_id": "1",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016043.1.9",
          "form": "뭐 사고 싶으세요?",
          "original_form": "뭐 사고 싶으세요?",
          "speaker_id": "1",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016043.1.10",
          "form": "저는 워치",
          "original_form": "저는 워치",
          "speaker_id": "2",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016043.1.11",
          "form": "ㅋㅋ 워치 사면",
          "original_form": "ㅋㅋ 워치 사면",
          "speaker_id": "2",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016043.1.12",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "1",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016043.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016043.1.14",
          "form": "워치로 핸드폰 어딨는지 알 수 있어요ㅋ",
          "original_form": "워치로 핸드폰 어딨는지 알 수 있어요ㅋ",
          "speaker_id": "2",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016043.1.15",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016043.1.16",
          "form": "핸드폰 만날 찾아서ㅋㅋ",
          "original_form": "핸드폰 만날 찾아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016043.1.17",
          "form": "맨날  찾으러 돌아다녀요",
          "original_form": "맨날  찾으러 돌아다녀요",
          "speaker_id": "1",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016043.1.18",
          "form": "그게 짱좋죠 ㅋㅋ",
          "original_form": "그게 짱좋죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016043.1.19",
          "form": "아 요새 워치 버즈 이런거 갖고 싶네요",
          "original_form": "아 요새 워치 버즈 이런거 갖고 싶네요",
          "speaker_id": "1",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016043.1.20",
          "form": "돈돔 모아야겠어요 ㅋㅋ",
          "original_form": "돈돔 모아야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016043.1.21",
          "form": "돈좀",
          "original_form": "돈좀",
          "speaker_id": "1",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016043.1.22",
          "form": "우울할땐 역시 지름신이 최고ㅋ",
          "original_form": "우울할땐 역시 지름신이 최고ㅋ",
          "speaker_id": "2",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016043.1.23",
          "form": "애들꺼 사주느라 제껀 못샀네요",
          "original_form": "애들꺼 사주느라 제껀 못샀네요",
          "speaker_id": "1",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016043.1.24",
          "form": "어른이 되어도",
          "original_form": "어른이 되어도",
          "speaker_id": "2",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016043.1.25",
          "form": "더못사네요ㅋㅋ",
          "original_form": "더못사네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016043.1.26",
          "form": "맞아요 비도와서 우중충",
          "original_form": "맞아요 비도와서 우중충",
          "speaker_id": "1",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016043.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016043.1.28",
          "form": "막내 태블릿 주말에 사러 용산가요",
          "original_form": "막내 태블릿 주말에 사러 용산가요",
          "speaker_id": "1",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016043.1.29",
          "form": "제것도 사고파요 윽",
          "original_form": "제것도 사고파요 윽",
          "speaker_id": "1",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016043.1.30",
          "form": "ㅋㅋㅋㅋ 같이 쓰면 아무래도. .힘들죠?ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 같이 쓰면 아무래도. .힘들죠?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016043.1.31",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016043.1.32",
          "form": "태블릿은 어디거 사세요?",
          "original_form": "태블릿은 어디거 사세요?",
          "speaker_id": "2",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016043.1.33",
          "form": "우선순위가 밀려요",
          "original_form": "우선순위가 밀려요",
          "speaker_id": "1",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016043.1.34",
          "form": "애플과 삼성!",
          "original_form": "애플과 삼성!",
          "speaker_id": "2",
          "time": "20210826 08:52"
        },
        {
          "id": "MDRW2100016043.1.35",
          "form": "요즘 근데 삼성 제품 좋더라고요",
          "original_form": "요즘 근데 삼성 제품 좋더라고요",
          "speaker_id": "2",
          "time": "20210826 08:52"
        }
      ]
