      "utterance": [
        {
          "id": "MDRW2100011271.1.1",
          "form": "영화보는거 좋아하세요?",
          "original_form": "영화보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 10:32"
        },
        {
          "id": "MDRW2100011271.1.2",
          "form": "아니요 막 즐겨보는편은 아닌데 친구들이 추천해주는거 보면 재밌긴하더라구요",
          "original_form": "아니요 막 즐겨보는편은 아닌데 친구들이 추천해주는거 보면 재밌긴하더라구요",
          "speaker_id": "2",
          "time": "20210824 12:19"
        },
        {
          "id": "MDRW2100011271.1.3",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 12:19"
        },
        {
          "id": "MDRW2100011271.1.4",
          "form": "좋아해요 제가 찾아서도 보고 추천받아서도 봐요",
          "original_form": "좋아해요 제가 찾아서도 보고 추천받아서도 봐요",
          "speaker_id": "1",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100011271.1.5",
          "form": "저는 친구가 블랙위도우 보자고 해서 봤는데 재밌더라구요",
          "original_form": "저는 친구가 블랙위도우 보자고 해서 봤는데 재밌더라구요",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100011271.1.6",
          "form": "최근에 어떤거 보셨어요?",
          "original_form": "최근에 어떤거 보셨어요?",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100011271.1.7",
          "form": "크루엘라 봤어요",
          "original_form": "크루엘라 봤어요",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011271.1.8",
          "form": "물론 블랙위도우도 봤습니다",
          "original_form": "물론 블랙위도우도 봤습니다",
          "speaker_id": "1",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100011271.1.9",
          "form": "크루엘라도 재밌다그러던데 어떠셨어요?",
          "original_form": "크루엘라도 재밌다그러던데 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210824 13:58"
        },
        {
          "id": "MDRW2100011271.1.10",
          "form": "엠마스톤이 엄청 잘 어울리더라구요 여기를 잘해서 그런가",
          "original_form": "엠마스톤이 엄청 잘 어울리더라구요 여기를 잘해서 그런가",
          "speaker_id": "1",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100011271.1.11",
          "form": "마자요 보진않았지만 찰떡인거같더라구요",
          "original_form": "마자요 보진않았지만 찰떡인거같더라구요",
          "speaker_id": "2",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100011271.1.12",
          "form": "영화추천해주실거있으신가요?",
          "original_form": "영화추천해주실거있으신가요?",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011271.1.13",
          "form": "최근에 친구랑 그 시절 우리가 좋아했던 소녀 봤는데 좋더라구요",
          "original_form": "최근에 친구랑 그 시절 우리가 좋아했던 소녀 봤는데 좋더라구요",
          "speaker_id": "1",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100011271.1.14",
          "form": "혹시 너의 결혼식 안보셨다면 그걸로 보세요",
          "original_form": "혹시 너의 결혼식 안보셨다면 그걸로 보세요",
          "speaker_id": "1",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100011271.1.15",
          "form": "어떤배우 나오나요?",
          "original_form": "어떤배우 나오나요?",
          "speaker_id": "2",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100011271.1.16",
          "form": "우리나라 영화가 아니라 배우 정보는 잘 모르겠어요",
          "original_form": "우리나라 영화가 아니라 배우 정보는 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100011271.1.17",
          "form": "대신 여자주인공 배우 분이 배역에 너무 잘 맞아서 좋았어요",
          "original_form": "대신 여자주인공 배우 분이 배역에 너무 잘 맞아서 좋았어요",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100011271.1.18",
          "form": "아 우리나라꺼가 아니군요",
          "original_form": "아 우리나라꺼가 아니군요",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100011271.1.19",
          "form": "오호 한번 찾아서 봐야겠네요",
          "original_form": "오호 한번 찾아서 봐야겠네요",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100011271.1.20",
          "form": "ㅎㅎ 재밌게 보세요",
          "original_form": "ㅎㅎ 재밌게 보세요",
          "speaker_id": "1",
          "time": "20210825 08:25"
        }
      ]
    }
