        {
          "id": "MDRW2100005301.1.1",
          "form": "ㅋㅋㅋㅋㅋ 싸이월드 진짜 뭐냐",
          "original_form": "ㅋㅋㅋㅋㅋ 싸이월드 진짜 뭐냐",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005301.1.2",
          "form": "싸이월드부활한대",
          "original_form": "싸이월드부활한대",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005301.1.3",
          "form": "그얘기만 10년째인듯",
          "original_form": "그얘기만 10년째인듯",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005301.1.4",
          "form": "아하 그런가 우리의추억",
          "original_form": "아하 그런가 우리의추억",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005301.1.5",
          "form": "pc의 감성인데 스마트폰이라 잘될라나",
          "original_form": "pc의 감성인데 스마트폰이라 잘될라나",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005301.1.6",
          "form": "슴폰으로부활하는구나! 도토리",
          "original_form": "슴폰으로부활하는구나! 도토리",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005301.1.7",
          "form": "아닌가? pc만하긴 힘들듯",
          "original_form": "아닌가? pc만하긴 힘들듯",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005301.1.8",
          "form": "싸이월드 영어로 치면 쵸재깅",
          "original_form": "싸이월드 영어로 치면 쵸재깅",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005301.1.9",
          "form": "미니미 귀여운데",
          "original_form": "미니미 귀여운데",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005301.1.10",
          "form": "꾸미는 맛이 있지",
          "original_form": "꾸미는 맛이 있지",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005301.1.11",
          "form": "네이트온은 아직있잖아",
          "original_form": "네이트온은 아직있잖아",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005301.1.12",
          "form": "헉? 몰랐어",
          "original_form": "헉? 몰랐어",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
