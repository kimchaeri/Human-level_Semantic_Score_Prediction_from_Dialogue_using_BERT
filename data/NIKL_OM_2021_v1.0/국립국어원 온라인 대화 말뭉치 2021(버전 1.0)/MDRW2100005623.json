      "utterance": [
        {
          "id": "MDRW2100005623.1.1",
          "form": "쇼핑 언제 햇지",
          "original_form": "쇼핑 언제 햇지",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005623.1.2",
          "form": "나 당근마켓 애용한다",
          "original_form": "나 당근마켓 애용한다",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005623.1.3",
          "form": "다 팔았음",
          "original_form": "다 팔았음",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005623.1.4",
          "form": "당근마켓",
          "original_form": "당근마켓",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005623.1.5",
          "form": "깔긴했지만",
          "original_form": "깔긴했지만",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005623.1.6",
          "form": "뭔가를 팔아본 적이 없다",
          "original_form": "뭔가를 팔아본 적이 없다",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005623.1.7",
          "form": "파는 재미가 있지",
          "original_form": "파는 재미가 있지",
          "speaker_id": "1",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100005623.1.8",
          "form": "난 파는 재미는 모르지만 사는 재미는 잘 안다",
          "original_form": "난 파는 재미는 모르지만 사는 재미는 잘 안다",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100005623.1.9",
          "form": "소비 최고",
          "original_form": "소비 최고",
          "speaker_id": "1",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100005623.1.10",
          "form": "인정한다",
          "original_form": "인정한다",
          "speaker_id": "2",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100005623.1.11",
          "form": "오타쿠 물건 더 사고싶다",
          "original_form": "오타쿠 물건 더 사고싶다",
          "speaker_id": "2",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100005623.1.12",
          "form": "굿즈.. 근데 그거 다시 팔면 너무 헐값됨",
          "original_form": "굿즈.. 근데 그거 다시 팔면 너무 헐값됨",
          "speaker_id": "1",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100005623.1.13",
          "form": "앙스타 굿즈는 파는 게 이득 아닌가",
          "original_form": "앙스타 굿즈는 파는 게 이득 아닌가",
          "speaker_id": "2",
          "time": "20210822 20:39"
        },
        {
          "id": "MDRW2100005623.1.14",
          "form": "요즘같은때..",
          "original_form": "요즘같은때..",
          "speaker_id": "2",
          "time": "20210822 20:39"
        },
        {
          "id": "MDRW2100005623.1.15",
          "form": "캐릭터 바이 캐릭터..",
          "original_form": "캐릭터 바이 캐릭터..",
          "speaker_id": "1",
          "time": "20210822 20:39"
        },
        {
          "id": "MDRW2100005623.1.16",
          "form": "돈없을 때 카나타누이 팔면..",
          "original_form": "돈없을 때 카나타누이 팔면..",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.17",
          "form": "나 사실",
          "original_form": "나 사실",
          "speaker_id": "1",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.18",
          "form": "앙스타 누이 팔았다",
          "original_form": "앙스타 누이 팔았다",
          "speaker_id": "1",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.19",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.20",
          "form": "챠키?",
          "original_form": "챠키?",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.21",
          "form": "레이?",
          "original_form": "레이?",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.22",
          "form": "카오루?",
          "original_form": "카오루?",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.23",
          "form": "굿즈에 욕심이 떨어져서",
          "original_form": "굿즈에 욕심이 떨어져서",
          "speaker_id": "1",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.24",
          "form": "전부 팔았지",
          "original_form": "전부 팔았지",
          "speaker_id": "1",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.25",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.26",
          "form": "진짜녀",
          "original_form": "진짜녀",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.27",
          "form": "와..",
          "original_form": "와..",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.28",
          "form": "카나타 이제 친없",
          "original_form": "카나타 이제 친없",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.29",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005623.1.30",
          "form": "무소유의 삶을 살거야",
          "original_form": "무소유의 삶을 살거야",
          "speaker_id": "1",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100005623.1.31",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100005623.1.32",
          "form": "무소유라니",
          "original_form": "무소유라니",
          "speaker_id": "2",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100005623.1.33",
          "form": "사람은 쇼핑할 수 밖에 없다",
          "original_form": "사람은 쇼핑할 수 밖에 없다",
          "speaker_id": "2",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100005623.1.34",
          "form": "히로아카 만화책도 팔았고",
          "original_form": "히로아카 만화책도 팔았고",
          "speaker_id": "1",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100005623.1.35",
          "form": "인피니트 앨범도 팔았음",
          "original_form": "인피니트 앨범도 팔았음",
          "speaker_id": "1",
          "time": "20210822 20:41"
        }
      ]
    }
