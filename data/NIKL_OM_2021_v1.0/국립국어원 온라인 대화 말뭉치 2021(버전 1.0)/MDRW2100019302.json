      "utterance": [
        {
          "id": "MDRW2100019302.1.1",
          "form": "타타라스나",
          "original_form": "타타라스나",
          "speaker_id": "2",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100019302.1.2",
          "form": "이거 이번 게임에서 나름 공들여 만든 컨텐츠인듯",
          "original_form": "이거 이번 게임에서 나름 공들여 만든 컨텐츠인듯",
          "speaker_id": "1",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019302.1.3",
          "form": "아 그래??",
          "original_form": "아 그래??",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019302.1.4",
          "form": "이것도 혹시",
          "original_form": "이것도 혹시",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019302.1.5",
          "form": "복잡한 문제풀이같은거",
          "original_form": "복잡한 문제풀이같은거",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019302.1.6",
          "form": "해야돼나요",
          "original_form": "해야돼나요",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019302.1.7",
          "form": "퍼즐 좀 있고",
          "original_form": "퍼즐 좀 있고",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019302.1.8",
          "form": "중요한건",
          "original_form": "중요한건",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019302.1.9",
          "form": "하루에 다 못깨",
          "original_form": "하루에 다 못깨",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019302.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019302.1.11",
          "form": "4일인가 5일에 걸쳐서",
          "original_form": "4일인가 5일에 걸쳐서",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019302.1.12",
          "form": "하루에 하나씩 퀘스트가 열림",
          "original_form": "하루에 하나씩 퀘스트가 열림",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019302.1.13",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019302.1.14",
          "form": "진짜 대형퀘네",
          "original_form": "진짜 대형퀘네",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019302.1.15",
          "form": "퍼즐 빡세?",
          "original_form": "퍼즐 빡세?",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019302.1.16",
          "form": "아니 퍼즐은 그닥 안빡세",
          "original_form": "아니 퍼즐은 그닥 안빡세",
          "speaker_id": "1",
          "time": "20210827 17:52"
        },
        {
          "id": "MDRW2100019302.1.17",
          "form": "대신 전투가 좀 빡센 느낌이었음",
          "original_form": "대신 전투가 좀 빡센 느낌이었음",
          "speaker_id": "1",
          "time": "20210827 17:52"
        },
        {
          "id": "MDRW2100019302.1.18",
          "form": "ㅇㅎ......",
          "original_form": "ㅇㅎ......",
          "speaker_id": "2",
          "time": "20210827 17:52"
        },
        {
          "id": "MDRW2100019302.1.19",
          "form": "큰일낫다",
          "original_form": "큰일낫다",
          "speaker_id": "2",
          "time": "20210827 17:52"
        },
        {
          "id": "MDRW2100019302.1.20",
          "form": "나 이제 슬슬 5성없는덱 투력차 느끼고있는데",
          "original_form": "나 이제 슬슬 5성없는덱 투력차 느끼고있는데",
          "speaker_id": "2",
          "time": "20210827 17:53"
        },
        {
          "id": "MDRW2100019302.1.21",
          "form": "일단 공격력 아이템 먹고",
          "original_form": "일단 공격력 아이템 먹고",
          "speaker_id": "1",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019302.1.22",
          "form": "방어템도 먹고",
          "original_form": "방어템도 먹고",
          "speaker_id": "1",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019302.1.23",
          "form": "방어랑 공격",
          "original_form": "방어랑 공격",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019302.1.24",
          "form": "같이 못쓸걸?",
          "original_form": "같이 못쓸걸?",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019302.1.25",
          "form": "물약을 먹어야겟지...",
          "original_form": "물약을 먹어야겟지...",
          "speaker_id": "2",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019302.1.26",
          "form": "아 그래???",
          "original_form": "아 그래???",
          "speaker_id": "1",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100019302.1.27",
          "form": "난 당연히 따로 적용되는줄...",
          "original_form": "난 당연히 따로 적용되는줄...",
          "speaker_id": "1",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100019302.1.28",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019302.1.29",
          "form": "그 머지",
          "original_form": "그 머지",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019302.1.30",
          "form": "스테미너음식이랑은",
          "original_form": "스테미너음식이랑은",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019302.1.31",
          "form": "중복적용되는데",
          "original_form": "중복적용되는데",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019302.1.32",
          "form": "그거는 안되더라구",
          "original_form": "그거는 안되더라구",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019302.1.33",
          "form": "아 그 효과표시 하나 뜨면 한번만 적용되는거야?",
          "original_form": "아 그 효과표시 하나 뜨면 한번만 적용되는거야?",
          "speaker_id": "1",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019302.1.34",
          "form": "방어랑 공격은 따로따로 표시 뜨지 않아?",
          "original_form": "방어랑 공격은 따로따로 표시 뜨지 않아?",
          "speaker_id": "1",
          "time": "20210827 18:01"
        },
        {
          "id": "MDRW2100019302.1.35",
          "form": "방어 먹고 공격먹으면",
          "original_form": "방어 먹고 공격먹으면",
          "speaker_id": "2",
          "time": "20210827 18:01"
        },
        {
          "id": "MDRW2100019302.1.36",
          "form": "후에 먹은것만",
          "original_form": "후에 먹은것만",
          "speaker_id": "2",
          "time": "20210827 18:01"
        },
        {
          "id": "MDRW2100019302.1.37",
          "form": "뜨지 않아?",
          "original_form": "뜨지 않아?",
          "speaker_id": "2",
          "time": "20210827 18:01"
        },
        {
          "id": "MDRW2100019302.1.38",
          "form": "둘이 문양이 달라서 따로따로 떴던거 같은데",
          "original_form": "둘이 문양이 달라서 따로따로 떴던거 같은데",
          "speaker_id": "1",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100019302.1.39",
          "form": "방어템 먹어본지 너무 오래돼서 가물가물 하다.",
          "original_form": "방어템 먹어본지 너무 오래돼서 가물가물 하다.",
          "speaker_id": "1",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100019302.1.40",
          "form": "이따가 함 해보지 머",
          "original_form": "이따가 함 해보지 머",
          "speaker_id": "2",
          "time": "20210827 18:07"
        }
      ]
    }
