      "utterance": [
        {
          "id": "MDRW2100006847.1.1",
          "form": "티비프로 뭐 보시나요",
          "original_form": "티비프로 뭐 보시나요",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006847.1.2",
          "form": "예전에 무한도전은",
          "original_form": "예전에 무한도전은",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006847.1.3",
          "form": "꼬박꼬박 챙겨봤는데",
          "original_form": "꼬박꼬박 챙겨봤는데",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006847.1.4",
          "form": "무도 끝나고 나서",
          "original_form": "무도 끝나고 나서",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006847.1.5",
          "form": "따로 챙겨보는 프로는 없네요",
          "original_form": "따로 챙겨보는 프로는 없네요",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006847.1.6",
          "form": "놀면뭐하니는요??",
          "original_form": "놀면뭐하니는요??",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006847.1.7",
          "form": "따로 챙겨보지는 않아요",
          "original_form": "따로 챙겨보지는 않아요",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006847.1.8",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006847.1.9",
          "form": "그래도 최른",
          "original_form": "그래도 최른",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006847.1.10",
          "form": "최근",
          "original_form": "최근",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006847.1.11",
          "form": "정준하 하하 나와서",
          "original_form": "정준하 하하 나와서",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006847.1.12",
          "form": "앵커부분 잠깐 봤네요",
          "original_form": "앵커부분 잠깐 봤네요",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006847.1.13",
          "form": "찾아보긴 했어요",
          "original_form": "찾아보긴 했어요",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006847.1.14",
          "form": "그 정과장 나오는거랑",
          "original_form": "그 정과장 나오는거랑",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006847.1.15",
          "form": "탁구도 봤어요",
          "original_form": "탁구도 봤어요",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006847.1.16",
          "form": "아 신유빈선수 나오구오?",
          "original_form": "아 신유빈선수 나오구오?",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006847.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006847.1.18",
          "form": "정말 옛날 무도색이 나서",
          "original_form": "정말 옛날 무도색이 나서",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006847.1.19",
          "form": "반갑더라고요",
          "original_form": "반갑더라고요",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006847.1.20",
          "form": "그러셨군요 무도는 여러번 보셨겠어요",
          "original_form": "그러셨군요 무도는 여러번 보셨겠어요",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006847.1.21",
          "form": "말 그대로",
          "original_form": "말 그대로",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006847.1.22",
          "form": "밥 먹을때 브금처럼",
          "original_form": "밥 먹을때 브금처럼",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006847.1.23",
          "form": "틀어놓죠",
          "original_form": "틀어놓죠",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006847.1.24",
          "form": "레전드편은 뭐라고 생각하세요",
          "original_form": "레전드편은 뭐라고 생각하세요",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006847.1.25",
          "form": "개인적으로는",
          "original_form": "개인적으로는",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006847.1.26",
          "form": "서해안 고속도로 가요제요",
          "original_form": "서해안 고속도로 가요제요",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006847.1.27",
          "form": "아 그당시 누가 나왔나요 가수들",
          "original_form": "아 그당시 누가 나왔나요 가수들",
          "speaker_id": "1",
          "time": "20210908 10:54"
        }
      ]
    }
