      "utterance": [
        {
          "id": "MDRW2100024561.1.1",
          "form": "안녕하세요~ 식사하셨나요?",
          "original_form": "안녕하세요~ 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210831 14:11"
        },
        {
          "id": "MDRW2100024561.1.2",
          "form": "네네~저 지금 커피마셔요",
          "original_form": "네네~저 지금 커피마셔요",
          "speaker_id": "2",
          "time": "20210831 14:13"
        },
        {
          "id": "MDRW2100024561.1.3",
          "form": "커피 좋아하시나요?",
          "original_form": "커피 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 14:13"
        },
        {
          "id": "MDRW2100024561.1.4",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "1",
          "time": "20210831 14:13"
        },
        {
          "id": "MDRW2100024561.1.5",
          "form": "아메리카노 자주 마셔요~",
          "original_form": "아메리카노 자주 마셔요~",
          "speaker_id": "1",
          "time": "20210831 14:13"
        },
        {
          "id": "MDRW2100024561.1.6",
          "form": "어떤 커피 드시고 계세요?",
          "original_form": "어떤 커피 드시고 계세요?",
          "speaker_id": "1",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100024561.1.7",
          "form": "바닐라라떼 먹고있어요!",
          "original_form": "바닐라라떼 먹고있어요!",
          "speaker_id": "2",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100024561.1.8",
          "form": "카페 보통 어디 많이가세요??",
          "original_form": "카페 보통 어디 많이가세요??",
          "speaker_id": "2",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100024561.1.9",
          "form": "저는 집 앞에 투썸이 있어서 투썸 자주가요",
          "original_form": "저는 집 앞에 투썸이 있어서 투썸 자주가요",
          "speaker_id": "1",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100024561.1.10",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100024561.1.11",
          "form": "아 전 스타벅스 많이가구요",
          "original_form": "아 전 스타벅스 많이가구요",
          "speaker_id": "2",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100024561.1.12",
          "form": "개인카페 돌아다니는거 좋아해요",
          "original_form": "개인카페 돌아다니는거 좋아해요",
          "speaker_id": "2",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100024561.1.13",
          "form": "혹시 빵 좋아하시나요?",
          "original_form": "혹시 빵 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100024561.1.14",
          "form": "분위기 좋은 개인 카페들 정말 많죠ㅎㅎ",
          "original_form": "분위기 좋은 개인 카페들 정말 많죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100024561.1.15",
          "form": "네 빵도 좋아해요",
          "original_form": "네 빵도 좋아해요",
          "speaker_id": "1",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100024561.1.16",
          "form": "아그럼 혹시 도넛 전문점들 가보셨나요?",
          "original_form": "아그럼 혹시 도넛 전문점들 가보셨나요?",
          "speaker_id": "2",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100024561.1.17",
          "form": "아뇨ㅠㅜ 가보고 싶은데 아직이요",
          "original_form": "아뇨ㅠㅜ 가보고 싶은데 아직이요",
          "speaker_id": "1",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100024561.1.18",
          "form": "다녀오신 곳 있나요?",
          "original_form": "다녀오신 곳 있나요?",
          "speaker_id": "1",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100024561.1.19",
          "form": "아 저도 가본적은 없구 그냥 tv에서 요즘 유행이라길래 여쭤봤어요",
          "original_form": "아 저도 가본적은 없구 그냥 tv에서 요즘 유행이라길래 여쭤봤어요",
          "speaker_id": "2",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100024561.1.20",
          "form": "그럼 보통 디저트는 뭐 같이 드세요?",
          "original_form": "그럼 보통 디저트는 뭐 같이 드세요?",
          "speaker_id": "2",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100024561.1.21",
          "form": "치즈케이크 좋아해요~ 커피도 좋아하는데 얼그레이도 좋아해서 케이크랑 얼그레이 같이 먹는 거 좋아해요",
          "original_form": "치즈케이크 좋아해요~ 커피도 좋아하는데 얼그레이도 좋아해서 케이크랑 얼그레이 같이 먹는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100024561.1.22",
          "form": "아 넘 좋은 조합이네요 ㅠㅠ",
          "original_form": "아 넘 좋은 조합이네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100024561.1.23",
          "form": "맞아요ㅠㅠ 치즈케이크 너무 맛있어요",
          "original_form": "맞아요ㅠㅠ 치즈케이크 너무 맛있어요",
