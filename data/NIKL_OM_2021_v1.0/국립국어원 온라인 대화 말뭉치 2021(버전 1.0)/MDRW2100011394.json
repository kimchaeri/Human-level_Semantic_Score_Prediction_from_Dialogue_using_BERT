      "utterance": [
        {
          "id": "MDRW2100011394.1.1",
          "form": "최근에 사신것있나요?",
          "original_form": "최근에 사신것있나요?",
          "speaker_id": "1",
          "time": "20210824 11:59"
        },
        {
          "id": "MDRW2100011394.1.2",
          "form": "최근에는 반지를 샀어요",
          "original_form": "최근에는 반지를 샀어요",
          "speaker_id": "2",
          "time": "20210824 11:59"
        },
        {
          "id": "MDRW2100011394.1.3",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "1",
          "time": "20210824 12:19"
        },
        {
          "id": "MDRW2100011394.1.4",
          "form": "어떤건가요?",
          "original_form": "어떤건가요?",
          "speaker_id": "1",
          "time": "20210824 12:19"
        },
        {
          "id": "MDRW2100011394.1.5",
          "form": "친구랑 각인반지하나했어요ㅎㅎ",
          "original_form": "친구랑 각인반지하나했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100011394.1.6",
          "form": "누누님은요?",
          "original_form": "누누님은요?",
          "speaker_id": "2",
          "time": "20210824 12:26"
        },
        {
          "id": "MDRW2100011394.1.7",
          "form": "와우 친한친구인가봐요? 커플링도끼고",
          "original_form": "와우 친한친구인가봐요? 커플링도끼고",
          "speaker_id": "1",
          "time": "20210824 12:31"
        },
        {
          "id": "MDRW2100011394.1.8",
          "form": "저는 운동화요!",
          "original_form": "저는 운동화요!",
          "speaker_id": "1",
          "time": "20210824 12:31"
        },
        {
          "id": "MDRW2100011394.1.9",
          "form": "오 운동화 혹시 모으시는 편이세요?",
          "original_form": "오 운동화 혹시 모으시는 편이세요?",
          "speaker_id": "2",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100011394.1.10",
          "form": "아뇨ㅋㅋㅋㅋ",
          "original_form": "아뇨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100011394.1.11",
          "form": "최근에 바꿀때돼서 구매했답니다ㅎㅎ",
          "original_form": "최근에 바꿀때돼서 구매했답니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100011394.1.12",
          "form": "ㅋㅋ 그러시구나",
          "original_form": "ㅋㅋ 그러시구나",
          "speaker_id": "2",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011394.1.13",
          "form": "저도 운동화좋아하는데요",
          "original_form": "저도 운동화좋아하는데요",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100011394.1.14",
          "form": "모아보니 다 흰색이더라구요ㅋㅋ",
          "original_form": "모아보니 다 흰색이더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100011394.1.15",
          "form": "ㅋㅋㅋ저도주로",
          "original_form": "ㅋㅋㅋ저도주로",
          "speaker_id": "1",
          "time": "20210824 13:18"
        },
        {
          "id": "MDRW2100011394.1.16",
          "form": "검은거보다 흰색이 낫더라고요",
          "original_form": "검은거보다 흰색이 낫더라고요",
          "speaker_id": "1",
          "time": "20210824 13:18"
        },
        {
          "id": "MDRW2100011394.1.17",
          "form": "흰색이 어디에나 어울리니까요 ㅎㅎ",
          "original_form": "흰색이 어디에나 어울리니까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011394.1.18",
          "form": "반지는 생각보다 저렴하게샀어요 원하는 문구를 각인해도 2만원이 안넘더라고요",
          "original_form": "반지는 생각보다 저렴하게샀어요 원하는 문구를 각인해도 2만원이 안넘더라고요",
          "speaker_id": "2",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100011394.1.19",
          "form": "와우 정말",
          "original_form": "와우 정말",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011394.1.20",
          "form": "저렴하게 잘나오네요?",
          "original_form": "저렴하게 잘나오네요?",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011394.1.21",
          "form": "네 일단 실물을 받아봐야 알겠는데",
          "original_form": "네 일단 실물을 받아봐야 알겠는데",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100011394.1.22",
          "form": "휘뚜루마뚜루 하고 다니기에는 좋을것 같아요ㅎㅎ",
          "original_form": "휘뚜루마뚜루 하고 다니기에는 좋을것 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100011394.1.23",
          "form": "ㅋㅋㅋ 휘뚜루템이 최고죠",
          "original_form": "ㅋㅋㅋ 휘뚜루템이 최고죠",
          "speaker_id": "1",
          "time": "20210824 13:31"
        }
      ]
