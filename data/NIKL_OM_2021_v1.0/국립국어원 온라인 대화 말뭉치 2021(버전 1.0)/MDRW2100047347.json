        },
        {
          "id": "MDRW2100047347.1.3",
          "form": "저는 겨울을 좋아해요",
          "original_form": "저는 겨울을 좋아해요",
          "speaker_id": "2",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100047347.1.4",
          "form": "이유는요?",
