      "utterance": [
        {
          "id": "MDRW2100016300.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016300.1.2",
          "form": "하시는 커뮤니티 있으세요?",
          "original_form": "하시는 커뮤니티 있으세요?",
          "speaker_id": "1",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016300.1.3",
          "form": "안녕하세욥",
          "original_form": "안녕하세욥",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016300.1.4",
          "form": "저 축구커뮤합니다 ㅋㅋ",
          "original_form": "저 축구커뮤합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016300.1.5",
          "form": "그럼 실제로 만나서 축구 시합도 하고 그러세요?",
          "original_form": "그럼 실제로 만나서 축구 시합도 하고 그러세요?",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016300.1.6",
          "form": "ㅋㅋ아뇨 친목행위 금지 카페라서요",
          "original_form": "ㅋㅋ아뇨 친목행위 금지 카페라서요",
          "speaker_id": "2",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016300.1.7",
          "form": "아하 저도 친목행위 금지 커뮤니티 하는데 어차피 익명이라서ㅋㅋ",
          "original_form": "아하 저도 친목행위 금지 커뮤니티 하는데 어차피 익명이라서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016300.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016300.1.9",
          "form": "저는 축구중계 단체관람같은거 하고싶어서",
          "original_form": "저는 축구중계 단체관람같은거 하고싶어서",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016300.1.10",
          "form": "친목되는 비슷한카페도 같이하고있어요ㅋㅋ",
          "original_form": "친목되는 비슷한카페도 같이하고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016300.1.11",
          "form": "커뮤니티 활용 되게 잘 하시는 거 같아요ㅋㅋㅋ",
          "original_form": "커뮤니티 활용 되게 잘 하시는 거 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016300.1.12",
          "form": "저는 화장품 얘기하는 커뮤니티합니당ㅋㅋ",
          "original_form": "저는 화장품 얘기하는 커뮤니티합니당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016300.1.13",
          "form": "오..화장품이요?",
          "original_form": "오..화장품이요?",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016300.1.14",
          "form": "정보 공유 커뮤인가봐요",
          "original_form": "정보 공유 커뮤인가봐요",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016300.1.15",
          "form": "넹 서로 발색샷 올리고 화장품 추천해 주고 그래요 축구랑은 공통되는 분모가 하나도 없네요ㅠㅠㅋㅋ",
          "original_form": "넹 서로 발색샷 올리고 화장품 추천해 주고 그래요 축구랑은 공통되는 분모가 하나도 없네요ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016300.1.16",
          "form": "ㅋㅋㅋㅋㅋ어쩔수없죵",
          "original_form": "ㅋㅋㅋㅋㅋ어쩔수없죵",
          "speaker_id": "2",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016300.1.17",
          "form": "화장품은 아니지만",
          "original_form": "화장품은 아니지만",
          "speaker_id": "2",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016300.1.18",
          "form": "저도 그런 류의 카페에서 어머님께 드릴 선물 정보얻었었어욥",
          "original_form": "저도 그런 류의 카페에서 어머님께 드릴 선물 정보얻었었어욥",
          "speaker_id": "2",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100016300.1.19",
          "form": "아 종종 여자 친구나 부모님 선물 물어보시는 분들이 계시긴 한데 그 상황이셨군요!ㅋㅋ",
          "original_form": "아 종종 여자 친구나 부모님 선물 물어보시는 분들이 계시긴 한데 그 상황이셨군요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100016300.1.20",
          "form": "ㅋㅋㅋ넵ㅋㅋ",
          "original_form": "ㅋㅋㅋ넵ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016300.1.21",
          "form": "그리고 제가 피부가 좀 예민해서",
          "original_form": "그리고 제가 피부가 좀 예민해서",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016300.1.22",
          "form": "저자극 썬크림 정보도 얻고요ㅋㅋ",
          "original_form": "저자극 썬크림 정보도 얻고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016300.1.23",
          "form": "저자극 선크림이면 무기자차가 좋아용!",
          "original_form": "저자극 선크림이면 무기자차가 좋아용!",
          "speaker_id": "1",
          "time": "20210826 17:41"
        }
      ]
    }
