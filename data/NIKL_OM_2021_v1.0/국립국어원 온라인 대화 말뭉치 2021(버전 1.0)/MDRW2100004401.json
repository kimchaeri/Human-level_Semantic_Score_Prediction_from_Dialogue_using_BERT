      "utterance": [
        {
          "id": "MDRW2100004401.1.1",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004401.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004401.1.3",
          "form": "주제가 종교네요! 종교가 있으신가요!?",
          "original_form": "주제가 종교네요! 종교가 있으신가요!?",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004401.1.4",
          "form": "음 없어요",
          "original_form": "음 없어요",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100004401.1.5",
          "form": "할머니는 성당 다니시고",
          "original_form": "할머니는 성당 다니시고",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100004401.1.6",
          "form": "외할머니는 절 다니시는데",
          "original_form": "외할머니는 절 다니시는데",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100004401.1.7",
          "form": "저와 부모님은 모교예요",
          "original_form": "저와 부모님은 모교예요",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100004401.1.8",
          "form": "아니 무교요",
          "original_form": "아니 무교요",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100004401.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100004401.1.10",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100004401.1.11",
          "form": "오오 저는 교회다녀요!",
          "original_form": "오오 저는 교회다녀요!",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100004401.1.12",
          "form": "외가는 기독교고 친가는 불교인데 이렇게 되었답니다ㅋㅋㅋ",
          "original_form": "외가는 기독교고 친가는 불교인데 이렇게 되었답니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100004401.1.13",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100004401.1.14",
          "form": "비슷하시네요",
          "original_form": "비슷하시네요",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100004401.1.15",
          "form": "교회 저는 한번도 안 가봐서 궁금해요",
          "original_form": "교회 저는 한번도 안 가봐서 궁금해요",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100004401.1.16",
          "form": "가면 뭐해요?",
          "original_form": "가면 뭐해요?",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100004401.1.17",
          "form": "찬양하고 설교듣고 기도하고 찬양또하고 하다보면 예배는 끝나고",
          "original_form": "찬양하고 설교듣고 기도하고 찬양또하고 하다보면 예배는 끝나고",
          "speaker_id": "2",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100004401.1.18",
          "form": "요즘은 코로나라 안하지만 예배끝나면 모임을 해요!",
          "original_form": "요즘은 코로나라 안하지만 예배끝나면 모임을 해요!",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100004401.1.19",
          "form": "신기해요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "신기해요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100004401.1.20",
          "form": "절은 가봤는데 교회는 가본 적 없어서요",
          "original_form": "절은 가봤는데 교회는 가본 적 없어서요",
          "speaker_id": "1",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100004401.1.21",
          "form": "언제부터 다니셨어요?",
          "original_form": "언제부터 다니셨어요?",
          "speaker_id": "1",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100004401.1.22",
          "form": "저는 애기때부터 다녔어요! 모태신앙입니다!",
          "original_form": "저는 애기때부터 다녔어요! 모태신앙입니다!",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100004401.1.23",
          "form": "근데 절도 몇번 가본적은 있어요!",
          "original_form": "근데 절도 몇번 가본적은 있어요!",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100004401.1.24",
          "form": "와 모태신앙 대단해요",
          "original_form": "와 모태신앙 대단해요",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100004401.1.25",
          "form": "아직까지 꾸준하게 다니시는 거잖아요",
          "original_form": "아직까지 꾸준하게 다니시는 거잖아요",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100004401.1.26",
          "form": "절은 저도 가봤어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "절은 저도 가봤어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100004401.1.27",
          "form": "혹시 불국사도 절에 포함될까요?",
          "original_form": "혹시 불국사도 절에 포함될까요?",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100004401.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100004401.1.29",
          "form": "ㅋㅋㅋ불국사도 절 아닐까요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ불국사도 절 아닐까요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100004401.1.30",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100004401.1.31",
          "form": "그렇다면 몇 번 가봤네요ㅋㅋㅋㅋㅋ",
          "original_form": "그렇다면 몇 번 가봤네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100004401.1.32",
          "form": "모태신앙이시면 교회 친구도 있어요?",
          "original_form": "모태신앙이시면 교회 친구도 있어요?",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100004401.1.33",
          "form": "네네! 제일 친한 친구들도 교회친구들이에요!",
          "original_form": "네네! 제일 친한 친구들도 교회친구들이에요!",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100004401.1.34",
          "form": "어렸을 때부터 알고지냈어가지고!",
          "original_form": "어렸을 때부터 알고지냈어가지고!",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100004401.1.35",
          "form": "20년 지기들 ㅎㅎ",
          "original_form": "20년 지기들 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100004401.1.36",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100004401.1.37",
          "form": "그건 진짜 부럽네요",
          "original_form": "그건 진짜 부럽네요",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100004401.1.38",
          "form": "20년지기면 진짜",
          "original_form": "20년지기면 진짜",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100004401.1.39",
          "form": "아직도 친한 게 대단한 것 같아요",
          "original_form": "아직도 친한 게 대단한 것 같아요",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100004401.1.40",
          "form": "부모님들끼리도 아시겠네요",
          "original_form": "부모님들끼리도 아시겠네요",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100004401.1.41",
          "form": "교회 다니는 부모님들끼리는 교회에서 보면 인사하시는 정도...?",
          "original_form": "교회 다니는 부모님들끼리는 교회에서 보면 인사하시는 정도...?",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100004401.1.42",
          "form": "오히려 특별한 일 없이 매주 얼굴보게되니까",
          "original_form": "오히려 특별한 일 없이 매주 얼굴보게되니까",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100004401.1.43",
          "form": "오랜시간 우정 이어올 수 있었던 것 같아요!",
          "original_form": "오랜시간 우정 이어올 수 있었던 것 같아요!",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100004401.1.44",
          "form": "아 맞는 것 같아요",
          "original_form": "아 맞는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100004401.1.45",
          "form": "정말 어렸을 때부터 항상 붙어있었으면",
          "original_form": "정말 어렸을 때부터 항상 붙어있었으면",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100004401.1.46",
          "form": "오히려 멀어졌을 수도 있었겠네요",
          "original_form": "오히려 멀어졌을 수도 있었겠네요",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100004401.1.47",
          "form": "설교는 무슨 내용이에요?",
          "original_form": "설교는 무슨 내용이에요?",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100004401.1.48",
          "form": "보통 성경 내용을 가르치시거나",
          "original_form": "보통 성경 내용을 가르치시거나",
          "speaker_id": "2",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100004401.1.49",
          "form": "이렇게 이렇게 살아야한다~ 하는 취지의 내용을 성경 내용 베이스로 말씀하세요!ㅋㅋㅋ",
          "original_form": "이렇게 이렇게 살아야한다~ 하는 취지의 내용을 성경 내용 베이스로 말씀하세요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100004401.1.50",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100004401.1.51",
          "form": "들으면 좋을 것 같아요",
          "original_form": "들으면 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100004401.1.52",
          "form": "자기 계발서를 음향으로 듣는 거네요",
          "original_form": "자기 계발서를 음향으로 듣는 거네요",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100004401.1.53",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100004401.1.54",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100004401.1.55",
          "form": "그런 느낌인 날들도 있고 성경 공부하는 느낌인 날들도있고..ㅋㅋㅋ",
          "original_form": "그런 느낌인 날들도 있고 성경 공부하는 느낌인 날들도있고..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100004401.1.56",
          "form": "같은 과목도 인강강사에 따라 재미나 성취도가 달라지듯이",
          "original_form": "같은 과목도 인강강사에 따라 재미나 성취도가 달라지듯이",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100004401.1.57",
          "form": "설교도 좀 그런것 같아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "설교도 좀 그런것 같아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
