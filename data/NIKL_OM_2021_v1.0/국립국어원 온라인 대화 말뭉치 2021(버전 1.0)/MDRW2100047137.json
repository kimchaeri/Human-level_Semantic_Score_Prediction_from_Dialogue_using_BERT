      "utterance": [
        {
          "id": "MDRW2100047137.1.1",
          "form": "반려동물 키우고 계신거 있나요?",
          "original_form": "반려동물 키우고 계신거 있나요?",
          "speaker_id": "1",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100047137.1.2",
          "form": "저는금붕어요!!",
          "original_form": "저는금붕어요!!",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100047137.1.3",
          "form": "물고기 키우는게 의외로 손이 많이 간다던데",
          "original_form": "물고기 키우는게 의외로 손이 많이 간다던데",
          "speaker_id": "1",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047137.1.4",
          "form": "맞아요진짜ㅠㅠ물가는것도그렇고",
          "original_form": "맞아요진짜ㅠㅠ물가는것도그렇고",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047137.1.5",
          "form": "필요한 기구도 많죠? 수조 안에 뭔가 많이 들어가더라구요",
          "original_form": "필요한 기구도 많죠? 수조 안에 뭔가 많이 들어가더라구요",
          "speaker_id": "1",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100047137.1.6",
          "form": "동물키우세요??",
          "original_form": "동물키우세요??",
          "speaker_id": "2",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100047137.1.7",
          "form": "저는 키워본적이 없어요",
          "original_form": "저는 키워본적이 없어요",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100047137.1.8",
          "form": "키운다면 웰시코기 키워보고 싶어요",
          "original_form": "키운다면 웰시코기 키워보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100047137.1.9",
          "form": "아정말요?? 저도여ㅠㅠㅠ 진짜강아지 너무 귀엽지않나요?",
          "original_form": "아정말요?? 저도여ㅠㅠㅠ 진짜강아지 너무 귀엽지않나요?",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100047137.1.10",
          "form": "진짜 귀여워요",
          "original_form": "진짜 귀여워요",
