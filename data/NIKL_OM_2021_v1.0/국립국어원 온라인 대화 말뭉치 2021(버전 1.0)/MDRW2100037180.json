        },
        {
          "id": "MDRW2100037180.1.10",
          "form": "거기서 파는건데 네이버에 레디톡 치면 나와용",
          "original_form": "거기서 파는건데 네이버에 레디톡 치면 나와용",
          "speaker_id": "2",
