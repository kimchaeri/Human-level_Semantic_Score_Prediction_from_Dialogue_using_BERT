      "utterance": [
        {
          "id": "MDRW2100022015.1.1",
          "form": "name2 머 먹는 거 조아해",
          "original_form": "&name2& 머 먹는 거 조아해",
          "speaker_id": "1",
          "time": "20210829 00:54"
        },
        {
          "id": "MDRW2100022015.1.2",
          "form": "우리 반말하자어때?",
          "original_form": "우리 반말하자어때?",
          "speaker_id": "2",
          "time": "20210829 00:54"
        },
        {
          "id": "MDRW2100022015.1.3",
          "form": "나는 마라탕!",
          "original_form": "나는 마라탕!",
          "speaker_id": "2",
          "time": "20210829 00:54"
        },
        {
          "id": "MDRW2100022015.1.4",
          "form": "네 선배님!!!",
          "original_form": "네 선배님!!!",
          "speaker_id": "1",
          "time": "20210829 00:55"
        },
        {
          "id": "MDRW2100022015.1.5",
          "form": "애신 사골 미연 골라",
          "original_form": "애신 사골 미연 골라",
          "speaker_id": "1",
          "time": "20210829 00:55"
        },
        {
          "id": "MDRW2100022015.1.6",
          "form": "사골!",
          "original_form": "사골!",
          "speaker_id": "2",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100022015.1.7",
          "form": "그 배달중에 108장 마라탕도 굿",
          "original_form": "그 배달중에 108장 마라탕도 굿",
          "speaker_id": "2",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100022015.1.8",
          "form": "하지만 사골은 못 이겨",
          "original_form": "하지만 사골은 못 이겨",
          "speaker_id": "2",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100022015.1.9",
          "form": "베이커데이가고파ㅠ",
          "original_form": "베이커데이가고파ㅠ",
          "speaker_id": "2",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100022015.1.10",
          "form": "헐 108장 마라샹궈 먹러봄?",
          "original_form": "헐 108장 마라샹궈 먹러봄?",
          "speaker_id": "1",
          "time": "20210829 00:59"
        },
        {
          "id": "MDRW2100022015.1.11",
          "form": "ㄹㅇ 맛잇어서 새벽 1시에",
          "original_form": "ㄹㅇ 맛잇어서 새벽 1시에",
          "speaker_id": "1",
          "time": "20210829 00:59"
        },
        {
          "id": "MDRW2100022015.1.12",
          "form": "시켜먹엇넛음",
          "original_form": "시켜먹엇넛음",
          "speaker_id": "1",
          "time": "20210829 00:59"
        },
        {
          "id": "MDRW2100022015.1.13",
          "form": "베이커데이 맛잇나? 덤도기 안 가봄",
          "original_form": "베이커데이 맛잇나? 덤도기 안 가봄",
          "speaker_id": "1",
          "time": "20210829 01:00"
        },
        {
          "id": "MDRW2100022015.1.14",
          "form": "마라샹궈 안조아해...나ㅠㅠ",
          "original_form": "마라샹궈 안조아해...나ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100022015.1.15",
          "form": "덤도기 나랑가여돼",
          "original_form": "덤도기 나랑가여돼",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100022015.1.16",
          "form": "헙 담에 같이 가준다",
          "original_form": "헙 담에 같이 가준다",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100022015.1.17",
          "form": "오늘 저녁은 머 머것서",
          "original_form": "오늘 저녁은 머 머것서",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100022015.1.18",
          "form": "회기버거!",
          "original_form": "회기버거!",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100022015.1.19",
          "form": "생각보다 가격값못해서 ..담엔 안먹게..",
          "original_form": "생각보다 가격값못해서 ..담엔 안먹게..",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100022015.1.20",
          "form": "허얼 진짜??",
          "original_form": "허얼 진짜??",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100022015.1.21",
          "form": "난 그 새우와사비 먹고",
          "original_form": "난 그 새우와사비 먹고",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100022015.1.22",
          "form": "푹 빠져서 맨날 시켜먹엇어",
          "original_form": "푹 빠져서 맨날 시켜먹엇어",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100022015.1.23",
          "form": "응 그거!!",
          "original_form": "응 그거!!",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100022015.1.24",
          "form": "근데콜라까지 9400원주고는 못먹게쏘..",
          "original_form": "근데콜라까지 9400원주고는 못먹게쏘..",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100022015.1.25",
          "form": "허넉!!!! 나 기절혀",
          "original_form": "허넉!!!! 나 기절혀",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100022015.1.26",
          "form": "야채가 부족해서그럴수도",
          "original_form": "야채가 부족해서그럴수도",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100022015.1.27",
          "form": "name1 나 화목공강이야",
          "original_form": "&name1& 나 화목공강이야",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100022015.1.28",
          "form": "어...?",
          "original_form": "어...?",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100022015.1.29",
          "form": "그게.. 가능해..?",
          "original_form": "그게.. 가능해..?",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100022015.1.30",
          "form": "시리 야채짱걸인가요?",
          "original_form": "시리 야채짱걸인가요?",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100022015.1.31",
          "form": "야채 머 젤 좋아하는데",
          "original_form": "야채 머 젤 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100022015.1.32",
          "form": "네 야채짱걸",
          "original_form": "네 야채짱걸",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100022015.1.33",
          "form": "난 상.추",
          "original_form": "난 상.추",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100022015.1.34",
          "form": "양상추!",
          "original_form": "양상추!",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100022015.1.35",
          "form": "영파!",
          "original_form": "영파!",
          "speaker_id": "2",
          "time": "20210901 22:48"
        }
      ]
