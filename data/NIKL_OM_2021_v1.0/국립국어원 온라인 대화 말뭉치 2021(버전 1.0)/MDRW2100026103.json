      "utterance": [
        {
          "id": "MDRW2100026103.1.1",
          "form": "안녕하세요 저녁 드셨나요??",
          "original_form": "안녕하세요 저녁 드셨나요??",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026103.1.2",
          "form": "아직이요!",
          "original_form": "아직이요!",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026103.1.3",
          "form": "name1님께서는 저녁 드셨나요?",
          "original_form": "&name1&님께서는 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026103.1.4",
          "form": "헉 배고프시겠어요!",
          "original_form": "헉 배고프시겠어요!",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026103.1.5",
          "form": "저는 떡볶이 먹었어용",
          "original_form": "저는 떡볶이 먹었어용",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026103.1.6",
          "form": "헉, 맛있겠다.",
          "original_form": "헉, 맛있겠다.",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026103.1.7",
          "form": "어떤 떡볶이가 제일 좋으세요??",
          "original_form": "어떤 떡볶이가 제일 좋으세요??",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026103.1.8",
          "form": "엽떡이나, 신전이나 그런거요!",
          "original_form": "엽떡이나, 신전이나 그런거요!",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026103.1.9",
          "form": "저는 엽떡 좋아해요!",
          "original_form": "저는 엽떡 좋아해요!",
          "speaker_id": "1",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100026103.1.10",
          "form": "비싸긴 하지만 ㅠㅠ 너무 맛있어요",
          "original_form": "비싸긴 하지만 ㅠㅠ 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100026103.1.11",
          "form": "name2님은요???",
          "original_form": "&name2&님은요???",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100026103.1.12",
          "form": "엽떡 저도 좋아해요!",
          "original_form": "엽떡 저도 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100026103.1.13",
          "form": "최근에 허니콤보랑 같이 먹으면 더 맛있다고 들어서",
          "original_form": "최근에 허니콤보랑 같이 먹으면 더 맛있다고 들어서",
          "speaker_id": "2",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100026103.1.14",
          "form": "친구들이랑 같이 먹어봤는데 달고 맵고 해서 끝없이 들어가더라구요 ㅋㅋㅋ",
          "original_form": "친구들이랑 같이 먹어봤는데 달고 맵고 해서 끝없이 들어가더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100026103.1.15",
          "form": "맞아요 진짜 맛있겠더라구요",
          "original_form": "맞아요 진짜 맛있겠더라구요",
          "speaker_id": "1",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100026103.1.16",
          "form": "근데 저 허니콤보 한번도 안 먹어봤어용...",
          "original_form": "근데 저 허니콤보 한번도 안 먹어봤어용...",
          "speaker_id": "1",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100026103.1.17",
          "form": "맛있나요??",
          "original_form": "맛있나요??",
          "speaker_id": "1",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100026103.1.18",
          "form": "맛있어요!",
          "original_form": "맛있어요!",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100026103.1.19",
          "form": "근데 단거 안좋아하시면 조금 별로일 수도 있을 것 같아요...",
          "original_form": "근데 단거 안좋아하시면 조금 별로일 수도 있을 것 같아요...",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100026103.1.20",
          "form": "많이 단 간장치킨?? 정도에요",
          "original_form": "많이 단 간장치킨?? 정도에요",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100026103.1.21",
          "form": "오 그래서 떡볶이랑 잘 어울리나봐요",
          "original_form": "오 그래서 떡볶이랑 잘 어울리나봐요",
          "speaker_id": "1",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100026103.1.22",
          "form": "저도 나중에 꼭 같이 먹어볼게요!!",
          "original_form": "저도 나중에 꼭 같이 먹어볼게요!!",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100026103.1.23",
          "form": "3~4명이서 먹으면 양 딱 맞아요!",
          "original_form": "3~4명이서 먹으면 양 딱 맞아요!",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026103.1.24",
          "form": "다음에 꼭 드셔보세요",
          "original_form": "다음에 꼭 드셔보세요",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026103.1.25",
          "form": "넵 ㅎㅎ 조아요",
          "original_form": "넵 ㅎㅎ 조아요",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026103.1.26",
          "form": "혹시 아직도 저녁 안 드셨나요??",
          "original_form": "혹시 아직도 저녁 안 드셨나요??",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026103.1.27",
          "form": "네... 배달시키려고 하는데 친구들이랑 이야기하다가",
          "original_form": "네... 배달시키려고 하는데 친구들이랑 이야기하다가",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026103.1.28",
          "form": "까먹어서 아직...",
          "original_form": "까먹어서 아직...",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026103.1.29",
          "form": "아이고...",
          "original_form": "아이고...",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026103.1.30",
          "form": "얼른 뭐라도 시키세요!",
          "original_form": "얼른 뭐라도 시키세요!",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026103.1.31",
          "form": "배 많이 고프실 것 같은데 ㅠㅠ",
          "original_form": "배 많이 고프실 것 같은데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026103.1.32",
          "form": "방금 시켰어요!!",
          "original_form": "방금 시켰어요!!",
          "speaker_id": "2",
          "time": "20210901 21:00"
        }
      ]
    }
