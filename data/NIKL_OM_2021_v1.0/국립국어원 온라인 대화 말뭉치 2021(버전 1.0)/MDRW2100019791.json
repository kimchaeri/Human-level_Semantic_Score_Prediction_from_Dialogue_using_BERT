      "utterance": [
        {
          "id": "MDRW2100019791.1.1",
          "form": "종교있으신가요",
          "original_form": "종교있으신가요",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019791.1.2",
          "form": "아니요 저는 무교예요",
          "original_form": "아니요 저는 무교예요",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019791.1.3",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019791.1.4",
          "form": "저도 무교에요 ㅎㅎ어릴때 교회랑절은 가봤어요",
          "original_form": "저도 무교에요 ㅎㅎ어릴때 교회랑절은 가봤어요",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019791.1.5",
          "form": "가보신적있으세요",
          "original_form": "가보신적있으세요",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019791.1.6",
          "form": "저도 어릴때는 교회 많이 가봤어요",
          "original_form": "저도 어릴때는 교회 많이 가봤어요",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019791.1.7",
          "form": "먹을거주고 해서 갔었던거같아욬ㅋㅋ",
          "original_form": "먹을거주고 해서 갔었던거같아욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019791.1.8",
          "form": "교회가서 놀기도 하고ㅋㅋ",
          "original_form": "교회가서 놀기도 하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019791.1.9",
          "form": "네 저두요 ㅋㅋㅋㅋ어릴때 친구따라 과자준다해서 많이갔었어요",
          "original_form": "네 저두요 ㅋㅋㅋㅋ어릴때 친구따라 과자준다해서 많이갔었어요",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019791.1.10",
          "form": "근데 그러다보니 성가대도하고 그랬던...",
          "original_form": "근데 그러다보니 성가대도하고 그랬던...",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019791.1.11",
          "form": "와 성가대라니..\r\n노래 잘하셨나봐요!",
          "original_form": "와 성가대라니..\r\n노래 잘하셨나봐요!",
          "speaker_id": "2",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019791.1.12",
          "form": "아뇨 전혀요...초등부 성가대 립싱크했었어요 ㅋㅋㅋㅋ",
          "original_form": "아뇨 전혀요...초등부 성가대 립싱크했었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019791.1.13",
          "form": "아ㅋㅋㅋㅋ입모양만 따라하셨군요ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ입모양만 따라하셨군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019791.1.14",
          "form": "절이랑  교회중에",
          "original_form": "절이랑  교회중에",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019791.1.15",
          "form": "뭐가 더 맞는거 같은세요?",
          "original_form": "뭐가 더 맞는거 같은세요?",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019791.1.16",
          "form": "전 절이요 ㅎㅎ일단 분위기자체가 오든말든 상관안하는분위기라",
          "original_form": "전 절이요 ㅎㅎ일단 분위기자체가 오든말든 상관안하는분위기라",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019791.1.17",
          "form": "오~저두요",
          "original_form": "오~저두요",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019791.1.18",
          "form": "약간 관광지느낌도있고요 왠지모르게편해요 name2님도 절이더편하세요?",
          "original_form": "약간 관광지느낌도있고요 왠지모르게편해요 &name2&님도 절이더편하세요?",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019791.1.19",
          "form": "교회는 안나가면  막 찾아오구",
          "original_form": "교회는 안나가면  막 찾아오구",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019791.1.20",
          "form": "전도하고 그런게 저는 좀 안맞더라구요ㅠ",
          "original_form": "전도하고 그런게 저는 좀 안맞더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019791.1.21",
          "form": "마자요 절은 뭔가 마음이 편해져요",
          "original_form": "마자요 절은 뭔가 마음이 편해져요",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019791.1.22",
          "form": "맞아요 시간도 정해져있고..쫌 부담스러운",
          "original_form": "맞아요 시간도 정해져있고..쫌 부담스러운",
