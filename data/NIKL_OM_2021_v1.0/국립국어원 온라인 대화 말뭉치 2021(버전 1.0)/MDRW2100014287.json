      "utterance": [
        {
          "id": "MDRW2100014287.1.1",
          "form": "2개 있잖아",
          "original_form": "2개 있잖아",
          "speaker_id": "2",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014287.1.2",
          "form": "엉 캐리어 2개중에 내셔널 그거",
          "original_form": "엉 캐리어 2개중에 내셔널 그거",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014287.1.3",
          "form": "왜 사용하지",
          "original_form": "왜 사용하지",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014287.1.4",
          "form": "이게 수화물용이라 내가 쓰기에는 높이가 너무 크다",
          "original_form": "이게 수화물용이라 내가 쓰기에는 높이가 너무 크다",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014287.1.5",
          "form": "그렇겠네",
          "original_form": "그렇겠네",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014287.1.6",
          "form": "응 들려고 하니까 힘들더라 바퀴가 게단에 걸리고",
          "original_form": "응 들려고 하니까 힘들더라 바퀴가 게단에 걸리고",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014287.1.7",
          "form": "그렇지 키가 있으니",
          "original_form": "그렇지 키가 있으니",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014287.1.8",
          "form": "당근에 내놓았는데 사겠다는 사람이 있엇는데",
          "original_form": "당근에 내놓았는데 사겠다는 사람이 있엇는데",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014287.1.9",
          "form": "팔았어",
          "original_form": "팔았어",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014287.1.10",
          "form": "아니 가격을 너무 내려서 얘기해서 안판다고 했지",
          "original_form": "아니 가격을 너무 내려서 얘기해서 안판다고 했지",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014287.1.11",
          "form": "잘했다",
          "original_form": "잘했다",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014287.1.12",
          "form": "가격을 만원 올려서 팔아라고 해서",
          "original_form": "가격을 만원 올려서 팔아라고 해서",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014287.1.13",
          "form": "그래서 거래했어",
          "original_form": "그래서 거래했어",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014287.1.14",
          "form": "응 팔겠다고 했는데 갑자기 연락 두절",
          "original_form": "응 팔겠다고 했는데 갑자기 연락 두절",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014287.1.15",
          "form": "뭐가 계속 연락두절이고",
          "original_form": "뭐가 계속 연락두절이고",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014287.1.16",
          "form": "그러니까 당근이 이렇다 갑자기 당일거래 취소하고",
          "original_form": "그러니까 당근이 이렇다 갑자기 당일거래 취소하고",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014287.1.17",
          "form": "홈표핑에 리복 츄리닝 세트 판다",
          "original_form": "홈표핑에 리복 츄리닝 세트 판다",
