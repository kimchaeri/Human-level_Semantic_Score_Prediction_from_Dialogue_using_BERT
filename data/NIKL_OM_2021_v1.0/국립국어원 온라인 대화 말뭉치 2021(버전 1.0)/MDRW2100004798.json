      "utterance": [
        {
          "id": "MDRW2100004798.1.1",
          "form": "싸이월드 부활했는데 사용해봤어요?",
          "original_form": "싸이월드 부활했는데 사용해봤어요?",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004798.1.2",
          "form": "오 아뇨 저 근데 예전에 싸이월드 있을땐",
          "original_form": "오 아뇨 저 근데 예전에 싸이월드 있을땐",
          "speaker_id": "2",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004798.1.3",
          "form": "사용해봤어요",
          "original_form": "사용해봤어요",
          "speaker_id": "2",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004798.1.4",
          "form": "저도 사용했었는데 궁금한데 흑역사라 열어보기 무서워요",
          "original_form": "저도 사용했었는데 궁금한데 흑역사라 열어보기 무서워요",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004798.1.5",
          "form": "맞아요 근데 그때 비지엠 썼던건 좀 궁금하네요ㅋㅋㅋ",
          "original_form": "맞아요 근데 그때 비지엠 썼던건 좀 궁금하네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004798.1.6",
          "form": "추억이 새록새록",
          "original_form": "추억이 새록새록",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004798.1.7",
          "form": "아 맞아요 비지엠도 좋았었는데",
          "original_form": "아 맞아요 비지엠도 좋았었는데",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004798.1.8",
          "form": "저는 다이어리 많이 썼었는데",
          "original_form": "저는 다이어리 많이 썼었는데",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004798.1.9",
          "form": "친구들 말로는 다이어리는 복구가 안됐다고 하더라구요 ㅠ 그건 좀 슬퍼요",
          "original_form": "친구들 말로는 다이어리는 복구가 안됐다고 하더라구요 ㅠ 그건 좀 슬퍼요",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004798.1.10",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "2",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004798.1.11",
          "form": "저도 다이어리 열심히 썼어서 슬퍼요ㅠㅠ",
          "original_form": "저도 다이어리 열심히 썼어서 슬퍼요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004798.1.12",
          "form": "네 지금은 사진만 복구됐다는 거 같더라고요 ㅜ",
          "original_form": "네 지금은 사진만 복구됐다는 거 같더라고요 ㅜ",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004798.1.13",
          "form": "좀 많이 썼었는데",
          "original_form": "좀 많이 썼었는데",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004798.1.14",
          "form": "저는 사진보다 다이어리가 더 궁금해요 ㅠ",
          "original_form": "저는 사진보다 다이어리가 더 궁금해요 ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004798.1.15",
          "form": "저두요ㅠㅠ물론 사춘기때의 감성이 장난 아니겠지만ㅎㅎ",
          "original_form": "저두요ㅠㅠ물론 사춘기때의 감성이 장난 아니겠지만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004798.1.16",
          "form": "다이어리도 복구해줬으면 좋겠네요",
          "original_form": "다이어리도 복구해줬으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004798.1.17",
          "form": "일촌들 목록도 남아 있나 모르겠어요 ㅠ",
          "original_form": "일촌들 목록도 남아 있나 모르겠어요 ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004798.1.18",
          "form": "지금은 연락 안하는 지인들 많을거 같은데",
          "original_form": "지금은 연락 안하는 지인들 많을거 같은데",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004798.1.19",
          "form": "저두요ㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004798.1.20",
          "form": "일촌평이나 방명록이 또 재밌었는데",
          "original_form": "일촌평이나 방명록이 또 재밌었는데",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004798.1.21",
          "form": "앗 그것도 궁금해요 ㅋㅋㅋ 비밀 방명록이 많아야 인싸였는데 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 그것도 궁금해요 ㅋㅋㅋ 비밀 방명록이 많아야 인싸였는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004798.1.22",
          "form": "미니미룸 열심히 꾸미던 것도 새록새록 기억나네요",
          "original_form": "미니미룸 열심히 꾸미던 것도 새록새록 기억나네요",
          "speaker_id": "1",
          "time": "20210821 18:48"
        },
        {
          "id": "MDRW2100004798.1.23",
          "form": "맞아요 서로 반사오라고 그랬던 거 같은데ㅋㅋㅋ",
          "original_form": "맞아요 서로 반사오라고 그랬던 거 같은데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004798.1.24",
          "form": "지금 생각하니까 재밌네요",
          "original_form": "지금 생각하니까 재밌네요",
          "speaker_id": "2",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004798.1.25",
          "form": "도토리는 사라졌다고 어디갔냐고 슬퍼하더라구요 ㅠ ㅋㅋㅋㅋㅋㅋ",
          "original_form": "도토리는 사라졌다고 어디갔냐고 슬퍼하더라구요 ㅠ ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:50"
        }
      ]
