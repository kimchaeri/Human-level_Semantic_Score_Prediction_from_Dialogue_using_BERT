        },
        {
          "id": "MDRW2100024780.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100024780.1.3",
          "form": "최근에 가성비 패드 샀어요",
          "original_form": "최근에 가성비 패드 샀어요",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100024780.1.4",
          "form": "뭐 사거나 중고거래 하시나요?",
          "original_form": "뭐 사거나 중고거래 하시나요?",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100024780.1.5",
          "form": "아니요아니요",
          "original_form": "아니요아니요",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100024780.1.6",
          "form": "전 조만간 가을옷사려구요!!",
          "original_form": "전 조만간 가을옷사려구요!!",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100024780.1.7",
          "form": "맞네요 딱 옷 살 시즌이구나",
          "original_form": "맞네요 딱 옷 살 시즌이구나",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100024780.1.8",
          "form": "코로나 때문에 옷 잘 안샀는데 올해는 좀 사야겠어요",
          "original_form": "코로나 때문에 옷 잘 안샀는데 올해는 좀 사야겠어요",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100024780.1.9",
          "form": "조아요",
          "original_form": "조아요",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100024780.1.10",
          "form": "그냥 기분이좋으니까요~~!",
          "original_form": "그냥 기분이좋으니까요~~!",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100024780.1.11",
          "form": "ㅋㅋ주로 어디서 사세요?",
          "original_form": "ㅋㅋ주로 어디서 사세요?",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100024780.1.12",
          "form": "전 인터넷이요!!!",
          "original_form": "전 인터넷이요!!!",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100024780.1.13",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100024780.1.14",
          "form": "저도 인터넷 주로 쓰네요",
          "original_form": "저도 인터넷 주로 쓰네요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100024780.1.15",
          "form": "유니클로가 잘 맞아서 자주 사고",
          "original_form": "유니클로가 잘 맞아서 자주 사고",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100024780.1.16",
          "form": "지오다노는 바지?",
          "original_form": "지오다노는 바지?",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100024780.1.17",
          "form": "저는 지그재그에 있는 인터넷쇼핑몰 많이써요ㅋㅋㅋ",
          "original_form": "저는 지그재그에 있는 인터넷쇼핑몰 많이써요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100024780.1.18",
          "form": "아 지그재그 선전 많이하더라구요ㅋㅋ",
          "original_form": "아 지그재그 선전 많이하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100024780.1.19",
          "form": "맞아요ㅋㅋㅋㅋ 지그재그 이제 진짜 머기업인가봐요",
          "original_form": "맞아요ㅋㅋㅋㅋ 지그재그 이제 진짜 머기업인가봐요",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100024780.1.20",
          "form": "이제 진짜 무신사 지그재그는 대기업 느낌",
          "original_form": "이제 진짜 무신사 지그재그는 대기업 느낌",
          "speaker_id": "2",
          "time": "20210902 08:28"
        },
        {
          "id": "MDRW2100024780.1.21",
          "form": "오늘 지그재그에서 운동화샀답니다 ㅋㅋ",
