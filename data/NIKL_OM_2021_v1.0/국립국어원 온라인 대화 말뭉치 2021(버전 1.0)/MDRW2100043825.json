      "utterance": [
        {
          "id": "MDRW2100043825.1.1",
          "form": "3시?",
          "original_form": "3시?",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043825.1.2",
          "form": "글케 일찍 안먹었어",
          "original_form": "글케 일찍 안먹었어",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043825.1.3",
          "form": "그래도 슬슬 출출하겠네",
          "original_form": "그래도 슬슬 출출하겠네",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043825.1.4",
          "form": "이따 먹으면 너무 늦긴 해..",
          "original_form": "이따 먹으면 너무 늦긴 해..",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043825.1.5",
          "form": "아니 부모 둘다",
          "original_form": "아니 부모 둘다",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043825.1.6",
          "form": "거실에 있어서",
          "original_form": "거실에 있어서",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043825.1.7",
          "form": "부담시러.....",
          "original_form": "부담시러.....",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043825.1.8",
          "form": "ㅇ ㅏ...",
          "original_form": "ㅇ ㅏ...",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043825.1.9",
          "form": "그럼 뭐 먹기 부담스럽긴 하지...",
          "original_form": "그럼 뭐 먹기 부담스럽긴 하지...",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043825.1.10",
          "form": "내가 뭐 먹으면 뭐먹는지 얼마나 먹는지 꼬치꼬치 신경쓰고",
          "original_form": "내가 뭐 먹으면 뭐먹는지 얼마나 먹는지 꼬치꼬치 신경쓰고",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043825.1.11",
          "form": "냉장고에 더 있는데",
          "original_form": "냉장고에 더 있는데",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043825.1.12",
          "form": "굳이 내꺼 한입씩 먹어",
          "original_form": "굳이 내꺼 한입씩 먹어",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043825.1.13",
          "form": "아 ㅋㅋㅋㅋㅋ 맞아",
          "original_form": "아 ㅋㅋㅋㅋㅋ 맞아",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043825.1.14",
          "form": "꼭 뺏어먹으면서",
          "original_form": "꼭 뺏어먹으면서",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043825.1.15",
          "form": "밤중에 뭐 먹냐고 머라 함",
          "original_form": "밤중에 뭐 먹냐고 머라 함",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043825.1.16",
          "form": "ㅇㅇㅇ진짜 기분나빠",
          "original_form": "ㅇㅇㅇ진짜 기분나빠",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043825.1.17",
          "form": "10시면 다 자러 들어가니까",
          "original_form": "10시면 다 자러 들어가니까",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043825.1.18",
          "form": "걍 10시 후에 맘 편히 먹을래......",
          "original_form": "걍 10시 후에 맘 편히 먹을래......",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043825.1.19",
          "form": "그래...",
          "original_form": "그래...",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043825.1.20",
          "form": "그때 맘 편하게 야식타임을 즐기렴...",
          "original_form": "그때 맘 편하게 야식타임을 즐기렴...",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043825.1.21",
          "form": "씻고 나왔어",
          "original_form": "씻고 나왔어",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100043825.1.22",
          "form": "만두는 먹엇니",
          "original_form": "만두는 먹엇니",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100043825.1.23",
          "form": "이제 저녁을 먹어야하지만",
          "original_form": "이제 저녁을 먹어야하지만",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100043825.1.24",
          "form": "양치를 햇더니",
          "original_form": "양치를 햇더니",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100043825.1.25",
          "form": "귀찮아......",
          "original_form": "귀찮아......",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100043825.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100043825.1.27",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100043825.1.28",
          "form": "3시에 밥 먹엇으면",
          "original_form": "3시에 밥 먹엇으면",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100043825.1.29",
          "form": "출출하지 않아?",
          "original_form": "출출하지 않아?",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100043825.1.30",
          "form": "나 요새",
          "original_form": "나 요새",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100043825.1.31",
          "form": "한끼씩만 먹기도 해서",
          "original_form": "한끼씩만 먹기도 해서",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100043825.1.32",
          "form": "그닥....?",
          "original_form": "그닥....?",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100043825.1.33",
          "form": "그럼 안돼 기력이 빠진다구",
          "original_form": "그럼 안돼 기력이 빠진다구",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100043825.1.34",
          "form": "근데 새벽 두시쯤되면",
          "original_form": "근데 새벽 두시쯤되면",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100043825.1.35",
          "form": "확실하게 배고파지긴 해",
          "original_form": "확실하게 배고파지긴 해",
          "speaker_id": "2",
          "time": "20210907 22:47"
        }
      ]
