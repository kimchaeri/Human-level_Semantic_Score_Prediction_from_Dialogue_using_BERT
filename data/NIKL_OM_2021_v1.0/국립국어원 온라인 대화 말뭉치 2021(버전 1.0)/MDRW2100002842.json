      "utterance": [
        {
          "id": "MDRW2100002842.1.1",
          "form": "오렌지는 다행히 환불받았어요 ㅎ",
          "original_form": "오렌지는 다행히 환불받았어요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002842.1.2",
          "form": "홈쇼핑으로 산거라 ㅋㅋ",
          "original_form": "홈쇼핑으로 산거라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002842.1.3",
          "form": "오 다행이예요! 저 홈쇼핑은 잘안하는데",
          "original_form": "오 다행이예요! 저 홈쇼핑은 잘안하는데",
          "speaker_id": "1",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002842.1.4",
          "form": "음식 잘못되고 이러면 빠르게처리해주는게 좋더라구요",
          "original_form": "음식 잘못되고 이러면 빠르게처리해주는게 좋더라구요",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002842.1.5",
          "form": "광고 넘기다가 홈쇼핑 채널걸리면 그때 조심해야해요",
          "original_form": "광고 넘기다가 홈쇼핑 채널걸리면 그때 조심해야해요",
          "speaker_id": "2",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002842.1.6",
          "form": "특히 늦은밤 먹는거",
          "original_form": "특히 늦은밤 먹는거",
          "speaker_id": "2",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002842.1.7",
          "form": "국물요리 같은거",
          "original_form": "국물요리 같은거",
          "speaker_id": "2",
          "time": "20210820 16:06"
        },
        {
          "id": "MDRW2100002842.1.8",
          "form": "ㅋㅋㅋㅋㅋ알죠! 그리고 튀김요리..",
          "original_form": "ㅋㅋㅋㅋㅋ알죠! 그리고 튀김요리..",
          "speaker_id": "1",
          "time": "20210820 16:06"
        },
        {
          "id": "MDRW2100002842.1.9",
          "form": "돈까스 보이면 큰일나요",
          "original_form": "돈까스 보이면 큰일나요",
          "speaker_id": "1",
          "time": "20210820 16:06"
        },
        {
          "id": "MDRW2100002842.1.10",
          "form": "사야 이득일것 같은 기분 알죠",
          "original_form": "사야 이득일것 같은 기분 알죠",
          "speaker_id": "2",
          "time": "20210820 16:07"
        },
        {
          "id": "MDRW2100002842.1.11",
          "form": "내가 조리해도 저정도 나올것 같은 자신감 막 생기고",
          "original_form": "내가 조리해도 저정도 나올것 같은 자신감 막 생기고",
          "speaker_id": "2",
          "time": "20210820 16:07"
        },
        {
          "id": "MDRW2100002842.1.12",
          "form": "그쵸 안사면 바보..되는기분",
          "original_form": "그쵸 안사면 바보..되는기분",
          "speaker_id": "1",
          "time": "20210820 16:07"
        },
        {
          "id": "MDRW2100002842.1.13",
          "form": "안사면 0원인데 안사면 안될꺼같고",
          "original_form": "안사면 0원인데 안사면 안될꺼같고",
          "speaker_id": "1",
          "time": "20210820 16:07"
        },
        {
          "id": "MDRW2100002842.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ홈쇼핑에선 어쩜 그리 맛있게 드시는지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ홈쇼핑에선 어쩜 그리 맛있게 드시는지",
          "speaker_id": "1",
          "time": "20210820 16:08"
        },
        {
          "id": "MDRW2100002842.1.15",
          "form": "지금 이 가격 놓치지말래요",
          "original_form": "지금 이 가격 놓치지말래요",
          "speaker_id": "2",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002842.1.16",
          "form": "주문 굉장히 많대요",
          "original_form": "주문 굉장히 많대요",
          "speaker_id": "2",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002842.1.17",
          "form": "그럼 나도 전화 들어야지 뭐 ㅋㅋㅋ",
          "original_form": "그럼 나도 전화 들어야지 뭐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002842.1.18",
          "form": "ㅋㅋㅋㅋ그렇다면 주문해야죠..돈가스..",
          "original_form": "ㅋㅋㅋㅋ그렇다면 주문해야죠..돈가스..",
          "speaker_id": "1",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002842.1.19",
          "form": "치즈까스..홀린듯",
          "original_form": "치즈까스..홀린듯",
          "speaker_id": "1",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002842.1.20",
          "form": "ㅋㅋㅋㅋ치즈 좋죠 쭈욱 들어나는 고거",
          "original_form": "ㅋㅋㅋㅋ치즈 좋죠 쭈욱 들어나는 고거",
          "speaker_id": "2",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002842.1.21",
          "form": "심지어 조리도 쉽다고하니까",
          "original_form": "심지어 조리도 쉽다고하니까",
          "speaker_id": "1",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002842.1.22",
          "form": "전 백종원 호텔 옆에 있다는 그 돈까스 먹어보고싶은데",
          "original_form": "전 백종원 호텔 옆에 있다는 그 돈까스 먹어보고싶은데",
          "speaker_id": "2",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002842.1.23",
          "form": "아 연돈이요?",
          "original_form": "아 연돈이요?",
          "speaker_id": "1",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002842.1.24",
          "form": "네 ㅎ 먹어보고싶어요",
          "original_form": "네 ㅎ 먹어보고싶어요",
          "speaker_id": "2",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002842.1.25",
          "form": "흘러나오는 치즈 멈추지않는 치즈",
          "original_form": "흘러나오는 치즈 멈추지않는 치즈",
          "speaker_id": "2",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002842.1.26",
          "form": "아 저두 연돈 먹어보고 싶은데 줄설 엄두가 나지않아요",
          "original_form": "아 저두 연돈 먹어보고 싶은데 줄설 엄두가 나지않아요",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002842.1.27",
          "form": "ㅋㅋㅋㅋ서울에 있을때 먹어볼껄..",
          "original_form": "ㅋㅋㅋㅋ서울에 있을때 먹어볼껄..",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002842.1.28",
          "form": "제주도로 갑시다",
          "original_form": "제주도로 갑시다",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002842.1.29",
          "form": "먹기위해 ㅎ",
          "original_form": "먹기위해 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002842.1.30",
          "form": "근데 비슷한거 많이 있죠?",
          "original_form": "근데 비슷한거 많이 있죠?",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002842.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ연돈이랑요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ연돈이랑요?",
          "speaker_id": "1",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002842.1.32",
          "form": "비슷한곳은 많은거같은데 먹어보질 않아서",
          "original_form": "비슷한곳은 많은거같은데 먹어보질 않아서",
          "speaker_id": "1",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002842.1.33",
          "form": "비교를 못해요...ㅠㅠ",
          "original_form": "비교를 못해요...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002842.1.34",
          "form": "비슷할거에요 우리 더 배고플때 먹어요",
          "original_form": "비슷할거에요 우리 더 배고플때 먹어요",
          "speaker_id": "2",
          "time": "20210820 16:17"
        }
      ]
