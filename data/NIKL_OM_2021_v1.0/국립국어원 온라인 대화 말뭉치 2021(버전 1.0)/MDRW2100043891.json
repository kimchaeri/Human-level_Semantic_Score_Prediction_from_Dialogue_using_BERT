      "utterance": [
        {
          "id": "MDRW2100043891.1.1",
          "form": "좋은 여행 경험이 있으신가요",
          "original_form": "좋은 여행 경험이 있으신가요",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043891.1.2",
          "form": "네저는 친구랑 인생첨갔던 도쿄여행이 기억에남네요",
          "original_form": "네저는 친구랑 인생첨갔던 도쿄여행이 기억에남네요",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043891.1.3",
          "form": "어떤 친구랑 가셨어나요",
          "original_form": "어떤 친구랑 가셨어나요",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100043891.1.4",
          "form": "대학친구인데 취업시험 끝나고 결과나오기전에 갔어요",
          "original_form": "대학친구인데 취업시험 끝나고 결과나오기전에 갔어요",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100043891.1.5",
          "form": "혹시나 결과 잘못되고 가면 분위기 이상해질까봐ㅋㅋ",
          "original_form": "혹시나 결과 잘못되고 가면 분위기 이상해질까봐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100043891.1.6",
          "form": "여행은 좋아었나요",
          "original_form": "여행은 좋아었나요",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043891.1.7",
          "form": "네 조금싸우긴했는데 비행기탈때부터 너무 설레고 좋더라구요",
          "original_form": "네 조금싸우긴했는데 비행기탈때부터 너무 설레고 좋더라구요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043891.1.8",
          "form": "여행 최근에 가셨나요?",
          "original_form": "여행 최근에 가셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043891.1.9",
          "form": "최근엔 코로나 땜에 가지 못했네요",
          "original_form": "최근엔 코로나 땜에 가지 못했네요",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043891.1.10",
          "form": "가고싶은곳은 있으세요?",
          "original_form": "가고싶은곳은 있으세요?",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043891.1.11",
          "form": "특별히 생각해 보지는 않았어요",
          "original_form": "특별히 생각해 보지는 않았어요",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043891.1.12",
          "form": "아 저는 시간과 돈이 있으면",
          "original_form": "아 저는 시간과 돈이 있으면",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043891.1.13",
          "form": "유럽은 꼭 추천드립니다",
          "original_form": "유럽은 꼭 추천드립니다",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043891.1.14",
          "form": "지금은 시간이 좀 어렵네요",
          "original_form": "지금은 시간이 좀 어렵네요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043891.1.15",
          "form": "그렇죠 돈이있을땐 시간이없는게 함정",
          "original_form": "그렇죠 돈이있을땐 시간이없는게 함정",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100043891.1.16",
          "form": "둘다 있기가 어렾네요",
          "original_form": "둘다 있기가 어렾네요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        }
      ]
