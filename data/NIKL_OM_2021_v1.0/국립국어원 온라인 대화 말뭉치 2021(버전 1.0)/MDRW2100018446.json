      "utterance": [
        {
          "id": "MDRW2100018446.1.1",
          "form": "최근에 산 물건 있으세요?",
          "original_form": "최근에 산 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210827 02:05"
        },
        {
          "id": "MDRW2100018446.1.2",
          "form": "저는 어제 제로콜라 30캔을 샀어요",
          "original_form": "저는 어제 제로콜라 30캔을 샀어요",
          "speaker_id": "1",
          "time": "20210827 02:05"
        },
        {
          "id": "MDRW2100018446.1.3",
          "form": "오 저도 제로콜라 좋아해요!",
          "original_form": "오 저도 제로콜라 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 02:08"
        },
        {
          "id": "MDRW2100018446.1.4",
          "form": "저는 최근에 바지를 샀어요",
          "original_form": "저는 최근에 바지를 샀어요{emoji:•}",
          "speaker_id": "2",
          "time": "20210827 02:08"
        },
        {
          "id": "MDRW2100018446.1.5",
          "form": "근데 사이즈가 안 맞더라고요..",
          "original_form": "근데 사이즈가 안 맞더라고요..",
          "speaker_id": "2",
          "time": "20210827 02:09"
        },
        {
          "id": "MDRW2100018446.1.6",
          "form": "...저도 티셔츠를 샀는데 사이즈가 안맞더라구요..",
          "original_form": "...저도 티셔츠를 샀는데 사이즈가 안맞더라구요..",
          "speaker_id": "1",
          "time": "20210827 02:09"
        },
        {
          "id": "MDRW2100018446.1.7",
          "form": "반품하셨나요?",
          "original_form": "반품하셨나요?",
          "speaker_id": "1",
          "time": "20210827 02:09"
        },
        {
          "id": "MDRW2100018446.1.8",
          "form": "아니요ㅎㅎ",
          "original_form": "아니요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 02:10"
        },
        {
          "id": "MDRW2100018446.1.9",
          "form": "할인을 많이 받고 산 제품이라..",
          "original_form": "할인을 많이 받고 산 제품이라..",
          "speaker_id": "2",
          "time": "20210827 02:10"
        },
        {
          "id": "MDRW2100018446.1.10",
          "form": "반품하기 그렇더라고요..",
          "original_form": "반품하기 그렇더라고요..",
          "speaker_id": "2",
          "time": "20210827 02:11"
        },
        {
          "id": "MDRW2100018446.1.11",
          "form": "name1은 셔츠 반품하셨나요?",
          "original_form": "&name1&은 셔츠 반품하셨나요?",
          "speaker_id": "2",
          "time": "20210827 02:11"
        },
        {
          "id": "MDRW2100018446.1.12",
          "form": "저는 교환을 했어요! 다행히 교환하니 잘 맞더라구요",
          "original_form": "저는 교환을 했어요! 다행히 교환하니 잘 맞더라구요",
          "speaker_id": "1",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100018446.1.13",
          "form": "요즘은 그냥 바로바로 반품이나 교환해요ㅠㅠ",
          "original_form": "요즘은 그냥 바로바로 반품이나 교환해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100018446.1.14",
          "form": "집에 쌓인 안맞는 옷들이 한가득이라",
          "original_form": "집에 쌓인 안맞는 옷들이 한가득이라",
          "speaker_id": "1",
          "time": "20210827 02:13"
        },
        {
          "id": "MDRW2100018446.1.15",
          "form": "아 그거 완전 공감해요!!",
          "original_form": "아 그거 완전 공감해요!!",
          "speaker_id": "2",
          "time": "20210827 02:13"
        },
        {
          "id": "MDRW2100018446.1.16",
          "form": "언젠간 살빼서 입을꺼야!라고 하지만..",
          "original_form": "언젠간 살빼서 입을꺼야!라고 하지만..",
          "speaker_id": "2",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018446.1.17",
          "form": "결국 무리였죠..",
          "original_form": "결국 무리였죠..",
          "speaker_id": "2",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018446.1.18",
          "form": "덕분에 옷은 많은데 입을 옷이 없어요..",
          "original_form": "덕분에 옷은 많은데 입을 옷이 없어요..",
          "speaker_id": "2",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018446.1.19",
          "form": "하.. 너무 공감...",
          "original_form": "하.. 너무 공감...",
          "speaker_id": "1",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018446.1.20",
          "form": "엄마는 있는데 왜 또사냐고 하시고..",
          "original_form": "엄마는 있는데 왜 또사냐고 하시고..",
          "speaker_id": "1",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018446.1.21",
          "form": "또 실패하고...",
          "original_form": "또 실패하고...",
          "speaker_id": "1",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018446.1.22",
          "form": "그래서 사실 요즘 옷 잘 안사는데 오랜만에 샀어요 ㅠㅠ",
          "original_form": "그래서 사실 요즘 옷 잘 안사는데 오랜만에 샀어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018446.1.23",
          "form": "이뻐보여서 샀는데",
          "original_form": "이뻐보여서 샀는데",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018446.1.24",
          "form": "실패하면 눈물나죠 흑흑",
          "original_form": "실패하면 눈물나죠 흑흑",
          "speaker_id": "2",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018446.1.25",
          "form": "이제 가을이니까",
          "original_form": "이제 가을이니까",
          "speaker_id": "2",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018446.1.26",
          "form": "가을 옷 사신걸까요??",
          "original_form": "가을 옷 사신걸까요??",
          "speaker_id": "2",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018446.1.27",
          "form": "후후 이동네는 아직 더워서 여름반팔을 샀답니다",
          "original_form": "후후 이동네는 아직 더워서 여름반팔을 샀답니다",
          "speaker_id": "1",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018446.1.28",
          "form": "바지는 어떤 바지 사셨어요?",
          "original_form": "바지는 어떤 바지 사셨어요?",
          "speaker_id": "1",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018446.1.29",
          "form": "블랙 슬랙스를 샀어요!",
          "original_form": "블랙 슬랙스를 샀어요!",
          "speaker_id": "2",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100018446.1.30",
          "form": "이제 가을 옷을 준비중인거죠ㅎㅎ",
          "original_form": "이제 가을 옷을 준비중인거죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018446.1.31",
          "form": "아하 저도 슬랙스 새로 사야하는데ㅎㅎㅎ",
          "original_form": "아하 저도 슬랙스 새로 사야하는데ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018446.1.32",
          "form": "근데 바지 쇼핑이 제일 힘든거 같아요 ㅠㅠ",
          "original_form": "근데 바지 쇼핑이 제일 힘든거 같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018446.1.33",
          "form": "저는 하비라 더 어렵더라구요",
          "original_form": "저는 하비라 더 어렵더라구요",
          "speaker_id": "1",
          "time": "20210827 02:27"
        },
        {
          "id": "MDRW2100018446.1.34",
          "form": "아.. 저도 하비라 사이즈 찾기가 어렵더라고요..",
          "original_form": "아.. 저도 하비라 사이즈 찾기가 어렵더라고요..",
          "speaker_id": "2",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018446.1.35",
          "form": "매번 실패할거 각오하고 사요ㅋㅋㅋ\r\nㅠㅠ",
          "original_form": "매번 실패할거 각오하고 사요ㅋㅋㅋ\r\nㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:33"
        },
        {
          "id": "MDRW2100018446.1.36",
          "form": "오프라인에서 사도 힘듦...",
          "original_form": "오프라인에서 사도 힘듦...",
          "speaker_id": "1",
          "time": "20210827 02:33"
        }
      ]
