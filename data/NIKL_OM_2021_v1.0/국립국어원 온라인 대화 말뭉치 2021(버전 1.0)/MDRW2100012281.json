      "utterance": [
        {
          "id": "MDRW2100012281.1.1",
          "form": "요즘 보시는 드라마가 있나요?",
          "original_form": "요즘 보시는 드라마가 있나요?",
          "speaker_id": "2",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100012281.1.2",
          "form": "저 빈센조 몰아보고있어요",
          "original_form": "저 빈센조 몰아보고있어요",
          "speaker_id": "1",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100012281.1.3",
          "form": "송중기 나온 드라마 맞죠?",
          "original_form": "송중기 나온 드라마 맞죠?",
          "speaker_id": "2",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100012281.1.4",
          "form": "저도 제목은 들어봤어요ㅎㅎ",
          "original_form": "저도 제목은 들어봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100012281.1.5",
          "form": "오 네 맞아요송중기",
          "original_form": "오 네 맞아요송중기",
          "speaker_id": "1",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100012281.1.6",
          "form": "요즘 드라마는 잘 안 보지만",
          "original_form": "요즘 드라마는 잘 안 보지만",
          "speaker_id": "2",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100012281.1.7",
          "form": "예~전에 성균관 스캔들이랑 뿌리 싶은 나무 재밌게 봤어요.",
          "original_form": "예~전에 성균관 스캔들이랑 뿌리 싶은 나무 재밌게 봤어요.",
          "speaker_id": "2",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100012281.1.8",
          "form": "한복 입은 송중기 너무 고왔어요ㅋㅋㅋ",
          "original_form": "한복 입은 송중기 너무 고왔어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100012281.1.9",
          "form": "와 뿌나 진짜 명작이죠",
          "original_form": "와 뿌나 진짜 명작이죠",
          "speaker_id": "1",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100012281.1.10",
          "form": "뿌나 아시는군요ㅠㅠ",
          "original_form": "뿌나 아시는군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012281.1.11",
          "form": "당시엔 나름 시청률도 잘 나왔던 거 같은데",
          "original_form": "당시엔 나름 시청률도 잘 나왔던 거 같은데",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012281.1.12",
          "form": "주변에 본 사람이 없네요ㅋㅋㅋ",
          "original_form": "주변에 본 사람이 없네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012281.1.13",
          "form": "그 조진웅 진짜 멋있었는데",
          "original_form": "그 조진웅 진짜 멋있었는데",
          "speaker_id": "1",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012281.1.14",
          "form": "엌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012281.1.15",
          "form": "저도 조진웅 제일 좋아했어요!",
          "original_form": "저도 조진웅 제일 좋아했어요!",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012281.1.16",
          "form": "주인공은 장혁이었던 것 같지만...",
          "original_form": "주인공은 장혁이었던 것 같지만...",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012281.1.17",
          "form": "와벌써 10년전이네요",
          "original_form": "와벌써 10년전이네요",
          "speaker_id": "1",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100012281.1.18",
          "form": "넘 오래됐죠ㅋㅋ",
          "original_form": "넘 오래됐죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012281.1.19",
          "form": "그래도 드라마가 재밌어서 주요장면은 거의 기억해요ㅎ",
          "original_form": "그래도 드라마가 재밌어서 주요장면은 거의 기억해요ㅎ",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012281.1.20",
          "form": "가물가물하네요ㅎㅎ",
          "original_form": "가물가물하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:00"
        }
