      "utterance": [
        {
          "id": "MDRW2100011543.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011543.1.2",
          "form": "요즘 뭐 재있거 보셨어요?",
          "original_form": "요즘 뭐 재있거 보셨어요?",
          "speaker_id": "2",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011543.1.3",
          "form": "뜨랑낄로 여행유튜브요!",
          "original_form": "뜨랑낄로 여행유튜브요!",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011543.1.4",
          "form": "오 처음들어봤어요",
          "original_form": "오 처음들어봤어요",
          "speaker_id": "2",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011543.1.5",
          "form": "지금 여행을 다니시는건가요?",
          "original_form": "지금 여행을 다니시는건가요?",
          "speaker_id": "2",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011543.1.6",
          "form": "어떤거 즐겨보시나요?",
          "original_form": "어떤거 즐겨보시나요?",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011543.1.7",
          "form": "저는 요새 영드에 빠졌어요",
          "original_form": "저는 요새 영드에 빠졌어요",
          "speaker_id": "2",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011543.1.8",
          "form": "오 뭐가 재밌나요?",
          "original_form": "오 뭐가 재밌나요?",
          "speaker_id": "1",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100011543.1.9",
          "form": "파더 브라운이라고 브라운신부 추천이요",
          "original_form": "파더 브라운이라고 브라운신부 추천이요",
          "speaker_id": "2",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100011543.1.10",
          "form": "여행유투버님은 코로나인데 어떻게 여행다니신대요?",
          "original_form": "여행유투버님은 코로나인데 어떻게 여행다니신대요?",
          "speaker_id": "2",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100011543.1.11",
          "form": "저는 예전에 셜록 봤었어요ㅎㅎ",
          "original_form": "저는 예전에 셜록 봤었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:54"
        },
        {
          "id": "MDRW2100011543.1.12",
          "form": "백신맞고 걍 나가더라구요ㅋㅋ",
          "original_form": "백신맞고 걍 나가더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:54"
        },
        {
          "id": "MDRW2100011543.1.13",
          "form": "그분은 그럼 지금 어디신가요 ㄷㄷ",
          "original_form": "그분은 그럼 지금 어디신가요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011543.1.14",
          "form": "지금 미국이요ㅎㅎ",
          "original_form": "지금 미국이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100011543.1.15",
          "form": "오 신기해",
          "original_form": "오 신기해",
          "speaker_id": "2",
          "time": "20210824 14:07"
        },
        {
          "id": "MDRW2100011543.1.16",
          "form": "브라운신부는 가벼운 수사물이에요",
          "original_form": "브라운신부는 가벼운 수사물이에요",
          "speaker_id": "2",
          "time": "20210824 14:07"
        },
        {
          "id": "MDRW2100011543.1.17",
          "form": "오 한번 검색해볼게요!",
          "original_form": "오 한번 검색해볼게요!",
          "speaker_id": "1",
          "time": "20210824 14:08"
        }
