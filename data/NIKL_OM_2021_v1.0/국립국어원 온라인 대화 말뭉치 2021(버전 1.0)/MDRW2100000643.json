        },
        {
          "id": "MDRW2100000643.1.2",
          "form": "게임하는거 있으세요?",
          "original_form": "게임하는거 있으세요?",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000643.1.3",
          "form": "저저번달까지는 쿠키런 킹덤을 했었는데",
          "original_form": "저저번달까지는 쿠키런 킹덤을 했었는데",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000643.1.4",
          "form": "음...하시는거 있으세요?",
          "original_form": "음...하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000643.1.5",
          "form": "쿠키런킹덤 재밌나요?",
          "original_form": "쿠키런킹덤 재밌나요?",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000643.1.6",
          "form": "쿠키런은 옛날에 해봤긴했는데",
          "original_form": "쿠키런은 옛날에 해봤긴했는데",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000643.1.7",
          "form": "오...그냥 다들 하길래 하러 갔었는데 제 취향에는 안 맞더라구요.",
          "original_form": "오...그냥 다들 하길래 하러 갔었는데 제 취향에는 안 맞더라구요.",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000643.1.8",
          "form": "저는 닌텐도 게임 위주로 많이해서 그런지",
          "original_form": "저는 닌텐도 게임 위주로 많이해서 그런지",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000643.1.9",
          "form": "모바일....뽑기겜ㅋㅋ은...",
          "original_form": "모바일....뽑기겜ㅋㅋ은...",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000643.1.10",
          "form": "저도 닌텐도 좋아해요",
          "original_form": "저도 닌텐도 좋아해요",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000643.1.11",
          "form": "너무 도박같아요",
          "original_form": "너무 도박같아요",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000643.1.12",
          "form": "닌텐도 게임들이 진짜 게임같아요",
          "original_form": "닌텐도 게임들이 진짜 게임같아요",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000643.1.13",
          "form": "즐거움을 주려는 목적을 가지고 만든 게임이라는 느낌",
          "original_form": "즐거움을 주려는 목적을 가지고 만든 게임이라는 느낌",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000643.1.14",
          "form": "닌텐도의 이념이 좀 그런 류인 것 같아서 너무 좋아요.",
          "original_form": "닌텐도의 이념이 좀 그런 류인 것 같아서 너무 좋아요.",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000643.1.15",
          "form": "모바일 게임 내는 것도...뽑기....있긴하지만...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "모바일 게임 내는 것도...뽑기....있긴하지만...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000643.1.16",
          "form": "그래서 차라리 유료로 팔아줬으면 하는게 있어요",
          "original_form": "그래서 차라리 유료로 팔아줬으면 하는게 있어요",
          "speaker_id": "2",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000643.1.17",
          "form": "마리오처럼",
          "original_form": "마리오처럼",
          "speaker_id": "2",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000643.1.18",
          "form": "아 맞아요. 제발 그냥 6만원 주고 풀프라이스 겜 하고 싶은데.",
          "original_form": "아 맞아요. 제발 그냥 6만원 주고 풀프라이스 겜 하고 싶은데.",
          "speaker_id": "1",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000643.1.19",
          "form": "모바일 게임은 어느 집은 가면 시민권이 있대요",
          "original_form": "모바일 게임은 어느 집은 가면 시민권이 있대요",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000643.1.20",
          "form": "이정도는 질러야 소과금이지~하는데",
          "original_form": "이정도는 질러야 소과금이지~하는데",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000643.1.21",
          "form": "너무 무서워요.",
          "original_form": "너무 무서워요.",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000643.1.22",
          "form": "리니지는 몇천이 기본이라하더라고요",
          "original_form": "리니지는 몇천이 기본이라하더라고요",
          "speaker_id": "2",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000643.1.23",
          "form": "아저씨들 게임은....끼는게 아닌 것 같아요...",
          "original_form": "아저씨들 게임은....끼는게 아닌 것 같아요...",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000643.1.24",
          "form": "몇백 지르는건 무과금이랑 같은 놀라운 세계...",
          "original_form": "몇백 지르는건 무과금이랑 같은 놀라운 세계...",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000643.1.25",
          "form": "현질 단위가 장난이 아니에요 거긴",
          "original_form": "현질 단위가 장난이 아니에요 거긴",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000643.1.26",
          "form": "물론 리니지가 뭐...돈을 벌수들 있다고는 하지만...",
          "original_form": "물론 리니지가 뭐...돈을 벌수들 있다고는 하지만...",
          "speaker_id": "1",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000643.1.27",
          "form": "근처에도 가면 안 될 것 같은 기분이 들어요",
          "original_form": "근처에도 가면 안 될 것 같은 기분이 들어요",
          "speaker_id": "1",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000643.1.28",
          "form": "아저씨들 파이팅!",
          "original_form": "아저씨들 파이팅!",
          "speaker_id": "1",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000643.1.29",
          "form": "저는 그냥 젤다의 전설하려구요",
          "original_form": "저는 그냥 젤다의 전설하려구요",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000643.1.30",
          "form": "그게 더 재밌어보이던데",
          "original_form": "그게 더 재밌어보이던데",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000643.1.31",
          "form": "아 젤다의 전설 진짜 짱잼이에요",
          "original_form": "아 젤다의 전설 진짜 짱잼이에요",
          "speaker_id": "1",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000643.1.32",
          "form": "꼭 하세요",
          "original_form": "꼭 하세요",
          "speaker_id": "1",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000643.1.33",
          "form": "무쌍말고",
          "original_form": "무쌍말고",
          "speaker_id": "1",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000643.1.34",
          "form": "젤다의 전설!",
          "original_form": "젤다의 전설!",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000643.1.35",
          "form": "무쌍은 어떻게 다른가요",
          "original_form": "무쌍은 어떻게 다른가요",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000643.1.36",
          "form": "괜히 온갖 게임들이 젤다의 전설 다 베끼는게 아닌것 같아요.",
          "original_form": "괜히 온갖 게임들이 젤다의 전설 다 베끼는게 아닌것 같아요.",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000643.1.37",
          "form": "원숨인가 논란이던데",
          "original_form": "원숨인가 논란이던데",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000643.1.38",
          "form": "어...무쌍류의 게임 해보셨나요?",
          "original_form": "어...무쌍류의 게임 해보셨나요?",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000643.1.39",
          "form": "원숨이 뭔지는 잘 모르겠는데",
          "original_form": "원숨이 뭔지는 잘 모르겠는데",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000643.1.40",
          "form": "원신이네요",
          "original_form": "원신이네요",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000643.1.41",
          "form": "무쌍게임자체가 그냥 버튼 누르고 다죽여~~다 날라가~~여서",
          "original_form": "무쌍게임자체가 그냥 버튼 누르고 다죽여~~다 날라가~~여서",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000643.1.42",
          "form": "무슨느낌인지 알았어요",
          "original_form": "무슨느낌인지 알았어요",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000643.1.43",
          "form": "젤다의 전설의 참 재미는 모험이라고 생각하기때문에...",
          "original_form": "젤다의 전설의 참 재미는 모험이라고 생각하기때문에...",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000643.1.44",
          "form": "옙옙",
          "original_form": "옙옙",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000643.1.45",
          "form": "꼭 하세요!",
          "original_form": "꼭 하세요!",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000643.1.46",
          "form": "name2님은 요즘 무슨 게임 하세요?",
          "original_form": "&name2&님은 요즘 무슨 게임 하세요?",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000643.1.47",
          "form": "이번에 나올 젤다2 기대중이에요",
          "original_form": "이번에 나올 젤다2 기대중이에요",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000643.1.48",
          "form": "와....그건....너무 미래 아니에요...?",
          "original_form": "와....그건....너무 미래 아니에요...?",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000643.1.49",
          "form": "숨참습니다",
          "original_form": "숨참습니다",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000643.1.50",
          "form": "22년까지...?!?",
          "original_form": "22년까지...?!?",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000643.1.51",
          "form": "아니 그 이후던가요??",
          "original_form": "아니 그 이후던가요??",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000643.1.52",
          "form": "내년 가을에 나온다고 들었던거같아요",
          "original_form": "내년 가을에 나온다고 들었던거같아요",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000643.1.53",
          "form": "근데 젤다의 전설 제작진을......믿기에는 너무 많이 당해서요...",
          "original_form": "근데 젤다의 전설 제작진을......믿기에는 너무 많이 당해서요...",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000643.1.54",
          "form": "그래도 설마 이번에는 제대로 나오겠죠^^",
          "original_form": "그래도 설마 이번에는 제대로 나오겠죠^^",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000643.1.55",
          "form": "출시일 미뤄지는거요?",
          "original_form": "출시일 미뤄지는거요?",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000643.1.56",
          "form": "네네. 야숨도 한 두번? 미뤄졌었거든요.",
          "original_form": "네네. 야숨도 한 두번? 미뤄졌었거든요.",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000643.1.57",
          "form": "아 야숨나올때가 사실 고3쯔음이라",
          "original_form": "아 야숨나올때가 사실 고3쯔음이라",
          "speaker_id": "2",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000643.1.58",
          "form": "16년인가 17년인가 나왔던걸로 기억해요",
          "original_form": "16년인가 17년인가 나왔던걸로 기억해요",
          "speaker_id": "2",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000643.1.59",
          "form": "아하, 중간에 게임을 두번 엎었댔나...해서 그게 늦게 나온거였을거에요.",
          "original_form": "아하, 중간에 게임을 두번 엎었댔나...해서 그게 늦게 나온거였을거에요.",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000643.1.60",
          "form": "결국 전설의 게임이 되었지만요...ㅋㅋㅋㅋ",
