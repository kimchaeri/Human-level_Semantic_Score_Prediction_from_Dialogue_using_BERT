      "utterance": [
        {
          "id": "MDRW2100021433.1.1",
          "form": "저한테까진 정보가 안오더라구요...",
          "original_form": "저한테까진 정보가 안오더라구요...",
          "speaker_id": "2",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021433.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021433.1.3",
          "form": "아 그런가요... ㅋㅋㅋㅋㅋㅋㅋㅋ 쩨쩨하다!",
          "original_form": "아 그런가요... ㅋㅋㅋㅋㅋㅋㅋㅋ 쩨쩨하다!",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021433.1.4",
          "form": "높은 사람들끼리만 알음알음 알고 마는 건가요",
          "original_form": "높은 사람들끼리만 알음알음 알고 마는 건가요",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021433.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021433.1.6",
          "form": "그러면 보통 무슨 일 하세요? 일상적으로 하시는 업무요",
          "original_form": "그러면 보통 무슨 일 하세요? 일상적으로 하시는 업무요",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021433.1.7",
          "form": "찌라시도 자기들끼리만 돌리고!",
          "original_form": "찌라시도 자기들끼리만 돌리고!",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021433.1.8",
          "form": "음 채권쪽 하고있어요!",
          "original_form": "음 채권쪽 하고있어요!",
          "speaker_id": "2",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100021433.1.9",
          "form": "아 그 유명한",
          "original_form": "아 그 유명한",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100021433.1.10",
          "form": "증권가",
          "original_form": "증권가",
          "speaker_id": "1",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100021433.1.11",
          "form": "찌라시",
          "original_form": "찌라시",
          "speaker_id": "1",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100021433.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100021433.1.13",
          "form": "허억",
          "original_form": "허억",
          "speaker_id": "1",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100021433.1.14",
          "form": "채권...  저는 경제공부를 해본답시고 경제 책 봤을 때 나왔었는데...",
          "original_form": "채권...  저는 경제공부를 해본답시고 경제 책 봤을 때 나왔었는데...",
          "speaker_id": "1",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100021433.1.15",
          "form": "친구들이 알려달라하는데",
          "original_form": "친구들이 알려달라하는데",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100021433.1.16",
          "form": "나도 너네랑 같은 기사 봐... 이래요 맨날",
          "original_form": "나도 너네랑 같은 기사 봐...{emoji:☆} 이래요 맨날",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100021433.1.17",
          "form": "오 경제공부하셨었구나",
          "original_form": "오 경제공부하셨었구나",
          "speaker_id": "2",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100021433.1.18",
          "form": "네네 제 직업이 또 박봉으로 유명하잖아요",
          "original_form": "네네 제 직업이 또 박봉으로 유명하잖아요",
          "speaker_id": "1",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100021433.1.19",
          "form": "먹고 살 각이 안 나와서...",
          "original_form": "먹고 살 각이 안 나와서...",
          "speaker_id": "1",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100021433.1.20",
          "form": "공부를... 해보고자...",
          "original_form": "공부를... 해보고자...",
          "speaker_id": "1",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100021433.1.21",
          "form": "또르륵",
          "original_form": "또르륵",
          "speaker_id": "1",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100021433.1.22",
          "form": "아악....",
          "original_form": "아악....",
          "speaker_id": "2",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100021433.1.23",
          "form": "진짜 속상해요",
          "original_form": "진짜 속상해요",
          "speaker_id": "2",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100021433.1.24",
          "form": "저도 많이 버는 타입은 아니라서...",
          "original_form": "저도 많이 버는 타입은 아니라서...",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021433.1.25",
          "form": "증권사라 하면 되게 많이벌줄아는데 아니거든용..ㅠㅠㅠ",
          "original_form": "증권사라 하면 되게 많이벌줄아는데 아니거든용..ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021433.1.26",
          "form": "(물론 저만 아닌 편)",
          "original_form": "(물론 저만 아닌 편)",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021433.1.27",
          "form": "저희 진짜.. 화이팅해서 부자돼여..",
          "original_form": "저희 진짜.. 화이팅해서 부자돼여..",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021433.1.28",
          "form": "진짜...  진심으로요 ",
          "original_form": "진짜...  진심으로요 ",
          "speaker_id": "1",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021433.1.29",
          "form": "그래도 그런 기업은 승진하면 월급 좀 오르고 그러지 않나요? 아닌가욤 제가 잘 몰라서",
          "original_form": "그래도 그런 기업은 승진하면 월급 좀 오르고 그러지 않나요? 아닌가욤 제가 잘 몰라서",
          "speaker_id": "1",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021433.1.30",
          "form": "제가 다니는 회사는 승진이 정말 느려요",
          "original_form": "제가 다니는 회사는 승진이 정말 느려요",
          "speaker_id": "2",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021433.1.31",
          "form": "특히 여직원들은 정말...",
          "original_form": "특히 여직원들은 정말...",
          "speaker_id": "2",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021433.1.32",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021433.1.33",
          "form": "^^,,",
          "original_form": "^^,,",
          "speaker_id": "2",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021433.1.34",
          "form": "문제가 많네요 그쪽 아주!",
          "original_form": "문제가 많네요 그쪽 아주!",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021433.1.35",
          "form": "승진 하는데 기준 같은 것도 따로 있나요",
          "original_form": "승진 하는데 기준 같은 것도 따로 있나요",
          "speaker_id": "1",
          "time": "20210828 19:13"
        }
      ]
