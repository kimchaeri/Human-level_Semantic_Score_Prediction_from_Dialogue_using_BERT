      "utterance": [
        {
          "id": "MDRW2100002633.1.1",
          "form": "룸메이트있으신가요?",
          "original_form": "룸메이트있으신가요?",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002633.1.2",
          "form": "남편이랑 엄마요 ^^~",
          "original_form": "남편이랑 엄마요 ^^~",
          "speaker_id": "2",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002633.1.3",
          "form": "아 룸메는 남편이네요 ㅋㅋ",
          "original_form": "아 룸메는 남편이네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002633.1.4",
          "form": "오 룸메가 남편이시라니 신박하네요ㅋㅋㅋ",
          "original_form": "오 룸메가 남편이시라니 신박하네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002633.1.5",
          "form": "결혼하셨군요?",
          "original_form": "결혼하셨군요?",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002633.1.6",
          "form": "ㅋㅋㅋㅋㅋ네 작년에 했어요",
          "original_form": "ㅋㅋㅋㅋㅋ네 작년에 했어요",
          "speaker_id": "2",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002633.1.7",
          "form": "룸메 있으세용?",
          "original_form": "룸메 있으세용?",
          "speaker_id": "2",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002633.1.8",
          "form": "저는 없답니다ㅎㅎ 학생도지나고",
          "original_form": "저는 없답니다ㅎㅎ 학생도지나고",
          "speaker_id": "1",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002633.1.9",
          "form": "결혼하면",
          "original_form": "결혼하면",
          "speaker_id": "1",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002633.1.10",
          "form": "좋은가요?!",
          "original_form": "좋은가요?!",
          "speaker_id": "1",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002633.1.11",
          "form": "아직은요 !",
          "original_form": "아직은요 !",
          "speaker_id": "2",
          "time": "20210820 14:21"
        },
        {
          "id": "MDRW2100002633.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:21"
        },
        {
          "id": "MDRW2100002633.1.13",
          "form": "무서운 말이죠 ㅋ.ㅋ",
          "original_form": "무서운 말이죠 ㅋ.ㅋ",
          "speaker_id": "2",
          "time": "20210820 14:21"
        },
        {
          "id": "MDRW2100002633.1.14",
          "form": "아직은이군요ㄷㄷㄷㄷ",
          "original_form": "아직은이군요ㄷㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002633.1.15",
          "form": "참 결혼은ㅎㅎㅎ",
          "original_form": "참 결혼은ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002633.1.16",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002633.1.17",
          "form": "넹 ㅎㅎ 가족분들과 같이사세요?",
          "original_form": "넹 ㅎㅎ 가족분들과 같이사세요?",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002633.1.18",
          "form": "네네 같이살아요",
          "original_form": "네네 같이살아요",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002633.1.19",
          "form": "참 집안일은 힘든것같아요ㅠ",
          "original_form": "참 집안일은 힘든것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002633.1.20",
          "form": "분담해서 하시나요?",
          "original_form": "분담해서 하시나요?",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002633.1.21",
          "form": "넵그런편이예요",
          "original_form": "넵그런편이예요",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100002633.1.22",
          "form": "저는 빨래보다 설거.지가너무 싫더라구요ㅠ",
          "original_form": "저는 빨래보다 설거.지가너무 싫더라구요ㅠ",
