      "utterance": [
        {
          "id": "MDRW2100022378.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022378.1.2",
          "form": "반갑습니다~ 날씨가 제법 선선해졌네요",
          "original_form": "반갑습니다~ 날씨가 제법 선선해졌네요",
          "speaker_id": "2",
          "time": "20210829 14:46"
        },
        {
          "id": "MDRW2100022378.1.3",
          "form": "처서이후 시원해졌어요",
          "original_form": "처서이후 시원해졌어요",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022378.1.4",
          "form": "다만 이 가을장마같은 날씨에 좀 습하기도 하네요",
          "original_form": "다만 이 가을장마같은 날씨에 좀 습하기도 하네요",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022378.1.5",
          "form": "맞아요~ 한동안 쾌청하더니 요즘 좀 습하네요",
          "original_form": "맞아요~ 한동안 쾌청하더니 요즘 좀 습하네요",
          "speaker_id": "2",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022378.1.6",
          "form": "다시 쾌청해지길 기다리고 있습니다",
          "original_form": "다시 쾌청해지길 기다리고 있습니다",
          "speaker_id": "1",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100022378.1.7",
          "form": "날이 흐리니 기분도 가라앉네요",
          "original_form": "날이 흐리니 기분도 가라앉네요",
          "speaker_id": "1",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022378.1.8",
          "form": "가을 장마 끝나도 계속 비예보가 있네요",
          "original_form": "가을 장마 끝나도 계속 비예보가 있네요",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022378.1.9",
          "form": "비피해는 없는 지역인가요?",
          "original_form": "비피해는 없는 지역인가요?",
          "speaker_id": "1",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022378.1.10",
          "form": "네 비 피해는 없어요~ 다만 저도 기분이 우울해지네요 계속 비가 오니",
          "original_form": "네 비 피해는 없어요~ 다만 저도 기분이 우울해지네요 계속 비가 오니",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022378.1.11",
          "form": "한국이 언제부터 가을장마가 있었나요",
          "original_form": "한국이 언제부터 가을장마가 있었나요",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022378.1.12",
          "form": "요즘 날씨가 정말 이상합니다",
          "original_form": "요즘 날씨가 정말 이상합니다",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022378.1.13",
          "form": "그러게요 하루에도 몇번씩 날씨가 변하고 이상하긴 합니다",
          "original_form": "그러게요 하루에도 몇번씩 날씨가 변하고 이상하긴 합니다",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022378.1.14",
          "form": "그러니까요 다음주도 비소식이 많더군요",
          "original_form": "그러니까요 다음주도 비소식이 많더군요",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022378.1.15",
          "form": "농가가 걱정이 됩니다",
          "original_form": "농가가 걱정이 됩니다",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022378.1.16",
          "form": "올래 과일 농사 엉망이더라고요",
          "original_form": "올래 과일 농사 엉망이더라고요",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022378.1.17",
          "form": "사과 다 썩고... 안타깝습니다",
          "original_form": "사과 다 썩고... 안타깝습니다",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022378.1.18",
          "form": "아이고 추석 앞두고 다들 힘들겠군요",
          "original_form": "아이고 추석 앞두고 다들 힘들겠군요",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022378.1.19",
          "form": "과일 가격이 많이 오를 것 같습니다",
          "original_form": "과일 가격이 많이 오를 것 같습니다",
          "speaker_id": "2",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022378.1.20",
          "form": "어떤 농가 인터뷰하는데 몇 트럭 버렸다고 하더라고요..",
          "original_form": "어떤 농가 인터뷰하는데 몇 트럭 버렸다고 하더라고요..",
          "speaker_id": "2",
          "time": "20210829 16:54"
        }
      ]
