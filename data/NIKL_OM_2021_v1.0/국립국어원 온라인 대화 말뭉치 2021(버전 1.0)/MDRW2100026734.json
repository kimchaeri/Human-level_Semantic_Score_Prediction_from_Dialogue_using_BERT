      "utterance": [
        {
          "id": "MDRW2100026734.1.1",
          "form": "백신 맞았어요 이차 기다리는 중이에요",
          "original_form": "백신 맞았어요 이차 기다리는 중이에요",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026734.1.2",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026734.1.3",
          "form": "name1은 맞으셨어요??",
          "original_form": "&name1&은 맞으셨어요??",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026734.1.4",
          "form": "일차 맞았어요",
          "original_form": "일차 맞았어요",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026734.1.5",
          "form": "부작용 없으셨어요?",
          "original_form": "부작용 없으셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026734.1.6",
          "form": "저는 마지막주에 또 맞아요",
          "original_form": "저는 마지막주에 또 맞아요",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026734.1.7",
          "form": "부작용은 없었어요 약간 뻐근한정도??",
          "original_form": "부작용은 없었어요 약간 뻐근한정도??",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026734.1.8",
          "form": "와 ᆢ",
          "original_form": "와 ᆢ",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026734.1.9",
          "form": "괜찮으셨어요??)",
          "original_form": "괜찮으셨어요??)",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026734.1.10",
          "form": "막주면 금방이네요",
          "original_form": "막주면 금방이네요",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026734.1.11",
          "form": "네네 저도 뻐근한정도였어요",
          "original_form": "네네 저도 뻐근한정도였어요",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026734.1.12",
          "form": "부작용 걱정 엄청했는데",
          "original_form": "부작용 걱정 엄청했는데",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026734.1.13",
          "form": "생각보다 괜찮았어요",
          "original_form": "생각보다 괜찮았어요",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026734.1.14",
          "form": "원래는 추석전에 맞을 예정이라 속시원하다 생각했는데 이주 미뤄졌더라고요",
          "original_form": "원래는 추석전에 맞을 예정이라 속시원하다 생각했는데 이주 미뤄졌더라고요",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026734.1.15",
          "form": "부작용 걱정은 이차때 조금 해야될수도 있을거 같아요",
          "original_form": "부작용 걱정은 이차때 조금 해야될수도 있을거 같아요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026734.1.16",
          "form": "아 ᆢ 수급이 힘든가봐요ㅜㅜ",
          "original_form": "아 ᆢ 수급이 힘든가봐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026734.1.17",
          "form": "화이자는 이차가 아프대요",
          "original_form": "화이자는 이차가 아프대요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026734.1.18",
          "form": "이차가 더 무섭긴해요ㅜ",
          "original_form": "이차가 더 무섭긴해요ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026734.1.19",
          "form": "화이자 맞으셨어요?",
          "original_form": "화이자 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026734.1.20",
          "form": "네 화이자였어요 뭐 맞으셨어요??",
          "original_form": "네 화이자였어요 뭐 맞으셨어요??",
