      "utterance": [
        {
          "id": "MDRW2100036473.1.1",
          "form": "너 빽다방 자주 가?",
          "original_form": "너 빽다방 자주 가?",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036473.1.2",
          "form": "요즘 영화 뭐 재밌는 거 있나",
          "original_form": "요즘 영화 뭐 재밌는 거 있나",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036473.1.3",
          "form": "응 나는 내일 코다라는 영화를 보러가",
          "original_form": "응 나는 내일 코다라는 영화를 보러가",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.4",
          "form": "코시국이라그런지 영화산업들이 침체된 거 같아",
          "original_form": "코시국이라그런지 영화산업들이 침체된 거 같아",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.5",
          "form": "어떤건지 들어봤어?",
          "original_form": "어떤건지 들어봤어?",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.6",
          "form": "코다리?",
          "original_form": "코다리?",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.7",
          "form": "코다리찜에 대한 일대기야?",
          "original_form": "코다리찜에 대한 일대기야?",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.8",
          "form": "아니ㅠㅠㅠ 영화제목이 코다",
          "original_form": "아니ㅠㅠㅠ 영화제목이 코다",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.9",
          "form": "음악영화야",
          "original_form": "음악영화야",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.10",
          "form": "아아 내가 실례했네",
          "original_form": "아아 내가 실례했네",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.11",
          "form": "한국영화야?",
          "original_form": "한국영화야?",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.12",
          "form": "예고편을 보니까 주인공들이",
          "original_form": "예고편을 보니까 주인공들이",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.13",
          "form": "노래를 잘하더라",
          "original_form": "노래를 잘하더라",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.14",
          "form": "음악을 좋아하는거야 너는?",
          "original_form": "음악을 좋아하는거야 너는?",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.15",
          "form": "아니아니 외국영화야",
          "original_form": "아니아니 외국영화야",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.16",
          "form": "예약은 한거야?",
          "original_form": "예약은 한거야?",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.17",
          "form": "응 나는 음악영화를 즐겨봐",
          "original_form": "응 나는 음악영화를 즐겨봐",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.18",
          "form": "아아 그럼그렇지",
          "original_form": "아아 그럼그렇지",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.19",
          "form": "예상했어",
          "original_form": "예상했어",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.20",
          "form": "응 당연하지",
          "original_form": "응 당연하지",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.21",
          "form": "오늘은 쿠폰을 써서 예매했어",
          "original_form": "오늘은 쿠폰을 써서 예매했어",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.22",
          "form": "음악공염도 자주 봐?",
          "original_form": "음악공염도 자주 봐?",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036473.1.23",
          "form": "알뜰하네",
          "original_form": "알뜰하네",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036473.1.24",
          "form": "그러면 공짜야?",
          "original_form": "그러면 공짜야?",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036473.1.25",
          "form": "천원이야?",
          "original_form": "천원이야?",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036473.1.26",
          "form": "음 공연은 시간 맞추기가 어려워서 자주 못봐",
          "original_form": "음 공연은 시간 맞추기가 어려워서 자주 못봐",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036473.1.27",
          "form": "공짜야!",
          "original_form": "공짜야!",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036473.1.28",
          "form": "부지런하구나",
          "original_form": "부지런하구나",
          "speaker_id": "2",
          "time": "20210831 00:06"
        }
      ]
    }
  ]
