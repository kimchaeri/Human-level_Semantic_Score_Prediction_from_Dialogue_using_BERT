      "utterance": [
        {
          "id": "MDRW2100017562.1.1",
          "form": "name2은 주로 어디서 드세요?",
          "original_form": "&name2&은 주로 어디서 드세요?",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017562.1.2",
          "form": "로제떡볶이 밀키트도 괜찮더라구요",
          "original_form": "로제떡볶이 밀키트도 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017562.1.3",
          "form": "밀키트도 좋네요ㅋㅋㅋㅋ",
          "original_form": "밀키트도 좋네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017562.1.4",
          "form": "네 저렴하게 살수있어 좋더라구여",
          "original_form": "네 저렴하게 살수있어 좋더라구여",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017562.1.5",
          "form": "혹시 밀키트 추천 제품 있나요? ㅋㅋㅋ",
          "original_form": "혹시 밀키트 추천 제품 있나요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017562.1.6",
          "form": "라비퀸 매콤크림떡볶이 괜찮더라구요 ㅋㅋㅋ",
          "original_form": "라비퀸 매콤크림떡볶이 괜찮더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017562.1.7",
          "form": "종류가 많아서 오히려 더 못고르겠더라고요ㅠㅠㅠ",
          "original_form": "종류가 많아서 오히려 더 못고르겠더라고요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017562.1.8",
          "form": "오 감사해요 ㅋㅋㅋㅋㅋ",
          "original_form": "오 감사해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017562.1.9",
          "form": "리뷰를 봐도 뭔가 광고같고....",
          "original_form": "리뷰를 봐도 뭔가 광고같고....",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017562.1.10",
          "form": "맞아요 요새 광고리뷰가 너무 많아서요 ㅠ",
          "original_form": "맞아요 요새 광고리뷰가 너무 많아서요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017562.1.11",
          "form": "쇼핑몰에서 다들 리뷰관리도 한다고 하니까ㅠㅠ",
          "original_form": "쇼핑몰에서 다들 리뷰관리도 한다고 하니까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017562.1.12",
          "form": "와 소름돋네요 ㅠㅠ",
          "original_form": "와 소름돋네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017562.1.13",
          "form": "마냥 좋다고만 하는거는 더 의심가더라고요",
          "original_form": "마냥 좋다고만 하는거는 더 의심가더라고요",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017562.1.14",
          "form": "그래서 전 일부러 별점 낮은 순으로 보기도 해요 ㅋㅋㅋ",
          "original_form": "그래서 전 일부러 별점 낮은 순으로 보기도 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017562.1.15",
          "form": "아 그거도 꿀팁이네요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 그거도 꿀팁이네요 ㅋㅋㅋㅋㅋ",
