      "utterance": [
        {
          "id": "MDRW2100022297.1.1",
          "form": "지금 생각해보니까 그래도 1등 소고기에요",
          "original_form": "지금 생각해보니까 그래도 1등 소고기에요",
          "speaker_id": "2",
          "time": "20210829 12:13"
        },
        {
          "id": "MDRW2100022297.1.2",
          "form": "소고기 맛은 있는데",
          "original_form": "소고기 맛은 있는데",
          "speaker_id": "1",
          "time": "20210829 12:13"
        },
        {
          "id": "MDRW2100022297.1.3",
          "form": "꽃등심 먹어봐써요?",
          "original_form": "꽃등심 먹어봐써요?",
          "speaker_id": "2",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.4",
          "form": "먹다보면 느끼해서 많이 못먹어요",
          "original_form": "먹다보면 느끼해서 많이 못먹어요",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.5",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.6",
          "form": "사실 꽃등심",
          "original_form": "사실 꽃등심",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.7",
          "form": "맛있는지 잘 모르겠어요",
          "original_form": "맛있는지 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.8",
          "form": "허걱 ㅜㅜㅜ",
          "original_form": "허걱 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.9",
          "form": "그러고보니 저 삼겹살 제주도에서 먹은거 잊을 수 없어요",
          "original_form": "그러고보니 저 삼겹살 제주도에서 먹은거 잊을 수 없어요",
          "speaker_id": "2",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.10",
          "form": "꽃등심 좋아하시는구사",
          "original_form": "꽃등심 좋아하시는구사",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.11",
          "form": "흑돼지요?",
          "original_form": "흑돼지요?",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.12",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.13",
          "form": "어때요",
          "original_form": "어때요",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.14",
          "form": "흑돼지 먹어봤어요?",
          "original_form": "흑돼지 먹어봤어요?",
          "speaker_id": "2",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.15",
          "form": "아니요ㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.16",
          "form": "저는 흑돼지 진짜 삼겹살왕이라고 생각해요",
          "original_form": "저는 흑돼지 진짜 삼겹살왕이라고 생각해요",
          "speaker_id": "2",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.17",
          "form": "큰 차이가 느껴져요?",
          "original_form": "큰 차이가 느껴져요?",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100022297.1.18",
          "form": "쫄깃한데 바삭하고 미쳤어요",
          "original_form": "쫄깃한데 바삭하고 미쳤어요",
          "speaker_id": "2",
          "time": "20210829 12:15"
        }
      ]
