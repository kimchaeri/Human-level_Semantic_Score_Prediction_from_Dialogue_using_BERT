      "utterance": [
        {
          "id": "MDRW2100002249.1.1",
          "form": "코로나라 조심스러워서",
          "original_form": "코로나라 조심스러워서",
          "speaker_id": "1",
          "time": "20210820 01:21"
        },
        {
          "id": "MDRW2100002249.1.2",
          "form": "집에 사람오는거싫어서요",
          "original_form": "집에 사람오는거싫어서요",
          "speaker_id": "2",
          "time": "20210820 01:21"
        },
        {
          "id": "MDRW2100002249.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 01:21"
        },
        {
          "id": "MDRW2100002249.1.4",
          "form": "그것도그렇구요",
          "original_form": "그것도그렇구요",
          "speaker_id": "2",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002249.1.5",
          "form": "혼자 힘드시겠어요",
          "original_form": "혼자 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002249.1.6",
          "form": "좁아서 불편할거같아요",
          "original_form": "좁아서 불편할거같아요",
          "speaker_id": "2",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002249.1.7",
          "form": "꼭 무리하지 마세요",
          "original_form": "꼭 무리하지 마세요",
          "speaker_id": "1",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002249.1.8",
          "form": "저 그러다 손목 발목이 아직 안좋아요",
          "original_form": "저 그러다 손목 발목이 아직 안좋아요",
          "speaker_id": "1",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002249.1.9",
          "form": "이런ㅠ",
          "original_form": "이런ㅠ",
          "speaker_id": "2",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002249.1.10",
          "form": "근데맘대로 안되여",
          "original_form": "근데맘대로 안되여",
          "speaker_id": "2",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002249.1.11",
          "form": "누구든 오면 좁고 불편해요",
          "original_form": "누구든 오면 좁고 불편해요",
          "speaker_id": "1",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002249.1.12",
          "form": "저도요 자꾸 움직이는 스타일이라",
          "original_form": "저도요 자꾸 움직이는 스타일이라",
          "speaker_id": "2",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002249.1.13",
          "form": "힘쓰고",
          "original_form": "힘쓰고",
          "speaker_id": "1",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002249.1.14",
          "form": "ㅋㅋ큰일인데요",
          "original_form": "ㅋㅋ큰일인데요",
          "speaker_id": "1",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002249.1.15",
          "form": "요새 답답하시겠어요 움직이면 자주 나가고 싶으실텐대",
          "original_form": "요새 답답하시겠어요 움직이면 자주 나가고 싶으실텐대",
          "speaker_id": "1",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002249.1.16",
          "form": "심심해요ㅠ",
          "original_form": "심심해요ㅠ",
          "speaker_id": "2",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002249.1.17",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002249.1.18",
          "form": "애랑 둘이ᆞ",
          "original_form": "애랑 둘이ᆞ",
          "speaker_id": "2",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002249.1.19",
          "form": "전 이사오고 코로나 터져서 친구도 못사귀고",
          "original_form": "전 이사오고 코로나 터져서 친구도 못사귀고",
          "speaker_id": "1",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002249.1.20",
          "form": "심심해요",
          "original_form": "심심해요",
          "speaker_id": "1",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002249.1.21",
          "form": "심심하시조ᆢ",
          "original_form": "심심하시조ᆢ",
          "speaker_id": "2",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002249.1.22",
          "form": "요새 참 누굴 만날수가 없어요",
          "original_form": "요새 참 누굴 만날수가 없어요",
          "speaker_id": "1",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002249.1.23",
          "form": "네 그전에 엄청 돌아 다녔으니 심심해요",
          "original_form": "네 그전에 엄청 돌아 다녔으니 심심해요",
          "speaker_id": "1",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002249.1.24",
          "form": "그렁 더 심심하죠",
          "original_form": "그렁 더 심심하죠",
          "speaker_id": "2",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002249.1.25",
          "form": "갭이커서",
          "original_form": "갭이커서",
          "speaker_id": "2",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002249.1.26",
          "form": "네 ㅡㅡ지금은 삶이 고요하죠",
          "original_form": "네 ㅡㅡ지금은 삶이 고요하죠",
          "speaker_id": "1",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002249.1.27",
          "form": "애가 크면 더 엄마들끼리 사귀기 힘들어요",
          "original_form": "애가 크면 더 엄마들끼리 사귀기 힘들어요",
          "speaker_id": "1",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002249.1.28",
          "form": "혼자사는 시대에요",
          "original_form": "혼자사는 시대에요",
          "speaker_id": "2",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002249.1.29",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002249.1.30",
          "form": "혼자 또 잘 놀고 있어요",
          "original_form": "혼자 또 잘 놀고 있어요",
          "speaker_id": "1",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002249.1.31",
          "form": "저도 혼자놀고싶어요ㅎㅎ애빼고",
          "original_form": "저도 혼자놀고싶어요ㅎㅎ애빼고",
          "speaker_id": "2",
          "time": "20210820 01:26"
        },
        {
          "id": "MDRW2100002249.1.32",
          "form": "ㅋㅋ 엄마들 꿈이죠",
          "original_form": "ㅋㅋ 엄마들 꿈이죠",
          "speaker_id": "1",
          "time": "20210820 01:26"
        },
        {
          "id": "MDRW2100002249.1.33",
          "form": "애들 없는 시간이 꿀이예요",
          "original_form": "애들 없는 시간이 꿀이예요",
          "speaker_id": "1",
          "time": "20210820 01:26"
        },
        {
          "id": "MDRW2100002249.1.34",
          "form": "ㅋㅋ제 꿈이요",
          "original_form": "ㅋㅋ제 꿈이요",
          "speaker_id": "2",
          "time": "20210820 01:26"
        },
        {
          "id": "MDRW2100002249.1.35",
          "form": "ㅎㅎ 이룰날 옵니다",
          "original_form": "ㅎㅎ 이룰날 옵니다",
          "speaker_id": "1",
          "time": "20210820 01:27"
        },
        {
          "id": "MDRW2100002249.1.36",
          "form": "ㅋㅋ제발요",
          "original_form": "ㅋㅋ제발요",
          "speaker_id": "2",
          "time": "20210820 01:27"
        },
        {
          "id": "MDRW2100002249.1.37",
          "form": "ㅋㅋ 와요",
          "original_form": "ㅋㅋ 와요",
          "speaker_id": "1",
          "time": "20210820 01:27"
        },
        {
          "id": "MDRW2100002249.1.38",
          "form": "어서요",
          "original_form": "어서요",
          "speaker_id": "2",
          "time": "20210820 01:27"
        },
        {
          "id": "MDRW2100002249.1.39",
          "form": "어서와요 와라와라 홧팅",
          "original_form": "어서와요 와라와라 홧팅",
          "speaker_id": "1",
          "time": "20210820 01:27"
        },
        {
          "id": "MDRW2100002249.1.40",
          "form": "오늘은 이만하고 또봐요 눈이 아파요 ㅡㅡ",
          "original_form": "오늘은 이만하고 또봐요 눈이 아파요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210820 01:28"
        },
        {
          "id": "MDRW2100002249.1.41",
          "form": "넵ㅋ주무세요",
          "original_form": "넵ㅋ주무세요",
          "speaker_id": "2",
          "time": "20210820 01:28"
        },
        {
          "id": "MDRW2100002249.1.42",
          "form": "주무세요 ㅎㅎ",
          "original_form": "주무세요 ㅎㅎ",
          "speaker_id": "1",
