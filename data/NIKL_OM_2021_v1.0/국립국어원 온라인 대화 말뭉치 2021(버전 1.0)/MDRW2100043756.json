        },
        {
          "id": "MDRW2100043756.1.2",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043756.1.3",
          "form": "이미 충분히 배부른데도",
          "original_form": "이미 충분히 배부른데도",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043756.1.4",
          "form": "ㅎㅎㅎ당기네요",
          "original_form": "ㅎㅎㅎ당기네요",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043756.1.5",
          "form": "치킨 먹고싶어요ㅠ",
          "original_form": "치킨 먹고싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043756.1.6",
          "form": "아는언니두 치킨먹는다해서",
