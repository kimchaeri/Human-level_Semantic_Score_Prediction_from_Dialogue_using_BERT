      "utterance": [
        {
          "id": "MDRW2100043748.1.1",
          "form": "진정한 결혼에 대한 그린 드라마라네",
          "original_form": "진정한 결혼에 대한 그린 드라마라네",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043748.1.2",
          "form": "진정한 결혼이라는게 있나ㅋ",
          "original_form": "진정한 결혼이라는게 있나ㅋ",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043748.1.3",
          "form": "50부작",
          "original_form": "50부작",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043748.1.4",
          "form": "이거는 좀 줄긴 해도 그래도 50부작이네요",
          "original_form": "이거는 좀 줄긴 해도 그래도 50부작이네요",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043748.1.5",
          "form": "최명길이 주연이고",
          "original_form": "최명길이 주연이고",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043748.1.6",
          "form": "당연히 주연이겠죠 ㅋ",
          "original_form": "당연히 주연이겠죠 ㅋ",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043748.1.7",
          "form": "서브주연이 대단",
          "original_form": "서브주연이 대단",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043748.1.8",
          "form": "또 뭐 찍었어요?",
          "original_form": "또 뭐 찍었어요?",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043748.1.9",
          "form": "조민수 유호정",
          "original_form": "조민수 유호정",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043748.1.10",
          "form": "사랑은 없다를 그다음해에",
          "original_form": "사랑은 없다를 그다음해에",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043748.1.11",
          "form": "엄청 주연작이 많다",
          "original_form": "엄청 주연작이 많다",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043748.1.12",
          "form": "100편이 넘겠다",
          "original_form": "100편이 넘겠다",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043748.1.13",
          "form": "이건 좀 짧네",
          "original_form": "이건 좀 짧네",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100043748.1.14",
          "form": "최명길 영화도 많이 찍었자나요",
          "original_form": "최명길 영화도 많이 찍었자나요",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100043748.1.15",
          "form": "영화는 조금 찍었지",
          "original_form": "영화는 조금 찍었지",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100043748.1.16",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043748.1.17",
          "form": "엉 드라마가 상당히 많음",
          "original_form": "엉 드라마가 상당히 많음",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043748.1.18",
          "form": "영화배우는 아니라서 그런가보네요",
          "original_form": "영화배우는 아니라서 그런가보네요",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043748.1.19",
          "form": "엉 영화는 써니 특별출연",
          "original_form": "엉 영화는 써니 특별출연",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043748.1.20",
          "form": "저 써니 봤는데 기억 안나요",
          "original_form": "저 써니 봤는데 기억 안나요",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043748.1.21",
          "form": "장미빛 인생",
          "original_form": "장미빛 인생",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043748.1.22",
          "form": "영화죠?  강석우 나왔죠?",
          "original_form": "영화죠?  강석우 나왔죠?",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043748.1.23",
          "form": "나도 기억안난다 ㅋㅋ",
          "original_form": "나도 기억안난다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:25"
        }
      ]
