      "utterance": [
        {
          "id": "MDRW2100023819.1.1",
          "form": "헐 그럼 어케요?",
          "original_form": "헐 그럼 어케요?",
          "speaker_id": "1",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100023819.1.2",
          "form": "적어도 두 시간정도는 솜 물고 있었던거같아요",
          "original_form": "적어도 두 시간정도는 솜 물고 있었던거같아요",
          "speaker_id": "2",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100023819.1.3",
          "form": "생니 잡아뽑고 피가 멈추길 기다려야하나요?",
          "original_form": "생니 잡아뽑고 피가 멈추길 기다려야하나요?",
          "speaker_id": "1",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100023819.1.4",
          "form": "악 너무무서워요",
          "original_form": "악 너무무서워요",
          "speaker_id": "1",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100023819.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100023819.1.6",
          "form": "마취 풀리면 아픈가요?",
          "original_form": "마취 풀리면 아픈가요?",
          "speaker_id": "1",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100023819.1.7",
          "form": "아뇨 저는 안 아팠어요",
          "original_form": "아뇨 저는 안 아팠어요",
          "speaker_id": "2",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100023819.1.8",
          "form": "근데 이 빠진 곳을 꿰메거든요",
          "original_form": "근데 이 빠진 곳을 꿰메거든요",
          "speaker_id": "2",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100023819.1.9",
          "form": "아 헐",
          "original_form": "아 헐",
          "speaker_id": "1",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100023819.1.10",
          "form": "잇몸을 꿰멜수도 있군요",
          "original_form": "잇몸을 꿰멜수도 있군요",
          "speaker_id": "1",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100023819.1.11",
          "form": "처음 알았어요",
          "original_form": "처음 알았어요",
          "speaker_id": "1",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100023819.1.12",
          "form": "네 잇몸을 꿰메더라구요..",
          "original_form": "네 잇몸을 꿰메더라구요..",
          "speaker_id": "2",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100023819.1.13",
          "form": "입 안 살이랑 같이..",
          "original_form": "입 안 살이랑 같이..",
          "speaker_id": "2",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100023819.1.14",
          "form": "으악",
          "original_form": "으악",
          "speaker_id": "1",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100023819.1.15",
          "form": "공포실화",
          "original_form": "공포실화",
          "speaker_id": "1",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100023819.1.16",
          "form": "입벌릴때 마다 압안 살이 땡겨져서 아프더라구요..",
          "original_form": "입벌릴때 마다 압안 살이 땡겨져서 아프더라구요..",
          "speaker_id": "2",
          "time": "20210830 18:05"
        },
        {
          "id": "MDRW2100023819.1.17",
          "form": "금방 나으셨나요???",
          "original_form": "금방 나으셨나요???",
          "speaker_id": "1",
          "time": "20210830 18:05"
        },
        {
          "id": "MDRW2100023819.1.18",
          "form": "네 저는 어릴때 빼서 그런지 아물긴 금방 아물었어요",
          "original_form": "네 저는 어릴때 빼서 그런지 아물긴 금방 아물었어요",
          "speaker_id": "2",
          "time": "20210830 18:05"
        },
        {
          "id": "MDRW2100023819.1.19",
          "form": "이 빼고 이틀뒤에 실 빼고",
          "original_form": "이 빼고 이틀뒤에 실 빼고",
          "speaker_id": "2",
          "time": "20210830 18:05"
        },
        {
          "id": "MDRW2100023819.1.20",
          "form": "악 실 뽑아야해요?",
          "original_form": "악 실 뽑아야해요?",
          "speaker_id": "1",
          "time": "20210830 18:06"
        },
        {
          "id": "MDRW2100023819.1.21",
          "form": "일주일정도 있다가 다 아문거같아요",
          "original_form": "일주일정도 있다가 다 아문거같아요",
          "speaker_id": "2",
          "time": "20210830 18:06"
        },
        {
          "id": "MDRW2100023819.1.22",
          "form": "이 무시무시한걸 사람들이 하고있다니",
          "original_form": "이 무시무시한걸 사람들이 하고있다니",
          "speaker_id": "1",
