      "utterance": [
        {
          "id": "MDRW2100031178.1.1",
          "form": "어머니와 식사 했어요",
          "original_form": "어머니와 식사 했어요",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031178.1.2",
          "form": "점심 먹었어요",
          "original_form": "점심 먹었어요",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031178.1.3",
          "form": "저는 혼자 먹었어요",
          "original_form": "저는 혼자 먹었어요",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031178.1.4",
          "form": "주말인데 혼자 계시나요",
          "original_form": "주말인데 혼자 계시나요",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031178.1.5",
          "form": "혼자 먹으면 간편하게 먹어서 좋아요",
          "original_form": "혼자 먹으면 간편하게 먹어서 좋아요",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031178.1.6",
          "form": "뭘 드셨나요",
          "original_form": "뭘 드셨나요",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031178.1.7",
          "form": "남은 반찬을 먹었어요 밥이랑 함께",
          "original_form": "남은 반찬을 먹었어요 밥이랑 함께",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031178.1.8",
          "form": "반찬이 맛있는 곳이 있나요",
          "original_form": "반찬이 맛있는 곳이 있나요",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031178.1.9",
          "form": "소고기 요리랑 미역이 있어요",
          "original_form": "소고기 요리랑 미역이 있어요",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031178.1.10",
          "form": "해조류를 좋아하시나요",
          "original_form": "해조류를 좋아하시나요",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031178.1.11",
          "form": "다 좋아해요 음식은 가리지 않습니다",
          "original_form": "다 좋아해요 음식은 가리지 않습니다",
          "speaker_id": "1",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031178.1.12",
          "form": "저도요 근데 저는 채소 채소가 좋아요",
          "original_form": "저도요 근데 저는 채소 채소가 좋아요",
          "speaker_id": "2",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031178.1.13",
          "form": "저는 해산물을 좋아합니다",
          "original_form": "저는 해산물을 좋아합니다",
          "speaker_id": "1",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031178.1.14",
          "form": "해산물 중에 어떤 걸 좋아하세요",
          "original_form": "해산물 중에 어떤 걸 좋아하세요",
          "speaker_id": "2",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031178.1.15",
          "form": "새우를 가장 좋아해요",
          "original_form": "새우를 가장 좋아해요",
          "speaker_id": "1",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031178.1.16",
          "form": "저녁은 뭘 먹을 건가요",
          "original_form": "저녁은 뭘 먹을 건가요",
