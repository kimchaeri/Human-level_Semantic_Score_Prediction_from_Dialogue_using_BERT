      "utterance": [
        {
          "id": "MDRW2100037293.1.1",
          "form": "저두 동그란 얼굴이에요 !!",
          "original_form": "저두 동그란 얼굴이에요 !!",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037293.1.2",
          "form": "안그래도 머리하고싶어서 ! 숏컷 찾아보고 있었거든요",
          "original_form": "안그래도 머리하고싶어서 ! 숏컷 찾아보고 있었거든요",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037293.1.3",
          "form": "엇 그러면 자주 가시는  미용실 가셔서 상담 받아보시는 건 어떨까요??",
          "original_form": "엇 그러면 자주 가시는  미용실 가셔서 상담 받아보시는 건 어떨까요??",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037293.1.4",
          "form": "아아 하긴",
          "original_form": "아아 하긴",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037293.1.5",
          "form": "최근에 안산선수 로이드 화보 혹시 보셨나요?",
          "original_form": "최근에 안산선수 로이드 화보 혹시 보셨나요?",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037293.1.6",
          "form": "네네 !!!",
          "original_form": "네네 !!!",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037293.1.7",
          "form": "저 그런 머리였는데 잘 어울린다는 소리 많이 들었어요!",
          "original_form": "저 그런 머리였는데 잘 어울린다는 소리 많이 들었어요!",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037293.1.8",
          "form": "약간 쉼표 같은 머리요 ㅎㅎ",
          "original_form": "약간 쉼표 같은 머리요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037293.1.9",
          "form": "아아 ! 산이 머리보구",
          "original_form": "아아 ! 산이 머리보구",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037293.1.10",
          "form": "숏컷 뽐뿌왔는데 ㅎㅎ",
          "original_form": "숏컷 뽐뿌왔는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037293.1.11",
          "form": "상담 한번 받아야겠어요 ㅋㅋㅋ",
          "original_form": "상담 한번 받아야겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037293.1.12",
          "form": "상담해보고 아닌 것 같으면 그냥 나오면 되죠 ㅎㅎ",
          "original_form": "상담해보고 아닌 것 같으면 그냥 나오면 되죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100037293.1.13",
          "form": "맞아요 ㅎㅎㅎ 이번에 지원금 주는걸로",
          "original_form": "맞아요 ㅎㅎㅎ 이번에 지원금 주는걸로",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037293.1.14",
          "form": "하러가겠습니돠 ㅎㅎ",
          "original_form": "하러가겠습니돠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037293.1.15",
          "form": "상담받구 아니다싶으면 염색이라도 해야겠어요 ㅋㅋ",
          "original_form": "상담받구 아니다싶으면 염색이라도 해야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037293.1.16",
          "form": "오 저는 염색을 한번도 안해봤는데",
          "original_form": "오 저는 염색을 한번도 안해봤는데",
          "speaker_id": "2",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037293.1.17",
          "form": "미용실에서 하면 보통 얼마나 걸리나요??",
          "original_form": "미용실에서 하면 보통 얼마나 걸리나요??",
          "speaker_id": "2",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037293.1.18",
          "form": "빠르면 1시간이요 !!",
          "original_form": "빠르면 1시간이요 !!",
          "speaker_id": "1",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037293.1.19",
          "form": "아 머리길이가 길면 오래걸릴텐데",
          "original_form": "아 머리길이가 길면 오래걸릴텐데",
          "speaker_id": "1",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037293.1.20",
          "form": "전 단발이구 숱도 안많아서",
          "original_form": "전 단발이구 숱도 안많아서",
          "speaker_id": "1",
          "time": "20210831 22:21"
        },
        {
          "id": "MDRW2100037293.1.21",
          "form": "보통 바르는데 15분 30분 방치 감고 말리는데 15분",
          "original_form": "보통 바르는데 15분 30분 방치 감고 말리는데 15분",
          "speaker_id": "1",
          "time": "20210831 22:21"
        },
        {
          "id": "MDRW2100037293.1.22",
          "form": "방치 30분 ! 뭔가 문장이 어렵군요 ㅋㅋ",
          "original_form": "방치 30분 ! 뭔가 문장이 어렵군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100037293.1.23",
          "form": "오 생각보다 금방 끝나네요 ㅋㅋㅋㅋ",
          "original_form": "오 생각보다 금방 끝나네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100037293.1.24",
          "form": "네네 ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100037293.1.25",
          "form": "머리 짧을 때 염색 한번 해볼까 싶어요 ㅋㅋㅋ",
          "original_form": "머리 짧을 때 염색 한번 해볼까 싶어요 ㅋㅋㅋ",
