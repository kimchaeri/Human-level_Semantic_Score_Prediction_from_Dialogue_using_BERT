      "utterance": [
        {
          "id": "MDRW2100019147.1.1",
          "form": "혹시 모가디슈 보셨어요?",
          "original_form": "혹시 모가디슈 보셨어요?",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019147.1.2",
          "form": "아뇨 영화관에서 하는 영화는 본지 오래됐어요 ㅠㅠ",
          "original_form": "아뇨 영화관에서 하는 영화는 본지 오래됐어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019147.1.3",
          "form": "저도요ㅜㅜ",
          "original_form": "저도요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019147.1.4",
          "form": "보고픈데ㅜ",
          "original_form": "보고픈데ㅜ",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019147.1.5",
          "form": "영화관 자체를 간지가 ㅋㅋㅋ",
          "original_form": "영화관 자체를 간지가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100019147.1.6",
          "form": "전 모가디슈가 뭔지도 몰랐어요 ㅋㅋㅋ",
          "original_form": "전 모가디슈가 뭔지도 몰랐어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100019147.1.7",
          "form": "ㅋㅋ 그거랑 인질, 싱크홀 3작품이 유행이라고",
          "original_form": "ㅋㅋ 그거랑 인질, 싱크홀 3작품이 유행이라고",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019147.1.8",
          "form": "아그래요? 다 초면이네요 ㅋㅋㅋㅋㅋ",
          "original_form": "아그래요? 다 초면이네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100019147.1.9",
          "form": "요새 영화 예고편도 티비에서 안 하더라구요",
          "original_form": "요새 영화 예고편도 티비에서 안 하더라구요",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100019147.1.10",
          "form": "그러더라구요ㅋㅋㅋ",
          "original_form": "그러더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019147.1.11",
          "form": "혹시 예능 좋아하세용",
          "original_form": "혹시 예능 좋아하세용",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019147.1.12",
          "form": "좋아하는데 요새 너무 볼게없더라구요 저는 ㅠㅠ",
          "original_form": "좋아하는데 요새 너무 볼게없더라구요 저는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019147.1.13",
          "form": "보시는거 있으세요?",
          "original_form": "보시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019147.1.14",
          "form": "저는 식스센스 재밌더라구요",
          "original_form": "저는 식스센스 재밌더라구요",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100019147.1.15",
          "form": "빵빵 터져요",
          "original_form": "빵빵 터져요",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100019147.1.16",
          "form": "아 유재석 나오는건가요?",
          "original_form": "아 유재석 나오는건가요?",
          "speaker_id": "2",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100019147.1.17",
          "form": "네 미주랑 제시랑",
          "original_form": "네 미주랑 제시랑",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100019147.1.18",
          "form": "진짜 웃깁니다",
          "original_form": "진짜 웃깁니다",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100019147.1.19",
          "form": "오 저도 봐봐야겠어요",
          "original_form": "오 저도 봐봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100019147.1.20",
          "form": "강추에요 유재석당하는거 보는 재미도 있어요",
          "original_form": "강추에요 유재석당하는거 보는 재미도 있어요",
          "speaker_id": "1",
