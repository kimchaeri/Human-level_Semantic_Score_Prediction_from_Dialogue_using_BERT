      "utterance": [
        {
          "id": "MDRW2100042686.1.1",
          "form": "인터넷에서 뭐 산 거 없니",
          "original_form": "인터넷에서 뭐 산 거 없니",
          "speaker_id": "2",
          "time": "20210907 01:21"
        },
        {
          "id": "MDRW2100042686.1.2",
          "form": "나 최근에 레토르트 상품을 좀 많이 샀어",
          "original_form": "나 최근에 레토르트 상품을 좀 많이 샀어",
          "speaker_id": "1",
          "time": "20210907 01:21"
        },
        {
          "id": "MDRW2100042686.1.3",
          "form": "냉동식품이니",
          "original_form": "냉동식품이니",
          "speaker_id": "2",
          "time": "20210907 01:21"
        },
        {
          "id": "MDRW2100042686.1.4",
          "form": "맞아 그래서 냉동고 가 가득 차 있어",
          "original_form": "맞아 그래서 냉동고 가 가득 차 있어",
          "speaker_id": "1",
          "time": "20210907 01:22"
        },
        {
          "id": "MDRW2100042686.1.5",
          "form": "주로 어떤 음식이야",
          "original_form": "주로 어떤 음식이야",
          "speaker_id": "2",
          "time": "20210907 01:22"
        },
        {
          "id": "MDRW2100042686.1.6",
          "form": "오븐구이 치킨",
          "original_form": "오븐구이 치킨",
          "speaker_id": "1",
          "time": "20210907 01:22"
        },
        {
          "id": "MDRW2100042686.1.7",
          "form": "신기한 상품이네",
          "original_form": "신기한 상품이네",
          "speaker_id": "2",
          "time": "20210907 01:22"
        },
        {
          "id": "MDRW2100042686.1.8",
          "form": "신세계에서 나온 신작품이야",
          "original_form": "신세계에서 나온 신작품이야",
          "speaker_id": "1",
          "time": "20210907 01:23"
        },
        {
          "id": "MDRW2100042686.1.9",
          "form": "나는 요즘 냉동 만두를 많이 샀어",
          "original_form": "나는 요즘 냉동 만두를 많이 샀어",
          "speaker_id": "2",
          "time": "20210907 01:23"
        },
        {
          "id": "MDRW2100042686.1.10",
          "form": "냉동 만두는 무슨 만두니",
          "original_form": "냉동 만두는 무슨 만두니",
          "speaker_id": "1",
          "time": "20210907 01:23"
        },
        {
          "id": "MDRW2100042686.1.11",
          "form": "명란 만도 야 진짜",
          "original_form": "명란 만도 야 진짜",
          "speaker_id": "2",
          "time": "20210907 01:23"
        },
        {
          "id": "MDRW2100042686.1.12",
          "form": "군만두?",
          "original_form": "군만두?",
          "speaker_id": "1",
          "time": "20210907 01:24"
        },
        {
          "id": "MDRW2100042686.1.13",
          "form": "맞아 군만두 였어",
          "original_form": "맞아 군만두 였어",
          "speaker_id": "2",
          "time": "20210907 01:24"
        },
        {
          "id": "MDRW2100042686.1.14",
          "form": "맛있겠다",
