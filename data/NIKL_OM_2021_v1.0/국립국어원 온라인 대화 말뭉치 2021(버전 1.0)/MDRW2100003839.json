      "utterance": [
        {
          "id": "MDRW2100003839.1.1",
          "form": "아 장난 많으면 힘들죠",
          "original_form": "아 장난 많으면 힘들죠",
          "speaker_id": "2",
          "time": "20210821 12:47"
        },
        {
          "id": "MDRW2100003839.1.2",
          "form": "초반엔 울기도 많이 울었어요",
          "original_form": "초반엔 울기도 많이 울었어요",
          "speaker_id": "1",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003839.1.3",
          "form": "전 성격이 장난치고 그런거 안좋아하는데",
          "original_form": "전 성격이 장난치고 그런거 안좋아하는데",
          "speaker_id": "1",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003839.1.4",
          "form": "헉 심한 장난이에요?",
          "original_form": "헉 심한 장난이에요?",
          "speaker_id": "2",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003839.1.5",
          "form": "좀 짓궂은?",
          "original_form": "좀 짓궂은?",
          "speaker_id": "1",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003839.1.6",
          "form": "근데 싫다고하면 안하긴해요",
          "original_form": "근데 싫다고하면 안하긴해요",
          "speaker_id": "1",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003839.1.7",
          "form": "아~~너무 싫겠어요",
          "original_form": "아~~너무 싫겠어요",
          "speaker_id": "2",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003839.1.8",
          "form": "성격차이죠 뭐ㅋㅋㅋ",
          "original_form": "성격차이죠 뭐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003839.1.9",
          "form": "이런게 안맞아요",
          "original_form": "이런게 안맞아요",
          "speaker_id": "1",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003839.1.10",
          "form": "근데 싫다고하면 바로 안하니까",
          "original_form": "근데 싫다고하면 바로 안하니까",
          "speaker_id": "1",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.11",
          "form": "저희 남편도 장난이 심해서 싸와요 ㅋ",
          "original_form": "저희 남편도 장난이 심해서 싸와요 ㅋ",
          "speaker_id": "2",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.12",
          "form": "그러려니하고...",
          "original_form": "그러려니하고...",
          "speaker_id": "1",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.13",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.14",
          "form": "남자들 다 비슷한가봐요",
          "original_form": "남자들 다 비슷한가봐요",
          "speaker_id": "1",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.15",
          "form": "말로도 엄청 놀려요",
          "original_form": "말로도 엄청 놀려요",
          "speaker_id": "2",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.16",
          "form": "헐 저도ㅡㅡ",
          "original_form": "헐 저도ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.17",
          "form": "심지어 잠깐 헤어졌을때",
          "original_form": "심지어 잠깐 헤어졌을때",
          "speaker_id": "1",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.18",
          "form": "참다참다 싸우게 되요",
          "original_form": "참다참다 싸우게 되요",
          "speaker_id": "2",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.19",
          "form": "친구들이랑 여행갔다왔거든요?",
          "original_form": "친구들이랑 여행갔다왔거든요?",
          "speaker_id": "1",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.20",
          "form": "네~~무슨일이?",
          "original_form": "네~~무슨일이?",
          "speaker_id": "2",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.21",
          "form": "근데 그거가지고 새남친 생겼다고",
          "original_form": "근데 그거가지고 새남친 생겼다고",
          "speaker_id": "1",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.22",
          "form": "뭐만하면 니 새남친이랑 놀아",
          "original_form": "뭐만하면 니 새남친이랑 놀아",
          "speaker_id": "1",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003839.1.23",
          "form": "새남친이랑 해",
          "original_form": "새남친이랑 해",
          "speaker_id": "1",
          "time": "20210821 12:50"
        },
        {
          "id": "MDRW2100003839.1.24",
          "form": "아이고 진짜",
          "original_form": "아이고 진짜",
          "speaker_id": "1",
          "time": "20210821 12:50"
        },
        {
          "id": "MDRW2100003839.1.25",
          "form": "헉!!! 대박",
          "original_form": "헉!!! 대박",
          "speaker_id": "2",
          "time": "20210821 12:50"
        },
        {
          "id": "MDRW2100003839.1.26",
          "form": "1절까지만 하는법이 없어요",
          "original_form": "1절까지만 하는법이 없어요",
          "speaker_id": "1",
          "time": "20210821 12:50"
        },
        {
          "id": "MDRW2100003839.1.27",
          "form": "그러면 너무 짜증나는데",
          "original_form": "그러면 너무 짜증나는데",
          "speaker_id": "2",
          "time": "20210821 12:50"
        }
