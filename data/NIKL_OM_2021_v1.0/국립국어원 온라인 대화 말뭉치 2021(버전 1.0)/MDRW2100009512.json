      "utterance": [
        {
          "id": "MDRW2100009512.1.1",
          "form": "연애와 결혼이라",
          "original_form": "연애와 결혼이라",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009512.1.2",
          "form": "방금 연애 상대해줬지",
          "original_form": "방금 연애 상대해줬지",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009512.1.3",
          "form": "고등학생인데",
          "original_form": "고등학생인데",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009512.1.4",
          "form": "왠만한 성인보다",
          "original_form": "왠만한 성인보다",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009512.1.5",
          "form": "서로를 배려하고 생각하면서",
          "original_form": "서로를 배려하고 생각하면서",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009512.1.6",
          "form": "아름답고 깔끔하게 이별했더라",
          "original_form": "아름답고 깔끔하게 이별했더라",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009512.1.7",
          "form": "멋져멋져",
          "original_form": "멋져멋져",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009512.1.8",
          "form": "응 뭐가?",
          "original_form": "응 뭐가?",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009512.1.9",
          "form": "아는 동생인",
          "original_form": "아는 동생인",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009512.1.10",
          "form": "고등학생이",
          "original_form": "고등학생이",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009512.1.11",
          "form": "헤어져서 힘들다고",
          "original_form": "헤어져서 힘들다고",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009512.1.12",
          "form": "연애 상담했거든",
          "original_form": "연애 상담했거든",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009512.1.13",
          "form": "아하.. 고등학생ㅜ",
          "original_form": "아하.. 고등학생ㅜ",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009512.1.14",
          "form": "고1때 알게 됐는데",
          "original_form": "고1때 알게 됐는데",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009512.1.15",
          "form": "난 성인이 되었지",
          "original_form": "난 성인이 되었지",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009512.1.16",
          "form": "나만 늙네~~",
          "original_form": "나만 늙네~~",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009512.1.17",
          "form": "ㅠㅠ 내가 성인이라니 안 믿긴다",
          "original_form": "ㅠㅠ 내가 성인이라니 안 믿긴다",
          "speaker_id": "1",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009512.1.18",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009512.1.19",
          "form": "심지어 3학년???",
          "original_form": "심지어 3학년???",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009512.1.20",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009512.1.21",
          "form": "22살인데 20대를 즐기지 못 한 기분",
          "original_form": "22살인데 20대를 즐기지 못 한 기분",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009512.1.22",
          "form": "그니까.. 조선시대에는 지금 애낳을 나이인데",
          "original_form": "그니까.. 조선시대에는 지금 애낳을 나이인데",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100009512.1.23",
          "form": "그렇긴하지",
          "original_form": "그렇긴하지",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100009512.1.24",
          "form": "사화적인 영향도 있겠지만",
          "original_form": "사화적인 영향도 있겠지만",
          "speaker_id": "2",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100009512.1.25",
          "form": "일찍 결혼 한다는 게 신기하다",
          "original_form": "일찍 결혼 한다는 게 신기하다",
          "speaker_id": "2",
          "time": "20210824 18:13"
        },
        {
          "id": "MDRW2100009512.1.26",
          "form": "일찍 결혼 할 수록 배우자와 함께하는 시간이 길잖아",
          "original_form": "일찍 결혼 할 수록 배우자와 함께하는 시간이 길잖아",
          "speaker_id": "2",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100009512.1.27",
          "form": "못 해본 것도 많을 거고ㅠㅜ",
          "original_form": "못 해본 것도 많을 거고ㅠㅜ",
          "speaker_id": "2",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100009512.1.28",
          "form": "ㅠㅠ 난 일찍 하고싶지않아~~",
          "original_form": "ㅠㅠ 난 일찍 하고싶지않아~~",
          "speaker_id": "1",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100009512.1.29",
          "form": "나도!! 최대한 해 볼 거 다 하고",
          "original_form": "나도!! 최대한 해 볼 거 다 하고",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100009512.1.30",
          "form": "연이 되면 만나겠지",
          "original_form": "연이 되면 만나겠지",
          "speaker_id": "2",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100009512.1.31",
          "form": "맞다 안 만나도 그만..",
