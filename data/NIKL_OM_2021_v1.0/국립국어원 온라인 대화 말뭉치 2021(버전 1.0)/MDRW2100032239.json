      "utterance": [
        {
          "id": "MDRW2100032239.1.1",
          "form": "입주청소 확실히 잘해야겠네요",
          "original_form": "입주청소 확실히 잘해야겠네요",
          "speaker_id": "2",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100032239.1.2",
          "form": "가성비도 좋았는데 아쉽네요ㅠㅠ",
          "original_form": "가성비도 좋았는데 아쉽네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100032239.1.3",
          "form": "그니까요...ㅋ",
          "original_form": "그니까요...ㅋ",
          "speaker_id": "1",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100032239.1.4",
          "form": "입주청소도알아봐야하고ㅋㅋㅋ",
          "original_form": "입주청소도알아봐야하고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100032239.1.5",
          "form": "모든게돈입니다..증말ㅋㅋ",
          "original_form": "모든게돈입니다..증말ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100032239.1.6",
          "form": "돈쇼핑ㅋㅋㅋㅋ",
          "original_form": "돈쇼핑ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100032239.1.7",
          "form": "베이크아웃쇼핑",
          "original_form": "베이크아웃쇼핑",
          "speaker_id": "2",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100032239.1.8",
          "form": "ㅋㅋㅋ입주청소",
          "original_form": "ㅋㅋㅋ입주청소",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100032239.1.9",
          "form": "줄눈ㅋㅋㅋ",
          "original_form": "줄눈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100032239.1.10",
          "form": "새집증후군시공ㅋㅋ",
          "original_form": "새집증후군시공ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100032239.1.11",
          "form": "코팅인가?",
          "original_form": "코팅인가?",
          "speaker_id": "2",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100032239.1.12",
          "form": "그건안하시나요ㅋㅋ",
          "original_form": "그건안하시나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100032239.1.13",
          "form": "탄성ㅋㅋ",
          "original_form": "탄성ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100032239.1.14",
          "form": "나노코팅이요?ㅋㅋ",
          "original_form": "나노코팅이요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100032239.1.15",
          "form": "첨엔당연히하려했죠ㅋㅋㅋ",
          "original_form": "첨엔당연히하려했죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100032239.1.16",
          "form": "탄성말구",
          "original_form": "탄성말구",
          "speaker_id": "2",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100032239.1.17",
          "form": "뭔지는모르지만ㅋㅋ돈쓰는거하려고ㅋㅋ",
          "original_form": "뭔지는모르지만ㅋㅋ돈쓰는거하려고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100032239.1.18",
          "form": "화장실때안끼는코팅이있대요",
          "original_form": "화장실때안끼는코팅이있대요",
          "speaker_id": "2",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100032239.1.19",
          "form": "욕조,주방상판,싱크볼하는코팅ㅋㅋ",
          "original_form": "욕조,주방상판,싱크볼하는코팅ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100032239.1.20",
          "form": "그거죠?",
          "original_form": "그거죠?",
          "speaker_id": "1",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100032239.1.21",
          "form": "아 맞는듯",
          "original_form": "아 맞는듯",
          "speaker_id": "2",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100032239.1.22",
          "form": "처음엔돈쓰는거니하려했는데ㅋㅋ",
          "original_form": "처음엔돈쓰는거니하려했는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100032239.1.23",
          "form": "지금은총알이딸려서ㅋㅋㅋ",
          "original_form": "지금은총알이딸려서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100032239.1.24",
          "form": "근데코팅벗겨질까봐솔청소못한대요ㅋ",
          "original_form": "근데코팅벗겨질까봐솔청소못한대요ㅋ",
          "speaker_id": "2",
          "time": "20210905 14:25"
        },
        {
