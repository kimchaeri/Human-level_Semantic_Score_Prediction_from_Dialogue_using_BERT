      "utterance": [
        {
          "id": "MDRW2100033718.1.1",
          "form": "계속 검열뜨나봄ㅋㅋㅋ",
          "original_form": "계속 검열뜨나봄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033718.1.2",
          "form": "들어보세요",
          "original_form": "들어보세요",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033718.1.3",
          "form": "꼴뜽도",
          "original_form": "꼴뜽도",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033718.1.4",
          "form": "검열뜨더라",
          "original_form": "검열뜨더라",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033718.1.5",
          "form": "나눈 아조 할말이 마나요",
          "original_form": "나눈 아조 할말이 마나요",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033718.1.6",
          "form": "ㅋㅋ 그래",
          "original_form": "ㅋㅋ 그래",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033718.1.7",
          "form": "앞에 그 하얀천인가 그걸 뭐라하죠",
          "original_form": "앞에 그 하얀천인가 그걸 뭐라하죠",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033718.1.8",
          "form": "그 선생님 컴퓨터 화면을 보여주고",
          "original_form": "그 선생님 컴퓨터 화면을 보여주고",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100033718.1.9",
          "form": "근데 이번주부터 다녔는데 왤케할말이마너ㅋㅋ",
          "original_form": "근데 이번주부터 다녔는데 왤케할말이마너ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100033718.1.10",
          "form": "설명 들으면서",
          "original_form": "설명 들으면서",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100033718.1.11",
          "form": "스크린?",
          "original_form": "스크린?",
          "speaker_id": "2",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100033718.1.12",
          "form": "아하 스크린",
          "original_form": "아하 스크린",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100033718.1.13",
          "form": "여하간 화면+설명 들으면서",
          "original_form": "여하간 화면+설명 들으면서",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100033718.1.14",
          "form": "고대로 따라한단 말이죠",
          "original_form": "고대로 따라한단 말이죠",
          "speaker_id": "1",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100033718.1.15",
          "form": "오 맞아",
          "original_form": "오 맞아",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100033718.1.16",
          "form": "근데 왜 나만 에러가 뜨죠?",
          "original_form": "근데 왜 나만 에러가 뜨죠?",
          "speaker_id": "1",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100033718.1.17",
          "form": "name2은 너무너무 억울해요",
          "original_form": "&name2&은 너무너무 억울해요",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033718.1.18",
          "form": "왜 선생님불러",
          "original_form": "왜 선생님불러",
          "speaker_id": "2",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033718.1.19",
          "form": "근데 선생님은 계속 설명하고 계신단말이죠",
          "original_form": "근데 선생님은 계속 설명하고 계신단말이죠",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033718.1.20",
          "form": "그렇게 조금 버벅이다보면 2번을 하고계세요",
          "original_form": "그렇게 조금 버벅이다보면 2번을 하고계세요",
          "speaker_id": "1",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100033718.1.21",
          "form": "에러뜬다구 해야징",
          "original_form": "에러뜬다구 해야징",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100033718.1.22",
          "form": "아 진도 마이웨이네",
          "original_form": "아 진도 마이웨이네",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100033718.1.23",
          "form": "그리고 선생님은 앞에서 선생님자리 앉아계시고",
          "original_form": "그리고 선생님은 앞에서 선생님자리 앉아계시고",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033718.1.24",
          "form": "선생님 컴퓨터 계속 조작하고 계시니 부르기도 뭣해요",
          "original_form": "선생님 컴퓨터 계속 조작하고 계시니 부르기도 뭣해요",
