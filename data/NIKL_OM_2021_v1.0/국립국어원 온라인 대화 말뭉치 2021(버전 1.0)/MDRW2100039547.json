      "utterance": [
        {
          "id": "MDRW2100039547.1.1",
          "form": "맞제 갑자기 추워졌다",
          "original_form": "맞제 갑자기 추워졌다",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100039547.1.2",
          "form": "이제는 선풍기도 못 틀겠네",
          "original_form": "이제는 선풍기도 못 틀겠네",
          "speaker_id": "1",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039547.1.3",
          "form": "밤에도 창문 꼭꼭 닫고 자라",
          "original_form": "밤에도 창문 꼭꼭 닫고 자라",
          "speaker_id": "2",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039547.1.4",
          "form": "감기 걸린다",
          "original_form": "감기 걸린다",
          "speaker_id": "2",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039547.1.5",
          "form": "ㅇㅇ 감기 안 걸리게 문 닫고 자야겠더라",
          "original_form": "ㅇㅇ 감기 안 걸리게 문 닫고 자야겠더라",
          "speaker_id": "1",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039547.1.6",
          "form": "얼마전까지만해도 에어컨 켰는데 가을이 오긴왔나보다",
          "original_form": "얼마전까지만해도 에어컨 켰는데 가을이 오긴왔나보다",
          "speaker_id": "2",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039547.1.7",
          "form": "ㅇㅇ 이제 더 안 더워질든",
          "original_form": "ㅇㅇ 이제 더 안 더워질든",
          "speaker_id": "1",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039547.1.8",
          "form": "진짜 이제 밤에 너무 추워",
          "original_form": "진짜 이제 밤에 너무 추워",
          "speaker_id": "1",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039547.1.9",
          "form": "지금도 창문 열어놓으니 찬바람이 분다",
          "original_form": "지금도 창문 열어놓으니 찬바람이 분다",
          "speaker_id": "2",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039547.1.10",
          "form": "맞다 찬바람 아직 저녁때인데 찬바람부네",
          "original_form": "맞다 찬바람 아직 저녁때인데 찬바람부네",
          "speaker_id": "1",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039547.1.11",
          "form": "추우니까 뜨끈한거 먹고싶네",
          "original_form": "추우니까 뜨끈한거 먹고싶네",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039547.1.12",
          "form": "뜨끈한거 뭐?",
          "original_form": "뜨끈한거 뭐?",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039547.1.13",
          "form": "칼국수 수제비많지 우동도 좋고",
          "original_form": "칼국수 수제비많지 우동도 좋고",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039547.1.14",
          "form": "국물있는걸로~~",
          "original_form": "국물있는걸로~~",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039547.1.15",
          "form": "나도 갑자기 땡기네 집에서 한번 해먹자 집에 칼국수면 있나",
          "original_form": "나도 갑자기 땡기네 집에서 한번 해먹자 집에 칼국수면 있나",
