        {
          "id": "MDRW2100005377.1.1",
          "form": "나 요즘 편스토랑 꼬박꼬박 봐",
          "original_form": "나 요즘 편스토랑 꼬박꼬박 봐",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005377.1.2",
          "form": "누구 나오는데?",
          "original_form": "누구 나오는데?",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005377.1.3",
          "form": "이영자 이경규 이유리 사람들 바뀌면서 나와",
          "original_form": "이영자 이경규 이유리 사람들 바뀌면서 나와",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005377.1.4",
          "form": "재밌어?",
          "original_form": "재밌어?",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005377.1.5",
          "form": "응 재밌어",
          "original_form": "응 재밌어",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005377.1.6",
          "form": "요리 개발하는 프로그램인데",
          "original_form": "요리 개발하는 프로그램인데",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005377.1.7",
          "form": "아 편의점 레스토랑!",
          "original_form": "아 편의점 레스토랑!",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005377.1.8",
          "form": "응응 맞아",
          "original_form": "응응 맞아",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005377.1.9",
          "form": "편의점 음식 만드나",
          "original_form": "편의점 음식 만드나",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005377.1.10",
          "form": "오 잘아네",
          "original_form": "오 잘아네",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005377.1.11",
          "form": "들어 본 거 같아 ㅋㅋ",
          "original_form": "들어 본 거 같아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005377.1.12",
          "form": "거기에 김재원 아들이 나오는데 완전 귀여워",
          "original_form": "거기에 김재원 아들이 나오는데 완전 귀여워",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005377.1.13",
          "form": "김재원? 결혼했어?",
          "original_form": "김재원? 결혼했어?",
