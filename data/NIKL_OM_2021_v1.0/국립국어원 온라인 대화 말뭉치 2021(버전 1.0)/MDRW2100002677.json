      "utterance": [
        {
          "id": "MDRW2100002677.1.1",
          "form": "운동좋아하시나요??",
          "original_form": "운동좋아하시나요??",
          "speaker_id": "2",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002677.1.2",
          "form": "운동안좋아하는데 코로나 직전에 줌바해봤어요ㅎ",
          "original_form": "운동안좋아하는데 코로나 직전에 줌바해봤어요ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002677.1.3",
          "form": "오 줌바댄스인가요??",
          "original_form": "오 줌바댄스인가요??",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002677.1.4",
          "form": "네네 정적인 운동은 재미없고 지루해서  한번 등록해봤는데 신나더라구요",
          "original_form": "네네 정적인 운동은 재미없고 지루해서  한번 등록해봤는데 신나더라구요",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002677.1.5",
          "form": "참고로 몸치예요ㅋ",
          "original_form": "참고로 몸치예요ㅋ",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002677.1.6",
          "form": "그래도 스트레스풀리고 너무재밌을거같아요ㅋㅋㅋ",
          "original_form": "그래도 스트레스풀리고 너무재밌을거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002677.1.7",
          "form": "그게 줌바의 매력같아요!",
          "original_form": "그게 줌바의 매력같아요!",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002677.1.8",
          "form": "네 음악도 라틴 레게 이런거라 ㅎ",
          "original_form": "네 음악도 라틴 레게 이런거라 ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002677.1.9",
          "form": "스피닝도 재밌어요",
          "original_form": "스피닝도 재밌어요",
          "speaker_id": "1",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002677.1.10",
          "form": "스피닝도 진짜좋아보여요ㅋㅋㅋ",
          "original_form": "스피닝도 진짜좋아보여요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002677.1.11",
          "form": "활기찬운동을",
          "original_form": "활기찬운동을",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002677.1.12",
          "form": "좋아하시는군요?",
          "original_form": "좋아하시는군요?",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002677.1.13",
          "form": "원래 취미는 숨쉬기 운동이요ㅎㅎ",
          "original_form": "원래 취미는 숨쉬기 운동이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002677.1.14",
          "form": "회사서 복지로 30만원 지원해줘서 억지로 쓰느라 배워봤어요",
          "original_form": "회사서 복지로 30만원 지원해줘서 억지로 쓰느라 배워봤어요",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002677.1.15",
          "form": "오 복지좋네요ㅋㅋㅋ",
          "original_form": "오 복지좋네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002677.1.16",
          "form": "저는 피티하고싶더라고요ㅠ",
          "original_form": "저는 피티하고싶더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002677.1.17",
          "form": "요즘은 안줘요ㅋㅋ  피티는 넘 비싸서 ㅠㅠ",
          "original_form": "요즘은 안줘요ㅋㅋ  피티는 넘 비싸서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002677.1.18",
          "form": "일대일하면 진짜 몸 만들수 있을거같은데ㅎ",
          "original_form": "일대일하면 진짜 몸 만들수 있을거같은데ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002677.1.19",
          "form": "맞아요ㅠ 피티는진짜넘비싸요",
          "original_form": "맞아요ㅠ 피티는진짜넘비싸요",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002677.1.20",
          "form": "필라테스도 하고싶은데 은근 귀찮더라고요",
          "original_form": "필라테스도 하고싶은데 은근 귀찮더라고요",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002677.1.21",
          "form": "필라테스도 잠깐 했는데 자세교정도 되고 괜찮은거같아요",
          "original_form": "필라테스도 잠깐 했는데 자세교정도 되고 괜찮은거같아요",
          "speaker_id": "1",
          "time": "20210820 15:38"
        }
      ]
    }
