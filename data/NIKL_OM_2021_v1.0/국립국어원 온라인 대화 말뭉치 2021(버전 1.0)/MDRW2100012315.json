        {
          "id": "MDRW2100012315.1.1",
          "form": "영상 주로 뭐보세요?",
          "original_form": "영상 주로 뭐보세요?",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012315.1.2",
          "form": "여행유튜브요!",
          "original_form": "여행유튜브요!",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012315.1.3",
          "form": "오 어떤분이요??",
          "original_form": "오 어떤분이요??",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012315.1.4",
          "form": "빠니보틀 재밌어요",
          "original_form": "빠니보틀 재밌어요",
          "speaker_id": "1",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100012315.1.5",
          "form": "오 한번 찾아볼게요",
          "original_form": "오 한번 찾아볼게요",
          "speaker_id": "2",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012315.1.6",
          "form": "다른 건 또 뭐보나용",
          "original_form": "다른 건 또 뭐보나용",
          "speaker_id": "2",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012315.1.7",
          "form": "무한도전 오분순삭도 재밌어요",
          "original_form": "무한도전 오분순삭도 재밌어요",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012315.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ 오 저두 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 오 저두 좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012315.1.9",
          "form": "오분순삭 시리즈는 다 재밌어요",
          "original_form": "오분순삭 시리즈는 다 재밌어요",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012315.1.10",
          "form": "아 무도 시즌2했으면...",
          "original_form": "아 무도 시즌2했으면...",
          "speaker_id": "1",
