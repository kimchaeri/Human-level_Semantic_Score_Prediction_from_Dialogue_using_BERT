      "utterance": [
        {
          "id": "MDRW2100004224.1.1",
          "form": "안년하세요 최근에 여행 다녀오셨나요?",
          "original_form": "안년하세요 최근에 여행 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004224.1.2",
          "form": "아니요.ㅠㅠ 코로나때문에..",
          "original_form": "아니요.ㅠㅠ 코로나때문에..",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004224.1.3",
          "form": "어디 다녀오신곳 있으세요??",
          "original_form": "어디 다녀오신곳 있으세요??",
          "speaker_id": "2",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004224.1.4",
          "form": "아이구 저는 속초 짧게ㅠㅠ 해외가고 싶었는데ㅠㅠ",
          "original_form": "아이구 저는 속초 짧게ㅠㅠ 해외가고 싶었는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004224.1.5",
          "form": "흑흑 저도요.",
          "original_form": "흑흑 저도요.",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004224.1.6",
          "form": "이전에라도 해외 다녀 오신곳 있으세요?",
          "original_form": "이전에라도 해외 다녀 오신곳 있으세요?",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004224.1.7",
          "form": "유럽다녀왔는데 너무 좋았어요",
          "original_form": "유럽다녀왔는데 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004224.1.8",
          "form": "이젠 그전처럼 여행 못하겠죠ㅠㅠ",
          "original_form": "이젠 그전처럼 여행 못하겠죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004224.1.9",
          "form": "흑.. 그럴꺼 같아요.ㅠ",
          "original_form": "흑.. 그럴꺼 같아요.ㅠ",
          "speaker_id": "2",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004224.1.10",
          "form": "여행가면 구경하시는거 좋아하시나봐요.",
          "original_form": "여행가면 구경하시는거 좋아하시나봐요.",
          "speaker_id": "2",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004224.1.11",
          "form": "저는 휴양지 타입이라.. 몰디브가 너무 좋더라고요.",
          "original_form": "저는 휴양지 타입이라.. 몰디브가 너무 좋더라고요.",
          "speaker_id": "2",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004224.1.12",
          "form": "와 몰디브 맞아요 가보고 싶었는데",
          "original_form": "와 몰디브 맞아요 가보고 싶었는데",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004224.1.13",
          "form": "진짜 천국 같다면서요",
          "original_form": "진짜 천국 같다면서요",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004224.1.14",
          "form": "진짜 좋더라고요.",
          "original_form": "진짜 좋더라고요.",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004224.1.15",
          "form": "한가롭고...",
          "original_form": "한가롭고...",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004224.1.16",
          "form": "물빛도 너무 예쁘고..",
          "original_form": "물빛도 너무 예쁘고..",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004224.1.17",
          "form": "좀만 나가도 거북이랑 상어랑 수영할수 있고요.",
          "original_form": "좀만 나가도 거북이랑 상어랑 수영할수 있고요.",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004224.1.18",
          "form": "흑.. 다시 못갈꺼 같아요.ㅠ",
          "original_form": "흑.. 다시 못갈꺼 같아요.ㅠ",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004224.1.19",
          "form": "꼭 가보고 싶어요ㅠㅠ 투명한 바다가 너무 좋아요",
          "original_form": "꼭 가보고 싶어요ㅠㅠ 투명한 바다가 너무 좋아요",
          "speaker_id": "1",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004224.1.20",
          "form": "고래상어 볼수 있는....",
          "original_form": "고래상어 볼수 있는....",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004224.1.21",
          "form": "어디였더라...",
          "original_form": "어디였더라...",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004224.1.22",
          "form": "세부 이쪽도 가보고 싶어요.ㅠ",
          "original_form": "세부 이쪽도 가보고 싶어요.ㅠ",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004224.1.23",
          "form": "오 저도요 하와이도 가보고싶고ㅠㅠ",
          "original_form": "오 저도요 하와이도 가보고싶고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004224.1.24",
          "form": "하와이가 진짜 다 있다고 하더라고요",
          "original_form": "하와이가 진짜 다 있다고 하더라고요",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004224.1.25",
          "form": "휴양. 레저. 쇼핑..",
          "original_form": "휴양. 레저. 쇼핑..",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004224.1.26",
          "form": "여행가면 어디에 중점 두는 편이세요??",
          "original_form": "여행가면 어디에 중점 두는 편이세요??",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004224.1.27",
          "form": "네 다녀온분들말로는 음식도 맛있다고ㅎㅎ",
          "original_form": "네 다녀온분들말로는 음식도 맛있다고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004224.1.28",
          "form": "휴양도 좋고 음식도 좋아해요ㅎㅎ",
          "original_form": "휴양도 좋고 음식도 좋아해요ㅎㅎ",
          "speaker_id": "1",
