      "utterance": [
        {
          "id": "MDRW2100014546.1.1",
          "form": "이번주  매우 기대되요 ㅋㅋ",
          "original_form": "이번주  매우 기대되요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014546.1.2",
          "form": "딸램이 반응이 궁금함",
          "original_form": "딸램이 반응이 궁금함",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014546.1.3",
          "form": "완전 기절할 것 같은데요 ㅠㅠㅠㅠㅠ",
          "original_form": "완전 기절할 것 같은데요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014546.1.4",
          "form": "이번 드라마 끝나고 다들 다른 역할로 이미지변신하기 어려울 것 같아요",
          "original_form": "이번 드라마 끝나고 다들 다른 역할로 이미지변신하기 어려울 것 같아요",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014546.1.5",
          "form": "다들연기능 인정요",
          "original_form": "다들연기능 인정요",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014546.1.6",
          "form": "아역들도 대단하고",
          "original_form": "아역들도 대단하고",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014546.1.7",
          "form": "어른들도 대단하고요!",
          "original_form": "어른들도 대단하고요!",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014546.1.8",
          "form": "이제 곧 새로할거 재미있어보이던ㄷ0",
          "original_form": "이제 곧 새로할거 재미있어보이던ㄷ0",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014546.1.9",
          "form": "갯마을 머시기 인디",
          "original_form": "갯마을 머시기 인디",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014546.1.10",
          "form": "김선호!!!!! 나오는 거요",
          "original_form": "김선호!!!!! 나오는 거요",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014546.1.11",
          "form": "김선호 좋아라해서요 ㅋㅋ",
          "original_form": "김선호 좋아라해서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014546.1.12",
          "form": "저도 완전 기대중",
          "original_form": "저도 완전 기대중",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014546.1.13",
          "form": "이상이도 좋고  인교진도 나오고",
          "original_form": "이상이도 좋고  인교진도 나오고",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014546.1.14",
          "form": "웃기고 재밍잇을듯요",
          "original_form": "웃기고 재밍잇을듯요",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014546.1.15",
          "form": "네 좋아하는 배우들 총출동!",
          "original_form": "네 좋아하는 배우들 총출동!",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014546.1.16",
          "form": "빨리 보고 싶어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "빨리 보고 싶어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014546.1.17",
          "form": "밖에 못나가니까 맨날 드라마만 보고ㅠㅠㅠ",
          "original_form": "밖에 못나가니까 맨날 드라마만 보고ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014546.1.18",
          "form": "글쵸 오늘은 ㅜ뭐볼거에요?",
          "original_form": "글쵸 오늘은 ㅜ뭐볼거에요?",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014546.1.19",
          "form": "지금은 무한도전 틀어놓고 청소하고 있어요",
          "original_form": "지금은 무한도전 틀어놓고 청소하고 있어요",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014546.1.20",
          "form": "저는 빨강구두요",
          "original_form": "저는 빨강구두요",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014546.1.21",
          "form": "드라마 엄빠랑 보고있어요 ㅋ",
          "original_form": "드라마 엄빠랑 보고있어요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 19:53"
        }
      ]
