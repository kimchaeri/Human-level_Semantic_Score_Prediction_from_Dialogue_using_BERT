      "utterance": [
        {
          "id": "MDRW2100034918.1.1",
          "form": "저녁뭐드셧어요?",
          "original_form": "저녁뭐드셧어요?",
          "speaker_id": "1",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100034918.1.2",
          "form": "불고기 먹엇어요 ㅋㅋㅋㅋ name1님은요?",
          "original_form": "불고기 먹엇어요 ㅋㅋㅋㅋ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100034918.1.3",
          "form": "저는 토스트해먹었어요 ㅋㅋㅋㅋ",
          "original_form": "저는 토스트해먹었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:30"
        },
        {
          "id": "MDRW2100034918.1.4",
          "form": "오늘 제 아침이엇는데 ㅋㅋㅋㅋㅋ",
          "original_form": "오늘 제 아침이엇는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100034918.1.5",
          "form": "오ㅋㅋㅋㅋㅋ재료뭐넣으셧어요",
          "original_form": "오ㅋㅋㅋㅋㅋ재료뭐넣으셧어요",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100034918.1.6",
          "form": "아 그냥 달걀적신 프렌치토스트에 오뚜기크림스프먹엇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 그냥 달걀적신 프렌치토스트에 오뚜기크림스프먹엇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100034918.1.7",
          "form": "크림스프ㅠㅠ 맛있는조합으로 드셧네요 ㅋㅋㅋㅋ",
          "original_form": "크림스프ㅠㅠ 맛있는조합으로 드셧네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100034918.1.8",
          "form": "네 ㅋㅋㅋㅋ 야채라고는 없는 조합으로 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ 야채라고는 없는 조합으로 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100034918.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ야채 잘 안드세여?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ야채 잘 안드세여?",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100034918.1.10",
          "form": "아니요 원래 잘 먹는데 오늘따라 ㅋㅋㅋㅋ",
          "original_form": "아니요 원래 잘 먹는데 오늘따라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100034918.1.11",
          "form": "먹는건 다좋아합니다",
          "original_form": "먹는건 다좋아합니다",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100034918.1.12",
          "form": "야채도 코끼리처럼 먹어요",
          "original_form": "야채도 코끼리처럼 먹어요",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100034918.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그건 저두그래요 ㅎ..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그건 저두그래요 ㅎ..",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100034918.1.14",
          "form": "안드시는거 잇으세요?",
          "original_form": "안드시는거 잇으세요?",
          "speaker_id": "1",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100034918.1.15",
          "form": "음...웬만한건 다 잘먹긴하는데",
          "original_form": "음...웬만한건 다 잘먹긴하는데",
          "speaker_id": "2",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100034918.1.16",
