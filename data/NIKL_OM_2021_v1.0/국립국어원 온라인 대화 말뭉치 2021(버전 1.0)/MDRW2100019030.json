      "utterance": [
        {
          "id": "MDRW2100019030.1.1",
          "form": "안아픈게 최고예요",
          "original_form": "안아픈게 최고예요",
          "speaker_id": "2",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019030.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019030.1.3",
          "form": "어디 아픈곳은 없으세요?",
          "original_form": "어디 아픈곳은 없으세요?",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019030.1.4",
          "form": "네 아직은 특별히요",
          "original_form": "네 아직은 특별히요",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019030.1.5",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019030.1.6",
          "form": "전 딸이 낼 치과 치료해요",
          "original_form": "전 딸이 낼 치과 치료해요",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019030.1.7",
          "form": "아 치과는ㅟ",
          "original_form": "아 치과는ㅟ",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019030.1.8",
          "form": "저도그냥 많이 놓고 있죠ㅠ",
          "original_form": "저도그냥 많이 놓고 있죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019030.1.9",
          "form": "금으로 살짝 때우는데 38만 이라니",
          "original_form": "금으로 살짝 때우는데 38만 이라니",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019030.1.10",
          "form": "치아가 젤 비싸고 관리 잘해야 하는 과예요",
          "original_form": "치아가 젤 비싸고 관리 잘해야 하는 과예요",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019030.1.11",
          "form": "어떻게 보면 쓰는거에 비해",
          "original_form": "어떻게 보면 쓰는거에 비해",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019030.1.12",
          "form": "싼것같으면서도ㅋㅋ",
          "original_form": "싼것같으면서도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019030.1.13",
          "form": "매일 쓰니 그런가요 ㅜㅜ",
          "original_form": "매일 쓰니 그런가요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019030.1.14",
          "form": "임플란트는 진짜 큰맘먹고 해야해요",
          "original_form": "임플란트는 진짜 큰맘먹고 해야해요",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019030.1.15",
          "form": "저도 ㅋㅋ목돈 나갔죠",
          "original_form": "저도 ㅋㅋ목돈 나갔죠",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019030.1.16",
          "form": "치아관리 3개월마다 가요",
          "original_form": "치아관리 3개월마다 가요",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019030.1.17",
          "form": "관리 받는건 없으세요?",
          "original_form": "관리 받는건 없으세요?",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019030.1.18",
          "form": "운동은 뭐하시나요?",
          "original_form": "운동은 뭐하시나요?",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019030.1.19",
          "form": "운동은 그냥 집에서ㅠㅠ 홈트봐요",
          "original_form": "운동은 그냥 집에서ㅠㅠ 홈트봐요",
          "speaker_id": "2",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019030.1.20",
          "form": "뭐요?",
          "original_form": "뭐요?",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019030.1.21",
          "form": "대부분 땅끄 보시던데요",
          "original_form": "대부분 땅끄 보시던데요",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019030.1.22",
          "form": "여기 분들 공통이더라구요 ㅎㅎ",
          "original_form": "여기 분들 공통이더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019030.1.23",
          "form": "ㅋㅋㅋ 인기인가봐요",
          "original_form": "ㅋㅋㅋ 인기인가봐요",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019030.1.24",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019030.1.25",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019030.1.26",
          "form": "걷기요",
          "original_form": "걷기요",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019030.1.27",
          "form": "딸이랑",
          "original_form": "딸이랑",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019030.1.28",
          "form": "근데 방학 끝나서 혼자 재미 없네요",
          "original_form": "근데 방학 끝나서 혼자 재미 없네요",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019030.1.29",
          "form": "걷기가 제일 좋은 운동이래요",
          "original_form": "걷기가 제일 좋은 운동이래요",
          "speaker_id": "2",
