      "utterance": [
        {
          "id": "MDRW2100035388.1.1",
          "form": "저녁은..뭘먹나 고민중이에여",
          "original_form": "저녁은..뭘먹나 고민중이에여",
          "speaker_id": "1",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100035388.1.2",
          "form": "항상 메뉴는 고민되죠 ..",
          "original_form": "항상 메뉴는 고민되죠 ..",
          "speaker_id": "2",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100035388.1.3",
          "form": "음.. name2님은?뭘드실껀가요",
          "original_form": "음.. &name2&님은?뭘드실껀가요",
          "speaker_id": "1",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100035388.1.4",
          "form": "저는 양배추피자 해먹을까 생각중이에요!",
          "original_form": "저는 양배추피자 해먹을까 생각중이에요!",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100035388.1.5",
          "form": "오 색다른 메뉴네요",
          "original_form": "오 색다른 메뉴네요",
          "speaker_id": "1",
          "time": "20210829 23:15"
        },
        {
          "id": "MDRW2100035388.1.6",
          "form": "근데 결국에는 귀찮아서 엄마가 해주신 저녁 먹었답니다..",
          "original_form": "근데 결국에는 귀찮아서 엄마가 해주신 저녁 먹었답니다..",
          "speaker_id": "2",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100035388.1.7",
          "form": "저녁 뭐드셨나요?",
          "original_form": "저녁 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100035388.1.8",
          "form": "ㅋㅋㅋㅋㅋ 엄마가 해주신 밥이 젤 좋죠 저는 짜장밥",
          "original_form": "ㅋㅋㅋㅋㅋ 엄마가 해주신 밥이 젤 좋죠 저는 짜장밥",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100035388.1.9",
          "form": "짜장밥 맛있죠 ㅋㅋ 김치랑 먹으면 한그릇 뚝딱이에요",
          "original_form": "짜장밥 맛있죠 ㅋㅋ 김치랑 먹으면 한그릇 뚝딱이에요",
          "speaker_id": "2",
          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100035388.1.10",
          "form": "맞아요 ㅎㅎ 짜장밥은 김치가 필수",
          "original_form": "맞아요 ㅎㅎ 짜장밥은 김치가 필수",
          "speaker_id": "1",
          "time": "20210830 14:38"
        },
        {
          "id": "MDRW2100035388.1.11",
          "form": "그쵸 김치랑 먹어야 꿀맛이죠 ㅎㅎ",
          "original_form": "그쵸 김치랑 먹어야 꿀맛이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:11"
        },
        {
          "id": "MDRW2100035388.1.12",
          "form": "저녁 드셨나요??",
          "original_form": "저녁 드셨나요??",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100035388.1.13",
          "form": "네~ 두부김치먹었어요",
          "original_form": "네~ 두부김치먹었어요",
          "speaker_id": "2",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100035388.1.14",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100035388.1.15",
          "form": "네. 집밥ㅎㅎ 고등어 구이 랑 주꾸미볶음",
          "original_form": "네. 집밥ㅎㅎ 고등어 구이 랑 주꾸미볶음",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100035388.1.16",
          "form": "헉 주메뉴가 두개나 되네요ㅋㅋ",
          "original_form": "헉 주메뉴가 두개나 되네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100035388.1.17",
