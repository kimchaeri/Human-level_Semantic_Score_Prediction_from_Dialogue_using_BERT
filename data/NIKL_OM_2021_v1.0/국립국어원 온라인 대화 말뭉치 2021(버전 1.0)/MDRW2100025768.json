        {
          "id": "MDRW2100025768.1.1",
          "form": "싫어요",
          "original_form": "싫어요",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025768.1.2",
          "form": "저는 초밥먹고 싶어요",
          "original_form": "저는 초밥먹고 싶어요",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025768.1.3",
          "form": "초밥에 냉모밀",
          "original_form": "초밥에 냉모밀",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025768.1.4",
          "form": "후로롭",
          "original_form": "후로롭",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025768.1.5",
          "form": "둘다 같이 먹어요",
          "original_form": "둘다 같이 먹어요",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025768.1.6",
          "form": "노머니라 불가능입니다 ..",
          "original_form": "노머니라 불가능입니다 ..",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025768.1.7",
          "form": "한번 해 볼까요 ?",
          "original_form": "한번 해 볼까요 ?",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025768.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025768.1.9",
          "form": "까르보나라!!!!!!!!!!",
          "original_form": "까르보나라!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025768.1.10",
          "form": "뭐 어쩌자는 거죠 ?",
          "original_form": "뭐 어쩌자는 거죠 ?",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100025768.1.11",
          "form": "까르보나라 땡기세요 ?",
          "original_form": "까르보나라 땡기세요 ?",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100025768.1.12",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100025768.1.13",
          "form": "오늘 저녁 뭐 드셨어요 ?",
          "original_form": "오늘 저녁 뭐 드셨어요 ?",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100025768.1.14",
          "form": "저 떡군이네 로제떡볶이에 중당 분모자 추가해서 먹었어요",
          "original_form": "저 떡군이네 로제떡볶이에 중당 분모자 추가해서 먹었어요",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100025768.1.15",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100025768.1.16",
          "form": "저는 김밥이요",
          "original_form": "저는 김밥이요",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100025768.1.17",
          "form": "웬지 저랑 통하셨을 것 같아요",
          "original_form": "웬지 저랑 통하셨을 것 같아요",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100025768.1.18",
          "form": "안통했네요",
          "original_form": "안통했네요",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100025768.1.19",
          "form": "저는 깻잎김밥이요",
          "original_form": "저는 깻잎김밥이요",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100025768.1.20",
          "form": "아 그런가요 ?",
          "original_form": "아 그런가요 ?",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100025768.1.21",
          "form": "맛있거든요",
          "original_form": "맛있거든요",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100025768.1.22",
          "form": "깻잎김밥이랑 로제떡볶이랑 같이 먹었으면 더 맛있었겠네요",
          "original_form": "깻잎김밥이랑 로제떡볶이랑 같이 먹었으면 더 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100025768.1.23",
          "form": "아니죠 날치알비빔밥이죠!",
          "original_form": "아니죠 날치알비빔밥이죠!",
          "speaker_id": "1",
          "time": "20210903 21:19"
        }
      ]
