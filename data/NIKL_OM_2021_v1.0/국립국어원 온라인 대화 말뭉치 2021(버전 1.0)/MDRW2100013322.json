      "utterance": [
        {
          "id": "MDRW2100013322.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013322.1.2",
          "form": "오늘 날씨 어떤가요?",
          "original_form": "오늘 날씨 어떤가요?",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013322.1.3",
          "form": "저희 동네는 오전에 비왔다가 지금은 그쳤네요!",
          "original_form": "저희 동네는 오전에 비왔다가 지금은 그쳤네요!",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013322.1.4",
          "form": "저희도요ㅎ",
          "original_form": "저희도요ㅎ",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013322.1.5",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013322.1.6",
          "form": "전 가을 겨울이요",
          "original_form": "전 가을 겨울이요",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013322.1.7",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013322.1.8",
          "form": "전 여름 원래 좋아하는데 올여름 너무 더워서 가을오길 기다리는 중이에요 ㅎㅎ",
          "original_form": "전 여름 원래 좋아하는데 올여름 너무 더워서 가을오길 기다리는 중이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013322.1.9",
          "form": "전 더위가 넘 약해서 차라리 겨울이 좋아요",
          "original_form": "전 더위가 넘 약해서 차라리 겨울이 좋아요",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013322.1.10",
          "form": "아 전 반대로 겨울에 약한데 ㅎㅎ",
          "original_form": "아 전 반대로 겨울에 약한데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013322.1.11",
          "form": "근데 겨울 특유의 분위기는 좋은거 같아요",
          "original_form": "근데 겨울 특유의 분위기는 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013322.1.12",
          "form": "설경도 멋지고",
          "original_form": "설경도 멋지고",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013322.1.13",
          "form": "그쵸 눈오면 뭔가 낭만적인 느낌나서 좋아요 ㅎㅎ",
          "original_form": "그쵸 눈오면 뭔가 낭만적인 느낌나서 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013322.1.14",
          "form": "걸을땐 불편하지만",
          "original_form": "걸을땐 불편하지만",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013322.1.15",
          "form": "미끌미끌",
          "original_form": "미끌미끌",
          "speaker_id": "2",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100013322.1.16",
          "form": "ㅎㅎ 안 미끄러지려고 뒤뚱뒤뚱 걷게 돼요 ㅎㅎ",
          "original_form": "ㅎㅎ 안 미끄러지려고 뒤뚱뒤뚱 걷게 돼요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013322.1.17",
          "form": "이제 장마 온다는데 비오는 건 좋아하세요?",
          "original_form": "이제 장마 온다는데 비오는 건 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013322.1.18",
          "form": "빗소리는 좋아해요",
          "original_form": "빗소리는 좋아해요",
          "speaker_id": "2",
          "time": "20210825 15:55"
        }
      ]
