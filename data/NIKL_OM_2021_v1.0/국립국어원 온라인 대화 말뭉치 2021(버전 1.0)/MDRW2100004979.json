      "utterance": [
        {
          "id": "MDRW2100004979.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004979.1.2",
          "form": "안녕하세요 !",
          "original_form": "안녕하세요 !",
          "speaker_id": "2",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004979.1.3",
          "form": "여행 조아하세요?",
          "original_form": "여행 조아하세요?",
          "speaker_id": "1",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004979.1.4",
          "form": "네네 ! 너무 좋아해요",
          "original_form": "네네 ! 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004979.1.5",
          "form": "코로나라 여행을 자주 못가서 아쉬워요",
          "original_form": "코로나라 여행을 자주 못가서 아쉬워요",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004979.1.6",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004979.1.7",
          "form": "여행 어디가보셨나요?",
          "original_form": "여행 어디가보셨나요?",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004979.1.8",
          "form": "좋았던곳이 있으면 추천해주세요!",
          "original_form": "좋았던곳이 있으면 추천해주세요!",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004979.1.9",
          "form": "저는 세계여행 좋아해서",
          "original_form": "저는 세계여행 좋아해서",
          "speaker_id": "1",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004979.1.10",
          "form": "제일 좋았던 곳은 음... 볼리비아?",
          "original_form": "제일 좋았던 곳은 음... 볼리비아?",
          "speaker_id": "1",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004979.1.11",
          "form": "우유니 소금 사막이 예뻐요",
          "original_form": "우유니 소금 사막이 예뻐요",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004979.1.12",
          "form": "볼리비아 가보지 못했어요 ㅠㅠ",
          "original_form": "볼리비아 가보지 못했어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004979.1.13",
          "form": "저도 소금사막 사진으로 본적이있는데 너무 예뻐서 가보고싶더라구요",
          "original_form": "저도 소금사막 사진으로 본적이있는데 너무 예뻐서 가보고싶더라구요",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004979.1.14",
          "form": "그쳐ㅜㅜ 쿼카님은 가보신 곳 있나요? 된다면 추천도 받고 싶어영",
          "original_form": "그쳐ㅜㅜ 쿼카님은 가보신 곳 있나요? 된다면 추천도 받고 싶어영",
          "speaker_id": "1",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004979.1.15",
          "form": "저도 세계여행 위주로 다녔는데 다시 가보고싶은 곳은 스위스예요 ㅠ",
          "original_form": "저도 세계여행 위주로 다녔는데 다시 가보고싶은 곳은 스위스예요 ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004979.1.16",
          "form": "코로나 끝나면 다시 가보고싶네요.. 풍경이 너무 예뻤어요",
          "original_form": "코로나 끝나면 다시 가보고싶네요.. 풍경이 너무 예뻤어요",
          "speaker_id": "2",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004979.1.17",
          "form": "헉 저도요ㅠㅡㅠ",
          "original_form": "헉 저도요ㅠㅡㅠ",
          "speaker_id": "1",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004979.1.18",
          "form": "스위스에서 먹었던 젤라또가 아직도 기억에 남습니다...",
          "original_form": "스위스에서 먹었던 젤라또가 아직도 기억에 남습니다...",
          "speaker_id": "1",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004979.1.19",
          "form": "스위스에도 젤라또가 맛있나요?ㅠㅠ 못먹어봐서 아쉽네요",
          "original_form": "스위스에도 젤라또가 맛있나요?ㅠㅠ 못먹어봐서 아쉽네요",
          "speaker_id": "2",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004979.1.20",
          "form": "그리고 아이슬란드에가서 오로라도 꼭한번 보고싶네요..",
          "original_form": "그리고 아이슬란드에가서 오로라도 꼭한번 보고싶네요..",
          "speaker_id": "2",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004979.1.21",
          "form": "저도ㅠㅠㅠㅠ요ㅠ ㅠ  ㅜㅜ",
          "original_form": "저도ㅠㅠㅠㅠ요ㅠ ㅠ  ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004979.1.22",
          "form": "저는 고등학교 졸업하고 캐나다 가서 오로라 보려 했는데",
          "original_form": "저는 고등학교 졸업하고 캐나다 가서 오로라 보려 했는데",
          "speaker_id": "1",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004979.1.23",
          "form": "마침 코로나더라고요....",
          "original_form": "마침 코로나더라고요....",
          "speaker_id": "1",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004979.1.24",
          "form": "그쵸.. 너무 아쉬워요 ..ㅠㅠ",
          "original_form": "그쵸.. 너무 아쉬워요 ..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100004979.1.25",
          "form": "내년에는 코로나가 잠잠해져서 여행을 꼭 갈 수 있었으면 좋겠어요",
          "original_form": "내년에는 코로나가 잠잠해져서 여행을 꼭 갈 수 있었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210821 19:52"
        },
        {
          "id": "MDRW2100004979.1.26",
          "form": "그러니까요,, 코로나 잠잠해지면 국내여행이라도 바로 가고 싶네여",
          "original_form": "그러니까요,, 코로나 잠잠해지면 국내여행이라도 바로 가고 싶네여",
          "speaker_id": "1",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100004979.1.27",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100004979.1.28",
          "form": "저는 국내여행을 많이 안가봤는데",
          "original_form": "저는 국내여행을 많이 안가봤는데",
          "speaker_id": "2",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004979.1.29",
          "form": "가보셨던 국내여행지 추천해주실 수 있으세요?",
          "original_form": "가보셨던 국내여행지 추천해주실 수 있으세요?",
          "speaker_id": "2",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004979.1.30",
          "form": "저도 국내여행은 많이 안 가봤어요ㅠㅠ 내리 서울에만 살고 외가 친가 모두 서울에 있어서",
          "original_form": "저도 국내여행은 많이 안 가봤어요ㅠㅠ 내리 서울에만 살고 외가 친가 모두 서울에 있어서",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004979.1.31",
          "form": "아쉽네요.. 국내에도 좋은 여행지가 많더라구요!!",
          "original_form": "아쉽네요.. 국내에도 좋은 여행지가 많더라구요!!",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100004979.1.32",
          "form": "우선 나중에 제주도도 마스크 벗고 여행다니고싶어요 ㅠㅠ",
          "original_form": "우선 나중에 제주도도 마스크 벗고 여행다니고싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100004979.1.33",
          "form": "맞아요...ㅠㅠ",
          "original_form": "맞아요...ㅠㅠ",
