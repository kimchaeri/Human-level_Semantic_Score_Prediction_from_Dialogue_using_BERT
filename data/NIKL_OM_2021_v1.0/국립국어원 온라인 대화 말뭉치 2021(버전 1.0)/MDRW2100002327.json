      "utterance": [
        {
          "id": "MDRW2100002327.1.1",
          "form": "즐겨보는 프로가 있나요?",
          "original_form": "즐겨보는 프로가 있나요?",
          "speaker_id": "1",
          "time": "20210820 09:36"
        },
        {
          "id": "MDRW2100002327.1.2",
          "form": "저는 최근에 본게 여고추리반 놀라운토요일 금쪽이나",
          "original_form": "저는 최근에 본게 여고추리반 놀라운토요일 금쪽이나",
          "speaker_id": "2",
          "time": "20210820 09:39"
        },
        {
          "id": "MDRW2100002327.1.3",
          "form": "동물관련 그런걸 자주봐요 ㅎㅎ",
          "original_form": "동물관련 그런걸 자주봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 09:39"
        },
        {
          "id": "MDRW2100002327.1.4",
          "form": "식센이나 문특도 가끔봐요!",
          "original_form": "식센이나 문특도 가끔봐요!",
          "speaker_id": "2",
          "time": "20210820 09:39"
        },
        {
          "id": "MDRW2100002327.1.5",
          "form": "저도 좋아해요ㅎㅎ 일요일 아침은 동물농장~",
          "original_form": "저도 좋아해요ㅎㅎ 일요일 아침은 동물농장~",
          "speaker_id": "1",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100002327.1.6",
          "form": "지원님은 이 중에 보시는게 있나요??!",
          "original_form": "지원님은 이 중에 보시는게 있나요??!",
          "speaker_id": "2",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100002327.1.7",
          "form": "놀라운토요일 문특 봐요ㅎㅎ",
          "original_form": "놀라운토요일 문특 봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 09:41"
        },
        {
          "id": "MDRW2100002327.1.8",
          "form": "요즘 재밌더라구요 특히 문특에 재재님 너무 유쾌해요",
          "original_form": "요즘 재밌더라구요 특히 문특에 재재님 너무 유쾌해요",
          "speaker_id": "1",
          "time": "20210820 09:41"
        },
        {
          "id": "MDRW2100002327.1.9",
          "form": "엌ㅋㅋㅋㅋ 동물농장 재밌죠 1000화 넘었잖아요ㅠ 기념으로 나왔던 동물들 보여주는데 새록새록 떠오르더라구요",
          "original_form": "엌ㅋㅋㅋㅋ 동물농장 재밌죠 1000화 넘었잖아요ㅠ 기념으로 나왔던 동물들 보여주는데 새록새록 떠오르더라구요",
          "speaker_id": "2",
          "time": "20210820 09:41"
        },
        {
          "id": "MDRW2100002327.1.10",
          "form": "맞아요 재재님 너무 진행도 잘하시고 너무 좋아요ㅠㅠ",
          "original_form": "맞아요 재재님 너무 진행도 잘하시고 너무 좋아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 09:42"
        },
        {
          "id": "MDRW2100002327.1.11",
          "form": "사람을 편하게만드는게 있달까",
          "original_form": "사람을 편하게만드는게 있달까",
          "speaker_id": "2",
          "time": "20210820 09:42"
        },
        {
          "id": "MDRW2100002327.1.12",
          "form": "그 질문이라던가 분위기라던가",
          "original_form": "그 질문이라던가 분위기라던가",
          "speaker_id": "1",
          "time": "20210820 09:42"
        },
        {
          "id": "MDRW2100002327.1.13",
          "form": "맞아요 부담없어요",
          "original_form": "맞아요 부담없어요",
          "speaker_id": "1",
          "time": "20210820 09:42"
        },
        {
          "id": "MDRW2100002327.1.14",
          "form": "그래서 다들 좋아하나봐요",
          "original_form": "그래서 다들 좋아하나봐요",
          "speaker_id": "1",
          "time": "20210820 09:42"
        },
        {
          "id": "MDRW2100002327.1.15",
          "form": "맞아요! 정말 좋아요ㅠㅠ\r\n문명특급 투피엠 편 보셨어요??",
          "original_form": "맞아요! 정말 좋아요ㅠㅠ\r\n문명특급 투피엠 편 보셨어요??",
          "speaker_id": "2",
          "time": "20210820 09:45"
        },
        {
          "id": "MDRW2100002327.1.16",
          "form": "봤어요! 우리집 준호가 나오는데 어떻게 안볼 수 있겠어요",
          "original_form": "봤어요! 우리집 준호가 나오는데 어떻게 안볼 수 있겠어요",
          "speaker_id": "1",
          "time": "20210820 09:46"
        },
        {
          "id": "MDRW2100002327.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ name1님은 누구집으로 가실건가요 ㅋㅋㅋ 영상 보니까 다 가고싶던데 ㅎㅎ..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ &name1&님은 누구집으로 가실건가요 ㅋㅋㅋ 영상 보니까 다 가고싶던데 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210820 09:50"
        },
        {
          "id": "MDRW2100002327.1.18",
          "form": "저는.. 택연집으로 가려구요",
          "original_form": "저는.. 택연집으로 가려구요",
          "speaker_id": "1",
          "time": "20210820 09:50"
        },
        {
          "id": "MDRW2100002327.1.19",
          "form": "사실 남자는 피지컬이죠 (흐뭇)",
          "original_form": "사실 남자는 피지컬이죠 (흐뭇)",
          "speaker_id": "1",
          "time": "20210820 09:51"
        },
        {
          "id": "MDRW2100002327.1.20",
          "form": "name2님은요? 그래도 고른다면?",
          "original_form": "&name2&님은요? 그래도 고른다면?",
          "speaker_id": "1",
          "time": "20210820 09:51"
        },
        {
          "id": "MDRW2100002327.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 찢택연 흐뭇합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 찢택연 흐뭇합니다",
          "speaker_id": "2",
          "time": "20210820 09:53"
        },
        {
          "id": "MDRW2100002327.1.22",
          "form": "고민되네요 몸 좋은 택연집..이나 준호집!!",
          "original_form": "고민되네요 몸 좋은 택연집..이나 준호집!!",
          "speaker_id": "2",
          "time": "20210820 09:53"
        },
        {
          "id": "MDRW2100002327.1.23",
          "form": "역시..아무래도..",
          "original_form": "역시..아무래도..",
          "speaker_id": "1",
          "time": "20210820 09:53"
        },
        {
          "id": "MDRW2100002327.1.24",
          "form": "우리집은 준호..",
          "original_form": "우리집은 준호..",
          "speaker_id": "1",
          "time": "20210820 09:53"
        },
        {
          "id": "MDRW2100002327.1.25",
          "form": "끼를 발산하는 그 몸짓",
          "original_form": "끼를 발산하는 그 몸짓",
          "speaker_id": "1",
          "time": "20210820 09:53"
        },
        {
          "id": "MDRW2100002327.1.26",
          "form": "어떻게 안갈 수가 있겠습니까..ㅎ",
          "original_form": "어떻게 안갈 수가 있겠습니까..ㅎ",
          "speaker_id": "1",
          "time": "20210820 09:54"
        },
        {
          "id": "MDRW2100002327.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 진짜 ㅋㅋㅋㅋ 매력이 넘치죠",
