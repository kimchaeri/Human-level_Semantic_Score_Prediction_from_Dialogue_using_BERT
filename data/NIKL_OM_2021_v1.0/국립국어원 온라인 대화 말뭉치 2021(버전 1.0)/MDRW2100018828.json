      "utterance": [
        {
          "id": "MDRW2100018828.1.1",
          "form": "마저",
          "original_form": "마저",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018828.1.2",
          "form": "메이플 진짜재밌어",
          "original_form": "메이플 진짜재밌어",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018828.1.3",
          "form": "케릭터 꾸미기 재밌다",
          "original_form": "케릭터 꾸미기 재밌다",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018828.1.4",
          "form": "스라벨 로얄 까는것도 재밋구",
          "original_form": "스라벨 로얄 까는것도 재밋구",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018828.1.5",
          "form": "마저마저",
          "original_form": "마저마저",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018828.1.6",
          "form": "좀 비싸지만",
          "original_form": "좀 비싸지만",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018828.1.7",
          "form": "10개에 5만원인건",
          "original_form": "10개에 5만원인건",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018828.1.8",
          "form": "그렇긴해",
          "original_form": "그렇긴해",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018828.1.9",
          "form": "그니까....",
          "original_form": "그니까....",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018828.1.10",
          "form": "근데 또 머리랑 눈이 너무예뻐",
          "original_form": "근데 또 머리랑 눈이 너무예뻐",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.11",
          "form": "그니까말야",
          "original_form": "그니까말야",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.12",
          "form": "근데 레벨 101까지 키우는게힘들어",
          "original_form": "근데 레벨 101까지 키우는게힘들어",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.13",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.14",
          "form": "그 뭐야",
          "original_form": "그 뭐야",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.15",
          "form": "버닝서버에서 키우다가 사라졋자나 우리 캐릴터",
          "original_form": "버닝서버에서 키우다가 사라졋자나 우리 캐릴터",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.16",
          "form": "그니까ㅠㅜㅜㅠㅜ",
          "original_form": "그니까ㅠㅜㅜㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.17",
          "form": "아슬프다 다시 생각하니까",
          "original_form": "아슬프다 다시 생각하니까",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.18",
          "form": "버닝...",
          "original_form": "버닝...",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.19",
          "form": "거기가 조은 서번줄 알구...",
          "original_form": "거기가 조은 서번줄 알구...",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.20",
          "form": "개열시미햇는데..",
          "original_form": "개열시미햇는데..",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.21",
          "form": "그니까ㅠㅠ",
          "original_form": "그니까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.22",
          "form": "캐릭터가 소멸됐어여",
          "original_form": "캐릭터가 소멸됐어여",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018828.1.23",
          "form": "그냥 어디론가로 없어졋어요",
          "original_form": "그냥 어디론가로 없어졋어요",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018828.1.24",
          "form": "다시 망든 캐릭터는 그렇지않길..",
          "original_form": "다시 망든 캐릭터는 그렇지않길..",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018828.1.25",
          "form": "또 사라지면 메이플 안할거야",
          "original_form": "또 사라지면 메이플 안할거야",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018828.1.26",
          "form": "나도 안할래",
          "original_form": "나도 안할래",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018828.1.27",
          "form": "근데 우리 아델이잖아 그 캐릭터",
          "original_form": "근데 우리 아델이잖아 그 캐릭터",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018828.1.28",
          "form": "응 글치",
          "original_form": "응 글치",
          "speaker_id": "2",
          "time": "20210827 12:35"
        }
      ]
    }
