      "utterance": [
        {
          "id": "MDRW2100028607.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028607.1.2",
          "form": "좋아해요.",
          "original_form": "좋아해요.",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028607.1.3",
          "form": "최근에는 전혀 못다니고 있지만요",
          "original_form": "최근에는 전혀 못다니고 있지만요",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028607.1.4",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028607.1.5",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028607.1.6",
          "form": "아니었으면 친구들이랑 여행 다녔을거예요",
          "original_form": "아니었으면 친구들이랑 여행 다녔을거예요",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028607.1.7",
          "form": "올해는 호캉스라도 갈까 했는데 그것도 못갔네요",
          "original_form": "올해는 호캉스라도 갈까 했는데 그것도 못갔네요",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028607.1.8",
          "form": "왜오?",
          "original_form": "왜오?",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028607.1.9",
          "form": "가족도 친구도 스케줄이 안맞았어요ㅠㅠ",
          "original_form": "가족도 친구도 스케줄이 안맞았어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100028607.1.10",
          "form": "혼자는 별로 안땡기더라구요",
          "original_form": "혼자는 별로 안땡기더라구요",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100028607.1.11",
          "form": "아 외럽죠",
          "original_form": "아 외럽죠",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100028607.1.12",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100028607.1.13",
          "form": "그래서 그냥 집에서 쉬었답니다",
          "original_form": "그래서 그냥 집에서 쉬었답니다",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100028607.1.14",
          "form": "가능하면 부산에 가고싶었어요",
          "original_form": "가능하면 부산에 가고싶었어요",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100028607.1.15",
          "form": "전시회가 괜찮은게 있었거든요",
          "original_form": "전시회가 괜찮은게 있었거든요",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100028607.1.16",
          "form": "어떤거요",
          "original_form": "어떤거요",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100028607.1.17",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100028607.1.18",
          "form": "해양관련 전시회였어요",
          "original_form": "해양관련 전시회였어요",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100028607.1.19",
          "form": "못가는걸 알고 포기해서 자세히는 기억이 안나네요",
          "original_form": "못가는걸 알고 포기해서 자세히는 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100028607.1.20",
          "form": "다양하게 있네여",
          "original_form": "다양하게 있네여",
          "speaker_id": "1",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100028607.1.21",
          "form": "전시회가요?",
          "original_form": "전시회가요?",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100028607.1.22",
          "form": "이것저것 있더라구요",
          "original_form": "이것저것 있더라구요",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100028607.1.23",
          "form": "이번에 대전에 신세계 백화점 오픈한 곳에",
          "original_form": "이번에 대전에 신세계 백화점 오픈한 곳에",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100028607.1.24",
          "form": "led 수족관?도 오픈할 예정이라는데",
          "original_form": "led 수족관?도 오픈할 예정이라는데",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100028607.1.25",
          "form": "거기도 가보고싶어요",
          "original_form": "거기도 가보고싶어요",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100028607.1.26",
          "form": "오 좋은 정보감사합니다.",
          "original_form": "오 좋은 정보감사합니다.",
