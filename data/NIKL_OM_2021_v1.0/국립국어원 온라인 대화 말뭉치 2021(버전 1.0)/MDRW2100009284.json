      "utterance": [
        {
          "id": "MDRW2100009284.1.1",
          "form": "싸이월드 하셨었나요",
          "original_form": "싸이월드 하셨었나요",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009284.1.2",
          "form": "아니요 전 그 세대 보다 조금 어렸었어요",
          "original_form": "아니요 전 그 세대 보다 조금 어렸었어요",
          "speaker_id": "2",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009284.1.3",
          "form": "아그래요? 지금은 성인이신가용 ?",
          "original_form": "아그래요? 지금은 성인이신가용 ?",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009284.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009284.1.5",
          "form": "싸이월드가 페이스북같은 느낌인가요",
          "original_form": "싸이월드가 페이스북같은 느낌인가요",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009284.1.6",
          "form": "그런 느낌인데",
          "original_form": "그런 느낌인데",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009284.1.7",
          "form": "훨씬 유치하고 중독적이었어요ㅋㅋㅋㅋ",
          "original_form": "훨씬 유치하고 중독적이었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009284.1.8",
          "form": "앗 그래서 지금 다시 부활도 시키고 사람들이 열광하나봐요",
          "original_form": "앗 그래서 지금 다시 부활도 시키고 사람들이 열광하나봐요",
          "speaker_id": "2",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009284.1.9",
          "form": "옛날인데도 기능도 꽤많았거든요 ㅎㅎ",
          "original_form": "옛날인데도 기능도 꽤많았거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009284.1.10",
          "form": "페이스북은 아직하세요?",
          "original_form": "페이스북은 아직하세요?",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009284.1.11",
          "form": "아뇨 사실 sns를 잘안해서요",
          "original_form": "아뇨 사실 sns를 잘안해서요",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009284.1.12",
          "form": "싸이월드에 도토리같은게 있다고 들었는데",
          "original_form": "싸이월드에 도토리같은게 있다고 들었는데",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009284.1.13",
          "form": "많이 모으셨었나요",
          "original_form": "많이 모으셨었나요",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009284.1.14",
          "form": "ㅋㅋㅋㅋㅋ도토리 용돈받으면 매일 샀었죵",
          "original_form": "ㅋㅋㅋㅋㅋ도토리 용돈받으면 매일 샀었죵",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009284.1.15",
          "form": "뽐내기용이었어요 ㅋㅋ",
          "original_form": "뽐내기용이었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009284.1.16",
          "form": "뽐내기용ㅋㅋㅋㅋ 귀엽네요 도토리로 뽐내다니",
          "original_form": "뽐내기용ㅋㅋㅋㅋ 귀엽네요 도토리로 뽐내다니",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009284.1.17",
          "form": "ㅋㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋㅋ네",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009284.1.18",
          "form": "내캐릭터 꾸미고",
          "original_form": "내캐릭터 꾸미고",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009284.1.19",
          "form": "음악사서 깔고했어용",
          "original_form": "음악사서 깔고했어용",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009284.1.20",
          "form": "저도 해봤으면 재밌었을것 같아요",
          "original_form": "저도 해봤으면 재밌었을것 같아요",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009284.1.21",
          "form": "네네 ㅎㅎ 인스타도 안하세용?",
          "original_form": "네네 ㅎㅎ 인스타도 안하세용?",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009284.1.22",
          "form": "네 그냥 인플루언서들 게시물 보기만해요",
          "original_form": "네 그냥 인플루언서들 게시물 보기만해요",
          "speaker_id": "2",
          "time": "20210822 22:41"
