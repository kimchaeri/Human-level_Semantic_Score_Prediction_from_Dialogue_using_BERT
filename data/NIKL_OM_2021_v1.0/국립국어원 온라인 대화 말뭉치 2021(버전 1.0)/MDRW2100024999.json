      "utterance": [
        {
          "id": "MDRW2100024999.1.1",
          "form": "수제소시지 구워서 치즈 얹어 먹으면 맛있는데",
          "original_form": "수제소시지 구워서 치즈 얹어 먹으면 맛있는데",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024999.1.2",
          "form": "와 소세지",
          "original_form": "와 소세지",
          "speaker_id": "2",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100024999.1.3",
          "form": "며칠 전부터 먹고 싶어서 드릉드릉했음",
          "original_form": "며칠 전부터 먹고 싶어서 드릉드릉했음",
          "speaker_id": "2",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100024999.1.4",
          "form": "수제소시지 먹고싶어",
          "original_form": "수제소시지 먹고싶어",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100024999.1.5",
          "form": "식자재에는 수제소시지 안팔아",
          "original_form": "식자재에는 수제소시지 안팔아",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100024999.1.6",
          "form": "인터넷에 팔지않아?",
          "original_form": "인터넷에 팔지않아?",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100024999.1.7",
          "form": "그게 더 빨리 배송오겠다",
          "original_form": "그게 더 빨리 배송오겠다",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100024999.1.8",
          "form": "식품을 배송시키는 건 좀 그렇달가...",
          "original_form": "식품을 배송시키는 건 좀 그렇달가...",
          "speaker_id": "1",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100024999.1.9",
          "form": "여름은 좀 지났지만 습해서",
          "original_form": "여름은 좀 지났지만 습해서",
          "speaker_id": "1",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100024999.1.10",
          "form": "습한건 어쩔 수 없지",
          "original_form": "습한건 어쩔 수 없지",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100024999.1.11",
          "form": "그래도 냉동이니까 아이스팩 담아서 오지 않겠나",
          "original_form": "그래도 냉동이니까 아이스팩 담아서 오지 않겠나",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100024999.1.12",
          "form": "하지만 내가 최근에 시킨 냉동빵은 아이스팩이 녹아서 왔다 흑흑",
          "original_form": "하지만 내가 최근에 시킨 냉동빵은 아이스팩이 녹아서 왔다 흑흑",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100024999.1.13",
          "form": "하긴 여름도 다 지났는데",
          "original_form": "하긴 여름도 다 지났는데",
          "speaker_id": "1",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100024999.1.14",
          "form": "이제 연어장도 시켜볼까",
          "original_form": "이제 연어장도 시켜볼까",
          "speaker_id": "1",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100024999.1.15",
          "form": "연어사서 만들지",
          "original_form": "연어사서 만들지",
          "speaker_id": "2",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100024999.1.16",
          "form": "그럴까 근데 사서 만들면 또 남을거 같아서",
          "original_form": "그럴까 근데 사서 만들면 또 남을거 같아서",
          "speaker_id": "1",
          "time": "20210901 01:11"
        },
        {
          "id": "MDRW2100024999.1.17",
          "form": "많이 남았어?",
          "original_form": "많이 남았어?",
          "speaker_id": "2",
          "time": "20210901 08:14"
        },
        {
          "id": "MDRW2100024999.1.18",
          "form": "아니 아직 사진 않았지 근데 울 집에 입이 나 뿐이니 남을 거 같다는 말",
          "original_form": "아니 아직 사진 않았지 근데 울 집에 입이 나 뿐이니 남을 거 같다는 말",
          "speaker_id": "1",
          "time": "20210901 08:19"
        },
        {
          "id": "MDRW2100024999.1.19",
          "form": "아침마다 같이 밥 먹잖아 조금이라도 빨리 먹을 수 있지 않을까",
          "original_form": "아침마다 같이 밥 먹잖아 조금이라도 빨리 먹을 수 있지 않을까",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100024999.1.20",
          "form": "하긴 그건 그래 오늘 아침도 샐러드 먹었지",
          "original_form": "하긴 그건 그래 오늘 아침도 샐러드 먹었지",
          "speaker_id": "1",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100024999.1.21",
          "form": "연어사서 연어샐러드 해먹어",
          "original_form": "연어사서 연어샐러드 해먹어",
          "speaker_id": "2",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100024999.1.22",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100024999.1.23",
          "form": "연어 안 먹은지 오래됐네",
          "original_form": "연어 안 먹은지 오래됐네",
          "speaker_id": "2",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100024999.1.24",
          "form": "연어 하니까 연어초밥 먹고싶네",
          "original_form": "연어 하니까 연어초밥 먹고싶네",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100024999.1.25",
          "form": "연어장 생각나니까 새우장도 먹고싶고",
          "original_form": "연어장 생각나니까 새우장도 먹고싶고",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100024999.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:07"
        },
        {
          "id": "MDRW2100024999.1.27",
          "form": "다먹어",
          "original_form": "다먹어",
          "speaker_id": "2",
          "time": "20210901 12:07"
        },
        {
          "id": "MDRW2100024999.1.28",
          "form": "먹고싶은거 다 해먹어",
          "original_form": "먹고싶은거 다 해먹어",
          "speaker_id": "2",
          "time": "20210901 12:07"
        },
        {
          "id": "MDRW2100024999.1.29",
          "form": "초밥 맛있는데 가고싶다",
          "original_form": "초밥 맛있는데 가고싶다",
          "speaker_id": "1",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100024999.1.30",
          "form": "초밥 좋지",
          "original_form": "초밥 좋지",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100024999.1.31",
          "form": "나는 연어회만 맛있다면 어디든 좋아",
          "original_form": "나는 연어회만 맛있다면 어디든 좋아",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100024999.1.32",
          "form": "먹고싶은데 너무 달아서 안먹는거",
          "original_form": "먹고싶은데 너무 달아서 안먹는거",
          "speaker_id": "1",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100024999.1.33",
          "form": "달고나라떼류 크로플 아이스크림와플 마카롱",
          "original_form": "달고나라떼류 크로플 아이스크림와플 마카롱",
          "speaker_id": "1",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100024999.1.34",
          "form": "근데 요즘 이거 다 먹로싶음",
          "original_form": "근데 요즘 이거 다 먹로싶음",
          "speaker_id": "1",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100024999.1.35",
          "form": "곧 그 날이 오려카",
          "original_form": "곧 그 날이 오려카",
          "speaker_id": "2",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100024999.1.36",
          "form": "오려고 그러나",
          "original_form": "오려고 그러나",
          "speaker_id": "2",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100024999.1.37",
          "form": "크로플 얼마전에 먹었는데 맛은 있었고",
          "original_form": "크로플 얼마전에 먹었는데 맛은 있었고",
          "speaker_id": "2",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100024999.1.38",
          "form": "그날은 지났고 스트레스받아서 그런듯",
          "original_form": "그날은 지났고 스트레스받아서 그런듯",
          "speaker_id": "1",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100024999.1.39",
          "form": "카페 분위기도 좋았지맠",
          "original_form": "카페 분위기도 좋았지맠",
          "speaker_id": "2",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100024999.1.40",
          "form": "가격에서 조금 흔들렸던 기억이 난다 ㅋㅋㅋ",
          "original_form": "가격에서 조금 흔들렸던 기억이 난다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100024999.1.41",
          "form": "앗...힘쇼...",
          "original_form": "앗...힘쇼...",
          "speaker_id": "2",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100024999.1.42",
          "form": "크로플은 생지사서 걍 바로 해먹는게 더 맛있는데",
          "original_form": "크로플은 생지사서 걍 바로 해먹는게 더 맛있는데",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.43",
          "form": "괜히 생지 사고싶네",
          "original_form": "괜히 생지 사고싶네",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.44",
          "form": "마자 요즘 카페 가격 사악해",
          "original_form": "마자 요즘 카페 가격 사악해",
          "speaker_id": "1",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.45",
          "form": "냉동실에 자리도 없건만 8ㅁ8",
          "original_form": "냉동실에 자리도 없건만 8ㅁ8",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.46",
          "form": "생지 사도 와플기계 없음",
          "original_form": "생지 사도 와플기계 없음",
          "speaker_id": "1",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.47",
          "form": "우리집 얘기",
          "original_form": "우리집 얘기",
          "speaker_id": "1",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.48",
          "form": "우리집은 2대있어",
          "original_form": "우리집은 2대있어",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.49",
          "form": "생지사서 우리집와",
          "original_form": "생지사서 우리집와",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.50",
          "form": "그러니까 니가 생지 사서 해줘",
          "original_form": "그러니까 니가 생지 사서 해줘",
          "speaker_id": "1",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.51",
          "form": "먹고 가",
          "original_form": "먹고 가",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.52",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.53",
          "form": "바닐라 아이스크림 사갈게",
          "original_form": "바닐라 아이스크림 사갈게",
          "speaker_id": "1",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100024999.1.54",
          "form": "그러면 아이스크림 크로플 완성",
          "original_form": "그러면 아이스크림 크로플 완성",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100024999.1.55",
          "form": "집에 묵혀둔 투게더는 있다",
          "original_form": "집에 묵혀둔 투게더는 있다",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100024999.1.56",
          "form": "먹어야 하는데  날이 없네",
          "original_form": "먹어야 하는데  날이 없네",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100024999.1.57",
          "form": "와 가서 먹기만 하면 되나",
          "original_form": "와 가서 먹기만 하면 되나",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100024999.1.58",
          "form": "니가 집에 오래 있으면 너네집 자주 갈텐데",
          "original_form": "니가 집에 오래 있으면 너네집 자주 갈텐데",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100024999.1.59",
          "form": "물론 너의 의사는 중요치 않고",
          "original_form": "물론 너의 의사는 중요치 않고",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100024999.1.60",
          "form": "너네집이 맛집이잖어",
          "original_form": "너네집이 맛집이잖어",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100024999.1.61",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100024999.1.62",
          "form": "나 없어도 들렀다 가",
          "original_form": "나 없어도 들렀다 가",
          "speaker_id": "2",
          "time": "20210901 13:48"
        },
