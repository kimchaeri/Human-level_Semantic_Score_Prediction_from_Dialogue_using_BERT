      "utterance": [
        {
          "id": "MDRW2100011114.1.1",
          "form": "최근에 무슨 물건 샀어?",
          "original_form": "최근에 무슨 물건 샀어?",
          "speaker_id": "1",
          "time": "20210824 01:09"
        },
        {
          "id": "MDRW2100011114.1.2",
          "form": "난 가을 옷 샀어!",
          "original_form": "난 가을 옷 샀어!",
          "speaker_id": "2",
          "time": "20210824 01:10"
        },
        {
          "id": "MDRW2100011114.1.3",
          "form": "우와 어떤거?",
          "original_form": "우와 어떤거?",
          "speaker_id": "1",
          "time": "20210824 01:10"
        },
        {
          "id": "MDRW2100011114.1.4",
          "form": "가을 옷 살 때구만 벌써~~",
          "original_form": "가을 옷 살 때구만 벌써~~",
          "speaker_id": "1",
          "time": "20210824 01:10"
        },
        {
          "id": "MDRW2100011114.1.5",
          "form": "후드티랑 자켓!",
          "original_form": "후드티랑 자켓!",
          "speaker_id": "2",
          "time": "20210824 01:10"
        },
        {
          "id": "MDRW2100011114.1.6",
          "form": "이제 제법 날씨가 쌀쌀해져서 옷 많이 사고싶음ㅁ",
          "original_form": "이제 제법 날씨가 쌀쌀해져서 옷 많이 사고싶음ㅁ",
          "speaker_id": "2",
          "time": "20210824 01:11"
        },
        {
          "id": "MDRW2100011114.1.7",
          "form": "오 후드티 진짜 오랜 만이다",
          "original_form": "오 후드티 진짜 오랜 만이다",
          "speaker_id": "1",
          "time": "20210824 01:11"
        },
        {
          "id": "MDRW2100011114.1.8",
          "form": "나는 요즘 가을 원피스 사고 싶어",
          "original_form": "나는 요즘 가을 원피스 사고 싶어",
          "speaker_id": "1",
          "time": "20210824 01:12"
        },
        {
          "id": "MDRW2100011114.1.9",
          "form": "크 어떤 종류?",
          "original_form": "크 어떤 종류?",
          "speaker_id": "2",
          "time": "20210824 01:13"
        },
        {
          "id": "MDRW2100011114.1.10",
          "form": "음 바바리 원피스라던지..",
          "original_form": "음 바바리 원피스라던지..",
          "speaker_id": "1",
          "time": "20210824 01:14"
        },
        {
          "id": "MDRW2100011114.1.11",
          "form": "남방형식의 원피스라던지 나는 그런 걸 좋아해!!",
          "original_form": "남방형식의 원피스라던지 나는 그런 걸 좋아해!!",
          "speaker_id": "1",
          "time": "20210824 01:14"
        },
        {
          "id": "MDRW2100011114.1.12",
          "form": "우와 듣고보니까 나도 그런 종류 사고싶다ㅠ",
          "original_form": "우와 듣고보니까 나도 그런 종류 사고싶다ㅠ",
          "speaker_id": "2",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011114.1.13",
          "form": "중고거래는 해봤어?",
          "original_form": "중고거래는 해봤어?",
          "speaker_id": "2",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011114.1.14",
          "form": "웅웅 나는 주로 내 물건 파는거 자주 했어",
          "original_form": "웅웅 나는 주로 내 물건 파는거 자주 했어",
          "speaker_id": "1",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011114.1.15",
          "form": "안 입는 옷들 많이 팔았다! ㅋㅋㅋ",
          "original_form": "안 입는 옷들 많이 팔았다! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100011114.1.16",
          "form": "헐 난 옷은 거의 안팔리던데",
          "original_form": "헐 난 옷은 거의 안팔리던데",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011114.1.17",
          "form": "어떻게??",
          "original_form": "어떻게??",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011114.1.18",
          "form": "아 옷은 안팔리나? 나는 그냥 사진 찍어서 팔았었어",
          "original_form": "아 옷은 안팔리나? 나는 그냥 사진 찍어서 팔았었어",
          "speaker_id": "1",
          "time": "20210824 01:22"
        },
        {
          "id": "MDRW2100011114.1.19",
          "form": "우와 사진을 잘 찍나보다!",
          "original_form": "우와 사진을 잘 찍나보다!",
          "speaker_id": "2",
          "time": "20210824 01:22"
        },
        {
          "id": "MDRW2100011114.1.20",
          "form": "엥 전혀 아니었는데 ㅋㅋㅋㅋ 왜 팔렸지!! 신기하네",
          "original_form": "엥 전혀 아니었는데 ㅋㅋㅋㅋ 왜 팔렸지!! 신기하네",
          "speaker_id": "1",
          "time": "20210824 01:23"
        }
      ]
