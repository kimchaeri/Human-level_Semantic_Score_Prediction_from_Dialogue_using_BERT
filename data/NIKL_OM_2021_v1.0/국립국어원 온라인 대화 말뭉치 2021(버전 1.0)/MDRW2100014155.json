      "utterance": [
        {
          "id": "MDRW2100014155.1.1",
          "form": "안녕하세요  ㅎㅎ",
          "original_form": "안녕하세요  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014155.1.2",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014155.1.3",
          "form": "아뇨ㅠㅠ키우는거 있으세요?",
          "original_form": "아뇨ㅠㅠ키우는거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014155.1.4",
          "form": "고양이 두마리랑 푸들 한 마리 키웁니당ㅎㅎ",
          "original_form": "고양이 두마리랑 푸들 한 마리 키웁니당ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014155.1.5",
          "form": "어머 너무 좋을거같아요ㅜㅜ전 항상 상상만...",
          "original_form": "어머 너무 좋을거같아요ㅜㅜ전 항상 상상만...",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014155.1.6",
          "form": "집에 가면 행복해요 ㅋㅋ",
          "original_form": "집에 가면 행복해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014155.1.7",
          "form": "안 키우는 이유가 있으신가요",
          "original_form": "안 키우는 이유가 있으신가요",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014155.1.8",
          "form": "그냥 상황상...ㅎㅎ 엄두가 안나더라구요ㅠ",
          "original_form": "그냥 상황상...ㅎㅎ 엄두가 안나더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014155.1.9",
          "form": "강아지랑 고양이 중에 뭘 좋아하시나요 ㅎ",
          "original_form": "강아지랑 고양이 중에 뭘 좋아하시나요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014155.1.10",
          "form": "둘다 좋아해요ㅋㅋ 굳이 고르자면 강아지?ㅎㅎ",
          "original_form": "둘다 좋아해요ㅋㅋ 굳이 고르자면 강아지?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014155.1.11",
          "form": "크 저도 강아지파 임니다",
          "original_form": "크 저도 강아지파 임니다",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014155.1.12",
          "form": "사람애기 같아요",
          "original_form": "사람애기 같아요",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014155.1.13",
          "form": "ㅎㅎ 애교있구ㅎㅎ 말을 알아듣는다 하더라구요ㅋㅋ",
          "original_form": "ㅎㅎ 애교있구ㅎㅎ 말을 알아듣는다 하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014155.1.14",
          "form": "맞아요 ㅎㅎ 자기 필요한 건 기가막히게 알아들어요",
          "original_form": "맞아요 ㅎㅎ 자기 필요한 건 기가막히게 알아들어요",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014155.1.15",
          "form": "특히 간식",
          "original_form": "특히 간식",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014155.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014155.1.17",
          "form": "주변 강아지 키우는 집보면 진짜 사람사는 집같구ㅋㅋㅋ부러워요",
          "original_form": "주변 강아지 키우는 집보면 진짜 사람사는 집같구ㅋㅋㅋ부러워요",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014155.1.18",
          "form": "똑똑하구ㅋㅋ",
          "original_form": "똑똑하구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014155.1.19",
          "form": "시끌시끌하죠 ㅋㅋ 애교쟁이들",
          "original_form": "시끌시끌하죠 ㅋㅋ 애교쟁이들",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014155.1.20",
          "form": "고양이는 확실히 시크해요",
          "original_form": "고양이는 확실히 시크해요",
