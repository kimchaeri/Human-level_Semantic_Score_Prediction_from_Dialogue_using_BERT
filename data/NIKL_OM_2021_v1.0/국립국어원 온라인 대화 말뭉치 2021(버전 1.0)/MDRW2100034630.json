      "utterance": [
        {
          "id": "MDRW2100034630.1.1",
          "form": "그쵸 밤에 시키고 새벽에 신선하게 받으니 참 좋더라구요",
          "original_form": "그쵸 밤에 시키고 새벽에 신선하게 받으니 참 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034630.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034630.1.3",
          "form": "근데 또 장단점이",
          "original_form": "근데 또 장단점이",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034630.1.4",
          "form": "늦잠자고플땐 불편해요",
          "original_form": "늦잠자고플땐 불편해요",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034630.1.5",
          "form": "그러겠네요ㅎㅎ",
          "original_form": "그러겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034630.1.6",
          "form": "혹시 당근마켓 이용해보셨어요",
          "original_form": "혹시 당근마켓 이용해보셨어요",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034630.1.7",
          "form": "네 ㅋㅋㅋ가끔이요",
          "original_form": "네 ㅋㅋㅋ가끔이요",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034630.1.8",
          "form": "전 주로 파는거같아요",
          "original_form": "전 주로 파는거같아요",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034630.1.9",
          "form": "전 눈팅만하고",
          "original_form": "전 눈팅만하고",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034630.1.10",
          "form": "직접거래는 안 해봐성",
          "original_form": "직접거래는 안 해봐성",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034630.1.11",
          "form": "아아 ㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034630.1.12",
          "form": "전 채팅거래해봤어요",
          "original_form": "전 채팅거래해봤어요",
          "speaker_id": "1",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034630.1.13",
          "form": "기프티콘이여 ㅋㅋㅋㅋ",
          "original_form": "기프티콘이여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034630.1.14",
          "form": "오오 금방 팔리는 편인가요?",
          "original_form": "오오 금방 팔리는 편인가요?",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034630.1.15",
          "form": "음 보통 이삼일 정도면",
          "original_form": "음 보통 이삼일 정도면",
          "speaker_id": "1",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034630.1.16",
          "form": "거의팔려요 ㅋㅋㅋ",
          "original_form": "거의팔려요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034630.1.17",
          "form": "글쿤요 유저가 많은가봐요",
          "original_form": "글쿤요 유저가 많은가봐요",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034630.1.18",
          "form": "동네에 많나봐요",
          "original_form": "동네에 많나봐요",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034630.1.19",
          "form": "기프티콘은 평소에 많이 구입하는 편이세용?",
          "original_form": "기프티콘은 평소에 많이 구입하는 편이세용?",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034630.1.20",
          "form": "아저는 구입해서 파는건 아니구",
          "original_form": "아저는 구입해서 파는건 아니구",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034630.1.21",
          "form": "가끔 안쓰는거있으면 팔아요",
          "original_form": "가끔 안쓰는거있으면 팔아요",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034630.1.22",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210828 16:44"
        }
      ]
