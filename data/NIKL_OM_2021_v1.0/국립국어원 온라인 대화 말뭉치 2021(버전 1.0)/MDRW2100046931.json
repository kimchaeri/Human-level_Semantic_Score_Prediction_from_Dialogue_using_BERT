      "utterance": [
        {
          "id": "MDRW2100046931.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211109 00:40"
        },
        {
          "id": "MDRW2100046931.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211109 00:41"
        },
        {
          "id": "MDRW2100046931.1.3",
          "form": "틱톡 이용해보셨나요?",
          "original_form": "틱톡 이용해보셨나요?",
          "speaker_id": "2",
          "time": "20211109 00:41"
        },
        {
          "id": "MDRW2100046931.1.4",
          "form": "틱톡 사용해봤어요",
          "original_form": "틱톡 사용해봤어요",
          "speaker_id": "1",
          "time": "20211109 00:46"
        },
        {
          "id": "MDRW2100046931.1.5",
          "form": "오 ㅎㅎ",
          "original_form": "오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211109 00:51"
        },
        {
          "id": "MDRW2100046931.1.6",
          "form": "자주 사용하시나요?",
          "original_form": "자주 사용하시나요?",
          "speaker_id": "2",
          "time": "20211109 00:51"
        },
        {
          "id": "MDRW2100046931.1.7",
          "form": "네 심심할때 자주 봐요",
          "original_form": "네 심심할때 자주 봐요",
          "speaker_id": "1",
          "time": "20211109 00:53"
        },
        {
          "id": "MDRW2100046931.1.8",
          "form": "영상 올려보신 적은 있나요 ㅎㅎ",
          "original_form": "영상 올려보신 적은 있나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211109 00:57"
        },
        {
          "id": "MDRW2100046931.1.9",
          "form": "틱톡 보면 시간 너무 잘갈 거 같아요 ㅋㅋㅋㅋ",
          "original_form": "틱톡 보면 시간 너무 잘갈 거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211109 00:57"
        },
        {
          "id": "MDRW2100046931.1.10",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20211109 00:57"
        },
        {
          "id": "MDRW2100046931.1.11",
          "form": "올려본적은없고 다른사람 영상 보기만했어요",
          "original_form": "올려본적은없고 다른사람 영상 보기만했어요",
          "speaker_id": "1",
          "time": "20211109 00:58"
        },
        {
          "id": "MDRW2100046931.1.12",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20211109 00:58"
        },
        {
          "id": "MDRW2100046931.1.13",
          "form": "사실 틱톡 깔아 본 적이 없는데",
          "original_form": "사실 틱톡 깔아 본 적이 없는데",
          "speaker_id": "2",
          "time": "20211109 00:58"
        },
        {
          "id": "MDRW2100046931.1.14",
          "form": "제 친구가 틱톡 엄청 자주 봐서 어쩌다 같이 보고 그러기만 했는데",
          "original_form": "제 친구가 틱톡 엄청 자주 봐서 어쩌다 같이 보고 그러기만 했는데",
          "speaker_id": "2",
          "time": "20211109 00:58"
        },
        {
          "id": "MDRW2100046931.1.15",
          "form": "친구는 이제",
          "original_form": "친구는 이제",
          "speaker_id": "2",
          "time": "20211109 00:58"
        },
        {
          "id": "MDRW2100046931.1.16",
          "form": "유튜브를 못보겠다하더라구요",
          "original_form": "유튜브를 못보겠다하더라구요",
          "speaker_id": "2",
          "time": "20211109 00:58"
        },
        {
          "id": "MDRW2100046931.1.17",
          "form": "ㅋㅋㅋ너무 길다고 ㅋㅋ",
          "original_form": "ㅋㅋㅋ너무 길다고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211109 00:58"
        },
        {
          "id": "MDRW2100046931.1.18",
          "form": "유튜브광고가 많긴하죠",
          "original_form": "유튜브광고가 많긴하죠",
          "speaker_id": "1",
          "time": "20211108 13:20"
        },
        {
          "id": "MDRW2100046931.1.19",
          "form": "유튜브 광고차단어플이 있는데 아시나요?",
          "original_form": "유튜브 광고차단어플이 있는데 아시나요?",
          "speaker_id": "1",
          "time": "20211108 13:20"
        },
        {
          "id": "MDRW2100046931.1.20",
          "form": "엇?",
          "original_form": "엇?",
          "speaker_id": "2",
          "time": "20211108 13:23"
        },
        {
          "id": "MDRW2100046931.1.21",
          "form": "저는 유튜브 프리미엄 쓰고 있는데",
          "original_form": "저는 유튜브 프리미엄 쓰고 있는데",
          "speaker_id": "2",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046931.1.22",
          "form": "그런",
          "original_form": "그런",
          "speaker_id": "2",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046931.1.23",
          "form": "좋은 어플이 있나요..?!",
          "original_form": "좋은 어플이 있나요..?!",
          "speaker_id": "2",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046931.1.24",
          "form": "안드로이드 쓰시나요?",
          "original_form": "안드로이드 쓰시나요?",
          "speaker_id": "1",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046931.1.25",
          "form": "",
          "original_form": "{share:info}",
          "speaker_id": "1",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046931.1.26",
          "form": "앗 저 아이폰 유저입니다 ㅠㅠ",
          "original_form": "앗 저 아이폰 유저입니다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046931.1.27",
          "form": "라고 있는데",
          "original_form": "라고 있는데",
          "speaker_id": "1",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046931.1.28",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046931.1.29",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046931.1.30",
          "form": "이게 안드로이드에서만",
          "original_form": "이게 안드로이드에서만",
          "speaker_id": "2",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046931.1.31",
          "form": "작동이되는군요 흑흑",
          "original_form": "작동이되는군요 흑흑",
          "speaker_id": "2",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046931.1.32",
          "form": "네 안드로이드에서만 가능해요",
          "original_form": "네 안드로이드에서만 가능해요",
          "speaker_id": "1",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046931.1.33",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "2",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046931.1.34",
          "form": "일주일 전에 바꿨는데.. 갤럭시에서 아이폰으로 ㅠ",
          "original_form": "일주일 전에 바꿨는데.. 갤럭시에서 아이폰으로 ㅠ",
          "speaker_id": "2",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046931.1.35",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046931.1.36",
          "form": "광고도 차단되고 백그라운드 재생도 되요",
          "original_form": "광고도 차단되고 백그라운드 재생도 되요",
          "speaker_id": "1",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046931.1.37",
          "form": "헉 그냥 유튜브 프리미엄이네요;;",
          "original_form": "헉 그냥 유튜브 프리미엄이네요;;",
          "speaker_id": "2",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046931.1.38",
          "form": "저는 못쓰지만",
          "original_form": "저는 못쓰지만",
          "speaker_id": "2",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046931.1.39",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046931.1.40",
          "form": "친구들한테 알려줘야겠어요 ㅠ",
          "original_form": "친구들한테 알려줘야겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20211108 13:26"
        },
