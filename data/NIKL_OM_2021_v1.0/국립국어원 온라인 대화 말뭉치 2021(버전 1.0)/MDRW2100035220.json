      "utterance": [
        {
          "id": "MDRW2100035220.1.1",
          "form": "제일 좋아하는 파스타는 어떤건가요?",
          "original_form": "제일 좋아하는 파스타는 어떤건가요?",
          "speaker_id": "1",
          "time": "20210829 12:06"
        },
        {
          "id": "MDRW2100035220.1.2",
          "form": "치즈오븐스파게티요!!",
          "original_form": "치즈오븐스파게티요!!",
          "speaker_id": "2",
          "time": "20210829 12:06"
        },
        {
          "id": "MDRW2100035220.1.3",
          "form": "그거때문에 오븐사고 싶어요",
          "original_form": "그거때문에 오븐사고 싶어요",
          "speaker_id": "2",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100035220.1.4",
          "form": "그거 에어프라이기로도 가능하대요!",
          "original_form": "그거 에어프라이기로도 가능하대요!",
          "speaker_id": "1",
          "time": "20210829 12:13"
        },
        {
          "id": "MDRW2100035220.1.5",
          "form": "헐???!!..!.!!)))",
          "original_form": "헐???!!..!.!!)))",
          "speaker_id": "2",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100035220.1.6",
          "form": "처음알았어요 가능한거에요?",
          "original_form": "처음알았어요 가능한거에요?",
          "speaker_id": "2",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100035220.1.7",
          "form": "그거 에프용으로도 파는거 본거같아요",
          "original_form": "그거 에프용으로도 파는거 본거같아요",
          "speaker_id": "1",
          "time": "20210829 12:17"
        },
        {
          "id": "MDRW2100035220.1.8",
          "form": "치즈 에프넣고 구으면 오븐에 구은거처럼 돼요",
          "original_form": "치즈 에프넣고 구으면 오븐에 구은거처럼 돼요",
          "speaker_id": "1",
          "time": "20210829 12:17"
        },
        {
          "id": "MDRW2100035220.1.9",
          "form": "레시피 검색해보셔요!",
          "original_form": "레시피 검색해보셔요!",
          "speaker_id": "1",
          "time": "20210829 12:17"
        },
        {
          "id": "MDRW2100035220.1.10",
          "form": "우와.. 감사합니당",
          "original_form": "우와.. 감사합니당",
          "speaker_id": "2",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100035220.1.11",
          "form": "에프용 그릇이 파는건가요?",
          "original_form": "에프용 그릇이 파는건가요?",
          "speaker_id": "2",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100035220.1.12",
          "form": "아마두요.. ?",
          "original_form": "아마두요.. ?",
          "speaker_id": "1",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100035220.1.13",
          "form": "저도 사본적은 없어서...",
          "original_form": "저도 사본적은 없어서...",
          "speaker_id": "1",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100035220.1.14",
          "form": "오오.. 신세계 만난 기분이에오ㅋㅋ",
          "original_form": "오오.. 신세계 만난 기분이에오ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100035220.1.15",
          "form": "찾아보니 그릇 넣어서 하면 된다는데 그릇이 어떤건지 못찾았어요ㅜ",
          "original_form": "찾아보니 그릇 넣어서 하면 된다는데 그릇이 어떤건지 못찾았어요ㅜ",
          "speaker_id": "2",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100035220.1.16",
          "form": "밖에서 파는거처럼생긴 호일용 그릇 말하는거 같아요",
          "original_form": "밖에서 파는거처럼생긴 호일용 그릇 말하는거 같아요",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100035220.1.17",
          "form": "아아!! 라면먹을때 같은거죠??ㅈ",
          "original_form": "아아!! 라면먹을때 같은거죠??ㅈ",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100035220.1.18",
          "form": "일회용 은박지 그릇? 도시락 이런거 치니까 나와요!",
          "original_form": "일회용 은박지 그릇? 도시락 이런거 치니까 나와요!",
          "speaker_id": "1",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100035220.1.19",
          "form": "오오오 감사합니당!!",
          "original_form": "오오오 감사합니당!!",
          "speaker_id": "2",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100035220.1.20",
          "form": "빠른일시에 사서 해먹어야겠어요",
          "original_form": "빠른일시에 사서 해먹어야겠어요",
          "speaker_id": "2",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100035220.1.21",
          "form": "맛있었으면 좋겠네요!",
          "original_form": "맛있었으면 좋겠네요!",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035220.1.22",
          "form": "감사합니당~",
          "original_form": "감사합니당~",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100035220.1.23",
          "form": "후기를 들려드리고 싶네욥",
          "original_form": "후기를 들려드리고 싶네욥",
