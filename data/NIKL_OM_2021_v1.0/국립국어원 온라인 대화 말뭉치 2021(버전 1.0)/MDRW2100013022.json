        {
          "id": "MDRW2100013022.1.2",
          "form": "넵ㅎㅎ",
          "original_form": "넵ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100013022.1.3",
          "form": "곧가야죠",
          "original_form": "곧가야죠",
          "speaker_id": "2",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100013022.1.4",
          "form": "ㅋㅋㅋㅋㅋ요즘은 2년보다 더 짧아졌다는데",
          "original_form": "ㅋㅋㅋㅋㅋ요즘은 2년보다 더 짧아졌다는데",
          "speaker_id": "1",
          "time": "20210826 09:36"
        },
        {
          "id": "MDRW2100013022.1.5",
          "form": "18개월? 그렇더라고요",
          "original_form": "18개월? 그렇더라고요",
          "speaker_id": "2",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100013022.1.6",
          "form": "name1님은 다녀오셨나요ㅋㅋ",
          "original_form": "&name1&님은 다녀오셨나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:48"
        },
        {
          "id": "MDRW2100013022.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ전 법적으로 면제입니다^^~!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전 법적으로 면제입니다^^~!",
          "speaker_id": "1",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100013022.1.8",
          "form": "ㅋㅋㅋ아ㅋㄲ 여성분이시구나",
          "original_form": "ㅋㅋㅋ아ㅋㄲ 여성분이시구나",
          "speaker_id": "2",
          "time": "20210826 09:56"
        },
        {
          "id": "MDRW2100013022.1.9",
          "form": "정보를 못 봤었어요ㅋㅋ",
          "original_form": "정보를 못 봤었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:56"
        },
        {
          "id": "MDRW2100013022.1.10",
          "form": "ㅋㅋㅋㅋ네ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ네ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100013022.1.11",
          "form": "군대 특별히 생각하는 곳 있으신가요??",
          "original_form": "군대 특별히 생각하는 곳 있으신가요??",
          "speaker_id": "1",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100013022.1.12",
          "form": "전 해군가고 싶어요ㅋㅋ",
          "original_form": "전 해군가고 싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:58"
        },
        {
          "id": "MDRW2100013022.1.13",
          "form": "오 해군 멋지죠! 멀미 없으시나요?",
          "original_form": "오 해군 멋지죠! 멀미 없으시나요?",
          "speaker_id": "1",
          "time": "20210826 10:03"
        },
        {
          "id": "MDRW2100013022.1.14",
          "form": "배 타본지가 오래돼서 기억이 안나네요ㅎㅎ",
          "original_form": "배 타본지가 오래돼서 기억이 안나네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:11"
        },
        {
          "id": "MDRW2100013022.1.15",
          "form": "차멀미는 안합니다!",
          "original_form": "차멀미는 안합니다!",
          "speaker_id": "2",
          "time": "20210826 10:11"
        },
        {
          "id": "MDRW2100013022.1.16",
          "form": "그렇군요...ㅎㅎ",
          "original_form": "그렇군요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100013022.1.17",
          "form": "만약 저는 가게 된다면 공군 가보고 싶네요! 원래 꿈이 그쪽이었거든요",
          "original_form": "만약 저는 가게 된다면 공군 가보고 싶네요! 원래 꿈이 그쪽이었거든요",
          "speaker_id": "1",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100013022.1.18",
          "form": "오호?",
          "original_form": "오호?",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100013022.1.19",
          "form": "조종사 쪽이 꿈이셨나요?",
          "original_form": "조종사 쪽이 꿈이셨나요?",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100013022.1.20",
          "form": "네 그 공군사관학교 가고 싶었는데 키가 안돼서 포기했어요ㅋㅋ",
          "original_form": "네 그 공군사관학교 가고 싶었는데 키가 안돼서 포기했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:15"
        },
        {
          "id": "MDRW2100013022.1.21",
          "form": "ㅠ..  아쉽네요",
          "original_form": "ㅠ..  아쉽네요",
          "speaker_id": "2",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100013022.1.22",
          "form": "전 해군가면 해외 파병 기회가 많다고해서",
          "original_form": "전 해군가면 해외 파병 기회가 많다고해서",
          "speaker_id": "2",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100013022.1.23",
          "form": "해군가고 싶어요ㅎㅎ",
          "original_form": "해군가고 싶어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100013022.1.24",
          "form": "오 해외 파병... 모험 좋아하시나봐요",
          "original_form": "오 해외 파병... 모험 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210826 10:22"
        },
        {
          "id": "MDRW2100013022.1.25",
          "form": "ㅋㅋ그냥 새로운게 좋은것같아요ㅋㅋ",
          "original_form": "ㅋㅋ그냥 새로운게 좋은것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100013022.1.26",
          "form": "사실 군대도 기대중",
          "original_form": "사실 군대도 기대중",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100013022.1.27",
          "form": "아ㅋㅋㅋㅋ 군대 기대 중이신 분 첨봐요ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ 군대 기대 중이신 분 첨봐요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100013022.1.28",
          "form": "사실 언제 이런 데 가보겠어요",
          "original_form": "사실 언제 이런 데 가보겠어요",
