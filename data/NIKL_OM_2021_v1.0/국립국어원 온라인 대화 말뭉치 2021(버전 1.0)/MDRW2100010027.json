      "utterance": [
        {
          "id": "MDRW2100010027.1.1",
          "form": "청소년 수련관에서",
          "original_form": "청소년 수련관에서",
          "speaker_id": "2",
          "time": "20210823 13:21"
        },
        {
          "id": "MDRW2100010027.1.2",
          "form": "모집해서 나이가 12부터였어요",
          "original_form": "모집해서 나이가 12부터였어요",
          "speaker_id": "2",
          "time": "20210823 13:21"
        },
        {
          "id": "MDRW2100010027.1.3",
          "form": "그래서 아이가 코로나로 심심하니 보냈어요",
          "original_form": "그래서 아이가 코로나로 심심하니 보냈어요",
          "speaker_id": "2",
          "time": "20210823 13:21"
        },
        {
          "id": "MDRW2100010027.1.4",
          "form": "와 잘 하셧네요",
          "original_form": "와 잘 하셧네요",
          "speaker_id": "1",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010027.1.5",
          "form": "육아할 때 정보가 필수인가봐요 역시",
          "original_form": "육아할 때 정보가 필수인가봐요 역시",
          "speaker_id": "1",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010027.1.6",
          "form": "네 ㅎㅎ좋은프로가 많아요",
          "original_form": "네 ㅎㅎ좋은프로가 많아요",
          "speaker_id": "2",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010027.1.7",
          "form": "개인적으로는 비싸니까요",
          "original_form": "개인적으로는 비싸니까요",
          "speaker_id": "2",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010027.1.8",
          "form": "맞아요. 그래도 따님이 성실하신가봐요. 귀찮아할 텐데",
          "original_form": "맞아요. 그래도 따님이 성실하신가봐요. 귀찮아할 텐데",
          "speaker_id": "1",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010027.1.9",
          "form": "안한가 했었어요 ㅡㅡ",
          "original_form": "안한가 했었어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210823 13:43"
        },
        {
          "id": "MDRW2100010027.1.10",
          "form": "제가 막시켰는데 하니 좋아해요 ㅎㅎ",
          "original_form": "제가 막시켰는데 하니 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 13:43"
        },
        {
          "id": "MDRW2100010027.1.11",
          "form": "ㅎㅎ 아이들은 그때는 모르는거 같아요",
          "original_form": "ㅎㅎ 아이들은 그때는 모르는거 같아요",
          "speaker_id": "1",
          "time": "20210823 13:52"
        },
        {
          "id": "MDRW2100010027.1.12",
          "form": "네 지나고 나면 좋다고 해요",
          "original_form": "네 지나고 나면 좋다고 해요",
          "speaker_id": "2",
          "time": "20210823 13:55"
        },
        {
          "id": "MDRW2100010027.1.13",
          "form": "커피 갈고나면 뒷처리 힘들던데ㅋㅋ",
          "original_form": "커피 갈고나면 뒷처리 힘들던데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010027.1.14",
          "form": "ㅋㅋ반은 아이가 반은 제 숙제죠",
          "original_form": "ㅋㅋ반은 아이가 반은 제 숙제죠",
          "speaker_id": "2",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100010027.1.15",
          "form": "그래도 어느새 다커서 커피를 내리다니..ㅠ 육아는 좀 덜 힘드시겠어요^^",
          "original_form": "그래도 어느새 다커서 커피를 내리다니..ㅠ 육아는 좀 덜 힘드시겠어요^^",
          "speaker_id": "1",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010027.1.16",
          "form": "아니예요 크니까 친구관계 사춘기 이런문제들이 더 힘들어 졌어요",
          "original_form": "아니예요 크니까 친구관계 사춘기 이런문제들이 더 힘들어 졌어요",
          "speaker_id": "2",
          "time": "20210823 14:03"
        },
        {
          "id": "MDRW2100010027.1.17",
          "form": "어릴때 단순한 문제 인데 클수록 엄마가 해줄수 있는것도 해결법도 모르겠어요",
          "original_form": "어릴때 단순한 문제 인데 클수록 엄마가 해줄수 있는것도 해결법도 모르겠어요",
          "speaker_id": "2",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010027.1.18",
          "form": "어렸을때가 좋은거 같아요",
          "original_form": "어렸을때가 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010027.1.19",
          "form": "요새 코로나라 아이들 활동이 줄었지만 그래도 하니 알아봐서 해주세요",
          "original_form": "요새 코로나라 아이들 활동이 줄었지만 그래도 하니 알아봐서 해주세요",
          "speaker_id": "2",
          "time": "20210823 14:51"
        },
        {
          "id": "MDRW2100010027.1.20",
          "form": "아이들 가야금도 가르쳐주고 쿠킹 할게많아요",
          "original_form": "아이들 가야금도 가르쳐주고 쿠킹 할게많아요",
          "speaker_id": "2",
          "time": "20210823 14:51"
        },
        {
          "id": "MDRW2100010027.1.21",
          "form": "가야금 같은 전통악기 진짜 좋네요",
          "original_form": "가야금 같은 전통악기 진짜 좋네요",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010027.1.22",
          "form": "크면 육아에 몸보다 마음이 힘들군요ㅠㅠ",
          "original_form": "크면 육아에 몸보다 마음이 힘들군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010027.1.23",
          "form": "네 마음이 힘든게 더 안좋아요",
          "original_form": "네 마음이 힘든게 더 안좋아요",
          "speaker_id": "2",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010027.1.24",
          "form": "사춘기 사라지면 좋겠네요",
          "original_form": "사춘기 사라지면 좋겠네요",
