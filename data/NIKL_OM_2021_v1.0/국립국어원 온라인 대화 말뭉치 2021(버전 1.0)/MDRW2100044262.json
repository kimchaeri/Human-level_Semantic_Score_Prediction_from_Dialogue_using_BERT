      "utterance": [
        {
          "id": "MDRW2100044262.1.1",
          "form": "요즘 영화나 드라마 본거있어?",
          "original_form": "요즘 영화나 드라마 본거있어?",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044262.1.2",
          "form": "주말에 영화보구 안본듯ㅎ",
          "original_form": "주말에 영화보구 안본듯ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044262.1.3",
          "form": "지금은 유튜브 보는중",
          "original_form": "지금은 유튜브 보는중",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044262.1.4",
          "form": "유튜브 뭐봐",
          "original_form": "유튜브 뭐봐",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044262.1.5",
          "form": "ㅇ지금은 예전 무한도전",
          "original_form": "ㅇ지금은 예전 무한도전",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044262.1.6",
          "form": "정현돈 나옴",
          "original_form": "정현돈 나옴",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044262.1.7",
          "form": "오 무도 재밌지!",
          "original_form": "오 무도 재밌지!",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044262.1.8",
          "form": "무도피디가 놀뭐피디인데",
          "original_form": "무도피디가 놀뭐피디인데",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044262.1.9",
          "form": "이번에 엠비씨 나간대",
          "original_form": "이번에 엠비씨 나간대",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044262.1.10",
          "form": "티비엔 간데?",
          "original_form": "티비엔 간데?",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044262.1.11",
          "form": "아니 독자적으로 회사설립한대",
          "original_form": "아니 독자적으로 회사설립한대",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044262.1.12",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044262.1.13",
          "form": "대단",
          "original_form": "대단",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044262.1.14",
          "form": "그러게 보니까 자기 맘대로 하고싶은가봐",
          "original_form": "그러게 보니까 자기 맘대로 하고싶은가봐",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044262.1.15",
          "form": "그런듯하네 성에 안차고 힘들었나보다",
          "original_form": "그런듯하네 성에 안차고 힘들었나보다",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044262.1.16",
          "form": "엉 엄청 오래 있었는데 프로그램 두개했대",
          "original_form": "엉 엄청 오래 있었는데 프로그램 두개했대",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044262.1.17",
          "form": "둘다 너무 인기가많아서",
          "original_form": "둘다 너무 인기가많아서",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044262.1.18",
          "form": "다른프로 못했지",
          "original_form": "다른프로 못했지",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044262.1.19",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044262.1.20",
          "form": "근디 보면 장수하는 프로는 뭔가 있긴하더라",
          "original_form": "근디 보면 장수하는 프로는 뭔가 있긴하더라",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044262.1.21",
          "form": "ㅇㅇ 재밌으니 ㅜㅜ",
          "original_form": "ㅇㅇ 재밌으니 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044262.1.22",
          "form": "유튜브 또 재미난거있나",
          "original_form": "유튜브 또 재미난거있나",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044262.1.23",
          "form": "난 요즘 짧은 영상 보는 재미에 빠짐",
          "original_form": "난 요즘 짧은 영상 보는 재미에 빠짐",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044262.1.24",
          "form": "숏폼이라 하던데",
          "original_form": "숏폼이라 하던데",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044262.1.25",
          "form": "CU꿀젤리라는 신제품 나왔데ㅋ",
          "original_form": "CU꿀젤리라는 신제품 나왔데ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044262.1.26",
          "form": "난 보통 유튜브 밑에 짧게 나오는거 잘봐져",
          "original_form": "난 보통 유튜브 밑에 짧게 나오는거 잘봐져",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044262.1.27",
          "form": "그거 숏폼이래",
          "original_form": "그거 숏폼이래",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044262.1.28",
          "form": "숏폼 이라는게 있구나ㅎ",
          "original_form": "숏폼 이라는게 있구나ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044262.1.29",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044262.1.30",
          "form": "나도 그거봐지더라",
          "original_form": "나도 그거봐지더라",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044262.1.31",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044262.1.32",
          "form": "간단해서 좋아",
          "original_form": "간단해서 좋아",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044262.1.33",
          "form": "1분요리같은거 보면재밌더라",
          "original_form": "1분요리같은거 보면재밌더라",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044262.1.34",
          "form": "웃긴건 웃끼고",
          "original_form": "웃긴건 웃끼고",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044262.1.35",
          "form": "신기한건 신기하고",
          "original_form": "신기한건 신기하고",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044262.1.36",
          "form": "ㅇㅇ 짧은게 대세긴 하나봐",
          "original_form": "ㅇㅇ 짧은게 대세긴 하나봐",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044262.1.37",
          "form": "사람들이 긴 영상을 요즘 못봐서",
          "original_form": "사람들이 긴 영상을 요즘 못봐서",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044262.1.38",
          "form": "짧은영상이 유행이래",
          "original_form": "짧은영상이 유행이래",
          "speaker_id": "1",
          "time": "20210907 22:21"
        }
      ]
