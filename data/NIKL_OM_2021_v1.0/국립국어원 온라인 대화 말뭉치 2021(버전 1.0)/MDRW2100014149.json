      "utterance": [
        {
          "id": "MDRW2100014149.1.1",
          "form": "멸치를 언제 넣어야해요?끓이고 멸치는 빼는거죠?",
          "original_form": "멸치를 언제 넣어야해요?끓이고 멸치는 빼는거죠?",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014149.1.2",
          "form": "아 멸치액젓넣는거에요",
          "original_form": "아 멸치액젓넣는거에요",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014149.1.3",
          "form": "마지막에넣으세요",
          "original_form": "마지막에넣으세요",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014149.1.4",
          "form": "그리고참.... 다진마늘 넣어야해요",
          "original_form": "그리고참.... 다진마늘 넣어야해요",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014149.1.5",
          "form": "넵 감사합니다 ",
          "original_form": "넵 감사합니다 ",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014149.1.6",
          "form": "다진마늘을 깜빡했네요",
          "original_form": "다진마늘을 깜빡했네요",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014149.1.7",
          "form": "ㅋㅋㅋㅋ네! 다진마늘 추가",
          "original_form": "ㅋㅋㅋㅋ네! 다진마늘 추가",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014149.1.8",
          "form": "넵 성공하시길^^",
          "original_form": "넵 성공하시길^^",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014149.1.9",
          "form": "음 공통주제가 집안일 뿐인데 청소는 어떻게 하세요?",
          "original_form": "음 공통주제가 집안일 뿐인데 청소는 어떻게 하세요?",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014149.1.10",
          "form": "루틴이 있는지 궁금해요",
          "original_form": "루틴이 있는지 궁금해요",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014149.1.11",
          "form": "청소 닦는게 제일힘들어서",
          "original_form": "청소 닦는게 제일힘들어서",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014149.1.12",
          "form": "3m청소포 사용해요",
          "original_form": "3m청소포 사용해요",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014149.1.13",
          "form": "물기있는거요?",
          "original_form": "물기있는거요?",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014149.1.14",
          "form": "아뇨 없는걸루요",
          "original_form": "아뇨 없는걸루요",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014149.1.15",
          "form": "아! 닦는게 힘들다고 하셔서 물기 있는거 말씀하시는줄 알았어요",
          "original_form": "아! 닦는게 힘들다고 하셔서 물기 있는거 말씀하시는줄 알았어요",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014149.1.16",
          "form": "저도 물기 없는걸로 싸악해서 먼지랑 머리카락부터 제거하고",
          "original_form": "저도 물기 없는걸로 싸악해서 먼지랑 머리카락부터 제거하고",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014149.1.17",
          "form": "물기는 없어도 미세먼지같은게 해결되서요 ㅎ",
          "original_form": "물기는 없어도 미세먼지같은게 해결되서요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014149.1.18",
          "form": "젤편하죠? ㅎ",
          "original_form": "젤편하죠? ㅎ",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014149.1.19",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014149.1.20",
          "form": "손목에 무리도 덜가구요",
          "original_form": "손목에 무리도 덜가구요",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014149.1.21",
          "form": "맞아요 청소기 아무리 가볍다고 한들 한바퀴 돌고오면 힘들어요",
          "original_form": "맞아요 청소기 아무리 가볍다고 한들 한바퀴 돌고오면 힘들어요",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014149.1.22",
          "form": "네 물기있음 손목에 힘이 더들어가구요 ㅠ",
          "original_form": "네 물기있음 손목에 힘이 더들어가구요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014149.1.23",
          "form": "청소기 필터는 쓸때마다 비우세요?",
          "original_form": "청소기 필터는 쓸때마다 비우세요?",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014149.1.24",
          "form": "아니요 꽉차면요..\r\n그것도 하기싫은것중 하나에요 ㅠㅠ",
          "original_form": "아니요 꽉차면요..\r\n그것도 하기싫은것중 하나에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014149.1.25",
          "form": "저는 살땐 그런 생각으로 샀는데 맘처럼 쉽지않네요 ㅠ",
          "original_form": "저는 살땐 그런 생각으로 샀는데 맘처럼 쉽지않네요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014149.1.26",
          "form": "앜ㅋㅋㅋ 역시",
          "original_form": "앜ㅋㅋㅋ 역시",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014149.1.27",
          "form": "다 똑같나봐요",
          "original_form": "다 똑같나봐요",
          "speaker_id": "2",
          "time": "20210825 17:39"
        }
      ]
    }
