      "utterance": [
        {
          "id": "MDRW2100007954.1.1",
          "form": "싸이월드 해보셨나요?",
          "original_form": "싸이월드 해보셨나요?",
          "speaker_id": "1",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007954.1.2",
          "form": "예전에 가입은 했었는데ㅎㅎ",
          "original_form": "예전에 가입은 했었는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007954.1.3",
          "form": "저는 사실 싸이월드 세대가 아니에요",
          "original_form": "저는 사실 싸이월드 세대가 아니에요",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007954.1.4",
          "form": "대신 얘기는 많이 들었어요",
          "original_form": "대신 얘기는 많이 들었어요",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007954.1.5",
          "form": "아ㅋㅋ페북 세대 맞으시죠?",
          "original_form": "아ㅋㅋ페북 세대 맞으시죠?",
          "speaker_id": "2",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007954.1.6",
          "form": "페이스북도 많이 하진 않아요",
          "original_form": "페이스북도 많이 하진 않아요",
          "speaker_id": "1",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007954.1.7",
          "form": "싸이월드는 어떤 곳이었나요?",
          "original_form": "싸이월드는 어떤 곳이었나요?",
          "speaker_id": "1",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007954.1.8",
          "form": "페북이랑 비슷할거에요ㅎㅎ 근데 약간 일기장같은 느낌이 더 많았어요",
          "original_form": "페북이랑 비슷할거에요ㅎㅎ 근데 약간 일기장같은 느낌이 더 많았어요",
          "speaker_id": "2",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007954.1.9",
          "form": "일기장이라고 말씀해주시니 이해가 잘 되네요",
          "original_form": "일기장이라고 말씀해주시니 이해가 잘 되네요",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007954.1.10",
          "form": "그래서 약간 부끄러워서 안했던것같아요ㅎㅎ",
          "original_form": "그래서 약간 부끄러워서 안했던것같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007954.1.11",
          "form": "그러셨구나",
          "original_form": "그러셨구나",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007954.1.12",
          "form": "가끔 싸이월드 유명곡이라고 뜨는걸 봤는데",
          "original_form": "가끔 싸이월드 유명곡이라고 뜨는걸 봤는데",
          "speaker_id": "1",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007954.1.13",
          "form": "아 맞다 그런게 있었죠ㅋㅋ멜론 탑백같은ㅋㅋ",
          "original_form": "아 맞다 그런게 있었죠ㅋㅋ멜론 탑백같은ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007954.1.14",
          "form": "아하 그런 느낌이군요",
          "original_form": "아하 그런 느낌이군요",
          "speaker_id": "1",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007954.1.15",
          "form": "네 돌고도는거같아요",
          "original_form": "네 돌고도는거같아요",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007954.1.16",
          "form": "어떤 노래 제일 좋아하셨나요?",
          "original_form": "어떤 노래 제일 좋아하셨나요?",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007954.1.17",
          "form": "저는 팝종류 좋아했던거같아요",
          "original_form": "저는 팝종류 좋아했던거같아요",
          "speaker_id": "2",
          "time": "20210822 12:31"
        }
      ]
