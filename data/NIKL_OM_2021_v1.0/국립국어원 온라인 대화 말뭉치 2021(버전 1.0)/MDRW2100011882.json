      "utterance": [
        {
          "id": "MDRW2100011882.1.1",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011882.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011882.1.3",
          "form": "결혼식 자체가 너무 싫어서 결혼식 가기가 싫어욬ㅋㅋㅋ",
          "original_form": "결혼식 자체가 너무 싫어서 결혼식 가기가 싫어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011882.1.4",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011882.1.5",
          "form": "신랑의 신부획득 쇼쇼쇼",
          "original_form": "신랑의 신부획득 쇼쇼쇼",
          "speaker_id": "1",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011882.1.6",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011882.1.7",
          "form": "가고싶지않아요!!",
          "original_form": "가고싶지않아요!!",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011882.1.8",
          "form": "맞아요 ㄹㅇ",
          "original_form": "맞아요 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011882.1.9",
          "form": "축의금만 내고말지..",
          "original_form": "축의금만 내고말지..",
          "speaker_id": "1",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011882.1.10",
          "form": "그걸 제눈에 담고싶지않아욬ㅋㅋㅋ",
          "original_form": "그걸 제눈에 담고싶지않아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011882.1.11",
          "form": "친한만큼 안가면 서운할 거 알지만",
          "original_form": "친한만큼 안가면 서운할 거 알지만",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100011882.1.12",
          "form": "속마음은 안가고싶어요",
          "original_form": "속마음은 안가고싶어요",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100011882.1.13",
          "form": "맞아요 그냥 제 눈으로 굳이",
          "original_form": "맞아요 그냥 제 눈으로 굳이",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100011882.1.14",
          "form": "축하하다는 말도 잘 못꺼낼 것 같아서",
          "original_form": "축하하다는 말도 잘 못꺼낼 것 같아서",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100011882.1.15",
          "form": "맞아요 사실 안축하하니까..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 사실 안축하하니까..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100011882.1.16",
          "form": "네 진짜",
          "original_form": "네 진짜",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100011882.1.17",
          "form": "안축하해서",
          "original_form": "안축하해서",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100011882.1.18",
          "form": "근데 뭐 어쩔 수 없죠",
          "original_form": "근데 뭐 어쩔 수 없죠",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100011882.1.19",
          "form": "표정관리안돼요",
          "original_form": "표정관리안돼요",
          "speaker_id": "1",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100011882.1.20",
          "form": "ㅋㅋㅋㅌㅌㅋㅋ",
          "original_form": "ㅋㅋㅋㅌㅌㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100011882.1.21",
          "form": "진짜 기묘하네요",
          "original_form": "진짜 기묘하네요",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100011882.1.22",
          "form": "어찌보면 사회생활이니",
          "original_form": "어찌보면 사회생활이니",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100011882.1.23",
          "form": "축의금도 많이 뜯기고.. 이거 회수 불가능한거 생각하면 슬픕니다",
          "original_form": "축의금도 많이 뜯기고.. 이거 회수 불가능한거 생각하면 슬픕니다",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011882.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011882.1.25",
          "form": "비혼식이 있다하긴 하던데",
          "original_form": "비혼식이 있다하긴 하던데",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011882.1.26",
          "form": "사회생활 맞아요 ㅜ 불가피한..",
          "original_form": "사회생활 맞아요 ㅜ 불가피한..",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011882.1.27",
          "form": "잘 자리잡을지도 모르겠고",
          "original_form": "잘 자리잡을지도 모르겠고",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011882.1.28",
          "form": "비혼식 하면 많이 올까요..",
          "original_form": "비혼식 하면 많이 올까요..",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011882.1.29",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011882.1.30",
          "form": "유난이라고 취급받을거같아요 ㅜ",
          "original_form": "유난이라고 취급받을거같아요 ㅜ",
          "speaker_id": "1",
          "time": "20210824 17:04"
        }
      ]
