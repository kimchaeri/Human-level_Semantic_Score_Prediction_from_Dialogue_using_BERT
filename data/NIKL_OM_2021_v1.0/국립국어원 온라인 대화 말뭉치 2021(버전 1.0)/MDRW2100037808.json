        {
          "id": "MDRW2100037808.1.2",
          "form": "저 반지사고싶어요,,,",
          "original_form": "저 반지사고싶어요,,,",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100037808.1.3",
          "form": "우와 무슨 반지요?",
          "original_form": "우와 무슨 반지요?",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100037808.1.4",
          "form": "그냥 조금 두꺼운 반지를 갖고싶어졌어요! ㅋㄲ",
          "original_form": "그냥 조금 두꺼운 반지를 갖고싶어졌어요! ㅋㄲ",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037808.1.5",
          "form": "신발도사고싶고,,,",
          "original_form": "신발도사고싶고,,,",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037808.1.6",
          "form": "저 예전에 실반지 사고싶었는데",
          "original_form": "저 예전에 실반지 사고싶었는데",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037808.1.7",
          "form": "실반지도 하나있어야죠",
          "original_form": "실반지도 하나있어야죠",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037808.1.8",
          "form": "반지는 끼면 빼고싶어져서 실패했어요 ㅋㅋㅋㅋ",
          "original_form": "반지는 끼면 빼고싶어져서 실패했어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037808.1.9",
          "form": "엄지에,,,",
          "original_form": "엄지에,,,",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037808.1.10",
          "form": "엄지에 실반지 검지에 두꺼운반지",
