      "utterance": [
        {
          "id": "MDRW2100031034.1.1",
          "form": "가격이 한봉지에 1400원정도",
          "original_form": "가격이 한봉지에 1400원정도",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100031034.1.2",
          "form": "헉 저렴하다",
          "original_form": "헉 저렴하다",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100031034.1.3",
          "form": "다음에 나도 먹어봐야지",
          "original_form": "다음에 나도 먹어봐야지",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100031034.1.4",
          "form": "간식도 먹었니",
          "original_form": "간식도 먹었니",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100031034.1.5",
          "form": "한끼대용으로 먹기 좋더라",
          "original_form": "한끼대용으로 먹기 좋더라",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100031034.1.6",
          "form": "간식은 초코과자 먹었어",
          "original_form": "간식은 초코과자 먹었어",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100031034.1.7",
          "form": "맛나겠다 초코쿠키?",
          "original_form": "맛나겠다 초코쿠키?",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100031034.1.8",
          "form": "너는 점심으로 뭐먹었니",
          "original_form": "너는 점심으로 뭐먹었니",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100031034.1.9",
          "form": "응 초코과자랑 우유",
          "original_form": "응 초코과자랑 우유",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100031034.1.10",
          "form": "맛있겠다 칙촉먹었니",
          "original_form": "맛있겠다 칙촉먹었니",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031034.1.11",
          "form": "아니ABC초콜릿이",
          "original_form": "아니ABC초콜릿이",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031034.1.12",
          "form": "과자로 나오더라",
          "original_form": "과자로 나오더라",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031034.1.13",
          "form": "헐 신기하다ㅋㅋㅋㅋ별게 다 나오네",
          "original_form": "헐 신기하다ㅋㅋㅋㅋ별게 다 나오네",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031034.1.14",
          "form": "점심 뭐 먹었는지 맞춰볼래?",
          "original_form": "점심 뭐 먹었는지 맞춰볼래?",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031034.1.15",
          "form": "그래 좋아",
          "original_form": "그래 좋아",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100031034.1.16",
          "form": "뭐든 물어봐",
          "original_form": "뭐든 물어봐",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100031034.1.17",
          "form": "매운거 먹었니",
          "original_form": "매운거 먹었니",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100031034.1.18",
          "form": "아니 안 매운거였어!",
          "original_form": "아니 안 매운거였어!",
