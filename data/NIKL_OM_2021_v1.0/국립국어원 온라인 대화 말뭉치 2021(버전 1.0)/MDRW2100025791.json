      "utterance": [
        {
          "id": "MDRW2100025791.1.1",
          "form": "오 책 좋아하세요?",
          "original_form": "오 책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100025791.1.2",
          "form": "책! 은 읽고싶은데",
          "original_form": "책! 은 읽고싶은데",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100025791.1.3",
          "form": "독서를너무안해서막막하네요ㅜㅜ",
          "original_form": "독서를너무안해서막막하네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100025791.1.4",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100025791.1.5",
          "form": "ㅋㅋㅋㅋㅋ 최근에 책사신거없어용",
          "original_form": "ㅋㅋㅋㅋㅋ 최근에 책사신거없어용",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100025791.1.6",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100025791.1.7",
          "form": "최근에는 영 만화책만 산거같아요ㅜㅜㅋㅋㅋ",
          "original_form": "최근에는 영 만화책만 산거같아요ㅜㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100025791.1.8",
          "form": "가을되니까 시집같는거 읽고싶어요!",
          "original_form": "가을되니까 시집같는거 읽고싶어요!",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100025791.1.9",
          "form": "오 만화책 뭐요?",
          "original_form": "오 만화책 뭐요?",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100025791.1.10",
          "form": "요즘은 웹툰도 다 책으로 나오더라구욥",
          "original_form": "요즘은 웹툰도 다 책으로 나오더라구욥",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100025791.1.11",
          "form": "저도 사려구요 !",
          "original_form": "저도 사려구요 !",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100025791.1.12",
          "form": "일본만화를 주로 샀네요ㅜㅜ",
          "original_form": "일본만화를 주로 샀네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100025791.1.13",
          "form": "웹툰! 웹툰은 허5파6님것만 가지고있어요!",
          "original_form": "웹툰! 웹툰은 허5파6님것만 가지고있어요!",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100025791.1.14",
          "form": "헐 그분 .. 그 뭐죠",
          "original_form": "헐 그분 .. 그 뭐죠",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100025791.1.15",
          "form": "최근에 한 웹툰",
          "original_form": "최근에 한 웹툰",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100025791.1.16",
          "form": "엄청 잘봣는데 !",
          "original_form": "엄청 잘봣는데 !",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100025791.1.17",
          "form": "죽음에 대하여 보셨나요?",
          "original_form": "죽음에 대하여 보셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100025791.1.18",
          "form": "최근작이 오라존미?였나요",
          "original_form": "최근작이 오라존미?였나요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100025791.1.19",
          "form": "오 아니요!",
          "original_form": "오 아니요!",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100025791.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100025791.1.21",
          "form": "오라존미요 !",
          "original_form": "오라존미요 !",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100025791.1.22",
          "form": "재밌게 봐썽요",
          "original_form": "재밌게 봐썽요",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100025791.1.23",
          "form": "죽음에 대하여는 어떤 작가님 책인가요?",
          "original_form": "죽음에 대하여는 어떤 작가님 책인가요?",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100025791.1.24",
          "form": "시니 현호요 !",
          "original_form": "시니 현호요 !",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100025791.1.25",
          "form": "네이버에 무료면 한번보세요",
          "original_form": "네이버에 무료면 한번보세요",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100025791.1.26",
          "form": "인생웹툰이에용 ㅎㅎ",
          "original_form": "인생웹툰이에용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100025791.1.27",
          "form": "오 추천감사합니다!",
          "original_form": "오 추천감사합니다!",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100025791.1.28",
          "form": "허5파6",
          "original_form": "허5파6",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100025791.1.29",
          "form": "허5파6 님꺼는 다른거 추천해주실거있나요",
          "original_form": "허5파6 님꺼는 다른거 추천해주실거있나요",
