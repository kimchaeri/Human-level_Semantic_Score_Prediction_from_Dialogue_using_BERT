      "utterance": [
        {
          "id": "MDRW2100003348.1.1",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210820 22:02"
        },
        {
          "id": "MDRW2100003348.1.2",
          "form": "네안녕요",
          "original_form": "네안녕요",
          "speaker_id": "2",
          "time": "20210820 22:06"
        },
        {
          "id": "MDRW2100003348.1.3",
          "form": "혹시 반려동물 키우시나요?",
          "original_form": "혹시 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003348.1.4",
          "form": "아니요님은요",
          "original_form": "아니요님은요",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003348.1.5",
          "form": "반려동물 이라하긴 묘하지만 물고기 키우네여 ㅎ",
          "original_form": "반려동물 이라하긴 묘하지만 물고기 키우네여 ㅎ",
          "speaker_id": "1",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003348.1.6",
          "form": "웅느ㅏ!!!!신기하네요",
          "original_form": "웅느ㅏ!!!!신기하네요",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003348.1.7",
          "form": "열대어종류인가요",
          "original_form": "열대어종류인가요",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003348.1.8",
          "form": "네 열대어에요.",
          "original_form": "네 열대어에요.",
          "speaker_id": "1",
          "time": "20210820 22:12"
        },
        {
          "id": "MDRW2100003348.1.9",
          "form": "구피라고 작은 물고기 인대 관리도 편해서 좋내요",
          "original_form": "구피라고 작은 물고기 인대 관리도 편해서 좋내요",
          "speaker_id": "1",
          "time": "20210820 22:12"
        },
        {
          "id": "MDRW2100003348.1.10",
          "form": "우와들어봤어요",
          "original_form": "우와들어봤어요",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003348.1.11",
          "form": "이름도있나요",
          "original_form": "이름도있나요",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003348.1.12",
          "form": "이름은 없내요.. 20마리 넘개 키우다보니..ㅠ\r\n이름 붙여주기가 힘드내요.",
          "original_form": "이름은 없내요.. 20마리 넘개 키우다보니..ㅠ\r\n이름 붙여주기가 힘드내요.",
          "speaker_id": "1",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003348.1.13",
          "form": "키우고 싶으신 동물 있으새요?",
          "original_form": "키우고 싶으신 동물 있으새요?",
          "speaker_id": "1",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003348.1.14",
          "form": "저는 토끼요",
          "original_form": "저는 토끼요",
          "speaker_id": "2",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003348.1.15",
          "form": "이름지어주세요ㅋㅋㅋ",
          "original_form": "이름지어주세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003348.1.16",
          "form": "깡충이 어떠세요 ? ㅋㅋ",
          "original_form": "깡충이 어떠세요 ? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:34"
        },
        {
          "id": "MDRW2100003348.1.17",
          "form": "이상해요",
          "original_form": "이상해요",
          "speaker_id": "2",
          "time": "20210820 22:40"
        },
        {
          "id": "MDRW2100003348.1.18",
