      "utterance": [
        {
          "id": "MDRW2100001861.1.1",
          "form": "여행 좋아하세요???",
          "original_form": "여행 좋아하세요???",
          "speaker_id": "1",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001861.1.2",
          "form": "당근이죠!!!!",
          "original_form": "당근이죠!!!!",
          "speaker_id": "2",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001861.1.3",
          "form": "가본 여행지 중 가장 좋았던 곳은 어딘가요?",
          "original_form": "가본 여행지 중 가장 좋았던 곳은 어딘가요?",
          "speaker_id": "1",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001861.1.4",
          "form": "저어.. 딱히 많이 안가봐서 ㅠㅠ",
          "original_form": "저어.. 딱히 많이 안가봐서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001861.1.5",
          "form": "제주도?",
          "original_form": "제주도?",
          "speaker_id": "2",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001861.1.6",
          "form": "1달전에 방학하고 갔어요!",
          "original_form": "1달전에 방학하고 갔어요!",
          "speaker_id": "2",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001861.1.7",
          "form": "친구들이랑",
          "original_form": "친구들이랑",
          "speaker_id": "2",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001861.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ 이제 성인 되셨으니 그럴 수 있죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 이제 성인 되셨으니 그럴 수 있죠",
          "speaker_id": "1",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001861.1.9",
          "form": "저는 학교다닐때 제주도로 국토대장정 갔었어요",
          "original_form": "저는 학교다닐때 제주도로 국토대장정 갔었어요",
          "speaker_id": "1",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001861.1.10",
          "form": "와 부럽다..",
          "original_form": "와 부럽다..",
          "speaker_id": "2",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.11",
          "form": "다들 예쁜 옷에 예쁜 사진찍는데",
          "original_form": "다들 예쁜 옷에 예쁜 사진찍는데",
          "speaker_id": "1",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.12",
          "form": "저는 냉장고바지입고 돌아다녔어요 300키로 걷기",
          "original_form": "저는 냉장고바지입고 돌아다녔어요 300키로 걷기",
          "speaker_id": "1",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.13",
          "form": "한라산도 가셧어요?",
          "original_form": "한라산도 가셧어요?",
          "speaker_id": "1",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.14",
          "form": "저희는 학교다닐땨 대학교만 갔는데",
          "original_form": "저희는 학교다닐땨 대학교만 갔는데",
          "speaker_id": "2",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.16",
          "form": "네 !",
          "original_form": "네 !",
          "speaker_id": "2",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.17",
          "form": "한라산 갔어요!!",
          "original_form": "한라산 갔어요!!",
          "speaker_id": "2",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.18",
          "form": "와대박",
          "original_form": "와대박",
          "speaker_id": "1",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.19",
          "form": "힘들지 않았어요?",
          "original_form": "힘들지 않았어요?",
          "speaker_id": "1",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.20",
          "form": "정상까지는 못갔지만..",
          "original_form": "정상까지는 못갔지만..",
          "speaker_id": "2",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.21",
          "form": "늦게 가서..",
          "original_form": "늦게 가서..",
          "speaker_id": "2",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001861.1.22",
          "form": "절반 오르고 1시부터는 정상 못올라사게 하더라구요 ㅠㅠ",
          "original_form": "절반 오르고 1시부터는 정상 못올라사게 하더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001861.1.23",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001861.1.24",
          "form": "저도 6시엔가 부터 올라갔거든요",
          "original_form": "저도 6시엔가 부터 올라갔거든요",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001861.1.25",
          "form": "조장 오빠랑 대판 싸울뻔 계단 겁나 많고",
          "original_form": "조장 오빠랑 대판 싸울뻔 계단 겁나 많고",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001861.1.26",
          "form": "전 올라가기 싫었는데 같이 가야된다고 하도 그래서 울면서 올라갔어요",
          "original_form": "전 올라가기 싫었는데 같이 가야된다고 하도 그래서 울면서 올라갔어요",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001861.1.27",
          "form": "아침 6시요?",
          "original_form": "아침 6시요?",
          "speaker_id": "2",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001861.1.28",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001861.1.29",
          "form": "... 짜증나겠다",
          "original_form": "... 짜증나겠다",
          "speaker_id": "2",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001861.1.30",
          "form": "ㅋㅋㅋㅋㅋ계단 진짜 많아요 진짜 근데 뿌듯하긴 하더라구요",
          "original_form": "ㅋㅋㅋㅋㅋ계단 진짜 많아요 진짜 근데 뿌듯하긴 하더라구요",
          "speaker_id": "1",
          "time": "20210819 21:59"
        }
      ]
    }
