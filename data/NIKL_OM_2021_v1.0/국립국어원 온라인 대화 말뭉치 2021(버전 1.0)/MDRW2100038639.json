        {
          "id": "MDRW2100038639.1.1",
          "form": "머사셨나요또",
          "original_form": "머사셨나요또",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100038639.1.2",
          "form": "지플립3 갖고 싶어요",
          "original_form": "지플립3 갖고 싶어요",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100038639.1.3",
          "form": "저도...",
          "original_form": "저도...",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100038639.1.4",
          "form": "비싸자나요",
          "original_form": "비싸자나요",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100038639.1.5",
          "form": "근데 이쁘더라구요",
          "original_form": "근데 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100038639.1.6",
          "form": "콜라보한 케이스들 어제 첨봤는데 이쁘더라구요",
          "original_form": "콜라보한 케이스들 어제 첨봤는데 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100038639.1.7",
          "form": "아그져 ㄹㅇㄹㅇ",
          "original_form": "아그져 ㄹㅇㄹㅇ",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100038639.1.8",
          "form": "ㅜㅜ넘탐나요",
          "original_form": "ㅜㅜ넘탐나요",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100038639.1.9",
          "form": "노티드도넛 콜라보 짱 귀엽더라구요",
          "original_form": "노티드도넛 콜라보 짱 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100038639.1.10",
          "form": "아 ㄹㅇㄹㅇ ㅜㅜ",
          "original_form": "아 ㄹㅇㄹㅇ ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100038639.1.11",
          "form": "근데 그거 래미안 이런거도 잇대요 ㅋㅋㅋ",
          "original_form": "근데 그거 래미안 이런거도 잇대요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100038639.1.12",
          "form": "이마트24랑ㅋㅋㅋㅋ",
          "original_form": "이마트24랑ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100038639.1.13",
          "form": "별게다잇어",
          "original_form": "별게다잇어",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100038639.1.14",
          "form": "래미안 궁금하네욬ㅋㅋㅋㅋ",
          "original_form": "래미안 궁금하네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100038639.1.15",
          "form": "플립 진짜 초인기인듯",
          "original_form": "플립 진짜 초인기인듯",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100038639.1.16",
          "form": "여기 랩에만 예약구매자 두명",
          "original_form": "여기 랩에만 예약구매자 두명",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100038639.1.17",
          "form": "한명은 평생 아이폰이엇음",
          "original_form": "한명은 평생 아이폰이엇음",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100038639.1.18",
          "form": "와 진짜요?? ㅋㅋㅋ신기하다 ㅋㅋㅋ진짜 지금 다 품절이라면서요",
          "original_form": "와 진짜요?? ㅋㅋㅋ신기하다 ㅋㅋㅋ진짜 지금 다 품절이라면서요",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100038639.1.19",
          "form": "그죠...인기초인기",
          "original_form": "그죠...인기초인기",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100038639.1.20",
          "form": "ㅎㅎ 다들 돈만타",
          "original_form": "ㅎㅎ 다들 돈만타",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100038639.1.21",
          "form": "그르게나 말이에요",
          "original_form": "그르게나 말이에요",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100038639.1.22",
          "form": "갤럭시 워치도 인기라고하고",
          "original_form": "갤럭시 워치도 인기라고하고",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100038639.1.23",
          "form": "저도 그거 알아봣거등요 워치...",
          "original_form": "저도 그거 알아봣거등요 워치...",
          "speaker_id": "2",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100038639.1.24",
          "form": "인바디도되고 가격도 합리적",
          "original_form": "인바디도되고 가격도 합리적",
          "speaker_id": "2",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100038639.1.25",
          "form": "인바디 되는거 넘 신기해요",
          "original_form": "인바디 되는거 넘 신기해요",
          "speaker_id": "1",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100038639.1.26",
          "form": "그죠 ㅋㅋ 기술력 참",
