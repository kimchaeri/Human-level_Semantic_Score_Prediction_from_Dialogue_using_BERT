      "utterance": [
        {
          "id": "MDRW2100033686.1.1",
          "form": "티비 뭐하는데",
          "original_form": "티비 뭐하는데",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100033686.1.2",
          "form": "아빠가 티비조선 뉴스본다ㅋ",
          "original_form": "아빠가 티비조선 뉴스본다ㅋ",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100033686.1.3",
          "form": "난 형부가 중국 드라마본다",
          "original_form": "난 형부가 중국 드라마본다",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100033686.1.4",
          "form": "ㅋㅋ 중국 드라마 좋아하나보네",
          "original_form": "ㅋㅋ 중국 드라마 좋아하나보네",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100033686.1.5",
          "form": "갑자기 보기 시작한다",
          "original_form": "갑자기 보기 시작한다",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100033686.1.6",
          "form": "갑자기?  웃기네",
          "original_form": "갑자기?  웃기네",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100033686.1.7",
          "form": "엉 좀 그런편이다",
          "original_form": "엉 좀 그런편이다",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100033686.1.8",
          "form": "즉흥적인 편이시네. 우리 아빠는 한결같이 티비조선",
          "original_form": "즉흥적인 편이시네. 우리 아빠는 한결같이 티비조선",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100033686.1.9",
          "form": "내가 손홍민다큐보고 있는데 그거 재밌냐하더니 어느순간 손홍민 외국 경기 보고 ㅋㅋ",
          "original_form": "내가 손홍민다큐보고 있는데 그거 재밌냐하더니 어느순간 손홍민 외국 경기 보고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100033686.1.10",
          "form": "따라쟁이인 건가? ㅋㅋ",
          "original_form": "따라쟁이인 건가? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100033686.1.11",
          "form": "인생의 모든게 즉흥적이다",
          "original_form": "인생의 모든게 즉흥적이다",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100033686.1.12",
          "form": "티비 조선은 중립적이가?",
          "original_form": "티비 조선은 중립적이가?",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100033686.1.13",
          "form": "아니지. 보수적이지. 편향적이다",
          "original_form": "아니지. 보수적이지. 편향적이다",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100033686.1.14",
          "form": "그렇구나 난 중립적으로 봐주는 시각이 필요하다",
          "original_form": "그렇구나 난 중립적으로 봐주는 시각이 필요하다",
          "speaker_id": "2",
          "time": "20210825 21:26"
        }
