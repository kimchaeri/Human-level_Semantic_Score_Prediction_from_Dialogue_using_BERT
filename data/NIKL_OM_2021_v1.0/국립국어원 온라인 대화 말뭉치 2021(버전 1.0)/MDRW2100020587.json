      "utterance": [
        {
          "id": "MDRW2100020587.1.1",
          "form": "커피 좋아해요 ㅎㅎ",
          "original_form": "커피 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020587.1.2",
          "form": "집에 커피머신도 있습니다 v",
          "original_form": "집에 커피머신도 있습니다 v",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020587.1.3",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020587.1.4",
          "form": "어디꺼에요?",
          "original_form": "어디꺼에요?",
          "speaker_id": "1",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020587.1.5",
          "form": "저는 커피 잘 못마시는데 커피 잘마시는분들 보면 우아해보여요",
          "original_form": "저는 커피 잘 못마시는데 커피 잘마시는분들 보면 우아해보여요",
          "speaker_id": "1",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020587.1.6",
          "form": "드롱기 전자동 커피머신 구매해서 잘쓰고있어요 ㅎㅎ",
          "original_form": "드롱기 전자동 커피머신 구매해서 잘쓰고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020587.1.7",
          "form": "아메리카노를 가족 다같이 잘 마시니까 유용하더라구요",
          "original_form": "아메리카노를 가족 다같이 잘 마시니까 유용하더라구요",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020587.1.8",
          "form": "우와 가족분들도 다 커피 잘드시나봐요",
          "original_form": "우와 가족분들도 다 커피 잘드시나봐요",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020587.1.9",
          "form": "아아를 제일 좋아하세요?",
          "original_form": "아아를 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020587.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020587.1.11",
          "form": "라떼도 좋아하는데 아아가 제일 좋죠 ㅎㅎ",
          "original_form": "라떼도 좋아하는데 아아가 제일 좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020587.1.12",
          "form": "얼죽아 회원이랄까요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "얼죽아 회원이랄까요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020587.1.13",
          "form": "오홍ㅋㅋㅋ",
          "original_form": "오홍ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020587.1.14",
          "form": "얼죽아 좋죠ㅋㅋㅋ",
          "original_form": "얼죽아 좋죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020587.1.15",
          "form": "아아 시럽없이는 못마셔서 슬프네요",
          "original_form": "아아 시럽없이는 못마셔서 슬프네요",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020587.1.16",
          "form": "아아 시럽넣어서는 안마셔봤는데",
          "original_form": "아아 시럽넣어서는 안마셔봤는데",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020587.1.17",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020587.1.18",
          "form": "많이들 헤이즐넛시럽 넣어먹던데",
          "original_form": "많이들 헤이즐넛시럽 넣어먹던데",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020587.1.19",
          "form": "씁쓸한 맛이",
          "original_form": "씁쓸한 맛이",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020587.1.20",
          "form": "어떤 시럽 넣어드세요?",
          "original_form": "어떤 시럽 넣어드세요?",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020587.1.21",
          "form": "많이 사라지고",
          "original_form": "많이 사라지고",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020587.1.22",
          "form": "부드러워요",
          "original_form": "부드러워요",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020587.1.23",
          "form": "저 그냥 스벅가면 있는",
          "original_form": "저 그냥 스벅가면 있는",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020587.1.24",
          "form": "시럽 넣는데 이게 무슨 시럽인지",
          "original_form": "시럽 넣는데 이게 무슨 시럽인지",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020587.1.25",
          "form": "모르겠네요ㅠㅠ",
          "original_form": "모르겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020587.1.26",
          "form": "ㅇㅎ 씁쓸한 맛이 사라지는군요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅎ 씁쓸한 맛이 사라지는군요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020587.1.27",
          "form": "아마 기본 카페시럽일 것 같아요!",
          "original_form": "아마 기본 카페시럽일 것 같아요!",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020587.1.28",
          "form": "다음에 쓴맛 먹기 싫어질때 도전해보겠습니다 ㅋㅋㅋ",
          "original_form": "다음에 쓴맛 먹기 싫어질때 도전해보겠습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020587.1.29",
          "form": "꿀팁 감사합니다",
          "original_form": "꿀팁 감사합니다",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020587.1.30",
          "form": "가장 좋아하시는",
          "original_form": "가장 좋아하시는",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020587.1.31",
          "form": "커피 브랜드는 무엇인가용?",
          "original_form": "커피 브랜드는 무엇인가용?",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020587.1.32",
          "form": "아아 잘 내린다 하는곳 있으세요?",
          "original_form": "아아 잘 내린다 하는곳 있으세요?",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020587.1.33",
          "form": "저 스타벅스 원두 사먹어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 스타벅스 원두 사먹어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020587.1.34",
          "form": "역시 스벅인가요?ㅋㅋㅋ",
          "original_form": "역시 스벅인가요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020587.1.35",
          "form": "저희 엄마도 스타벅스만 드시던데",
          "original_form": "저희 엄마도 스타벅스만 드시던데",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020587.1.36",
          "form": "개인적으로 라떼는 폴바셋! 인데",
          "original_form": "개인적으로 라떼는 폴바셋! 인데",
          "speaker_id": "2",
          "time": "20210828 10:18"
        }
      ]
