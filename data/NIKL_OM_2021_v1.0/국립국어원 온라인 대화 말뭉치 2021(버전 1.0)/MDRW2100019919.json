      "utterance": [
        {
          "id": "MDRW2100019919.1.1",
          "form": "요즘 무슨책을 그렇게 많이 읽어?",
          "original_form": "요즘 무슨책을 그렇게 많이 읽어?",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019919.1.2",
          "form": "나 나이먹었나봐",
          "original_form": "나 나이먹었나봐",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019919.1.3",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019919.1.4",
          "form": "ㅋㅋ 달달한 책이좋네~~~",
          "original_form": "ㅋㅋ 달달한 책이좋네~~~",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019919.1.5",
          "form": "로맨스소설읽어",
          "original_form": "로맨스소설읽어",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019919.1.6",
          "form": "헐~~ 그런게 재미있냐?",
          "original_form": "헐~~ 그런게 재미있냐?",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019919.1.7",
          "form": "그럼 내가하지 못하는거 대리만족이라고나 할까~~",
          "original_form": "그럼 내가하지 못하는거 대리만족이라고나 할까~~",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019919.1.8",
          "form": "그래 그런거라도 읽으니 그냥있는것보다는 낫겠지",
          "original_form": "그래 그런거라도 읽으니 그냥있는것보다는 낫겠지",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019919.1.9",
          "form": "한동안 주식책읽었는데 요즘 지수 넘빠져서 쉬는타이밍이야.",
          "original_form": "한동안 주식책읽었는데 요즘 지수 넘빠져서 쉬는타이밍이야.",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019919.1.10",
          "form": "그래 그렇게라도 손해를 잊어버려!",
          "original_form": "그래 그렇게라도 손해를 잊어버려!",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019919.1.11",
          "form": "응. 조만간 다시 책안읽음 주식으로 돌아가야지",
          "original_form": "응. 조만간 다시 책안읽음 주식으로 돌아가야지",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019919.1.12",
          "form": "지금은 쫌 잊을 필요가있어",
          "original_form": "지금은 쫌 잊을 필요가있어",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019919.1.13",
          "form": "그래.   너하고싶은거 다해",
          "original_form": "그래.   너하고싶은거 다해",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019919.1.14",
          "form": "고마워~~ 난 쫌이따 달달한책 읽으러 갈꺼야~~~",
          "original_form": "고마워~~ 난 쫌이따 달달한책 읽으러 갈꺼야~~~",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019919.1.15",
          "form": "그래. 잠좀 일찍자고",
          "original_form": "그래. 잠좀 일찍자고",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019919.1.16",
          "form": "오늘 도서관에서 책 엄청 많이 빌려왔어",
          "original_form": "오늘 도서관에서 책 엄청 많이 빌려왔어",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019919.1.17",
          "form": "빨리보고 반납해야지~~",
          "original_form": "빨리보고 반납해야지~~",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019919.1.18",
          "form": "대단하다.  공부를 그렇게좀 하지그랬어",
          "original_form": "대단하다.  공부를 그렇게좀 하지그랬어",
          "speaker_id": "1",
          "time": "20210827 22:56"
        }
