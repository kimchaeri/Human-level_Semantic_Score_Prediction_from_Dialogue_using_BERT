      "utterance": [
        {
          "id": "MDRW2100013210.1.1",
          "form": "유튜브 무서운 채널은 뭐에요?",
          "original_form": "유튜브 무서운 채널은 뭐에요?",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013210.1.2",
          "form": "돌비랑",
          "original_form": "돌비랑",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013210.1.3",
          "form": "코비엣티비요",
          "original_form": "코비엣티비요",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013210.1.4",
          "form": "무서워요?",
          "original_form": "무서워요?",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013210.1.5",
          "form": "그냥 그래요",
          "original_form": "그냥 그래요",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013210.1.6",
          "form": "코비엣은 아예 안 무서워서",
          "original_form": "코비엣은 아예 안 무서워서",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013210.1.7",
          "form": "그냥 라디오 ㅋㅋ",
          "original_form": "그냥 라디오 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013210.1.8",
          "form": "아 저 옛날에 쌈무이 라디오 들었었어요",
          "original_form": "아 저 옛날에 쌈무이 라디오 들었었어요",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013210.1.9",
          "form": "엇 쌈무이",
          "original_form": "엇 쌈무이",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013210.1.10",
          "form": "저도 알아요 ㅋㅋ",
          "original_form": "저도 알아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013210.1.11",
          "form": "저 많이 들었어요",
          "original_form": "저 많이 들었어요",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013210.1.12",
          "form": "쌈무이도 별로 안무서운 ㅎㅎ",
          "original_form": "쌈무이도 별로 안무서운 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013210.1.13",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013210.1.14",
          "form": "너무 이미 다 아는 얘기들뿐이고 ㅋㅋ",
          "original_form": "너무 이미 다 아는 얘기들뿐이고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013210.1.15",
          "form": "네 그냥 심심할때 백색소음 정도로 ㅎ",
          "original_form": "네 그냥 심심할때 백색소음 정도로 ㅎ",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013210.1.16",
          "form": "네 ㅋㅋㅋ 그러다 끊었어요",
          "original_form": "네 ㅋㅋㅋ 그러다 끊었어요",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013210.1.17",
          "form": "돌비는 좀 새롭긴 한데",
          "original_form": "돌비는 좀 새롭긴 한데",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013210.1.18",
          "form": "진행을 너무 못해요",
          "original_form": "진행을 너무 못해요",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013210.1.19",
          "form": "여자에요?",
          "original_form": "여자에요?",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013210.1.20",
          "form": "아니요 남자요",
          "original_form": "아니요 남자요",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013210.1.21",
          "form": "그냥.. 볼거 없어서 봐요 ㅋㅋ",
          "original_form": "그냥.. 볼거 없어서 봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013210.1.22",
          "form": "네 컨텐츠가 많지 않으니..ㅎ",
          "original_form": "네 컨텐츠가 많지 않으니..ㅎ",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013210.1.23",
          "form": "오 오늘은 심야괴담회에 실화가 나오네요??",
          "original_form": "오 오늘은 심야괴담회에 실화가 나오네요??",
