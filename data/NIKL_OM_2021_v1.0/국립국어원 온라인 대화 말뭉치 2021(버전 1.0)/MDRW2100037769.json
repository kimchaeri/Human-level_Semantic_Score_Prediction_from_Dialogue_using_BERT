      "utterance": [
        {
          "id": "MDRW2100037769.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037769.1.2",
          "form": "비가엄청와요! 날씨어떠신가요",
          "original_form": "비가엄청와요! 날씨어떠신가요",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037769.1.3",
          "form": "여기는 비가 오다가말고 오다말고 그래요 ㅋㅋㅋㅋ",
          "original_form": "여기는 비가 오다가말고 오다말고 그래요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037769.1.4",
          "form": "날씨가 되게 습하네요 ㅠㅠ",
          "original_form": "날씨가 되게 습하네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037769.1.5",
          "form": "비가 엄청나게 왔습니다 천둥번개도 치구요 ㅜ",
          "original_form": "비가 엄청나게 왔습니다 천둥번개도 치구요 ㅜ",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100037769.1.6",
          "form": "아 엄청오나봐요 춥지는 않으세요?",
          "original_form": "아 엄청오나봐요 춥지는 않으세요?",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037769.1.7",
          "form": "바람이 꽤나 쌀쌀해졌어요 가을은 가을인가봐요 벌레도 많이 울어요 ㅋㅋ",
          "original_form": "바람이 꽤나 쌀쌀해졌어요 가을은 가을인가봐요 벌레도 많이 울어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037769.1.8",
          "form": "그쵸 바람이 꽤 시원해졌더라구요",
          "original_form": "그쵸 바람이 꽤 시원해졌더라구요",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100037769.1.9",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100037769.1.10",
          "form": "날씨만보자면 봄가을이 좋은데 알러지때문에 여름이요... ㅜㅜ",
          "original_form": "날씨만보자면 봄가을이 좋은데 알러지때문에 여름이요... ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100037769.1.11",
          "form": "아 혹시 꽃가루 알러지인가요??ㅠ",
          "original_form": "아 혹시 꽃가루 알러지인가요??ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037769.1.12",
          "form": "네 꽃가루 알러지때문에 봄가을은 괴로워요 ㅜㅜ 그래도 마스크끼니 확실히 덜해요",
          "original_form": "네 꽃가루 알러지때문에 봄가을은 괴로워요 ㅜㅜ 그래도 마스크끼니 확실히 덜해요",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100037769.1.13",
          "form": "아 힘들겟네요 ㅠㅠ",
          "original_form": "아 힘들겟네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100037769.1.14",
          "form": "오늘도 또 비가 오네요 ㅋㅋㅋ 장마도 아니고 ㅋㅋ",
          "original_form": "오늘도 또 비가 오네요 ㅋㅋㅋ 장마도 아니고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100037769.1.15",
          "form": "그쵸 여기두요 ㅠㅠ 아침엔 밝더니만요",
          "original_form": "그쵸 여기두요 ㅠㅠ 아침엔 밝더니만요",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100037769.1.16",
          "form": "날씨때문에 빨래말리기가 애매해요 ㅋㅋㅋㅋ",
          "original_form": "날씨때문에 빨래말리기가 애매해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100037769.1.17",
          "form": "건조기가 필요한 날씨에요 ㅜㅜ",
          "original_form": "건조기가 필요한 날씨에요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100037769.1.18",
          "form": "맞아요 날씨가 워낙 왓다갓다거려서 ㅠㅠ",
          "original_form": "맞아요 날씨가 워낙 왓다갓다거려서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:49"
        }
