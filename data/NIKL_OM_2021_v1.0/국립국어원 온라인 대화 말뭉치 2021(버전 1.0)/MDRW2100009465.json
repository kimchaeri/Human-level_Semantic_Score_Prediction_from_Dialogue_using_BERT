        },
        {
          "id": "MDRW2100009465.1.2",
          "form": "좋아하는 계절이 어떻게 되세요?",
          "original_form": "좋아하는 계절이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009465.1.3",
          "form": "일편단심 겨울이요~ 님은요?",
          "original_form": "일편단심 겨울이요~ 님은요?",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009465.1.4",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009465.1.5",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009465.1.6",
          "form": "더위에 취약해서요:)",
          "original_form": "더위에 취약해서요:)",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009465.1.7",
          "form": "겨울 왜 좋아하세요?",
          "original_form": "겨울 왜 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009465.1.8",
          "form": "같은 이유요",
          "original_form": "같은 이유요",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009465.1.9",
          "form": "눈 오는 것도 좋구요",
          "original_form": "눈 오는 것도 좋구요",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009465.1.10",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009465.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009465.1.12",
          "form": "빨리 겨울 옷 입고 싶어요",
          "original_form": "빨리 겨울 옷 입고 싶어요",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009465.1.13",
          "form": "눈도 좋아해요",
          "original_form": "눈도 좋아해요",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009465.1.14",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009465.1.15",
          "form": "여름 옷은 그다지 기대가 되지 않아요",
          "original_form": "여름 옷은 그다지 기대가 되지 않아요",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009465.1.16",
          "form": "옷도 그렇고..여름 자체가 기대가 되지 않아요",
          "original_form": "옷도 그렇고..여름 자체가 기대가 되지 않아요",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009465.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009465.1.18",
          "form": "너무 덥고",
          "original_form": "너무 덥고",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009465.1.19",
          "form": "벌레도 득실거리고.....",
          "original_form": "벌레도 득실거리고.....",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009465.1.20",
          "form": "평생 경기북부나 강원도 살다가",
          "original_form": "평생 경기북부나 강원도 살다가",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009465.1.21",
          "form": "경남에서 몇년째 살게 됐는데",
          "original_form": "경남에서 몇년째 살게 됐는데",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009465.1.22",
          "form": "여름이 더 싫어졌어요",
          "original_form": "여름이 더 싫어졌어요",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009465.1.23",
          "form": "특히 햇빛 세죠..매 해 점점 따가워지는 것 같아요",
          "original_form": "특히 햇빛 세죠..매 해 점점 따가워지는 것 같아요",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009465.1.24",
          "form": "진짜 그런 것 같아요",
          "original_form": "진짜 그런 것 같아요",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009465.1.25",
          "form": "이번달이 특히 햇빛이 셌어요",
          "original_form": "이번달이 특히 햇빛이 셌어요",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009465.1.26",
          "form": "더워서 모기도 줄었다고 하더라고요",
          "original_form": "더워서 모기도 줄었다고 하더라고요",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009465.1.27",
          "form": "그러게요 확실히 해가 갈수록 기후가 달라지는 게 보여요",
          "original_form": "그러게요 확실히 해가 갈수록 기후가 달라지는 게 보여요",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009465.1.28",
          "form": "모기가 줄어들 정도면 휴",
          "original_form": "모기가 줄어들 정도면 휴",
          "speaker_id": "2",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009465.1.29",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009465.1.30",
          "form": "전에는 사과를 경북에서 재배했는데",
          "original_form": "전에는 사과를 경북에서 재배했는데",
          "speaker_id": "1",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009465.1.31",
          "form": "몇년 전부터는 강원까지 북상했대요",
          "original_form": "몇년 전부터는 강원까지 북상했대요",
          "speaker_id": "1",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009465.1.32",
          "form": "지구온난화가 진짜 심각한 것 같아요",
          "original_form": "지구온난화가 진짜 심각한 것 같아요",
          "speaker_id": "1",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009465.1.33",
          "form": "와.. 그런 기사는 매 해 보게 되네요",
          "original_form": "와.. 그런 기사는 매 해 보게 되네요",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009465.1.34",
          "form": "어디는 열대과일 재배한다는 소식도 있더라구요",
          "original_form": "어디는 열대과일 재배한다는 소식도 있더라구요",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009465.1.35",
          "form": "제주도나 대구에서",
          "original_form": "제주도나 대구에서",
          "speaker_id": "1",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009465.1.36",
          "form": "바나나 재배한대요",
          "original_form": "바나나 재배한대요",
          "speaker_id": "1",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009465.1.37",
          "form": "망고나",
          "original_form": "망고나",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009465.1.38",
          "form": "ㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009465.1.39",
          "form": "겨울이 좋은데",
          "original_form": "겨울이 좋은데",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009465.1.40",
          "form": "눈 좋아하세요?",
          "original_form": "눈 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009465.1.41",
          "form": "눈 너무 좋아요!!!",
          "original_form": "눈 너무 좋아요!!!",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009465.1.42",
          "form": "기후 변화 심해질거.. 차라리 눈 펑펑 왔으면 좋겠어요",
          "original_form": "기후 변화 심해질거.. 차라리 눈 펑펑 왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009465.1.43",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009465.1.44",
          "form": "눈 너무 예뻐요",
          "original_form": "눈 너무 예뻐요",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009465.1.45",
          "form": "아 근데 저희 지역은 이미 충분히 많이 와요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 근데 저희 지역은 이미 충분히 많이 와요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009465.1.46",
          "form": "초등학교 때는 3일간 고립된 적 있어요",
          "original_form": "초등학교 때는 3일간 고립된 적 있어요",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009465.1.47",
          "form": "헐 정말 펑펑 왔나봐요",
          "original_form": "헐 정말 펑펑 왔나봐요",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009465.1.48",
          "form": "차가 못 다닐 정도였나요?",
          "original_form": "차가 못 다닐 정도였나요?",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009465.1.49",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009465.1.50",
          "form": "트럭도 못 다녀서",
          "original_form": "트럭도 못 다녀서",
          "speaker_id": "1",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009465.1.51",
          "form": "국가에서 보낸 제설차가 올 때까지",
          "original_form": "국가에서 보낸 제설차가 올 때까지",
          "speaker_id": "1",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009465.1.52",
          "form": "감자만 먹으며 버텼어요",
          "original_form": "감자만 먹으며 버텼어요",
          "speaker_id": "1",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009465.1.53",
          "form": "심지어 시골이었거든요",
          "original_form": "심지어 시골이었거든요",
          "speaker_id": "1",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009465.1.54",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009465.1.55",
          "form": "와우 날씨 이야기가 굉장히",
          "original_form": "와우 날씨 이야기가 굉장히",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009465.1.56",
          "form": "다이나믹해지는..ㅋㅋ",
          "original_form": "다이나믹해지는..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009465.1.57",
          "form": "제설차가 필요할 정도면",
          "original_form": "제설차가 필요할 정도면",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009465.1.58",
          "form": "눈은 원없이 보셨겠어요!",
          "original_form": "눈은 원없이 보셨겠어요!",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009465.1.59",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009465.1.60",
          "form": "사실 매년 뉴스에 한번은 나와요",
          "original_form": "사실 매년 뉴스에 한번은 나와요",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009465.1.61",
          "form": "그 지역이요",
          "original_form": "그 지역이요",
          "speaker_id": "1",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009465.1.62",
          "form": "폭설로 유명해서요",
          "original_form": "폭설로 유명해서요",
          "speaker_id": "1",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009465.1.63",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009465.1.64",
          "form": "눈 자주 보세요?",
          "original_form": "눈 자주 보세요?",
          "speaker_id": "1",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009465.1.65",
          "form": "오 폭설 하면 생각나는 지역들 있긴 하네요",
          "original_form": "오 폭설 하면 생각나는 지역들 있긴 하네요",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009465.1.66",
          "form": "전 겨울에 추울 때만 봐요ㅜㅜ",
          "original_form": "전 겨울에 추울 때만 봐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009465.1.67",
          "form": "강릉이었어요",
