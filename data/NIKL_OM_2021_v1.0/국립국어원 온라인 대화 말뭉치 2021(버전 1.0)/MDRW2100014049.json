      "utterance": [
        {
          "id": "MDRW2100014049.1.1",
          "form": "저는 칼국수파입니다",
          "original_form": "저는 칼국수파입니다",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014049.1.2",
          "form": "바지락칼국수 크으",
          "original_form": "바지락칼국수 크으",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014049.1.3",
          "form": "크으 바지락칼국수도 마싯죠 ㅠㅠ",
          "original_form": "크으 바지락칼국수도 마싯죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014049.1.4",
          "form": "배고프네요ㅠ",
          "original_form": "배고프네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014049.1.5",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014049.1.6",
          "form": "방금 감바스 먹엇습니다 밀키틐ㅋㅋ",
          "original_form": "방금 감바스 먹엇습니다 밀키틐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014049.1.7",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014049.1.8",
          "form": "술은요?",
          "original_form": "술은요?",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014049.1.9",
          "form": "ㅋㅋㅋㅋ 안먹었어요 운동가야해섴ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 안먹었어요 운동가야해섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014049.1.10",
          "form": "와인같이마심 좋았을텐데",
          "original_form": "와인같이마심 좋았을텐데",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014049.1.11",
          "form": "저녁 언제드시나요",
          "original_form": "저녁 언제드시나요",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014049.1.12",
          "form": "이제 먹으려구요",
          "original_form": "이제 먹으려구요",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014049.1.13",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014049.1.14",
          "form": "헬스장인가요?",
          "original_form": "헬스장인가요?",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014049.1.15",
          "form": "뭐드세요",
          "original_form": "뭐드세요",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014049.1.16",
          "form": "아검도요! ㅋㅋ",
          "original_form": "아검도요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014049.1.17",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014049.1.18",
          "form": "남자분이신가요?",
          "original_form": "남자분이신가요?",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014049.1.19",
          "form": "아뇽 여잔데용 ㅋㅋㅋㅋㅋ",
          "original_form": "아뇽 여잔데용 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014049.1.20",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014049.1.21",
          "form": "검도재밌나요?",
          "original_form": "검도재밌나요?",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014049.1.22",
          "form": "재밋어요! ㅋㅋ시작한지 1주일도안된 왕초보입니다",
          "original_form": "재밋어요! ㅋㅋ시작한지 1주일도안된 왕초보입니다",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014049.1.23",
          "form": "하고오면 식욕당겨요 지금도 아이스크림먹는중",
          "original_form": "하고오면 식욕당겨요 지금도 아이스크림먹는중",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014049.1.24",
          "form": "와우 검도는",
          "original_form": "와우 검도는",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100014049.1.25",
          "form": "얼마쯤하나요?",
          "original_form": "얼마쯤하나요?",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100014049.1.26",
          "form": "아무래도 운동후엔 식욕이 당기겠어요",
          "original_form": "아무래도 운동후엔 식욕이 당기겠어요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100014049.1.27",
          "form": "한시간쯤해용ㅋㅋ먹어서 살은 안빠질듯",
          "original_form": "한시간쯤해용ㅋㅋ먹어서 살은 안빠질듯",
          "speaker_id": "2",
          "time": "20210825 22:06"
        }
      ]
