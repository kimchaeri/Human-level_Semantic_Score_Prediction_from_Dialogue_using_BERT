        },
        {
          "id": "MDRW2100000851.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 20:02"
        },
        {
          "id": "MDRW2100000851.1.3",
          "form": "최근에 여행다녀온적이있으신가요",
          "original_form": "최근에 여행다녀온적이있으신가요",
          "speaker_id": "1",
          "time": "20210819 20:02"
        },
        {
          "id": "MDRW2100000851.1.4",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 20:02"
        },
        {
          "id": "MDRW2100000851.1.5",
          "form": "코로나때문에 여행은  힘든것같아요",
          "original_form": "코로나때문에 여행은  힘든것같아요",
          "speaker_id": "2",
          "time": "20210819 20:03"
        },
        {
          "id": "MDRW2100000851.1.6",
          "form": "그쵸 저도 최근에 여행을 다녀보지않았는대1",
          "original_form": "그쵸 저도 최근에 여행을 다녀보지않았는대1",
          "speaker_id": "1",
          "time": "20210819 20:03"
        },
        {
          "id": "MDRW2100000851.1.7",
          "form": "옛날엔 강원도 통영",
          "original_form": "옛날엔 강원도 통영",
          "speaker_id": "1",
          "time": "20210819 20:03"
        },
        {
          "id": "MDRW2100000851.1.8",
          "form": "자주갔는대 요즘 못가서 너무 아쉬워요",
          "original_form": "자주갔는대 요즘 못가서 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210819 20:03"
        },
        {
          "id": "MDRW2100000851.1.9",
          "form": "그럼 여행 다녔던곳중에 어디가 젤 좋으셨나요",
          "original_form": "그럼 여행 다녔던곳중에 어디가 젤 좋으셨나요",
          "speaker_id": "1",
          "time": "20210819 20:03"
        },
        {
          "id": "MDRW2100000851.1.10",
          "form": "그쵸  통영하니까 꿀빵 생각나네요~",
          "original_form": "그쵸  통영하니까 꿀빵 생각나네요~",
          "speaker_id": "2",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000851.1.11",
          "form": "전 경주요!ㅎㅎ",
          "original_form": "전 경주요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000851.1.12",
          "form": "임신출산육아에 코로나까지껴서 해외여행은 너무 오래되서.ㅠㅠ",
          "original_form": "임신출산육아에 코로나까지껴서 해외여행은 너무 오래되서.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000851.1.13",
          "form": "경주 어떤점이 재밋으셧어요",
          "original_form": "경주 어떤점이 재밋으셧어요",
          "speaker_id": "1",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000851.1.14",
          "form": "아 그러시구나 힐링이 정말 필요하시겠어요",
          "original_form": "아 그러시구나 힐링이 정말 필요하시겠어요",
          "speaker_id": "1",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000851.1.15",
          "form": "전 아직 해외못가봣어요 한번 가보려고 햇는대",
          "original_form": "전 아직 해외못가봣어요 한번 가보려고 햇는대",
          "speaker_id": "1",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000851.1.16",
          "form": "이제 한동안 못가겠죠..",
          "original_form": "이제 한동안 못가겠죠..",
          "speaker_id": "1",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000851.1.17",
          "form": "글램핑은 혹시 해보셨나요?",
          "original_form": "글램핑은 혹시 해보셨나요?",
          "speaker_id": "1",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000851.1.18",
          "form": "네ㅠㅠ한동안은 힘들겠죠",
          "original_form": "네ㅠㅠ한동안은 힘들겠죠",
          "speaker_id": "2",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000851.1.19",
          "form": "글램핑은 안해봤어요 해보셨어요?",
          "original_form": "글램핑은 안해봤어요 해보셨어요?",
          "speaker_id": "2",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000851.1.20",
          "form": "주변에 엄청 많이많이가는분이 계시는데",
          "original_form": "주변에 엄청 많이많이가는분이 계시는데",
          "speaker_id": "2",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000851.1.21",
          "form": "얼마전에 한번 가볼까 싶더라구요",
          "original_form": "얼마전에 한번 가볼까 싶더라구요",
          "speaker_id": "2",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000851.1.22",
          "form": "아니요 아직 못해봤는대",
          "original_form": "아니요 아직 못해봤는대",
          "speaker_id": "1",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000851.1.23",
          "form": "해보고싶더라구요",
          "original_form": "해보고싶더라구요",
          "speaker_id": "1",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000851.1.24",
          "form": "경주말고 다른곳은 어디잇나요",
          "original_form": "경주말고 다른곳은 어디잇나요",
          "speaker_id": "1",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000851.1.25",
          "form": "저는 여행을 자주다녀보고싶은대 막 엄청 잘.알지못해서",
          "original_form": "저는 여행을 자주다녀보고싶은대 막 엄청 잘.알지못해서",
          "speaker_id": "1",
          "time": "20210819 20:07"
        },
        {
          "id": "MDRW2100000851.1.26",
          "form": "제주도도 좋았어요",
          "original_form": "제주도도 좋았어요",
          "speaker_id": "2",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000851.1.27",
          "form": "통영 거제도 좋고  광주옆에 담양부터 순천여수광양도 좋았고",
          "original_form": "통영 거제도 좋고  광주옆에 담양부터 순천여수광양도 좋았고",
          "speaker_id": "2",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000851.1.28",
          "form": "남해도좋았고 ㅎㅎㅎ",
          "original_form": "남해도좋았고 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000851.1.29",
          "form": "아! 진주도 재밌어요 고성 공룡나라도 좋구요",
          "original_form": "아! 진주도 재밌어요 고성 공룡나라도 좋구요",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000851.1.30",
          "form": "아랫쪽만 다니죠 ㅋㅋㅋ 지방살아서 ㅋㅋ",
          "original_form": "아랫쪽만 다니죠 ㅋㅋㅋ 지방살아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000851.1.31",
          "form": "한 2박3일을 잡고가면",
          "original_form": "한 2박3일을 잡고가면",
          "speaker_id": "1",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000851.1.32",
          "form": "어디가 젤 좋으셨어요?",
          "original_form": "어디가 젤 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000851.1.33",
          "form": "아..2박3일...이면!\r\n  어디서 출발하느냐의 문제가 있긴한데...ㅋㅋ",
          "original_form": "아..2박3일...이면!\r\n  어디서 출발하느냐의 문제가 있긴한데...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100000851.1.34",
          "form": "해외도 좋지만 생각보다 국내도 좋은곳 많더라구요",
          "original_form": "해외도 좋지만 생각보다 국내도 좋은곳 많더라구요",
          "speaker_id": "2",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100000851.1.35",
          "form": "해외는 한번 가려면 큰맘먹고 가야할거같아서",
          "original_form": "해외는 한번 가려면 큰맘먹고 가야할거같아서",
          "speaker_id": "1",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000851.1.36",
          "form": "기념품 생각하면 해외도 나가고싶은대 여태 용기가 안났어요",
          "original_form": "기념품 생각하면 해외도 나가고싶은대 여태 용기가 안났어요",
          "speaker_id": "1",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000851.1.37",
          "form": "근데 국내도 비싼곳이 많아서  해외여행만큼 가격이 나오죠..ㅠㅠ",
          "original_form": "근데 국내도 비싼곳이 많아서  해외여행만큼 가격이 나오죠..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000851.1.38",
          "form": "여행하려면  가고싶은곳을 서치하고 적금하나 들어보는건 어때요?",
          "original_form": "여행하려면  가고싶은곳을 서치하고 적금하나 들어보는건 어때요?",
          "speaker_id": "2",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000851.1.39",
          "form": "당분간은 코로나때문에도 못갈테니  열심히 모으면  코로나끝나고 슝~ 좋은곳으로 갈 기회가 올거에요!",
          "original_form": "당분간은 코로나때문에도 못갈테니  열심히 모으면  코로나끝나고 슝~ 좋은곳으로 갈 기회가 올거에요!",
          "speaker_id": "2",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000851.1.40",
          "form": "그랬으면 정말.좋을거같아요",
          "original_form": "그랬으면 정말.좋을거같아요",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000851.1.41",
          "form": "여행이라도 다녀야 스트레스가 날라갈텐ㄷ9",
          "original_form": "여행이라도 다녀야 스트레스가 날라갈텐ㄷ9",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000851.1.42",
          "form": "쌓여만가고",
          "original_form": "쌓여만가고",
          "speaker_id": "1",
          "time": "20210819 20:22"
        }
      ]
