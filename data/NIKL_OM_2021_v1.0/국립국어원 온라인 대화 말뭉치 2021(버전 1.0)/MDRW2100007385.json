      "utterance": [
        {
          "id": "MDRW2100007385.1.1",
          "form": "하이볼 좋아하세요?",
          "original_form": "하이볼 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007385.1.2",
          "form": "그럼요 제가 name2입니다",
          "original_form": "그럼요 제가 &name2&입니다",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007385.1.3",
          "form": "ㅋㅋㅋㅋㅋ그럴거같아서",
          "original_form": "ㅋㅋㅋㅋㅋ그럴거같아서",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007385.1.4",
          "form": "여쭤봣어요",
          "original_form": "여쭤봣어요",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007385.1.5",
          "form": "사실 name3선생님이 계셔서 name3선생님이신가 했다가",
          "original_form": "사실 &name3&선생님이 계셔서 &name3&선생님이신가 했다가",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007385.1.6",
          "form": "앗ㅋㅋ 첨부터 알고 계신줄 알았어여ㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋ 첨부터 알고 계신줄 알았어여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007385.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007385.1.8",
          "form": "오 name3쌤도 하고 계시는군요ㅋㅋㅋㅋ 몰랐네요",
          "original_form": "오 &name3&쌤도 하고 계시는군요ㅋㅋㅋㅋ 몰랐네요",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007385.1.9",
          "form": "아 하고계시는진 모르겟는뎈ㅋㅋ 혹시나해서욬ㅋㅋㅋㅋ",
          "original_form": "아 하고계시는진 모르겟는뎈ㅋㅋ 혹시나해서욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007385.1.10",
          "form": "저는 지금도",
          "original_form": "저는 지금도",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007385.1.11",
          "form": "아 그러셨군요ㅋㅋㅋ",
          "original_form": "아 그러셨군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007385.1.12",
          "form": "맥주와 함께..ㅎㅎ",
          "original_form": "맥주와 함께..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007385.1.13",
          "form": "좋은 시간이시군요ㅋㅋㅋ 전 저녁먹으면서 한잔했답니다",
          "original_form": "좋은 시간이시군요ㅋㅋㅋ 전 저녁먹으면서 한잔했답니다",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007385.1.14",
          "form": "무슨 맥주드세요?",
          "original_form": "무슨 맥주드세요?",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007385.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007385.1.16",
          "form": "저 타이거 자몽이요!",
          "original_form": "저 타이거 자몽이요!",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007385.1.17",
          "form": "요즘은 좀 음료수같은 술을 주로마셔요",
          "original_form": "요즘은 좀 음료수같은 술을 주로마셔요",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007385.1.18",
          "form": "음료수 줄이려고...",
          "original_form": "음료수 줄이려고...",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007385.1.19",
          "form": "무슨 의민진 잘 모르겟어요",
          "original_form": "무슨 의민진 잘 모르겟어요",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007385.1.20",
          "form": "오 전 쉐퍼호퍼 자몽만 마셔봤는데 그것도 맛날거 같네요",
          "original_form": "오 전 쉐퍼호퍼 자몽만 마셔봤는데 그것도 맛날거 같네요",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007385.1.21",
          "form": "과일술이 홀짝홀짝 마시다보면 잘 들어가지 않나요",
          "original_form": "과일술이 홀짝홀짝 마시다보면 잘 들어가지 않나요",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007385.1.22",
          "form": "맞아요 ㅋㅋㅋ 더워가지고",
          "original_form": "맞아요 ㅋㅋㅋ 더워가지고",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007385.1.23",
          "form": "이런게 잘먹히네요",
          "original_form": "이런게 잘먹히네요",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007385.1.24",
          "form": "예전 계정에서 샘 술트윗보는게 참 반가웟는데",
          "original_form": "예전 계정에서 샘 술트윗보는게 참 반가웟는데",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007385.1.25",
          "form": "ㅜㅜㅜㅜ 계정 이사가신줄도 모르고ㅜㅠㅜㅜ",
          "original_form": "ㅜㅜㅜㅜ 계정 이사가신줄도 모르고ㅜㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007385.1.26",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007385.1.27",
          "form": "제 불찰입니다",
          "original_form": "제 불찰입니다",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007385.1.28",
          "form": "술친구를 잃어버리다니",
          "original_form": "술친구를 잃어버리다니",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007385.1.29",
          "form": "아녀... 그러실수도 있져",
          "original_form": "아녀... 그러실수도 있져",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007385.1.30",
          "form": "못 알아차린제가 바봅니다",
          "original_form": "못 알아차린제가 바봅니다",
          "speaker_id": "2",
