        },
        {
          "id": "MDRW2100026437.1.2",
          "form": "크림카레파스타",
          "original_form": "크림카레파스타",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026437.1.3",
          "form": "먹을거에요",
          "original_form": "먹을거에요",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026437.1.4",
          "form": "대박맛있겠다",
          "original_form": "대박맛있겠다",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026437.1.5",
          "form": "..",
          "original_form": "..",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026437.1.6",
          "form": "큭..",
          "original_form": "큭..",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026437.1.7",
          "form": "아 헐",
          "original_form": "아 헐",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026437.1.8",
          "form": "저거 치즈케이크 말하는구나",
          "original_form": "저거 치즈케이크 말하는구나",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026437.1.9",
          "form": "뭬친 저거",
          "original_form": "뭬친 저거",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026437.1.10",
          "form": "진심",
          "original_form": "진심",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026437.1.11",
          "form": "ㅁ맛있는제",
          "original_form": "ㅁ맛있는제",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026437.1.12",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026437.1.13",
          "form": "저거 맨날 먹음 예전에..",
          "original_form": "저거 맨날 먹음 예전에..",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026437.1.14",
          "form": "한개에 천원이잔아",
          "original_form": "한개에 천원이잔아",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026437.1.15",
          "form": "와굴쿠나",
          "original_form": "와굴쿠나",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026437.1.16",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026437.1.17",
          "form": "근데 입에서",
          "original_form": "근데 입에서",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026437.1.18",
          "form": "너무 사르르녹아서",
          "original_form": "너무 사르르녹아서",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026437.1.19",
          "form": "살짝 덜먹은기분듬",
          "original_form": "살짝 덜먹은기분듬",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026437.1.20",
          "form": "저거 우유랑 먹으먄",
          "original_form": "저거 우유랑 먹으먄",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026437.1.21",
          "form": "넘나뤼 맛잇지",
          "original_form": "넘나뤼 맛잇지",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026437.1.22",
          "form": "똥싸는중",
          "original_form": "똥싸는중",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026437.1.23",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026437.1.24",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026437.1.25",
          "form": "힘들다",
          "original_form": "힘들다",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026437.1.26",
          "form": "갑자기 혈육도 파스타 먹는대거",
          "original_form": "갑자기 혈육도 파스타 먹는대거",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026437.1.27",
          "form": "2인분 만듬",
          "original_form": "2인분 만듬",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026437.1.28",
          "form": "파스타만드나요",
          "original_form": "파스타만드나요",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026437.1.29",
          "form": "돌았나",
          "original_form": "돌았나",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026437.1.30",
          "form": "니가해",
          "original_form": "니가해",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026437.1.31",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026437.1.32",
          "form": "ㅎㅎ 근데",
          "original_form": "ㅎㅎ 근데",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026437.1.33",
          "form": "엄마가 도와줘서",
          "original_form": "엄마가 도와줘서",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026437.1.34",
          "form": "나 좀 꿀빠는 중",
          "original_form": "나 좀 꿀빠는 중",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026437.1.35",
          "form": "님아 우리라테일하고",
          "original_form": "님아 우리라테일하고",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026437.1.36",
          "form": "영화한편 때리고",
          "original_form": "영화한편 때리고",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026437.1.37",
          "form": "영화보면안돼요",
          "original_form": "영화보면안돼요",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026437.1.38",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026437.1.39",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026437.1.40",
          "form": "그 다음에 도안",
          "original_form": "그 다음에 도안",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026437.1.41",
          "form": "하면 안돼요?",
          "original_form": "하면 안돼요?",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026437.1.42",
          "form": "좋지",
          "original_form": "좋지",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026437.1.43",
          "form": "라테일뻘리없애야겟다",
          "original_form": "라테일뻘리없애야겟다",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026437.1.44",
          "form": "저 아까 또 꿈꿧음",
          "original_form": "저 아까 또 꿈꿧음",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026437.1.45",
          "form": "아까 꿈에서 제가",
          "original_form": "아까 꿈에서 제가",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026437.1.46",
          "form": "영화배우엿는데",
          "original_form": "영화배우엿는데",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026437.1.47",
          "form": "촬영 끝나고",
          "original_form": "촬영 끝나고",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026437.1.48",
          "form": "동네에 숨은 맛집으로",
          "original_form": "동네에 숨은 맛집으로",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026437.1.49",
          "form": "백숙 먹으러가는 꿈",
          "original_form": "백숙 먹으러가는 꿈",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026437.1.50",
          "form": "꿧음",
          "original_form": "꿧음",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026437.1.51",
          "form": "아웃겨",
          "original_form": "아웃겨",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026437.1.52",
