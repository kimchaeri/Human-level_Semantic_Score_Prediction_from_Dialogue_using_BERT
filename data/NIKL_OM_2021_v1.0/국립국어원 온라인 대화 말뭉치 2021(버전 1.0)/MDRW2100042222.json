      "utterance": [
        {
          "id": "MDRW2100042222.1.1",
          "form": "코로나 터지고 함번도 못봤어",
          "original_form": "코로나 터지고 함번도 못봤어",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042222.1.2",
          "form": "나도 못가고 그 친구도 못들어오니",
          "original_form": "나도 못가고 그 친구도 못들어오니",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042222.1.3",
          "form": "헐 그럼 거의 2년째네....ㅠ",
          "original_form": "헐 그럼 거의 2년째네....ㅠ",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042222.1.4",
          "form": "그래도 안헤어지고 잘 사귀고있는거보면 사이 조은가봐ㅎㅎ",
          "original_form": "그래도 안헤어지고 잘 사귀고있는거보면 사이 조은가봐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042222.1.5",
          "form": "맞아 사람은 너무 저은데 상황이 난 너무 지긋지긋해",
          "original_form": "맞아 사람은 너무 저은데 상황이 난 너무 지긋지긋해",
          "speaker_id": "1",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042222.1.6",
          "form": "지금이 위기인거같아",
          "original_form": "지금이 위기인거같아",
          "speaker_id": "1",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100042222.1.7",
          "form": "이게 무슨 이산가족도아니고 이게 뭐야ㅋㅋㅋㅠㅠ",
          "original_form": "이게 무슨 이산가족도아니고 이게 뭐야ㅋㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:48"
        },
        {
          "id": "MDRW2100042222.1.8",
          "form": "그니까 너무 힘들어",
          "original_form": "그니까 너무 힘들어",
          "speaker_id": "1",
          "time": "20210906 20:48"
        },
        {
          "id": "MDRW2100042222.1.9",
          "form": "다녀오면 결혼할수 있을까?",
          "original_form": "다녀오면 결혼할수 있을까?",
          "speaker_id": "1",
          "time": "20210906 20:48"
        },
        {
          "id": "MDRW2100042222.1.10",
          "form": "나이가 애매해 둘다 ㅋㅋㅋㅋ",
          "original_form": "나이가 애매해 둘다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042222.1.11",
          "form": "근데 다녀오면 다시 얼굴보면서 적응도해야하고",
          "original_form": "근데 다녀오면 다시 얼굴보면서 적응도해야하고",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042222.1.12",
          "form": "이래저래 갑갑하겠당....ㅜ",
          "original_form": "이래저래 갑갑하겠당....ㅜ",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042222.1.13",
          "form": "마자 어색할거같지?",
          "original_form": "마자 어색할거같지?",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042222.1.14",
          "form": "나는 완전어색할것같애ㅜㅜㅜ",
          "original_form": "나는 완전어색할것같애ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042222.1.15",
          "form": "맞아ㅠㅠ",
          "original_form": "맞아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042222.1.16",
          "form": "근데 맨날 영상통화해서 괜찮을거여",
          "original_form": "근데 맨날 영상통화해서 괜찮을거여",
