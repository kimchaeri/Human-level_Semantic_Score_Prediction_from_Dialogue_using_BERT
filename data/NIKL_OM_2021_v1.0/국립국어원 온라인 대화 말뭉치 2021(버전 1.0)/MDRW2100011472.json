      "utterance": [
        {
          "id": "MDRW2100011472.1.1",
          "form": "해외 가서 일 할 거야?",
          "original_form": "해외 가서 일 할 거야?",
          "speaker_id": "2",
          "time": "20210824 13:00"
        },
        {
          "id": "MDRW2100011472.1.2",
          "form": "부모님은 걱정 안하셔?",
          "original_form": "부모님은 걱정 안하셔?",
          "speaker_id": "2",
          "time": "20210824 13:00"
        },
        {
          "id": "MDRW2100011472.1.3",
          "form": "ㅇㅇㅇ졸업하자마자나가려고",
          "original_form": "ㅇㅇㅇ졸업하자마자나가려고",
          "speaker_id": "1",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100011472.1.4",
          "form": "나",
          "original_form": "나",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100011472.1.5",
          "form": "나간다음에",
          "original_form": "나간다음에",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100011472.1.6",
          "form": "엄마만데리고오려고했는데",
          "original_form": "엄마만데리고오려고했는데",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100011472.1.7",
          "form": "싫다고해서",
          "original_form": "싫다고해서",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100011472.1.8",
          "form": "혼자나가려고",
          "original_form": "혼자나가려고",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100011472.1.9",
          "form": "너는어머님이괜찮으시대?",
          "original_form": "너는어머님이괜찮으시대?",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100011472.1.10",
          "form": "해외나가도",
          "original_form": "해외나가도",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100011472.1.11",
          "form": "나는 말씀 안드렸는데?",
          "original_form": "나는 말씀 안드렸는데?",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100011472.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100011472.1.13",
          "form": "나중에지원안받으려고?",
          "original_form": "나중에지원안받으려고?",
          "speaker_id": "1",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100011472.1.14",
          "form": "가기 3개월 전에 알려드리면 되지 낞을까?",
          "original_form": "가기 3개월 전에 알려드리면 되지 낞을까?",
          "speaker_id": "2",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011472.1.15",
          "form": "아 그걸 생각못했다",
          "original_form": "아 그걸 생각못했다",
          "speaker_id": "2",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011472.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ뭐야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ뭐야",
          "speaker_id": "1",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011472.1.17",
          "form": "해외면 가족 들의 지원이 중요하다는 걸",
          "original_form": "해외면 가족 들의 지원이 중요하다는 걸",
          "speaker_id": "2",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011472.1.18",
          "form": "깜빡했어",
          "original_form": "깜빡했어",
          "speaker_id": "2",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011472.1.19",
          "form": "그치 솔직히 그냥 나가기에는",
          "original_form": "그치 솔직히 그냥 나가기에는",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011472.1.20",
          "form": "부담스러움",
          "original_form": "부담스러움",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011472.1.21",
          "form": "특히미국이면",
          "original_form": "특히미국이면",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011472.1.22",
          "form": "집값이너무비쌈",
          "original_form": "집값이너무비쌈",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011472.1.23",
          "form": "아 그지 집값도 비싸고 아는 커뮤니티도 없잖아",
          "original_form": "아 그지 집값도 비싸고 아는 커뮤니티도 없잖아",
          "speaker_id": "2",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011472.1.24",
          "form": "ㅇㅇㅇ맞아",
          "original_form": "ㅇㅇㅇ맞아",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011472.1.25",
          "form": "가족들한테 아는 사람있냐고 물어봐야 하는데",
          "original_form": "가족들한테 아는 사람있냐고 물어봐야 하는데",
          "speaker_id": "2",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011472.1.26",
          "form": "ㄱ친척중에있다하지않았음?",
          "original_form": "ㄱ친척중에있다하지않았음?",
          "speaker_id": "1",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011472.1.27",
          "form": "그러면영주권받기쉬울텐데",
          "original_form": "그러면영주권받기쉬울텐데",
          "speaker_id": "1",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011472.1.28",
          "form": "안 친한데",
          "original_form": "안 친한데",
          "speaker_id": "2",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011472.1.29",
          "form": "별로 안좋아하는 분들인데",
          "original_form": "별로 안좋아하는 분들인데",
          "speaker_id": "2",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011472.1.30",
          "form": "그래도그친척분들",
          "original_form": "그래도그친척분들",
          "speaker_id": "1",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011472.1.31",
          "form": "시민권까지있음?",
          "original_form": "시민권까지있음?",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011472.1.32",
          "form": "시민권까지있으면친척영주권으로초대항수있을걸",
          "original_form": "시민권까지있으면친척영주권으로초대항수있을걸",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011472.1.33",
          "form": "안 친해서 안 물어봤어",
          "original_form": "안 친해서 안 물어봤어",
          "speaker_id": "2",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011472.1.34",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011472.1.35",
          "form": "대박이네",
          "original_form": "대박이네",
          "speaker_id": "2",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011472.1.36",
          "form": "나중에나갈때쯤친한척해",
          "original_form": "나중에나갈때쯤친한척해",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011472.1.37",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011472.1.38",
          "form": "확실하지는않음",
          "original_form": "확실하지는않음",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011472.1.39",
          "form": "가족초대할수있는건",
          "original_form": "가족초대할수있는건",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011472.1.40",
          "form": "아는데",
          "original_form": "아는데",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011472.1.41",
          "form": "그기준이어디친척까지인지ㅣ",
          "original_form": "그기준이어디친척까지인지ㅣ",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011472.1.42",
          "form": "확실하지않음",
          "original_form": "확실하지않음",
          "speaker_id": "1",
          "time": "20210824 13:11"
        }
      ]
    }
  ]
