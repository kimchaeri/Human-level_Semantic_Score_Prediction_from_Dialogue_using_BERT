      "utterance": [
        {
          "id": "MDRW2100003497.1.1",
          "form": "티비 자주 보시나요?",
          "original_form": "티비 자주 보시나요?",
          "speaker_id": "2",
          "time": "20210821 10:38"
        },
        {
          "id": "MDRW2100003497.1.2",
          "form": "티비에서 가끔 영화나 무한도전 다시보기같은거 자주봐요",
          "original_form": "티비에서 가끔 영화나 무한도전 다시보기같은거 자주봐요",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003497.1.3",
          "form": "티비 자주 보세요?",
          "original_form": "티비 자주 보세요?",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003497.1.4",
          "form": "아~무도 팬이셨군요",
          "original_form": "아~무도 팬이셨군요",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003497.1.5",
          "form": "무도 좋아하세요?",
          "original_form": "무도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 11:54"
        },
        {
          "id": "MDRW2100003497.1.6",
          "form": "네~예전에는 무조건 본방사수 !!",
          "original_form": "네~예전에는 무조건 본방사수 !!",
          "speaker_id": "2",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003497.1.7",
          "form": "그러면 어떤노래 좋아하세요?",
          "original_form": "그러면 어떤노래 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 11:57"
        },
        {
          "id": "MDRW2100003497.1.8",
          "form": "요즘에는 sg워너비요",
          "original_form": "요즘에는 sg워너비요",
          "speaker_id": "2",
          "time": "20210821 11:58"
        },
        {
          "id": "MDRW2100003497.1.9",
          "form": "name1님은요!",
          "original_form": "&name1&님은요!",
          "speaker_id": "2",
          "time": "20210821 12:05"
        },
        {
          "id": "MDRW2100003497.1.10",
          "form": "저는 퀸이나 리쌍노래가 좋더라구요",
          "original_form": "저는 퀸이나 리쌍노래가 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003497.1.11",
          "form": "아~ 찾아 들어보진 않았는데 한번 들어봐야 겠어요",
          "original_form": "아~ 찾아 들어보진 않았는데 한번 들어봐야 겠어요",
          "speaker_id": "2",
          "time": "20210821 12:19"
        },
        {
          "id": "MDRW2100003497.1.12",
          "form": "넷플릭스로는 뭐 보세요?",
          "original_form": "넷플릭스로는 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003497.1.13",
          "form": "앗 넥플릭스 안해요ㅠㅠ",
          "original_form": "앗 넥플릭스 안해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003497.1.14",
          "form": "그거 좋나여?",
          "original_form": "그거 좋나여?",
