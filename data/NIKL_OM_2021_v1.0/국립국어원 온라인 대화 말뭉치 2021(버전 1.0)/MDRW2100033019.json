      "utterance": [
        {
          "id": "MDRW2100033019.1.1",
          "form": "안산은 여자가봐도 멋지죠",
          "original_form": "안산은 여자가봐도 멋지죠",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033019.1.2",
          "form": "예쁘고 멋지고 다하는거같아요",
          "original_form": "예쁘고 멋지고 다하는거같아요",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033019.1.3",
          "form": "누가 봐도 멋진 사람이에요",
          "original_form": "누가 봐도 멋진 사람이에요",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033019.1.4",
          "form": "맞아요 ㅎㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033019.1.5",
          "form": "그래서 남자들이 열폭하나 ㅎㅎ",
          "original_form": "그래서 남자들이 열폭하나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033019.1.6",
          "form": "오오 이번 주제는 특이하네요",
          "original_form": "오오 이번 주제는 특이하네요",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100033019.1.7",
          "form": "음ㅎㅎ이건 솔직히 쌤통입니다",
          "original_form": "음ㅎㅎ이건 솔직히 쌤통입니다",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100033019.1.8",
          "form": "역시 구글이 미국 회사라 진보적이네요!",
          "original_form": "역시 구글이 미국 회사라 진보적이네요!",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100033019.1.9",
          "form": "어떤 면에서요??",
          "original_form": "어떤 면에서요??",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100033019.1.10",
          "form": "그동안 구글이 너무 수수료를 많이받은거같아요",
          "original_form": "그동안 구글이 너무 수수료를 많이받은거같아요",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033019.1.11",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033019.1.12",
          "form": "그래서 규제가 필요한거같아요",
          "original_form": "그래서 규제가 필요한거같아요",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033019.1.13",
          "form": "그건 그래요 진짜",
          "original_form": "그건 그래요 진짜",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033019.1.14",
          "form": "유튜브도",
          "original_form": "유튜브도",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033019.1.15",
          "form": "구글꺼죠?",
          "original_form": "구글꺼죠?",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033019.1.16",
          "form": "대기업의 횡포가 이번기회에 좀 줄겠네요",
          "original_form": "대기업의 횡포가 이번기회에 좀 줄겠네요",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033019.1.17",
          "form": "아아 갑질방지법이",
          "original_form": "아아 갑질방지법이",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033019.1.18",
          "form": "구글꺼같아요",
          "original_form": "구글꺼같아요",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033019.1.19",
          "form": "수수료 관련한거였군요!",
          "original_form": "수수료 관련한거였군요!",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033019.1.20",
          "form": "네메",
          "original_form": "네메",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033019.1.21",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033019.1.22",
          "form": "넵넵 안산말구용ㅋㅋㅋ",
          "original_form": "넵넵 안산말구용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033019.1.23",
          "form": "유투브 프리미엄도 너무 비싸고...",
          "original_form": "유투브 프리미엄도 너무 비싸고...",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033019.1.24",
          "form": "아 저는 갑질방지법이라는게",
          "original_form": "아 저는 갑질방지법이라는게",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033019.1.25",
          "form": "직원 관련 문제인줄 알았어요 ㅎㅎ",
          "original_form": "직원 관련 문제인줄 알았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:02"
        }
      ]
