      "utterance": [
        {
          "id": "MDRW2100035628.1.1",
          "form": "실버푸들도 너무 귀엽더라고요",
          "original_form": "실버푸들도 너무 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035628.1.2",
          "form": "아아 푸들 귀엽죠",
          "original_form": "아아 푸들 귀엽죠",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035628.1.3",
          "form": "미용잘해서",
          "original_form": "미용잘해서",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035628.1.4",
          "form": "발목이 볼록볼록한거 귀여워요",
          "original_form": "발목이 볼록볼록한거 귀여워요",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035628.1.5",
          "form": "그리고 애들이 똑똑해서 말귀도 잘 알아듣는다고 하더라고용 ㅋㅋ",
          "original_form": "그리고 애들이 똑똑해서 말귀도 잘 알아듣는다고 하더라고용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035628.1.6",
          "form": "오오 그렇군요 똑똑하군요",
          "original_form": "오오 그렇군요 똑똑하군요",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035628.1.7",
          "form": "어르신분들이나 강아지 처음 키우시는 분들한테 추천한대여 ㅋㅋ",
          "original_form": "어르신분들이나 강아지 처음 키우시는 분들한테 추천한대여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100035628.1.8",
          "form": "오오 네",
          "original_form": "오오 네",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035628.1.9",
          "form": "강아지가 알아서 잘 하나봐요",
          "original_form": "강아지가 알아서 잘 하나봐요",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035628.1.10",
          "form": "사실 한번도 안키워보고 다 ㅋㅋ유투브에서 본 지식이지만요 ㅋ",
          "original_form": "사실 한번도 안키워보고 다 ㅋㅋ유투브에서 본 지식이지만요 ㅋ",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035628.1.11",
          "form": "아아 그러시군요 맞겠죠 머 ㅎㅎ",
          "original_form": "아아 그러시군요 맞겠죠 머 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035628.1.12",
          "form": "사실 은퇴하시고 적적하실수있다길래 한번 키워보시면어떨까 생각도했네요",
          "original_form": "사실 은퇴하시고 적적하실수있다길래 한번 키워보시면어떨까 생각도했네요",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035628.1.13",
          "form": "부모님이 은퇴하신거에요?",
          "original_form": "부모님이 은퇴하신거에요?",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035628.1.14",
          "form": "아직은 안하셨는데",
          "original_form": "아직은 안하셨는데",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035628.1.15",
          "form": "또 강아지가있음",
          "original_form": "또 강아지가있음",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035628.1.16",
          "form": "집에 활기가돈달까",
          "original_form": "집에 활기가돈달까",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035628.1.17",
          "form": "자식삼아 많이들 키우는거 같더라구요",
          "original_form": "자식삼아 많이들 키우는거 같더라구요",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035628.1.18",
          "form": "그죠 오늘도 이렇게  꿈만 키웁니다 ㅋㅋ정작 엄마는 싫으시다니 절대무리지만 ㅋ",
          "original_form": "그죠 오늘도 이렇게  꿈만 키웁니다 ㅋㅋ정작 엄마는 싫으시다니 절대무리지만 ㅋ",
          "speaker_id": "2",
          "time": "20210829 22:12"
        }
      ]
