      "utterance": [
        {
          "id": "MDRW2100034908.1.1",
          "form": "알바때문에 생긴 직업병 같은 거 있어?",
          "original_form": "알바때문에 생긴 직업병 같은 거 있어?",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100034908.1.2",
          "form": "가끔 저기요 하면",
          "original_form": "가끔 저기요 하면",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034908.1.3",
          "form": "흠칫...",
          "original_form": "흠칫...",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034908.1.4",
          "form": "아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034908.1.5",
          "form": "그리고 맥주 따르다보니까",
          "original_form": "그리고 맥주 따르다보니까",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034908.1.6",
          "form": "손목이 안좋아짐...",
          "original_form": "손목이 안좋아짐...",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034908.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034908.1.8",
          "form": "헐ㅠㅠ",
          "original_form": "헐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034908.1.9",
          "form": "잔 무겁다고 했지",
          "original_form": "잔 무겁다고 했지",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034908.1.10",
          "form": "기울여서 따르는데 손목 살짝 예...",
          "original_form": "기울여서 따르는데 손목 살짝 예...",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034908.1.11",
          "form": "엉.. 살짝?",
          "original_form": "엉.. 살짝?",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034908.1.12",
          "form": "손목 보호대 같은 거 차면 좀 나으려나?",
          "original_form": "손목 보호대 같은 거 차면 좀 나으려나?",
          "speaker_id": "1",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100034908.1.13",
          "form": "일할때는 멀쩡함",
          "original_form": "일할때는 멀쩡함",
          "speaker_id": "2",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100034908.1.14",
          "form": "가끔씩 아플뿐",
          "original_form": "가끔씩 아플뿐",
          "speaker_id": "2",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100034908.1.15",
          "form": "글쿠만!",
          "original_form": "글쿠만!",
          "speaker_id": "1",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100034908.1.16",
          "form": "예에!",
          "original_form": "예에!",
          "speaker_id": "2",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100034908.1.17",
          "form": "나중에 많이 아프면 손목보호대 한번 알아봐봐!",
          "original_form": "나중에 많이 아프면 손목보호대 한번 알아봐봐!",
          "speaker_id": "1",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100034908.1.18",
          "form": "엉엉!!",
          "original_form": "엉엉!!",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100034908.1.19",
