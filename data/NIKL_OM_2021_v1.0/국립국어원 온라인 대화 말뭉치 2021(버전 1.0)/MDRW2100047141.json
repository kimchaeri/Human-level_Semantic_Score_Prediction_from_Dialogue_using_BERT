      "utterance": [
        {
          "id": "MDRW2100047141.1.1",
          "form": "어떤 노래 자주 들으세요?",
          "original_form": "어떤 노래 자주 들으세요?",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100047141.1.2",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100047141.1.3",
          "form": "저는 시스타요",
          "original_form": "저는 시스타요",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100047141.1.4",
          "form": "시스타 예전에 정말 잘나갔는데 생각보다 일찍 해체해서 아쉬워요",
          "original_form": "시스타 예전에 정말 잘나갔는데 생각보다 일찍 해체해서 아쉬워요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100047141.1.5",
          "form": "전 레드벨벳 노래 많이 들어요",
          "original_form": "전 레드벨벳 노래 많이 들어요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100047141.1.6",
          "form": "아아ㅜㅜ",
          "original_form": "아아ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100047141.1.7",
          "form": "전 조이좋아해요",
          "original_form": "전 조이좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100047141.1.8",
          "form": "넘귀엽",
          "original_form": "넘귀엽",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100047141.1.9",
          "form": "상큼",
          "original_form": "상큼",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100047141.1.10",
          "form": "발랄",
          "original_form": "발랄",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100047141.1.11",
          "form": "맞아요 음색도 너무 좋아요",
          "original_form": "맞아요 음색도 너무 좋아요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100047141.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100047141.1.13",
          "form": "솔로앨범 나와서 너무 좋았어요 조이 매력이 잘 드러나서 귀가 즐거웠어요",
          "original_form": "솔로앨범 나와서 너무 좋았어요 조이 매력이 잘 드러나서 귀가 즐거웠어요",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100047141.1.14",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100047141.1.15",
          "form": "어쩜 노래랑 감성이",
          "original_form": "어쩜 노래랑 감성이",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100047141.1.16",
          "form": "옛곡이랑 찰떡인지",
          "original_form": "옛곡이랑 찰떡인지",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100047141.1.17",
          "form": "쥬뗌므",
          "original_form": "쥬뗌므",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100047141.1.18",
          "form": "현석이아세요?",
          "original_form": "현석이아세요?",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100047141.1.19",
          "form": "현석이요? 아뇨",
          "original_form": "현석이요? 아뇨",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100047141.1.20",
          "form": "한번 찾아보세요",
          "original_form": "한번 찾아보세요",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100047141.1.21",
          "form": "ㄹㅇ 띵작",
          "original_form": "ㄹㅇ 띵작",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100047141.1.22",
          "form": "노래예요?",
          "original_form": "노래예요?",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100047141.1.23",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100047141.1.24",
          "form": "수필요",
          "original_form": "수필요",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100047141.1.25",
          "form": "누가 쓴 건가요?",
          "original_form": "누가 쓴 건가요?",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100047141.1.26",
          "form": "그..건 잘",
          "original_form": "그..건 잘",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100047141.1.27",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100047141.1.28",
          "form": "첫사랑 이야기에요",
          "original_form": "첫사랑 이야기에요",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100047141.1.29",
          "form": "ㅋㅋㅋ 읽어볼게요",
          "original_form": "ㅋㅋㅋ 읽어볼게요",
          "speaker_id": "1",
          "time": "20210825 22:50"
        }
      ]
    }
