        },
        {
          "id": "MDRW2100028600.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028600.1.3",
          "form": "요즘 비가 왕창 오다가 말다가 해서 넘 꿀꿀해요",
          "original_form": "요즘 비가 왕창 오다가 말다가 해서 넘 꿀꿀해요",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028600.1.4",
          "form": "헉 저두요! 요새 날씨가 오락가락하더라구요",
          "original_form": "헉 저두요! 요새 날씨가 오락가락하더라구요",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028600.1.5",
          "form": "그래서 저는 일기예보랑 상관없이",
          "original_form": "그래서 저는 일기예보랑 상관없이",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028600.1.6",
          "form": "늘 우산을 챙겨다녀요",
          "original_form": "늘 우산을 챙겨다녀요",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028600.1.7",
          "form": "저는 일기예보를 챙겨보질 않아서",
          "original_form": "저는 일기예보를 챙겨보질 않아서",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028600.1.8",
          "form": "또 덜렁대는 성격이라 우산도 자주 두고다녀요",
          "original_form": "또 덜렁대는 성격이라 우산도 자주 두고다녀요",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028600.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028600.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028600.1.11",
          "form": "저도 매번 보는 건 아닌데 날씨가 이상하다보니",
          "original_form": "저도 매번 보는 건 아닌데 날씨가 이상하다보니",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028600.1.12",
          "form": "보게 되던..",
          "original_form": "보게 되던..",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028600.1.13",
          "form": "그렇군용 그래두 요즘 더위가 좀 가셔서",
          "original_form": "그렇군용 그래두 요즘 더위가 좀 가셔서",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028600.1.14",
          "form": "여름이 물러가는 느낌??이에요",
          "original_form": "여름이 물러가는 느낌??이에요",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028600.1.15",
          "form": "아침이랑 밤에는 조금 선선하더라구요",
          "original_form": "아침이랑 밤에는 조금 선선하더라구요",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028600.1.16",
          "form": "이런 날씨에는 옷을 얇게 입으면 감기걸리기 쉽상이니 조심하세요",
          "original_form": "이런 날씨에는 옷을 얇게 입으면 감기걸리기 쉽상이니 조심하세요",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028600.1.17",
          "form": "저는 그래서 얇은 겉옷을 챙겨다녀요ㅋㅋㅋ",
          "original_form": "저는 그래서 얇은 겉옷을 챙겨다녀요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028600.1.18",
          "form": "얼른 여름이 끝나고 가을 겨울이 지나서 봄이 왔으면 좋겠어요",
          "original_form": "얼른 여름이 끝나고 가을 겨울이 지나서 봄이 왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028600.1.19",
          "form": "봄을 제일 좋아하시나봐요~",
          "original_form": "봄을 제일 좋아하시나봐요~",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028600.1.20",
          "form": "네ㅋㅋㅋ 님은요?",
          "original_form": "네ㅋㅋㅋ 님은요?",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028600.1.21",
          "form": "저는 겨울이요~",
          "original_form": "저는 겨울이요~",
          "speaker_id": "2",
          "time": "20210902 21:06"
        }
      ]
