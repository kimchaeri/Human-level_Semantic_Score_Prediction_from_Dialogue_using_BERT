      "utterance": [
        {
          "id": "MDRW2100017372.1.1",
          "form": "맛있는 점심메뉴는 무엇인가요?",
          "original_form": "맛있는 점심메뉴는 무엇인가요?",
          "speaker_id": "1",
          "time": "20210827 11:12"
        },
        {
          "id": "MDRW2100017372.1.2",
          "form": "점심 메뉴는 무엇인가요",
          "original_form": "점심 메뉴는 무엇인가요",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100017372.1.3",
          "form": "나물이에요\r\n버서 감자 상추등",
          "original_form": "나물이에요\r\n버서 감자 상추등",
          "speaker_id": "2",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100017372.1.4",
          "form": "버섯~",
          "original_form": "버섯~",
          "speaker_id": "2",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100017372.1.5",
          "form": "모드셨어요?",
          "original_form": "모드셨어요?",
          "speaker_id": "2",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100017372.1.6",
          "form": "아 난 아직이요",
          "original_form": "아 난 아직이요",
          "speaker_id": "1",
          "time": "20210829 12:13"
        },
        {
          "id": "MDRW2100017372.1.7",
          "form": "떡볶이 먹을거예요",
          "original_form": "떡볶이 먹을거예요",
          "speaker_id": "1",
          "time": "20210829 12:13"
        },
        {
          "id": "MDRW2100017372.1.8",
          "form": "자연식 좋아여",
          "original_form": "자연식 좋아여",
          "speaker_id": "1",
          "time": "20210829 12:13"
        },
        {
          "id": "MDRW2100017372.1.9",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "2",
          "time": "20210829 12:13"
        },
        {
          "id": "MDRW2100017372.1.10",
          "form": "어디꺼?",
          "original_form": "어디꺼?",
          "speaker_id": "2",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100017372.1.11",
          "form": "담에 같이 먹어요",
          "original_form": "담에 같이 먹어요",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100017372.1.12",
          "form": "자연식 좋아요~",
          "original_form": "자연식 좋아요~",
          "speaker_id": "2",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100017372.1.13",
          "form": "먹슐랭",
          "original_form": "먹슐랭",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100017372.1.14",
          "form": "오 맛있나요?",
          "original_form": "오 맛있나요?",
          "speaker_id": "2",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100017372.1.15",
          "form": "같이 먹으면 더 좋은데요",
          "original_form": "같이 먹으면 더 좋은데요",
          "speaker_id": "1",
          "time": "20210829 12:14"
        },
        {
          "id": "MDRW2100017372.1.16",
          "form": "내가 먼저 먹구 날려줄게요",
          "original_form": "내가 먼저 먹구 날려줄게요",
          "speaker_id": "1",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100017372.1.17",
          "form": "그래요~~",
          "original_form": "그래요~~",
          "speaker_id": "2",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100017372.1.18",
          "form": "알려",
          "original_form": "알려",
          "speaker_id": "1",
          "time": "20210829 12:15"
        },
        {
