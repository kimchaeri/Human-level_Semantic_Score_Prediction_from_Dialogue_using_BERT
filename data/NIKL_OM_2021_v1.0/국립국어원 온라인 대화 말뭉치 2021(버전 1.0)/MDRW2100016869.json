      "utterance": [
        {
          "id": "MDRW2100016869.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016869.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016869.1.3",
          "form": "혹시 알바나 직업 있으신가요?",
          "original_form": "혹시 알바나 직업 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016869.1.4",
          "form": "네 저는 미술보조강사예요 ㅋ",
          "original_form": "네 저는 미술보조강사예요 ㅋ",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016869.1.5",
          "form": "무슨일 하세요?",
          "original_form": "무슨일 하세요?",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016869.1.6",
          "form": "우와 멋져요~",
          "original_form": "우와 멋져요~",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016869.1.7",
          "form": "전 알바해요",
          "original_form": "전 알바해요",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016869.1.8",
          "form": "아 멋있는건 아니구요. ㅋ저도 알바에요",
          "original_form": "아 멋있는건 아니구요. ㅋ저도 알바에요",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016869.1.9",
          "form": "그렇군요! 어떤업무하세요?",
          "original_form": "그렇군요! 어떤업무하세요?",
          "speaker_id": "1",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016869.1.10",
          "form": "장애인분들 미술하는거 옆에서 도와드리고 있어요.",
          "original_form": "장애인분들 미술하는거 옆에서 도와드리고 있어요.",
          "speaker_id": "2",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016869.1.11",
          "form": "무슨 알바하세요?",
          "original_form": "무슨 알바하세요?",
          "speaker_id": "2",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016869.1.12",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016869.1.13",
          "form": "저는 서포터즈알바요!",
          "original_form": "저는 서포터즈알바요!",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016869.1.14",
          "form": "온라인수업하시는 강사분들 도와드리고있어요",
          "original_form": "온라인수업하시는 강사분들 도와드리고있어요",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016869.1.15",
          "form": "아아ㅋㅋㅋ 그것도 멋있네요ㅋㅋ",
          "original_form": "아아ㅋㅋㅋ 그것도 멋있네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016869.1.16",
          "form": "일은 안힘드세요?",
          "original_form": "일은 안힘드세요?",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016869.1.17",
          "form": "전혀안힘들어요 ㅎㅎ 강사님들 워낙 잘해주고계셔서요~",
          "original_form": "전혀안힘들어요 ㅎㅎ 강사님들 워낙 잘해주고계셔서요~",
          "speaker_id": "1",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016869.1.18",
          "form": "그러시군요 ㅋㅋ 멋져요ㅋㅋ",
          "original_form": "그러시군요 ㅋㅋ 멋져요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:15"
        },
        {
          "id": "MDRW2100016869.1.19",
          "form": "아녜요~ㅋㅋㅋ그래도적성에맞아다행인거같아요",
          "original_form": "아녜요~ㅋㅋㅋ그래도적성에맞아다행인거같아요",
          "speaker_id": "1",
