      "utterance": [
        {
          "id": "MDRW2100030604.1.1",
          "form": "광복절 한참 지났는데",
          "original_form": "광복절 한참 지났는데",
          "speaker_id": "2",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100030604.1.2",
          "form": "집에계셨나요",
          "original_form": "집에계셨나요",
          "speaker_id": "2",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100030604.1.3",
          "form": "네~~",
          "original_form": "네~~",
          "speaker_id": "1",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100030604.1.4",
          "form": "네네저도요",
          "original_form": "네네저도요",
          "speaker_id": "2",
          "time": "20210904 10:10"
        },
        {
          "id": "MDRW2100030604.1.5",
          "form": "거리두기 잘하고있는편입니다",
          "original_form": "거리두기 잘하고있는편입니다",
          "speaker_id": "2",
          "time": "20210904 10:10"
        },
        {
          "id": "MDRW2100030604.1.6",
          "form": "저두요..안다가고 집에서",
          "original_form": "저두요..안다가고 집에서",
          "speaker_id": "1",
          "time": "20210904 10:13"
        },
        {
          "id": "MDRW2100030604.1.7",
          "form": "어디서 어떻게 될지모르니",
          "original_form": "어디서 어떻게 될지모르니",
          "speaker_id": "1",
          "time": "20210904 10:13"
        },
        {
          "id": "MDRW2100030604.1.8",
          "form": "맞아요 광안리는 거의 코로나끝인거같더라고요",
          "original_form": "맞아요 광안리는 거의 코로나끝인거같더라고요",
          "speaker_id": "2",
          "time": "20210904 10:20"
        },
        {
          "id": "MDRW2100030604.1.9",
          "form": "마스크도안하고 열받아요",
          "original_form": "마스크도안하고 열받아요",
          "speaker_id": "2",
          "time": "20210904 10:20"
        },
        {
          "id": "MDRW2100030604.1.10",
          "form": "부산이시구나",
          "original_form": "부산이시구나",
          "speaker_id": "1",
          "time": "20210904 10:21"
        },
        {
          "id": "MDRW2100030604.1.11",
          "form": "네네맞아요",
          "original_form": "네네맞아요",
          "speaker_id": "2",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100030604.1.12",
          "form": "다시 3단계로 내려가 걱정이에요",
          "original_form": "다시 3단계로 내려가 걱정이에요",
          "speaker_id": "2",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100030604.1.13",
          "form": "광복절연휴는 4단계여도 심했는데",
          "original_form": "광복절연휴는 4단계여도 심했는데",
          "speaker_id": "2",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100030604.1.14",
          "form": "추석은 어쩔지",
          "original_form": "추석은 어쩔지",
          "speaker_id": "2",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100030604.1.15",
          "form": "여긴 대전인데 여기도 4단계였다가 지금3단계내려갔네요",
          "original_form": "여긴 대전인데 여기도 4단계였다가 지금3단계내려갔네요",
          "speaker_id": "1",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100030604.1.16",
          "form": "어제또 바뀔지",
          "original_form": "어제또 바뀔지",
          "speaker_id": "1",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100030604.1.17",
          "form": "다들조심해야되는데",
          "original_form": "다들조심해야되는데",
          "speaker_id": "1",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100030604.1.18",
          "form": "밥업소가는사람들 이해가안가요",
          "original_form": "밥업소가는사람들 이해가안가요",
          "speaker_id": "1",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100030604.1.19",
          "form": "맞아요 정말 이해안가요",
          "original_form": "맞아요 정말 이해안가요",
          "speaker_id": "2",
          "time": "20210904 10:36"
        },
        {
          "id": "MDRW2100030604.1.20",
          "form": "영원히안끝날듯",
          "original_form": "영원히안끝날듯",
          "speaker_id": "2",
          "time": "20210904 10:36"
        },
        {
          "id": "MDRW2100030604.1.21",
          "form": "빨리 끝났음좋겠는데",
          "original_form": "빨리 끝났음좋겠는데",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030604.1.22",
          "form": "저는 잔여백신 얼렁 맞았거든요",
          "original_form": "저는 잔여백신 얼렁 맞았거든요",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030604.1.23",
          "form": "오 2차까지 맞우셨나요",
          "original_form": "오 2차까지 맞우셨나요",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030604.1.24",
          "form": "얀센이라 한방에",
          "original_form": "얀센이라 한방에",
          "speaker_id": "1",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030604.1.25",
          "form": "오오 얀센이군요",
          "original_form": "오오 얀센이군요",
          "speaker_id": "2",
          "time": "20210904 11:23"
        },
        {
          "id": "MDRW2100030604.1.26",
          "form": "아프진않으셨나요",
          "original_form": "아프진않으셨나요",
