        },
        {
          "id": "MDRW2100019964.1.2",
          "form": "머만드셨어요?",
          "original_form": "머만드셨어요?",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019964.1.3",
          "form": "머만드셨어요?",
          "original_form": "머만드셨어요?",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019964.1.4",
          "form": "깐풍두부요",
          "original_form": "깐풍두부요",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019964.1.5",
          "form": "두부로 깐풍기를요?",
