      "utterance": [
        {
          "id": "MDRW2100030328.1.1",
          "form": "그럼 된거죠 머",
          "original_form": "그럼 된거죠 머",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030328.1.2",
          "form": "잘 안싸우시죠?",
          "original_form": "잘 안싸우시죠?",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030328.1.3",
          "form": "부부싸움은 칼로 물베기랬는데",
          "original_form": "부부싸움은 칼로 물베기랬는데",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030328.1.4",
          "form": "잘싸워요",
          "original_form": "잘싸워요",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030328.1.5",
          "form": "오지게 싸웁니다",
          "original_form": "오지게 싸웁니다",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030328.1.6",
          "form": "오지게요?ㅋㅋㅋ",
          "original_form": "오지게요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030328.1.7",
          "form": "싸우면서 정드는거죠",
          "original_form": "싸우면서 정드는거죠",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030328.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030328.1.9",
          "form": "네ㅋㅋ 싸우면서 정들어서\r\n연애하게되고 결혼까지 했네여",
          "original_form": "네ㅋㅋ 싸우면서 정들어서\r\n연애하게되고 결혼까지 했네여",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030328.1.10",
          "form": "동갑을 한번도 안만나봤어요",
          "original_form": "동갑을 한번도 안만나봤어요",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030328.1.11",
          "form": "한살 연하까지만 만나봄",
          "original_form": "한살 연하까지만 만나봄",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030328.1.12",
          "form": "연상은요? ㅋㅋㅋㅋㅋ",
          "original_form": "연상은요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030328.1.13",
          "form": "연상두요",
          "original_form": "연상두요",
          "speaker_id": "2",
          "time": "20210904 08:10"
        },
        {
          "id": "MDRW2100030328.1.14",
          "form": "전 그렇게 안엮여서 ㅋㅋㅋ",
          "original_form": "전 그렇게 안엮여서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 08:11"
        },
        {
          "id": "MDRW2100030328.1.15",
          "form": "연하킬러셨구낭 이 분 ㅋㅋ",
          "original_form": "연하킬러셨구낭 이 분 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 09:27"
        },
        {
          "id": "MDRW2100030328.1.16",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030328.1.17",
          "form": "킬러라뇨",
          "original_form": "킬러라뇨",
          "speaker_id": "2",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030328.1.18",
          "form": "그런 무서운말을",
          "original_form": "그런 무서운말을",
          "speaker_id": "2",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030328.1.19",
          "form": "그럼 헌터? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그럼 헌터? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030328.1.20",
          "form": "전 그냥 연하들이 좋아하는 남자일뿐",
          "original_form": "전 그냥 연하들이 좋아하는 남자일뿐",
          "speaker_id": "2",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030328.1.21",
          "form": "풉ㅋㅋㅋㅋ미티겠다 지짜",
          "original_form": "풉ㅋㅋㅋㅋ미티겠다 지짜",
          "speaker_id": "1",
          "time": "20210904 10:40"
        },
        {
          "id": "MDRW2100030328.1.22",
          "form": "name2님 캐릭 컨셉 잡으신겁니까ㅋㅋ",
          "original_form": "&name2&님 캐릭 컨셉 잡으신겁니까ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:40"
        },
        {
          "id": "MDRW2100030328.1.23",
          "form": "사실인데",
          "original_form": "사실인데",
          "speaker_id": "2",
          "time": "20210904 10:41"
        },
        {
          "id": "MDRW2100030328.1.24",
          "form": "왜 웃으십니까",
          "original_form": "왜 웃으십니까",
          "speaker_id": "2",
          "time": "20210904 10:41"
        },
        {
          "id": "MDRW2100030328.1.25",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 10:41"
        },
        {
          "id": "MDRW2100030328.1.26",
          "form": "ㅋㅋㅋ아쉽네요\r\n제가 유부라 검증할 길이 없단게",
          "original_form": "ㅋㅋㅋ아쉽네요\r\n제가 유부라 검증할 길이 없단게",
          "speaker_id": "1",
          "time": "20210904 10:42"
        },
        {
          "id": "MDRW2100030328.1.27",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210904 10:42"
        },
        {
          "id": "MDRW2100030328.1.28",
          "form": "하지만",
          "original_form": "하지만",
          "speaker_id": "2",
          "time": "20210904 10:43"
        },
        {
          "id": "MDRW2100030328.1.29",
          "form": "느끼고 계시죠? 제가 좋은 남자라는걸",
          "original_form": "느끼고 계시죠? 제가 좋은 남자라는걸",
          "speaker_id": "2",
          "time": "20210904 10:43"
        },
        {
          "id": "MDRW2100030328.1.30",
          "form": "하핫",
          "original_form": "하핫",
          "speaker_id": "2",
          "time": "20210904 10:43"
        },
        {
          "id": "MDRW2100030328.1.31",
          "form": "하핫 이것도 인정ㅋㅋㅋㅋㅋ",
          "original_form": "하핫 이것도 인정ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:43"
        },
        {
          "id": "MDRW2100030328.1.32",
          "form": "쑥스럽구만유",
          "original_form": "쑥스럽구만유",
          "speaker_id": "2",
          "time": "20210904 10:43"
        },
        {
          "id": "MDRW2100030328.1.33",
          "form": "제가 직접....",
          "original_form": "제가 직접....",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100030328.1.34",
          "form": "죄송해유 ㅋㅋ",
          "original_form": "죄송해유 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100030328.1.35",
          "form": "센스있게 선칭찬\r\n해드렸어야 했는데 ㅋㅋㅋㅋ",
          "original_form": "센스있게 선칭찬\r\n해드렸어야 했는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100030328.1.36",
          "form": "뭐가요?",
          "original_form": "뭐가요?",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100030328.1.37",
          "form": "지금 웃고있죠?",
          "original_form": "지금 웃고있죠?",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100030328.1.38",
          "form": "아니 그럼 웁니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 그럼 웁니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
