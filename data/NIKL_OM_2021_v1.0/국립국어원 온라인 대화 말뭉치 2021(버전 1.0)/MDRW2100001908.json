      "utterance": [
        {
          "id": "MDRW2100001908.1.1",
          "form": "안녕하세요%",
          "original_form": "안녕하세요%",
          "speaker_id": "1",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001908.1.2",
          "form": "안녕하세요 가장 좋아하은 계절은 뭐세요??",
          "original_form": "안녕하세요 가장 좋아하은 계절은 뭐세요??",
          "speaker_id": "2",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001908.1.3",
          "form": "가을이요~ 제생일이 있어서요ㅎㅎ",
          "original_form": "가을이요~ 제생일이 있어서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001908.1.4",
          "form": "무슨계절 좋아하세요?",
          "original_form": "무슨계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001908.1.5",
          "form": "어 저도 가을 좋아해요 담달에 생일이랔ㅋㅋㅋㅋ",
          "original_form": "어 저도 가을 좋아해요 담달에 생일이랔ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001908.1.6",
          "form": "가을이 짱이죵 요즘 저녁에 시원해서 넘 조ㅗ아요",
          "original_form": "가을이 짱이죵 요즘 저녁에 시원해서 넘 조ㅗ아요",
          "speaker_id": "2",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001908.1.7",
          "form": "어머 저도 9월이요ㅋㅋ",
          "original_form": "어머 저도 9월이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001908.1.8",
          "form": "에어컨 꺼도 지낼만하네요~",
          "original_form": "에어컨 꺼도 지낼만하네요~",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001908.1.9",
          "form": "그쵸 저번달엔 정말 에어컨 없이는 살 수 없었는데",
          "original_form": "그쵸 저번달엔 정말 에어컨 없이는 살 수 없었는데",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001908.1.10",
          "form": "가을 빼고",
          "original_form": "가을 빼고",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001908.1.11",
          "form": "겨울과 여름중엔 뭐가 더 좋으세요?",
          "original_form": "겨울과 여름중엔 뭐가 더 좋으세요?",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001908.1.12",
          "form": "겨울이요ㅎ",
          "original_form": "겨울이요ㅎ",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001908.1.13",
          "form": "니트입는거 좋아해서요",
          "original_form": "니트입는거 좋아해서요",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001908.1.14",
          "form": "오 겨울에 코트와 롱패딩중에 뭘 자주 입르시나요?",
          "original_form": "오 겨울에 코트와 롱패딩중에 뭘 자주 입르시나요?",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001908.1.15",
          "form": "롱패딩이요~ 뚜벅이라서 추위에 강한게 필요하네요",
          "original_form": "롱패딩이요~ 뚜벅이라서 추위에 강한게 필요하네요",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001908.1.16",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001908.1.17",
          "form": "저도 롱패딩이요 저는 추운걸 정말정말 싫어해서",
          "original_form": "저도 롱패딩이요 저는 추운걸 정말정말 싫어해서",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100001908.1.18",
          "form": "모든 냉기를 막을 수 있어야 하거든요",
          "original_form": "모든 냉기를 막을 수 있어야 하거든요",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100001908.1.19",
          "form": "코트는 하나뿐인데 롱패딩은 한 네개 있는 것 같아요",
          "original_form": "코트는 하나뿐인데 롱패딩은 한 네개 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100001908.1.20",
          "form": "기상 이변에 대해 어떻게 생각하시나요",
          "original_form": "기상 이변에 대해 어떻게 생각하시나요",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100001908.1.21",
          "form": "롱패딩 4개라니 많으시네요",
          "original_form": "롱패딩 4개라니 많으시네요",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100001908.1.22",
          "form": "기상이변은 심각하죠ㅜ",
          "original_form": "기상이변은 심각하죠ㅜ",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100001908.1.23",
          "form": "기상이변을 극복하기위해 사소하게 노력하는게 잇으세요?",
          "original_form": "기상이변을 극복하기위해 사소하게 노력하는게 잇으세요?",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100001908.1.24",
          "form": "저는 배달음식.줄이기 이런거..... 텀블러나..",
          "original_form": "저는 배달음식.줄이기 이런거..... 텀블러나..",
          "speaker_id": "2",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100001908.1.25",
          "form": "저두요 배달거의 안먹어요",
          "original_form": "저두요 배달거의 안먹어요",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001908.1.26",
          "form": "일회용품 안쓰려고 노력하구요",
          "original_form": "일회용품 안쓰려고 노력하구요",
          "speaker_id": "1",
          "time": "20210819 22:54"
        }
      ]
