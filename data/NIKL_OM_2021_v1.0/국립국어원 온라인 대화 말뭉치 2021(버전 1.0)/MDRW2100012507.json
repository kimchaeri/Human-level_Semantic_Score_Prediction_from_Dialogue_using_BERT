      "utterance": [
        {
          "id": "MDRW2100012507.1.1",
          "form": "학교 다니고 계시너용",
          "original_form": "학교 다니고 계시너용",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012507.1.2",
          "form": "아니염.. 학교졸업한지? 10몇년된듯.. ㅋㅋㅋ",
          "original_form": "아니염.. 학교졸업한지? 10몇년된듯.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012507.1.3",
          "form": "학생이세여?",
          "original_form": "학생이세여?",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012507.1.4",
          "form": "저도 아니에요ㅋㅋㅋㅋㅋ",
          "original_form": "저도 아니에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012507.1.5",
          "form": "학교 다닐 때 스트레스 뭐였어요?",
          "original_form": "학교 다닐 때 스트레스 뭐였어요?",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012507.1.6",
          "form": "저는 없었어요..인기도 많고 장학금도 받고 ㅍㅎㅎㅎㅎ",
          "original_form": "저는 없었어요..인기도 많고 장학금도 받고 ㅍㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012507.1.7",
          "form": "모범생이겻나여?",
          "original_form": "모범생이겻나여?",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012507.1.8",
          "form": "셧",
          "original_form": "셧",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100012507.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 헉 인기쟁이에",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 헉 인기쟁이에",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100012507.1.10",
          "form": "공부도 잘했군요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "공부도 잘했군요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100012507.1.11",
          "form": "전 아니요..",
          "original_form": "전 아니요..",
          "speaker_id": "2",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100012507.1.12",
          "form": "그 있자나여.. 적당히 못생기지 않고 적당한 성격이라.. 적당히 말걸기 쉬운.. 그런 타입.. ㅋㅋ",
          "original_form": "그 있자나여.. 적당히 못생기지 않고 적당한 성격이라.. 적당히 말걸기 쉬운.. 그런 타입.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100012507.1.13",
          "form": "공주 안좋아하셧구나..",
          "original_form": "공주 안좋아하셧구나..",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100012507.1.14",
          "form": "공부",
          "original_form": "공부",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100012507.1.15",
          "form": "F도 받아보셕나여?",
          "original_form": "F도 받아보셕나여?",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012507.1.16",
          "form": "네ㅋㅋㅋㅋㅋㅋㅋ 공부 안좋아했어요",
          "original_form": "네ㅋㅋㅋㅋㅋㅋㅋ 공부 안좋아했어요",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012507.1.17",
          "form": "그냥저냥한...",
          "original_form": "그냥저냥한...",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012507.1.18",
          "form": "아니요....",
          "original_form": "아니요....",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012507.1.19",
          "form": "ㅋㅋ 아 그럼 공부 잘 하셧네여",
          "original_form": "ㅋㅋ 아 그럼 공부 잘 하셧네여",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012507.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ f가 기준이신가요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ f가 기준이신가요?",
          "speaker_id": "2",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012507.1.21",
          "form": "f는 안받아보셨죠?",
          "original_form": "f는 안받아보셨죠?",
          "speaker_id": "2",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012507.1.22",
          "form": "전 b+이하는 받아본적 없어요..ㅋㅋㅋ",
          "original_form": "전 b+이하는 받아본적 없어요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100012507.1.23",
          "form": "자격증 같은거도 많이 공부하셧나용?",
          "original_form": "자격증 같은거도 많이 공부하셧나용?",
          "speaker_id": "1",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100012507.1.24",
          "form": "열심히 하셨나봐요",
          "original_form": "열심히 하셨나봐요",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012507.1.25",
          "form": "아뇨 전 그냥 노는데 ㅎ ㅎ...",
          "original_form": "아뇨 전 그냥 노는데 ㅎ ㅎ...",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012507.1.26",
          "form": "졸업하고 땄어요",
          "original_form": "졸업하고 땄어요",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012507.1.27",
          "form": "전 분기마다 자격증따면 돈줘서.. ㅋㅋㅋ",
          "original_form": "전 분기마다 자격증따면 돈줘서.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012507.1.28",
          "form": "알바처럼 쉬운거만 골라서 땄죠.. ㅋㅋ",
          "original_form": "알바처럼 쉬운거만 골라서 땄죠.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012507.1.29",
          "form": "어 저희 학교도 그랬어오ㅋㅋㅋㅋㅋ",
          "original_form": "어 저희 학교도 그랬어오ㅋㅋㅋㅋㅋ",
