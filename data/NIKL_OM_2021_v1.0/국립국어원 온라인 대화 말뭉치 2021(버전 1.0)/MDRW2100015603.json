      "utterance": [
        {
          "id": "MDRW2100015603.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015603.1.2",
          "form": "아니요 졸업했어요",
          "original_form": "아니요 졸업했어요",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015603.1.3",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015603.1.4",
          "form": "앗 네! 전 아직 학생이에요ㅎㅎ",
          "original_form": "앗 네! 전 아직 학생이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015603.1.5",
          "form": "선배님이셨네요",
          "original_form": "선배님이셨네요",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015603.1.6",
          "form": "저 담주에 개강하는데ㅠ 부럽씁늬다...",
          "original_form": "저 담주에 개강하는데ㅠ 부럽씁늬다...",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015603.1.7",
          "form": "벌써 다음주예요?",
          "original_form": "벌써 다음주예요?",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015603.1.8",
          "form": "네에ㅠ",
          "original_form": "네에ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015603.1.9",
          "form": "수강신청 하셨겠다",
          "original_form": "수강신청 하셨겠다",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015603.1.10",
          "form": "했죠...",
          "original_form": "했죠...",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015603.1.11",
          "form": "금공강은 만들었는데",
          "original_form": "금공강은 만들었는데",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015603.1.12",
          "form": "들어야하는 수업이 10시인가 11시 시작...ㅠㅠㅠㅠ",
          "original_form": "들어야하는 수업이 10시인가 11시 시작...ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015603.1.13",
          "form": "앗.....",
          "original_form": "앗.....",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015603.1.14",
          "form": "통학시간이 긴가요?",
          "original_form": "통학시간이 긴가요?",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015603.1.15",
          "form": "움...전 기숙사라 우주공강이어도 괜찮긴 한데요...",
          "original_form": "움...전 기숙사라 우주공강이어도 괜찮긴 한데요...",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015603.1.16",
          "form": "점심쯤엔 거의 평균 3시간? 정도 비어있어요ㅋㅋㅋ",
          "original_form": "점심쯤엔 거의 평균 3시간? 정도 비어있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015603.1.17",
          "form": "그래도 기숙사라서 다행이네요!",
          "original_form": "그래도 기숙사라서 다행이네요!",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015603.1.18",
          "form": "저도 공강긴적있었는데 4시간인가 그랬어요",
          "original_form": "저도 공강긴적있었는데 4시간인가 그랬어요",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015603.1.19",
          "form": "하지만 저는 통학이였죠...",
          "original_form": "하지만 저는 통학이였죠...",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015603.1.20",
          "form": "아앗..........",
          "original_form": "아앗..........",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015603.1.21",
          "form": "강제 학교 지박령이 되셨었네요....",
          "original_form": "강제 학교 지박령이 되셨었네요....",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015603.1.22",
          "form": "전 그 친구보면서 웃는 역할이었는데",
          "original_form": "전 그 친구보면서 웃는 역할이었는데",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015603.1.23",
          "form": "가끔은 집에 왔다갔어요",
          "original_form": "가끔은 집에 왔다갔어요",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015603.1.24",
          "form": "반성하겠습니다",
          "original_form": "반성하겠습니다",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015603.1.25",
          "form": "앜ㅋㄲㄱㅋㄱㅋㅋ",
          "original_form": "앜ㅋㄲㄱㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015603.1.26",
          "form": "나쁜사람!",
          "original_form": "나쁜사람!",
          "speaker_id": "1",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015603.1.27",
          "form": "ㅠㅠ기숙사 합격하든가ㅠ 에벱베",
          "original_form": "ㅠㅠ기숙사 합격하든가ㅠ 에벱베",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015603.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015603.1.29",
          "form": "이 역할 저에게 딱이었습니다....",
          "original_form": "이 역할 저에게 딱이었습니다....",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015603.1.30",
          "form": "ㄲㅋㅋㅋㄲ 근데 진짜 기숙사생 부러웠어요",
          "original_form": "ㄲㅋㅋㅋㄲ 근데 진짜 기숙사생 부러웠어요",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015603.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ근데 진짜 학교 지박령은",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ근데 진짜 학교 지박령은",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015603.1.32",
          "form": "기숙사생이라는 사실...",
          "original_form": "기숙사생이라는 사실...",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015603.1.33",
          "form": "뭐 대학교는 거의 다 오르막길이고 산에 있지만",
          "original_form": "뭐 대학교는 거의 다 오르막길이고 산에 있지만",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015603.1.34",
          "form": "저희 학교가 산에 있어서ㅠㅋㅋㅋㅋㅋㅋ 시설좋은 감옥같았어요",
          "original_form": "저희 학교가 산에 있어서ㅠㅋㅋㅋㅋㅋㅋ 시설좋은 감옥같았어요",
          "speaker_id": "2",
          "time": "20210825 23:52"
        }
      ]
