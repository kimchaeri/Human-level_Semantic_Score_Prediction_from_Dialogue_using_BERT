        {
          "id": "MDRW2100017470.1.1",
          "form": "대만 언제갈수있을까... 간절하네",
          "original_form": "대만 언제갈수있을까... 간절하네",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017470.1.2",
          "form": "정말 꼭 가고싶은데...",
          "original_form": "정말 꼭 가고싶은데...",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017470.1.3",
          "form": "가면 허관한 만날수있는거가?",
          "original_form": "가면 허관한 만날수있는거가?",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017470.1.4",
          "form": "허광한~ㅋㅋㅋㅋ대스타라서ㅜ만날 기회를~~",
          "original_form": "허광한~ㅋㅋㅋㅋ대스타라서ㅜ만날 기회를~~",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017470.1.5",
          "form": "대만만 간다고해서 볼수는 없잖아",
          "original_form": "대만만 간다고해서 볼수는 없잖아",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017470.1.6",
          "form": "대만은 도시투어야?",
          "original_form": "대만은 도시투어야?",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100017470.1.7",
          "form": "한국이랑 비슷 대중교통타고 이동 가능",
          "original_form": "한국이랑 비슷 대중교통타고 이동 가능",
          "speaker_id": "1",
          "time": "20210827 06:19"
        },
        {
          "id": "MDRW2100017470.1.8",
          "form": "도시가....한국이랑 비슷하다고?",
          "original_form": "도시가....한국이랑 비슷하다고?",
          "speaker_id": "2",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100017470.1.9",
          "form": "느낌이 비슷해 안전한 도시느낌이야",
          "original_form": "느낌이 비슷해 안전한 도시느낌이야",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100017470.1.10",
          "form": "우리나라보다도 더 안전하다고?",
          "original_form": "우리나라보다도 더 안전하다고?",
          "speaker_id": "2",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100017470.1.11",
          "form": "그런 느낌이야~돌아다녀도 괜찮은",
          "original_form": "그런 느낌이야~돌아다녀도 괜찮은",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100017470.1.12",
          "form": "아..가보고싶네",
          "original_form": "아..가보고싶네",
          "speaker_id": "2",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100017470.1.13",
          "form": "진짜 가보고싶다~",
          "original_form": "진짜 가보고싶다~",
          "speaker_id": "1",
          "time": "20210827 08:02"
        },
        {
          "id": "MDRW2100017470.1.14",
          "form": "대만이랑 홍콩이랑 비슷한느낌이겠다",
          "original_form": "대만이랑 홍콩이랑 비슷한느낌이겠다",
          "speaker_id": "2",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100017470.1.15",
          "form": "대만이나 가고싶다 혼자라도",
          "original_form": "대만이나 가고싶다 혼자라도",
          "speaker_id": "1",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100017470.1.16",
          "form": "이제 나혼자라도 여유지게 삵거다",
