      "utterance": [
        {
          "id": "MDRW2100023203.1.1",
          "form": "그렇게 어그로를 끄는 디자인이면",
          "original_form": "그렇게 어그로를 끄는 디자인이면",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023203.1.2",
          "form": "기억 안날리 없는데",
          "original_form": "기억 안날리 없는데",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023203.1.3",
          "form": "걔 왜?",
          "original_form": "걔 왜?",
          "speaker_id": "2",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023203.1.4",
          "form": "아직 거기까지 진행 안했나보다",
          "original_form": "아직 거기까지 진행 안했나보다",
          "speaker_id": "1",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023203.1.5",
          "form": "ㄹㅇ 대충그린것도 대충 그린건데",
          "original_form": "ㄹㅇ 대충그린것도 대충 그린건데",
          "speaker_id": "1",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023203.1.6",
          "form": "그림체가 거의",
          "original_form": "그림체가 거의",
          "speaker_id": "1",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023203.1.7",
          "form": "ㄹㅇ??",
          "original_form": "ㄹㅇ??",
          "speaker_id": "2",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023203.1.8",
          "form": "수메급으로 바뀜",
          "original_form": "수메급으로 바뀜",
          "speaker_id": "1",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023203.1.9",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023203.1.10",
          "form": "ㅇㅏ",
          "original_form": "ㅇㅏ",
          "speaker_id": "2",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023203.1.11",
          "form": "근데 문제가 뭔지 아니",
          "original_form": "근데 문제가 뭔지 아니",
          "speaker_id": "1",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023203.1.12",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023203.1.13",
          "form": "그거 텀블벅 50만원 후원 보상임 ㅋㅋㅋㅋㅋ",
          "original_form": "그거 텀블벅 50만원 후원 보상임 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023203.1.14",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023203.1.15",
          "form": "나도 몰랐는데",
          "original_form": "나도 몰랐는데",
          "speaker_id": "1",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023203.1.16",
          "form": "얘네가 정식 런칭 전에 그린 홍보만화에",
          "original_form": "얘네가 정식 런칭 전에 그린 홍보만화에",
          "speaker_id": "1",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023203.1.17",
          "form": "이 캐릭터를 언급해놔서 보고 알아차림",
          "original_form": "이 캐릭터를 언급해놔서 보고 알아차림",
          "speaker_id": "1",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023203.1.18",
          "form": "아니 사기꾼 아냐",
          "original_form": "아니 사기꾼 아냐",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023203.1.19",
          "form": "어디든",
          "original_form": "어디든",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023203.1.20",
          "form": "홍보는 스탠딩으로 하고",
          "original_form": "홍보는 스탠딩으로 하고",
          "speaker_id": "1",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023203.1.21",
          "form": "여성향 물로보는건",
          "original_form": "여성향 물로보는건",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023203.1.22",
          "form": "보상을 이꼴로 그려놓으면 어떡하냐고",
          "original_form": "보상을 이꼴로 그려놓으면 어떡하냐고",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023203.1.23",
          "form": "바뀌질 않는듯ㅋㅋ....",
          "original_form": "바뀌질 않는듯ㅋㅋ....",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023203.1.24",
          "form": "그니까 ㅋㅋ....",
          "original_form": "그니까 ㅋㅋ....",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023203.1.25",
          "form": "여성향 게임 좀 잘 좀 만들어보라고...",
          "original_form": "여성향 게임 좀 잘 좀 만들어보라고...",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023203.1.26",
          "form": "ㅇㅇ후원을",
          "original_form": "ㅇㅇ후원을",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023203.1.27",
          "form": "1억이나 해줘도",
          "original_form": "1억이나 해줘도",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023203.1.28",
          "form": "이지경이네",
          "original_form": "이지경이네",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023203.1.29",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023203.1.30",
          "form": "수메처럼 벌고싶으면",
          "original_form": "수메처럼 벌고싶으면",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023203.1.31",
          "form": "최소한 엔딩이나 내...",
          "original_form": "최소한 엔딩이나 내...",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023203.1.32",
          "form": "솔직히 투자금 1억들고 인디겜이라고 말하면",
          "original_form": "솔직히 투자금 1억들고 인디겜이라고 말하면",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023203.1.33",
          "form": "ㄹㅇ양심없는듯",
          "original_form": "ㄹㅇ양심없는듯",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023203.1.34",
          "form": "ㅇㅇ 글고 팀원도",
          "original_form": "ㅇㅇ 글고 팀원도",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023203.1.35",
          "form": "7명이나 되던데",
          "original_form": "7명이나 되던데",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023203.1.36",
          "form": "보통 인디는 4인 이하가",
          "original_form": "보통 인디는 4인 이하가",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023203.1.37",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023203.1.38",
          "form": "본업 따로있고 팀으로 모여서 만드는",
          "original_form": "본업 따로있고 팀으로 모여서 만드는",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023203.1.39",
          "form": "열악한 환경인거 감안해주는거지",
          "original_form": "열악한 환경인거 감안해주는거지",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023203.1.40",
          "form": "ㅇㅇ그것도 보통 취미로 하는거지",
          "original_form": "ㅇㅇ그것도 보통 취미로 하는거지",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023203.1.41",
          "form": "그걸로 돈벌어먹으려고는 잘 안하는데",
          "original_form": "그걸로 돈벌어먹으려고는 잘 안하는데",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023203.1.42",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023203.1.43",
          "form": "컨텐츠 있는거라곤 스토리밖에 없으면서",
          "original_form": "컨텐츠 있는거라곤 스토리밖에 없으면서",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023203.1.44",
          "form": "그거하나도 똑바로 못하는꼴 보고잇ㅈ으면",
          "original_form": "그거하나도 똑바로 못하는꼴 보고잇ㅈ으면",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023203.1.45",
          "form": "좀 짜증나ㅋㅋ;;",
          "original_form": "좀 짜증나ㅋㅋ;;",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023203.1.46",
          "form": "그니까 시장조사도 안한건가?",
          "original_form": "그니까 시장조사도 안한건가?",
          "speaker_id": "1",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023203.1.47",
          "form": "아ㅎ이래서",
          "original_form": "아ㅎ이래서",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023203.1.48",
          "form": "당장 비슷한 계열 게임중에서",
          "original_form": "당장 비슷한 계열 게임중에서",
          "speaker_id": "1",
          "time": "20210829 22:56"
        }
      ]
