      "utterance": [
        {
          "id": "MDRW2100017282.1.1",
          "form": "난사실 물건 이런거저런거말고",
          "original_form": "난사실 물건 이런거저런거말고",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017282.1.2",
          "form": "넌 중고거래 어떻게 생각해?",
          "original_form": "넌 중고거래 어떻게 생각해?",
          "speaker_id": "2",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017282.1.3",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017282.1.4",
          "form": "집을 제일 사고싶다",
          "original_form": "집을 제일 사고싶다",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017282.1.5",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017282.1.6",
          "form": "넘사벽이다",
          "original_form": "넘사벽이다",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017282.1.7",
          "form": "갑자기 서글픈데",
          "original_form": "갑자기 서글픈데",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017282.1.8",
          "form": "뭐가 서글퍼",
          "original_form": "뭐가 서글퍼",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017282.1.9",
          "form": "살수있지",
          "original_form": "살수있지",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017282.1.10",
          "form": "야망을가져",
          "original_form": "야망을가져",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017282.1.11",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017282.1.12",
          "form": "나는 20평초반 정도에",
          "original_form": "나는 20평초반 정도에",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017282.1.13",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017282.1.14",
          "form": "방은 작게 3개있엇으면좋겠음",
          "original_form": "방은 작게 3개있엇으면좋겠음",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017282.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017282.1.16",
          "form": "난 안방처럼 큰방에 서재 만들고",
          "original_form": "난 안방처럼 큰방에 서재 만들고",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017282.1.17",
          "form": "침실하나 서재겸 공부하는방하나",
          "original_form": "침실하나 서재겸 공부하는방하나",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017282.1.18",
          "form": "작은방에 침대놓고",
          "original_form": "작은방에 침대놓고",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017282.1.19",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017282.1.20",
          "form": "난 하나는 드레스룸아니면 손님방",
          "original_form": "난 하나는 드레스룸아니면 손님방",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017282.1.21",
          "form": "드레스 룸은",
          "original_form": "드레스 룸은",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017282.1.22",
          "form": "안방에 붙어있는 아파트로 가자",
          "original_form": "안방에 붙어있는 아파트로 가자",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017282.1.23",
          "form": "침실에 딸려있으려나?",
          "original_form": "침실에 딸려있으려나?",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017282.1.24",
          "form": "ㅋㅋㅋㅋㅋ그래",
          "original_form": "ㅋㅋㅋㅋㅋ그래",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017282.1.25",
          "form": "그리고 손님방은",
          "original_form": "그리고 손님방은",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017282.1.26",
          "form": "뭐로 하지",
          "original_form": "뭐로 하지",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017282.1.27",
          "form": "너 고양이 키울꺼니까",
          "original_form": "너 고양이 키울꺼니까",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017282.1.28",
          "form": "고양이 방으로 해라",
          "original_form": "고양이 방으로 해라",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017282.1.29",
          "form": "난 강아지 방으로 할게",
          "original_form": "난 강아지 방으로 할게",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017282.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
