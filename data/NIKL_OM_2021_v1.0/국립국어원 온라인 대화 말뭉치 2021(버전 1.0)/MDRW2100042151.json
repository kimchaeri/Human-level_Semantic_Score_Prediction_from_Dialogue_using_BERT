      "utterance": [
        {
          "id": "MDRW2100042151.1.1",
          "form": "혹시 반려동물 키우시나요?",
          "original_form": "혹시 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100042151.1.2",
          "form": "네 고양이 키워요!",
          "original_form": "네 고양이 키워요!",
          "speaker_id": "2",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042151.1.3",
          "form": "헉 정말요ㅠㅠㅜㅜㅠ진짜 너무 부러워요ㅠㅠㅠ",
          "original_form": "헉 정말요ㅠㅠㅜㅜㅠ진짜 너무 부러워요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042151.1.4",
          "form": "저 자랑 좀 해주세요",
          "original_form": "저 자랑 좀 해주세요",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042151.1.5",
          "form": "아ㅋ큐ㅠㅠㅠ 애기... 한 5개월쯤 된 코숏이에요",
          "original_form": "아ㅋ큐ㅠㅠㅠ 애기... 한 5개월쯤 된 코숏이에요",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042151.1.6",
          "form": "엄청 착하고 귀여워요",
          "original_form": "엄청 착하고 귀여워요",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042151.1.7",
          "form": "헉 진짜 애기ㅠㅜㅠㅠㅠㅠ",
          "original_form": "헉 진짜 애기ㅠㅜㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042151.1.8",
          "form": "너무 귀엽겠다ㅜㅠㅜㅠ 혹시 혼자 키우세요?",
          "original_form": "너무 귀엽겠다ㅜㅠㅜㅠ 혹시 혼자 키우세요?",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042151.1.9",
          "form": "가족들이랑 사는데 제가 거의 키우긴 해요!",
          "original_form": "가족들이랑 사는데 제가 거의 키우긴 해요!",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042151.1.10",
          "form": "name2님이 엄마네요!!ㅠㅠ 이번에 처음 입양하신거에요?",
          "original_form": "&name2&님이 엄마네요!!ㅠㅠ 이번에 처음 입양하신거에요?",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042151.1.11",
          "form": "네네 처음이라 좀 걱정했는데 이것저것 신경쓰다보니까 익숙해지더라구요",
          "original_form": "네네 처음이라 좀 걱정했는데 이것저것 신경쓰다보니까 익숙해지더라구요",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042151.1.12",
          "form": "잠도 같이 자고 너무 귀여워요...",
          "original_form": "잠도 같이 자고 너무 귀여워요...",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042151.1.13",
          "form": "저도 나중에 꼭 고양이 키워보고싶어서 조금씩 공부하고 있는데 아직 걱정이 더 커요ㅠㅠ",
          "original_form": "저도 나중에 꼭 고양이 키워보고싶어서 조금씩 공부하고 있는데 아직 걱정이 더 커요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042151.1.14",
          "form": "악 진짜 천사...",
          "original_form": "악 진짜 천사...",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042151.1.15",
          "form": "심장 아파요ㅠㅠ",
          "original_form": "심장 아파요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042151.1.16",
          "form": "ㅋㅋ큐ㅠ 뭐가 제일 걱정되세요?",
          "original_form": "ㅋㅋ큐ㅠ 뭐가 제일 걱정되세요?",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042151.1.17",
          "form": "저는 직장 다니면 혼자서 키울 거 같은데 그럼 애가 혼자 있는 시간이 길 거 같아서요ㅠㅠ",
          "original_form": "저는 직장 다니면 혼자서 키울 거 같은데 그럼 애가 혼자 있는 시간이 길 거 같아서요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042151.1.18",
          "form": "에구... 그럴 수 있죠 집에 늦게 들어오세요??",
          "original_form": "에구... 그럴 수 있죠 집에 늦게 들어오세요??",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042151.1.19",
          "form": "지금 직장은 괜찮은데",
          "original_form": "지금 직장은 괜찮은데",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042151.1.20",
          "form": "곧 이직할 곳이 야근을 자주 할 거 같아요",
          "original_form": "곧 이직할 곳이 야근을 자주 할 거 같아요",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042151.1.21",
          "form": "힘드시겠어요...",
          "original_form": "힘드시겠어요...",
          "speaker_id": "2",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100042151.1.22",
          "form": "애기 혼자 두는 시간이 너무 길면 아무래도 좀 그렇죠...",
          "original_form": "애기 혼자 두는 시간이 너무 길면 아무래도 좀 그렇죠...",
          "speaker_id": "2",
          "time": "20210906 20:40"
        },
        {
          "id": "MDRW2100042151.1.23",
          "form": "그쵸.. 그래서 프리랜서 분들이 고양이 키우시는 거 볼 때 부러워요",
          "original_form": "그쵸.. 그래서 프리랜서 분들이 고양이 키우시는 거 볼 때 부러워요",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100042151.1.24",
          "form": "재택근무하시는 분들",
          "original_form": "재택근무하시는 분들",
