      "utterance": [
        {
          "id": "MDRW2100021733.1.1",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021733.1.2",
          "form": "네 된장찌개 먹었어요",
          "original_form": "네 된장찌개 먹었어요",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021733.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021733.1.4",
          "form": "저는 돼지갈비에 냉면 먹었어요",
          "original_form": "저는 돼지갈비에 냉면 먹었어요",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021733.1.5",
          "form": "와 제대로 드셨네요",
          "original_form": "와 제대로 드셨네요",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021733.1.6",
          "form": "외식하셨나요",
          "original_form": "외식하셨나요",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021733.1.7",
          "form": "네 친구랑 둘이서 호로록 먹고 왔슴당ㅋㅋ",
          "original_form": "네 친구랑 둘이서 호로록 먹고 왔슴당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021733.1.8",
          "form": "ㅎㅎ 면요리 좋아하시나봐요",
          "original_form": "ㅎㅎ 면요리 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021733.1.9",
          "form": "면을 더 좋아하는 건 아니고 대부분의 음식을 좋아해요ㅋㅋㅋㅋ",
          "original_form": "면을 더 좋아하는 건 아니고 대부분의 음식을 좋아해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021733.1.10",
          "form": "물론 면도 좋아합니다",
          "original_form": "물론 면도 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021733.1.11",
          "form": "굳이 따지면 밥을 더 좋아해요",
          "original_form": "굳이 따지면 밥을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021733.1.12",
          "form": "닉네임이 장칼국수인 이유는 그냥 그때그때 생각나는 음식으로 닉네임을 정해요ㅋㅋ",
          "original_form": "닉네임이 장칼국수인 이유는 그냥 그때그때 생각나는 음식으로 닉네임을 정해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021733.1.13",
          "form": "ㅎㅎ 아 그렇군요",
          "original_form": "ㅎㅎ 아 그렇군요",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021733.1.14",
          "form": "닉네임보니 얼큰한 장칼국수가 당기네요",
          "original_form": "닉네임보니 얼큰한 장칼국수가 당기네요",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021733.1.15",
          "form": "장칼국수 맛있죠",
          "original_form": "장칼국수 맛있죠",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021733.1.16",
          "form": "가장 좋아하는 음식은 뭔가요",
          "original_form": "가장 좋아하는 음식은 뭔가요",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021733.1.17",
          "form": "음 김치찌개랑 떡볶이가 엎치락뒤치락해요ㅋㅋ",
          "original_form": "음 김치찌개랑 떡볶이가 엎치락뒤치락해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021733.1.18",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021733.1.19",
          "form": "ㅎㅎ 전 해산물이요!",
          "original_form": "ㅎㅎ 전 해산물이요!",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021733.1.20",
          "form": "회라면 다 좋아해요",
          "original_form": "회라면 다 좋아해요",
