      "utterance": [
        {
          "id": "MDRW2100005552.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005552.1.2",
          "form": "안녕하세요~  오늘 거긴 날씨 어땠어요?",
          "original_form": "안녕하세요~  오늘 거긴 날씨 어땠어요?",
          "speaker_id": "2",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005552.1.3",
          "form": "오늘 낮에 비가 심하게 내리긴 하더라고요",
          "original_form": "오늘 낮에 비가 심하게 내리긴 하더라고요",
          "speaker_id": "1",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005552.1.4",
          "form": "저녁즈음에 그치고서 습도가 높고 선선하긴하더라고요",
          "original_form": "저녁즈음에 그치고서 습도가 높고 선선하긴하더라고요",
          "speaker_id": "1",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005552.1.5",
          "form": "여기도 일어나서부터 비가 하루종일 퍼부었어요",
          "original_form": "여기도 일어나서부터 비가 하루종일 퍼부었어요",
          "speaker_id": "2",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005552.1.6",
          "form": "지금은 비슷한 날씨네요 ㅎㅎ",
          "original_form": "지금은 비슷한 날씨네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005552.1.7",
          "form": "비가 내렸으니 내일부턴 날씨가 좀 시원하질까요?",
          "original_form": "비가 내렸으니 내일부턴 날씨가 좀 시원하질까요?",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005552.1.8",
          "form": "수도권쪽이였는지요?",
          "original_form": "수도권쪽이였는지요?",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005552.1.9",
          "form": "이미 여름은 가고 선선한 날씨이니 시원할겁니다 ㅎㅎ",
          "original_form": "이미 여름은 가고 선선한 날씨이니 시원할겁니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005552.1.10",
          "form": "아니요 저는 더 밑지방에 살고 있어요",
          "original_form": "아니요 저는 더 밑지방에 살고 있어요",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005552.1.11",
          "form": "우와 가을은 대환영이에요",
          "original_form": "우와 가을은 대환영이에요",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005552.1.12",
          "form": "선선하고 하늘도 높아서 좋아하는 계절이거든요",
          "original_form": "선선하고 하늘도 높아서 좋아하는 계절이거든요",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005552.1.13",
          "form": "좋아하는 계절이 있으실까요?",
          "original_form": "좋아하는 계절이 있으실까요?",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005552.1.14",
          "form": "저는 겨울 좋아합니다 ㅎㅎ",
          "original_form": "저는 겨울 좋아합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005552.1.15",
          "form": "가을철이면 옷좀 긴옷으로 입는게 좋겠지요",
          "original_form": "가을철이면 옷좀 긴옷으로 입는게 좋겠지요",
          "speaker_id": "1",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005552.1.16",
          "form": "겨울의 어떤점이 좋으세요? 포근한 옷 입으면 기분이 좋을 거 같아요!",
          "original_form": "겨울의 어떤점이 좋으세요? 포근한 옷 입으면 기분이 좋을 거 같아요!",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005552.1.17",
          "form": "여름은 아무리 얇게 입고 다녀도 더운점이 싫지만",
          "original_form": "여름은 아무리 얇게 입고 다녀도 더운점이 싫지만",
          "speaker_id": "1",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005552.1.18",
          "form": "겨울은 따땃하게 입고 다니기만해도 큰 문제없어서 좋기도하고 겨울철 과일 귤이 많이 나와서 좋아하기도하죠!",
          "original_form": "겨울은 따땃하게 입고 다니기만해도 큰 문제없어서 좋기도하고 겨울철 과일 귤이 많이 나와서 좋아하기도하죠!",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005552.1.19",
          "form": "우와 저도 귤 좋아해요! 그리고 겨울 간식들 매력적인 것 같아요",
          "original_form": "우와 저도 귤 좋아해요! 그리고 겨울 간식들 매력적인 것 같아요",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005552.1.20",
          "form": "붕어빵이나 어묵도 겨울에 더 맛있는 느낌이에요",
          "original_form": "붕어빵이나 어묵도 겨울에 더 맛있는 느낌이에요",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005552.1.21",
          "form": "미세먼지도 없고 눈도 내림에 따라",
          "original_form": "미세먼지도 없고 눈도 내림에 따라",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005552.1.22",
          "form": "그런 매력덕에 겨울을 좋아합니다",
          "original_form": "그런 매력덕에 겨울을 좋아합니다",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005552.1.23",
          "form": "그러면 여름에는 더운거말고 좋아하는점은 무엇이 있으신가요?",
          "original_form": "그러면 여름에는 더운거말고 좋아하는점은 무엇이 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005552.1.24",
          "form": "겨울이 기다려지겠어요",
          "original_form": "겨울이 기다려지겠어요",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005552.1.25",
          "form": "봄은 어때요?",
          "original_form": "봄은 어때요?",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005552.1.26",
          "form": "꽃가루 알레르기 있어서 저에게는 좀 최악의 계절 입니다 ㅠㅠ",
          "original_form": "꽃가루 알레르기 있어서 저에게는 좀 최악의 계절 입니다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 23:08"
        }
      ]
