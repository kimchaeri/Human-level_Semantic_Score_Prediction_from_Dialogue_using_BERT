      "utterance": [
        {
          "id": "MDRW2100043948.1.1",
          "form": "저도 잘 챙겨먹는건 아니지만",
          "original_form": "저도 잘 챙겨먹는건 아니지만",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043948.1.2",
          "form": "매번 먹고 싶어해요 ㅋㅋㄱㄱ",
          "original_form": "매번 먹고 싶어해요 ㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043948.1.3",
          "form": "최근에 이마트에서 샤인머스캣을 샀는데",
          "original_form": "최근에 이마트에서 샤인머스캣을 샀는데",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043948.1.4",
          "form": "가격이 진짜 많이 싸졌더라구요",
          "original_form": "가격이 진짜 많이 싸졌더라구요",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043948.1.5",
          "form": "맞아요 아까 봤는데",
          "original_form": "맞아요 아까 봤는데",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043948.1.6",
          "form": "9600원인가??",
          "original_form": "9600원인가??",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043948.1.7",
          "form": "그러더라구요?",
          "original_form": "그러더라구요?",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043948.1.8",
          "form": "큰건 아니긴했지망요",
          "original_form": "큰건 아니긴했지망요",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043948.1.9",
          "form": "전 8800원에 샀어요!!",
          "original_form": "전 8800원에 샀어요!!",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043948.1.10",
          "form": "샤인머스캣 처음 나올땐 한송이가 이만원 가까이 하지 않았나요?",
          "original_form": "샤인머스캣 처음 나올땐 한송이가 이만원 가까이 하지 않았나요?",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043948.1.11",
          "form": "진짜 비싸다하면서 먹었던 것 같은데 ㅋㅋㅋ",
          "original_form": "진짜 비싸다하면서 먹었던 것 같은데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043948.1.12",
          "form": "저희 동네가 원래 비싸요...ㅠㅠㅠ흑 좀만 더 내려가면 싼데 비와서 ㅠㅠ",
          "original_form": "저희 동네가 원래 비싸요...ㅠㅠㅠ흑 좀만 더 내려가면 싼데 비와서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043948.1.13",
          "form": "이만원인지는 기억안나는데 엄청 비싸서 눈길도 주지 말아야지 했어요",
          "original_form": "이만원인지는 기억안나는데 엄청 비싸서 눈길도 주지 말아야지 했어요",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043948.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043948.1.15",
          "form": "저한텐 진짜 너무 큰 돈이라",
          "original_form": "저한텐 진짜 너무 큰 돈이라",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043948.1.16",
          "form": "저도 그때 엄마찬스로 먹었던거라서요 ㅋㄱㅋㅋ",
          "original_form": "저도 그때 엄마찬스로 먹었던거라서요 ㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043948.1.17",
          "form": "진짜 싸게 잘사셨네요",
          "original_form": "진짜 싸게 잘사셨네요",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043948.1.18",
          "form": "근데 너무 맛있었어서",
          "original_form": "근데 너무 맛있었어서",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043948.1.19",
          "form": "맛은 똑같이 맛있어요?",
          "original_form": "맛은 똑같이 맛있어요?",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043948.1.20",
          "form": "네 근데 느낌상인지 몰라도 당도가 좀 낮은 느낌?",
          "original_form": "네 근데 느낌상인지 몰라도 당도가 좀 낮은 느낌?",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043948.1.21",
          "form": "헐 진짜요? 뭐지 왜그럴까요",
          "original_form": "헐 진짜요? 뭐지 왜그럴까요",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100043948.1.22",
          "form": "근데 맛있긴해요 처음 먹었을때 그 감동은 아니었지만요 ㅋㅋ",
          "original_form": "근데 맛있긴해요 처음 먹었을때 그 감동은 아니었지만요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100043948.1.23",
          "form": "아마 처음이라 더 확 오는게 있었던것도 있을 것 같아요ㅋㅋㅋ",
          "original_form": "아마 처음이라 더 확 오는게 있었던것도 있을 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100043948.1.24",
          "form": "name2님 내일 뭐 드실건가용",
          "original_form": "&name2&님 내일 뭐 드실건가용",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100043948.1.25",
          "form": "ㅋㄱㅋㄱ내일은 계란말이를 해먹을거에요",
          "original_form": "ㅋㄱㅋㄱ내일은 계란말이를 해먹을거에요",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100043948.1.26",
          "form": "원래 오늘 저녁에 해먹으려고했는데 후라이팬 설거지가 안돼있어서 패스했거든요",
          "original_form": "원래 오늘 저녁에 해먹으려고했는데 후라이팬 설거지가 안돼있어서 패스했거든요",
