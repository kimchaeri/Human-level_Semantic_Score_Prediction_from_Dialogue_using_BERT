      "utterance": [
        {
          "id": "MDRW2100042205.1.1",
          "form": "운동하시는 것 있나요?",
          "original_form": "운동하시는 것 있나요?",
          "speaker_id": "1",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100042205.1.2",
          "form": "없어용 ㅜㅜ 집에 사놓은건 많은데 게을러서 ..",
          "original_form": "없어용 ㅜㅜ 집에 사놓은건 많은데 게을러서 ..",
          "speaker_id": "2",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042205.1.3",
          "form": "해야하는데 안하게 되네요ㅠ",
          "original_form": "해야하는데 안하게 되네요ㅠ",
          "speaker_id": "2",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042205.1.4",
          "form": "저도 집에선 안하게 돼서",
          "original_form": "저도 집에선 안하게 돼서",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042205.1.5",
          "form": "필라테스 등록했어요",
          "original_form": "필라테스 등록했어요",
          "speaker_id": "1",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100042205.1.6",
          "form": "오 최근에 등록하셨어요?",
          "original_form": "오 최근에 등록하셨어요?",
          "speaker_id": "2",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042205.1.7",
          "form": "정보 공유 괜찮을까요!!",
          "original_form": "정보 공유 괜찮을까요!!",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042205.1.8",
          "form": "보통 얼마하는지 알고싶어요~",
          "original_form": "보통 얼마하는지 알고싶어요~",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042205.1.9",
          "form": "아 저는 1대1이랑 그룹 병행 중이라",
          "original_form": "아 저는 1대1이랑 그룹 병행 중이라",
          "speaker_id": "1",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042205.1.10",
          "form": "저는 가장 최근엔 몇년전에 피티만 해봐서요ㅠ",
          "original_form": "저는 가장 최근엔 몇년전에 피티만 해봐서요ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100042205.1.11",
          "form": "그룹은 운동횟수가 많아지면 가격이 내려가는데",
          "original_form": "그룹은 운동횟수가 많아지면 가격이 내려가는데",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042205.1.12",
          "form": "저희는 24회에 39만원이예요",
          "original_form": "저희는 24회에 39만원이예요",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042205.1.13",
          "form": "오~ 그룹은 몇명이서 하는건가요?",
          "original_form": "오~ 그룹은 몇명이서 하는건가요?",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042205.1.14",
          "form": "생각보다 저렴하네여..?",
          "original_form": "생각보다 저렴하네여..?",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042205.1.15",
          "form": "6명? 8명?",
          "original_form": "6명? 8명?",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042205.1.16",
          "form": "6인이요",
          "original_form": "6인이요",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042205.1.17",
          "form": "네 그룹운동은 확실히 저렴해요",
          "original_form": "네 그룹운동은 확실히 저렴해요",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042205.1.18",
          "form": "생각보다 네~ 진짜 저렴하네요",
          "original_form": "생각보다 네~ 진짜 저렴하네요",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042205.1.19",
          "form": "확실히 일대일이 비싼거 같아요",
          "original_form": "확실히 일대일이 비싼거 같아요",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042205.1.20",
          "form": "네 일대일은 1회에 거의 7만원...",
          "original_form": "네 일대일은 1회에 거의 7만원...",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042205.1.21",
          "form": "근데 일대일이 확실히 맞춤형 운동이 되죠ㅜ",
          "original_form": "근데 일대일이 확실히 맞춤형 운동이 되죠ㅜ",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042205.1.22",
          "form": "맞아요 ㅠㅠ 비싼값을 하는 ㅎㅎ",
          "original_form": "맞아요 ㅠㅠ 비싼값을 하는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042205.1.23",
          "form": "쭉 하고 싶어도 금액이 좀 부담이긴 해여ㅠ",
          "original_form": "쭉 하고 싶어도 금액이 좀 부담이긴 해여ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042205.1.24",
          "form": "네 그래서 전 1대1과 그룹운동 병행해요",
          "original_form": "네 그래서 전 1대1과 그룹운동 병행해요",
