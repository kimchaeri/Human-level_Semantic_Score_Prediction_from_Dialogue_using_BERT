      "utterance": [
        {
          "id": "MDRW2100037297.1.1",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037297.1.2",
          "form": "교수불륜 흔한 얘기인가봐요",
          "original_form": "교수불륜 흔한 얘기인가봐요",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037297.1.3",
          "form": "문 두들겨서 소리질러서 대학원생들이 달래느라 힘들었대욬ㅋㅋㅋㅋ",
          "original_form": "문 두들겨서 소리질러서 대학원생들이 달래느라 힘들었대욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037297.1.4",
          "form": "저도 들어본적 있어요",
          "original_form": "저도 들어본적 있어요",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037297.1.5",
          "form": "맞아요 !!!! 진짜 흔해요",
          "original_form": "맞아요 !!!! 진짜 흔해요",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037297.1.6",
          "form": "그리고 학생들 성희롱도 많이 해요",
          "original_form": "그리고 학생들 성희롱도 많이 해요",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037297.1.7",
          "form": "상대가 학생이에요??? 교수에요?",
          "original_form": "상대가 학생이에요??? 교수에요?",
          "speaker_id": "1",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037297.1.8",
          "form": "아 그죠;;;",
          "original_form": "아 그죠;;;",
          "speaker_id": "1",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037297.1.9",
          "form": "누구랑 불륜을 저질렀는지는 모르겠어요ㅠ",
          "original_form": "누구랑 불륜을 저질렀는지는 모르겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037297.1.10",
          "form": "어떤 교수가 불륜저질러서",
          "original_form": "어떤 교수가 불륜저질러서",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037297.1.11",
          "form": "근데 대학원생 - 교수 조합도 있고",
          "original_form": "근데 대학원생 - 교수 조합도 있고",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037297.1.12",
          "form": "말씀처럼 부인이 학교에왔댔나그랬어요 근데 소문이 나서 그 학교 출신 각각 다른 사람들이 너만 알고있으라며 말해주더라구요",
          "original_form": "말씀처럼 부인이 학교에왔댔나그랬어요 근데 소문이 나서 그 학교 출신 각각 다른 사람들이 너만 알고있으라며 말해주더라구요",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037297.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037297.1.14",
          "form": "근데 교수 - 교수 조합은 잘 못봤어요! 아무래도 교수 정도 되려면 여자가 나이가 꽤 있으니까 그런가...",
          "original_form": "근데 교수 - 교수 조합은 잘 못봤어요! 아무래도 교수 정도 되려면 여자가 나이가 꽤 있으니까 그런가...",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100037297.1.15",
          "form": "헉 서울대 교수 남편 자랑하던 아줌마가",
          "original_form": "헉 서울대 교수 남편 자랑하던 아줌마가",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100037297.1.16",
          "form": "남편이 바람핀거예요???",
          "original_form": "남편이 바람핀거예요???",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100037297.1.17",
          "form": "아뇨 그사람말고 다른 학교 교수이야기에요",
          "original_form": "아뇨 그사람말고 다른 학교 교수이야기에요",
          "speaker_id": "1",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100037297.1.18",
          "form": "아하 네네네!!!!",
          "original_form": "아하 네네네!!!!",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100037297.1.19",
          "form": "근데 교수들 속이 일반인보다 결코 깨끗하지않고 사람들 이목은 있으니까 더 비틀리는거 같아요",
          "original_form": "근데 교수들 속이 일반인보다 결코 깨끗하지않고 사람들 이목은 있으니까 더 비틀리는거 같아요",
          "speaker_id": "1",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100037297.1.20",
          "form": "맞아요 교수들 얼마나 이기적인데요",
          "original_form": "맞아요 교수들 얼마나 이기적인데요",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037297.1.21",
          "form": "대학원생 대하는 것만 봐도...",
          "original_form": "대학원생 대하는 것만 봐도...",
          "speaker_id": "2",
          "time": "20210831 22:27"
        }
      ]
