      "utterance": [
        {
          "id": "MDRW2100041068.1.1",
          "form": "엄마의 정원에서 주연으로 출연",
          "original_form": "엄마의 정원에서 주연으로 출연",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100041068.1.2",
          "form": "들어본 드라마네요",
          "original_form": "들어본 드라마네요",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100041068.1.3",
          "form": "서브 주연이다",
          "original_form": "서브 주연이다",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100041068.1.4",
          "form": "조연에서 경력 쌓아 주연으로",
          "original_form": "조연에서 경력 쌓아 주연으로",
          "speaker_id": "1",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100041068.1.5",
          "form": "정유미가 주연",
          "original_form": "정유미가 주연",
          "speaker_id": "2",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100041068.1.6",
          "form": "아 정유미 강타가 사귀는 애죠?",
          "original_form": "아 정유미 강타가 사귀는 애죠?",
          "speaker_id": "1",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100041068.1.7",
          "form": "그러게 차근차근 쌓아 올라왓네",
          "original_form": "그러게 차근차근 쌓아 올라왓네",
          "speaker_id": "2",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100041068.1.8",
          "form": "이렇게 올라오는게 정상이죠",
          "original_form": "이렇게 올라오는게 정상이죠",
          "speaker_id": "1",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100041068.1.9",
          "form": "진짜 난 몰랏는데",
          "original_form": "진짜 난 몰랏는데",
          "speaker_id": "2",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100041068.1.10",
          "form": "그래야 연기도 늘어나고",
          "original_form": "그래야 연기도 늘어나고",
          "speaker_id": "1",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100041068.1.11",
          "form": "최고의 결혼에서 현명이 주연",
          "original_form": "최고의 결혼에서 현명이 주연",
          "speaker_id": "2",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100041068.1.12",
          "form": "최고의 결혼도 본 적이 없네요",
          "original_form": "최고의 결혼도 본 적이 없네요",
          "speaker_id": "1",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100041068.1.13",
          "form": "맞아 처음부터 연기를 잘해서 주연은 괜찮지만 아니면 차근차근",
          "original_form": "맞아 처음부터 연기를 잘해서 주연은 괜찮지만 아니면 차근차근",
          "speaker_id": "2",
          "time": "20210905 16:31"
        }
      ]
