      "utterance": [
        {
          "id": "MDRW2100018873.1.1",
          "form": "게임 하시나용?",
          "original_form": "게임 하시나용?",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018873.1.2",
          "form": "게임하세요?",
          "original_form": "게임하세요?",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018873.1.3",
          "form": "지금은 잘 안해요..",
          "original_form": "지금은 잘 안해요..",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018873.1.4",
          "form": "뭔겜하세요?",
          "original_form": "뭔겜하세요?",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018873.1.5",
          "form": "저도 잘안해요 요즘 제페토앱 해요",
          "original_form": "저도 잘안해요 요즘 제페토앱 해요",
          "speaker_id": "1",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018873.1.6",
          "form": "메타버스앱",
          "original_form": "메타버스앱",
          "speaker_id": "1",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018873.1.7",
          "form": "어.. 그거 요즘 유명하쥬.. \r\n사람 많아여?",
          "original_form": "어.. 그거 요즘 유명하쥬.. \r\n사람 많아여?",
          "speaker_id": "2",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018873.1.8",
          "form": "막 많지는않아요 이것도 저는 사내 이벤트 때문에 시작했어요",
          "original_form": "막 많지는않아요 이것도 저는 사내 이벤트 때문에 시작했어요",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100018873.1.9",
          "form": "구글 플레이카드도 주더라구요 아바타 꾸미라구 ㅋㅋ",
          "original_form": "구글 플레이카드도 주더라구요 아바타 꾸미라구 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100018873.1.10",
          "form": "ㅋㅋㅋㅋ 할만은 해요?",
          "original_form": "ㅋㅋㅋㅋ 할만은 해요?",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100018873.1.11",
          "form": "네 그냥 싸이월드 아바타 같아요 ㅋㅋ",
          "original_form": "네 그냥 싸이월드 아바타 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018873.1.12",
          "form": "근데 조작법은 좀 어려워요 30대라그런가ㅠㅠ",
          "original_form": "근데 조작법은 좀 어려워요 30대라그런가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018873.1.13",
          "form": "뭔가 언론에서 떠느는거 보니.. 금장 접을거 같기도하고..",
          "original_form": "뭔가 언론에서 떠느는거 보니.. 금장 접을거 같기도하고..",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100018873.1.14",
          "form": "현질 같은건 안해도 되죠?",
          "original_form": "현질 같은건 안해도 되죠?",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100018873.1.15",
          "form": "네네맞아요 옷 살때 코인 이랑 젬이 필요한데",
          "original_form": "네네맞아요 옷 살때 코인 이랑 젬이 필요한데",
          "speaker_id": "1",
          "time": "20210827 15:06"
        },
        {
          "id": "MDRW2100018873.1.16",
          "form": "이거 쓰라고 구글플레이카드 준거같아요 이거아님 현질 저는 게임에 절대안해요ㅠㅠ",
          "original_form": "이거 쓰라고 구글플레이카드 준거같아요 이거아님 현질 저는 게임에 절대안해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 15:06"
        },
        {
          "id": "MDRW2100018873.1.17",
          "form": "겜에 원래 현질 안하세여?",
          "original_form": "겜에 원래 현질 안하세여?",
          "speaker_id": "2",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100018873.1.18",
          "form": "네 안해요 ㅎㅎ 게임도 잘 안하능 편이에요",
          "original_form": "네 안해요 ㅎㅎ 게임도 잘 안하능 편이에요",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100018873.1.19",
          "form": "현질하시는 게임 있으세요?",
          "original_form": "현질하시는 게임 있으세요?",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100018873.1.20",
          "form": "전 딱한번 고스톱에.. ㅋㅋㅋㅋ",
          "original_form": "전 딱한번 고스톱에.. ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100018873.1.21",
          "form": "겜.자체을 안하다보니..",
          "original_form": "겜.자체을 안하다보니..",
          "speaker_id": "2",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100018873.1.22",
          "form": "아하 고스톱 ㅋㅋㅋㅋ",
          "original_form": "아하 고스톱 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100018873.1.23",
          "form": "저는 30이지만 고스톱 잘 몰라요ㅠㅠ",
          "original_form": "저는 30이지만 고스톱 잘 몰라요ㅠㅠ",
