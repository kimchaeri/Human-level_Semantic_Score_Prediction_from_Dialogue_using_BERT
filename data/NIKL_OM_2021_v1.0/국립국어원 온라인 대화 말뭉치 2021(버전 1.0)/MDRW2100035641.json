      "utterance": [
        {
          "id": "MDRW2100035641.1.1",
          "form": "재미있으셨나여",
          "original_form": "재미있으셨나여",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100035641.1.2",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100035641.1.3",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100035641.1.4",
          "form": "나중에는 버스안에서 잠만 잤어요ㅠㅠ 못내리고ㅠㅠㅠ",
          "original_form": "나중에는 버스안에서 잠만 잤어요ㅠㅠ 못내리고ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100035641.1.5",
          "form": "부모님이랑 가는 여행이라",
          "original_form": "부모님이랑 가는 여행이라",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100035641.1.6",
          "form": "대중교통도 타고 그러는것보다는",
          "original_form": "대중교통도 타고 그러는것보다는",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100035641.1.7",
          "form": "ㅋㅋㅋ 부모님은 다 보셨어요?",
          "original_form": "ㅋㅋㅋ 부모님은 다 보셨어요?",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035641.1.8",
          "form": "대절 버스로 가는게 났겠다 싶어서",
          "original_form": "대절 버스로 가는게 났겠다 싶어서",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035641.1.9",
          "form": "네네 다 보시더라구욬ㅋㅋㅋ",
          "original_form": "네네 다 보시더라구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035641.1.10",
          "form": "체력대단하시다ㅋㅋ",
          "original_form": "체력대단하시다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035641.1.11",
          "form": "그러게욬ㅋㅋㅋㅋ 오히려 저보다 더 신나셔서ㅋㅋㅋ",
          "original_form": "그러게욬ㅋㅋㅋㅋ 오히려 저보다 더 신나셔서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035641.1.12",
          "form": "하나부터 열까지",
          "original_form": "하나부터 열까지",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035641.1.13",
          "form": "자녀는 차에서 기절...",
          "original_form": "자녀는 차에서 기절...",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035641.1.14",
          "form": "숙소 교통편 식당",
          "original_form": "숙소 교통편 식당",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035641.1.15",
          "form": "다 해주니까",
          "original_form": "다 해주니까",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035641.1.16",
          "form": "다 정해진대로 따라가기만 하면 되는데",
          "original_form": "다 정해진대로 따라가기만 하면 되는데",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035641.1.17",
          "form": "그게 뭐라고 힘들더라구요ㅠㅠㅋㅋㅋ",
          "original_form": "그게 뭐라고 힘들더라구요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100035641.1.18",
          "form": "ㅋㅋ그냥 여행사 스탈이 하드했나봐요",
          "original_form": "ㅋㅋ그냥 여행사 스탈이 하드했나봐요",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100035641.1.19",
          "form": "예약할땐 글로만 보니까 그런 느낌 전혀 못받았는데",
          "original_form": "예약할땐 글로만 보니까 그런 느낌 전혀 못받았는데",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100035641.1.20",
          "form": "막상 체험해서 그런가봐요ㅋㅋㅋ",
          "original_form": "막상 체험해서 그런가봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100035641.1.21",
          "form": "ㅋㄱㄱ직접가보니",
          "original_form": "ㅋㄱㄱ직접가보니",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100035641.1.22",
          "form": "30분단위ㅋㅋ",
          "original_form": "30분단위ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100035641.1.23",
          "form": "ㅋㅋㅋㅋㅋ하ㅜ네ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ하ㅜ네ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035641.1.24",
