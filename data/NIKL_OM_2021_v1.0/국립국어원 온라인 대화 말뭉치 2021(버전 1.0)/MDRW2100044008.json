      "utterance": [
        {
          "id": "MDRW2100044008.1.1",
          "form": "막내 추석때 오라 할말",
          "original_form": "막내 추석때 오라 할말",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044008.1.2",
          "form": "뭔말이야ㅋㅋ",
          "original_form": "뭔말이야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044008.1.3",
          "form": "우리집 결혼한 막둥이 ㅋㅋㅋ이름쓰기싫어서 ㅋㅋㅋ",
          "original_form": "우리집 결혼한 막둥이 ㅋㅋㅋ이름쓰기싫어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044008.1.4",
          "form": "ㄷ 양 이 추석때 오고싶다는데 코로나 때문에 와라고 해야할지모르겠음",
          "original_form": "ㄷ 양 이 추석때 오고싶다는데 코로나 때문에 와라고 해야할지모르겠음",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044008.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044008.1.6",
          "form": "자차니까 괜찮지읺아?",
          "original_form": "자차니까 괜찮지읺아?",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044008.1.7",
          "form": "사실 와도 귀찮음 ㅋㅋㅋ",
          "original_form": "사실 와도 귀찮음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044008.1.8",
          "form": "ㅋㅋㅋㅋ그건그렇겠지만ㅋㅋㅋ그래도 다음번볼때는 애기데꼬오겠는뎈ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그건그렇겠지만ㅋㅋㅋ그래도 다음번볼때는 애기데꼬오겠는뎈ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044008.1.9",
          "form": "시댁에서는 마음대로 하라했다는데 사실 첫명절이잖아 ㅋㅋㅋ 진짜 괜찮은건지 ㅋㅋㅋㅋ",
          "original_form": "시댁에서는 마음대로 하라했다는데 사실 첫명절이잖아 ㅋㅋㅋ 진짜 괜찮은건지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044008.1.10",
          "form": "아 결혼하고 첫명절이야? 흠........애매하겠다ㅜㅜ",
          "original_form": "아 결혼하고 첫명절이야? 흠........애매하겠다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044008.1.11",
          "form": "그체? ㅋㅋㅋㅋ 진짜 괜찮으신걸까? ㅋㅋㅋ",
          "original_form": "그체? ㅋㅋㅋㅋ 진짜 괜찮으신걸까? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044008.1.12",
          "form": "사실우리집입장에서는 진짜괜찮거든? 근데 옆에서 말 엄청많이함",
          "original_form": "사실우리집입장에서는 진짜괜찮거든? 근데 옆에서 말 엄청많이함",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044008.1.13",
          "form": "옆에서라면 어디?",
          "original_form": "옆에서라면 어디?",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044008.1.14",
          "form": "친척들이라던지 주위 사람들ㅋㅋㅋ",
          "original_form": "친척들이라던지 주위 사람들ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044008.1.15",
          "form": "말 한마디씩 얹는 그런거구만....",
          "original_form": "말 한마디씩 얹는 그런거구만....",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044008.1.16",
          "form": "응응ㅋㅋ 막내는 떨어져 지내니까 보고싶겠다ㅜㅜ",
          "original_form": "응응ㅋㅋ 막내는 떨어져 지내니까 보고싶겠다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044008.1.17",
          "form": "해산물먹고싶어서 오고싶다던데?",
          "original_form": "해산물먹고싶어서 오고싶다던데?",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
