      "utterance": [
        {
          "id": "MDRW2100013984.1.1",
          "form": "가장 최근에 사신 물건 있으세요?",
          "original_form": "가장 최근에 사신 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100013984.1.2",
          "form": "최근에...",
          "original_form": "최근에...",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100013984.1.3",
          "form": "음.. 치킨너겟",
          "original_form": "음.. 치킨너겟",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100013984.1.4",
          "form": "샀어요",
          "original_form": "샀어요",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100013984.1.5",
          "form": "ㅋㅋㅋ집에 에어프라이어 있으신가요??",
          "original_form": "ㅋㅋㅋ집에 에어프라이어 있으신가요??",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100013984.1.6",
          "form": "냉동식품 돌려먹으면 거의 맛있어요",
          "original_form": "냉동식품 돌려먹으면 거의 맛있어요",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100013984.1.7",
          "form": "넹 맨날 뭐 돌려먹어서 살이 ...",
          "original_form": "넹 맨날 뭐 돌려먹어서 살이 ...",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100013984.1.8",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100013984.1.9",
          "form": "ㅎ..맞아요",
          "original_form": "ㅎ..맞아요",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100013984.1.10",
          "form": "저도 처음에 샀을때 진짜 다돌려먹어봤어요",
          "original_form": "저도 처음에 샀을때 진짜 다돌려먹어봤어요",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100013984.1.11",
          "form": "냉동생지반죽 사서 빵도 돌려먹을수 있어서 좋더라구요!!!",
          "original_form": "냉동생지반죽 사서 빵도 돌려먹을수 있어서 좋더라구요!!!",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100013984.1.12",
          "form": "마자요 그게 개꿀이져",
          "original_form": "마자요 그게 개꿀이져",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100013984.1.13",
          "form": "어릴때 오븐에 베이킹하고싶었는데 그걸 대신 이뤄준 에어프라이어..!ㅎㅎ",
          "original_form": "어릴때 오븐에 베이킹하고싶었는데 그걸 대신 이뤄준 에어프라이어..!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100013984.1.14",
          "form": "진짜 만족해요",
          "original_form": "진짜 만족해요",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100013984.1.15",
          "form": "저두 완전 만족 이런걸 어떻게 생각하고 만들어 내는지 ㅋㅋㅋㅋ",
          "original_form": "저두 완전 만족 이런걸 어떻게 생각하고 만들어 내는지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100013984.1.16",
          "form": "ㅋㅋㅋㅋㅋ누가 에프 어떠냐고하면 완전 추천해요ㅎㅎ\r\n전 저번달에 레인부츠 샀는데 한동안 비가 안와서 안신가다 요즘 만족하면서 신구있어요!",
          "original_form": "ㅋㅋㅋㅋㅋ누가 에프 어떠냐고하면 완전 추천해요ㅎㅎ\r\n전 저번달에 레인부츠 샀는데 한동안 비가 안와서 안신가다 요즘 만족하면서 신구있어요!",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100013984.1.17",
          "form": "오 레인부츠 그런거 신어보고 싶은데",
          "original_form": "오 레인부츠 그런거 신어보고 싶은데",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100013984.1.18",
          "form": "왠지 아무도 안 신어서",
          "original_form": "왠지 아무도 안 신어서",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100013984.1.19",
          "form": "좀 그렇더라구여",
          "original_form": "좀 그렇더라구여",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100013984.1.20",
          "form": "ㅋㅋㅋㅋ맞아요 사실 비오다가 그치면 그냥 레인부츠 신고 나온 사람 되지만,,",
          "original_form": "ㅋㅋㅋㅋ맞아요 사실 비오다가 그치면 그냥 레인부츠 신고 나온 사람 되지만,,",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100013984.1.21",
          "form": "신발양말 젖을 걱정 없어서 좋아요",
          "original_form": "신발양말 젖을 걱정 없어서 좋아요",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100013984.1.22",
          "form": "그건 좋을서 같아여",
          "original_form": "그건 좋을서 같아여",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100013984.1.23",
          "form": "하루라도 젊을 때 해야되는데",
          "original_form": "하루라도 젊을 때 해야되는데",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100013984.1.24",
          "form": "앗  그런말이 어딧나용ㅎㅎ 전 할머니 돼도 신을래요..ㅎㅎ같이 신어요!ㅋㅋㅋ",
          "original_form": "앗  그런말이 어딧나용ㅎㅎ 전 할머니 돼도 신을래요..ㅎㅎ같이 신어요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100013984.1.25",
          "form": "락피쉬에서 나오는 레인부츠는 딱 레인부츠같지않은 디자인도 나와서 좋아요!!",
          "original_form": "락피쉬에서 나오는 레인부츠는 딱 레인부츠같지않은 디자인도 나와서 좋아요!!",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100013984.1.26",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100013984.1.27",
          "form": "약간 저는 애기들이 신는거 밖에 못봐서",
          "original_form": "약간 저는 애기들이 신는거 밖에 못봐서",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100013984.1.28",
          "form": "자신이 없어여",
          "original_form": "자신이 없어여",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100013984.1.29",
          "form": "뭔가 레인부츠하면 그런느낌이긴하죠ㅠㅡㅠ",
          "original_form": "뭔가 레인부츠하면 그런느낌이긴하죠ㅠㅡㅠ",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100013984.1.30",
          "form": "그쳐 나중에 자신이 생기면 한번 도전해보는걸로..",
          "original_form": "그쳐 나중에 자신이 생기면 한번 도전해보는걸로..",
          "speaker_id": "1",
          "time": "20210825 20:26"
        }
      ]
