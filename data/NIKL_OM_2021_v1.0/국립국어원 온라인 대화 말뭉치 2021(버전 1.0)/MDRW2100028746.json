      "utterance": [
        {
          "id": "MDRW2100028746.1.1",
          "form": "우리집 강아지",
          "original_form": "우리집 강아지",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028746.1.2",
          "form": "귀여워",
          "original_form": "귀여워",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028746.1.3",
          "form": "포메야",
          "original_form": "포메야",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028746.1.4",
          "form": "나는 햄스터도 기르고 싶구",
          "original_form": "나는 햄스터도 기르고 싶구",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028746.1.5",
          "form": "거북이도 좋구",
          "original_form": "거북이도 좋구",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028746.1.6",
          "form": "파충류도 좋아",
          "original_form": "파충류도 좋아",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028746.1.7",
          "form": "근데 못길러",
          "original_form": "근데 못길러",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028746.1.8",
          "form": "ㅜㅜ 슬프다",
          "original_form": "ㅜㅜ 슬프다",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028746.1.9",
          "form": "반려동물 키우기 쉽지않지...",
          "original_form": "반려동물 키우기 쉽지않지...",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028746.1.10",
          "form": "그치.. 부모님도 반대하고 나도 책임감 없고",
          "original_form": "그치.. 부모님도 반대하고 나도 책임감 없고",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028746.1.11",
          "form": "여러모로 반려동물이란건...죽음까지 생각해야돼서... ㅠ 조금 슬픈것같아",
          "original_form": "여러모로 반려동물이란건...죽음까지 생각해야돼서... ㅠ 조금 슬픈것같아",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028746.1.12",
          "form": "우리 강아지 name3는 간식 고구마를 제일 좋아해",
          "original_form": "우리 강아지 &name3&는 간식 고구마를 제일 좋아해",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028746.1.13",
          "form": "고구마 나도 좋아해",
          "original_form": "고구마 나도 좋아해",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028746.1.14",
          "form": "name3는 생식 안먹어?",
          "original_form": "&name3&는 생식 안먹어?",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028746.1.15",
          "form": "아주 가끔? 먹는것같아",
          "original_form": "아주 가끔? 먹는것같아",
          "speaker_id": "2",
          "time": "20210904 10:18"
        },
        {
          "id": "MDRW2100028746.1.16",
          "form": "오히려 북어 삶은거 많이 주는듯!",
          "original_form": "오히려 북어 삶은거 많이 주는듯!",
          "speaker_id": "2",
          "time": "20210904 10:19"
        },
        {
          "id": "MDRW2100028746.1.17",
          "form": "아 순간 누구신데 반말을 이생각함",
          "original_form": "아 순간 누구신데 반말을 이생각함",
          "speaker_id": "1",
          "time": "20210904 11:23"
        },
        {
          "id": "MDRW2100028746.1.18",
          "form": "북어먹는 name3 귀엽다",
          "original_form": "북어먹는 &name3& 귀엽다",
          "speaker_id": "1",
          "time": "20210904 11:23"
        },
        {
          "id": "MDRW2100028746.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나야!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나야!",
          "speaker_id": "2",
          "time": "20210904 11:24"
        },
        {
          "id": "MDRW2100028746.1.20",
          "form": "지금은 사료먹고있어",
          "original_form": "지금은 사료먹고있어",
          "speaker_id": "2",
          "time": "20210904 11:24"
        },
        {
          "id": "MDRW2100028746.1.21",
          "form": "이이잉 귀여워",
          "original_form": "이이잉 귀여워",
          "speaker_id": "1",
          "time": "20210904 11:24"
        },
        {
          "id": "MDRW2100028746.1.22",
          "form": "아까는 name3가 좋아하는 고구마먹었지!",
          "original_form": "아까는 &name3&가 좋아하는 고구마먹었지!",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100028746.1.23",
          "form": "애들 고구마 단호박 좋아하는거",
          "original_form": "애들 고구마 단호박 좋아하는거",
          "speaker_id": "1",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100028746.1.24",
          "form": "갱얼쥐 국룰이야?",
          "original_form": "갱얼쥐 국룰이야?",
          "speaker_id": "1",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100028746.1.25",
          "form": "강아지 국룰임.",
          "original_form": "강아지 국룰임.",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100028746.1.26",
          "form": "당연함",
          "original_form": "당연함",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100028746.1.27",
          "form": "달고맛있음",
          "original_form": "달고맛있음",
          "speaker_id": "2",
