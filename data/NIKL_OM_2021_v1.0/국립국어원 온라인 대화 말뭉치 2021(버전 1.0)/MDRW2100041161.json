      "utterance": [
        {
          "id": "MDRW2100041161.1.1",
          "form": "이건 제가 먹어보진 않았고요",
          "original_form": "이건 제가 먹어보진 않았고요",
          "speaker_id": "1",
          "time": "20210905 18:54"
        },
        {
          "id": "MDRW2100041161.1.2",
          "form": "좀 먹는거에 신경쓰고 까다로운 사람이 추천하던건데",
          "original_form": "좀 먹는거에 신경쓰고 까다로운 사람이 추천하던건데",
          "speaker_id": "1",
          "time": "20210905 18:54"
        },
        {
          "id": "MDRW2100041161.1.3",
          "form": "우주인 피자 맛있대요",
          "original_form": "우주인 피자 맛있대요",
          "speaker_id": "1",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100041161.1.4",
          "form": "아!! 진짜 유용합니다!!!!",
          "original_form": "아!! 진짜 유용합니다!!!!",
          "speaker_id": "2",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100041161.1.5",
          "form": "안그래도 유목민이에요ㅜ",
          "original_form": "안그래도 유목민이에요ㅜ",
          "speaker_id": "2",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100041161.1.6",
          "form": "살림 깔끔하게 하는 분이라",
          "original_form": "살림 깔끔하게 하는 분이라",
          "speaker_id": "1",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041161.1.7",
          "form": "안먹어봤지만 추천드려봅니다",
          "original_form": "안먹어봤지만 추천드려봅니다",
          "speaker_id": "1",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100041161.1.8",
          "form": "보통이상은 될거에요",
          "original_form": "보통이상은 될거에요",
          "speaker_id": "1",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100041161.1.9",
          "form": "아 감사합니다 동네잇는지 찾아봐야겠어요",
          "original_form": "아 감사합니다 동네잇는지 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100041161.1.10",
          "form": "인터넷 검색하면 나와요",
          "original_form": "인터넷 검색하면 나와요",
          "speaker_id": "1",
          "time": "20210905 18:59"
        },
        {
          "id": "MDRW2100041161.1.11",
          "form": "밀키트사면 되겠네요!!",
          "original_form": "밀키트사면 되겠네요!!",
          "speaker_id": "2",
          "time": "20210905 19:03"
        },
        {
          "id": "MDRW2100041161.1.12",
          "form": "후기보니 밀키트는 맛없다네요",
          "original_form": "후기보니 밀키트는 맛없다네요",
          "speaker_id": "1",
          "time": "20210905 19:06"
        },
        {
          "id": "MDRW2100041161.1.13",
          "form": "참고하세요 ㅜㅜ 아니면 좀더 검색해보세요",
          "original_form": "참고하세요 ㅜㅜ 아니면 좀더 검색해보세요",
          "speaker_id": "1",
          "time": "20210905 19:06"
        },
        {
          "id": "MDRW2100041161.1.14",
          "form": "ㅜㅠ 넵 마지막 팁까지 감사합니다",
          "original_form": "ㅜㅠ 넵 마지막 팁까지 감사합니다",
          "speaker_id": "2",
          "time": "20210905 19:09"
        },
        {
          "id": "MDRW2100041161.1.15",
          "form": "혹시 해산물 쇼핑몰 필요하세요?",
          "original_form": "혹시 해산물 쇼핑몰 필요하세요?",
          "speaker_id": "1",
          "time": "20210905 19:11"
        },
        {
          "id": "MDRW2100041161.1.16",
          "form": "어!??  따로 있나요?",
          "original_form": "어!??  따로 있나요?",
          "speaker_id": "2",
          "time": "20210905 19:12"
        },
        {
          "id": "MDRW2100041161.1.17",
          "form": "오오푸드 라고 있는데요",
          "original_form": "오오푸드 라고 있는데요",
          "speaker_id": "1",
          "time": "20210905 19:13"
        },
        {
          "id": "MDRW2100041161.1.18",
          "form": "여기서 꽃게 산거랑 꼴뚜기 사봤는데",
          "original_form": "여기서 꽃게 산거랑 꼴뚜기 사봤는데",
          "speaker_id": "1",
          "time": "20210905 19:13"
        },
        {
          "id": "MDRW2100041161.1.19",
          "form": "싱싱하게 왔어요",
          "original_form": "싱싱하게 왔어요",
          "speaker_id": "1",
          "time": "20210905 19:13"
        },
        {
          "id": "MDRW2100041161.1.20",
          "form": "해산물요리 잘하시네요!",
          "original_form": "해산물요리 잘하시네요!",
          "speaker_id": "2",
          "time": "20210905 19:18"
        },
        {
          "id": "MDRW2100041161.1.21",
          "form": "전 사실 해산물손질 안해봤어요ㅜ",
          "original_form": "전 사실 해산물손질 안해봤어요ㅜ",
          "speaker_id": "2",
          "time": "20210905 19:18"
        },
        {
          "id": "MDRW2100041161.1.22",
          "form": "요리는 엄마가 다했어요 ㅎㅎ",
          "original_form": "요리는 엄마가 다했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 19:18"
        },
        {
          "id": "MDRW2100041161.1.23",
          "form": "아 ㅜㅜ",
          "original_form": "아 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 19:19"
        },
        {
          "id": "MDRW2100041161.1.24",
          "form": "그럼 저기 해신탕 세트 추천드려요",
          "original_form": "그럼 저기 해신탕 세트 추천드려요",
          "speaker_id": "1",
          "time": "20210905 19:19"
        },
        {
          "id": "MDRW2100041161.1.25",
          "form": "ㅋㅋ 아 솔직하시군요! ㅋㅋ",
          "original_form": "ㅋㅋ 아 솔직하시군요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:19"
        }
      ]
    }
