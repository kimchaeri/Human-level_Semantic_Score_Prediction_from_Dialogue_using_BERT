      "utterance": [
        {
          "id": "MDRW2100031655.1.1",
          "form": "아뇨 ㅎ 님은요 ?",
          "original_form": "아뇨 ㅎ 님은요 ?",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031655.1.2",
          "form": "저는 본적이 없어요!",
          "original_form": "저는 본적이 없어요!",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031655.1.3",
          "form": "보고싶으세요 ?",
          "original_form": "보고싶으세요 ?",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031655.1.4",
          "form": "네 친구들이 뮤지컬 재밌다고하더라고요",
          "original_form": "네 친구들이 뮤지컬 재밌다고하더라고요",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031655.1.5",
          "form": "어떤 뮤지컬?",
          "original_form": "어떤 뮤지컬?",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031655.1.6",
          "form": "위키드요!",
          "original_form": "위키드요!",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031655.1.7",
          "form": "아 들어봤어요~",
          "original_form": "아 들어봤어요~",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031655.1.8",
          "form": "여성투탑이라고 하더라고요",
          "original_form": "여성투탑이라고 하더라고요",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031655.1.9",
          "form": "재밌겠네요",
          "original_form": "재밌겠네요",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031655.1.10",
          "form": "기회되면 보고싶어요",
          "original_form": "기회되면 보고싶어요",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031655.1.11",
          "form": "네ㅠ 이또한 코로나끝나면",
          "original_form": "네ㅠ 이또한 코로나끝나면",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031655.1.12",
          "form": "할수있지않을까요",
          "original_form": "할수있지않을까요",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031655.1.13",
          "form": "그렇겠죠 ,,",
          "original_form": "그렇겠죠 ,,",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031655.1.14",
          "form": "영화보는건 좋아해요 ?",
          "original_form": "영화보는건 좋아해요 ?",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031655.1.15",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031655.1.16",
          "form": "좋아합니다",
          "original_form": "좋아합니다",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031655.1.17",
          "form": "최근본건??",
          "original_form": "최근본건??",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031655.1.18",
          "form": "크루엘라요",
          "original_form": "크루엘라요",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031655.1.19",
          "form": "보셨나요?!!",
          "original_form": "보셨나요?!!",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031655.1.20",
          "form": "네 저도 봤네요 ㅎ 재밌더군요",
          "original_form": "네 저도 봤네요 ㅎ 재밌더군요",
