      "utterance": [
        {
          "id": "MDRW2100002115.1.1",
          "form": "치킨은 뭐먹었어",
          "original_form": "치킨은 뭐먹었어",
          "speaker_id": "2",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100002115.1.2",
          "form": "치킨은 배그지",
          "original_form": "치킨은 배그지",
          "speaker_id": "1",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002115.1.3",
          "form": "배틀그라운드 치킨!",
          "original_form": "배틀그라운드 치킨!",
          "speaker_id": "1",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002115.1.4",
          "form": "배틀그라운드 아나?",
          "original_form": "배틀그라운드 아나?",
          "speaker_id": "1",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002115.1.5",
          "form": "배틀그라운드 해본적있나",
          "original_form": "배틀그라운드 해본적있나",
          "speaker_id": "2",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002115.1.6",
          "form": "게임이 사실적이어서 좋더라고",
          "original_form": "게임이 사실적이어서 좋더라고",
          "speaker_id": "1",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002115.1.7",
          "form": "나는 게임을 너무 못해서",
          "original_form": "나는 게임을 너무 못해서",
          "speaker_id": "2",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002115.1.8",
          "form": "낙하산을 타고 떨어지는 모습이 매력적이야",
          "original_form": "낙하산을 타고 떨어지는 모습이 매력적이야",
          "speaker_id": "1",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002115.1.9",
          "form": "한 세판하고 그뒤로는 해본적없어",
          "original_form": "한 세판하고 그뒤로는 해본적없어",
          "speaker_id": "2",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002115.1.10",
          "form": "그리고 상자를 열면 아이템을 주울 수 있다는 것도",
          "original_form": "그리고 상자를 열면 아이템을 주울 수 있다는 것도",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.11",
          "form": "크레이지아케이드는 어때.",
          "original_form": "크레이지아케이드는 어때.",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.12",
          "form": "크아는 재밌지",
          "original_form": "크아는 재밌지",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.13",
          "form": "우리 한때 그걸 했었지. 또 생각난다.",
          "original_form": "우리 한때 그걸 했었지. 또 생각난다.",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.14",
          "form": "크아도 모바일로 할수있으면 좋을텐데",
          "original_form": "크아도 모바일로 할수있으면 좋을텐데",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.15",
          "form": "최근에 한 카트라이더도 재밋었어.",
          "original_form": "최근에 한 카트라이더도 재밋었어.",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.16",
          "form": "조만간 하지 않을래?",
          "original_form": "조만간 하지 않을래?",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.17",
          "form": "좋아 나는 항상 폰에 깔려있어",
          "original_form": "좋아 나는 항상 폰에 깔려있어",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.18",
          "form": "모바일로하면 조작이 어려울 것 같기도 해.",
          "original_form": "모바일로하면 조작이 어려울 것 같기도 해.",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.19",
          "form": "정말?",
          "original_form": "정말?",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.20",
          "form": "하긴 생각해보니까 그렇다",
          "original_form": "하긴 생각해보니까 그렇다",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.21",
          "form": "나도 그럼 새 폰이 오면 한번 해볼래. 언젠간.",
          "original_form": "나도 그럼 새 폰이 오면 한번 해볼래. 언젠간.",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002115.1.22",
          "form": "니도 이제 폰 새로사면 용량이 많으니까 게임하기도 쉽겠다",
          "original_form": "니도 이제 폰 새로사면 용량이 많으니까 게임하기도 쉽겠다",
          "speaker_id": "2",
          "time": "20210820 00:05"
        }
      ]
