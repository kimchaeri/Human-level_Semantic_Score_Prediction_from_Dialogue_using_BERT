        },
        {
          "id": "MDRW2100043354.1.2",
          "form": "거북목이신가요?",
          "original_form": "거북목이신가요?",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043354.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043354.1.4",
          "form": "네 아직 중증은 아니고 세미 거북이입니다",
          "original_form": "네 아직 중증은 아니고 세미 거북이입니다",
          "speaker_id": "1",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043354.1.5",
          "form": "세미거북잌ㅋㅋㅋㅋ",
          "original_form": "세미거북잌ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043354.1.6",
          "form": "저도요 세미거북이라",
          "original_form": "저도요 세미거북이라",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043354.1.7",
          "form": "아마 일자일듯합니다(?)",
          "original_form": "아마 일자일듯합니다(?)",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043354.1.8",
          "form": "아 저도 일자목 있는 것 같아요",
          "original_form": "아 저도 일자목 있는 것 같아요",
          "speaker_id": "1",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043354.1.9",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043354.1.10",
          "form": "수건으로 스트레칭 열심히 하고있는데 언제 돌아올 지 미지수예요",
          "original_form": "수건으로 스트레칭 열심히 하고있는데 언제 돌아올 지 미지수예요",
          "speaker_id": "1",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043354.1.11",
          "form": "직장생활이 name1님을 거북목으로 만든건가요??",
          "original_form": "직장생활이 &name1&님을 거북목으로 만든건가요??",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043354.1.12",
          "form": "눼 비슷해요 책상에 계속 앉아있으니까 자꾸 구부정하세 앉게 되더라구요 어흑",
          "original_form": "눼 비슷해요 책상에 계속 앉아있으니까 자꾸 구부정하세 앉게 되더라구요 어흑",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043354.1.13",
          "form": "name2님은 무엇이 거북이로 만들었나요",
          "original_form": "&name2&님은 무엇이 거북이로 만들었나요",
          "speaker_id": "1",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043354.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ질문이 너무 웃겨요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ질문이 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043354.1.15",
          "form": "케케 감사합니다",
          "original_form": "케케 감사합니다",
          "speaker_id": "2",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043354.1.16",
          "form": "저도 아마 학업이...? 절이렇게 만드는것같아요",
          "original_form": "저도 아마 학업이...? 절이렇게 만드는것같아요",
          "speaker_id": "2",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043354.1.17",
          "form": "역시 공부는 해로워요(?)",
          "original_form": "역시 공부는 해로워요(?)",
          "speaker_id": "2",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043354.1.18",
          "form": "ㅋㅋㅋㅋㅋ무진장 해롭네요",
          "original_form": "ㅋㅋㅋㅋㅋ무진장 해롭네요",
          "speaker_id": "1",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043354.1.19",
          "form": "영원히 고통받는 현대인들",
          "original_form": "영원히 고통받는 현대인들",
          "speaker_id": "1",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043354.1.20",
          "form": "아까 백신 맞았냐고 물어보셨죠?",
          "original_form": "아까 백신 맞았냐고 물어보셨죠?",
          "speaker_id": "1",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043354.1.21",
          "form": "아직 안 맞았어요 저는!",
          "original_form": "아직 안 맞았어요 저는!",
          "speaker_id": "1",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043354.1.22",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043354.1.23",
