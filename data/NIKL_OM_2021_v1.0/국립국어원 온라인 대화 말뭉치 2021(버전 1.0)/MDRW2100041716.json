      "utterance": [
        {
          "id": "MDRW2100041716.1.1",
          "form": "햄버거는 좋아하세여??",
          "original_form": "햄버거는 좋아하세여??",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041716.1.2",
          "form": "햄버거는 맘스터치죠",
          "original_form": "햄버거는 맘스터치죠",
          "speaker_id": "1",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041716.1.3",
          "form": "저는 지금 햄버거 생각하고 있었어요",
          "original_form": "저는 지금 햄버거 생각하고 있었어요",
          "speaker_id": "2",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041716.1.4",
          "form": "ㅋㅋㅋㅋㅋ 저녁은 모르겠고 간식??",
          "original_form": "ㅋㅋㅋㅋㅋ 저녁은 모르겠고 간식??",
          "speaker_id": "2",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041716.1.5",
          "form": "아ㅋㅋㄱ",
          "original_form": "아ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041716.1.6",
          "form": "싸이버거 맛잇어요",
          "original_form": "싸이버거 맛잇어요",
          "speaker_id": "1",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041716.1.7",
          "form": "그쵸 저는 싸이버거도 좋아하고 할라피뇨버거도 좋아해요",
          "original_form": "그쵸 저는 싸이버거도 좋아하고 할라피뇨버거도 좋아해요",
          "speaker_id": "2",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041716.1.8",
          "form": "그 햄이랑 화이트소스 들어간것도 맛있던데 다 맛있져",
          "original_form": "그 햄이랑 화이트소스 들어간것도 맛있던데 다 맛있져",
          "speaker_id": "2",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041716.1.9",
          "form": "맞아요 다맛잇더라고요ㅋㅋ",
          "original_form": "맞아요 다맛잇더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041716.1.10",
          "form": "저 배민에서 쿠폰 사놓은것도 있어서 먹긴 해야돼요",
          "original_form": "저 배민에서 쿠폰 사놓은것도 있어서 먹긴 해야돼요",
          "speaker_id": "2",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041716.1.11",
          "form": "물론 기한은 내년까지지만ㅋㅋㅋㅋㅋ",
          "original_form": "물론 기한은 내년까지지만ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041716.1.12",
          "form": "전 배달앱은 쿠폰줄때 많이 시켜요ㅋㅋ",
          "original_form": "전 배달앱은 쿠폰줄때 많이 시켜요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041716.1.13",
          "form": "저번에도 뽑기잇어서 만원뽑아서 먹엇어요",
          "original_form": "저번에도 뽑기잇어서 만원뽑아서 먹엇어요",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041716.1.14",
          "form": "저는 그런 쿠폰 많이 줄때 까먹고 못하는 편이에요ㅋㅋㅋㅋㅋ",
          "original_form": "저는 그런 쿠폰 많이 줄때 까먹고 못하는 편이에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041716.1.15",
          "form": "타이밍 못맞추는애 ㅠㅠ",
          "original_form": "타이밍 못맞추는애 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041716.1.16",
          "form": "근데 한번 그렇게 먹으면 쿠폰없이 먹기 아까워요",
          "original_form": "근데 한번 그렇게 먹으면 쿠폰없이 먹기 아까워요",
          "speaker_id": "1",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041716.1.17",
          "form": "어차피 할인해줄거 매일 하면 안되나옄ㅋㅋㅋ",
          "original_form": "어차피 할인해줄거 매일 하면 안되나옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041716.1.18",
          "form": "맞아요 진짜",
          "original_form": "맞아요 진짜",
          "speaker_id": "1",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041716.1.19",
          "form": "그러니까요! 배달비 아까워서",
          "original_form": "그러니까요! 배달비 아까워서",
          "speaker_id": "2",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041716.1.20",
          "form": "요즘배달비도 비싼덴 막 오육천ㅠ",
          "original_form": "요즘배달비도 비싼덴 막 오육천ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041716.1.21",
          "form": "와 진짜 비싸네여",
          "original_form": "와 진짜 비싸네여",
          "speaker_id": "2",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100041716.1.22",
          "form": "전 오천원이상이면 안먹고 걍 포장해올거 같아욬ㅋㅋ",
          "original_form": "전 오천원이상이면 안먹고 걍 포장해올거 같아욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100041716.1.23",
          "form": "삼천원도 부들부들인뎅!!!",
          "original_form": "삼천원도 부들부들인뎅!!!",
          "speaker_id": "2",
