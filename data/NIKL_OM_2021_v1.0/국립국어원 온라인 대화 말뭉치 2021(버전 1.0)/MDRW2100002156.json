      "utterance": [
        {
          "id": "MDRW2100002156.1.1",
          "form": "낼 도서관 갔다가 베라 들러서 아이스크림 사와야겠다",
          "original_form": "낼 도서관 갔다가 베라 들러서 아이스크림 사와야겠다",
          "speaker_id": "2",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002156.1.2",
          "form": "무슨 맛 살거니",
          "original_form": "무슨 맛 살거니",
          "speaker_id": "1",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002156.1.3",
          "form": "젤큰걸로 살껀데",
          "original_form": "젤큰걸로 살껀데",
          "speaker_id": "2",
          "time": "20210820 00:12"
        },
        {
          "id": "MDRW2100002156.1.4",
          "form": "아몬드봉봉 엄마는외계아 사랑에빠진딸기 쿠앤크 등등",
          "original_form": "아몬드봉봉 엄마는외계아 사랑에빠진딸기 쿠앤크 등등",
          "speaker_id": "2",
          "time": "20210820 00:12"
        },
        {
          "id": "MDRW2100002156.1.5",
          "form": "어머 젤 큰거.. 부내 나는구나",
          "original_form": "어머 젤 큰거.. 부내 나는구나",
          "speaker_id": "1",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002156.1.6",
          "form": "기프티콘 5개 쓴다ㅋㅋㅋㅋ",
          "original_form": "기프티콘 5개 쓴다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002156.1.7",
          "form": "기프티콘 합산 안되는걸로 아는데",
          "original_form": "기프티콘 합산 안되는걸로 아는데",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002156.1.8",
          "form": "해피오더로 픽업주문하면 6개까지 가능함",
          "original_form": "해피오더로 픽업주문하면 6개까지 가능함",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002156.1.9",
          "form": "아 그렇게 쓰는구나 ㅋㅋ해피 오더 한번도 안해봐서 ㅋㅋㅋ",
          "original_form": "아 그렇게 쓰는구나 ㅋㅋ해피 오더 한번도 안해봐서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002156.1.10",
          "form": "나도 최근에 알게됨ㅋㅋ",
          "original_form": "나도 최근에 알게됨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002156.1.11",
          "form": "기프티콘이 많구나",
          "original_form": "기프티콘이 많구나",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002156.1.12",
          "form": "배스킨에 무슨 음료중에 맛있는거 있댔는데 기억이 안 나네",
          "original_form": "배스킨에 무슨 음료중에 맛있는거 있댔는데 기억이 안 나네",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002156.1.13",
          "form": "무슨 음료인지 나도 궁금허네ㅋㅋ",
          "original_form": "무슨 음료인지 나도 궁금허네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002156.1.14",
          "form": "라떼 종류였던거같은데",
          "original_form": "라떼 종류였던거같은데",
          "speaker_id": "1",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002156.1.15",
          "form": "베라 음료는 블라스트밖에 모름ㅋㅋ",
          "original_form": "베라 음료는 블라스트밖에 모름ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002156.1.16",
          "form": "바닐라라떼였나 암튼 은근 배스킨이 음료 맛집이랬던듯",
          "original_form": "바닐라라떼였나 암튼 은근 배스킨이 음료 맛집이랬던듯",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002156.1.17",
          "form": "은정이는 아몬드봉봉 블라스트 맛있다고 노래를 부르더라ㅋㅋㅋ",
          "original_form": "은정이는 아몬드봉봉 블라스트 맛있다고 노래를 부르더라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002156.1.18",
          "form": "배라 가본지도 오래됐군 그린티만 해서 사고싶은데 그린티 아이스크림 푸기 힘든 것 중에 하나라더라 ㅋㅋㅋ",
          "original_form": "배라 가본지도 오래됐군 그린티만 해서 사고싶은데 그린티 아이스크림 푸기 힘든 것 중에 하나라더라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002156.1.19",
          "form": "그린티가 많이 딱딱한가 봄ㅋㅋ",
          "original_form": "그린티가 많이 딱딱한가 봄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002156.1.20",
          "form": "아이스크림 맛있겠고나",
          "original_form": "아이스크림 맛있겠고나",
          "speaker_id": "1",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002156.1.21",
          "form": "낼 인증샷 날려주겠어ㅋㅋ",
          "original_form": "낼 인증샷 날려주겠어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002156.1.22",
          "form": "나도 키프티콘 이써여! 냉동실에 있는거 다 먹고 사야지",
          "original_form": "나도 키프티콘 이써여! 냉동실에 있는거 다 먹고 사야지",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002156.1.23",
          "form": "냉동실에 뭐 있는데ㅋㅋ",
          "original_form": "냉동실에 뭐 있는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002156.1.24",
          "form": "매그넘 바도 몇개있고 그때 산 콘이랑 바도 몇개 남음 ㅋㅋ",
          "original_form": "매그넘 바도 몇개있고 그때 산 콘이랑 바도 몇개 남음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002156.1.25",
          "form": "부지런히 먹거라ㅋㅋ 먹어야 또 채우지",
