      "utterance": [
        {
          "id": "MDRW2100011797.1.1",
          "form": "아맞어 그거 나도 도착함",
          "original_form": "아맞어 그거 나도 도착함",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011797.1.2",
          "form": "아 난 핸드타월 필요해서 일부러 사긴했는데",
          "original_form": "아 난 핸드타월 필요해서 일부러 사긴했는데",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011797.1.3",
          "form": "고리가 바뀌었더라 저번 핸드타월이랑 다르게",
          "original_form": "고리가 바뀌었더라 저번 핸드타월이랑 다르게",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011797.1.4",
          "form": "아아 지난번에도 핸드타월 했던적 있었어???",
          "original_form": "아아 지난번에도 핸드타월 했던적 있었어???",
          "speaker_id": "1",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011797.1.5",
          "form": "나 어느달 아무것도 못했을때 했던건가보다 ㅋㅋㅋ",
          "original_form": "나 어느달 아무것도 못했을때 했던건가보다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011797.1.6",
          "form": "작아서 띠용했어 ㅋㅋ",
          "original_form": "작아서 띠용했어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011797.1.7",
          "form": "저번에도 했었엌ㅋㅋㅋㅋㅋ",
          "original_form": "저번에도 했었엌ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011797.1.8",
          "form": "아 작지 ㅋㅋㅋ 아 그때 산거 한개는 좀 더러워져서 그냥 버렸거든",
          "original_form": "아 작지 ㅋㅋㅋ 아 그때 산거 한개는 좀 더러워져서 그냥 버렸거든",
          "speaker_id": "2",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011797.1.9",
          "form": "아아 ㅋㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100011797.1.10",
          "form": "좀 쓰다 버려야겠다 ㅜ 주방에서 좀 쓰고 ㅋㅋㅋ",
          "original_form": "좀 쓰다 버려야겠다 ㅜ 주방에서 좀 쓰고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100011797.1.11",
          "form": "맞아ㅜ 그래야해",
          "original_form": "맞아ㅜ 그래야해",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100011797.1.12",
          "form": "아 배구유니폼 산거 오늘도착함 ㅎㅎ",
          "original_form": "아 배구유니폼 산거 오늘도착함 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100011797.1.13",
          "form": "오오 ㅋㅋㅋㅋㅋ",
          "original_form": "오오 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011797.1.14",
          "form": "어때????",
          "original_form": "어때????",
          "speaker_id": "1",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011797.1.15",
          "form": "사이즈 잘 맞아???",
          "original_form": "사이즈 잘 맞아???",
          "speaker_id": "1",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011797.1.16",
          "form": "어어 잘맞더라 ㅎㅎ",
          "original_form": "어어 잘맞더라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:50"
        },
        {
          "id": "MDRW2100011797.1.17",
          "form": "나는 남색으로 산거인데 흰색도 이쁘더라 ㅎㅎ",
          "original_form": "나는 남색으로 산거인데 흰색도 이쁘더라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:50"
        },
        {
          "id": "MDRW2100011797.1.18",
          "form": "아아 색 두가지구나",
          "original_form": "아아 색 두가지구나",
          "speaker_id": "1",
          "time": "20210825 09:57"
        },
        {
          "id": "MDRW2100011797.1.19",
          "form": "궁금하다 ㅋㅋㅋㅋㅋ",
          "original_form": "궁금하다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:57"
        },
        {
          "id": "MDRW2100011797.1.20",
          "form": "나중에 실물 보여줘 ㅋㅋㅋ",
          "original_form": "나중에 실물 보여줘 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:58"
        },
        {
          "id": "MDRW2100011797.1.21",
          "form": "실물로 ㅎㅎ 그럴게",
          "original_form": "실물로 ㅎㅎ 그럴게",
          "speaker_id": "2",
          "time": "20210825 10:06"
        },
        {
          "id": "MDRW2100011797.1.22",
          "form": "이제 가을이 곧이라 가을옷도 사야겠닼ㅋㅋㅋㅋ",
          "original_form": "이제 가을이 곧이라 가을옷도 사야겠닼ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:06"
        },
        {
          "id": "MDRW2100011797.1.23",
          "form": "그니까... 가을옷 ㅜㅜ 두꺼워서 또 비싸고 ㅋㅋㅋ",
          "original_form": "그니까... 가을옷 ㅜㅜ 두꺼워서 또 비싸고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:23"
        },
        {
          "id": "MDRW2100011797.1.24",
          "form": "셔츠 좀 봐야지 작년엔 뭐 입고 다녔지 ㅋㅋㅋ",
          "original_form": "셔츠 좀 봐야지 작년엔 뭐 입고 다녔지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:23"
        },
        {
          "id": "MDRW2100011797.1.25",
          "form": "트렌치코트 이월상품 최근에 사서",
          "original_form": "트렌치코트 이월상품 최근에 사서",
          "speaker_id": "2",
          "time": "20210825 11:16"
        },
        {
          "id": "MDRW2100011797.1.26",
          "form": "나도 셔츠좀 보려고 ㅎㅎ",
          "original_form": "나도 셔츠좀 보려고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100011797.1.27",
          "form": "가을 좋다",
          "original_form": "가을 좋다",
          "speaker_id": "2",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100011797.1.28",
          "form": "오오 ㅋㅋㅋㅋ 괜찮은거 구입??",
          "original_form": "오오 ㅋㅋㅋㅋ 괜찮은거 구입??",
          "speaker_id": "1",
          "time": "20210825 12:54"
        },
        {
          "id": "MDRW2100011797.1.29",
          "form": "나도 가을 옷 사들여야지 ㅋㅋㅋ",
          "original_form": "나도 가을 옷 사들여야지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:55"
        },
        {
          "id": "MDRW2100011797.1.30",
          "form": "겨울도 준비해야되는데 일단 가을옷부터...",
          "original_form": "겨울도 준비해야되는데 일단 가을옷부터...",
