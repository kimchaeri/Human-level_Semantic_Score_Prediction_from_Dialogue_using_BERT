      "utterance": [
        {
          "id": "MDRW2100009892.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009892.1.2",
          "form": "스포츠 경기 직관한적 있으세요?",
          "original_form": "스포츠 경기 직관한적 있으세요?",
          "speaker_id": "2",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009892.1.3",
          "form": "직관은 한번도 못 해봤어요",
          "original_form": "직관은 한번도 못 해봤어요",
          "speaker_id": "1",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009892.1.4",
          "form": "name2님은 가보신 적 있으신가요??",
          "original_form": "&name2&님은 가보신 적 있으신가요??",
          "speaker_id": "1",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009892.1.5",
          "form": "저는 야구경기 보러간적 있어요",
          "original_form": "저는 야구경기 보러간적 있어요",
          "speaker_id": "2",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009892.1.6",
          "form": "재미있었겠어요ㅎㅎ 그날 응원하시는 팀이 이겼나요?",
          "original_form": "재미있었겠어요ㅎㅎ 그날 응원하시는 팀이 이겼나요?",
          "speaker_id": "1",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009892.1.7",
          "form": "롯데여서 이기진 못했어요",
          "original_form": "롯데여서 이기진 못했어요",
          "speaker_id": "2",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009892.1.8",
          "form": "슬픈 일이죠",
          "original_form": "슬픈 일이죠",
          "speaker_id": "2",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009892.1.9",
          "form": "아ㅠㅠㅜㅠㅜㅠㅜㅠ이럴수가",
          "original_form": "아ㅠㅠㅜㅠㅜㅠㅜㅠ이럴수가",
          "speaker_id": "1",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009892.1.10",
          "form": "야구는 잘 모르는데 이번 올림픽 데드볼이 너무 웃겼어요",
          "original_form": "야구는 잘 모르는데 이번 올림픽 데드볼이 너무 웃겼어요",
          "speaker_id": "1",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100009892.1.11",
          "form": "저도 야구는 잘 모르는데",
          "original_form": "저도 야구는 잘 모르는데",
          "speaker_id": "2",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009892.1.12",
          "form": "배 치고 나가는거 웃겼죠",
          "original_form": "배 치고 나가는거 웃겼죠",
          "speaker_id": "2",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009892.1.13",
          "form": "맞아요 그걸로 이긴것도 웃기고ㅋㅋ",
          "original_form": "맞아요 그걸로 이긴것도 웃기고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:57"
        },
        {
          "id": "MDRW2100009892.1.14",
          "form": "배구도 엄청 흥미진진하게 봤는데 마침 오늘부터 코보컵 시작이라 하더라구요",
          "original_form": "배구도 엄청 흥미진진하게 봤는데 마침 오늘부터 코보컵 시작이라 하더라구요",
          "speaker_id": "1",
          "time": "20210823 11:57"
        },
        {
          "id": "MDRW2100009892.1.15",
          "form": "name2님도 배구경기 보셨나요?",
          "original_form": "&name2&님도 배구경기 보셨나요?",
          "speaker_id": "1",
          "time": "20210823 11:58"
        },
        {
          "id": "MDRW2100009892.1.16",
          "form": "저는 클립만 봤어요",
          "original_form": "저는 클립만 봤어요",
          "speaker_id": "2",
          "time": "20210823 12:10"
        },
        {
          "id": "MDRW2100009892.1.17",
          "form": "코보컵 시작하나요!",
          "original_form": "코보컵 시작하나요!",
          "speaker_id": "2",
          "time": "20210823 12:11"
        },
        {
          "id": "MDRW2100009892.1.18",
          "form": "네 오늘부터래요!",
          "original_form": "네 오늘부터래요!",
          "speaker_id": "1",
          "time": "20210823 12:13"
        },
        {
          "id": "MDRW2100009892.1.19",
          "form": "올림픽도 출전했던 선수들은 되게 피곤하겠어요",
          "original_form": "올림픽도 출전했던 선수들은 되게 피곤하겠어요",
          "speaker_id": "1",
          "time": "20210823 12:13"
        },
        {
          "id": "MDRW2100009892.1.20",
          "form": "그러게요 몸 아프겠다",
          "original_form": "그러게요 몸 아프겠다",
          "speaker_id": "2",
          "time": "20210823 12:17"
        },
        {
          "id": "MDRW2100009892.1.21",
          "form": "괜히 더 부상입는 건 아닌지 걱정도 되고요...",
          "original_form": "괜히 더 부상입는 건 아닌지 걱정도 되고요...",
          "speaker_id": "1",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009892.1.22",
          "form": "일요일이 결승이라니까 그건 꼭 봐야겠어요!",
          "original_form": "일요일이 결승이라니까 그건 꼭 봐야겠어요!",
          "speaker_id": "1",
          "time": "20210823 12:29"
        }
      ]
