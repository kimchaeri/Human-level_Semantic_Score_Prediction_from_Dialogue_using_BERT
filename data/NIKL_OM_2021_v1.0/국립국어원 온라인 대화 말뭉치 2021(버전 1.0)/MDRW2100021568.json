      "utterance": [
        {
          "id": "MDRW2100021568.1.1",
          "form": "드라마 보시나용?",
          "original_form": "드라마 보시나용?",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021568.1.2",
          "form": "저는 드라마 챙겨보는걸 잘 못해서",
          "original_form": "저는 드라마 챙겨보는걸 잘 못해서",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021568.1.3",
          "form": "그리고 챙겨보던 드라마는 꼭 마지막화를 못보는 징크스가 있어요",
          "original_form": "그리고 챙겨보던 드라마는 꼭 마지막화를 못보는 징크스가 있어요",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021568.1.4",
          "form": "앗 그 징크스 너무 슬픈데요..?",
          "original_form": "앗 그 징크스 너무 슬픈데요..?",
          "speaker_id": "1",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100021568.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100021568.1.6",
          "form": "지금 보시는건 없으세요?",
          "original_form": "지금 보시는건 없으세요?",
          "speaker_id": "1",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100021568.1.7",
          "form": "마지막화 못본 드라마가 너무 너무 많아요",
          "original_form": "마지막화 못본 드라마가 너무 너무 많아요",
          "speaker_id": "2",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100021568.1.8",
          "form": "요즘 유투브로 블랙독 클립으로 좀 보고있고",
          "original_form": "요즘 유투브로 블랙독 클립으로 좀 보고있고",
          "speaker_id": "2",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100021568.1.9",
          "form": "그거말곤없어요",
          "original_form": "그거말곤없어요",
          "speaker_id": "2",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100021568.1.10",
          "form": "정말 많으신가보네욥",
          "original_form": "정말 많으신가보네욥",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021568.1.11",
          "form": "아항~ 저는 광자매 보는데 끝이 얼마 안남아서 보고 있어요ㅋㅋㅋㅋ",
          "original_form": "아항~ 저는 광자매 보는데 끝이 얼마 안남아서 보고 있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021568.1.12",
          "form": "오 광자매",
          "original_form": "오 광자매",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021568.1.13",
          "form": "코시국 반영됐다던 그드라마..",
          "original_form": "코시국 반영됐다던 그드라마..",
          "speaker_id": "2",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100021568.1.14",
          "form": "재밌나요!?",
          "original_form": "재밌나요!?",
          "speaker_id": "2",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100021568.1.15",
          "form": "욕하면서도 봐요ㅋㅋㅋ",
          "original_form": "욕하면서도 봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021568.1.16",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021568.1.17",
          "form": "저는 그 전에 한다다 봤었어요!",
          "original_form": "저는 그 전에 한다다 봤었어요!",
          "speaker_id": "2",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021568.1.18",
          "form": "오!! 저두요ㅋㅋㅋ",
          "original_form": "오!! 저두요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021568.1.19",
          "form": "부모님따라 봤었는데 생각보다 재밌었어요ㄱㄲ",
          "original_form": "부모님따라 봤었는데 생각보다 재밌었어요ㄱㄲ",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021568.1.20",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021568.1.21",
          "form": "주말드라마는 화내면서도 보게되요ㅋㅋ",
          "original_form": "주말드라마는 화내면서도 보게되요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100021568.1.22",
          "form": "한다다 다음에는",
          "original_form": "한다다 다음에는",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021568.1.23",
          "form": "오삼광빌라 봤었는데",
          "original_form": "오삼광빌라 봤었는데",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021568.1.24",
          "form": "한숨 엄청 쉬면서 봤어요",
          "original_form": "한숨 엄청 쉬면서 봤어요",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021568.1.25",
          "form": "저도..ㅋㅋㄱ",
          "original_form": "저도..ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100021568.1.26",
          "form": "주말드라마 매니아로서 갈수록 너무해요",
          "original_form": "주말드라마 매니아로서 갈수록 너무해요",
          "speaker_id": "1",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100021568.1.27",
          "form": "주말드라마 매니아시구나!!",
          "original_form": "주말드라마 매니아시구나!!",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021568.1.28",
          "form": "저는 한다다때문에 될 뻔 했다가",
          "original_form": "저는 한다다때문에 될 뻔 했다가",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021568.1.29",
          "form": "ㅠㅠ 끝나버렸네요",
          "original_form": "ㅠㅠ 끝나버렸네요",
          "speaker_id": "2",
          "time": "20210828 21:20"
        }
      ]
