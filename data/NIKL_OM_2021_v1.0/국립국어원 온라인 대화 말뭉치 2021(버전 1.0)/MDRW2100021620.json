      "utterance": [
        {
          "id": "MDRW2100021620.1.1",
          "form": "요즘에 반려동물의 폭이 넓어진거같아요",
          "original_form": "요즘에 반려동물의 폭이 넓어진거같아요",
          "speaker_id": "2",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021620.1.2",
          "form": "맞아요 진짜 다양해요",
          "original_form": "맞아요 진짜 다양해요",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100021620.1.3",
          "form": "혹시 반려동물 있으신가요?",
          "original_form": "혹시 반려동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100021620.1.4",
          "form": "네! 고양이와 함께예요~",
          "original_form": "네! 고양이와 함께예요~",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021620.1.5",
          "form": "반려동물 있으신가요?",
          "original_form": "반려동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021620.1.6",
          "form": "아 저만 없나봐요",
          "original_form": "아 저만 없나봐요",
          "speaker_id": "1",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021620.1.7",
          "form": "고양이 키우는 분과 대화한 기억이 나네요",
          "original_form": "고양이 키우는 분과 대화한 기억이 나네요",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021620.1.8",
          "form": "아앗ㅋㅋ",
          "original_form": "아앗ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021620.1.9",
          "form": "여기 고양이 키우시는분 많으신거 같아요",
          "original_form": "여기 고양이 키우시는분 많으신거 같아요",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021620.1.10",
          "form": "그래요? 오 혹시 냥이 이름이 물어봐도 될까요?",
          "original_form": "그래요? 오 혹시 냥이 이름이 물어봐도 될까요?",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021620.1.11",
          "form": "name3 name4 예요!",
          "original_form": "&name3& &name4& 예요!",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021620.1.12",
          "form": "귀여운 이름이네요ㅎㅎ",
          "original_form": "귀여운 이름이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021620.1.13",
          "form": "히히 감사합니다",
          "original_form": "히히 감사합니다",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021620.1.14",
          "form": "애들덕에 늘 즐거워용",
          "original_form": "애들덕에 늘 즐거워용",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021620.1.15",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100021620.1.16",
          "form": "키우실 예정 있으세요??",
          "original_form": "키우실 예정 있으세요??",
          "speaker_id": "2",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021620.1.17",
          "form": "아뇨 잘 돌보지 못할 것 같아서 그냥 보기만 해요",
          "original_form": "아뇨 잘 돌보지 못할 것 같아서 그냥 보기만 해요",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021620.1.18",
          "form": "길 가다가 보고 영상보고 뭐 그 정도만...",
          "original_form": "길 가다가 보고 영상보고 뭐 그 정도만...",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021620.1.19",
          "form": "아 확실히 책임감이 무겁긴하죠",
          "original_form": "아 확실히 책임감이 무겁긴하죠",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021620.1.20",
          "form": "병원비도 많이 들고요ㅜ",
          "original_form": "병원비도 많이 들고요ㅜ",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021620.1.21",
          "form": "그래서 키우시는 분들 보면 대단하다고 생각해요",
          "original_form": "그래서 키우시는 분들 보면 대단하다고 생각해요",
