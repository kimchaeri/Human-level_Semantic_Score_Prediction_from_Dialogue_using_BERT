      "utterance": [
        {
          "id": "MDRW2100001927.1.1",
          "form": "이번 겨울은 추우려나",
          "original_form": "이번 겨울은 추우려나",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001927.1.2",
          "form": "여름도 더웠으니까 추울듯 ㅠ",
          "original_form": "여름도 더웠으니까 추울듯 ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001927.1.3",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001927.1.4",
          "form": "추울 거 같다",
          "original_form": "추울 거 같다",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001927.1.5",
          "form": "겨울옷 좀 꺼내야겠다",
          "original_form": "겨울옷 좀 꺼내야겠다",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001927.1.6",
          "form": "아직 가을인데?!ㅋㅋ",
          "original_form": "아직 가을인데?!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001927.1.7",
          "form": "근데 밤에 좀 춥긴하다 ㅎㅎ",
          "original_form": "근데 밤에 좀 춥긴하다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001927.1.8",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001927.1.9",
          "form": "넘 춥기 전에 옷장 정리해야지",
          "original_form": "넘 춥기 전에 옷장 정리해야지",
          "speaker_id": "1",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001927.1.10",
          "form": "구래 같이 합셍",
          "original_form": "구래 같이 합셍",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001927.1.11",
          "form": "그래 딸래미가 착착 정리해 봐라",
          "original_form": "그래 딸래미가 착착 정리해 봐라",
          "speaker_id": "1",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001927.1.12",
          "form": "옹 ㅋㅋ내가 잘하지~ 여름옷 안입는것도 다 정리해야겠다",
          "original_form": "옹 ㅋㅋ내가 잘하지~ 여름옷 안입는것도 다 정리해야겠다",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001927.1.13",
          "form": "여름 넘~ 길었다",
          "original_form": "여름 넘~ 길었다",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001927.1.14",
          "form": "그래 더운 것보단 시원한 게 낫다",
          "original_form": "그래 더운 것보단 시원한 게 낫다",
          "speaker_id": "1",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001927.1.15",
          "form": "나둥 ㅋㅋ",
          "original_form": "나둥 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001927.1.16",
          "form": "너무 추운 것도 싫지만",
          "original_form": "너무 추운 것도 싫지만",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001927.1.17",
          "form": "구래 ㅠ적당한 날씨 ㅠㅠ",
          "original_form": "구래 ㅠ적당한 날씨 ㅠㅠ",
