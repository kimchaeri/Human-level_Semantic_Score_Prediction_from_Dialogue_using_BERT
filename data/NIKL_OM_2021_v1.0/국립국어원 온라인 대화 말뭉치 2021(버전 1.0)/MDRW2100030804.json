      "utterance": [
        {
          "id": "MDRW2100030804.1.1",
          "form": "되따!",
          "original_form": "되따!",
          "speaker_id": "2",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030804.1.2",
          "form": "그래서 페이크주인공",
          "original_form": "그래서 페이크주인공",
          "speaker_id": "2",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030804.1.3",
          "form": "많이 당하셨어?",
          "original_form": "많이 당하셨어?",
          "speaker_id": "2",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030804.1.4",
          "form": "근데 파파는 주인공 안 좋아하고 오히려 페이크주인공이랄거같은 애 좋아할고같앗는데",
          "original_form": "근데 파파는 주인공 안 좋아하고 오히려 페이크주인공이랄거같은 애 좋아할고같앗는데",
          "speaker_id": "2",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030804.1.5",
          "form": "사실 이거 하면 욕 오질라게 먹어",
          "original_form": "사실 이거 하면 욕 오질라게 먹어",
          "speaker_id": "1",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030804.1.6",
          "form": "그렇긴하지",
          "original_form": "그렇긴하지",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030804.1.7",
          "form": "주인공보다 잘난 놈이 조연으로 있어야지",
          "original_form": "주인공보다 잘난 놈이 조연으로 있어야지",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030804.1.8",
          "form": "재미있는 거지",
          "original_form": "재미있는 거지",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030804.1.9",
          "form": "걔가 주인공 누르고 주인공 되는 건 좋아하지 않음",
          "original_form": "걔가 주인공 누르고 주인공 되는 건 좋아하지 않음",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030804.1.10",
          "form": "페이크 주인공이면",
          "original_form": "페이크 주인공이면",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030804.1.11",
          "form": "처음부터 주인공이덩거 ㅇㅇ",
          "original_form": "처음부터 주인공이덩거 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030804.1.12",
          "form": "딱 1화 치고빠지는",
          "original_form": "딱 1화 치고빠지는",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030804.1.13",
          "form": "애 정도가 좋아",
          "original_form": "애 정도가 좋아",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030804.1.14",
          "form": "1화에 죽어",
          "original_form": "1화에 죽어",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030804.1.15",
          "form": "ㅇㅇ 그렇게 처리해야함",
          "original_form": "ㅇㅇ 그렇게 처리해야함",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030804.1.16",
          "form": "대표적인 예가 신암행어사",
          "original_form": "대표적인 예가 신암행어사",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030804.1.17",
          "form": "몽룡아",
          "original_form": "몽룡아",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030804.1.18",
          "form": "신암행어사 초딩때 1권 빌려보고",
          "original_form": "신암행어사 초딩때 1권 빌려보고",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030804.1.19",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030804.1.20",
          "form": "춘향이가",
          "original_form": "춘향이가",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030804.1.21",
          "form": "넘 옷이...",
          "original_form": "넘 옷이...",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030804.1.22",
          "form": "나쁜말같아서",
          "original_form": "나쁜말같아서",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030804.1.23",
          "form": "후딱 치워버렸던 기억",
          "original_form": "후딱 치워버렸던 기억",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030804.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그래서 웹툰에선 붕대감음",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그래서 웹툰에선 붕대감음",
          "speaker_id": "1",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030804.1.25",
          "form": "ㅋㅋㅋ천을 주세오...",
          "original_form": "ㅋㅋㅋ천을 주세오...",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030804.1.26",
          "form": "욕먹은 페이크주인공은",
          "original_form": "욕먹은 페이크주인공은",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030804.1.27",
          "form": "누가있더라",
          "original_form": "누가있더라",
          "speaker_id": "2",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030804.1.28",
          "form": "닥터스톤도 처음엔 맷집맨이 주인공처럼 묘사되다가 바로 슥 했고..",
          "original_form": "닥터스톤도 처음엔 맷집맨이 주인공처럼 묘사되다가 바로 슥 했고..",
          "speaker_id": "1",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030804.1.29",
          "form": "슥...",
          "original_form": "슥...",
          "speaker_id": "2",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030804.1.30",
          "form": "사실 주인공 서사 안주고 주변한테 주인공 서사 몰빵하고 절벽푸쉬하는 경우가",
          "original_form": "사실 주인공 서사 안주고 주변한테 주인공 서사 몰빵하고 절벽푸쉬하는 경우가",
          "speaker_id": "1",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030804.1.31",
          "form": "게임에선 테오제 애니에선 발브레이브",
          "original_form": "게임에선 테오제 애니에선 발브레이브",
          "speaker_id": "1",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030804.1.32",
          "form": "뭐 좀 많지 사실",
          "original_form": "뭐 좀 많지 사실",
          "speaker_id": "1",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030804.1.33",
          "form": "ㅋㅋㅋㅋㅋ둘이",
          "original_form": "ㅋㅋㅋㅋㅋ둘이",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030804.1.34",
          "form": "여캐 비슷해",
          "original_form": "여캐 비슷해",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030804.1.35",
          "form": "생긴것도",
          "original_form": "생긴것도",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030804.1.36",
          "form": "그런타입이 스테레오라서 인기가 좋은 건 당연한데",
          "original_form": "그런타입이 스테레오라서 인기가 좋은 건 당연한데",
          "speaker_id": "1",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030804.1.37",
          "form": "뭔가 창작자들이 훅 꽂혀서 절벽푸쉬하나봐",
          "original_form": "뭔가 창작자들이 훅 꽂혀서 절벽푸쉬하나봐",
          "speaker_id": "1",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030804.1.38",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 12:51"
        },
        {
          "id": "MDRW2100030804.1.39",
          "form": "나도 내 애캐있으면 절벽푸쉬하고싶긴할듯",
          "original_form": "나도 내 애캐있으면 절벽푸쉬하고싶긴할듯",
          "speaker_id": "2",
          "time": "20210904 12:51"
        },
        {
          "id": "MDRW2100030804.1.40",
          "form": "근데 그 팬들은",
          "original_form": "근데 그 팬들은",
          "speaker_id": "2",
          "time": "20210904 12:51"
        },
        {
          "id": "MDRW2100030804.1.41",
          "form": "이도저도 아닐거같아 ㅋㅋㅋㅋㅋ",
          "original_form": "이도저도 아닐거같아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:51"
        },
        {
          "id": "MDRW2100030804.1.42",
          "form": "많이나와사 좋은데",
          "original_form": "많이나와사 좋은데",
          "speaker_id": "2",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030804.1.43",
          "form": "개연성?",
          "original_form": "개연성?",
          "speaker_id": "2",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030804.1.44",
          "form": "타팬들한테 욕받이 되구",
          "original_form": "타팬들한테 욕받이 되구",
          "speaker_id": "2",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030804.1.45",
          "form": "등장 많은거랑 작품을 해치는 건 다른 거라",
          "original_form": "등장 많은거랑 작품을 해치는 건 다른 거라",
          "speaker_id": "1",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030804.1.46",
          "form": "후자는 팬들도 그냥 고통스러워 하고",
          "original_form": "후자는 팬들도 그냥 고통스러워 하고",
          "speaker_id": "1",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030804.1.47",
          "form": "주인공 대신 서사 쌓아서 작품을 해치면 팬들도 힘들어하고",
          "original_form": "주인공 대신 서사 쌓아서 작품을 해치면 팬들도 힘들어하고",
          "speaker_id": "1",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030804.1.48",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030804.1.49",
          "form": "화남",
          "original_form": "화남",
          "speaker_id": "2",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030804.1.50",
          "form": "주인공 포지션은 명확하게 유지하되 서사쌓아서 작품 해치면 모르쇠가 되더라",
          "original_form": "주인공 포지션은 명확하게 유지하되 서사쌓아서 작품 해치면 모르쇠가 되더라",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030804.1.51",
          "form": "후자가 내스급 ㅇㅇ",
          "original_form": "후자가 내스급 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030804.1.52",
          "form": "후자는 비엘로 파서 그런 거 아닐까",
          "original_form": "후자는 비엘로 파서 그런 거 아닐까",
          "speaker_id": "2",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030804.1.53",
          "form": "비엘을 보통 떼려야 뗄 수 없지",
          "original_form": "비엘을 보통 떼려야 뗄 수 없지",
          "speaker_id": "1",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100030804.1.54",
          "form": "근데 에르에르후는 약간 좀",
          "original_form": "근데 에르에르후는 약간 좀",
          "speaker_id": "1",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100030804.1.55",
          "form": "흐린눈 하잖아ㅋㅋㅋㅋㅋ",
          "original_form": "흐린눈 하잖아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100030804.1.56",
          "form": "ㅋㅋㅋㅋㅋ에르에흐후 얘기였어?",
          "original_form": "ㅋㅋㅋㅋㅋ에르에흐후 얘기였어?",
          "speaker_id": "2",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030804.1.57",
          "form": "그 여자애 얘기아니었어?",
          "original_form": "그 여자애 얘기아니었어?",
          "speaker_id": "2",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030804.1.58",
          "form": "발브레이브는 푸쉬가 두개가 들어갔는데",
          "original_form": "발브레이브는 푸쉬가 두개가 들어갔는데",
          "speaker_id": "1",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030804.1.59",
          "form": "그래서 그 작품이 너무 잔인했던 거ㅇㅇ",
          "original_form": "그래서 그 작품이 너무 잔인했던 거ㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030804.1.60",
          "form": "주인공을 굴리긴 굴리되",
          "original_form": "주인공을 굴리긴 굴리되",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030804.1.61",
          "form": "그로 얻는 겅 쇼코가 다 가져가고",
          "original_form": "그로 얻는 겅 쇼코가 다 가져가고",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
