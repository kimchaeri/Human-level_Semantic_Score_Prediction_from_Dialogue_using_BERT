        },
        {
          "id": "MDRW2100037926.1.2",
          "form": "백신 제때 못맞게됨",
          "original_form": "백신 제때 못맞게됨",
          "speaker_id": "2",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100037926.1.3",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100037926.1.4",
          "form": "요즘 등이 패션은 어떤스탈일인고",
          "original_form": "요즘 등이 패션은 어떤스탈일인고",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037926.1.5",
          "form": "난 아직 여름 반팔을 못 벗어남",
          "original_form": "난 아직 여름 반팔을 못 벗어남",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037926.1.6",
          "form": "호공 제주라서긍가",
          "original_form": "호공 제주라서긍가",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037926.1.7",
          "form": "나는 벌써 긴팔옷 입고다님 난방",
          "original_form": "나는 벌써 긴팔옷 입고다님 난방",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037926.1.8",
          "form": "목에 수건도했슈",
          "original_form": "목에 수건도했슈",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037926.1.9",
          "form": "서울은 좀 추운가보더라",
          "original_form": "서울은 좀 추운가보더라",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037926.1.10",
          "form": "벌써 추위가 ㄷㄷ",
          "original_form": "벌써 추위가 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037926.1.11",
          "form": "면연력이 떨어집니다",
          "original_form": "면연력이 떨어집니다",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100037926.1.12",
          "form": "운동하세요",
          "original_form": "운동하세요",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100037926.1.13",
          "form": "숨쉬기함",
          "original_form": "숨쉬기함",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037926.1.14",
          "form": "아니자너 바닥에서 체조갘은거 하자너",
          "original_form": "아니자너 바닥에서 체조갘은거 하자너",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037926.1.15",
          "form": "수건 운동이랑",
          "original_form": "수건 운동이랑",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037926.1.16",
          "form": "웅 함 ㅜㅜㅜ",
          "original_form": "웅 함 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037926.1.17",
          "form": "name1님도 하셔야죠",
          "original_form": "&name1&님도 하셔야죠",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037926.1.18",
          "form": "요즘 밥을 잘 안챙겨먹는데",
          "original_form": "요즘 밥을 잘 안챙겨먹는데",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100037926.1.19",
          "form": "되려 살찌는 거 가테",
          "original_form": "되려 살찌는 거 가테",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100037926.1.20",
          "form": "비축하려나보다 에너지를",
          "original_form": "비축하려나보다 에너지를",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100037926.1.21",
          "form": "통달한의원 확장공사하고 다시 열었쇼",
          "original_form": "통달한의원 확장공사하고 다시 열었쇼",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100037926.1.22",
          "form": "아진짜",
          "original_form": "아진짜",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037926.1.23",
          "form": "한약보담은 운동인데",
          "original_form": "한약보담은 운동인데",
