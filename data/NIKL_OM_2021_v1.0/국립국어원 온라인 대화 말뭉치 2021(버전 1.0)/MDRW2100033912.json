      "utterance": [
        {
          "id": "MDRW2100033912.1.1",
          "form": "근데 탈모 예방하려면 검은콩을 먹으면 좋대요",
          "original_form": "근데 탈모 예방하려면 검은콩을 먹으면 좋대요",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100033912.1.2",
          "form": "요새 검은콩 두유 먹어요",
          "original_form": "요새 검은콩 두유 먹어요",
          "speaker_id": "1",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100033912.1.3",
          "form": "좋은ㅌ정보 주셔서 감사해요",
          "original_form": "좋은ㅌ정보 주셔서 감사해요",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100033912.1.4",
          "form": "정말 매번 건강 정보 감사해요",
          "original_form": "정말 매번 건강 정보 감사해요",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100033912.1.5",
          "form": "저 다욧 심할때 너무식단을 줄였었어요",
          "original_form": "저 다욧 심할때 너무식단을 줄였었어요",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100033912.1.6",
          "form": "탄수화물도 안먹고",
          "original_form": "탄수화물도 안먹고",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100033912.1.7",
          "form": "요즘 탈모인 분들이 많아서.. 저도 심하진 않지만 이제 슬슬 머리카락을 관리하긴 해야돼서 알아보고 있죠",
          "original_form": "요즘 탈모인 분들이 많아서.. 저도 심하진 않지만 이제 슬슬 머리카락을 관리하긴 해야돼서 알아보고 있죠",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100033912.1.8",
          "form": "네 꼭 하세요",
          "original_form": "네 꼭 하세요",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100033912.1.9",
          "form": "맞다 너무 밥양을 줄이면 그렇다고 하더라구요",
          "original_form": "맞다 너무 밥양을 줄이면 그렇다고 하더라구요",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033912.1.10",
          "form": "전 탈모 샴푸써요",
          "original_form": "전 탈모 샴푸써요",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033912.1.11",
          "form": "그래서 그렇군요 ㅜㅜ",
          "original_form": "그래서 그렇군요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033912.1.12",
          "form": "저도 탈모샴푸 샀어요.. 미리 관리하려구요",
          "original_form": "저도 탈모샴푸 샀어요.. 미리 관리하려구요",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033912.1.13",
          "form": "아 슬퍼요",
          "original_form": "아 슬퍼요",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033912.1.14",
          "form": "네 전 종류별로 샀어요",
          "original_form": "네 전 종류별로 샀어요",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033912.1.15",
          "form": "근데 다시 균형잡힌 식단 먹으면 다시 날 수도 있으니까요..",
          "original_form": "근데 다시 균형잡힌 식단 먹으면 다시 날 수도 있으니까요..",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033912.1.16",
          "form": "뭐든지 심하게 하면 반작용이 오는 거 같아요",
          "original_form": "뭐든지 심하게 하면 반작용이 오는 거 같아요",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100033912.1.17",
          "form": "그런것 같아요",
          "original_form": "그런것 같아요",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100033912.1.18",
          "form": "운동도 4시간씩했어요",
          "original_form": "운동도 4시간씩했어요",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100033912.1.19",
          "form": "ㅜㅜ미쳤었어요",
          "original_form": "ㅜㅜ미쳤었어요",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100033912.1.20",
          "form": "와.. 근데 그때 그만큼 절박했나봐요.. 전 아무리해도 그렇게 못하겠던데",
          "original_form": "와.. 근데 그때 그만큼 절박했나봐요.. 전 아무리해도 그렇게 못하겠던데",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100033912.1.21",
          "form": "그냥 꽂힌다고하죠",
          "original_form": "그냥 꽂힌다고하죠",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100033912.1.22",
          "form": "그랬었어요",
          "original_form": "그랬었어요",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100033912.1.23",
          "form": "은동관련해서 대화 더 나눠보고 싶네요",
          "original_form": "은동관련해서 대화 더 나눠보고 싶네요",
          "speaker_id": "2",
          "time": "20210825 15:28"
        }
      ]
