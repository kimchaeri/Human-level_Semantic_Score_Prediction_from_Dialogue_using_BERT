      "utterance": [
        {
          "id": "MDRW2100035497.1.1",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "2",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100035497.1.2",
          "form": "저는 아직이요 저녁드셨나요?",
          "original_form": "저는 아직이요 저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100035497.1.3",
          "form": "방금요! 케이크 먹었어요~",
          "original_form": "방금요! 케이크 먹었어요~",
          "speaker_id": "2",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100035497.1.4",
          "form": "우와 식사로 케이크 드신거예요?",
          "original_form": "우와 식사로 케이크 드신거예요?",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100035497.1.5",
          "form": "어떤케이크였나요",
          "original_form": "어떤케이크였나요",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100035497.1.6",
          "form": "넹ㅋㅋ 치즈랑 고구마요!ㅋㅋ",
          "original_form": "넹ㅋㅋ 치즈랑 고구마요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100035497.1.7",
          "form": "사놓은건데 있는걸 까먹고 있었다가 급히 먹었어요ㅋㅋ",
          "original_form": "사놓은건데 있는걸 까먹고 있었다가 급히 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100035497.1.8",
          "form": "빵 좋아하시나보다!",
          "original_form": "빵 좋아하시나보다!",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100035497.1.9",
          "form": "네! 안좋아하는줄 알았는데 얼마전 인정해버렸어요ㅋㅋㅋ",
          "original_form": "네! 안좋아하는줄 알았는데 얼마전 인정해버렸어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100035497.1.10",
          "form": "빵 좋아하세요?",
          "original_form": "빵 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100035497.1.11",
          "form": "저는 잘 안먹어요ㅋㅋㅋ",
          "original_form": "저는 잘 안먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100035497.1.12",
          "form": "케잌은 가끔먹는거 좋아해요",
          "original_form": "케잌은 가끔먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100035497.1.13",
          "form": "디카페인 커피랑같이!",
          "original_form": "디카페인 커피랑같이!",
          "speaker_id": "1",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100035497.1.14",
          "form": "크으 단거에 딱이죠ㅋㅋ",
          "original_form": "크으 단거에 딱이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100035497.1.15",
          "form": "저 커피 잘안마시는데 케이크 먹을때는 가끔 마셔요",
          "original_form": "저 커피 잘안마시는데 케이크 먹을때는 가끔 마셔요",
          "speaker_id": "2",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100035497.1.16",
          "form": "저도 카페인 안맞아서 안먹는데 케잌은 디카페인이라도 꼭 마셔요",
          "original_form": "저도 카페인 안맞아서 안먹는데 케잌은 디카페인이라도 꼭 마셔요",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100035497.1.17",
          "form": "케익은 찾게 되긴하더라구요ㄱㄲ",
          "original_form": "케익은 찾게 되긴하더라구요ㄱㄲ",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100035497.1.18",
          "form": "깔끔ㅎㅁ이 필요해요",
          "original_form": "깔끔ㅎㅁ이 필요해요",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100035497.1.19",
          "form": "그죠 못해도 우유는 있어야해요",
          "original_form": "그죠 못해도 우유는 있어야해요",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035497.1.20",
          "form": "맞아요! 적어도 우유는 있어줘야해요",
          "original_form": "맞아요! 적어도 우유는 있어줘야해요",
          "speaker_id": "2",
          "time": "20210829 19:28"
        }
      ]
