      "utterance": [
        {
          "id": "MDRW2100020264.1.1",
          "form": "스포츠 경기 보러 간 적 있으세요?",
          "original_form": "스포츠 경기 보러 간 적 있으세요?",
          "speaker_id": "1",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020264.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100020264.1.3",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100020264.1.4",
          "form": "네 최근에 올림픽 재밌게 봤어요",
          "original_form": "네 최근에 올림픽 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100020264.1.5",
          "form": "올림픽 너무 재밌었죠",
          "original_form": "올림픽 너무 재밌었죠",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100020264.1.6",
          "form": "네 이번 올림픽 유난히 재밌더라고요",
          "original_form": "네 이번 올림픽 유난히 재밌더라고요",
          "speaker_id": "2",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100020264.1.7",
          "form": "어떤경기가 제일 재밌으셨어요?!",
          "original_form": "어떤경기가 제일 재밌으셨어요?!",
          "speaker_id": "1",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100020264.1.8",
          "form": "저는 배구 울면서 봤어요",
          "original_form": "저는 배구 울면서 봤어요",
          "speaker_id": "1",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100020264.1.9",
          "form": "오 저도요! 배구 진짜 손에 땀을 쥐고 봤어요",
          "original_form": "오 저도요! 배구 진짜 손에 땀을 쥐고 봤어요",
          "speaker_id": "2",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100020264.1.10",
          "form": "5세트까지 갈때마다 엄청 떨리고.. ㅋㅋㅋㅋ",
          "original_form": "5세트까지 갈때마다 엄청 떨리고.. ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100020264.1.11",
          "form": "진짜 5세트까지 가서 다ㅋㅋ엄청 떨렸죠",
          "original_form": "진짜 5세트까지 가서 다ㅋㅋ엄청 떨렸죠",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100020264.1.12",
          "form": "코로나끝나면 직관도 가보고 싶어요",
          "original_form": "코로나끝나면 직관도 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100020264.1.13",
          "form": "와 저도요!! 한번도 보러간적 없는데 배구는 꼭 직접가서 보고싶네요",
          "original_form": "와 저도요!! 한번도 보러간적 없는데 배구는 꼭 직접가서 보고싶네요",
          "speaker_id": "2",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100020264.1.14",
          "form": "저도 어느 스포츠도 직관간적 한번도 없어요 !",
          "original_form": "저도 어느 스포츠도 직관간적 한번도 없어요 !",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100020264.1.15",
          "form": "혹시 배구 말고도 다른 스포츠도 좋아하시는거 있나요?",
          "original_form": "혹시 배구 말고도 다른 스포츠도 좋아하시는거 있나요?",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100020264.1.16",
          "form": "핸드볼 보고싶어요",
          "original_form": "핸드볼 보고싶어요",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100020264.1.17",
          "form": "예능노는언니통해서 알게된건데",
          "original_form": "예능노는언니통해서 알게된건데",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100020264.1.18",
          "form": "너무재밌어보이더라고요",
          "original_form": "너무재밌어보이더라고요",
          "speaker_id": "1",
          "time": "20210828 15:14"
        }
      ]
