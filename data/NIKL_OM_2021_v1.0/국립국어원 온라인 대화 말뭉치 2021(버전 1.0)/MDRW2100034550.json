      "utterance": [
        {
          "id": "MDRW2100034550.1.1",
          "form": "네 당분이 많아서 ㅠ",
          "original_form": "네 당분이 많아서 ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100034550.1.2",
          "form": "근데 뭐 단 음료보다는",
          "original_form": "근데 뭐 단 음료보다는",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034550.1.3",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "1",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034550.1.4",
          "form": "과일이 더 좋죠",
          "original_form": "과일이 더 좋죠",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034550.1.5",
          "form": "비타민도 있고",
          "original_form": "비타민도 있고",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034550.1.6",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034550.1.7",
          "form": "그거 아세요?",
          "original_form": "그거 아세요?",
          "speaker_id": "1",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034550.1.8",
          "form": "무기질도 있고ㅋㅋ",
          "original_form": "무기질도 있고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034550.1.9",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034550.1.10",
          "form": "크리스피도넛 엄청 달잖아요",
          "original_form": "크리스피도넛 엄청 달잖아요",
          "speaker_id": "1",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034550.1.11",
          "form": "근데 바나나우유가 당분이 훨씬 많대요..!",
          "original_form": "근데 바나나우유가 당분이 훨씬 많대요..!",
          "speaker_id": "1",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034550.1.12",
          "form": "아ㅋㅋ그 글 오늘 봤어요ㅋㅋ",
          "original_form": "아ㅋㅋ그 글 오늘 봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034550.1.13",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100034550.1.14",
          "form": "진짜 충격적이더라구요",
          "original_form": "진짜 충격적이더라구요",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100034550.1.15",
          "form": "정말 충격이에요. .",
          "original_form": "정말 충격이에요. .",
          "speaker_id": "1",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100034550.1.16",
          "form": "확실히 음료가 당이 많이들어가나봐요",
          "original_form": "확실히 음료가 당이 많이들어가나봐요",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100034550.1.17",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100034550.1.18",
          "form": "그뒤로 바나나우유 못먹겠어요 ㅋㅋㅋㅋ",
          "original_form": "그뒤로 바나나우유 못먹겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100034550.1.19",
          "form": "아ㅋㅋㅋ진짜 그러겠네요ㅠ",
          "original_form": "아ㅋㅋㅋ진짜 그러겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034550.1.20",
          "form": "근데 또 맛있으니까ㅠ",
          "original_form": "근데 또 맛있으니까ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034550.1.21",
          "form": "눈물..",
          "original_form": "눈물..",
          "speaker_id": "1",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034550.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034550.1.23",
          "form": "근데 먹을때마다 엄청 달다 느끼긴했어요",
          "original_form": "근데 먹을때마다 엄청 달다 느끼긴했어요",
          "speaker_id": "1",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034550.1.24",
          "form": "뚱바는 원유함량도 높다는데",
          "original_form": "뚱바는 원유함량도 높다는데",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034550.1.25",
          "form": "다른우유는 더 당이 많이 들어가겠어요",
          "original_form": "다른우유는 더 당이 많이 들어가겠어요",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034550.1.26",
          "form": "달긴하죠 진짜ㅠ",
          "original_form": "달긴하죠 진짜ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034550.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100034550.1.28",
          "form": "왜 몸에안좋은거만 맛있는지 ㅋㅋㅋㅋ",
          "original_form": "왜 몸에안좋은거만 맛있는지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:57"
        }
      ]
    }
