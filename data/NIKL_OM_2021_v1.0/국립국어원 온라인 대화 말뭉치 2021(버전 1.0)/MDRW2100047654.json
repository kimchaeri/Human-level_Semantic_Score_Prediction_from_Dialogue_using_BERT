      "utterance": [
        {
          "id": "MDRW2100047654.1.1",
          "form": "소금은 그냥 위에 솔솔 뿌리는거예요?",
          "original_form": "소금은 그냥 위에 솔솔 뿌리는거예요?",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100047654.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100047654.1.3",
          "form": "구우며 뿌려요",
          "original_form": "구우며 뿌려요",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100047654.1.4",
          "form": "맛이 궁금해지네요",
          "original_form": "맛이 궁금해지네요",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100047654.1.5",
          "form": "참기름에만 구워도 고소할텐데",
          "original_form": "참기름에만 구워도 고소할텐데",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100047654.1.6",
          "form": "소금이더해지믄...어떤맛일지 ㅎ",
          "original_form": "소금이더해지믄...어떤맛일지 ㅎ",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100047654.1.7",
          "form": "소금해야 맛나요",
          "original_form": "소금해야 맛나요",
          "speaker_id": "2",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100047654.1.8",
          "form": "간이 맞아지나봐요 ㅎ",
          "original_form": "간이 맞아지나봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 23:54"
        },
        {
          "id": "MDRW2100047654.1.9",
          "form": "네 애들이 사실 쩜 간해야 좋아해요",
          "original_form": "네 애들이 사실 쩜 간해야 좋아해요",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100047654.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100047654.1.11",
          "form": "사진 찍으셨어요?",
          "original_form": "사진 찍으셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100047654.1.12",
          "form": "저도 떡 어제 샀는데 ㅋㅋ",
          "original_form": "저도 떡 어제 샀는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100047654.1.13",
          "form": "이유식 먹일때 진짜 맛없는거 먹이는거같은 죄책감 ㅋㅋ",
          "original_form": "이유식 먹일때 진짜 맛없는거 먹이는거같은 죄책감 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100047654.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100047654.1.15",
          "form": "간이 하나도 안되서 나도 맛없던데 ㅎㅎ",
          "original_form": "간이 하나도 안되서 나도 맛없던데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100047654.1.16",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100047654.1.17",
          "form": "사진 아직이요 ㅋㅋ",
          "original_form": "사진 아직이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100047654.1.18",
          "form": "저사실 소금 살짝했어요",
          "original_form": "저사실 소금 살짝했어요",
          "speaker_id": "2",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100047654.1.19",
          "form": "너무 맛었는 거예요 ㅜㅜ",
          "original_form": "너무 맛었는 거예요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 00:00"
        },
        {
          "id": "MDRW2100047654.1.20",
          "form": "제가 미안해서 살짝",
          "original_form": "제가 미안해서 살짝",
          "speaker_id": "2",
          "time": "20210827 00:00"
        },
        {
          "id": "MDRW2100047654.1.21",
          "form": "저는 하면안된다고해서",
          "original_form": "저는 하면안된다고해서",
          "speaker_id": "1",
          "time": "20210827 00:00"
        },
        {
          "id": "MDRW2100047654.1.22",
          "form": "12개월 다됐을때요",
          "original_form": "12개월 다됐을때요",
          "speaker_id": "2",
          "time": "20210827 00:00"
        },
        {
          "id": "MDRW2100047654.1.23",
          "form": "절대안했는데요 ㅎ",
          "original_form": "절대안했는데요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 00:00"
        },
        {
          "id": "MDRW2100047654.1.24",
          "form": "전 했네요",
          "original_form": "전 했네요",
          "speaker_id": "2",
          "time": "20210827 00:00"
        },
        {
          "id": "MDRW2100047654.1.25",
          "form": "양심이 찔려요",
          "original_form": "양심이 찔려요",
          "speaker_id": "2",
          "time": "20210827 00:00"
        },
        {
          "id": "MDRW2100047654.1.26",
          "form": "나뿐엄마 같아서 ㅡㅡ죄책감이",
          "original_form": "나뿐엄마 같아서 ㅡㅡ죄책감이",
          "speaker_id": "2",
          "time": "20210827 00:01"
        },
        {
          "id": "MDRW2100047654.1.27",
          "form": "그래도 잘 먹으면되는거ㅛ",
          "original_form": "그래도 잘 먹으면되는거ㅛ",
          "speaker_id": "1",
          "time": "20210827 00:01"
        },
        {
          "id": "MDRW2100047654.1.28",
          "form": "죠",
          "original_form": "죠",
          "speaker_id": "1",
          "time": "20210827 00:01"
        },
        {
          "id": "MDRW2100047654.1.29",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100047654.1.30",
          "form": "진짜 이유식 다신 못해요",
          "original_form": "진짜 이유식 다신 못해요",
          "speaker_id": "2",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100047654.1.31",
          "form": "안먹기도 하고요",
          "original_form": "안먹기도 하고요",
          "speaker_id": "2",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100047654.1.32",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100047654.1.33",
          "form": "사서먹였는데도 먹이기힘들었어요",
          "original_form": "사서먹였는데도 먹이기힘들었어요",
          "speaker_id": "1",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100047654.1.34",
          "form": "네 해도 사도 안먹어요",
          "original_form": "네 해도 사도 안먹어요",
          "speaker_id": "2",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100047654.1.35",
          "form": "정말 오죽하면 소금을 ㅜㅜ",
          "original_form": "정말 오죽하면 소금을 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100047654.1.36",
          "form": "해서 먹이려니 하는것도 힘들고\n먹이기도 힘들고",
          "original_form": "해서 먹이려니 하는것도 힘들고\n먹이기도 힘들고",
          "speaker_id": "1",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100047654.1.37",
          "form": "네 요샌 사먹이는게 나아요",
          "original_form": "네 요샌 사먹이는게 나아요",
          "speaker_id": "2",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100047654.1.38",
          "form": "그래서 하나의 힘듦을 제거하러고 주문으로 ㅎㅎ",
          "original_form": "그래서 하나의 힘듦을 제거하러고 주문으로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100047654.1.39",
          "form": "식단도 다양하고",
          "original_form": "식단도 다양하고",
          "speaker_id": "2",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100047654.1.40",
          "form": "집에서 하면 너무 똑같은것만 막이고",
          "original_form": "집에서 하면 너무 똑같은것만 막이고",
          "speaker_id": "2",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100047654.1.41",
          "form": "먹이고",
          "original_form": "먹이고",
          "speaker_id": "2",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100047654.1.42",
          "form": "ㅎㅎ다 그래요",
          "original_form": "ㅎㅎ다 그래요",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047654.1.43",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047654.1.44",
          "form": "단조로워지는 식단 ㅋㅋ 반복식단",
          "original_form": "단조로워지는 식단 ㅋㅋ 반복식단",
          "speaker_id": "1",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047654.1.45",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047654.1.46",
          "form": "맞아요 할때 왕창",
          "original_form": "맞아요 할때 왕창",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047654.1.47",
          "form": "다 똑같네요 ㅎㅎ",
          "original_form": "다 똑같네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100047654.1.48",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100047654.1.49",
          "form": "사는게 먹는게 다같죠",
          "original_form": "사는게 먹는게 다같죠",
          "speaker_id": "2",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100047654.1.50",
          "form": "12시 넘었는데 주무셔야죠",
          "original_form": "12시 넘었는데 주무셔야죠",
          "speaker_id": "1",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100047654.1.51",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:06"
        },
