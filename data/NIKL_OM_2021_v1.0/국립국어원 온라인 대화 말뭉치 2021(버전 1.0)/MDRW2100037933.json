      "utterance": [
        {
          "id": "MDRW2100037933.1.1",
          "form": "기다리고 있었지요ㅋㅋㅋ",
          "original_form": "기다리고 있었지요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037933.1.2",
          "form": "근데 안자나",
          "original_form": "근데 안자나",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037933.1.3",
          "form": "언니 나 백신맞고",
          "original_form": "언니 나 백신맞고",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037933.1.4",
          "form": "초저녁 잠이 좀 없어진 듯 요ㅋㅋㅋ",
          "original_form": "초저녁 잠이 좀 없어진 듯 요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037933.1.5",
          "form": "아까 잠들었닸겠다 했잖아",
          "original_form": "아까 잠들었닸겠다 했잖아",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037933.1.6",
          "form": "잠들었다가 깼다고 했잖아",
          "original_form": "잠들었다가 깼다고 했잖아",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037933.1.7",
          "form": "자꾸 뻥 좀 치지마",
          "original_form": "자꾸 뻥 좀 치지마",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037933.1.8",
          "form": "다이어트 한다고 해놓고 비빔밥 먹고",
          "original_form": "다이어트 한다고 해놓고 비빔밥 먹고",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037933.1.9",
          "form": "초저녁 잠이 없어졌다 해놓고 자다 깨고",
          "original_form": "초저녁 잠이 없어졌다 해놓고 자다 깨고",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037933.1.10",
          "form": "장난하냐 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "장난하냐 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037933.1.11",
          "form": "원래는 푹 자는데",
          "original_form": "원래는 푹 자는데",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100037933.1.12",
          "form": "지금은 깼잖아요 언니ㅋㅋㅋㅋ",
          "original_form": "지금은 깼잖아요 언니ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100037933.1.13",
          "form": "그저께도 그랬고ㅋㅋㅋㅋ",
          "original_form": "그저께도 그랬고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100037933.1.14",
          "form": "아휴 그래 그렇다고 치자",
          "original_form": "아휴 그래 그렇다고 치자",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037933.1.15",
          "form": "믿어줘요 언니ㅋㅋ",
          "original_form": "믿어줘요 언니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037933.1.16",
          "form": "백신 2차 맞고 나서는 뭐가 또 달라질지",
          "original_form": "백신 2차 맞고 나서는 뭐가 또 달라질지",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037933.1.17",
          "form": "기대하고 있을게",
          "original_form": "기대하고 있을게",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037933.1.18",
          "form": "그래 그래 알았다고",
          "original_form": "그래 그래 알았다고",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037933.1.19",
          "form": "아 웃겨ㅋㅋㅋㅋ",
          "original_form": "아 웃겨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037933.1.20",
          "form": "안 그래도 name3한테 계속 나쁘곤하다고",
          "original_form": "안 그래도 &name3&한테 계속 나쁘곤하다고",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037933.1.21",
          "form": "피곤",
          "original_form": "피곤",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037933.1.22",
          "form": "화이자 때문이라며",
          "original_form": "화이자 때문이라며",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037933.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 화이자가 니 땜에 피곤하겠다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 화이자가 니 땜에 피곤하겠다",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037933.1.24",
          "form": "name3는 오늘 카시트 세탁까지 했다던데",
          "original_form": "&name3&는 오늘 카시트 세탁까지 했다던데",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037933.1.25",
          "form": "카시트 세탁은 진짜 연중행사다",
          "original_form": "카시트 세탁은 진짜 연중행사다",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037933.1.26",
          "form": "카시트 애초에 더럽히지 마요ㅋㅋㅋ",
          "original_form": "카시트 애초에 더럽히지 마요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100037933.1.27",
          "form": "힘든 일을 만들지 맙시다",
          "original_form": "힘든 일을 만들지 맙시다",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100037933.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 천재네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 천재네",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100037933.1.29",
          "form": "그냥 달고다니지마까 ㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "그냥 달고다니지마까 ㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100037933.1.30",
          "form": "우린 첫 찌는 카시트 안 타요",
          "original_form": "우린 첫 찌는 카시트 안 타요",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100037933.1.31",
          "form": "초딩 때까지 태우라고는 하던데",
          "original_form": "초딩 때까지 태우라고는 하던데",
          "speaker_id": "1",
          "time": "20210901 21:17"
        }
