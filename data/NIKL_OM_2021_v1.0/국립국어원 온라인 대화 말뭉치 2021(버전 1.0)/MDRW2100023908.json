      "utterance": [
        {
          "id": "MDRW2100023908.1.1",
          "form": "쇼핑은 주로 어디서 하세요~?",
          "original_form": "쇼핑은 주로 어디서 하세요~?",
          "speaker_id": "1",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023908.1.2",
          "form": "생필품같은거!",
          "original_form": "생필품같은거!",
          "speaker_id": "1",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023908.1.3",
          "form": "생필품은 쿠팡로켓이죠ㅎㅎ",
          "original_form": "생필품은 쿠팡로켓이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100023908.1.4",
          "form": "코로나로 쇼핑나가기 힘들어서 ㅜㅠ",
          "original_form": "코로나로 쇼핑나가기 힘들어서 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023908.1.5",
          "form": "쿠팡 배송서비스 좋다고 얘기 많이 들었어요!ㅎㅎ",
          "original_form": "쿠팡 배송서비스 좋다고 얘기 많이 들었어요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023908.1.6",
          "form": "제품도 파손되는거 없이 잘 오나요?",
          "original_form": "제품도 파손되는거 없이 잘 오나요?",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023908.1.7",
          "form": "네 광고는 아닌데ㅋㅋ\n와우회원은 무료반품서비스가 되어서 파손되면 바로 반품서비스가 좋더라구요",
          "original_form": "네 광고는 아닌데ㅋㅋ\n와우회원은 무료반품서비스가 되어서 파손되면 바로 반품서비스가 좋더라구요",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023908.1.8",
          "form": "아 이것도 구독서비스가 있구나",
          "original_form": "아 이것도 구독서비스가 있구나",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023908.1.9",
          "form": "첨 알았어요ㅋㅋㅋㅋ",
          "original_form": "첨 알았어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023908.1.10",
          "form": "좋다좋다하니 팔랑거리네요",
          "original_form": "좋다좋다하니 팔랑거리네요",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023908.1.11",
          "form": "주위에서 좋다좋다ㅋㅋㅋ",
          "original_form": "주위에서 좋다좋다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100023908.1.12",
          "form": "ㅎㅎㅎㅎㅎㅎ아마 코로나 시국이라 나가기  찝찝하다보니 더 좋다고 하는것 같아요",
          "original_form": "ㅎㅎㅎㅎㅎㅎ아마 코로나 시국이라 나가기  찝찝하다보니 더 좋다고 하는것 같아요",
          "speaker_id": "2",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100023908.1.13",
          "form": "아아 그런것도 있죠ㅠㅠ",
          "original_form": "아아 그런것도 있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023908.1.14",
          "form": "저도 코로나 터지고",
          "original_form": "저도 코로나 터지고",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023908.1.15",
          "form": "비누하나를 사도 인터넷쇼핑으로 사게되더라구요",
          "original_form": "비누하나를 사도 인터넷쇼핑으로 사게되더라구요",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023908.1.16",
          "form": "그쵸\n근데 로켓은 보통 다음날이나 새벽에 도착하니까 많이들 사용하는것 같아요ㅎ",
          "original_form": "그쵸\n근데 로켓은 보통 다음날이나 새벽에 도착하니까 많이들 사용하는것 같아요ㅎ",
          "speaker_id": "2",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100023908.1.17",
          "form": "아아 급히 필요할때 넘 좋네요",
          "original_form": "아아 급히 필요할때 넘 좋네요",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100023908.1.18",
          "form": "내일까지",
          "original_form": "내일까지",
          "speaker_id": "1",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100023908.1.19",
          "form": "하나로마트에서 계란 한판에 4900원에 판대요!",
          "original_form": "하나로마트에서 계란 한판에 4900원에 판대요!",
          "speaker_id": "1",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100023908.1.20",
          "form": "오오오 엄청난 소식이네요!",
          "original_form": "오오오 엄청난 소식이네요!",
          "speaker_id": "2",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100023908.1.21",
          "form": "요새 거의 구천원이라ㅜㅜ",
          "original_form": "요새 거의 구천원이라ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100023908.1.22",
          "form": "네네 내일 저도 달려가려구요ㅋㅋㅋ",
          "original_form": "네네 내일 저도 달려가려구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100023908.1.23",
          "form": "농협이라 품질은 보증된것같아요",
          "original_form": "농협이라 품질은 보증된것같아요",
          "speaker_id": "1",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100023908.1.24",
          "form": "우와 좋은 정보감사해요!",
          "original_form": "우와 좋은 정보감사해요!",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100023908.1.25",
          "form": "추석 다가오면 계란값 엄청 오르니까",
          "original_form": "추석 다가오면 계란값 엄청 오르니까",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100023908.1.26",
          "form": "내일 가셔서 득템하셨으면 좋겠네요ㅋㅋㅋ",
          "original_form": "내일 가셔서 득템하셨으면 좋겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:14"
        }
      ]
    }
