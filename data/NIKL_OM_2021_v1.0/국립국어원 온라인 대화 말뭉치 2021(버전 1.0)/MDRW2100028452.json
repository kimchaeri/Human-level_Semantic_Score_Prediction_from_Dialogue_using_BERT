        {
          "id": "MDRW2100028452.1.1",
          "form": "사계절이 있으면 옷장이 가득차",
          "original_form": "사계절이 있으면 옷장이 가득차",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028452.1.2",
          "form": "이불도 계절별로 필요하",
          "original_form": "이불도 계절별로 필요하",
          "speaker_id": "1",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028452.1.3",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028452.1.4",
          "form": "준비할게 많아",
          "original_form": "준비할게 많아",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028452.1.5",
          "form": "날씨 상품들도 필요해",
          "original_form": "날씨 상품들도 필요해",
          "speaker_id": "1",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028452.1.6",
          "form": "겨울엔 히터",
          "original_form": "겨울엔 히터",
          "speaker_id": "1",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028452.1.7",
          "form": "여름에 에어컨",
          "original_form": "여름에 에어컨",
          "speaker_id": "1",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028452.1.8",
          "form": "환절기엔 가습기",
          "original_form": "환절기엔 가습기",
          "speaker_id": "1",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028452.1.9",
          "form": "그래도 날씨변화로 세월을 알게돼",
          "original_form": "그래도 날씨변화로 세월을 알게돼",
          "speaker_id": "2",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028452.1.10",
          "form": "날씨가곧 세월이야",
          "original_form": "날씨가곧 세월이야",
          "speaker_id": "2",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028452.1.11",
          "form": "가을 좋아하니",
          "original_form": "가을 좋아하니",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028452.1.12",
          "form": "은행잎 예쁘게 물들어",
          "original_form": "은행잎 예쁘게 물들어",
          "speaker_id": "2",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100028452.1.13",
          "form": "겨울보단 가을이지",
          "original_form": "겨울보단 가을이지",
