      "utterance": [
        {
          "id": "MDRW2100043347.1.1",
          "form": "안 그래도 그거",
          "original_form": "안 그래도 그거",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043347.1.2",
          "form": "2년 지나서",
          "original_form": "2년 지나서",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043347.1.3",
          "form": "재계약해싼아",
          "original_form": "재계약해싼아",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043347.1.4",
          "form": "돈 또오르대",
          "original_form": "돈 또오르대",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043347.1.5",
          "form": "계속 돈 오르노",
          "original_form": "계속 돈 오르노",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043347.1.6",
          "form": "얼마 살지 못하고 나가야 되는 거 아닌가 그렇게 계속 오르면",
          "original_form": "얼마 살지 못하고 나가야 되는 거 아닌가 그렇게 계속 오르면",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.7",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.8",
          "form": "계속 오르대요",
          "original_form": "계속 오르대요",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.9",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.10",
          "form": "재계약 갈 때마다 오름",
          "original_form": "재계약 갈 때마다 오름",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.11",
          "form": "아니 거기다 돈 없어서",
          "original_form": "아니 거기다 돈 없어서",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.12",
          "form": "바로 신청해요",
          "original_form": "바로 신청해요",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.13",
          "form": "사는 건데",
          "original_form": "사는 건데",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.14",
          "form": "계속 돈 오르니까",
          "original_form": "계속 돈 오르니까",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.15",
          "form": "맞아요 정말 돈 없으면",
          "original_form": "맞아요 정말 돈 없으면",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.16",
          "form": "엘에이치도 못 살아요",
          "original_form": "엘에이치도 못 살아요",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.17",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.18",
          "form": "월세도 비싸다",
          "original_form": "월세도 비싸다",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043347.1.19",
          "form": "관리비도 많이 나오고",
          "original_form": "관리비도 많이 나오고",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043347.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043347.1.21",
          "form": "근데 생각보다",
          "original_form": "근데 생각보다",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043347.1.22",
          "form": "관리비도 생각보다 비 쌈",
          "original_form": "관리비도 생각보다 비 쌈",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043347.1.23",
          "form": "관리비이 더 많이 나오지 않나??",
          "original_form": "관리비이 더 많이 나오지 않나??",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043347.1.24",
          "form": "그니까 어",
          "original_form": "그니까 어",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043347.1.25",
          "form": "거기 관리비 왜 그래 비싼데",
          "original_form": "거기 관리비 왜 그래 비싼데",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043347.1.26",
          "form": "맞아요 한칠만원 정도",
          "original_form": "맞아요 한칠만원 정도",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043347.1.27",
          "form": "세대 수도 진짜 많은데",
          "original_form": "세대 수도 진짜 많은데",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043347.1.28",
          "form": "아빠 혼자 사는데ㅋㅋㅋㅋ",
          "original_form": "아빠 혼자 사는데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043347.1.29",
          "form": "관리비가 너무 많다",
          "original_form": "관리비가 너무 많다",
          "speaker_id": "2",
          "time": "20210907 18:05"
