      "utterance": [
        {
          "id": "MDRW2100010256.1.1",
          "form": "요새 날씨 개똥구림",
          "original_form": "요새 날씨 개똥구림",
          "speaker_id": "2",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010256.1.2",
          "form": "지금 비 오니",
          "original_form": "지금 비 오니",
          "speaker_id": "1",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010256.1.3",
          "form": "여기는 안와",
          "original_form": "여기는 안와",
          "speaker_id": "2",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010256.1.4",
          "form": "비때문에 야구를 안해서 너무 슬퍼",
          "original_form": "비때문에 야구를 안해서 너무 슬퍼",
          "speaker_id": "1",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010256.1.5",
          "form": "여기는 오는지 안오는지 모르겠다",
          "original_form": "여기는 오는지 안오는지 모르겠다",
          "speaker_id": "1",
          "time": "20210823 16:08"
        },
        {
          "id": "MDRW2100010256.1.6",
          "form": "내일도 모레도 비와",
          "original_form": "내일도 모레도 비와",
          "speaker_id": "1",
          "time": "20210823 16:08"
        },
        {
          "id": "MDRW2100010256.1.7",
          "form": "태풍이래",
          "original_form": "태풍이래",
          "speaker_id": "1",
          "time": "20210823 16:08"
        },
        {
          "id": "MDRW2100010256.1.8",
          "form": "내일이 많이 온대",
          "original_form": "내일이 많이 온대",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010256.1.9",
          "form": "재작년에 이맘때쯤 태풍때문에 일본에서 못돌아온생각이나",
          "original_form": "재작년에 이맘때쯤 태풍때문에 일본에서 못돌아온생각이나",
          "speaker_id": "1",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010256.1.10",
          "form": "학교가기 정말 귀찮겠어",
          "original_form": "학교가기 정말 귀찮겠어",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010256.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010256.1.12",
          "form": "그때... 아찔했지",
          "original_form": "그때... 아찔했지",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010256.1.13",
          "form": "정말 끔찍한 태풍이었어..",
          "original_form": "정말 끔찍한 태풍이었어..",
          "speaker_id": "1",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010256.1.14",
          "form": "내일부터는 장화신고다녀야지",
          "original_form": "내일부터는 장화신고다녀야지",
          "speaker_id": "1",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010256.1.15",
          "form": "장화샀니",
          "original_form": "장화샀니",
          "speaker_id": "2",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010256.1.16",
          "form": "아주 예쁜 파란장화야",
          "original_form": "아주 예쁜 파란장화야",
          "speaker_id": "1",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010256.1.17",
          "form": "멋지구나",
          "original_form": "멋지구나",
          "speaker_id": "2",
          "time": "20210823 17:07"
        },
        {
          "id": "MDRW2100010256.1.18",
          "form": "나는 이런날에 옷이랑 신발에 곰팡이 피는게 정말 싫어",
          "original_form": "나는 이런날에 옷이랑 신발에 곰팡이 피는게 정말 싫어",
          "speaker_id": "2",
          "time": "20210823 17:07"
        },
        {
          "id": "MDRW2100010256.1.19",
          "form": "너무 싫어 습기",
          "original_form": "너무 싫어 습기",
          "speaker_id": "1",
          "time": "20210823 17:07"
        },
        {
          "id": "MDRW2100010256.1.20",
          "form": "최악이야",
          "original_form": "최악이야",
          "speaker_id": "2",
          "time": "20210823 17:07"
        },
        {
          "id": "MDRW2100010256.1.21",
          "form": "곰팡이 잘 없어지지도 않는다고",
          "original_form": "곰팡이 잘 없어지지도 않는다고",
          "speaker_id": "2",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010256.1.22",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010256.1.23",
          "form": "비염도 심해져",
          "original_form": "비염도 심해져",
          "speaker_id": "1",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010256.1.24",
          "form": "백신맞으러가는날도 비온대 너무해~~",
          "original_form": "백신맞으러가는날도 비온대 너무해~~",
