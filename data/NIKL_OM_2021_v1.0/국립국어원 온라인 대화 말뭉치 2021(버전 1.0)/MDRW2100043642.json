      "utterance": [
        {
          "id": "MDRW2100043642.1.1",
          "form": "통영에선 다찌라고 하는데 가봤는데",
          "original_form": "통영에선 다찌라고 하는데 가봤는데",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043642.1.2",
          "form": "해산물 다양하게 나와서 좋았어요",
          "original_form": "해산물 다양하게 나와서 좋았어요",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043642.1.3",
          "form": "음 해물탕집인가요?",
          "original_form": "음 해물탕집인가요?",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043642.1.4",
          "form": "아니면 구이?",
          "original_form": "아니면 구이?",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043642.1.5",
          "form": "그냥 식당같은건데 주문안하고 인당 코스요리처럼 나와요ㅋㅋ",
          "original_form": "그냥 식당같은건데 주문안하고 인당 코스요리처럼 나와요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043642.1.6",
          "form": "회랑 무침이랑 매운탕이랑",
          "original_form": "회랑 무침이랑 매운탕이랑",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043642.1.7",
          "form": "와 신기하네요ㅋㅋㅋㅋ",
          "original_form": "와 신기하네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043642.1.8",
          "form": "반찬도 많았는데 다 기억안나네요",
          "original_form": "반찬도 많았는데 다 기억안나네요",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043642.1.9",
          "form": "해산물 좋아하세요?",
          "original_form": "해산물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043642.1.10",
          "form": "네 좋아해요ㅋㅋ",
          "original_form": "네 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043642.1.11",
          "form": "해산물 중에 제일 좋아하시는거 뭔가요!",
          "original_form": "해산물 중에 제일 좋아하시는거 뭔가요!",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043642.1.12",
          "form": "저는 새우요!!!",
          "original_form": "저는 새우요!!!",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043642.1.13",
          "form": "저두 새우 좋아해요",
          "original_form": "저두 새우 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043642.1.14",
          "form": "연어도요ㅋㅋㅋ",
          "original_form": "연어도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043642.1.15",
          "form": "아 생선 종류까지 나올줄은 몰랐는데요!ㅋㅋㅋㅋㅋ",
          "original_form": "아 생선 종류까지 나올줄은 몰랐는데요!ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043642.1.16",
          "form": "그럼 저는 광어하겠어요",
          "original_form": "그럼 저는 광어하겠어요",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043642.1.17",
          "form": "name2님 독도새우 드셔보셨어요?",
          "original_form": "&name2&님 독도새우 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043642.1.18",
          "form": "그게 그렇게 맛있다던데",
          "original_form": "그게 그렇게 맛있다던데",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043642.1.19",
          "form": "전 본적도 없어요",
          "original_form": "전 본적도 없어요",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043642.1.20",
          "form": "저도 못먹어봤어요",
          "original_form": "저도 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043642.1.21",
          "form": "비싸기도 하던데요",
          "original_form": "비싸기도 하던데요",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043642.1.22",
          "form": "달다던데 어떤 맛일지 궁금해요",
          "original_form": "달다던데 어떤 맛일지 궁금해요",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043642.1.23",
          "form": "맞아요 엄청 비싸다고 그러던데 로또 맞으면 제가 먹어볼게요ㅋㅋㅋ",
          "original_form": "맞아요 엄청 비싸다고 그러던데 로또 맞으면 제가 먹어볼게요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043642.1.24",
          "form": "ㅋㅋㅋㅋ비싼거 잔뜩 먹어볼 수 있겠네요",
