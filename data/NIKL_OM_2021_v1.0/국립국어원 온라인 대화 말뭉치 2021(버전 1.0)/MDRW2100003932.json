      "utterance": [
        {
          "id": "MDRW2100003932.1.1",
          "form": "거기도 비가 많이 오나요?",
          "original_form": "거기도 비가 많이 오나요?",
          "speaker_id": "1",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003932.1.2",
          "form": "와 딱 방금 그쳤어요.",
          "original_form": "와 딱 방금 그쳤어요.",
          "speaker_id": "2",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003932.1.3",
          "form": "2시까지 비온다더니 정말 딱....해가 떴습니다.",
          "original_form": "2시까지 비온다더니 정말 딱....해가 떴습니다.",
          "speaker_id": "2",
          "time": "20210821 14:00"
        },
        {
          "id": "MDRW2100003932.1.4",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "1",
          "time": "20210821 14:00"
        },
        {
          "id": "MDRW2100003932.1.5",
          "form": "여기는 비가 엄청나게 와요 ㅠㅠㅠㅠ",
          "original_form": "여기는 비가 엄청나게 와요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:00"
        },
        {
          "id": "MDRW2100003932.1.6",
          "form": "진짜 오늘 비가 무슨 홍수날 것처럼 엄청나게 내리더라구요",
          "original_form": "진짜 오늘 비가 무슨 홍수날 것처럼 엄청나게 내리더라구요",
          "speaker_id": "2",
          "time": "20210821 14:03"
        },
        {
          "id": "MDRW2100003932.1.7",
          "form": "맞아요... 하염없이 계속 내려요",
          "original_form": "맞아요... 하염없이 계속 내려요",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003932.1.8",
          "form": "번개도 칠까 두렴네요",
          "original_form": "번개도 칠까 두렴네요",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003932.1.9",
          "form": "아 번개 너무 싫어요",
          "original_form": "아 번개 너무 싫어요",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100003932.1.10",
          "form": "천둥 소리에 깜짝깜짝 놀라게 되어요..",
          "original_form": "천둥 소리에 깜짝깜짝 놀라게 되어요..",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100003932.1.11",
          "form": "맞아요 다행히 지금은",
          "original_form": "맞아요 다행히 지금은",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100003932.1.12",
          "form": "비가 멈췄어요",
          "original_form": "비가 멈췄어요",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100003932.1.13",
          "form": "또 비가 오다가 그쳤다가 하네요",
          "original_form": "또 비가 오다가 그쳤다가 하네요",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100003932.1.14",
          "form": "줏대 없는 날씨입니다...",
          "original_form": "줏대 없는 날씨입니다...",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100003932.1.15",
          "form": "정말요....",
          "original_form": "정말요....",
          "speaker_id": "1",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100003932.1.16",
          "form": "지금도 안오는데 바람이 많이 안불어요",
          "original_form": "지금도 안오는데 바람이 많이 안불어요",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100003932.1.17",
          "form": "그래서 조금 더운 거 같아요",
          "original_form": "그래서 조금 더운 거 같아요",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100003932.1.18",
          "form": "지금 그래서 창문을 사방 팔방 다 열어 놨어요",
          "original_form": "지금 그래서 창문을 사방 팔방 다 열어 놨어요",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100003932.1.19",
          "form": "조금이라도 공기 순환해서 시원해지라고",
          "original_form": "조금이라도 공기 순환해서 시원해지라고",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100003932.1.20",
          "form": "저도 그래야겠어요",
          "original_form": "저도 그래야겠어요",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100003932.1.21",
          "form": "비온 뒤라 공기도 맑아서 환기도 되고 좋죠",
          "original_form": "비온 뒤라 공기도 맑아서 환기도 되고 좋죠",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100003932.1.22",
          "form": "네 !!! 저도 문 열어놨아요",
          "original_form": "네 !!! 저도 문 열어놨아요",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100003932.1.23",
          "form": "바람은 안오지만..",
          "original_form": "바람은 안오지만..",
          "speaker_id": "1",
          "time": "20210822 01:39"
        }
      ]
