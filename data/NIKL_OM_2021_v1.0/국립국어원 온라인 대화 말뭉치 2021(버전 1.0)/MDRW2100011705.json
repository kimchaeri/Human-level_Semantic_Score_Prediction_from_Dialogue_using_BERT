      "utterance": [
        {
          "id": "MDRW2100011705.1.1",
          "form": "점심식사는 뭐하셨어요?",
          "original_form": "점심식사는 뭐하셨어요?",
          "speaker_id": "1",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011705.1.2",
          "form": "순대국밥이요",
          "original_form": "순대국밥이요",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011705.1.3",
          "form": "도드람순대국 인데 맛있어요",
          "original_form": "도드람순대국 인데 맛있어요",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011705.1.4",
          "form": "와 맛있었겠어요 저는 컵라면 먹었어요",
          "original_form": "와 맛있었겠어요 저는 컵라면 먹었어요",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011705.1.5",
          "form": "체인점인가요",
          "original_form": "체인점인가요",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011705.1.6",
          "form": "그럴거같아요  처음엔 홍진영이 였는데 지금은 류수영이 네요 모델이",
          "original_form": "그럴거같아요  처음엔 홍진영이 였는데 지금은 류수영이 네요 모델이",
          "speaker_id": "2",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011705.1.7",
          "form": "아 기회되면 도드람 먹어봐야겠어요",
          "original_form": "아 기회되면 도드람 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011705.1.8",
          "form": "진짜 다르더라고요",
          "original_form": "진짜 다르더라고요",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011705.1.9",
          "form": "남편이 제일좋아하는 음식인데 근처 매장을 찾아봐야겠어요",
          "original_form": "남편이 제일좋아하는 음식인데 근처 매장을 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 15:27"
        },
        {
          "id": "MDRW2100011705.1.10",
          "form": "맛있길 바래요 ㅎㅎ",
          "original_form": "맛있길 바래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:34"
        },
        {
          "id": "MDRW2100011705.1.11",
          "form": "이름이 본래순대 네요",
          "original_form": "이름이 본래순대 네요",
          "speaker_id": "2",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011705.1.12",
          "form": "네 감사해요 저녁식사 고민이었어요",
          "original_form": "네 감사해요 저녁식사 고민이었어요",
          "speaker_id": "1",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011705.1.13",
          "form": "ㅎㅎ 요샌더우서 해먹기도 싫네요",
          "original_form": "ㅎㅎ 요샌더우서 해먹기도 싫네요",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011705.1.14",
          "form": "네 그래서 배달음식도 자주 먹게 되어요",
          "original_form": "네 그래서 배달음식도 자주 먹게 되어요",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011705.1.15",
          "form": "뭐 배날시키시나요?",
          "original_form": "뭐 배날시키시나요?",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011705.1.16",
          "form": "배달",
          "original_form": "배달",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011705.1.17",
          "form": "저는 족발이나 마라탕 배달해먹어요",
          "original_form": "저는 족발이나 마라탕 배달해먹어요",
          "speaker_id": "1",
          "time": "20210825 11:29"
        },
        {
          "id": "MDRW2100011705.1.18",
          "form": "어제는 교촌 배달했어요",
          "original_form": "어제는 교촌 배달했어요",
          "speaker_id": "1",
          "time": "20210825 11:29"
        },
        {
          "id": "MDRW2100011705.1.19",
          "form": "치킨은 언제나 좋죠",
          "original_form": "치킨은 언제나 좋죠",
          "speaker_id": "2",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100011705.1.20",
          "form": "네 비오니 기름에 튀긴게 생각나더라고요 ㅎㅎ",
          "original_form": "네 비오니 기름에 튀긴게 생각나더라고요 ㅎㅎ",
