      "utterance": [
        {
          "id": "MDRW2100026397.1.1",
          "form": "그거 막 집게로 집게",
          "original_form": "그거 막 집게로 집게",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026397.1.2",
          "form": "두거나 하더라",
          "original_form": "두거나 하더라",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026397.1.3",
          "form": "아 다들 말 고만걸어~",
          "original_form": "아 다들 말 고만걸어~",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.4",
          "form": "그래도찝찝해이재는",
          "original_form": "그래도찝찝해이재는",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.5",
          "form": "수강신청을 성공햿냐니",
          "original_form": "수강신청을 성공햿냐니",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.6",
          "form": "인기많은데~",
          "original_form": "인기많은데~",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.7",
          "form": "졸. 업. 생. 입니다",
          "original_form": "졸. 업. 생. 입니다",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.8",
          "form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.9",
          "form": "대학을다니냐부터",
          "original_form": "대학을다니냐부터",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.10",
          "form": "물어봐야하는거 아닌가? ㅡㅡ",
          "original_form": "물어봐야하는거 아닌가? ㅡㅡ",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.11",
          "form": "ㄱㄴㄲ 너무 당연하게",
          "original_form": "ㄱㄴㄲ 너무 당연하게",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.12",
          "form": "물어보네!?!!",
          "original_form": "물어보네!?!!",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.13",
          "form": "닌 실패햇니?",
          "original_form": "닌 실패햇니?",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.14",
          "form": "저 집에 마들렌 있어요",
          "original_form": "저 집에 마들렌 있어요",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.15",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026397.1.16",
          "form": "진짜맛잇겟다",
          "original_form": "진짜맛잇겟다",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.17",
          "form": "먹고싳은데",
          "original_form": "먹고싳은데",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.18",
          "form": "밥도 먹어야해서",
          "original_form": "밥도 먹어야해서",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.19",
          "form": "마들렌 최근에 좀 땡기더라",
          "original_form": "마들렌 최근에 좀 땡기더라",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.20",
          "form": "입맛 없어질까봐",
          "original_form": "입맛 없어질까봐",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.21",
          "form": "냅두는 중",
          "original_form": "냅두는 중",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.22",
          "form": "마들렌에",
          "original_form": "마들렌에",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.23",
          "form": "초코같은거",
          "original_form": "초코같은거",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.24",
          "form": "코팅된거",
          "original_form": "코팅된거",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.25",
          "form": "달달한 브레드가 막있어",
          "original_form": "달달한 브레드가 막있어",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.26",
          "form": "먹어보고싶음",
          "original_form": "먹어보고싶음",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.27",
          "form": "파운드케이크도 먹고싶다",
          "original_form": "파운드케이크도 먹고싶다",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.28",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.29",
          "form": "아맛잇겟다",
          "original_form": "아맛잇겟다",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.30",
          "form": "수제케이크",
          "original_form": "수제케이크",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026397.1.31",
          "form": "디저트류",
          "original_form": "디저트류",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026397.1.32",
          "form": "먹고싶음",
          "original_form": "먹고싶음",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026397.1.33",
          "form": "나는 빵이 아무맛이 안나는게 너무 맛없다",
          "original_form": "나는 빵이 아무맛이 안나는게 너무 맛없다",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026397.1.34",
          "form": "너무 먹고싶음이",
          "original_form": "너무 먹고싶음이",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026397.1.35",
          "form": "빵에 설탕을 아주 팍팍 넣ㅇ느게 맛나",
          "original_form": "빵에 설탕을 아주 팍팍 넣ㅇ느게 맛나",
          "speaker_id": "2",
          "time": "20210901 22:03"
        }
      ]
    }
