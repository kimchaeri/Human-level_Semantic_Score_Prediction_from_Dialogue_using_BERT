      "utterance": [
        {
          "id": "MDRW2100032340.1.1",
          "form": "문스독 중에 다자이가 좋더라고요 ㅎㅎ",
          "original_form": "문스독 중에 다자이가 좋더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032340.1.2",
          "form": "오, 다자이ㅋㅋㅋ 머리좋은 캐릭터 좋아하세요?",
          "original_form": "오, 다자이ㅋㅋㅋ 머리좋은 캐릭터 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100032340.1.3",
          "form": "네 똑똑하면 매력있잖아요 얼굴도 좋고 ㅋㅋ",
          "original_form": "네 똑똑하면 매력있잖아요 얼굴도 좋고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100032340.1.4",
          "form": "맞아요ㅋㅋㅋ 재색겸비",
          "original_form": "맞아요ㅋㅋㅋ 재색겸비",
          "speaker_id": "2",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100032340.1.5",
          "form": "문스독 2기까지 봤어요??",
          "original_form": "문스독 2기까지 봤어요??",
          "speaker_id": "1",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100032340.1.6",
          "form": "저 3기랑 극장판까지요ㅋㅋㅋ 네타도 보고있어요",
          "original_form": "저 3기랑 극장판까지요ㅋㅋㅋ 네타도 보고있어요",
          "speaker_id": "2",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100032340.1.7",
          "form": "3기까지 나왔어요????",
          "original_form": "3기까지 나왔어요????",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100032340.1.8",
          "form": "네네, 19년도에 나온게 3기요",
          "original_form": "네네, 19년도에 나온게 3기요",
          "speaker_id": "2",
          "time": "20210905 16:05"
        },
        {
          "id": "MDRW2100032340.1.9",
          "form": "너무 모르고 있었네요...",
          "original_form": "너무 모르고 있었네요...",
          "speaker_id": "1",
          "time": "20210905 16:05"
        },
        {
          "id": "MDRW2100032340.1.10",
          "form": "근데 네타가 뭔가요?",
          "original_form": "근데 네타가 뭔가요?",
          "speaker_id": "1",
          "time": "20210905 16:05"
        },
        {
          "id": "MDRW2100032340.1.11",
          "form": "일본 만화잡지 연재분이요",
          "original_form": "일본 만화잡지 연재분이요",
          "speaker_id": "2",
          "time": "20210905 16:06"
        },
        {
          "id": "MDRW2100032340.1.12",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210905 16:07"
        },
        {
          "id": "MDRW2100032340.1.13",
          "form": "문스독 왓챠에는 안올라왔을까요?",
          "original_form": "문스독 왓챠에는 안올라왔을까요?",
          "speaker_id": "1",
          "time": "20210905 16:07"
        },
        {
          "id": "MDRW2100032340.1.14",
          "form": "넷플릭스에는 있었던 것 같아요, 왓챠는 모르겠네요",
          "original_form": "넷플릭스에는 있었던 것 같아요, 왓챠는 모르겠네요",
          "speaker_id": "2",
          "time": "20210905 16:08"
        },
        {
          "id": "MDRW2100032340.1.15",
          "form": "나중에 확인해봐야겠어요",
          "original_form": "나중에 확인해봐야겠어요",
          "speaker_id": "1",
          "time": "20210905 16:09"
        },
        {
          "id": "MDRW2100032340.1.16",
          "form": "제가 다음달 중에 왓챠 구독할 예정이라서 ㅎㅎ",
          "original_form": "제가 다음달 중에 왓챠 구독할 예정이라서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:09"
        },
        {
          "id": "MDRW2100032340.1.17",
          "form": "아하, 왓챠 구독하시려는구나",
          "original_form": "아하, 왓챠 구독하시려는구나",
          "speaker_id": "2",
          "time": "20210905 16:12"
        }
