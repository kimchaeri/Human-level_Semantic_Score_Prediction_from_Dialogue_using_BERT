      "utterance": [
        {
          "id": "MDRW2100005820.1.1",
          "form": "이스라엘 여행을 갔었어요",
          "original_form": "이스라엘 여행을 갔었어요",
          "speaker_id": "1",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100005820.1.2",
          "form": "예전에 다녀온거죠?",
          "original_form": "예전에 다녀온거죠?",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100005820.1.3",
          "form": "네 한 15년전이요 이제는 못갈거에요",
          "original_form": "네 한 15년전이요 이제는 못갈거에요",
          "speaker_id": "1",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100005820.1.4",
          "form": "어떻게 가셨지요",
          "original_form": "어떻게 가셨지요",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100005820.1.5",
          "form": "두바이 거쳐서 15시간 걸려서 이집트로 갔어요",
          "original_form": "두바이 거쳐서 15시간 걸려서 이집트로 갔어요",
          "speaker_id": "1",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100005820.1.6",
          "form": "출장인가요",
          "original_form": "출장인가요",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100005820.1.7",
          "form": "아니요 성지순례였어요 요르단-이집트-이스라엘 세 곳이요",
          "original_form": "아니요 성지순례였어요 요르단-이집트-이스라엘 세 곳이요",
          "speaker_id": "1",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100005820.1.8",
          "form": "특별한 여행하셨네요",
          "original_form": "특별한 여행하셨네요",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100005820.1.9",
          "form": "이스라엘에서는 데드씨가 기억나요",
          "original_form": "이스라엘에서는 데드씨가 기억나요",
          "speaker_id": "1",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100005820.1.10",
          "form": "진짜 물에뜨나요?",
          "original_form": "진짜 물에뜨나요?",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005820.1.11",
          "form": "염도가 너무 높아서 그냥 둥둥떠요 신기했어요",
          "original_form": "염도가 너무 높아서 그냥 둥둥떠요 신기했어요",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005820.1.12",
          "form": "그리고요?",
          "original_form": "그리고요?",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005820.1.13",
          "form": "갈릴리 호수와 베들레헴도 좋았어요",
          "original_form": "갈릴리 호수와 베들레헴도 좋았어요",
