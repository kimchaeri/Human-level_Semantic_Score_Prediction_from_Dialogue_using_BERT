      "utterance": [
        {
          "id": "MDRW2100003382.1.1",
          "form": "혹시 해외여행 가신적 있으세요?",
          "original_form": "혹시 해외여행 가신적 있으세요?",
          "speaker_id": "1",
          "time": "20210820 22:23"
        },
        {
          "id": "MDRW2100003382.1.2",
          "form": "저 일본 가봤어요",
          "original_form": "저 일본 가봤어요",
          "speaker_id": "2",
          "time": "20210820 22:41"
        },
        {
          "id": "MDRW2100003382.1.3",
          "form": "그게 마지막입니다 하하",
          "original_form": "그게 마지막입니다 하하",
          "speaker_id": "2",
          "time": "20210820 22:41"
        },
        {
          "id": "MDRW2100003382.1.4",
          "form": "일본 어디가보셨어요?",
          "original_form": "일본 어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210820 22:42"
        },
        {
          "id": "MDRW2100003382.1.5",
          "form": "전 오사카요 ㅎ",
          "original_form": "전 오사카요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 22:42"
        },
        {
          "id": "MDRW2100003382.1.6",
          "form": "name1님은 일본 어디 가셨나요?",
          "original_form": "&name1&님은 일본 어디 가셨나요?",
          "speaker_id": "2",
          "time": "20210820 22:42"
        },
        {
          "id": "MDRW2100003382.1.7",
          "form": "저는 해외여행 못가봤어요..",
          "original_form": "저는 해외여행 못가봤어요..",
          "speaker_id": "1",
          "time": "20210820 22:43"
        },
        {
          "id": "MDRW2100003382.1.8",
          "form": "1~2년후에 일본 가지않을까 싶네요",
          "original_form": "1~2년후에 일본 가지않을까 싶네요",
          "speaker_id": "1",
          "time": "20210820 22:43"
        },
        {
          "id": "MDRW2100003382.1.9",
          "form": "그때 정도면 코로나가 풀릴테니까요",
          "original_form": "그때 정도면 코로나가 풀릴테니까요",
          "speaker_id": "2",
          "time": "20210821 01:09"
        },
        {
          "id": "MDRW2100003382.1.10",
          "form": "일본말고 어디 또 가고싶으세요?",
          "original_form": "일본말고 어디 또 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210821 01:09"
        },
        {
          "id": "MDRW2100003382.1.11",
          "form": "유럽쪽이나 우리나라랑 분위기 많이 다른곳 가보고싶네요",
          "original_form": "유럽쪽이나 우리나라랑 분위기 많이 다른곳 가보고싶네요",
          "speaker_id": "1",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003382.1.12",
          "form": "오 저두요 ㅎ",
          "original_form": "오 저두요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003382.1.13",
          "form": "정말 가고싶네요",
          "original_form": "정말 가고싶네요",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003382.1.14",
          "form": "일본 가셨을때 뭐가 제일 좋았어요?",
          "original_form": "일본 가셨을때 뭐가 제일 좋았어요?",
          "speaker_id": "1",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003382.1.15",
          "form": "분위기랑이 좋았어요!!",
          "original_form": "분위기랑이 좋았어요!!",
          "speaker_id": "2",
          "time": "20210821 13:20"
        },
        {
          "id": "MDRW2100003382.1.16",
          "form": "일본음식 맛있었어요?",
          "original_form": "일본음식 맛있었어요?",
          "speaker_id": "1",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100003382.1.17",
          "form": "네 오꼬나미야끼가 맛있었어요",
          "original_form": "네 오꼬나미야끼가 맛있었어요",
          "speaker_id": "2",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100003382.1.18",
          "form": "온천도 가보셨어요?",
          "original_form": "온천도 가보셨어요?",
