      "utterance": [
        {
          "id": "MDRW2100010371.1.1",
          "form": "올여름 여행다녀오셨나요?",
          "original_form": "올여름 여행다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010371.1.2",
          "form": "전 코로나땜에 집콕이요",
          "original_form": "전 코로나땜에 집콕이요",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010371.1.3",
          "form": "휴가 다녀오셨어요?",
          "original_form": "휴가 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010371.1.4",
          "form": "가평으로 피서다녀왔어요!",
          "original_form": "가평으로 피서다녀왔어요!",
          "speaker_id": "2",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010371.1.5",
          "form": "오 가평좋죠ㅎ",
          "original_form": "오 가평좋죠ㅎ",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010371.1.6",
          "form": "가족여행가신거예요?",
          "original_form": "가족여행가신거예요?",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010371.1.7",
          "form": "네! 기회가 돼서 다녀왔어요",
          "original_form": "네! 기회가 돼서 다녀왔어요",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010371.1.8",
          "form": "닭갈비가 맛있더라구요",
          "original_form": "닭갈비가 맛있더라구요",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010371.1.9",
          "form": "가평도 닭갈비가 유명한가요?",
          "original_form": "가평도 닭갈비가 유명한가요?",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010371.1.10",
          "form": "춘천이아니고?ㅎ",
          "original_form": "춘천이아니고?ㅎ",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010371.1.11",
          "form": "그렇더라고요ㅋㅋㅋ 숯불닭갈비!",
          "original_form": "그렇더라고요ㅋㅋㅋ 숯불닭갈비!",
          "speaker_id": "2",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010371.1.12",
          "form": "아 저도 가평가서 먹어본거같기도ㅎ",
          "original_form": "아 저도 가평가서 먹어본거같기도ㅎ",
          "speaker_id": "1",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010371.1.13",
          "form": "냉면집 다음으로 많더라고요ㅋㅋ",
          "original_form": "냉면집 다음으로 많더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010371.1.14",
          "form": "티비에 나온곳이랬는데 십년전이라 기억이 가물가물하네요",
          "original_form": "티비에 나온곳이랬는데 십년전이라 기억이 가물가물하네요",
          "speaker_id": "1",
          "time": "20210823 17:27"
        },
        {
          "id": "MDRW2100010371.1.15",
          "form": "엄청 맛집이었나봐용",
          "original_form": "엄청 맛집이었나봐용",
          "speaker_id": "2",
          "time": "20210823 17:28"
        },
        {
          "id": "MDRW2100010371.1.16",
          "form": "맛있긴 하더라구요 보통은 철판인데 숯불이라 색다르고",
          "original_form": "맛있긴 하더라구요 보통은 철판인데 숯불이라 색다르고",
          "speaker_id": "1",
          "time": "20210823 17:28"
        },
        {
          "id": "MDRW2100010371.1.17",
          "form": "가평은 뭐 구경하고 오셨어요?",
          "original_form": "가평은 뭐 구경하고 오셨어요?",
          "speaker_id": "1",
          "time": "20210823 17:29"
        },
        {
          "id": "MDRW2100010371.1.18",
          "form": "계곡이랑 아침고요수목원다녀왔어요",
          "original_form": "계곡이랑 아침고요수목원다녀왔어요",
          "speaker_id": "2",
          "time": "20210823 17:30"
        }
      ]
