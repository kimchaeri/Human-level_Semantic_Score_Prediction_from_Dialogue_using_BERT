      "utterance": [
        {
          "id": "MDRW2100039866.1.1",
          "form": "저녁 뭐드셨어요",
          "original_form": "저녁 뭐드셨어요",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039866.1.2",
          "form": "저는 땅땅치킨 먹엌ㅅ어요",
          "original_form": "저는 땅땅치킨 먹엌ㅅ어요",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039866.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039866.1.4",
          "form": "저눈 감자탕이요",
          "original_form": "저눈 감자탕이요",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100039866.1.5",
          "form": "아 아까 말해주셨는데 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 아까 말해주셨는데 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100039866.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100039866.1.7",
          "form": "감자탕 좋죠 ㅠㅠ",
          "original_form": "감자탕 좋죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039866.1.8",
          "form": "볶음밥은요?",
          "original_form": "볶음밥은요?",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039866.1.9",
          "form": "그냥 맨밥에 먹었어요",
          "original_form": "그냥 맨밥에 먹었어요",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039866.1.10",
          "form": "마지막에 먹는 살짝탄밥 땡기네요 ㅎㅎ",
          "original_form": "마지막에 먹는 살짝탄밥 땡기네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039866.1.11",
          "form": "으악 맞아요 긁어먹는 밥 ㅋㅋ",
          "original_form": "으악 맞아요 긁어먹는 밥 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039866.1.12",
          "form": "뼈에 살이 많던가요?",
          "original_form": "뼈에 살이 많던가요?",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100039866.1.13",
          "form": "네네많았어요",
          "original_form": "네네많았어요",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039866.1.14",
          "form": "낼 아침은 또 뭘먹죠",
          "original_form": "낼 아침은 또 뭘먹죠",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039866.1.15",
          "form": "오 좋은곳이네요",
          "original_form": "오 좋은곳이네요",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039866.1.16",
          "form": "전 일단 늦잠 잘래요 ㅎㅎ",
          "original_form": "전 일단 늦잠 잘래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039866.1.17",
          "form": "저는내일 회사 당직입니다 ㅠㅠ",
          "original_form": "저는내일 회사 당직입니다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039866.1.18",
          "form": "슬퍼요",
          "original_form": "슬퍼요",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039866.1.19",
          "form": "ㅠㅠㅠㅠ 아침 든든하게 드셔요",
          "original_form": "ㅠㅠㅠㅠ 아침 든든하게 드셔요",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039866.1.20",
          "form": "네네 먹고 출근해야져",
          "original_form": "네네 먹고 출근해야져",
          "speaker_id": "1",
