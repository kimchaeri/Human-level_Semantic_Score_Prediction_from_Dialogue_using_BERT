      "utterance": [
        {
          "id": "MDRW2100029219.1.1",
          "form": "부활에 관심있으신가요",
          "original_form": "부활에 관심있으신가요",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029219.1.2",
          "form": "예전에 싸이월드 하던때가 생각나네요",
          "original_form": "예전에 싸이월드 하던때가 생각나네요",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029219.1.3",
          "form": "고심해서 브금 고르고 그랬는데",
          "original_form": "고심해서 브금 고르고 그랬는데",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029219.1.4",
          "form": "아랸",
          "original_form": "아랸",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029219.1.5",
          "form": "아랸해진 name1님....",
          "original_form": "아랸해진 &name1&님....",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029219.1.6",
          "form": "사실 전 싸이월드는 안해봤어요ㅋㅋㅋㅋㅋ 네이버 블로그만 했어서",
          "original_form": "사실 전 싸이월드는 안해봤어요ㅋㅋㅋㅋㅋ 네이버 블로그만 했어서",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029219.1.7",
          "form": "싸이월드에 무슨 글 올리셧나요 흥미생김",
          "original_form": "싸이월드에 무슨 글 올리셧나요 흥미생김",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029219.1.8",
          "form": "ㅇㄴ진짜요?",
          "original_form": "ㅇㄴ진짜요?",
          "speaker_id": "1",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029219.1.9",
          "form": "하긴 저 싸이월드 하던 시절에... 싸이월드 진짜 반짝 지나갔거든요",
          "original_form": "하긴 저 싸이월드 하던 시절에... 싸이월드 진짜 반짝 지나갔거든요",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029219.1.10",
          "form": "완전 딩초시절에 좀 하다가 중학교때 잠깐?",
          "original_form": "완전 딩초시절에 좀 하다가 중학교때 잠깐?",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029219.1.11",
          "form": "중학교때....\r\n......\r\n했다고 하면 대충 예상 가능하지 않나요",
          "original_form": "중학교때....\r\n......\r\n했다고 하면 대충 예상 가능하지 않나요",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029219.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네...",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029219.1.13",
          "form": "뭔가여러가지 스쳐가네요",
          "original_form": "뭔가여러가지 스쳐가네요",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029219.1.14",
          "form": "님의 과거가 남아있겠군요...",
          "original_form": "님의 과거가 남아있겠군요...",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029219.1.15",
          "form": "근데전에 정말 부활해서 예전자료 살린다는 말을 얼핏 들은거같은데",
          "original_form": "근데전에 정말 부활해서 예전자료 살린다는 말을 얼핏 들은거같은데",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029219.1.16",
          "form": "보셧나요",
          "original_form": "보셧나요",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029219.1.17",
          "form": "그럼 이제.... 네이트 아이디 살려서 완전히 폭파시켜야함",
          "original_form": "그럼 이제.... 네이트 아이디 살려서 완전히 폭파시켜야함",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029219.1.18",
          "form": "싸이월드 어차피 할 사람도 없을 텐디 왜 살리는지 몰겟어요",
          "original_form": "싸이월드 어차피 할 사람도 없을 텐디 왜 살리는지 몰겟어요",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029219.1.19",
          "form": "다들 인스타 할건데",
          "original_form": "다들 인스타 할건데",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029219.1.20",
          "form": "ㅋㅋㅋㅋㅋ그건그래요 좀 옛날 감성 되찾고싶은 그런건가...",
          "original_form": "ㅋㅋㅋㅋㅋ그건그래요 좀 옛날 감성 되찾고싶은 그런건가...",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029219.1.21",
          "form": "싸이에 뭔가 재밋는 글을 쓰셧나보군요 폭파시켜야한다니",
          "original_form": "싸이에 뭔가 재밋는 글을 쓰셧나보군요 폭파시켜야한다니",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029219.1.22",
          "form": "레트로 레트로 하니까 이제",
          "original_form": "레트로 레트로 하니까 이제",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029219.1.23",
          "form": "낄끼빠빠를 모르는 거지",
          "original_form": "낄끼빠빠를 모르는 거지",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029219.1.24",
          "form": "좀 중이스러운 글도 썻던 것 같아요 대부분 노래 가사 쓴듯 하지만",
          "original_form": "좀 중이스러운 글도 썻던 것 같아요 대부분 노래 가사 쓴듯 하지만",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029219.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ 그런거 궁금해하면 이제 name1님이 떼끼이놈하겟죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그런거 궁금해하면 이제 &name1&님이 떼끼이놈하겟죠",
          "speaker_id": "2",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029219.1.26",
          "form": "저도 제 네이버 블로그 옛 글은 보여줄수없는것처럼...",
          "original_form": "저도 제 네이버 블로그 옛 글은 보여줄수없는것처럼...",
          "speaker_id": "2",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029219.1.27",
          "form": "싸이는 그럿게 과거에 머물러야 좋은 녀석이엇던걸로",
          "original_form": "싸이는 그럿게 과거에 머물러야 좋은 녀석이엇던걸로",
          "speaker_id": "2",
          "time": "20210903 00:48"
        },
        {
          "id": "MDRW2100029219.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ웃기다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ웃기다",
          "speaker_id": "1",
          "time": "20210903 00:48"
