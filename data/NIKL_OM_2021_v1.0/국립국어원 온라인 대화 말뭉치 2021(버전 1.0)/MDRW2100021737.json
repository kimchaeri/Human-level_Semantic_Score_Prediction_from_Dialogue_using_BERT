      "utterance": [
        {
          "id": "MDRW2100021737.1.1",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "1",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021737.1.2",
          "form": "네 전 밥먹고 과일을 먹었어요",
          "original_form": "네 전 밥먹고 과일을 먹었어요",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021737.1.3",
          "form": "님은 드셨나요?",
          "original_form": "님은 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021737.1.4",
          "form": "네 저는 돼지갈비에 냉면 먹었어요",
          "original_form": "네 저는 돼지갈비에 냉면 먹었어요",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021737.1.5",
          "form": "와 맛있겠다..",
          "original_form": "와 맛있겠다..",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021737.1.6",
          "form": "비냉인가요 물냉인가요",
          "original_form": "비냉인가요 물냉인가요",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021737.1.7",
          "form": "물냉요",
          "original_form": "물냉요",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021737.1.8",
          "form": "오늘은 물냉이 땡기더라구용ㅋㅋ",
          "original_form": "오늘은 물냉이 땡기더라구용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021737.1.9",
          "form": "전 비냉을 좋아하는데 맵 찔이에요",
          "original_form": "전 비냉을 좋아하는데 맵 찔이에요",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021737.1.10",
          "form": "그래서 다대기를 따로 달라고해요..",
          "original_form": "그래서 다대기를 따로 달라고해요..",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021737.1.11",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021737.1.12",
          "form": "저는 비슷할지도 모르겠는데 비냉에 육수 부어먹는 걸 좋아해요",
          "original_form": "저는 비슷할지도 모르겠는데 비냉에 육수 부어먹는 걸 좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021737.1.13",
          "form": "다대기를 따로 덜어넣음 조절이 가능하니까요",
          "original_form": "다대기를 따로 덜어넣음 조절이 가능하니까요",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021737.1.14",
          "form": "아 그 방법도 잘 써요 ㅋㅋㅋ",
          "original_form": "아 그 방법도 잘 써요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021737.1.15",
          "form": "그렇게 먹음 덜매워요",
          "original_form": "그렇게 먹음 덜매워요",
          "speaker_id": "2",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021737.1.16",
          "form": "저는 매운 걸 못먹는 편은 아닌데 저렇게 먹는 게 더 맛있더라구요ㅋㅋ",
          "original_form": "저는 매운 걸 못먹는 편은 아닌데 저렇게 먹는 게 더 맛있더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021737.1.17",
          "form": "물론 덜맵기도 하구요",
          "original_form": "물론 덜맵기도 하구요",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021737.1.18",
          "form": "매콤 달콤 육수가 맛있죠!",
          "original_form": "매콤 달콤 육수가 맛있죠!",
          "speaker_id": "2",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021737.1.19",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021737.1.20",
          "form": "고기랑 같이 먹는 냉면 정말 젛아해요",
          "original_form": "고기랑 같이 먹는 냉면 정말 젛아해요",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021737.1.21",
          "form": "비싼 고기집가서 먹어도 좋고 육쌈냉면가도 좋고",
          "original_form": "비싼 고기집가서 먹어도 좋고 육쌈냉면가도 좋고",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021737.1.22",
          "form": "고기에 면을 감아서 먹으면 맛있죠",
          "original_form": "고기에 면을 감아서 먹으면 맛있죠",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021737.1.23",
          "form": "네네 그리고 따뜻한 고기랑 차가운 냉면차이도조아요",
          "original_form": "네네 그리고 따뜻한 고기랑 차가운 냉면차이도조아요",
          "speaker_id": "2",
          "time": "20210828 22:06"
        }
      ]
