      "utterance": [
        {
          "id": "MDRW2100002160.1.1",
          "form": "너희 집",
          "original_form": "너희 집",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002160.1.2",
          "form": "이사를 한다던데",
          "original_form": "이사를 한다던데",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002160.1.3",
          "form": "집 청소는 다 한 것이니?",
          "original_form": "집 청소는 다 한 것이니?",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002160.1.4",
          "form": "음 아직 전 세입자가 말이없네",
          "original_form": "음 아직 전 세입자가 말이없네",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.5",
          "form": "그렇게 오래 살던 집을 두고 다른 데로 간다니..",
          "original_form": "그렇게 오래 살던 집을 두고 다른 데로 간다니..",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.6",
          "form": "여간 큰 일이 아니구나.",
          "original_form": "여간 큰 일이 아니구나.",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.7",
          "form": "아무래도...정이 많이 들긴했어",
          "original_form": "아무래도...정이 많이 들긴했어",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.8",
          "form": "그렇지... 비록 세간살이 하나 마련한 건 없었겠지만",
          "original_form": "그렇지... 비록 세간살이 하나 마련한 건 없었겠지만",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.9",
          "form": "아무래도 살아온 정이 있으니까.",
          "original_form": "아무래도 살아온 정이 있으니까.",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.10",
          "form": "지금 살던 집 청소는 언제 할까...",
          "original_form": "지금 살던 집 청소는 언제 할까...",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.11",
          "form": "그래서 요즘 청소를 열심히 하고 있다고 들었는데 맞아?",
          "original_form": "그래서 요즘 청소를 열심히 하고 있다고 들었는데 맞아?",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.12",
          "form": "혹시 와서 대신해줄 생각은 없어?",
          "original_form": "혹시 와서 대신해줄 생각은 없어?",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.13",
          "form": "아직 청소를 시작하지 않았구나",
          "original_form": "아직 청소를 시작하지 않았구나",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.14",
          "form": "그건 잘못된 소문같아",
          "original_form": "그건 잘못된 소문같아",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.15",
          "form": "나는 내 집 가사일에도 바쁘단다.",
          "original_form": "나는 내 집 가사일에도 바쁘단다.",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002160.1.16",
          "form": "집에서 무슨 가사일하니",
          "original_form": "집에서 무슨 가사일하니",
          "speaker_id": "2",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002160.1.17",
          "form": "가사일은 정말 하지 않으면 티가 나고 하면 티가 안나는 거 같아.",
          "original_form": "가사일은 정말 하지 않으면 티가 나고 하면 티가 안나는 거 같아.",
          "speaker_id": "1",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002160.1.18",
          "form": "나는 나의 생활반경을 청소해.",
          "original_form": "나는 나의 생활반경을 청소해.",
          "speaker_id": "1",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002160.1.19",
          "form": "나의 반려동물 주위도 청소한단다.",
          "original_form": "나의 반려동물 주위도 청소한단다.",
          "speaker_id": "1",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002160.1.20",
          "form": "그는 정말 털이 많이 날려.",
          "original_form": "그는 정말 털이 많이 날려.",
          "speaker_id": "1",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002160.1.21",
          "form": "음 반려동물 주변 중요하지",
          "original_form": "음 반려동물 주변 중요하지",
          "speaker_id": "2",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002160.1.22",
          "form": "오늘도 열심히 청소기를 돌렸단다.",
          "original_form": "오늘도 열심히 청소기를 돌렸단다.",
          "speaker_id": "1",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002160.1.23",
          "form": "깃털이 청소기에 들어가?",
          "original_form": "깃털이 청소기에 들어가?",
          "speaker_id": "2",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002160.1.24",
          "form": "온집안을 돌리니 청소기도 힘든지, 전원이 그냥 꺼져버렸어.",
          "original_form": "온집안을 돌리니 청소기도 힘든지, 전원이 그냥 꺼져버렸어.",
          "speaker_id": "1",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002160.1.25",
          "form": "작은 깃털이란다.",
          "original_form": "작은 깃털이란다.",
          "speaker_id": "1",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002160.1.26",
          "form": "ㅠㅠ청소기 힘내",
          "original_form": "ㅠㅠ청소기 힘내",
          "speaker_id": "2",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002160.1.27",
          "form": "그냥 털이야. 나중에 보여줄게.",
          "original_form": "그냥 털이야. 나중에 보여줄게.",
          "speaker_id": "1",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002160.1.28",
          "form": "손톱만 해.",
          "original_form": "손톱만 해.",
          "speaker_id": "1",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002160.1.29",
          "form": "아 나는 습관적으로",
          "original_form": "아 나는 습관적으로",
          "speaker_id": "2",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002160.1.30",
          "form": "만년필 그 깃털이 아니야.",
          "original_form": "만년필 그 깃털이 아니야.",
          "speaker_id": "1",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002160.1.31",
          "form": "그런거는 그냥 슥슥 주우면 된단다.",
          "original_form": "그런거는 그냥 슥슥 주우면 된단다.",
          "speaker_id": "1",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002160.1.32",
          "form": "어 맞아 그거 상상했어",
          "original_form": "어 맞아 그거 상상했어",
          "speaker_id": "2",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002160.1.33",
          "form": "집안일은 정말 힘들구나.",
          "original_form": "집안일은 정말 힘들구나.",
          "speaker_id": "1",
          "time": "20210820 14:49"
        },
        {
          "id": "MDRW2100002160.1.34",
          "form": "나는 방금 청소를 했어.",
          "original_form": "나는 방금 청소를 했어.",
          "speaker_id": "1",
          "time": "20210820 14:49"
        }
      ]
    }
