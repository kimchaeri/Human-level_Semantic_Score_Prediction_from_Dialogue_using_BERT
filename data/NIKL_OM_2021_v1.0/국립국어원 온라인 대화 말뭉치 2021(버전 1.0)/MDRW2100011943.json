      "utterance": [
        {
          "id": "MDRW2100011943.1.1",
          "form": "나 그거 유튜브로 봤어",
          "original_form": "나 그거 유튜브로 봤어",
          "speaker_id": "2",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011943.1.2",
          "form": "나도 유튜브에서 봤어",
          "original_form": "나도 유튜브에서 봤어",
          "speaker_id": "1",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011943.1.3",
          "form": "같은 채널인거 같은데",
          "original_form": "같은 채널인거 같은데",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011943.1.4",
          "form": "수달 키우는 유튜브 그 사람 밖에 없을걸?",
          "original_form": "수달 키우는 유튜브 그 사람 밖에 없을걸?",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011943.1.5",
          "form": "그렇긴 해",
          "original_form": "그렇긴 해",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011943.1.6",
          "form": "그거 키우려고 동물원 신고까지 했대",
          "original_form": "그거 키우려고 동물원 신고까지 했대",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011943.1.7",
          "form": "펭귄도 있던데?",
          "original_form": "펭귄도 있던데?",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011943.1.8",
          "form": "아 그 유튜버가?",
          "original_form": "아 그 유튜버가?",
          "speaker_id": "1",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011943.1.9",
          "form": "수달도 귀여운거 같음",
          "original_form": "수달도 귀여운거 같음",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011943.1.10",
          "form": "너무 귀여워 근데 키우려면 동물원 차려야 되잖아",
          "original_form": "너무 귀여워 근데 키우려면 동물원 차려야 되잖아",
          "speaker_id": "1",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011943.1.11",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011943.1.12",
          "form": "돈도 많이들고",
          "original_form": "돈도 많이들고",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011943.1.13",
          "form": "맞아 수조 엄청 큰거 타려줬던데",
          "original_form": "맞아 수조 엄청 큰거 타려줬던데",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011943.1.14",
          "form": "차려줬던데",
          "original_form": "차려줬던데",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011943.1.15",
          "form": "말 잘 안들을 듯 ㅋㅋ",
          "original_form": "말 잘 안들을 듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011943.1.16",
          "form": "거북이를 애완용으로 키우기도 하고 그러더라",
          "original_form": "거북이를 애완용으로 키우기도 하고 그러더라",
          "speaker_id": "1",
          "time": "20210825 18:12"
        }
      ]
