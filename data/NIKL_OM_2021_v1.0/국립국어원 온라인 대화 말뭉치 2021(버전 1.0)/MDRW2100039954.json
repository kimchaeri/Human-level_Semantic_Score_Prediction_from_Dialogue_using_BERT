      "utterance": [
        {
          "id": "MDRW2100039954.1.1",
          "form": "징하다 진짜 ㅠ",
          "original_form": "징하다 진짜 ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039954.1.2",
          "form": "지금은 자나??",
          "original_form": "지금은 자나??",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039954.1.3",
          "form": "난 신랑이랑 팝콘브레인 얘기했음 ㅠ",
          "original_form": "난 신랑이랑 팝콘브레인 얘기했음 ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039954.1.4",
          "form": "응 11시넘어서 잤어ㅋㅋ",
          "original_form": "응 11시넘어서 잤어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039954.1.5",
          "form": "아구 그랬구나ㅜ",
          "original_form": "아구 그랬구나ㅜ",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100039954.1.6",
          "form": "나두 어젠가했음ㅋㅋ",
          "original_form": "나두 어젠가했음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100039954.1.7",
          "form": "이제 잘라한다",
          "original_form": "이제 잘라한다",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100039954.1.8",
          "form": "주말이라 늦었군 ㅎㅎ",
          "original_form": "주말이라 늦었군 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100039954.1.9",
          "form": "늦게잤네ㅋㅋ",
          "original_form": "늦게잤네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 08:54"
        },
        {
          "id": "MDRW2100039954.1.10",
          "form": "울신랑은 벌초하러갔음. 오늘 아이들과 하루 보내야된다ㅋㅋ",
          "original_form": "울신랑은 벌초하러갔음. 오늘 아이들과 하루 보내야된다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 08:54"
        },
        {
          "id": "MDRW2100039954.1.11",
          "form": "이번 주 벌초하러 많이 가 드만",
          "original_form": "이번 주 벌초하러 많이 가 드만",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100039954.1.12",
          "form": "우리 신랑은 내일 편의점에 일하러 간다",
          "original_form": "우리 신랑은 내일 편의점에 일하러 간다",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100039954.1.13",
          "form": "오늘은 집에 있고",
          "original_form": "오늘은 집에 있고",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100039954.1.14",
          "form": "아고 글쿠마잉",
          "original_form": "아고 글쿠마잉",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100039954.1.15",
          "form": "다들바쁘다이",
          "original_form": "다들바쁘다이",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100039954.1.16",
          "form": "name3일찍 일어났나??",
          "original_form": "&name3&일찍 일어났나??",
          "speaker_id": "2",
          "time": "20210904 10:52"
        },
        {
          "id": "MDRW2100039954.1.17",
          "form": "난 오늘 늦잠자서 좀전에 일어나고",
          "original_form": "난 오늘 늦잠자서 좀전에 일어나고",
          "speaker_id": "2",
          "time": "20210904 10:53"
        },
        {
          "id": "MDRW2100039954.1.18",
          "form": "name4이는 9시에 일어나고 ㅎㅎ",
          "original_form": "&name4&이는 9시에 일어나고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 10:53"
        },
        {
          "id": "MDRW2100039954.1.19",
          "form": "name5은 몇시에 일어났나몰라",
          "original_form": "&name5&은 몇시에 일어났나몰라",
          "speaker_id": "2",
          "time": "20210904 10:53"
        },
        {
          "id": "MDRW2100039954.1.20",
          "form": "응 8시30분에일어났지",
          "original_form": "응 8시30분에일어났지",
          "speaker_id": "1",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100039954.1.21",
          "form": "첵스초코 우유에 말아먹고 이제 볶음밥하려고 밥했다ㅋ",
          "original_form": "첵스초코 우유에 말아먹고 이제 볶음밥하려고 밥했다ㅋ",
          "speaker_id": "1",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100039954.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ 잘한다 부지런하다이",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 잘한다 부지런하다이",
          "speaker_id": "2",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100039954.1.23",
          "form": "나도 이제 미역국에 밥 말아먹고 있음",
          "original_form": "나도 이제 미역국에 밥 말아먹고 있음",
          "speaker_id": "2",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100039954.1.24",
          "form": "오늘 친구오기로 했나??",
          "original_form": "오늘 친구오기로 했나??",
          "speaker_id": "2",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100039954.1.25",
          "form": "아니 안온다ㅋㅋ 연락없네ㅋㅋ",
          "original_form": "아니 안온다ㅋㅋ 연락없네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100039954.1.26",
          "form": "name6이는 자전거타러나가고싶다카고 name3는 협조안하고ㅡㅡㅋㅋ",
          "original_form": "&name6&이는 자전거타러나가고싶다카고 &name3&는 협조안하고ㅡㅡㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100039954.1.27",
          "form": "벌써  피곤하드아",
          "original_form": "벌써  피곤하드아",
          "speaker_id": "1",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100039954.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100039954.1.29",
          "form": "언니네는 뭐해?",
          "original_form": "언니네는 뭐해?",
          "speaker_id": "1",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100039954.1.30",
          "form": "우린 삼각김밥싸서 ㅋㅋ 밖에 자전거타러나간다ㅋㅋ",
          "original_form": "우린 삼각김밥싸서 ㅋㅋ 밖에 자전거타러나간다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100039954.1.31",
          "form": "오 삼각김밥도 싸고",
          "original_form": "오 삼각김밥도 싸고",
          "speaker_id": "2",
          "time": "20210904 11:48"
        },
        {
          "id": "MDRW2100039954.1.32",
          "form": "못하는 게 없구만",
          "original_form": "못하는 게 없구만",
          "speaker_id": "2",
          "time": "20210904 11:48"
        },
        {
          "id": "MDRW2100039954.1.33",
          "form": "name6이 혼자서 자전거 잘 타든데",
          "original_form": "&name6&이 혼자서 자전거 잘 타든데",
          "speaker_id": "2",
          "time": "20210904 11:48"
        },
        {
          "id": "MDRW2100039954.1.34",
          "form": "name6ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "&name6&ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:48"
        },
        {
          "id": "MDRW2100039954.1.35",
          "form": "난 2시에 영어학원간담회 있어서 갔다가",
          "original_form": "난 2시에 영어학원간담회 있어서 갔다가",
          "speaker_id": "2",
          "time": "20210904 11:48"
        },
        {
          "id": "MDRW2100039954.1.36",
          "form": "뭐 할까 생각중",
          "original_form": "뭐 할까 생각중",
          "speaker_id": "2",
          "time": "20210904 11:48"
        },
        {
          "id": "MDRW2100039954.1.37",
          "form": "오늘이구만ㅋ",
          "original_form": "오늘이구만ㅋ",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100039954.1.38",
          "form": "잘다녀온나우ㅋ",
          "original_form": "잘다녀온나우ㅋ",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100039954.1.39",
          "form": "어 뻘쭘하게 name7언니를 만나고 오겠지 ㅋㅋㄱㅋ",
          "original_form": "어 뻘쭘하게 &name7&언니를 만나고 오겠지 ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100039954.1.40",
          "form": "그래도 name8언니랑 같이가서 그나마 다행ㅋㅋ",
          "original_form": "그래도 &name8&언니랑 같이가서 그나마 다행ㅋㅋ",
