      "utterance": [
        {
          "id": "MDRW2100028617.1.1",
          "form": "오늘도 비가왔나요?",
          "original_form": "오늘도 비가왔나요?",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028617.1.2",
          "form": "여긴 어제까지오고 오늘은 안왔어요",
          "original_form": "여긴 어제까지오고 오늘은 안왔어요",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028617.1.3",
          "form": "네 잠깐 소나기가 왔어요",
          "original_form": "네 잠깐 소나기가 왔어요",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028617.1.4",
          "form": "이제 고만왔음 좋겠네요.",
          "original_form": "이제 고만왔음 좋겠네요.",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028617.1.5",
          "form": "저번주부터 계속와서 넘 습해요",
          "original_form": "저번주부터 계속와서 넘 습해요",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028617.1.6",
          "form": "그러게요 가을장마 끝나면 좋겠어요",
          "original_form": "그러게요 가을장마 끝나면 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028617.1.7",
          "form": "담주에 비하루 뜨긴했는데...",
          "original_form": "담주에 비하루 뜨긴했는데...",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028617.1.8",
          "form": "담주 비오면 진짜 가을일꺼같아요",
          "original_form": "담주 비오면 진짜 가을일꺼같아요",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028617.1.9",
          "form": "지금도 아침 저녁으로는 살짝 서늘하더라구요",
          "original_form": "지금도 아침 저녁으로는 살짝 서늘하더라구요",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028617.1.10",
          "form": "절기가 백로인가 그럴 거예요",
          "original_form": "절기가 백로인가 그럴 거예요",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028617.1.11",
          "form": "맞아요 새벽엔 춥더라구요",
          "original_form": "맞아요 새벽엔 춥더라구요",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028617.1.12",
          "form": "점점 가을이 짧아져서 아쉬워요",
          "original_form": "점점 가을이 짧아져서 아쉬워요",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028617.1.13",
          "form": "봄도요 ㅜ 여름 겨울이 점점 길어져요",
          "original_form": "봄도요 ㅜ 여름 겨울이 점점 길어져요",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028617.1.14",
          "form": "글게요.  넘 덥고 춥고~~~",
          "original_form": "글게요.  넘 덥고 춥고~~~",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028617.1.15",
          "form": "이상기후가 18년도 이후로 점점 심해지네요ㅠㅠ",
          "original_form": "이상기후가 18년도 이후로 점점 심해지네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028617.1.16",
          "form": "그러니깐요.  중간이 없는거같아요",
          "original_form": "그러니깐요.  중간이 없는거같아요",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028617.1.17",
          "form": "예전 날씨가 그립네요 미세먼지도 없었던...",
          "original_form": "예전 날씨가 그립네요 미세먼지도 없었던...",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028617.1.18",
          "form": "다시 볼수없을꺼같아서 더 그립네요",
          "original_form": "다시 볼수없을꺼같아서 더 그립네요",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028617.1.19",
          "form": "네 다시는 오지 않을 날씨죠 ㅠㅠ",
          "original_form": "네 다시는 오지 않을 날씨죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028617.1.20",
          "form": "오늘 하늘보셨나요?",
          "original_form": "오늘 하늘보셨나요?",
          "speaker_id": "1",
          "time": "20210903 14:45"
        },
        {
          "id": "MDRW2100028617.1.21",
          "form": "넘 깨끗하고 구름이 이쁘더라구요",
