      "utterance": [
        {
          "id": "MDRW2100036068.1.1",
          "form": "아니오 ㅋㅋㅋㅋ",
          "original_form": "아니오 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:17"
        },
        {
          "id": "MDRW2100036068.1.2",
          "form": "집안일 안하는 불효녀",
          "original_form": "집안일 안하는 불효녀",
          "speaker_id": "2",
          "time": "20210830 16:17"
        },
        {
          "id": "MDRW2100036068.1.3",
          "form": "ㅋㅋㅋㅋㅋ저도안해서뭐..허허",
          "original_form": "ㅋㅋㅋㅋㅋ저도안해서뭐..허허",
          "speaker_id": "1",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036068.1.4",
          "form": "ㅋㅋㅋㅋㅋ이제 퇴근했어용",
          "original_form": "ㅋㅋㅋㅋㅋ이제 퇴근했어용",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100036068.1.5",
          "form": "너무 힘들어용",
          "original_form": "너무 힘들어용",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100036068.1.6",
          "form": "요즘 설거지는 대부분 동생이 해요",
          "original_form": "요즘 설거지는 대부분 동생이 해요",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100036068.1.7",
          "form": "와 동생착해요",
          "original_form": "와 동생착해요",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036068.1.8",
          "form": "철도들고",
          "original_form": "철도들고",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036068.1.9",
          "form": "그 모죠 그 택견이엇나요 그거하라할땐웃겻는데",
          "original_form": "그 모죠 그 택견이엇나요 그거하라할땐웃겻는데",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036068.1.10",
          "form": "설거지하구착하네요",
          "original_form": "설거지하구착하네요",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036068.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036068.1.12",
          "form": "백수라서 마음이 좀 불편하겟죵",
          "original_form": "백수라서 마음이 좀 불편하겟죵",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036068.1.13",
          "form": "집안일잘하면착한친구예요",
          "original_form": "집안일잘하면착한친구예요",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036068.1.14",
          "form": "그런마음있어도 안하는 놈들이 더많아요",
          "original_form": "그런마음있어도 안하는 놈들이 더많아요",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.15",
          "form": "마자용 엄마는 맨날 동생이 집안일 하면 칭찬해주니깐",
          "original_form": "마자용 엄마는 맨날 동생이 집안일 하면 칭찬해주니깐",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.16",
          "form": "좋은동생이예요",
          "original_form": "좋은동생이예요",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.17",
          "form": "저는 비뚤어져서 안하고",
          "original_form": "저는 비뚤어져서 안하고",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.18",
          "form": "맨날 칭찬받는 동생만 집안일하는 구조에요",
          "original_form": "맨날 칭찬받는 동생만 집안일하는 구조에요",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.19",
          "form": "엄마는 내가하면칭찬안하고 동생이하면칭찬해줘요",
          "original_form": "엄마는 내가하면칭찬안하고 동생이하면칭찬해줘요",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.20",
          "form": "ㅋㅋㅋㅋㅋ엇나간 장녀",
          "original_form": "ㅋㅋㅋㅋㅋ엇나간 장녀",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.21",
          "form": "삐뚤어져요",
          "original_form": "삐뚤어져요",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.22",
          "form": "마자요 내가 하면 칭찬안해서",
          "original_form": "마자요 내가 하면 칭찬안해서",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.24",
          "form": "엇나간장녀ㅋㅋㅋㄱㅋ",
          "original_form": "엇나간장녀ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.25",
          "form": "그래서 안하는게 이득",
          "original_form": "그래서 안하는게 이득",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036068.1.26",
          "form": "근데ㅈ돈잘쓰는장녀자나요 효녀예요",
          "original_form": "근데ㅈ돈잘쓰는장녀자나요 효녀예요",
