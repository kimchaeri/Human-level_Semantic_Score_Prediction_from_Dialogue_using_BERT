      "utterance": [
        {
          "id": "MDRW2100011401.1.1",
          "form": "점심 식사 드셨나요?",
          "original_form": "점심 식사 드셨나요?",
          "speaker_id": "1",
          "time": "20210824 12:04"
        },
        {
          "id": "MDRW2100011401.1.2",
          "form": "아뇨 아직이요ㅎㅎ방금 햄버거 시켰어용",
          "original_form": "아뇨 아직이요ㅎㅎ방금 햄버거 시켰어용",
          "speaker_id": "2",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011401.1.3",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011401.1.4",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "1",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011401.1.5",
          "form": "어디브랜드꺼 시키셨나요?",
          "original_form": "어디브랜드꺼 시키셨나요?",
          "speaker_id": "1",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011401.1.6",
          "form": "롯데리아에서 시켰어용ㅎㅎ",
          "original_form": "롯데리아에서 시켰어용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011401.1.7",
          "form": "점심 뮤 드셨어요?",
          "original_form": "점심 뮤 드셨어요?",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011401.1.8",
          "form": "뭐 드셨어용?",
          "original_form": "뭐 드셨어용?",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011401.1.9",
          "form": "저는 샐러드 먹었어요",
          "original_form": "저는 샐러드 먹었어요",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011401.1.10",
          "form": "무슨 햄버거 드셨나요?",
          "original_form": "무슨 햄버거 드셨나요?",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011401.1.11",
          "form": "모짜렐라 버거 먹었어요ㅎㅎㅎ",
          "original_form": "모짜렐라 버거 먹었어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 16:44"
        },
        {
          "id": "MDRW2100011401.1.12",
          "form": "샐러드면 혹시 다이어트 하세요?",
          "original_form": "샐러드면 혹시 다이어트 하세요?",
          "speaker_id": "2",
          "time": "20210824 16:44"
        },
        {
          "id": "MDRW2100011401.1.13",
          "form": "아니요 그냥 집에 있길래 먹었어요",
          "original_form": "아니요 그냥 집에 있길래 먹었어요",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011401.1.14",
          "form": "무슨 샐러드였어요?",
          "original_form": "무슨 샐러드였어요?",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100011401.1.15",
          "form": "감자샐러드요",
          "original_form": "감자샐러드요",
          "speaker_id": "1",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100011401.1.16",
          "form": "저녁 뭐 드실 예정이신가요?",
          "original_form": "저녁 뭐 드실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100011401.1.17",
          "form": "저녁은 배가 많이 안고파서 복숭아만 먹으려구요",
          "original_form": "저녁은 배가 많이 안고파서 복숭아만 먹으려구요",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100011401.1.18",
          "form": "뭐 드실거에요?",
          "original_form": "뭐 드실거에요?",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100011401.1.19",
          "form": "저는 피자요",
          "original_form": "저는 피자요",
          "speaker_id": "1",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100011401.1.20",
          "form": "복숭아 요즘도 맛있나요?",
          "original_form": "복숭아 요즘도 맛있나요?",
          "speaker_id": "1",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100011401.1.21",
          "form": "넵!! 복숭아때문에 여름이 끝나가는게 아쉬워요..",
          "original_form": "넵!! 복숭아때문에 여름이 끝나가는게 아쉬워요..",
          "speaker_id": "2",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011401.1.22",
          "form": "저는 수박보다 복숭아가 더 좋아요ㅎㅎ",
          "original_form": "저는 수박보다 복숭아가 더 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011401.1.23",
          "form": "말복??딱복??",
          "original_form": "말복??딱복??",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011401.1.24",
          "form": "저는 딱복파 입니당!!",
          "original_form": "저는 딱복파 입니당!!",
          "speaker_id": "2",
          "time": "20210824 17:01"
        }
      ]
