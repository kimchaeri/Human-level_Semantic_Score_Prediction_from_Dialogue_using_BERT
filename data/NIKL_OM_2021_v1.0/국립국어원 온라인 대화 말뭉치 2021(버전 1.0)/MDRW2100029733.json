      "utterance": [
        {
          "id": "MDRW2100029733.1.1",
          "form": "이제 또 주말이 다가오네요",
          "original_form": "이제 또 주말이 다가오네요",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100029733.1.2",
          "form": "이번주에는 데이트 하세영?",
          "original_form": "이번주에는 데이트 하세영?",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100029733.1.3",
          "form": "오늘부터 일배우러가서 ㅠㅠ",
          "original_form": "오늘부터 일배우러가서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100029733.1.4",
          "form": "한동안 못볼듯..ㅎ.ㅎ..",
          "original_form": "한동안 못볼듯..ㅎ.ㅎ..",
          "speaker_id": "2",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029733.1.5",
          "form": "남자친구 전역하고\r\n이제 일 하나염?",
          "original_form": "남자친구 전역하고\r\n이제 일 하나염?",
          "speaker_id": "1",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029733.1.6",
          "form": "시간나면 그때봐야딩",
          "original_form": "시간나면 그때봐야딩",
          "speaker_id": "2",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029733.1.7",
          "form": "보고싶어서 우시는거 아니죠? ㅜㅜ",
          "original_form": "보고싶어서 우시는거 아니죠? ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029733.1.8",
          "form": "음...눈물은 안나오는데",
          "original_form": "음...눈물은 안나오는데",
          "speaker_id": "2",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029733.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029733.1.10",
          "form": "헤헷",
          "original_form": "헤헷",
          "speaker_id": "2",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029733.1.11",
          "form": "지난주에는 라색해서 못보고",
          "original_form": "지난주에는 라색해서 못보고",
          "speaker_id": "1",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029733.1.12",
          "form": "돈벌어야된다거",
          "original_form": "돈벌어야된다거",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029733.1.13",
          "form": "이번주는 일 때문에 못보고",
          "original_form": "이번주는 일 때문에 못보고",
          "speaker_id": "1",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029733.1.14",
          "form": "근데저도",
          "original_form": "근데저도",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029733.1.15",
          "form": "두 분 너무 오래 못보는거 아닌가영?",
          "original_form": "두 분 너무 오래 못보는거 아닌가영?",
          "speaker_id": "1",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029733.1.16",
          "form": "내걍이 없으면",
          "original_form": "내걍이 없으면",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029733.1.17",
          "form": "나가질못해요...ㅜ",
          "original_form": "나가질못해요...ㅜ",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029733.1.18",
          "form": "내굥이가 기숙사ㅏ 들가서 몇달동안",
          "original_form": "내굥이가 기숙사ㅏ 들가서 몇달동안",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029733.1.19",
          "form": "name3랑 name4 땜에\r\n데이트도 못하나영?",
          "original_form": "&name3&랑 &name4& 땜에\r\n데이트도 못하나영?",
          "speaker_id": "1",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029733.1.20",
          "form": "네..ㅎ",
          "original_form": "네..ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029733.1.21",
          "form": "어차피 바쁠시기라...",
          "original_form": "어차피 바쁠시기라...",
          "speaker_id": "2",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100029733.1.22",
          "form": "아니 그래도 이제 전역했는데\r\n그 동안 못 한 데이트 다 해야쥬ㅠ",
          "original_form": "아니 그래도 이제 전역했는데\r\n그 동안 못 한 데이트 다 해야쥬ㅠ",
          "speaker_id": "1",
