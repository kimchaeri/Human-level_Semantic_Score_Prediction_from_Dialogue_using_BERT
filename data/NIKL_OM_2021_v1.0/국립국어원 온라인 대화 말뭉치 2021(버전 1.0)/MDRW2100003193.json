      "utterance": [
        {
          "id": "MDRW2100003193.1.1",
          "form": "요새 좋아하는 티비프로그램 있으세용?",
          "original_form": "요새 좋아하는 티비프로그램 있으세용?",
          "speaker_id": "1",
          "time": "20210820 20:21"
        },
        {
          "id": "MDRW2100003193.1.2",
          "form": "나혼자산다 봐야돼여",
          "original_form": "나혼자산다 봐야돼여",
          "speaker_id": "2",
          "time": "20210820 20:22"
        },
        {
          "id": "MDRW2100003193.1.3",
          "form": "오 요새 재밌어요?",
          "original_form": "오 요새 재밌어요?",
          "speaker_id": "1",
          "time": "20210820 20:27"
        },
        {
          "id": "MDRW2100003193.1.4",
          "form": "열심히 보다가 요새 잘 안봐요ㅠ",
          "original_form": "열심히 보다가 요새 잘 안봐요ㅠ",
          "speaker_id": "1",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003193.1.5",
          "form": "기안나와요 무조건 보세요ㅋㅋㅋ",
          "original_form": "기안나와요 무조건 보세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003193.1.6",
          "form": "기안 나오는거 재밌어요?",
          "original_form": "기안 나오는거 재밌어요?",
          "speaker_id": "1",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003193.1.7",
          "form": "전 제대로 본 적 없는거같아요",
          "original_form": "전 제대로 본 적 없는거같아요",
          "speaker_id": "1",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003193.1.8",
          "form": "네 기안은 치트키에요ㅋㅋㄱ 정말ㅋㅋㅋ",
          "original_form": "네 기안은 치트키에요ㅋㅋㄱ 정말ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003193.1.9",
          "form": "커피포트에 족발넣고 라면 끓여먹던거 안보셨어요?",
          "original_form": "커피포트에 족발넣고 라면 끓여먹던거 안보셨어요?",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003193.1.10",
          "form": "그거 캡쳐로만 본거같아요ㅋㅋㅋ",
          "original_form": "그거 캡쳐로만 본거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:30"
        },
        {
          "id": "MDRW2100003193.1.11",
          "form": "아 그 옷 커버로 가방 만드는건 봤어요",
          "original_form": "아 그 옷 커버로 가방 만드는건 봤어요",
          "speaker_id": "1",
          "time": "20210820 20:30"
        },
        {
          "id": "MDRW2100003193.1.12",
          "form": "심지어 커피포트안에 남은거 데워먹겠다고 전자레인지에 돌렸었대여ㅋㅋㅋㄱ",
          "original_form": "심지어 커피포트안에 남은거 데워먹겠다고 전자레인지에 돌렸었대여ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210820 20:30"
        },
        {
          "id": "MDRW2100003193.1.13",
          "form": "커피포트를요..?",
          "original_form": "커피포트를요..?",
          "speaker_id": "1",
          "time": "20210820 20:31"
        },
        {
          "id": "MDRW2100003193.1.14",
          "form": "세상엨ㅋㅋㅋ",
          "original_form": "세상엨ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:31"
        },
        {
          "id": "MDRW2100003193.1.15",
          "form": "넼ㅋㅋㅋ 불꽃이 피더라고 하지말라고 하더라구옄ㅋㄱ",
          "original_form": "넼ㅋㅋㅋ 불꽃이 피더라고 하지말라고 하더라구옄ㅋㄱ",
          "speaker_id": "2",
          "time": "20210820 20:31"
        },
        {
          "id": "MDRW2100003193.1.16",
          "form": "기안 아니면 누가 그런짓을 하겠어요ㅋㅋㅋㄱ",
          "original_form": "기안 아니면 누가 그런짓을 하겠어요ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210820 20:31"
        },
        {
          "id": "MDRW2100003193.1.17",
          "form": "불꽃놀이도 아니곸ㅋㅋㅋ",
          "original_form": "불꽃놀이도 아니곸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003193.1.18",
          "form": "안 다친게 다행이네요",
          "original_form": "안 다친게 다행이네요",
          "speaker_id": "1",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003193.1.19",
          "form": "ㅋㅋㅋㄱ 그 가방 계속 매고 다니더라구요ㅋㅋㅋ 그것도 너무 웃기잖아요ㅋㅋ",
          "original_form": "ㅋㅋㅋㄱ 그 가방 계속 매고 다니더라구요ㅋㅋㅋ 그것도 너무 웃기잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003193.1.20",
          "form": "그러니까요ㅋㅋㅋ",
          "original_form": "그러니까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003193.1.21",
          "form": "그 옷은 어딨는지ㅠㅠ",
          "original_form": "그 옷은 어딨는지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 20:33"
