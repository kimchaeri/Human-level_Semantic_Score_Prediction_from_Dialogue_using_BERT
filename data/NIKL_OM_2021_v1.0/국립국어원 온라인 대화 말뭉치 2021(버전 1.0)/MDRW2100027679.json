      "utterance": [
        {
          "id": "MDRW2100027679.1.1",
          "form": "얇은피 왕교자 6봉",
          "original_form": "얇은피 왕교자 6봉",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027679.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ꽤많이샀네",
          "original_form": "ㅋㅋㅋㅋㅋㅋ꽤많이샀네",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027679.1.3",
          "form": "무슴맛?",
          "original_form": "무슴맛?",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027679.1.4",
          "form": "맛? 그런곤 없었던 것 같은데",
          "original_form": "맛? 그런곤 없었던 것 같은데",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027679.1.5",
          "form": "그냥 일반 만두속일듯",
          "original_form": "그냥 일반 만두속일듯",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027679.1.6",
          "form": "고기 김치 이런거 ㅋㅋㅋㅋ",
          "original_form": "고기 김치 이런거 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027679.1.7",
          "form": "아 ㅇㅇ고기지",
          "original_form": "아 ㅇㅇ고기지",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027679.1.8",
          "form": "고기 못잃어",
          "original_form": "고기 못잃어",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027679.1.9",
          "form": "넌 뭐삿어",
          "original_form": "넌 뭐삿어",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027679.1.10",
          "form": "ㅋㅋㅋㅋㅋ나는 핫도그11개",
          "original_form": "ㅋㅋㅋㅋㅋ나는 핫도그11개",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027679.1.11",
          "form": "모짜체다?",
          "original_form": "모짜체다?",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027679.1.12",
          "form": "노노 일반맛 반은소세지고 반은 치즈도라",
          "original_form": "노노 일반맛 반은소세지고 반은 치즈도라",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027679.1.13",
          "form": "아하 그거 에어프라이어에 돌리는거?",
          "original_form": "아하 그거 에어프라이어에 돌리는거?",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027679.1.14",
          "form": "그게 더 맛잇대 바삭하게머그려면",
          "original_form": "그게 더 맛잇대 바삭하게머그려면",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027679.1.15",
          "form": "나는 먹어봤던건데 레인지에도 ㄱㅊㄱㅊ",
          "original_form": "나는 먹어봤던건데 레인지에도 ㄱㅊㄱㅊ",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027679.1.16",
          "form": "ㅋㅋㅋ역시 에프하나 사야한다니까",
          "original_form": "ㅋㅋㅋ역시 에프하나 사야한다니까",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027679.1.17",
          "form": "그러니까 요즘 전부 에프용많아서",
          "original_form": "그러니까 요즘 전부 에프용많아서",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027679.1.18",
          "form": "맞음 선택지 줄면 아쉽잖아...",
          "original_form": "맞음 선택지 줄면 아쉽잖아...",
          "speaker_id": "2",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027679.1.19",
          "form": "마자..근데 계속 해먹다가 살찔...",
          "original_form": "마자..근데 계속 해먹다가 살찔...",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027679.1.20",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027679.1.21",
          "form": "오랜만에 네일을 사봐따",
