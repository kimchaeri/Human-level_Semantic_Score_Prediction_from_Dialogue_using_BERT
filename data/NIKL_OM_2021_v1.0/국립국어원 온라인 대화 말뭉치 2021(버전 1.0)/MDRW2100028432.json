      "utterance": [
        {
          "id": "MDRW2100028432.1.1",
          "form": "호치민만 그래",
          "original_form": "호치민만 그래",
          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028432.1.2",
          "form": "남부지방은 여름만 있어",
          "original_form": "남부지방은 여름만 있어",
          "speaker_id": "2",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028432.1.3",
          "form": "지금은 우기야",
          "original_form": "지금은 우기야",
          "speaker_id": "1",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028432.1.4",
          "form": "베트남 날씨는 건기와 우기로 나뉘어",
          "original_form": "베트남 날씨는 건기와 우기로 나뉘어",
          "speaker_id": "2",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028432.1.5",
          "form": "우기는 4월부터 11월까지야",
          "original_form": "우기는 4월부터 11월까지야",
          "speaker_id": "2",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028432.1.6",
          "form": "지금이 우기야",
          "original_form": "지금이 우기야",
          "speaker_id": "1",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028432.1.7",
          "form": "날마다 비가 퍼붓듯이 오더라",
          "original_form": "날마다 비가 퍼붓듯이 오더라",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028432.1.8",
          "form": "퇴근시간만 되면 비가와",
          "original_form": "퇴근시간만 되면 비가와",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028432.1.9",
          "form": "발목까지 금새 물이차서  홍수가 돼",
          "original_form": "발목까지 금새 물이차서  홍수가 돼",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028432.1.10",
          "form": "중부지방엔 태풍도 와",
          "original_form": "중부지방엔 태풍도 와",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028432.1.11",
          "form": "남부지방엔 태풍이 오지 않아",
          "original_form": "남부지방엔 태풍이 오지 않아",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028432.1.12",
          "form": "여름만 있어도 좋아",
          "original_form": "여름만 있어도 좋아",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028432.1.13",
          "form": "이불이 사계절 똑같아",
          "original_form": "이불이 사계절 똑같아",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028432.1.14",
          "form": "옷도 항상 여름옷이지",
          "original_form": "옷도 항상 여름옷이지",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028432.1.15",
          "form": "건조하지않아서 피부에 좋아",
          "original_form": "건조하지않아서 피부에 좋아",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028432.1.16",
          "form": "여름만 있어서 햇볕때문에 문제도 많아",
          "original_form": "여름만 있어서 햇볕때문에 문제도 많아",
