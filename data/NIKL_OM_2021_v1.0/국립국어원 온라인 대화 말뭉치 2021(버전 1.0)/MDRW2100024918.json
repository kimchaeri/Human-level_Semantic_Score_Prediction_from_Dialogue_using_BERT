      "utterance": [
        {
          "id": "MDRW2100024918.1.1",
          "form": "근데 그래도 에펙이 기본이더라구요",
          "original_form": "근데 그래도 에펙이 기본이더라구요",
          "speaker_id": "2",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100024918.1.2",
          "form": "그쵸 에펙이 기본이예요",
          "original_form": "그쵸 에펙이 기본이예요",
          "speaker_id": "1",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100024918.1.3",
          "form": "저는 처음에 씨포디못해 가지고ㅜㅜ",
          "original_form": "저는 처음에 씨포디못해 가지고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100024918.1.4",
          "form": "영상과를 나왔는데",
          "original_form": "영상과를 나왔는데",
          "speaker_id": "1",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100024918.1.5",
          "form": "마야배웠거든요",
          "original_form": "마야배웠거든요",
          "speaker_id": "1",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100024918.1.6",
          "form": "흐네에엑",
          "original_form": "흐네에엑",
          "speaker_id": "2",
          "time": "20210831 20:49"
        },
        {
          "id": "MDRW2100024918.1.7",
          "form": "마야!",
          "original_form": "마야!",
          "speaker_id": "2",
          "time": "20210831 20:49"
        },
        {
          "id": "MDRW2100024918.1.8",
          "form": "그거 간지프로그램인데",
          "original_form": "그거 간지프로그램인데",
          "speaker_id": "2",
          "time": "20210831 20:50"
        },
        {
          "id": "MDRW2100024918.1.9",
          "form": "왜 마야를 가르쳤는지 모르겠어요ㅜ",
          "original_form": "왜 마야를 가르쳤는지 모르겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210831 20:50"
        },
        {
          "id": "MDRW2100024918.1.10",
          "form": "지금도 쓰세여?",
          "original_form": "지금도 쓰세여?",
          "speaker_id": "2",
          "time": "20210831 20:50"
        },
        {
          "id": "MDRW2100024918.1.11",
          "form": "아 그럼 안쓰시겠구남..",
          "original_form": "아 그럼 안쓰시겠구남..",
          "speaker_id": "2",
          "time": "20210831 20:50"
        },
        {
          "id": "MDRW2100024918.1.12",
          "form": "아뇨 지금 다까먹었어요ㅠ",
          "original_form": "아뇨 지금 다까먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 20:50"
        },
        {
          "id": "MDRW2100024918.1.13",
          "form": "회사에서 다 씨포디만 쓰더라구요",
          "original_form": "회사에서 다 씨포디만 쓰더라구요",
          "speaker_id": "1",
          "time": "20210831 20:50"
        },
        {
          "id": "MDRW2100024918.1.14",
          "form": "마야는 영화쪽이나 가야쓰일거 같아요 ᆢ",
          "original_form": "마야는 영화쪽이나 가야쓰일거 같아요 ᆢ",
          "speaker_id": "1",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100024918.1.15",
          "form": "그래도 뭔가 마야하면 3d끝판왕 느낌인데 ㅋㅋㅋ",
          "original_form": "그래도 뭔가 마야하면 3d끝판왕 느낌인데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100024918.1.16",
          "form": "ㅋㅋ 그래도 씨포디랑 보면 장단점이 있더라구요",
          "original_form": "ㅋㅋ 그래도 씨포디랑 보면 장단점이 있더라구요",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100024918.1.17",
          "form": "때깔은 씨포디가 낳고 어렵고 디테일하게 만드는건 마야가 낳고요 ㅋㅋ",
          "original_form": "때깔은 씨포디가 낳고 어렵고 디테일하게 만드는건 마야가 낳고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100024918.1.18",
          "form": "마자요 ㅋㅋㅋㅋㅋㅋㅋㅋ 그래서 게임쪽 친구는 보니깐 프로그램 2~3개가 기본이다ㅓ라구여",
          "original_form": "마자요 ㅋㅋㅋㅋㅋㅋㅋㅋ 그래서 게임쪽 친구는 보니깐 프로그램 2~3개가 기본이다ㅓ라구여",
          "speaker_id": "2",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100024918.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100024918.1.20",
          "form": "때깔은 사실 에펙도... ㅎㅎㅎ 후반작업으로 많이 하더라구요",
          "original_form": "때깔은 사실 에펙도... ㅎㅎㅎ 후반작업으로 많이 하더라구요",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100024918.1.21",
          "form": "아 게임쪽은 가본적없어서 궁금하네요",
          "original_form": "아 게임쪽은 가본적없어서 궁금하네요",
          "speaker_id": "1",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100024918.1.22",
          "form": "그쵸 에펙도 색보정하죠",
          "original_form": "그쵸 에펙도 색보정하죠",
          "speaker_id": "1",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100024918.1.23",
          "form": "친구 드라마쪽하는 애있는데 거긴 다 색보정 풀로 하더라구요",
          "original_form": "친구 드라마쪽하는 애있는데 거긴 다 색보정 풀로 하더라구요",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100024918.1.24",
          "form": "드라마!! 그쪽은 누크? 쓰지않나요?",
          "original_form": "드라마!! 그쪽은 누크? 쓰지않나요?",
          "speaker_id": "2",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024918.1.25",
          "form": "자세히는 모르겠어요",
          "original_form": "자세히는 모르겠어요",
          "speaker_id": "1",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024918.1.26",
          "form": "저도 안가본쪽은 무지해가지고 ㅋㅋ",
          "original_form": "저도 안가본쪽은 무지해가지고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:00"
        }
      ]
