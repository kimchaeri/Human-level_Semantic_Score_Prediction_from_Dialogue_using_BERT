      "utterance": [
        {
          "id": "MDRW2100000628.1.1",
          "form": "쇼핑하는거 좋아하세요?",
          "original_form": "쇼핑하는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000628.1.2",
          "form": "네 엄청요 최근에는 아이패드 삿어요",
          "original_form": "네 엄청요 최근에는 아이패드 삿어요",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000628.1.3",
          "form": "오오!! 아이패드!! 써보시니 어떤가요",
          "original_form": "오오!! 아이패드!! 써보시니 어떤가요",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000628.1.4",
          "form": "확실히 필기감이 좋은가요?",
          "original_form": "확실히 필기감이 좋은가요?",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000628.1.5",
          "form": "필기감은 사실 갤럭시가 더 좋대요!",
          "original_form": "필기감은 사실 갤럭시가 더 좋대요!",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000628.1.6",
          "form": "저는 그림그리고 싶어서 샀는데..",
          "original_form": "저는 그림그리고 싶어서 샀는데..",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000628.1.7",
          "form": "그림 잘 그리세요?",
          "original_form": "그림 잘 그리세요?",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000628.1.8",
          "form": "아뇨..ㅋㅋ넷플릭스만 봐요",
          "original_form": "아뇨..ㅋㅋ넷플릭스만 봐요",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000628.1.9",
          "form": "초기의 목적이랑 달라져버린..",
          "original_form": "초기의 목적이랑 달라져버린..",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000628.1.10",
          "form": "ㅋㅋㅋ그래도 저는 아이패드 사셨다는게 부럽네요",
          "original_form": "ㅋㅋㅋ그래도 저는 아이패드 사셨다는게 부럽네요",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000628.1.11",
          "form": "미래의 제가 부담중이에여 ㅋㅋㅋ",
          "original_form": "미래의 제가 부담중이에여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000628.1.12",
          "form": "name2님은 최근에 사신 물건 있으신가요?",
          "original_form": "&name2&님은 최근에 사신 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000628.1.13",
          "form": "ㅋㅋㅋㅋ저는 화장품에 관심이 많아서 며칠 전에 질렀습니다...",
          "original_form": "ㅋㅋㅋㅋ저는 화장품에 관심이 많아서 며칠 전에 질렀습니다...",
          "speaker_id": "2",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000628.1.14",
          "form": "앗 뭐 사셨나요? 파데? 립?",
          "original_form": "앗 뭐 사셨나요? 파데? 립?",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000628.1.15",
          "form": "입생로랑 뷰티 라이브방송에서 구성이 좋길래 립틴트도 사고요ㅋㅋ",
          "original_form": "입생로랑 뷰티 라이브방송에서 구성이 좋길래 립틴트도 사고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000628.1.16",
          "form": "톤업크림도 샀어요",
          "original_form": "톤업크림도 샀어요",
          "speaker_id": "2",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000628.1.17",
          "form": "입생로랑 패키지가 너무 이쁘죠",
          "original_form": "입생로랑 패키지가 너무 이쁘죠",
          "speaker_id": "1",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000628.1.18",
          "form": "오 톤업크림 파데같은건데 자연스럽게 톤보정 되는건가요?",
          "original_form": "오 톤업크림 파데같은건데 자연스럽게 톤보정 되는건가요?",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000628.1.19",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000628.1.20",
          "form": "선크림이면서 톤업되는?",
          "original_form": "선크림이면서 톤업되는?",
          "speaker_id": "2",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000628.1.21",
          "form": "오 편하겠어요!",
          "original_form": "오 편하겠어요!",
          "speaker_id": "1",
          "time": "20210819 17:29"
        }
      ]
