      "utterance": [
        {
          "id": "MDRW2100041063.1.1",
          "form": "점심은 드셨나요",
          "original_form": "점심은 드셨나요",
          "speaker_id": "1",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100041063.1.2",
          "form": "네 방금 라면 끓여먹었어요",
          "original_form": "네 방금 라면 끓여먹었어요",
          "speaker_id": "2",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100041063.1.3",
          "form": "name1님은 뭐 드셨어요?",
          "original_form": "&name1&님은 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100041063.1.4",
          "form": "저는 김치국이요",
          "original_form": "저는 김치국이요",
          "speaker_id": "1",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100041063.1.5",
          "form": "무슨라면 드셨어요?",
          "original_form": "무슨라면 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100041063.1.6",
          "form": "삼양라면이요",
          "original_form": "삼양라면이요",
          "speaker_id": "2",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100041063.1.7",
          "form": "name1님 좋아하는 음식 있으신가요",
          "original_form": "&name1&님 좋아하는 음식 있으신가요",
          "speaker_id": "2",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100041063.1.8",
          "form": "저는 샤브샤브요",
          "original_form": "저는 샤브샤브요",
          "speaker_id": "1",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100041063.1.9",
          "form": "맛없기 힘든 음식이죠",
          "original_form": "맛없기 힘든 음식이죠",
          "speaker_id": "1",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100041063.1.10",
          "form": "저도 샤브샤브 진짜 좋아해요",
          "original_form": "저도 샤브샤브 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210905 16:26"
        },
        {
          "id": "MDRW2100041063.1.11",
          "form": "혹시 싫어하는 음식 있으신가요",
          "original_form": "혹시 싫어하는 음식 있으신가요",
          "speaker_id": "2",
          "time": "20210905 16:26"
        },
        {
          "id": "MDRW2100041063.1.12",
          "form": "음 민초요",
          "original_form": "음 민초요",
          "speaker_id": "1",
          "time": "20210905 16:26"
        },
        {
          "id": "MDRW2100041063.1.13",
          "form": "민초 좋아하시나요?",
          "original_form": "민초 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 16:27"
        },
        {
          "id": "MDRW2100041063.1.14",
          "form": "저도 완전 싫어해요",
          "original_form": "저도 완전 싫어해요",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100041063.1.15",
          "form": "민초랑 녹차요",
          "original_form": "민초랑 녹차요",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100041063.1.16",
          "form": "오 통했네요",
          "original_form": "오 통했네요",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100041063.1.17",
          "form": "고수는 좋아하세요?",
          "original_form": "고수는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100041063.1.18",
          "form": "어.. 아뇨 향이 쎈 걸 안 좋아해요",
          "original_form": "어.. 아뇨 향이 쎈 걸 안 좋아해요",
          "speaker_id": "2",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100041063.1.19",
          "form": "헬로님은요?",
          "original_form": "헬로님은요?",
          "speaker_id": "2",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100041063.1.20",
          "form": "저도 싫어해요",
          "original_form": "저도 싫어해요",
          "speaker_id": "1",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100041063.1.21",
          "form": "항상 쌀국수에서 빼고먹어요",
          "original_form": "항상 쌀국수에서 빼고먹어요",
          "speaker_id": "1",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100041063.1.22",
          "form": "저도요 혹시 해산물 좋아하세요?",
          "original_form": "저도요 혹시 해산물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100041063.1.23",
          "form": "맛있긴하죠",
          "original_form": "맛있긴하죠",
          "speaker_id": "1",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100041063.1.24",
          "form": "근데 껍질땜에 번거로워서",
          "original_form": "근데 껍질땜에 번거로워서",
          "speaker_id": "1",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100041063.1.25",
          "form": "가끔 먹어요",
          "original_form": "가끔 먹어요",
          "speaker_id": "1",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100041063.1.26",
          "form": "전 이제 저녁먹으러 가려구요",
          "original_form": "전 이제 저녁먹으러 가려구요",
          "speaker_id": "1",
          "time": "20210905 17:54"
        }
