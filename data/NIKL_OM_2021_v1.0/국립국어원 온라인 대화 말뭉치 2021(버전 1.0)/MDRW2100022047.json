      "utterance": [
        {
          "id": "MDRW2100022047.1.1",
          "form": "요즘 파운데이션 어떤거 쓰세용?",
          "original_form": "요즘 파운데이션 어떤거 쓰세용?",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022047.1.2",
          "form": "저 요즘 피부가 너무 푸석하고 각질만 많이 생겨서ㅠㅠㅠ 파운데이션이 잘 안먹어요",
          "original_form": "저 요즘 피부가 너무 푸석하고 각질만 많이 생겨서ㅠㅠㅠ 파운데이션이 잘 안먹어요",
          "speaker_id": "1",
          "time": "20210829 01:23"
        },
        {
          "id": "MDRW2100022047.1.3",
          "form": "요즘은 화장을 잘 안해서 안쓰고 썪히는 중이에요ㅠㅠ",
          "original_form": "요즘은 화장을 잘 안해서 안쓰고 썪히는 중이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 01:23"
        },
        {
          "id": "MDRW2100022047.1.4",
          "form": "저도 블러셔는 다 썩히고 있어용...",
          "original_form": "저도 블러셔는 다 썩히고 있어용...",
          "speaker_id": "1",
          "time": "20210829 01:23"
        },
        {
          "id": "MDRW2100022047.1.5",
          "form": "돈아까워요ㅠㅠㅋㅋㅋ 빨리 써야하는뎅",
          "original_form": "돈아까워요ㅠㅠㅋㅋㅋ 빨리 써야하는뎅",
          "speaker_id": "1",
          "time": "20210829 01:23"
        },
        {
          "id": "MDRW2100022047.1.6",
          "form": "전에는 친구가 줬던 에스쁘아 썼었는데...",
          "original_form": "전에는 친구가 줬던 에스쁘아 썼었는데...",
          "speaker_id": "2",
          "time": "20210829 01:23"
        },
        {
          "id": "MDRW2100022047.1.7",
          "form": "스트라이덱스가 피부 각질 관리에 좋다고해서 샀는데",
          "original_form": "스트라이덱스가 피부 각질 관리에 좋다고해서 샀는데",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022047.1.8",
          "form": "이것도 곧 상하겠지... 하는 중이에요ㅠㅠㅠ",
          "original_form": "이것도 곧 상하겠지... 하는 중이에요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022047.1.9",
          "form": "넘 자극적이어서ㅠㅠ",
          "original_form": "넘 자극적이어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022047.1.10",
          "form": "아아ㅠㅠ",
          "original_form": "아아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022047.1.11",
          "form": "모든 화장품이 가루였으면 좋을텐데ㅠㅠ",
          "original_form": "모든 화장품이 가루였으면 좋을텐데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022047.1.12",
          "form": "순하고 각질제거 잘되는게 좋은데 간혹가다 그런것들이 있죠ㅠㅠㅠ",
          "original_form": "순하고 각질제거 잘되는게 좋은데 간혹가다 그런것들이 있죠ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022047.1.13",
          "form": "네네 찾기 어려워요ㅠㅠㅋㅋㅋ",
          "original_form": "네네 찾기 어려워요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022047.1.14",
          "form": "저는 필링폼으로 가란시아 쓰고 있는데",
          "original_form": "저는 필링폼으로 가란시아 쓰고 있는데",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022047.1.15",
          "form": "만족스러워요",
          "original_form": "만족스러워요",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022047.1.16",
          "form": "엇 가란시아 유황 클렌징폼 썼을때",
          "original_form": "엇 가란시아 유황 클렌징폼 썼을때",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022047.1.17",
          "form": "넘 좋아서 이 브랜드 호감이었는데",
          "original_form": "넘 좋아서 이 브랜드 호감이었는데",
          "speaker_id": "1",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100022047.1.18",
          "form": "필링도 써봐야겠어용",
          "original_form": "필링도 써봐야겠어용",
          "speaker_id": "1",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100022047.1.19",
          "form": "아무래도 가격이 그리 싸진 않아서 아직 다른 종류는 못써뵜는데 미스트도 써보고 싶구ㅠㅠ",
          "original_form": "아무래도 가격이 그리 싸진 않아서 아직 다른 종류는 못써뵜는데 미스트도 써보고 싶구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100022047.1.20",
          "form": "향도 은은하니 괜찮아서 너무 호감이에요ㅠㅠ",
          "original_form": "향도 은은하니 괜찮아서 너무 호감이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100022047.1.21",
          "form": "오 미스트도 나오나봐용",
          "original_form": "오 미스트도 나오나봐용",
          "speaker_id": "1",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100022047.1.22",
          "form": "아 그런거 너무 좋아요",
          "original_form": "아 그런거 너무 좋아요",
          "speaker_id": "1",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100022047.1.23",
          "form": "은은한거",
          "original_form": "은은한거",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100022047.1.24",
          "form": "저 여름에 피부 안타겠다고 진짜 선크림",
          "original_form": "저 여름에 피부 안타겠다고 진짜 선크림",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100022047.1.25",
          "form": "왕창 바르고 다녔는데",
          "original_form": "왕창 바르고 다녔는데",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100022047.1.26",
          "form": "어떤거 향이 너무 쎄서 머리가 빙빙 돌더라구요",
          "original_form": "어떤거 향이 너무 쎄서 머리가 빙빙 돌더라구요",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100022047.1.27",
          "form": "향독한건 정말....",
          "original_form": "향독한건 정말....",
          "speaker_id": "2",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100022047.1.28",
          "form": "향료 조금만 넣어나 무향인 제품들이 좋아용",
          "original_form": "향료 조금만 넣어나 무향인 제품들이 좋아용",
          "speaker_id": "1",
          "time": "20210829 01:27"
        }
      ]
    }
