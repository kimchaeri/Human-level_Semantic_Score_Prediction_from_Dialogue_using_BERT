      "utterance": [
        {
          "id": "MDRW2100047143.1.1",
          "form": "괜찮아요!",
          "original_form": "괜찮아요!",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100047143.1.2",
          "form": "ㅎㅎ 요새 할인을 많이 하더라구요 !",
          "original_form": "ㅎㅎ 요새 할인을 많이 하더라구요 !",
          "speaker_id": "1",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100047143.1.3",
          "form": "시즌오프기간인가봐요",
          "original_form": "시즌오프기간인가봐요",
          "speaker_id": "1",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100047143.1.4",
          "form": "아 그래요?\r\n뭐 사고싶은거 있으신가요?",
          "original_form": "아 그래요?\r\n뭐 사고싶은거 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100047143.1.5",
          "form": "음 저는 최근에 한바탕 질러서요 이미 ㅋㅌㅋ",
          "original_form": "음 저는 최근에 한바탕 질러서요 이미 ㅋㅌㅋ",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100047143.1.6",
          "form": "가을옷 장만했어요",
          "original_form": "가을옷 장만했어요",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100047143.1.7",
          "form": "아하! 이미 쇼핑 많이 하셨군요",
          "original_form": "아하! 이미 쇼핑 많이 하셨군요",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100047143.1.8",
          "form": "옷 어떤거 사셨어요?",
          "original_form": "옷 어떤거 사셨어요?",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100047143.1.9",
          "form": "네 세일할때 사야 아낀다는 마음으로 ㅎㅎ..",
          "original_form": "네 세일할때 사야 아낀다는 마음으로 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100047143.1.10",
          "form": "저는 맨투맨이랑 트렌치코트요",
          "original_form": "저는 맨투맨이랑 트렌치코트요",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100047143.1.11",
          "form": "오~트렌치코트 멋있네요!",
          "original_form": "오~트렌치코트 멋있네요!",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100047143.1.12",
          "form": "마자요 세일할때 사야죠",
          "original_form": "마자요 세일할때 사야죠",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100047143.1.13",
          "form": "트렌치코트를 좋아해서요 ㅋㅋㅋ",
          "original_form": "트렌치코트를 좋아해서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100047143.1.14",
          "form": "휘뚜루마뚜루 걸치기 좋더라구요",
          "original_form": "휘뚜루마뚜루 걸치기 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100047143.1.15",
          "form": "와~저는 잘 안어울릴거같아서",
          "original_form": "와~저는 잘 안어울릴거같아서",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100047143.1.16",
          "form": "심지어 슈퍼갈때도 입는답니다",
          "original_form": "심지어 슈퍼갈때도 입는답니다",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100047143.1.17",
          "form": "쉽게 못사겠더라구요",
          "original_form": "쉽게 못사겠더라구요",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100047143.1.18",
          "form": "멋있으시네요",
          "original_form": "멋있으시네요",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100047143.1.19",
          "form": "좀 캐쥬얼한거도 있어요!",
          "original_form": "좀 캐쥬얼한거도 있어요!",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100047143.1.20",
          "form": "그 끈 안 달린거루요 ㅋㅋㅋ 그냥 자켓느낌으로",
          "original_form": "그 끈 안 달린거루요 ㅋㅋㅋ 그냥 자켓느낌으로",
