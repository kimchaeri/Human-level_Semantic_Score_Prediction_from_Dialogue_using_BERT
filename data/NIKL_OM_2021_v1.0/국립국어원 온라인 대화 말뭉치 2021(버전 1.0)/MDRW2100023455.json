      "utterance": [
        {
          "id": "MDRW2100023455.1.1",
          "form": "글씨로만되고",
          "original_form": "글씨로만되고",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023455.1.2",
          "form": "렌즈 공짜로 받는게",
          "original_form": "렌즈 공짜로 받는게",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023455.1.3",
          "form": "더 조아",
          "original_form": "더 조아",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023455.1.4",
          "form": "렌즈통",
          "original_form": "렌즈통",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023455.1.5",
          "form": "아니거든!",
          "original_form": "아니거든!",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023455.1.6",
          "form": "통뒤집으면 왓다리갓다리되고",
          "original_form": "통뒤집으면 왓다리갓다리되고",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023455.1.7",
          "form": "너렌즈어디서사지?",
          "original_form": "너렌즈어디서사지?",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.8",
          "form": "아니거든!",
          "original_form": "아니거든!",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.9",
          "form": "친구야내가렌즈를 드림렌즈때부터",
          "original_form": "친구야내가렌즈를 드림렌즈때부터",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.10",
          "form": "나는 으뜸50안경이란데서 사",
          "original_form": "나는 으뜸50안경이란데서 사",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.11",
          "form": "10년넘게꼈다",
          "original_form": "10년넘게꼈다",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.12",
          "form": "전에내가추천해준",
          "original_form": "전에내가추천해준",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.13",
          "form": "전에 니가 추천해준 시내에 거기 어디",
          "original_form": "전에 니가 추천해준 시내에 거기 어디",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.14",
          "form": "렌즈피아는",
          "original_form": "렌즈피아는",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.15",
          "form": "거기도 좋았는데",
          "original_form": "거기도 좋았는데",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.16",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.17",
          "form": "시내 잘 안가게돼서ㅜ",
          "original_form": "시내 잘 안가게돼서ㅜ",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.18",
          "form": "거기가제일싸잖아",
          "original_form": "거기가제일싸잖아",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.19",
          "form": "으뜸50안경은 체인점이라 좋아",
          "original_form": "으뜸50안경은 체인점이라 좋아",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.20",
          "form": "경대북문에도 잇어",
          "original_form": "경대북문에도 잇어",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.21",
          "form": "난거기서택배로사",
          "original_form": "난거기서택배로사",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.22",
          "form": "건그런데 시내를 잘 안가 내가",
          "original_form": "건그런데 시내를 잘 안가 내가",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.23",
          "form": "가격차이얼마안나?",
          "original_form": "가격차이얼마안나?",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.24",
          "form": "택배는 10마넌 이상 무배다아냐?",
          "original_form": "택배는 10마넌 이상 무배다아냐?",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100023455.1.25",
          "form": "난 배송비 주기 싫어...",
          "original_form": "난 배송비 주기 싫어...",
          "speaker_id": "1",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.26",
          "form": "으뜸50안경 이번에한번가볼까",
          "original_form": "으뜸50안경 이번에한번가볼까",
          "speaker_id": "2",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.27",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.28",
          "form": "으뜸50안경?",
          "original_form": "으뜸50안경?",
          "speaker_id": "1",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.29",
          "form": "아니왜그러는데 너 직장도있잖아",
          "original_form": "아니왜그러는데 너 직장도있잖아",
          "speaker_id": "2",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.30",
          "form": "거기 얼마나싼진 모르겟는데",
          "original_form": "거기 얼마나싼진 모르겟는데",
          "speaker_id": "1",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.31",
          "form": "큰 차이 ㄹ안날걸?",
          "original_form": "큰 차이 ㄹ안날걸?",
          "speaker_id": "1",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.32",
          "form": "모르겟어 이렇게 살아와서그런가봐...",
          "original_form": "모르겟어 이렇게 살아와서그런가봐...",
          "speaker_id": "1",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.33",
          "form": "렌즈피아에서 배송비내도",
          "original_form": "렌즈피아에서 배송비내도",
          "speaker_id": "2",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.34",
          "form": "굳이 필요없는걸 더 사서라도 무배조건을 맞추고",
          "original_form": "굳이 필요없는걸 더 사서라도 무배조건을 맞추고",
          "speaker_id": "1",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.35",
          "form": "배송비를 내지않으려고해",
          "original_form": "배송비를 내지않으려고해",
          "speaker_id": "1",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.36",
          "form": "다른곳보다 5만원쯤싸던데?",
          "original_form": "다른곳보다 5만원쯤싸던데?",
          "speaker_id": "2",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.37",
          "form": "오히려그게 자산적으로는 손해임",
          "original_form": "오히려그게 자산적으로는 손해임",
          "speaker_id": "2",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.38",
          "form": "아 삼전삿냐?",
          "original_form": "아 삼전삿냐?",
          "speaker_id": "2",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.39",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "1",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.40",
          "form": "하지만...",
          "original_form": "하지만...",
          "speaker_id": "1",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100023455.1.41",
          "form": "삼전 5만원대 되면 살거임",
          "original_form": "삼전 5만원대 되면 살거임",
          "speaker_id": "1",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100023455.1.42",
          "form": "사실 친구야 난 주식 말로만 하고",
          "original_form": "사실 친구야 난 주식 말로만 하고",
          "speaker_id": "1",
          "time": "20210830 03:33"
        }
      ]
    }
