      "utterance": [
        {
          "id": "MDRW2100024587.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100024587.1.2",
          "form": "종교있으신가요",
          "original_form": "종교있으신가요",
          "speaker_id": "1",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100024587.1.3",
          "form": "아뇨 굳이 말하자면 절 믿는 나신교요",
          "original_form": "아뇨 굳이 말하자면 절 믿는 나신교요",
          "speaker_id": "2",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100024587.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아하 저도 무교에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아하 저도 무교에요",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100024587.1.5",
          "form": "어릴때 교회 절은 자주갔었는데",
          "original_form": "어릴때 교회 절은 자주갔었는데",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100024587.1.6",
          "form": "가보신적있으세요",
          "original_form": "가보신적있으세요",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100024587.1.7",
          "form": "절은 산에 가면 한번씩 가보는 편이고",
          "original_form": "절은 산에 가면 한번씩 가보는 편이고",
          "speaker_id": "2",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100024587.1.8",
          "form": "교회는 초딩때 친구따라 갔었어요",
          "original_form": "교회는 초딩때 친구따라 갔었어요",
          "speaker_id": "2",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100024587.1.9",
          "form": "ㅋㅋㅋㅋㅋ저도 그래요 한국인들 다비슷하네여",
          "original_form": "ㅋㅋㅋㅋㅋ저도 그래요 한국인들 다비슷하네여",
          "speaker_id": "1",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100024587.1.10",
          "form": "절은 아무래도 관광지느낌있어서 ㅋㅋㅋㅋ1년에한번은가는듯여",
          "original_form": "절은 아무래도 관광지느낌있어서 ㅋㅋㅋㅋ1년에한번은가는듯여",
          "speaker_id": "1",
          "time": "20210831 15:06"
        },
        {
          "id": "MDRW2100024587.1.11",
          "form": "진짜 모태신앙아닌 이상 종교를 계속 믿는 건 힘들 것같아요",
          "original_form": "진짜 모태신앙아닌 이상 종교를 계속 믿는 건 힘들 것같아요",
          "speaker_id": "2",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100024587.1.12",
          "form": "맞아요 저도 완전 5살때부터 다니긴했는데",
          "original_form": "맞아요 저도 완전 5살때부터 다니긴했는데",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100024587.1.13",
          "form": "친구따라간거라 애초에믿음도없고 ㅋㅋㅋ믿지도않으니까 걍 학원다니듯갔었어요",
          "original_form": "친구따라간거라 애초에믿음도없고 ㅋㅋㅋ믿지도않으니까 걍 학원다니듯갔었어요",
          "speaker_id": "1",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100024587.1.14",
          "form": "5살때 친구따라요?",
          "original_form": "5살때 친구따라요?",
          "speaker_id": "2",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100024587.1.15",
          "form": "믿음보다는 친구랑 주말에도 놀수있다는 명분이줘",
          "original_form": "믿음보다는 친구랑 주말에도 놀수있다는 명분이줘",
          "speaker_id": "2",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100024587.1.16",
          "form": "네 ㅋㅋㅋㅋ동네친구가 교회가길래 주말에 같이따라갔어요",
          "original_form": "네 ㅋㅋㅋㅋ동네친구가 교회가길래 주말에 같이따라갔어요",
          "speaker_id": "1",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100024587.1.17",
          "form": "그리고 누가보면 완전 모태신앙러처럼 열심히활동했었져",
          "original_form": "그리고 누가보면 완전 모태신앙러처럼 열심히활동했었져",
          "speaker_id": "1",
          "time": "20210831 15:44"
        },
        {
          "id": "MDRW2100024587.1.18",
          "form": "ㅋㅋ 추억의 달란트시장",
          "original_form": "ㅋㅋ 추억의 달란트시장",
          "speaker_id": "2",
          "time": "20210831 15:44"
        },
        {
          "id": "MDRW2100024587.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ여름성경학교",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ여름성경학교",
          "speaker_id": "1",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100024587.1.20",
          "form": "저 초등부성가대도 했었어요 ㅋㅋㅋ",
          "original_form": "저 초등부성가대도 했었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100024587.1.21",
          "form": "좀 마니 딥하셨네요",
          "original_form": "좀 마니 딥하셨네요",
          "speaker_id": "2",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100024587.1.22",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
