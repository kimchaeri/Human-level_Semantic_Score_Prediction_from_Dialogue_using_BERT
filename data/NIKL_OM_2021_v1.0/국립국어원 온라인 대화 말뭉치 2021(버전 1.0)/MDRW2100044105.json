      "utterance": [
        {
          "id": "MDRW2100044105.1.1",
          "form": "해산물 좋아하시나요?",
          "original_form": "해산물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044105.1.2",
          "form": "생선은 좋아하는데",
          "original_form": "생선은 좋아하는데",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044105.1.3",
          "form": "새우나 조개류는 아무맛도 못 느끼겠어요 저는 ㅠㅠ",
          "original_form": "새우나 조개류는 아무맛도 못 느끼겠어요 저는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044105.1.4",
          "form": "새우 좋아하세요?",
          "original_form": "새우 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044105.1.5",
          "form": "전 딱딱하거나 뼈 씹히는거 빼곤 좋아해요!",
          "original_form": "전 딱딱하거나 뼈 씹히는거 빼곤 좋아해요!",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044105.1.6",
          "form": "새우 완전 사랑해요..",
          "original_form": "새우 완전 사랑해요..",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044105.1.7",
          "form": "새우는 대체",
          "original_form": "새우는 대체",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044105.1.8",
          "form": "무슨맛인건가요..?",
          "original_form": "무슨맛인건가요..?",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044105.1.9",
          "form": "너무너무 궁금해요....",
          "original_form": "너무너무 궁금해요....",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044105.1.10",
          "form": "약간 .. 먼가 맛있어요.. 말로 설명하긴 어렵네요..",
          "original_form": "약간 .. 먼가 맛있어요.. 말로 설명하긴 어렵네요..",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044105.1.11",
          "form": "생선은 어떤 생선 좋아하시나요?",
          "original_form": "생선은 어떤 생선 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044105.1.12",
          "form": "저는 가자미요!",
          "original_form": "저는 가자미요!",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044105.1.13",
          "form": "크으으으....ㅠㅠㅠㅠㅠㅠ",
          "original_form": "크으으으....ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044105.1.14",
          "form": "원랜 고등어가 좋았는데 이젠 좀 삼삼한 생선이 좋더라그오",
          "original_form": "원랜 고등어가 좋았는데 이젠 좀 삼삼한 생선이 좋더라그오",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044105.1.15",
          "form": "가자미 맛있져.. 저는 고등어 꽁치 요런 등푸른 생선좋아하고 연어 방어 붉은 생선 좋아해요..!",
          "original_form": "가자미 맛있져.. 저는 고등어 꽁치 요런 등푸른 생선좋아하고 연어 방어 붉은 생선 좋아해요..!",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044105.1.16",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044105.1.17",
          "form": "저도 회는 연어 제일 좋아해요",
          "original_form": "저도 회는 연어 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044105.1.18",
          "form": "혹시 송어회도 좋아하시나요",
          "original_form": "혹시 송어회도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044105.1.19",
          "form": "송어도 먹긴하지만 연어 방어가 있으면 잘 안먹어요...",
          "original_form": "송어도 먹긴하지만 연어 방어가 있으면 잘 안먹어요...",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044105.1.20",
          "form": "연어회 진짜 맛있죠 초밥도 맛있구요 ㅠㅠ",
          "original_form": "연어회 진짜 맛있죠 초밥도 맛있구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044105.1.21",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044105.1.22",
          "form": "코스트코 가면",
          "original_form": "코스트코 가면",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044105.1.23",
          "form": "연어 꼭 털어옵니다..",
          "original_form": "연어 꼭 털어옵니다..",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044105.1.24",
          "form": "코스트코 연어 맛있죠.. 진짜 왕크고.. 맛있고..",
          "original_form": "코스트코 연어 맛있죠.. 진짜 왕크고.. 맛있고..",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044105.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044105.1.26",
          "form": "워낙 많으니까",
          "original_form": "워낙 많으니까",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044105.1.27",
          "form": "연어장 만들어먹어두 좋더라구요",
          "original_form": "연어장 만들어먹어두 좋더라구요",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044105.1.28",
          "form": "연어장 최고죠.. 거기다가 계란노른자 딱 얹어먹으면 ..맛있어요",
          "original_form": "연어장 최고죠.. 거기다가 계란노른자 딱 얹어먹으면 ..맛있어요",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044105.1.29",
          "form": "오 노른자를 띄워먹다니",
          "original_form": "오 노른자를 띄워먹다니",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
