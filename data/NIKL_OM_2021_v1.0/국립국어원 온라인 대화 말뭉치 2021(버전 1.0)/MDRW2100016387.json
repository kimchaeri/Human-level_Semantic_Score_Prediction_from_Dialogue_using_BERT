      "utterance": [
        {
          "id": "MDRW2100016387.1.1",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.2",
          "form": "여기서도 빠지지 않는 누나 이야기가 있는데",
          "original_form": "여기서도 빠지지 않는 누나 이야기가 있는데",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.3",
          "form": "스키 잘 ㅌ",
          "original_form": "스키 잘 ㅌ",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.4",
          "form": "ㅇㄴ",
          "original_form": "ㅇㄴ",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.5",
          "form": "그건 이따하고",
          "original_form": "그건 이따하고",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.7",
          "form": "조아요",
          "original_form": "조아요",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.8",
          "form": "스키 잘타요?",
          "original_form": "스키 잘타요?",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.9",
          "form": "잘은 못타고 그",
          "original_form": "잘은 못타고 그",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.10",
          "form": "배운적은 있는대",
          "original_form": "배운적은 있는대",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.11",
          "form": "전 날아다녀요 아주그냥",
          "original_form": "전 날아다녀요 아주그냥",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.12",
          "form": "ㅡ와 대박이다",
          "original_form": "ㅡ와 대박이다",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.13",
          "form": "스키장 날다람쥐인가요",
          "original_form": "스키장 날다람쥐인가요",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016387.1.14",
          "form": "와 쌩쌩달리죠",
          "original_form": "와 쌩쌩달리죠",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016387.1.15",
          "form": "가족들이랑 가시는 편이군요",
          "original_form": "가족들이랑 가시는 편이군요",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016387.1.16",
          "form": "근데 상급은 무서워서 못가고",
          "original_form": "근데 상급은 무서워서 못가고",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016387.1.17",
          "form": "그럼 스노보드는 타세요??",
          "original_form": "그럼 스노보드는 타세요??",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016387.1.18",
          "form": "엇 저도요",
          "original_form": "엇 저도요",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016387.1.19",
          "form": "보드는 안타깝게도..",
          "original_form": "보드는 안타깝게도..",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016387.1.20",
          "form": "중급에서 노는",
          "original_form": "중급에서 노는",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016387.1.21",
          "form": "스키만 잘탑니다",
          "original_form": "스키만 잘탑니다",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016387.1.22",
          "form": "아니 배우긴했는대",
          "original_form": "아니 배우긴했는대",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016387.1.23",
          "form": "어어어 이러면",
          "original_form": "어어어 이러면",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016387.1.24",
          "form": "그 스키장을",
          "original_form": "그 스키장을",
          "speaker_id": "1",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016387.1.25",
          "form": "가족이랑 간건아니고",
          "original_form": "가족이랑 간건아니고",
          "speaker_id": "1",
          "time": "20210826 12:26"
        }
      ]
