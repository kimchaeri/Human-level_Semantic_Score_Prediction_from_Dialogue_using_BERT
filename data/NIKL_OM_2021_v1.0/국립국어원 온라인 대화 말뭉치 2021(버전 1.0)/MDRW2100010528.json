      "utterance": [
        {
          "id": "MDRW2100010528.1.1",
          "form": "저 10년 가족결합 첨봐요",
          "original_form": "저 10년 가족결합 첨봐요",
          "speaker_id": "2",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010528.1.2",
          "form": "진짜 할인 해줄만 하네요",
          "original_form": "진짜 할인 해줄만 하네요",
          "speaker_id": "2",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010528.1.3",
          "form": "계속 그 1년마다 상품권 못받았다니",
          "original_form": "계속 그 1년마다 상품권 못받았다니",
          "speaker_id": "1",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010528.1.4",
          "form": "진심 배가 너무아팠어요",
          "original_form": "진심 배가 너무아팠어요",
          "speaker_id": "1",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010528.1.5",
          "form": "세상에 상품권을 안주다니 저 바로 피토할 일이네요",
          "original_form": "세상에 상품권을 안주다니 저 바로 피토할 일이네요",
          "speaker_id": "2",
          "time": "20210823 19:14"
        },
        {
          "id": "MDRW2100010528.1.6",
          "form": "그럴때 환장해요",
          "original_form": "그럴때 환장해요",
          "speaker_id": "2",
          "time": "20210823 19:14"
        },
        {
          "id": "MDRW2100010528.1.7",
          "form": "할인못받고 하면 진짜 것도 모르고 못받음 진짜 화나요",
          "original_form": "할인못받고 하면 진짜 것도 모르고 못받음 진짜 화나요",
          "speaker_id": "2",
          "time": "20210823 19:14"
        },
        {
          "id": "MDRW2100010528.1.8",
          "form": "알아서 챙겨줄것이지",
          "original_form": "알아서 챙겨줄것이지",
          "speaker_id": "1",
          "time": "20210823 19:15"
        },
        {
          "id": "MDRW2100010528.1.9",
          "form": "전화해서 달라고해야주는거라네요",
          "original_form": "전화해서 달라고해야주는거라네요",
          "speaker_id": "1",
          "time": "20210823 19:15"
        },
        {
          "id": "MDRW2100010528.1.10",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "1",
          "time": "20210823 19:15"
        },
        {
          "id": "MDRW2100010528.1.11",
          "form": "원래 그래요",
          "original_form": "원래 그래요",
          "speaker_id": "2",
          "time": "20210823 19:15"
        },
        {
          "id": "MDRW2100010528.1.12",
          "form": "절대 안줘요",
          "original_form": "절대 안줘요",
          "speaker_id": "2",
          "time": "20210823 19:15"
        },
        {
          "id": "MDRW2100010528.1.13",
          "form": "말해야 줘요 고객이 알아서 찾아야 해요",
          "original_form": "말해야 줘요 고객이 알아서 찾아야 해요",
          "speaker_id": "2",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010528.1.14",
          "form": "대기업의 호구였어요",
          "original_form": "대기업의 호구였어요",
          "speaker_id": "1",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010528.1.15",
          "form": "저야 들어서 이제 챙겨서 할인 받아야 겠지만",
          "original_form": "저야 들어서 이제 챙겨서 할인 받아야 겠지만",
          "speaker_id": "2",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010528.1.16",
          "form": "무려 10년간",
          "original_form": "무려 10년간",
          "speaker_id": "1",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010528.1.17",
          "form": "진짜 속상 하시겠어요",
          "original_form": "진짜 속상 하시겠어요",
          "speaker_id": "2",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010528.1.18",
          "form": "헉10년 넘심해요",
          "original_form": "헉10년 넘심해요",
          "speaker_id": "2",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010528.1.19",
          "form": "그리고 대리점마다 할인 하는것도 다르고",
          "original_form": "그리고 대리점마다 할인 하는것도 다르고",
          "speaker_id": "2",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010528.1.20",
          "form": "아무도 말안해줬어요",
          "original_form": "아무도 말안해줬어요",
          "speaker_id": "1",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010528.1.21",
          "form": "잘알려주지도 않아서",
          "original_form": "잘알려주지도 않아서",
          "speaker_id": "2",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010528.1.22",
          "form": "대리점도 잘만나야해요",
          "original_form": "대리점도 잘만나야해요",
          "speaker_id": "2",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010528.1.23",
          "form": "작년에 티비수리하는 기사분이 말해줘서",
          "original_form": "작년에 티비수리하는 기사분이 말해줘서",
          "speaker_id": "1",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010528.1.24",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010528.1.25",
          "form": "그때 처음알고",
          "original_form": "그때 처음알고",
          "speaker_id": "1",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010528.1.26",
          "form": "세상에 좋은분이네요",
          "original_form": "세상에 좋은분이네요",
          "speaker_id": "2",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010528.1.27",
          "form": "엄마랑 저랑 배신감?에",
          "original_form": "엄마랑 저랑 배신감?에",
          "speaker_id": "1",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010528.1.28",
          "form": "부들부들 했어요",
          "original_form": "부들부들 했어요",
          "speaker_id": "1",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010528.1.29",
          "form": "전 우연히 간 대리점 직원분이 상품권 할인 정보 알려줬어요",
          "original_form": "전 우연히 간 대리점 직원분이 상품권 할인 정보 알려줬어요",
          "speaker_id": "2",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010528.1.30",
          "form": "손 떨리네요",
          "original_form": "손 떨리네요",
          "speaker_id": "2",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010528.1.31",
          "form": "그때는 6만원이상은 안된다더니",
          "original_form": "그때는 6만원이상은 안된다더니",
          "speaker_id": "1",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010528.1.32",
          "form": "올해는 9만원주고",
          "original_form": "올해는 9만원주고",
          "speaker_id": "1",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010528.1.33",
          "form": "웃기는 짬뽕들이에요",
          "original_form": "웃기는 짬뽕들이에요",
          "speaker_id": "1",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010528.1.34",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010528.1.35",
          "form": "넘 차이나요",
          "original_form": "넘 차이나요",
          "speaker_id": "2",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010528.1.36",
          "form": "대기업의 할인혜택 못받으면 너무 짜증나요",
          "original_form": "대기업의 할인혜택 못받으면 너무 짜증나요",
          "speaker_id": "1",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010528.1.37",
          "form": "그 sk멤버십으로",
          "original_form": "그 sk멤버십으로",
          "speaker_id": "1",
          "time": "20210823 19:19"
        }
