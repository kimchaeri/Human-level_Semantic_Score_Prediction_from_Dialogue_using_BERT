        },
        {
          "id": "MDRW2100001063.1.2",
          "form": "하요",
          "original_form": "하요",
          "speaker_id": "2",
          "time": "20210722 22:29"
        },
        {
          "id": "MDRW2100001063.1.3",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210722 22:29"
        },
        {
          "id": "MDRW2100001063.1.4",
          "form": "저 운동 완전 좋아해요",
          "original_form": "저 운동 완전 좋아해요",
          "speaker_id": "1",
          "time": "20210722 22:29"
        },
        {
          "id": "MDRW2100001063.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210722 22:29"
        },
        {
          "id": "MDRW2100001063.1.6",
          "form": "무슨 운동 좋아해요?",
          "original_form": "무슨 운동 좋아해요?",
          "speaker_id": "2",
          "time": "20210722 22:29"
        },
        {
          "id": "MDRW2100001063.1.7",
          "form": "저 그냥 간단하게 러능 뛰는 것도 좋아하고 예전에 복싱 했었어요",
          "original_form": "저 그냥 간단하게 러능 뛰는 것도 좋아하고 예전에 복싱 했었어요",
          "speaker_id": "1",
          "time": "20210722 22:30"
        },
        {
          "id": "MDRW2100001063.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210722 22:30"
        },
        {
          "id": "MDRW2100001063.1.9",
          "form": "글쿠나",
          "original_form": "글쿠나",
          "speaker_id": "2",
          "time": "20210722 22:30"
        },
        {
          "id": "MDRW2100001063.1.10",
          "form": "저도 러닝 좋아해요",
          "original_form": "저도 러닝 좋아해요",
          "speaker_id": "2",
          "time": "20210722 22:30"
        },
        {
          "id": "MDRW2100001063.1.11",
          "form": "전 마라톤 자주 했엉ᆢ",
          "original_form": "전 마라톤 자주 했엉ᆢ",
          "speaker_id": "2",
          "time": "20210722 22:31"
        },
        {
          "id": "MDRW2100001063.1.12",
          "form": "오 진짜요?",
          "original_form": "오 진짜요?",
          "speaker_id": "1",
          "time": "20210722 22:31"
        },
        {
          "id": "MDRW2100001063.1.13",
          "form": "풀은 한번 밖에 안해봤지만",
          "original_form": "풀은 한번 밖에 안해봤지만",
          "speaker_id": "2",
          "time": "20210722 22:32"
        },
        {
          "id": "MDRW2100001063.1.14",
          "form": "하프나 쿼터는 많이 했어요",
          "original_form": "하프나 쿼터는 많이 했어요",
          "speaker_id": "2",
          "time": "20210722 22:32"
        },
        {
          "id": "MDRW2100001063.1.15",
          "form": "이야.. 대박이다..",
          "original_form": "이야.. 대박이다..",
          "speaker_id": "1",
          "time": "20210722 22:32"
        },
        {
          "id": "MDRW2100001063.1.16",
          "form": "복싱은 오래했어요?",
          "original_form": "복싱은 오래했어요?",
          "speaker_id": "2",
          "time": "20210722 22:32"
        },
        {
          "id": "MDRW2100001063.1.17",
          "form": "복싱 한 4년정도 했었어요!",
          "original_form": "복싱 한 4년정도 했었어요!",
          "speaker_id": "1",
          "time": "20210722 22:33"
        },
        {
          "id": "MDRW2100001063.1.18",
          "form": "선수 준비 하다가 부상으로 그만 뒀어요..ㅠㅠ",
          "original_form": "선수 준비 하다가 부상으로 그만 뒀어요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210722 22:33"
        },
        {
          "id": "MDRW2100001063.1.19",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210722 22:33"
        },
        {
          "id": "MDRW2100001063.1.20",
          "form": "그런일이 있었군요ㅠㅠㅠ",
          "original_form": "그런일이 있었군요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 22:33"
        },
        {
          "id": "MDRW2100001063.1.21",
          "form": "힘들었겠어요ㅠㅠ",
          "original_form": "힘들었겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 22:33"
        },
        {
          "id": "MDRW2100001063.1.22",
          "form": "초반엔 정말 많이 힘들었는데 많이 괜찮아졌어요 ㅎㅎ",
          "original_form": "초반엔 정말 많이 힘들었는데 많이 괜찮아졌어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210722 22:34"
        },
        {
          "id": "MDRW2100001063.1.23",
          "form": "마라톤 좋아하시면 육상 대회 같은 것도 도전 해보셨을 것 같은데",
          "original_form": "마라톤 좋아하시면 육상 대회 같은 것도 도전 해보셨을 것 같은데",
          "speaker_id": "1",
          "time": "20210722 22:34"
        },
        {
          "id": "MDRW2100001063.1.24",
          "form": "전 경쟁으로 하면 힘들어서 그낭 취미로만 했어요ㅎㅎ",
          "original_form": "전 경쟁으로 하면 힘들어서 그낭 취미로만 했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210722 22:35"
        },
        {
          "id": "MDRW2100001063.1.25",
          "form": "그렇게 잘하는편도 아니고요",
          "original_form": "그렇게 잘하는편도 아니고요",
          "speaker_id": "2",
          "time": "20210722 22:35"
        },
        {
          "id": "MDRW2100001063.1.26",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "1",
          "time": "20210722 22:35"
        },
        {
          "id": "MDRW2100001063.1.27",
          "form": "그래도 마라톤이라니 체력 장난아니겠는데요? ㅋㅋㅋ",
          "original_form": "그래도 마라톤이라니 체력 장난아니겠는데요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210722 22:35"
        },
        {
          "id": "MDRW2100001063.1.28",
          "form": "ㅎㅎ멋진 인생 되세요",
          "original_form": "ㅎㅎ멋진 인생 되세요",
