      "utterance": [
        {
          "id": "MDRW2100017206.1.1",
          "form": "name1님은 눈이 부시게",
          "original_form": "&name1&님은 눈이 부시게",
          "speaker_id": "2",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017206.1.2",
          "form": "어느 점이 좋으셨어요?",
          "original_form": "어느 점이 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017206.1.3",
          "form": "그 혹시 시한부 인생에 관한 내용 맞나요?",
          "original_form": "그 혹시 시한부 인생에 관한 내용 맞나요?",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017206.1.4",
          "form": "드라마 마지막에 김혜자씨가 나레이션하는게 있어요",
          "original_form": "드라마 마지막에 김혜자씨가 나레이션하는게 있어요",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017206.1.5",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017206.1.6",
          "form": "그거 저도 너무 좋아해요",
          "original_form": "그거 저도 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017206.1.7",
          "form": "시한부 아니고 치매내용인데",
          "original_form": "시한부 아니고 치매내용인데",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017206.1.8",
          "form": "삶의 사소한 순간들을 사랑하는 느띰",
          "original_form": "삶의 사소한 순간들을 사랑하는 느띰",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017206.1.9",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017206.1.10",
          "form": "그걸 좀 잘 연출했다 해야하나",
          "original_form": "그걸 좀 잘 연출했다 해야하나",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017206.1.11",
          "form": "약간 치매오면",
          "original_form": "약간 치매오면",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017206.1.12",
          "form": "정신이 어려진다ㅈ하잖아요",
          "original_form": "정신이 어려진다ㅈ하잖아요",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017206.1.13",
          "form": "아아 그렇구나",
          "original_form": "아아 그렇구나",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017206.1.14",
          "form": "그걸 한지민씨랑 김혜자씨로 표현을 잘했어요",
          "original_form": "그걸 한지민씨랑 김혜자씨로 표현을 잘했어요",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017206.1.15",
          "form": "아아 나중에 저도 한번 볼게요",
          "original_form": "아아 나중에 저도 한번 볼게요",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017206.1.16",
          "form": "드라마를 싫어하는 게 아니구",
          "original_form": "드라마를 싫어하는 게 아니구",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017206.1.17",
          "form": "쭉 볼 만큼 시간이 잘 안나요",
          "original_form": "쭉 볼 만큼 시간이 잘 안나요",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017206.1.18",
          "form": "그래서 나이 먹을 수록",
          "original_form": "그래서 나이 먹을 수록",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017206.1.19",
          "form": "영화를 보게돼요",
          "original_form": "영화를 보게돼요",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017206.1.20",
          "form": "영화최근에 뭐 보신거 있으신가요",
          "original_form": "영화최근에 뭐 보신거 있으신가요",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017206.1.21",
          "form": "영화....",
          "original_form": "영화....",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017206.1.22",
          "form": "아빠가 되다?",
          "original_form": "아빠가 되다?",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017206.1.23",
          "form": "넷플릭스 영화 하나 봤어요",
          "original_form": "넷플릭스 영화 하나 봤어요",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017206.1.24",
          "form": "그건 무슨내용인가요",
          "original_form": "그건 무슨내용인가요",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017206.1.25",
          "form": "사실 일하면서 봐서",
          "original_form": "사실 일하면서 봐서",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017206.1.26",
          "form": "조금만 봤는데",
          "original_form": "조금만 봤는데",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017206.1.27",
          "form": "약간 아기와 나 같은 느낌인가",
          "original_form": "약간 아기와 나 같은 느낌인가",
          "speaker_id": "1",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017206.1.28",
          "form": "아내가 갑자기 죽어서",
          "original_form": "아내가 갑자기 죽어서",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017206.1.29",
          "form": "아.. 사별",
          "original_form": "아.. 사별",
          "speaker_id": "1",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017206.1.30",
          "form": "남편 혼자 아기 키우는 내용이에요",
          "original_form": "남편 혼자 아기 키우는 내용이에요",
          "speaker_id": "2",
          "time": "20210826 18:06"
        }
      ]
    }
