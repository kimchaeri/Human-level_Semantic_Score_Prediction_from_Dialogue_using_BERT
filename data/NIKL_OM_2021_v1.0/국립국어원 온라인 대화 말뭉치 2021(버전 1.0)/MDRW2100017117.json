      "utterance": [
        {
          "id": "MDRW2100017117.1.1",
          "form": "오늘은 사퍼해야지",
          "original_form": "오늘은 사퍼해야지",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100017117.1.2",
          "form": "하세욤",
          "original_form": "하세욤",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100017117.1.3",
          "form": "쿠킹덤숙제먼저하고",
          "original_form": "쿠킹덤숙제먼저하고",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100017117.1.4",
          "form": "쿠킹덤 재밌나보지?",
          "original_form": "쿠킹덤 재밌나보지?",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100017117.1.5",
          "form": "글쎄 딱히 다른게 재미있지 않아서",
          "original_form": "글쎄 딱히 다른게 재미있지 않아서",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100017117.1.6",
          "form": "카트해",
          "original_form": "카트해",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100017117.1.7",
          "form": "카트하라구",
          "original_form": "카트하라구",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100017117.1.8",
          "form": "스피드 게임취향이아니에요",
          "original_form": "스피드 게임취향이아니에요",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100017117.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100017117.1.10",
          "form": "요번에 업댓했는데",
          "original_form": "요번에 업댓했는데",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100017117.1.11",
          "form": "어몽어스 같은거 생겼다ㅋㅋㅋ",
          "original_form": "어몽어스 같은거 생겼다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100017117.1.12",
          "form": "카트로 따라가서 몰래 킬하는거야",
          "original_form": "카트로 따라가서 몰래 킬하는거야",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100017117.1.13",
          "form": "아니 그냥 캐릭터들이 걸어다님ㅋㅋ",
          "original_form": "아니 그냥 캐릭터들이 걸어다님ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100017117.1.14",
          "form": "물풍선에 가두는거던데",
          "original_form": "물풍선에 가두는거던데",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100017117.1.15",
          "form": "엥ㅋㅌㅌ",
          "original_form": "엥ㅋㅌㅌ",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100017117.1.16",
          "form": "멀라 진짜 어몽어스 같음",
          "original_form": "멀라 진짜 어몽어스 같음",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100017117.1.17",
          "form": "표절인가?ㅋㅋㅋ",
          "original_form": "표절인가?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100017117.1.18",
          "form": "뭐 그렇게 따지면",
          "original_form": "뭐 그렇게 따지면",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100017117.1.19",
          "form": "마피아게임 아류니까",
          "original_form": "마피아게임 아류니까",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100017117.1.20",
          "form": "걍 창작의 영역이라 봄 ㅋㅋ",
          "original_form": "걍 창작의 영역이라 봄 ㅋㅋ",
