          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017656.1.9",
          "form": "그",
          "original_form": "그",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017656.1.10",
          "form": "카톡방",
          "original_form": "카톡방",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017656.1.11",
          "form": "있었자나요",
          "original_form": "있었자나요",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017656.1.12",
          "form": "아직안나갔나요?",
          "original_form": "아직안나갔나요?",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017656.1.13",
          "form": "무슨 카톡방이요?",
          "original_form": "무슨 카톡방이요?",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017656.1.14",
          "form": "그 전남칫있던곳이요",
          "original_form": "그 전남칫있던곳이요",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017656.1.15",
          "form": "남친이생겨서 잊어버린 그 방",
          "original_form": "남친이생겨서 잊어버린 그 방",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017656.1.16",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 맞다",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 맞다",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017656.1.17",
          "form": "ㄱㅋㄱㅋㄱㅋㄱ",
          "original_form": "ㄱㅋㄱㅋㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017656.1.18",
          "form": "아직있어요 참",
          "original_form": "아직있어요 참",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017656.1.19",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017656.1.20",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017656.1.21",
          "form": "오래가는곳이네요",
          "original_form": "오래가는곳이네요",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017656.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ그냥 한명이 맨날 대화주제 가져와서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그냥 한명이 맨날 대화주제 가져와서",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017656.1.23",
          "form": "아 심지어 얘기도 이어나가?",
          "original_form": "아 심지어 얘기도 이어나가?",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017656.1.24",
          "form": "그냥 잊혀져버린방도아니고",
          "original_form": "그냥 잊혀져버린방도아니고",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017656.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ어",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ어",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017656.1.26",
          "form": "난 말안한다 이제",
          "original_form": "난 말안한다 이제",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017656.1.27",
          "form": "눈팅만하는곳이되었네",
          "original_form": "눈팅만하는곳이되었네",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017656.1.28",
          "form": "하긴 할말도없지모",
          "original_form": "하긴 할말도없지모",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017656.1.29",
          "form": "*_*",
          "original_form": "*_*",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017656.1.30",
          "form": "엉ㅋㅋㅋㅋ근데 한 10년 넘게 온라인으로 떠든 방이었으니",
          "original_form": "엉ㅋㅋㅋㅋ근데 한 10년 넘게 온라인으로 떠든 방이었으니",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017656.1.31",
          "form": "떠들만큼 떠든거 아닐까",
          "original_form": "떠들만큼 떠든거 아닐까",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017656.1.32",
          "form": "신기하다 온라인으로만나서 그렇게 이어온게",
          "original_form": "신기하다 온라인으로만나서 그렇게 이어온게",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017656.1.33",
          "form": "다시 생각해도 신기해",
          "original_form": "다시 생각해도 신기해",
