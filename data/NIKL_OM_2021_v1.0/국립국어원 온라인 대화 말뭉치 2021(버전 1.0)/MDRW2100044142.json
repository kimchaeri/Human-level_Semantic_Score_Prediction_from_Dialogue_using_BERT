      "utterance": [
        {
          "id": "MDRW2100044142.1.1",
          "form": "배고파요 야식먹고싶당",
          "original_form": "배고파요 야식먹고싶당",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044142.1.2",
          "form": "저녁은 냉동피자먹었는데 맛나네요",
          "original_form": "저녁은 냉동피자먹었는데 맛나네요",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044142.1.3",
          "form": "맛나겠네여 전 카레먹고 홈런볼 먹는중",
          "original_form": "맛나겠네여 전 카레먹고 홈런볼 먹는중",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044142.1.4",
          "form": "ㅋㅋㅋㅋㅋ홈런볼 저도인디",
          "original_form": "ㅋㅋㅋㅋㅋ홈런볼 저도인디",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044142.1.5",
          "form": "오 진짜용?ㅋㅋ",
          "original_form": "오 진짜용?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044142.1.6",
          "form": "냉장고에 넣어놨어요 ㅋㅋㅋㅋ",
          "original_form": "냉장고에 넣어놨어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044142.1.7",
          "form": "오그럼 더 맛나죵 ㅋ전그냥 순삭..",
          "original_form": "오그럼 더 맛나죵 ㅋ전그냥 순삭..",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044142.1.8",
          "form": "ㅋㅋㅋㅋ초코류 들어간거 거의 냉장고에 넣어놔요",
          "original_form": "ㅋㅋㅋㅋ초코류 들어간거 거의 냉장고에 넣어놔요",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044142.1.9",
          "form": "초코송이 좋아하시나요 ㅠㅠ시원하게먹으면 넘 맛나여",
          "original_form": "초코송이 좋아하시나요 ㅠㅠ시원하게먹으면 넘 맛나여",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044142.1.10",
          "form": "아우 넘 좋아하죠 전 일할때 빼배로 먹었네요 냉장고 있어서 넣어두고욬ㅋㅋㅋ",
          "original_form": "아우 넘 좋아하죠 전 일할때 빼배로 먹었네요 냉장고 있어서 넣어두고욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044142.1.11",
          "form": "오 ㅋㅋ 빼빼로는 안넣어봤네요~! 담에해볼게용 ㅋㅋ",
          "original_form": "오 ㅋㅋ 빼빼로는 안넣어봤네요~! 담에해볼게용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044142.1.12",
          "form": "ㅋㅋㅋㅋ네넵",
          "original_form": "ㅋㅋㅋㅋ네넵",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044142.1.13",
          "form": "잘하는 요리 있으세요?",
          "original_form": "잘하는 요리 있으세요?",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044142.1.14",
          "form": "전 집에서 김밥하는거좋아해요 집김밥 좋고",
          "original_form": "전 집에서 김밥하는거좋아해요 집김밥 좋고",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044142.1.15",
          "form": "오 맛있겠네요 저는 요리 잘못해요 ㅠㅠ",
          "original_form": "오 맛있겠네요 저는 요리 잘못해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044142.1.16",
          "form": "하는건 좋아해요 ㅋㅋㅋ 계란 말이 ?ㅋㅋ",
          "original_form": "하는건 좋아해요 ㅋㅋㅋ 계란 말이 ?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044142.1.17",
          "form": "헐 계란말이 은근어려워서 맨날 터지는데",
          "original_form": "헐 계란말이 은근어려워서 맨날 터지는데",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044142.1.18",
          "form": "마자여 근데 터진대로 맛나서ㅋㅋㅋ",
          "original_form": "마자여 근데 터진대로 맛나서ㅋㅋㅋ",
