      "utterance": [
        {
          "id": "MDRW2100017563.1.1",
          "form": "여행 어디가고싶어요",
          "original_form": "여행 어디가고싶어요",
          "speaker_id": "1",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017563.1.2",
          "form": "여행은 진짜 서울",
          "original_form": "여행은 진짜 서울",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017563.1.3",
          "form": "서울 냄새 맡으러 가규싶다",
          "original_form": "서울 냄새 맡으러 가규싶다",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017563.1.4",
          "form": "서울가고싶구나",
          "original_form": "서울가고싶구나",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017563.1.5",
          "form": "난 바다루 통영 남해",
          "original_form": "난 바다루 통영 남해",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017563.1.6",
          "form": "아니면 별보러가기ㅠㅠ",
          "original_form": "아니면 별보러가기ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017563.1.7",
          "form": "바다 좋지 ㅠㅠㅠ 별도 좋다 ㅠㅠ",
          "original_form": "바다 좋지 ㅠㅠㅠ 별도 좋다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017563.1.8",
          "form": "야간 운전은 무서워도",
          "original_form": "야간 운전은 무서워도",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017563.1.9",
          "form": "그치 별 ㅁ솓아지는데가보고시러",
          "original_form": "그치 별 ㅁ솓아지는데가보고시러",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017563.1.10",
          "form": "싶어",
          "original_form": "싶어",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017563.1.11",
          "form": "다 산에 가야 보겠더라",
          "original_form": "다 산에 가야 보겠더라",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017563.1.12",
          "form": "혼자가는 건 좀 위험하고",
          "original_form": "혼자가는 건 좀 위험하고",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017563.1.13",
          "form": "안반데기가고싶어",
          "original_form": "안반데기가고싶어",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017563.1.14",
          "form": "여기서 얼마나 걸릴려나",
          "original_form": "여기서 얼마나 걸릴려나",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017563.1.15",
          "form": "흡흡",
          "original_form": "흡흡",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017563.1.16",
          "form": "난 서울에 전시회 보러가고싶당...",
          "original_form": "난 서울에 전시회 보러가고싶당...",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017563.1.17",
          "form": "맞아 박물관 전시회 미술관",
          "original_form": "맞아 박물관 전시회 미술관",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017563.1.18",
          "form": "부산은 잘없어ㅠ",
          "original_form": "부산은 잘없어ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017563.1.19",
          "form": "응... 진짜 보고싶은 전시는 다 서울이야",
          "original_form": "응... 진짜 보고싶은 전시는 다 서울이야",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017563.1.20",
          "form": "정말그래ㅠ",
          "original_form": "정말그래ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:54"
        }
      ]
