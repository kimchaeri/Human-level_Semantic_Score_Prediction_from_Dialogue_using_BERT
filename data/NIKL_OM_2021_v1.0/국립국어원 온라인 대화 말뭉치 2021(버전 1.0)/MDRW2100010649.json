      "utterance": [
        {
          "id": "MDRW2100010649.1.1",
          "form": "종교...있으신가요?",
          "original_form": "종교...있으신가요?",
          "speaker_id": "2",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010649.1.2",
          "form": "아니요 전 무교에요!!!",
          "original_form": "아니요 전 무교에요!!!",
          "speaker_id": "1",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010649.1.3",
          "form": "종교 있으세여?",
          "original_form": "종교 있으세여?",
          "speaker_id": "1",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010649.1.4",
          "form": "저희집은 기독교예요!!!!",
          "original_form": "저희집은 기독교예요!!!!",
          "speaker_id": "2",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100010649.1.5",
          "form": "근데 저만 약간 동떨어진 기독교....",
          "original_form": "근데 저만 약간 동떨어진 기독교....",
          "speaker_id": "2",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100010649.1.6",
          "form": "아 교회 매주 나가세요?",
          "original_form": "아 교회 매주 나가세요?",
          "speaker_id": "1",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010649.1.7",
          "form": "아뇨!ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨!ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010649.1.8",
          "form": "가족들도 저도 코로나 이후 한번도 간적 없는데.... 사실 코로나 전에도 잘안갔네요 저는 ㅠㅠㅋㅋ",
          "original_form": "가족들도 저도 코로나 이후 한번도 간적 없는데.... 사실 코로나 전에도 잘안갔네요 저는 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010649.1.9",
          "form": "가족들이 교회다녀서 가끔만 가시는군요ㅋㅋㅋㅋㅋ",
          "original_form": "가족들이 교회다녀서 가끔만 가시는군요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010649.1.10",
          "form": "저도 엄청 어릴 때 친척따라 간적은 있어요",
          "original_form": "저도 엄청 어릴 때 친척따라 간적은 있어요",
          "speaker_id": "1",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010649.1.11",
          "form": "네!!ㅋㅋㅋㅋㅋㅋㄴㅋㅋㅋ",
          "original_form": "네!!ㅋㅋㅋㅋㅋㅋㄴㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010649.1.12",
          "form": "어릴때 교회가면 그 선물 주는 게 좋아서 자주 갔었어요",
          "original_form": "어릴때 교회가면 그 선물 주는 게 좋아서 자주 갔었어요",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010649.1.13",
          "form": "스티커 주던게 기억나더라고요 ㅎㅋㅋ",
          "original_form": "스티커 주던게 기억나더라고요 ㅎㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010649.1.14",
          "form": "전 과자요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 과자요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010649.1.15",
          "form": "맛있는 걸 정말 많이 준 기억이낭6",
          "original_form": "맛있는 걸 정말 많이 준 기억이낭6",
          "speaker_id": "1",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010649.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 맞아요!!! 저 엄청 특이한 외국과자 받아본적있거든요 그거 또 받고 싶어서 오랜만에 나갔는데 안주더라고요....아쉽..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 맞아요!!! 저 엄청 특이한 외국과자 받아본적있거든요 그거 또 받고 싶어서 오랜만에 나갔는데 안주더라고요....아쉽..",
          "speaker_id": "2",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010649.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 처음 오는 사람만 주는 과자 아닐까요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 처음 오는 사람만 주는 과자 아닐까요?",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010649.1.18",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010649.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그럴수도 있겠네요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그럴수도 있겠네요!",
          "speaker_id": "2",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010649.1.20",
          "form": "사실 교회가 진짜 음식 많이 주잖아요!",
          "original_form": "사실 교회가 진짜 음식 많이 주잖아요!",
          "speaker_id": "2",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010649.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요 다 퍼주죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요 다 퍼주죠",
          "speaker_id": "1",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100010649.1.22",
          "form": "저 친구따라 절도 간적있는데 그 음식이...어린나이에 안맞아서.....",
          "original_form": "저 친구따라 절도 간적있는데 그 음식이...어린나이에 안맞아서.....",
          "speaker_id": "2",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100010649.1.23",
          "form": "다신 안가게 되더라고요..",
          "original_form": "다신 안가게 되더라고요..",
          "speaker_id": "2",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100010649.1.24",
          "form": "아ㅜ정말요????",
          "original_form": "아ㅜ정말요????",
          "speaker_id": "1",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100010649.1.25",
          "form": "전 절도 궁금했거든요",
          "original_form": "전 절도 궁금했거든요",
