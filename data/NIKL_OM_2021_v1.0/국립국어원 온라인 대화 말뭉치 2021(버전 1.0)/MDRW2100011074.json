      "utterance": [
        {
          "id": "MDRW2100011074.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100011074.1.2",
          "form": "어떤 날씨를 좋아하세요",
          "original_form": "어떤 날씨를 좋아하세요",
          "speaker_id": "2",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100011074.1.3",
          "form": "저는 가을 정도의 선선한 날씨를 좋아해요",
          "original_form": "저는 가을 정도의 선선한 날씨를 좋아해요",
          "speaker_id": "1",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100011074.1.4",
          "form": "오늘은 바람 많이 불어서 좋았어요.",
          "original_form": "오늘은 바람 많이 불어서 좋았어요.",
          "speaker_id": "1",
          "time": "20210824 00:38"
        },
        {
          "id": "MDRW2100011074.1.5",
          "form": "요즘은 날씨가근데",
          "original_form": "요즘은 날씨가근데",
          "speaker_id": "2",
          "time": "20210824 00:40"
        },
        {
          "id": "MDRW2100011074.1.6",
          "form": "반팔입으면 춥고",
          "original_form": "반팔입으면 춥고",
          "speaker_id": "2",
          "time": "20210824 00:40"
        },
        {
          "id": "MDRW2100011074.1.7",
          "form": "긴팔입으면 더워서ㅜㅜ 뭐입어야할지모르갰어요",
          "original_form": "긴팔입으면 더워서ㅜㅜ 뭐입어야할지모르갰어요",
          "speaker_id": "2",
          "time": "20210824 00:40"
        },
        {
          "id": "MDRW2100011074.1.8",
          "form": "맞아요. ㅠㅠ 저는 근데 긴팔 입었어요.",
          "original_form": "맞아요. ㅠㅠ 저는 근데 긴팔 입었어요.",
          "speaker_id": "1",
          "time": "20210824 00:40"
        },
        {
          "id": "MDRW2100011074.1.9",
          "form": "태풍오전이라 바람이 많이 분 것 같아요.",
          "original_form": "태풍오전이라 바람이 많이 분 것 같아요.",
          "speaker_id": "1",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100011074.1.10",
          "form": "흠 서울이신가용",
          "original_form": "흠 서울이신가용",
          "speaker_id": "2",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100011074.1.11",
          "form": "여기도 비가오다말다하네요ㅠ",
          "original_form": "여기도 비가오다말다하네요ㅠ",
          "speaker_id": "2",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100011074.1.12",
          "form": "네 저는 서울이요 ㅜㅜ",
          "original_form": "네 저는 서울이요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100011074.1.13",
          "form": "지방은 지금많이온데요",
          "original_form": "지방은 지금많이온데요",
          "speaker_id": "1",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100011074.1.14",
          "form": "내일 우산 챙기세요",
          "original_form": "내일 우산 챙기세요",
          "speaker_id": "1",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100011074.1.15",
          "form": "헉 ㅜㅜ꿀팁감사합니다",
          "original_form": "헉 ㅜㅜ꿀팁감사합니다",
          "speaker_id": "2",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100011074.1.16",
          "form": "레인부츠도",
          "original_form": "레인부츠도",
          "speaker_id": "2",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100011074.1.17",
          "form": "신으시나용",
          "original_form": "신으시나용",
          "speaker_id": "2",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100011074.1.18",
          "form": "아니요 ㅠㅠ",
          "original_form": "아니요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100011074.1.19",
          "form": "불편해서 잘 안신어요",
          "original_form": "불편해서 잘 안신어요",
          "speaker_id": "1",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100011074.1.20",
          "form": "너무 무거워요",
          "original_form": "너무 무거워요",
          "speaker_id": "1",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100011074.1.21",
          "form": "아아 사볼까했는데",
          "original_form": "아아 사볼까했는데",
          "speaker_id": "2",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100011074.1.22",
          "form": "무겁다니ㅜㅜ고민해봐야겠어요",
          "original_form": "무겁다니ㅜㅜ고민해봐야겠어요",
          "speaker_id": "2",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100011074.1.23",
          "form": "차라리 고무 재질 쪼리가 훨씬 좋아요 ㅠㅠ 샌들이나",
          "original_form": "차라리 고무 재질 쪼리가 훨씬 좋아요 ㅠㅠ 샌들이나",
          "speaker_id": "1",
          "time": "20210824 00:46"
        },
        {
          "id": "MDRW2100011074.1.24",
          "form": "무거워서 잘 안신어요 레인부츠 ㅠㅠ",
          "original_form": "무거워서 잘 안신어요 레인부츠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 00:46"
        },
        {
          "id": "MDRW2100011074.1.25",
          "form": "아아ㅜㅠ쪼리 넘 미끄러워가지고",
          "original_form": "아아ㅜㅠ쪼리 넘 미끄러워가지고",
          "speaker_id": "2",
          "time": "20210824 00:47"
        },
        {
          "id": "MDRW2100011074.1.26",
          "form": "그럼 버켄 샌들 종류 추천드려용. 진짜 비올땐 운동화 못신어요",
          "original_form": "그럼 버켄 샌들 종류 추천드려용. 진짜 비올땐 운동화 못신어요",
          "speaker_id": "1",
          "time": "20210824 00:47"
        }
      ]
    }
