      "utterance": [
        {
          "id": "MDRW2100008218.1.1",
          "form": "안녕하세요 여행 좋아하시나요?",
          "original_form": "안녕하세요 여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008218.1.2",
          "form": "좋아해요 여행ㅜㅜ 여행 가고프네여ㅜㅜㅜ",
          "original_form": "좋아해요 여행ㅜㅜ 여행 가고프네여ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008218.1.3",
          "form": "저도요! 한창 다녓는데ㅜㅜ",
          "original_form": "저도요! 한창 다녓는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008218.1.4",
          "form": "백신 맞아도 효과가 6개월이래요ㅜ",
          "original_form": "백신 맞아도 효과가 6개월이래요ㅜ",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008218.1.5",
          "form": "그럼 해외여행은 당분간은 정말 힘들것같아요",
          "original_form": "그럼 해외여행은 당분간은 정말 힘들것같아요",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008218.1.6",
          "form": "그쳐ㅜㅜ 국내도 다니기 눈치보이구요",
          "original_form": "그쳐ㅜㅜ 국내도 다니기 눈치보이구요",
          "speaker_id": "1",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008218.1.7",
          "form": "전 동생이 부산살아서 부산갈까했는데 거기도 4단계라길래 포기했네요",
          "original_form": "전 동생이 부산살아서 부산갈까했는데 거기도 4단계라길래 포기했네요",
          "speaker_id": "2",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008218.1.8",
          "form": "부산 좋은데 아쉽네요ㅠ",
          "original_form": "부산 좋은데 아쉽네요ㅠ",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008218.1.9",
          "form": "회도 먹곡 국밥도 먹고 좋았는데ㅠㅡ",
          "original_form": "회도 먹곡 국밥도 먹고 좋았는데ㅠㅡ",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008218.1.10",
          "form": "아무래도 22년까지 해외여행은 무리같고",
          "original_form": "아무래도 22년까지 해외여행은 무리같고",
          "speaker_id": "2",
          "time": "20210822 17:59"
        },
        {
          "id": "MDRW2100008218.1.11",
          "form": "전 캠핑도 그닥 취미가아니라서",
          "original_form": "전 캠핑도 그닥 취미가아니라서",
          "speaker_id": "2",
          "time": "20210822 17:59"
        },
        {
          "id": "MDRW2100008218.1.12",
          "form": "전 캠핑도 좋아햇는데 이제 애기때문에요ㅠ",
          "original_form": "전 캠핑도 좋아햇는데 이제 애기때문에요ㅠ",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008218.1.13",
          "form": "저는 글램핑이란건 한번 해보고 싶더라고요",
          "original_form": "저는 글램핑이란건 한번 해보고 싶더라고요",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008218.1.14",
          "form": "얼마나 호화롭게 캠핑할수있나",
          "original_form": "얼마나 호화롭게 캠핑할수있나",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008218.1.15",
          "form": "싶어서 ㅋㅋ",
          "original_form": "싶어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008218.1.16",
          "form": "오 그것도 괜찮네요!!",
          "original_form": "오 그것도 괜찮네요!!",
          "speaker_id": "1",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008218.1.17",
          "form": "풀빌라도 궁금하기는하고 ㅋㅋ",
          "original_form": "풀빌라도 궁금하기는하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008218.1.18",
          "form": "저는 동네에 호수 걸어요 요새 딴곳은 못가니까 ㅋㅋ",
          "original_form": "저는 동네에 호수 걸어요 요새 딴곳은 못가니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008218.1.19",
          "form": "ㅋㅋ저는 집에 붙박이장이에요",
          "original_form": "ㅋㅋ저는 집에 붙박이장이에요",
          "speaker_id": "1",
          "time": "20210822 18:55"
        },
        {
          "id": "MDRW2100008218.1.20",
          "form": "동네에 출렁다리가 생겼대서 억지로 가보려고요",
          "original_form": "동네에 출렁다리가 생겼대서 억지로 가보려고요",
          "speaker_id": "2",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100008218.1.21",
          "form": "국내에서 제일 긴 다리라고",
          "original_form": "국내에서 제일 긴 다리라고",
          "speaker_id": "2",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100008218.1.22",
          "form": "엄청 광고하네요",
          "original_form": "엄청 광고하네요",
          "speaker_id": "2",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100008218.1.23",
          "form": "구경삼아 다녀오세요ㅎ",
          "original_form": "구경삼아 다녀오세요ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008218.1.24",
          "form": "지금 갔는데 6시까지만 받네요ㅜㅜ",
          "original_form": "지금 갔는데 6시까지만 받네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008218.1.25",
          "form": "8월까지만 입장료 무료라서 얼른 갔다와야겄네요",
          "original_form": "8월까지만 입장료 무료라서 얼른 갔다와야겄네요",
          "speaker_id": "2",
          "time": "20210822 19:07"
        }
      ]
