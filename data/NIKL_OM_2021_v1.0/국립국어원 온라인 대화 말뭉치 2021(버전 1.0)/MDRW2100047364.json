      "utterance": [
        {
          "id": "MDRW2100047364.1.1",
          "form": "혹시 잘 보시는 티비프로나 이런거 있으신가요",
          "original_form": "혹시 잘 보시는 티비프로나 이런거 있으신가요",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100047364.1.2",
          "form": "저 전참시",
          "original_form": "저 전참시",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100047364.1.3",
          "form": "나혼산",
          "original_form": "나혼산",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100047364.1.4",
          "form": "펜트하우스요 ㅋㅋ",
          "original_form": "펜트하우스요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100047364.1.5",
          "form": "아 저 펜트하우스 잘 봤었는데 3부턴 못보고 있어요 ㅋㅋㅋ",
          "original_form": "아 저 펜트하우스 잘 봤었는데 3부턴 못보고 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100047364.1.6",
          "form": "맨날 죽였다 살려서 ㅋㅋㅋ",
          "original_form": "맨날 죽였다 살려서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100047364.1.7",
          "form": "그작가 원래 그래요 ㅜㅜ",
          "original_form": "그작가 원래 그래요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100047364.1.8",
          "form": "맨날 죽고",
          "original_form": "맨날 죽고",
          "speaker_id": "2",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100047364.1.9",
          "form": "살고",
          "original_form": "살고",
          "speaker_id": "2",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100047364.1.10",
          "form": "저번주 대박이였어요 ㅡㅡ",
          "original_form": "저번주 대박이였어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100047364.1.11",
          "form": "ㅋㅋㅋㅋ 그쵸.. 누가 죽어도 진짜 죽은거 맞는지 의심부터..",
          "original_form": "ㅋㅋㅋㅋ 그쵸.. 누가 죽어도 진짜 죽은거 맞는지 의심부터..",
          "speaker_id": "1",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100047364.1.12",
          "form": "참이게 봐야하나 싶고",
          "original_form": "참이게 봐야하나 싶고",
          "speaker_id": "2",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100047364.1.13",
          "form": "또 누가 살아났나요? ㅋㅋㅋㅋ",
          "original_form": "또 누가 살아났나요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047364.1.14",
          "form": "아뇨 천서진이",
          "original_form": "아뇨 천서진이",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047364.1.15",
          "form": "똭 배에",
          "original_form": "똭 배에",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047364.1.16",
          "form": "샹들리에이 떨어져서",
          "original_form": "샹들리에이 떨어져서",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047364.1.17",
          "form": "아 그거 영상으로 봤어요 샹들리에 맞죠",
          "original_form": "아 그거 영상으로 봤어요 샹들리에 맞죠",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047364.1.18",
          "form": "잔인했어요",
          "original_form": "잔인했어요",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047364.1.19",
          "form": "너무 자극적이던데 ㅠㅠ",
          "original_form": "너무 자극적이던데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047364.1.20",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047364.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047364.1.22",
          "form": "연기는 진짜 잘하더라고요",
          "original_form": "연기는 진짜 잘하더라고요",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047364.1.23",
          "form": "근데 이제 얼마 안남아서 달릴수 밖에 없네요",
          "original_form": "근데 이제 얼마 안남아서 달릴수 밖에 없네요",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100047364.1.24",
          "form": "다잘해요 ㅎㅎ",
          "original_form": "다잘해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100047364.1.25",
          "form": "ㅋㅋㅋㅋ 저도 궁금해서 마지막회는 볼거 같아요",
          "original_form": "ㅋㅋㅋㅋ 저도 궁금해서 마지막회는 볼거 같아요",
          "speaker_id": "1",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100047364.1.26",
          "form": "그럼요 막방은 봐야죠 ㅋㅋ",
          "original_form": "그럼요 막방은 봐야죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100047364.1.27",
          "form": "아 죽는 사람이 있긴 한지 모르겠어요 ㅋㅋ",
          "original_form": "아 죽는 사람이 있긴 한지 모르겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047364.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜 다 살아나면 웃길거 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜 다 살아나면 웃길거 같아요",
          "speaker_id": "1",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100047364.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100047364.1.30",
          "form": "근데 뭐 지금도 다살아나니",
          "original_form": "근데 뭐 지금도 다살아나니",
          "speaker_id": "2",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100047364.1.31",
          "form": "유진만 살아나면 될것 같아요",
          "original_form": "유진만 살아나면 될것 같아요",
          "speaker_id": "2",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100047364.1.32",
          "form": "아 유진은 아직 죽은 상태군요",
          "original_form": "아 유진은 아직 죽은 상태군요",
          "speaker_id": "1",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100047364.1.33",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100047364.1.34",
          "form": "어찌 살리나 몰라요",
          "original_form": "어찌 살리나 몰라요",
          "speaker_id": "2",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100047364.1.35",
          "form": "전 상상리 안되요",
          "original_form": "전 상상리 안되요",
          "speaker_id": "2",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100047364.1.36",
          "form": "안살아 날까요?",
          "original_form": "안살아 날까요?",
          "speaker_id": "2",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100047364.1.37",
          "form": "뭔가 주인공급이라 살거 같기도 하고요",
          "original_form": "뭔가 주인공급이라 살거 같기도 하고요",
          "speaker_id": "1",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100047364.1.38",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100047364.1.39",
          "form": "도대체 죽어도 믿을수가 없어요 ㅋㅋ",
          "original_form": "도대체 죽어도 믿을수가 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:44"
        }
      ]
    }
