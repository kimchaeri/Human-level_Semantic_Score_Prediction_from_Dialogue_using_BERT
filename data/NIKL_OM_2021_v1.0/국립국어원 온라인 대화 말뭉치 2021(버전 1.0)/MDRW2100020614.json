      "utterance": [
        {
          "id": "MDRW2100020614.1.1",
          "form": "아침은?",
          "original_form": "아침은?",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020614.1.2",
          "form": "아침 먹었어! 밥이랑 두부부침이랑 계란 팽이버섯전 또 오이무침!!",
          "original_form": "아침 먹었어! 밥이랑 두부부침이랑 계란 팽이버섯전 또 오이무침!!",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020614.1.3",
          "form": "다양하게 잘먹었네\n난 늦잠과 귀찮음으로 아직 암것도 안먹\n테아를 가야하나 고민중\n귀찮아서 밍기적ㅋㅋ",
          "original_form": "다양하게 잘먹었네\n난 늦잠과 귀찮음으로 아직 암것도 안먹\n테아를 가야하나 고민중\n귀찮아서 밍기적ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020614.1.4",
          "form": "테아 가자~~ 공짜밥 좋잖아 ㅋㅋ",
          "original_form": "테아 가자~~ 공짜밥 좋잖아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020614.1.5",
          "form": "그치만 방 나서는게 귀찮아서ㅋㅋㅋ\n고민중",
          "original_form": "그치만 방 나서는게 귀찮아서ㅋㅋㅋ\n고민중",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020614.1.6",
          "form": "후다닥 대충 나섯다ㅋ",
          "original_form": "후다닥 대충 나섯다ㅋ",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020614.1.7",
          "form": "오 잘했다 잘했다",
          "original_form": "오 잘했다 잘했다",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020614.1.8",
          "form": "공짜밥 좋아좋아",
          "original_form": "공짜밥 좋아좋아",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020614.1.9",
          "form": "씨리얼 거봉 가져옴",
          "original_form": "씨리얼 거봉 가져옴",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020614.1.10",
          "form": "어여 먹고 씻어야 지ㅋ",
          "original_form": "어여 먹고 씻어야 지ㅋ",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020614.1.11",
          "form": "오 좋으네 역시 가길 잘 했네",
          "original_form": "오 좋으네 역시 가길 잘 했네",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020614.1.12",
          "form": "우리집도 어제 포도 사왔더라",
          "original_form": "우리집도 어제 포도 사왔더라",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020614.1.13",
          "form": "포도철이 왔네",
          "original_form": "포도철이 왔네",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020614.1.14",
          "form": "그니까 ",
          "original_form": "그니까 ",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020614.1.15",
          "form": "장보러 같이 안가?",
          "original_form": "장보러 같이 안가?",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020614.1.16",
          "form": "ㅇㅇ난 안감 귀찮아서 ㅋㅋ",
          "original_form": "ㅇㅇ난 안감 귀찮아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020614.1.17",
          "form": "ㅋㅋㅋㄱ난 가끔 감\n내 먹고싶은거 고르고싶을때",
          "original_form": "ㅋㅋㅋㄱ난 가끔 감\n내 먹고싶은거 고르고싶을때",
          "speaker_id": "2",
          "time": "20210828 11:41"
        },
        {
          "id": "MDRW2100020614.1.18",
          "form": "ㅇㅇ 마트 따라가면 내가 먹을거 골라서 좋긴함",
          "original_form": "ㅇㅇ 마트 따라가면 내가 먹을거 골라서 좋긴함",
          "speaker_id": "1",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100020614.1.19",
          "form": "맞어ㅋㅋ물가가 점점 올라 장보기 무섭기도해",
          "original_form": "맞어ㅋㅋ물가가 점점 올라 장보기 무섭기도해",
          "speaker_id": "2",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020614.1.20",
          "form": "ㅜㅜ 진짜 요즘 채소값 장난아니고 돼지고기도 난리더라",
          "original_form": "ㅜㅜ 진짜 요즘 채소값 장난아니고 돼지고기도 난리더라",
          "speaker_id": "1",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020614.1.21",
          "form": "삼겹살 먹고싶당",
          "original_form": "삼겹살 먹고싶당",
          "speaker_id": "1",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020614.1.22",
          "form": "삼겹살 한번 먹어야겠네",
          "original_form": "삼겹살 한번 먹어야겠네",
          "speaker_id": "2",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020614.1.23",
          "form": "요즘 국산아니라도 고기 괜찮다던데\n함 먹어봐",
          "original_form": "요즘 국산아니라도 고기 괜찮다던데\n함 먹어봐",
          "speaker_id": "2",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020614.1.24",
          "form": "아빠엄마가 냄새에 예민해서",
          "original_form": "아빠엄마가 냄새에 예민해서",
          "speaker_id": "1",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020614.1.25",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020614.1.26",
          "form": "그랬구나 국산 삼겹 가야겠네",
          "original_form": "그랬구나 국산 삼겹 가야겠네",
          "speaker_id": "2",
          "time": "20210828 12:00"
        },
