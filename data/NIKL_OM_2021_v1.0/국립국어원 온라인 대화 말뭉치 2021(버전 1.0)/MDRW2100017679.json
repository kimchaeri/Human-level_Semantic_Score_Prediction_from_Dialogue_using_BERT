      "utterance": [
        {
          "id": "MDRW2100017679.1.1",
          "form": "그거에요",
          "original_form": "그거에요",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017679.1.2",
          "form": "다이어트 말로만 해요",
          "original_form": "다이어트 말로만 해요",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017679.1.3",
          "form": "주1회면 다이어트죠 뭐 ㅎㅎ",
          "original_form": "주1회면 다이어트죠 뭐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017679.1.4",
          "form": "샐러드 먹어도",
          "original_form": "샐러드 먹어도",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017679.1.5",
          "form": "너무 많이 먹어서",
          "original_form": "너무 많이 먹어서",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017679.1.6",
          "form": "치킨 샐러드?",
          "original_form": "치킨 샐러드?",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017679.1.7",
          "form": "의미가 없는것같아요ㅋㅋ",
          "original_form": "의미가 없는것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017679.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017679.1.9",
          "form": "귀여우시다 ㅋㅋㅋ",
          "original_form": "귀여우시다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017679.1.10",
          "form": "아니면 리코타치즈 샐러드먹어용",
          "original_form": "아니면 리코타치즈 샐러드먹어용",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017679.1.11",
          "form": "저도 그래요 근데 ㅋㅋㅋ",
          "original_form": "저도 그래요 근데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017679.1.12",
          "form": "치즈 좋져 ㅋㅋㅋ",
          "original_form": "치즈 좋져 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017679.1.13",
          "form": "요즘 그릭복숭아 핫하던데",
          "original_form": "요즘 그릭복숭아 핫하던데",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017679.1.14",
          "form": "생샐러드는 맛없어요 솔직히...",
          "original_form": "생샐러드는 맛없어요 솔직히...",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017679.1.15",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017679.1.16",
          "form": "오 그릭이 뭐에요?",
          "original_form": "오 그릭이 뭐에요?",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017679.1.17",
          "form": "그쳐ㅠㅠ 소스빨이에요",
          "original_form": "그쳐ㅠㅠ 소스빨이에요",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017679.1.18",
          "form": "신박한 음식을 많이 아시네요",
          "original_form": "신박한 음식을 많이 아시네요",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017679.1.19",
          "form": "오잉",
          "original_form": "오잉",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017679.1.20",
          "form": "복숭아랑 같이 먹는건데",
          "original_form": "복숭아랑 같이 먹는건데",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017679.1.21",
          "form": "맛있대요",
          "original_form": "맛있대요",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017679.1.22",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017679.1.23",
          "form": "그 그릭요거트에 복숭아 섞기?",
          "original_form": "그 그릭요거트에 복숭아 섞기?",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017679.1.24",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017679.1.25",
          "form": "근데 복숭아 철이",
          "original_form": "근데 복숭아 철이",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017679.1.26",
          "form": "이제 지나고있어서 아쉬워요ㅠ",
          "original_form": "이제 지나고있어서 아쉬워요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:40"
