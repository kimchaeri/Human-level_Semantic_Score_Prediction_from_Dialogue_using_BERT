      "utterance": [
        {
          "id": "MDRW2100035135.1.1",
          "form": "후기로는 마크니가 맛있대용",
          "original_form": "후기로는 마크니가 맛있대용",
          "speaker_id": "1",
          "time": "20210829 06:40"
        },
        {
          "id": "MDRW2100035135.1.2",
          "form": "오 참고해야겠어요",
          "original_form": "오 참고해야겠어요",
          "speaker_id": "2",
          "time": "20210829 06:40"
        },
        {
          "id": "MDRW2100035135.1.3",
          "form": "ㅎㅎ 커피는 무슨 커피 좋아하세요?",
          "original_form": "ㅎㅎ 커피는 무슨 커피 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 06:41"
        },
        {
          "id": "MDRW2100035135.1.4",
          "form": "인디아게이트인가? 거기서 완제품으로 나오는 거 맛있었어요",
          "original_form": "인디아게이트인가? 거기서 완제품으로 나오는 거 맛있었어요",
          "speaker_id": "2",
          "time": "20210829 06:41"
        },
        {
          "id": "MDRW2100035135.1.5",
          "form": "엇 추천 감사해요!",
          "original_form": "엇 추천 감사해요!",
          "speaker_id": "1",
          "time": "20210829 06:41"
        },
        {
          "id": "MDRW2100035135.1.6",
          "form": "음식 추천은 언제나 환영이에요ㅋㅋ",
          "original_form": "음식 추천은 언제나 환영이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:41"
        },
        {
          "id": "MDRW2100035135.1.7",
          "form": "맞아용   저 커피는 별로 안 마셔서",
          "original_form": "맞아용   저 커피는 별로 안 마셔서",
          "speaker_id": "2",
          "time": "20210829 06:41"
        },
        {
          "id": "MDRW2100035135.1.8",
          "form": "향만 좋아해요ㅋㅋㅋ",
          "original_form": "향만 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:41"
        },
        {
          "id": "MDRW2100035135.1.9",
          "form": "저도 카페인때문에 잘 마시지는 못하는데",
          "original_form": "저도 카페인때문에 잘 마시지는 못하는데",
          "speaker_id": "1",
          "time": "20210829 06:42"
        },
        {
          "id": "MDRW2100035135.1.10",
          "form": "모카커피 향을 정말 좋아해요",
          "original_form": "모카커피 향을 정말 좋아해요",
          "speaker_id": "1",
          "time": "20210829 06:42"
        },
        {
          "id": "MDRW2100035135.1.11",
          "form": "그러시군용",
          "original_form": "그러시군용",
          "speaker_id": "2",
          "time": "20210829 06:42"
        },
        {
          "id": "MDRW2100035135.1.12",
          "form": "그쵸 커피내리는 향이 진짜 예술",
          "original_form": "그쵸 커피내리는 향이 진짜 예술",
          "speaker_id": "2",
          "time": "20210829 06:42"
        },
        {
          "id": "MDRW2100035135.1.13",
          "form": "원두 중에 브라질 산토스였나?",
          "original_form": "원두 중에 브라질 산토스였나?",
          "speaker_id": "2",
          "time": "20210829 06:43"
        },
        {
          "id": "MDRW2100035135.1.14",
          "form": "그게 모카커피느낌이라 하더라구요",
          "original_form": "그게 모카커피느낌이라 하더라구요",
          "speaker_id": "2",
          "time": "20210829 06:43"
        },
        {
          "id": "MDRW2100035135.1.15",
          "form": "와아 브라질 커피도 맛있군용",
          "original_form": "와아 브라질 커피도 맛있군용",
          "speaker_id": "1",
          "time": "20210829 06:43"
        },
        {
          "id": "MDRW2100035135.1.16",
          "form": "참고할게용 메모메모...ㅋㅋ",
          "original_form": "참고할게용 메모메모...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:43"
        },
        {
          "id": "MDRW2100035135.1.17",
          "form": "그거 들어보셨어용?",
          "original_form": "그거 들어보셨어용?",
          "speaker_id": "1",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100035135.1.18",
          "form": "미국에서 요즘 인기라는데",
          "original_form": "미국에서 요즘 인기라는데",
          "speaker_id": "1",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100035135.1.19",
          "form": "뭐용?",
          "original_form": "뭐용?",
          "speaker_id": "2",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100035135.1.20",
          "form": "커피?",
          "original_form": "커피?",
          "speaker_id": "2",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100035135.1.21",
          "form": "굽기전에 쿠키반죽을",
          "original_form": "굽기전에 쿠키반죽을",
          "speaker_id": "1",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100035135.1.22",
          "form": "먹는대용",
          "original_form": "먹는대용",
          "speaker_id": "1",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100035135.1.23",
          "form": "아 쿠키도우",
          "original_form": "아 쿠키도우",
          "speaker_id": "2",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100035135.1.24",
          "form": "네네! 아시는군요!",
          "original_form": "네네! 아시는군요!",
          "speaker_id": "1",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100035135.1.25",
          "form": "네 들어봤어요",
          "original_form": "네 들어봤어요",
          "speaker_id": "2",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100035135.1.26",
          "form": "무슨 맛일까용?",
          "original_form": "무슨 맛일까용?",
          "speaker_id": "1",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100035135.1.27",
          "form": "밀가루맛일것같은뎅ㅋㅋ",
          "original_form": "밀가루맛일것같은뎅ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:45"
        }
      ]
    }
