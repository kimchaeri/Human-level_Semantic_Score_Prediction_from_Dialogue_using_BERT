      "utterance": [
        {
          "id": "MDRW2100002991.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002991.1.2",
          "form": "안녕 오랜만이다",
          "original_form": "안녕 오랜만이다",
          "speaker_id": "2",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002991.1.3",
          "form": "오랜만..은 아니고",
          "original_form": "오랜만..은 아니고",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002991.1.4",
          "form": "아직 학교야?",
          "original_form": "아직 학교야?",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002991.1.5",
          "form": "응 6시넘어야 끝날 것 같아",
          "original_form": "응 6시넘어야 끝날 것 같아",
          "speaker_id": "2",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002991.1.6",
          "form": "너 공부는 어때",
          "original_form": "너 공부는 어때",
          "speaker_id": "2",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002991.1.7",
          "form": "대체 교수님은 너를 언제 놓아주니?",
          "original_form": "대체 교수님은 너를 언제 놓아주니?",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002991.1.8",
          "form": "나는 앞에서 교수님이 설명중이야",
          "original_form": "나는 앞에서 교수님이 설명중이야",
          "speaker_id": "1",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002991.1.9",
          "form": "흐린귀 하고있어",
          "original_form": "흐린귀 하고있어",
          "speaker_id": "1",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002991.1.10",
          "form": "교슈님 말씀을 잘 들어야지",
          "original_form": "교슈님 말씀을 잘 들어야지",
          "speaker_id": "2",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002991.1.11",
          "form": "큰일이야 시험 잘봐야할텐데.",
          "original_form": "큰일이야 시험 잘봐야할텐데.",
          "speaker_id": "1",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002991.1.12",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "1",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002991.1.13",
          "form": "그러려먄 공부를 해야겠지",
          "original_form": "그러려먄 공부를 해야겠지",
          "speaker_id": "1",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002991.1.14",
          "form": "ㅠㅠ시험은 너무 스트레스야",
          "original_form": "ㅠㅠ시험은 너무 스트레스야",
          "speaker_id": "2",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002991.1.15",
          "form": "하지만 공부도 너무 스트레스야",
          "original_form": "하지만 공부도 너무 스트레스야",
          "speaker_id": "2",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002991.1.16",
          "form": "맞아 스트레스야",
          "original_form": "맞아 스트레스야",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002991.1.17",
          "form": "잘쳐야더ㅐ",
          "original_form": "잘쳐야더ㅐ",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002991.1.18",
          "form": "시험.",
          "original_form": "시험.",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002991.1.19",
          "form": "앉아있으면 허리아프지안ㄹ아?",
          "original_form": "앉아있으면 허리아프지안ㄹ아?",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002991.1.20",
          "form": "아프지",
          "original_form": "아프지",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002991.1.21",
          "form": "스트레칭 잘 해.",
          "original_form": "스트레칭 잘 해.",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002991.1.22",
          "form": "건강 조심해야해",
          "original_form": "건강 조심해야해",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002991.1.23",
          "form": "너도 오늘 하교가 늦네.",
          "original_form": "너도 오늘 하교가 늦네.",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002991.1.24",
          "form": "항상 그렇지 뭐",
          "original_form": "항상 그렇지 뭐",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002991.1.25",
          "form": "그래도 월말에 인건비를 받으니까",
          "original_form": "그래도 월말에 인건비를 받으니까",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002991.1.26",
          "form": "열심히해야지",
          "original_form": "열심히해야지",
          "speaker_id": "2",
          "time": "20210820 17:26"
        }
