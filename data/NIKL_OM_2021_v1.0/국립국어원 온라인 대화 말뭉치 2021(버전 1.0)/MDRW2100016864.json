      "utterance": [
        {
          "id": "MDRW2100016864.1.1",
          "form": "최근에 무슨 물건 구매하셨어요?",
          "original_form": "최근에 무슨 물건 구매하셨어요?",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016864.1.2",
          "form": "최근에 당근으로 에그팬 샀어요 ㅋㅋㅋ",
          "original_form": "최근에 당근으로 에그팬 샀어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016864.1.3",
          "form": "에그팬이 뭔가요??",
          "original_form": "에그팬이 뭔가요??",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016864.1.4",
          "form": "엄청작은팬인데 에그프라이만하기 딱 좋은크기에요!",
          "original_form": "엄청작은팬인데 에그프라이만하기 딱 좋은크기에요!",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016864.1.5",
          "form": "오~ 흥정 하셨나요?",
          "original_form": "오~ 흥정 하셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016864.1.6",
          "form": "음 아뇨 ㅋㅋ 워낙 싼가격에 올리셔서 그냥 그대로 응답했어요~",
          "original_form": "음 아뇨 ㅋㅋ 워낙 싼가격에 올리셔서 그냥 그대로 응답했어요~",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016864.1.7",
          "form": "위시리스트 있으세요??",
          "original_form": "위시리스트 있으세요??",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016864.1.8",
          "form": "네있어요! 제트플립 넘 갖고싶어요 ㅠ",
          "original_form": "네있어요! 제트플립 넘 갖고싶어요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016864.1.9",
          "form": "무슨색이요??",
          "original_form": "무슨색이요??",
          "speaker_id": "2",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016864.1.10",
          "form": "전 무조건 라벤더요!! 보라돌이라서욬ㅋㅋ",
          "original_form": "전 무조건 라벤더요!! 보라돌이라서욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016864.1.11",
          "form": "라벤다 예쁘더라고요!! 전 반지 사고 싶어요!",
          "original_form": "라벤다 예쁘더라고요!! 전 반지 사고 싶어요!",
          "speaker_id": "2",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016864.1.12",
          "form": "오 어떤브랜드 반지 관심있나여??",
          "original_form": "오 어떤브랜드 반지 관심있나여??",
          "speaker_id": "1",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016864.1.13",
          "form": "판도라요! 아시나요?",
          "original_form": "판도라요! 아시나요?",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016864.1.14",
          "form": "알죠 ㅎㅎ 저두몇개갖고있어요!",
          "original_form": "알죠 ㅎㅎ 저두몇개갖고있어요!",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016864.1.15",
          "form": "탄생석 사려는건가여??",
          "original_form": "탄생석 사려는건가여??",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016864.1.16",
