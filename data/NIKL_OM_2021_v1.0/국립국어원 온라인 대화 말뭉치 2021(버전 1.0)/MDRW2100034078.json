      "utterance": [
        {
          "id": "MDRW2100034078.1.1",
          "form": "가요는 요샌 발라드이고",
          "original_form": "가요는 요샌 발라드이고",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100034078.1.2",
          "form": "2ne1",
          "original_form": "2ne1",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100034078.1.3",
          "form": "컴백홈이 요새 자꾸 ㅋㅋ",
          "original_form": "컴백홈이 요새 자꾸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100034078.1.4",
          "form": "입에 맴돌아요",
          "original_form": "입에 맴돌아요",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100034078.1.5",
          "form": "다시 돌라오길 바라는 가수예요",
          "original_form": "다시 돌라오길 바라는 가수예요",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100034078.1.6",
          "form": "캐논도 좋아해요",
          "original_form": "캐논도 좋아해요",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100034078.1.7",
          "form": "딸이 피아노로 이제 치게 되서 넘좋아요 ㅎㅎ",
          "original_form": "딸이 피아노로 이제 치게 되서 넘좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100034078.1.8",
          "form": "전 아들이 꼭 피아노 잘쳤음 했는데 안배워서 ㅡㅡ음",
          "original_form": "전 아들이 꼭 피아노 잘쳤음 했는데 안배워서 ㅡㅡ음",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100034078.1.9",
          "form": "피아노 잘치는 남자 멋진데",
          "original_form": "피아노 잘치는 남자 멋진데",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100034078.1.10",
          "form": "보면 가수도 잘치면 넘 멋있잖아요",
          "original_form": "보면 가수도 잘치면 넘 멋있잖아요",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100034078.1.11",
          "form": "딸이 피아노 치는군요~ 어디까지 배웠어요? 저 요즘에 피아노 독학하는데 바이엘 3까지 배웠어요",
          "original_form": "딸이 피아노 치는군요~ 어디까지 배웠어요? 저 요즘에 피아노 독학하는데 바이엘 3까지 배웠어요",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100034078.1.12",
          "form": "ㅋㅋ체르니 30요",
          "original_form": "ㅋㅋ체르니 30요",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100034078.1.13",
          "form": "혼자서요",
          "original_form": "혼자서요",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100034078.1.14",
          "form": "와 전 음계도 잘 모르겠던데요",
          "original_form": "와 전 음계도 잘 모르겠던데요",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100034078.1.15",
          "form": "열정적 이십니다",
          "original_form": "열정적 이십니다",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100034078.1.16",
          "form": "음악 좋아 하시는군요",
          "original_form": "음악 좋아 하시는군요",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100034078.1.17",
          "form": "음악 진짜 좋아해요.. 듣는거요..ㅎㅎ 악기 연주 잘하는 사람들은 진짜 부러워요.. 그래서 유툽에서 가끔 연주 잘하는 컨텐츠 막 찾아볼때 있어요..",
          "original_form": "음악 진짜 좋아해요.. 듣는거요..ㅎㅎ 악기 연주 잘하는 사람들은 진짜 부러워요.. 그래서 유툽에서 가끔 연주 잘하는 컨텐츠 막 찾아볼때 있어요..",
          "speaker_id": "1",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100034078.1.18",
          "form": "혹시 리코더로 꿀벌의 비행 연주하는 거 본적 있나요?",
          "original_form": "혹시 리코더로 꿀벌의 비행 연주하는 거 본적 있나요?",
          "speaker_id": "1",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100034078.1.19",
          "form": "아뇨 잠깐요 보고 올께요",
          "original_form": "아뇨 잠깐요 보고 올께요",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100034078.1.20",
          "form": "헉 왠일",
          "original_form": "헉 왠일",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
