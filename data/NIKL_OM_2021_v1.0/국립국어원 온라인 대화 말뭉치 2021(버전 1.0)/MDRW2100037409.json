      "utterance": [
        {
          "id": "MDRW2100037409.1.1",
          "form": "빽다방 몽땅쉐이크 먹어봤어?",
          "original_form": "빽다방 몽땅쉐이크 먹어봤어?",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.2",
          "form": "오늘 먹어본다고 하지않았어m",
          "original_form": "오늘 먹어본다고 하지않았어m",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.3",
          "form": "나는 집 앞에 개인카페를 좋아해",
          "original_form": "나는 집 앞에 개인카페를 좋아해",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.4",
          "form": "좌석은 불편하지만 맛있어",
          "original_form": "좌석은 불편하지만 맛있어",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.5",
          "form": "오늘은 아니야",
          "original_form": "오늘은 아니야",
          "speaker_id": "1",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.6",
          "form": "맛있는데가 최고야",
          "original_form": "맛있는데가 최고야",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.7",
          "form": "비가 안오면 한번 가볼께",
          "original_form": "비가 안오면 한번 가볼께",
          "speaker_id": "1",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.8",
          "form": "스타벅스는 나의 신념과는 좀 맞지않아",
          "original_form": "스타벅스는 나의 신념과는 좀 맞지않아",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.9",
          "form": "나는 감성카페 투어를 좋아해",
          "original_form": "나는 감성카페 투어를 좋아해",
          "speaker_id": "1",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.10",
          "form": "메뉴 몇개 빼고는 그저그래",
          "original_form": "메뉴 몇개 빼고는 그저그래",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.11",
          "form": "나도 좋아!",
          "original_form": "나도 좋아!",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.12",
          "form": "다음에 같이 가자",
          "original_form": "다음에 같이 가자",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.13",
          "form": "스타벅스는 정말 시간떼우러가는거같아",
          "original_form": "스타벅스는 정말 시간떼우러가는거같아",
          "speaker_id": "1",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.14",
          "form": "요즘엔 역시 감성이 중요해",
          "original_form": "요즘엔 역시 감성이 중요해",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037409.1.15",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037409.1.16",
          "form": "선물하기에도 무난무난해",
          "original_form": "선물하기에도 무난무난해",
          "speaker_id": "2",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037409.1.17",
          "form": "다음에 앞산 카페를 가보자",
          "original_form": "다음에 앞산 카페를 가보자",
          "speaker_id": "1",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037409.1.18",
          "form": "항상 가보고싶은데 너무 멀어서 엄두가 안나",
          "original_form": "항상 가보고싶은데 너무 멀어서 엄두가 안나",
          "speaker_id": "1",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037409.1.19",
          "form": "앞산 멀지만 좋아",
          "original_form": "앞산 멀지만 좋아",
          "speaker_id": "2",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037409.1.20",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "2",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037409.1.21",
          "form": "다음에 우리집에서 출발해보자",
          "original_form": "다음에 우리집에서 출발해보자",
          "speaker_id": "1",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037409.1.22",
          "form": "그럼 조금 가깝지않을까?",
          "original_form": "그럼 조금 가깝지않을까?",
          "speaker_id": "1",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037409.1.23",
          "form": "아 그렇겠다. 좋은 생각이야",
          "original_form": "아 그렇겠다. 좋은 생각이야",
          "speaker_id": "2",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037409.1.24",
          "form": "아 니가 우리집에오면 내가 빽다방을 대접할께",
          "original_form": "아 니가 우리집에오면 내가 빽다방을 대접할께",
          "speaker_id": "1",
          "time": "20210831 23:25"
        }
      ]
    }
