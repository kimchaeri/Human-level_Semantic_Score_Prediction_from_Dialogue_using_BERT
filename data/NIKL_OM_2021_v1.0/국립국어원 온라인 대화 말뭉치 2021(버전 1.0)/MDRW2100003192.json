      "utterance": [
        {
          "id": "MDRW2100003192.1.1",
          "form": "크로플 먹어봤어?",
          "original_form": "크로플 먹어봤어?",
          "speaker_id": "2",
          "time": "20210820 20:22"
        },
        {
          "id": "MDRW2100003192.1.2",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210820 20:25"
        },
        {
          "id": "MDRW2100003192.1.3",
          "form": "그거 엄청 핫했잖아",
          "original_form": "그거 엄청 핫했잖아",
          "speaker_id": "1",
          "time": "20210820 20:25"
        },
        {
          "id": "MDRW2100003192.1.4",
          "form": "맞아맞아 난리였지",
          "original_form": "맞아맞아 난리였지",
          "speaker_id": "2",
          "time": "20210820 20:47"
        },
        {
          "id": "MDRW2100003192.1.5",
          "form": "나 크로와상 생지 사서 집에서 맨날 해먹었어",
          "original_form": "나 크로와상 생지 사서 집에서 맨날 해먹었어",
          "speaker_id": "1",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100003192.1.6",
          "form": "그거 만들기 어렵지 않아?",
          "original_form": "그거 만들기 어렵지 않아?",
          "speaker_id": "2",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100003192.1.7",
          "form": "쉬워",
          "original_form": "쉬워",
          "speaker_id": "1",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100003192.1.8",
          "form": "와플기계랑 생지만 있으면 돼",
          "original_form": "와플기계랑 생지만 있으면 돼",
          "speaker_id": "1",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100003192.1.9",
          "form": "오 전문가의 냄새",
          "original_form": "오 전문가의 냄새",
          "speaker_id": "2",
          "time": "20210820 20:49"
        },
        {
          "id": "MDRW2100003192.1.10",
          "form": "쉬워 엄청 ㅋㅋㅋ",
          "original_form": "쉬워 엄청 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:49"
        },
        {
          "id": "MDRW2100003192.1.11",
          "form": "크로플 아이스크림이랑 같이 먹으면 맛있는데",
          "original_form": "크로플 아이스크림이랑 같이 먹으면 맛있는데",
          "speaker_id": "2",
          "time": "20210820 20:49"
        },
        {
          "id": "MDRW2100003192.1.12",
          "form": "맞아 맛잘알!",
          "original_form": "맞아 맛잘알!",
          "speaker_id": "1",
          "time": "20210820 20:49"
        },
        {
          "id": "MDRW2100003192.1.13",
          "form": "먹고 싶다 ㅋㅋㅋ",
          "original_form": "먹고 싶다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100003192.1.14",
          "form": "크로넛은 알아?",
          "original_form": "크로넛은 알아?",
          "speaker_id": "1",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100003192.1.15",
          "form": "그게 뭐야 크로와상이랑 도넛인가",
          "original_form": "그게 뭐야 크로와상이랑 도넛인가",
          "speaker_id": "2",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100003192.1.16",
          "form": "응응 맞아",
          "original_form": "응응 맞아",
          "speaker_id": "1",
