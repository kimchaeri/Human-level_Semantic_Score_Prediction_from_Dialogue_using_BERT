      "utterance": [
        {
          "id": "MDRW2100037128.1.1",
          "form": "맛집 좋아하세요?",
          "original_form": "맛집 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100037128.1.2",
          "form": "네 좋아해요 !!",
          "original_form": "네 좋아해요 !!",
          "speaker_id": "1",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100037128.1.3",
          "form": "오옹 맛집 찾아다니시는편이세요?",
          "original_form": "오옹 맛집 찾아다니시는편이세요?",
          "speaker_id": "2",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037128.1.4",
          "form": "요즘은 못가지만 원래 종종 갔어요 ㅎㅎㅎ",
          "original_form": "요즘은 못가지만 원래 종종 갔어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:31"
        },
        {
          "id": "MDRW2100037128.1.5",
          "form": "요즘 못가셔서 아쉬우시겠어요ㅜㅜㅠ",
          "original_form": "요즘 못가셔서 아쉬우시겠어요ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100037128.1.6",
          "form": "네네 ㅜㅜ 좋아하는 쭈꾸미집이 있는데",
          "original_form": "네네 ㅜㅜ 좋아하는 쭈꾸미집이 있는데",
          "speaker_id": "1",
          "time": "20210831 20:38"
        },
        {
          "id": "MDRW2100037128.1.7",
          "form": "가고싶어요 ㅠㅠ",
          "original_form": "가고싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 20:38"
        },
        {
          "id": "MDRW2100037128.1.8",
          "form": "아아..ㅠㅠ 그건 가주셔야하는데ㅠ",
          "original_form": "아아..ㅠㅠ 그건 가주셔야하는데ㅠ",
          "speaker_id": "2",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100037128.1.9",
          "form": "제가 다 아쉽네요",
          "original_form": "제가 다 아쉽네요",
          "speaker_id": "2",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100037128.1.10",
          "form": "맞죠.. ㅜㅜ",
          "original_form": "맞죠.. ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037128.1.11",
          "form": "거긴 또 택배도 안해서",
          "original_form": "거긴 또 택배도 안해서",
          "speaker_id": "1",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037128.1.12",
          "form": "먹을 수 있는 방법이 없어요 ㅋㅋ ㅜㅜ",
          "original_form": "먹을 수 있는 방법이 없어요 ㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037128.1.13",
          "form": "좀 멀어서 포장도 힘들고..",
          "original_form": "좀 멀어서 포장도 힘들고..",
          "speaker_id": "1",
          "time": "20210831 20:43"
        },
        {
          "id": "MDRW2100037128.1.14",
          "form": "어후 그리 소중한 장소를 가셔야하는데ㅠ",
          "original_form": "어후 그리 소중한 장소를 가셔야하는데ㅠ",
          "speaker_id": "2",
          "time": "20210831 20:45"
        },
        {
          "id": "MDRW2100037128.1.15",
          "form": "그런 맛집있으시다니 부럽습니닷",
          "original_form": "그런 맛집있으시다니 부럽습니닷",
          "speaker_id": "2",
          "time": "20210831 20:45"
        },
        {
          "id": "MDRW2100037128.1.16",
          "form": "맛집 좋아하는곳 있으세요??!",
          "original_form": "맛집 좋아하는곳 있으세요??!",
          "speaker_id": "1",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100037128.1.17",
          "form": "칼국수랑 수육이요!",
          "original_form": "칼국수랑 수육이요!",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100037128.1.18",
          "form": "유일무이한곳이에요ㅋㅋ",
          "original_form": "유일무이한곳이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100037128.1.19",
          "form": "헐 좋은 조합이죠 !!!",
          "original_form": "헐 좋은 조합이죠 !!!",
          "speaker_id": "1",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100037128.1.20",
          "form": "어딘지 궁금해지네요 ㅋㅋㅋ",
          "original_form": "어딘지 궁금해지네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100037128.1.21",
          "form": "유일무이한 곳이라니!",
          "original_form": "유일무이한 곳이라니!",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100037128.1.22",
          "form": "아는곳이 거기뿐이라..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아는곳이 거기뿐이라..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037128.1.23",
          "form": "거창하게 붙여봤어욥ㅋㅋㅋ",
          "original_form": "거창하게 붙여봤어욥ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037128.1.24",
          "form": "ㅋㅋㅋㅋ맛집이 있는게 어디에요 !!",
          "original_form": "ㅋㅋㅋㅋ맛집이 있는게 어디에요 !!",
