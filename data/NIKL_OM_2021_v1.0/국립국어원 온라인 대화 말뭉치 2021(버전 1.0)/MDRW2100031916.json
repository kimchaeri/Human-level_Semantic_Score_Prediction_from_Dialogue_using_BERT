      "utterance": [
        {
          "id": "MDRW2100031916.1.1",
          "form": "오오ㅎㅎ 일에대한 스트레스는 많이 있으세요?",
          "original_form": "오오ㅎㅎ 일에대한 스트레스는 많이 있으세요?",
          "speaker_id": "1",
          "time": "20210905 08:31"
        },
        {
          "id": "MDRW2100031916.1.2",
          "form": "많죠ㅋㅋ",
          "original_form": "많죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:32"
        },
        {
          "id": "MDRW2100031916.1.3",
          "form": "우왐ㅎㅎ",
          "original_form": "우왐ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:32"
        },
        {
          "id": "MDRW2100031916.1.4",
          "form": "없는 사람 있을까요ㅜ",
          "original_form": "없는 사람 있을까요ㅜ",
          "speaker_id": "2",
          "time": "20210905 08:32"
        },
        {
          "id": "MDRW2100031916.1.5",
          "form": "상사가 스트레스 많이주세요?",
          "original_form": "상사가 스트레스 많이주세요?",
          "speaker_id": "1",
          "time": "20210905 08:32"
        },
        {
          "id": "MDRW2100031916.1.6",
          "form": "아뇨 제 할일만 잘하면ㅋㅋ",
          "original_form": "아뇨 제 할일만 잘하면ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:32"
        },
        {
          "id": "MDRW2100031916.1.7",
          "form": "회사다니시죠?",
          "original_form": "회사다니시죠?",
          "speaker_id": "2",
          "time": "20210905 08:33"
        },
        {
          "id": "MDRW2100031916.1.8",
          "form": "그렇죠ㅎㅎ",
          "original_form": "그렇죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:33"
        },
        {
          "id": "MDRW2100031916.1.9",
          "form": "한 가정의 아버지죠ㅎ",
          "original_form": "한 가정의 아버지죠ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:33"
        },
        {
          "id": "MDRW2100031916.1.10",
          "form": "확 때려치고 싶은데",
          "original_form": "확 때려치고 싶은데",
          "speaker_id": "1",
          "time": "20210905 08:33"
        },
        {
          "id": "MDRW2100031916.1.11",
          "form": "그냥 다녀야하는 처지?",
          "original_form": "그냥 다녀야하는 처지?",
          "speaker_id": "1",
          "time": "20210905 08:33"
        },
        {
          "id": "MDRW2100031916.1.12",
          "form": "아 기혼이시군요!",
          "original_form": "아 기혼이시군요!",
          "speaker_id": "2",
          "time": "20210905 08:33"
        },
        {
          "id": "MDRW2100031916.1.13",
          "form": "그럼 진짜 힘들거같아요",
          "original_form": "그럼 진짜 힘들거같아요",
          "speaker_id": "2",
          "time": "20210905 08:34"
        },
        {
          "id": "MDRW2100031916.1.14",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:34"
        },
        {
          "id": "MDRW2100031916.1.15",
          "form": "name2님은 아직?",
          "original_form": "&name2&님은 아직?",
          "speaker_id": "1",
          "time": "20210905 08:34"
        },
        {
          "id": "MDRW2100031916.1.16",
          "form": "생계를 책임지려면",
          "original_form": "생계를 책임지려면",
          "speaker_id": "2",
          "time": "20210905 08:34"
        },
        {
          "id": "MDRW2100031916.1.17",
          "form": "아무래도 그런것같애요ㅎ",
          "original_form": "아무래도 그런것같애요ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:34"
        },
        {
          "id": "MDRW2100031916.1.18",
          "form": "넵ㅋㅋ 전 미혼 벌어서 제가 쓰고\r\n부모님 드리고ㅋㅋ",
          "original_form": "넵ㅋㅋ 전 미혼 벌어서 제가 쓰고\r\n부모님 드리고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:35"
        },
        {
          "id": "MDRW2100031916.1.19",
          "form": "스트레스받아도 해야하는것ㅠ",
          "original_form": "스트레스받아도 해야하는것ㅠ",
          "speaker_id": "1",
          "time": "20210905 08:35"
        },
        {
          "id": "MDRW2100031916.1.20",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 08:35"
        }
      ]
