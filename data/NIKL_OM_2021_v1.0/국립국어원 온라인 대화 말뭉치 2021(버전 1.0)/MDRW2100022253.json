      "utterance": [
        {
          "id": "MDRW2100022253.1.1",
          "form": "빼빼로 좋아하세요?",
          "original_form": "빼빼로 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022253.1.2",
          "form": "빼빼로요? 싫어하지는 않아요 ㅎㅎ",
          "original_form": "빼빼로요? 싫어하지는 않아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022253.1.3",
          "form": "저는 아몬드 빼빼로랑 누드빼빼로 엄청 좋아해요",
          "original_form": "저는 아몬드 빼빼로랑 누드빼빼로 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022253.1.4",
          "form": "ㅎ달달하고 맛있죠",
          "original_form": "ㅎ달달하고 맛있죠",
          "speaker_id": "2",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022253.1.5",
          "form": "네 그리고 얼려서 먹으면 맛있어요",
          "original_form": "네 그리고 얼려서 먹으면 맛있어요",
          "speaker_id": "1",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022253.1.6",
          "form": "과잔데 얼려서먹나요?",
          "original_form": "과잔데 얼려서먹나요?",
          "speaker_id": "2",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022253.1.7",
          "form": "안에 초코가 단단해져서 맛있어요",
          "original_form": "안에 초코가 단단해져서 맛있어요",
          "speaker_id": "1",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022253.1.8",
          "form": "아 저도 초코파이 얼려서 먹는거 좋아해요",
          "original_form": "아 저도 초코파이 얼려서 먹는거 좋아해요",
          "speaker_id": "2",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022253.1.9",
          "form": "저도요! 그거랑 비슷해요 단단하게 얼린ㅋㅋ",
          "original_form": "저도요! 그거랑 비슷해요 단단하게 얼린ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022253.1.10",
          "form": "맞아요 요구르트랑 먹으면 궁합최고",
          "original_form": "맞아요 요구르트랑 먹으면 궁합최고",
          "speaker_id": "2",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022253.1.11",
          "form": "아 맞아요 요구르트랑 초코랑 잘어울려요",
          "original_form": "아 맞아요 요구르트랑 초코랑 잘어울려요",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022253.1.12",
          "form": "그쵸 맛있어요 오랜만에 먹고싶네요",
          "original_form": "그쵸 맛있어요 오랜만에 먹고싶네요",
          "speaker_id": "2",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022253.1.13",
          "form": "사실 저는 지금 누드 빼빼로 먹고있어요 ㅎㅎ",
          "original_form": "사실 저는 지금 누드 빼빼로 먹고있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022253.1.14",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
