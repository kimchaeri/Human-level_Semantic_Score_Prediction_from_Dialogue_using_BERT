      "utterance": [
        {
          "id": "MDRW2100037304.1.1",
          "form": "여행하면 생각나시는곳은 어디신가욥!?",
          "original_form": "여행하면 생각나시는곳은 어디신가욥!?",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037304.1.2",
          "form": "저는 아무래도 일본이 생각나요",
          "original_form": "저는 아무래도 일본이 생각나요",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037304.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037304.1.4",
          "form": "오 저두요!",
          "original_form": "오 저두요!",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100037304.1.5",
          "form": "국내는요?",
          "original_form": "국내는요?",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100037304.1.6",
          "form": "국내는 경주가생각나요",
          "original_form": "국내는 경주가생각나요",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037304.1.7",
          "form": "자주갔던곳이라서요",
          "original_form": "자주갔던곳이라서요",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037304.1.8",
          "form": "어디가 생각나세요?",
          "original_form": "어디가 생각나세요?",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037304.1.9",
          "form": "저는 부산이요!",
          "original_form": "저는 부산이요!",
          "speaker_id": "2",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100037304.1.10",
          "form": "부산갔을때 좋았어서 그런지 막 생각나네요",
          "original_form": "부산갔을때 좋았어서 그런지 막 생각나네요",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037304.1.11",
          "form": "다들 부산 관광지로 좋아하시더라구요 저는 부산사람이라... 그느낌을 모르겠어요",
          "original_form": "다들 부산 관광지로 좋아하시더라구요 저는 부산사람이라... 그느낌을 모르겠어요",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037304.1.12",
          "form": "앗.. 부산사심 그러실거같아요!",
          "original_form": "앗.. 부산사심 그러실거같아요!",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037304.1.13",
          "form": "부산 사시면 그러실 것 같아요!",
          "original_form": "부산 사시면 그러실 것 같아요!",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037304.1.14",
          "form": "제가 부산 맛집들 추천해드렸던가요?",
          "original_form": "제가 부산 맛집들 추천해드렸던가요?",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037304.1.15",
          "form": "아뇹!",
          "original_form": "아뇹!",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037304.1.16",
          "form": "추천해주시면 감사드립니다!",
          "original_form": "추천해주시면 감사드립니다!",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037304.1.17",
          "form": "보통 부산을 오시면 부산역으로 오시잖아요?",
          "original_form": "보통 부산을 오시면 부산역으로 오시잖아요?",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037304.1.18",
          "form": "부산역에 그집 이라는 곱도리탕 맛집이있어요",
          "original_form": "부산역에 그집 이라는 곱도리탕 맛집이있어요",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037304.1.19",
          "form": "오오 맛집이라니요",
          "original_form": "오오 맛집이라니요",
          "speaker_id": "2",
          "time": "20210831 23:30"
        },
        {
          "id": "MDRW2100037304.1.20",
          "form": "완전 현지맛집이네요!",
          "original_form": "완전 현지맛집이네요!",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037304.1.21",
          "form": "거기 가셨다가 가까운 남포동 가셔도 좋구요 흰여울길도 가셔도 좋아요",
          "original_form": "거기 가셨다가 가까운 남포동 가셔도 좋구요 흰여울길도 가셔도 좋아요",
          "speaker_id": "1",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100037304.1.22",
          "form": "다음에 가게도면 그 코스로 가볼게요!",
          "original_form": "다음에 가게도면 그 코스로 가볼게요!",
          "speaker_id": "2",
          "time": "20210831 23:49"
        }
      ]
