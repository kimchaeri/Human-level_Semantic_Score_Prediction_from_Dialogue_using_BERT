      "utterance": [
        {
          "id": "MDRW2100005123.1.1",
          "form": "히히 게임 이야기 재밌네요 :>",
          "original_form": "히히 게임 이야기 재밌네요 :>",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005123.1.2",
          "form": "핸드폰 게임도 하세요?",
          "original_form": "핸드폰 게임도 하세요?",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005123.1.3",
          "form": "네네 근데 요즘은 거의",
          "original_form": "네네 근데 요즘은 거의",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005123.1.4",
          "form": "PC게임만 하는 것 같아요",
          "original_form": "PC게임만 하는 것 같아요",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005123.1.5",
          "form": "헉 그렇구나!!!!!!",
          "original_form": "헉 그렇구나!!!!!!",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005123.1.6",
          "form": "전.. 피방을 못가서 핸펀게임 깔고이ㅛ거든요",
          "original_form": "전.. 피방을 못가서 핸펀게임 깔고이ㅛ거든요",
          "speaker_id": "2",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005123.1.7",
          "form": "저도 피방 못 가서 핸폰 게임 위주로 했는데",
          "original_form": "저도 피방 못 가서 핸폰 게임 위주로 했는데",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005123.1.8",
          "form": "노트북으로 게임 하면서 핸폰 게임 조금 안 하기 시작한 거 같아요",
          "original_form": "노트북으로 게임 하면서 핸폰 게임 조금 안 하기 시작한 거 같아요",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005123.1.9",
          "form": "휴 저도 노트북 바꾸면 이것저것 깔려고요 ㅎㅎ",
          "original_form": "휴 저도 노트북 바꾸면 이것저것 깔려고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005123.1.10",
          "form": "또 게임 뭐하세요?!",
          "original_form": "또 게임 뭐하세요?!",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005123.1.11",
          "form": "저 스팀게임 많이 해요!",
          "original_form": "저 스팀게임 많이 해요!",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005123.1.12",
          "form": "스타듀밸리 같은...",
          "original_form": "스타듀밸리 같은...",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005123.1.13",
          "form": "헐!@ 저 그거 해볼까 말까 고민하고 있었는데",
          "original_form": "헐!@ 저 그거 해볼까 말까 고민하고 있었는데",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005123.1.14",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005123.1.15",
          "form": "헐 너무 재밌어요...",
          "original_form": "헐 너무 재밌어요...",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005123.1.16",
          "form": "저 이거 고3 때 빠져서",
          "original_form": "저 이거 고3 때 빠져서",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005123.1.17",
          "form": "한달에 200시간 했었어요",
          "original_form": "한달에 200시간 했었어요",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005123.1.18",
          "form": "(그루면 안 됐는데...)",
          "original_form": "(그루면 안 됐는데...)",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005123.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005123.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 게임이 그렇죠 뭐... 저도 고삼때 야자 빼고 옵치하다가 잡혀가고 그랬어요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 게임이 그렇죠 뭐... 저도 고삼때 야자 빼고 옵치하다가 잡혀가고 그랬어요...",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005123.1.21",
          "form": "아앗... 스타듀밸리 막 설정도 귀엽게 바굴 수 있다던대",
          "original_form": "아앗... 스타듀밸리 막 설정도 귀엽게 바굴 수 있다던대",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005123.1.22",
          "form": "맞나요!?",
          "original_form": "맞나요!?",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005123.1.23",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005123.1.24",
          "form": "캐릭터나 잡초 디자인 같은 것도 다 바꿀 수 있어요",
          "original_form": "캐릭터나 잡초 디자인 같은 것도 다 바꿀 수 있어요",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005123.1.25",
          "form": "그래서 그림 잘 그리는 분들이 올려두신 파일로",
          "original_form": "그래서 그림 잘 그리는 분들이 올려두신 파일로",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005123.1.26",
          "form": "덮어쓰기 해서 바꾸면",
          "original_form": "덮어쓰기 해서 바꾸면",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005123.1.27",
          "form": "잘생기고 예쁜 캐릭터들과 연애할 수 있어요...",
          "original_form": "잘생기고 예쁜 캐릭터들과 연애할 수 있어요...",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005123.1.28",
          "form": "헐.... 그런 꾸미는 거 좋아하는 저...",
          "original_form": "헐.... 그런 꾸미는 거 좋아하는 저...",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005123.1.29",
          "form": "벌써 드릉드릉 하네요...",
          "original_form": "벌써 드릉드릉 하네요...",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005123.1.30",
          "form": "아무래도.. 사야겠어요",
          "original_form": "아무래도.. 사야겠어요",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005123.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그럼 너무 취향에 맞으실 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그럼 너무 취향에 맞으실 것 같아요",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005123.1.32",
          "form": "스팀 세일할 때 구매하세요!!",
          "original_form": "스팀 세일할 때 구매하세요!!",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005123.1.33",
          "form": "저도 그래서 만원 안 되게 주고 구매했어요",
          "original_form": "저도 그래서 만원 안 되게 주고 구매했어요",
          "speaker_id": "1",
          "time": "20210821 21:15"
        }
