      "utterance": [
        {
          "id": "MDRW2100023601.1.1",
          "form": "안녕하세요, 공연 좋아하세요?",
          "original_form": "안녕하세요, 공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100023601.1.2",
          "form": "전 뮤지컬 좋아해요",
          "original_form": "전 뮤지컬 좋아해요",
          "speaker_id": "1",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100023601.1.3",
          "form": "아하 저는 뮤지컬 딱 두번봣어요",
          "original_form": "아하 저는 뮤지컬 딱 두번봣어요",
          "speaker_id": "2",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100023601.1.4",
          "form": "공연보단 영화덕후에요",
          "original_form": "공연보단 영화덕후에요",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023601.1.5",
          "form": "글쿤요. 전 요즘 영화는 걍 넷플로만 봐서",
          "original_form": "글쿤요. 전 요즘 영화는 걍 넷플로만 봐서",
          "speaker_id": "1",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100023601.1.6",
          "form": "영화관 안간지 오래됐어요",
          "original_form": "영화관 안간지 오래됐어요",
          "speaker_id": "1",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100023601.1.7",
          "form": "넷플왓챠도 엄청봐요",
          "original_form": "넷플왓챠도 엄청봐요",
          "speaker_id": "2",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100023601.1.8",
          "form": "뮤지컬 최애가뭔가요",
          "original_form": "뮤지컬 최애가뭔가요",
          "speaker_id": "2",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100023601.1.9",
          "form": "최애라면 배우요? 아님 극?",
          "original_form": "최애라면 배우요? 아님 극?",
          "speaker_id": "1",
          "time": "20210830 12:41"
        },
        {
          "id": "MDRW2100023601.1.10",
          "form": "둘다 궁금해요!!",
          "original_form": "둘다 궁금해요!!",
          "speaker_id": "2",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100023601.1.11",
          "form": "배우로는 남자-  류정한, 여자-  조정은,차지연 배우님이구요",
          "original_form": "배우로는 남자-  류정한, 여자-  조정은,차지연 배우님이구요",
          "speaker_id": "1",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100023601.1.12",
          "form": "극은 대극장-레베카,드라큘라 소극장- 키다리아저씨 예요 ㅎㅎ",
          "original_form": "극은 대극장-레베카,드라큘라 소극장- 키다리아저씨 예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:44"
        },
        {
          "id": "MDRW2100023601.1.13",
          "form": "우와 진정한 뮤덬이시네요 와",
          "original_form": "우와 진정한 뮤덬이시네요 와",
          "speaker_id": "2",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100023601.1.14",
          "form": "레베카 유툽에서 엄청봣어요ㅜㅜ",
          "original_form": "레베카 유툽에서 엄청봣어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100023601.1.15",
          "form": "실제로보면 소름돋을꺼같아요",
          "original_form": "실제로보면 소름돋을꺼같아요",
          "speaker_id": "2",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100023601.1.16",
          "form": "네, 진짜 쩔어요",
          "original_form": "네, 진짜 쩔어요",
          "speaker_id": "1",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100023601.1.17",
          "form": "레베카 꽤 흥한 편이라 언젠가 다시 올테고 한번쯤은 보세요",
          "original_form": "레베카 꽤 흥한 편이라 언젠가 다시 올테고 한번쯤은 보세요",
          "speaker_id": "1",
          "time": "20210830 12:48"
        },
        {
          "id": "MDRW2100023601.1.18",
          "form": "레베카는 갠적으로 옥주현배우님 짱입니다",
          "original_form": "레베카는 갠적으로 옥주현배우님 짱입니다",
          "speaker_id": "1",
          "time": "20210830 12:48"
        },
        {
          "id": "MDRW2100023601.1.19",
          "form": "그거 옥주현님 레베카 레전드라그래서 꼭 보고싶어요 ㅎ",
          "original_form": "그거 옥주현님 레베카 레전드라그래서 꼭 보고싶어요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 12:49"
        },
        {
          "id": "MDRW2100023601.1.20",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210830 12:49"
        },
        {
          "id": "MDRW2100023601.1.21",
          "form": "티켓팅도 치열하다그래서",
          "original_form": "티켓팅도 치열하다그래서",
          "speaker_id": "2",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100023601.1.22",
          "form": "기회온다면 꼭 보고싶어욤",
          "original_form": "기회온다면 꼭 보고싶어욤",
          "speaker_id": "2",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100023601.1.23",
          "form": "흑, 초연때는 옥주현님 표도 자리잡기 쉬웠는데",
          "original_form": "흑, 초연때는 옥주현님 표도 자리잡기 쉬웠는데",
          "speaker_id": "1",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100023601.1.24",
          "form": "지금은 너무 힘들어요ㅠㅠ",
          "original_form": "지금은 너무 힘들어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100023601.1.25",
          "form": "그쳐ㅜㅜ 빛의속도로 순삭된다고 마니들엇어욥",
          "original_form": "그쳐ㅜㅜ 빛의속도로 순삭된다고 마니들엇어욥",
          "speaker_id": "2",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100023601.1.26",
          "form": "좋은자리에서 보고싶어요 흑흑",
          "original_form": "좋은자리에서 보고싶어요 흑흑",
          "speaker_id": "2",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100023601.1.27",
          "form": "전 4연? 4연때 맞나? 여하튼 걍 vip석은 포기했어요",
          "original_form": "전 4연? 4연때 맞나? 여하튼 걍 vip석은 포기했어요",
          "speaker_id": "1",
          "time": "20210830 12:54"
        },
        {
          "id": "MDRW2100023601.1.28",
          "form": "그럼 어쨌든 잡을수는 있더라구요...ㅎ..",
          "original_form": "그럼 어쨌든 잡을수는 있더라구요...ㅎ..",
          "speaker_id": "1",
          "time": "20210830 12:54"
        },
        {
          "id": "MDRW2100023601.1.29",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210830 12:54"
        },
        {
          "id": "MDRW2100023601.1.30",
          "form": "아이비님 공연 봣엇는데",
          "original_form": "아이비님 공연 봣엇는데",
          "speaker_id": "2",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100023601.1.31",
          "form": "좋았었어요",
          "original_form": "좋았었어요",
          "speaker_id": "2",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100023601.1.32",
          "form": "코시국 끝나면 꼭 레베카 노려보겟슴다!!",
          "original_form": "코시국 끝나면 꼭 레베카 노려보겟슴다!!",
