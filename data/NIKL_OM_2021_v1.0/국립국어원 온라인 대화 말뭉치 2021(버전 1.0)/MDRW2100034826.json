      "utterance": [
        {
          "id": "MDRW2100034826.1.1",
          "form": "아ㅜㅠ",
          "original_form": "아ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100034826.1.2",
          "form": "그러셨구나",
          "original_form": "그러셨구나",
          "speaker_id": "2",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100034826.1.3",
          "form": "그 후 코로나로 콘서트가 더 멀어진 기분이에요",
          "original_form": "그 후 코로나로 콘서트가 더 멀어진 기분이에요",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034826.1.4",
          "form": "맞아요 그때 혼자라두 갔어야했는데 ㅜㅜ",
          "original_form": "맞아요 그때 혼자라두 갔어야했는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034826.1.5",
          "form": "이렇게 될줄 몰랐네요",
          "original_form": "이렇게 될줄 몰랐네요",
          "speaker_id": "1",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100034826.1.6",
          "form": "진짜요.. 상상도 못했어요",
          "original_form": "진짜요.. 상상도 못했어요",
          "speaker_id": "2",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100034826.1.7",
          "form": "요즘은 온라인으로도 하던데",
          "original_form": "요즘은 온라인으로도 하던데",
          "speaker_id": "1",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100034826.1.8",
          "form": "전 그 소리를 좋아하거든요 ㅠ",
          "original_form": "전 그 소리를 좋아하거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100034826.1.9",
          "form": "현장 가보셨으면 더 그러실거같아요",
          "original_form": "현장 가보셨으면 더 그러실거같아요",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034826.1.10",
          "form": "방청만해도 티비랑 다르다고들 하던데 더 궁금하네요",
          "original_form": "방청만해도 티비랑 다르다고들 하던데 더 궁금하네요",
          "speaker_id": "2",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100034826.1.11",
          "form": "달라요 진짜 ㅜㅜ 뮤지컬 같은건 보셨어요?",
          "original_form": "달라요 진짜 ㅜㅜ 뮤지컬 같은건 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100034826.1.12",
          "form": "아니요ㅠ 가본건 멜론이랄까요?ㅋㅋ",
          "original_form": "아니요ㅠ 가본건 멜론이랄까요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100034826.1.13",
          "form": "아 멜론 ! 시상식 말씀하시나요?",
          "original_form": "아 멜론 ! 시상식 말씀하시나요?",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100034826.1.14",
          "form": "넹!!",
          "original_form": "넹!!",
          "speaker_id": "2",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100034826.1.15",
          "form": "언제 가셨어요??!",
          "original_form": "언제 가셨어요??!",
          "speaker_id": "1",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100034826.1.16",
          "form": "7년쯤 전이였어요..ㅎㅎ",
          "original_form": "7년쯤 전이였어요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100034826.1.17",
          "form": "아이유가 너무 이뻤던 기억이 남네요",
          "original_form": "아이유가 너무 이뻤던 기억이 남네요",
          "speaker_id": "2",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100034826.1.18",
          "form": "오오 ! 부러워요 아이유라니 ㅎㅎ",
          "original_form": "오오 ! 부러워요 아이유라니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100034826.1.19",
          "form": "노래도 불렀었죠 ???",
          "original_form": "노래도 불렀었죠 ???",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100034826.1.20",
          "form": "네! 근데 노래부르는건 잘 안보이는 자리라 아쉬웠어요ㅠ",
          "original_form": "네! 근데 노래부르는건 잘 안보이는 자리라 아쉬웠어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100034826.1.21",
          "form": "아 ㅠㅠ 저도 시상식 가봤는데",
          "original_form": "아 ㅠㅠ 저도 시상식 가봤는데",
          "speaker_id": "1",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100034826.1.22",
          "form": "너무 밀쳐서 ㅋㅋ 기억이 안나요",
          "original_form": "너무 밀쳐서 ㅋㅋ 기억이 안나요",
          "speaker_id": "1",
          "time": "20210828 21:12"
