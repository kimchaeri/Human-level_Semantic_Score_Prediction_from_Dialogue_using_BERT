      "utterance": [
        {
          "id": "MDRW2100045104.1.1",
          "form": "음원사이트 성적에 연연할거 아니고",
          "original_form": "음원사이트 성적에 연연할거 아니고",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045104.1.2",
          "form": "찐으로 음악감상 목적이면",
          "original_form": "찐으로 음악감상 목적이면",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045104.1.3",
          "form": "유튭뮤직이ㅇㅇㅇㅇ...",
          "original_form": "유튭뮤직이ㅇㅇㅇㅇ...",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045104.1.4",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045104.1.5",
          "form": "저는 이제 사이트 ㅇㄴ돌리고",
          "original_form": "저는 이제 사이트 ㅇㄴ돌리고",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045104.1.6",
          "form": "그냥 다운만 받아여 총공할때",
          "original_form": "그냥 다운만 받아여 총공할때",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045104.1.7",
          "form": "이제는 나이먹어서",
          "original_form": "이제는 나이먹어서",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045104.1.8",
          "form": "힘쓰는거 힘들더라",
          "original_form": "힘쓰는거 힘들더라",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045104.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그리고 폰배터리아까워",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그리고 폰배터리아까워",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045104.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045104.1.11",
          "form": "나 말고 다른 팬들이",
          "original_form": "나 말고 다른 팬들이",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045104.1.12",
          "form": "해줄거야...",
          "original_form": "해줄거야...",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045104.1.13",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045104.1.14",
          "form": "ㅈ전 믿어여",
          "original_form": "ㅈ전 믿어여",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045104.1.15",
          "form": "저ㅏㄹ고",
          "original_form": "저ㅏㄹ고",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045104.1.16",
          "form": "많은 분들이있어여",
          "original_form": "많은 분들이있어여",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045104.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045104.1.18",
          "form": "이제 나이먹어서",
          "original_form": "이제 나이먹어서",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.19",
          "form": "그리고 노래 성적은 솔직히",
          "original_form": "그리고 노래 성적은 솔직히",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.20",
          "form": "스밍같은걸로 스트레스 받는건",
          "original_form": "스밍같은걸로 스트레스 받는건",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.21",
          "form": "방셉엔(맞나) 급 아니면",
          "original_form": "방셉엔(맞나) 급 아니면",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.22",
          "form": "이젠더이상 네이버",
          "original_form": "이젠더이상 네이버",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.24",
          "form": "팬들이 백날 돌리는 것보다",
          "original_form": "팬들이 백날 돌리는 것보다",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.25",
          "form": "아 그러고보니",
          "original_form": "아 그러고보니",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.26",
          "form": "친구",
          "original_form": "친구",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.27",
          "form": "신창친구",
          "original_form": "신창친구",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.28",
          "form": "새로운 덕질대상이",
          "original_form": "새로운 덕질대상이",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.29",
          "form": "엔시티더라구요..",
          "original_form": "엔시티더라구요..",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.30",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.31",
          "form": "티는 안내는ㄴ데",
          "original_form": "티는 안내는ㄴ데",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.32",
          "form": "주변사람들의 덕질이",
          "original_form": "주변사람들의 덕질이",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.33",
          "form": "쉽지않은 길을",
          "original_form": "쉽지않은 길을",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.34",
          "form": "가시는군요...",
          "original_form": "가시는군요...",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.35",
          "form": "다 제겐 가시밭길...",
          "original_form": "다 제겐 가시밭길...",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045104.1.37",
          "form": "뭐 그래도 떡밥은 많을테니...",
          "original_form": "뭐 그래도 떡밥은 많을테니...",
          "speaker_id": "2",
          "time": "20210908 00:55"
        }
      ]
