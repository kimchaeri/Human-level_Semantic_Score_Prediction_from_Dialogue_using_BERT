      "utterance": [
        {
          "id": "MDRW2100034072.1.1",
          "form": "국내여행 가보셨나요?",
          "original_form": "국내여행 가보셨나요?",
          "speaker_id": "2",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100034072.1.2",
          "form": "네 대구, 전주 , 경주 .. 안동 이런곳 가봤어요",
          "original_form": "네 대구, 전주 , 경주 .. 안동 이런곳 가봤어요",
          "speaker_id": "1",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100034072.1.3",
          "form": "와 많이 가셨네요",
          "original_form": "와 많이 가셨네요",
          "speaker_id": "2",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100034072.1.4",
          "form": "여행 좋아하시나봐요?",
          "original_form": "여행 좋아하시나봐요?",
          "speaker_id": "2",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100034072.1.5",
          "form": "최근에는 못갔지만 국내여행 일년에 한두번가는거 좋아했어요",
          "original_form": "최근에는 못갔지만 국내여행 일년에 한두번가는거 좋아했어요",
          "speaker_id": "1",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100034072.1.6",
          "form": "국내여행다니신곳 있나요?",
          "original_form": "국내여행다니신곳 있나요?",
          "speaker_id": "1",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100034072.1.7",
          "form": "춘천 정도밖에 기억이 안나네요",
          "original_form": "춘천 정도밖에 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100034072.1.8",
          "form": "춘천에서 좋았던것중 기억나는게 있나요?",
          "original_form": "춘천에서 좋았던것중 기억나는게 있나요?",
          "speaker_id": "1",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100034072.1.9",
          "form": "남이섬이 정말 예뻤어요",
          "original_form": "남이섬이 정말 예뻤어요",
          "speaker_id": "2",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100034072.1.10",
          "form": "배 타고 가는데 다른 세상에 놀러간 느낌이었어요 ㅎㅎ",
          "original_form": "배 타고 가는데 다른 세상에 놀러간 느낌이었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100034072.1.11",
          "form": "우와 저 가본적없는데 가고싶어졌어요",
          "original_form": "우와 저 가본적없는데 가고싶어졌어요",
          "speaker_id": "1",
          "time": "20210827 14:28"
        },
        {
          "id": "MDRW2100034072.1.12",
          "form": "정말 좋아요~ 아기자기한 가게들도 있고 큰 나무들이 많아서 예뻐요",
          "original_form": "정말 좋아요~ 아기자기한 가게들도 있고 큰 나무들이 많아서 예뻐요",
          "speaker_id": "2",
          "time": "20210827 14:34"
        },
        {
          "id": "MDRW2100034072.1.13",
          "form": "기념품 사오셨나요?",
          "original_form": "기념품 사오셨나요?",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100034072.1.14",
          "form": "아뇨 제가 기념품 사는 스타일이 아니라서요",
          "original_form": "아뇨 제가 기념품 사는 스타일이 아니라서요",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100034072.1.15",
          "form": "대신 사진은 많이 찍었어요",
          "original_form": "대신 사진은 많이 찍었어요",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100034072.1.16",
          "form": "그러시구나 다른곳 가보고싶은곳 있으세요?",
          "original_form": "그러시구나 다른곳 가보고싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100034072.1.17",
          "form": "한번 대전 가보고 싶어요",
          "original_form": "한번 대전 가보고 싶어요",
