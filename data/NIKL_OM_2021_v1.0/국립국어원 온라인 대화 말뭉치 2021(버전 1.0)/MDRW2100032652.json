      "utterance": [
        {
          "id": "MDRW2100032652.1.1",
          "form": "예능 어떤거 보세요",
          "original_form": "예능 어떤거 보세요",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032652.1.2",
          "form": "최근에 스우파 봤어여!",
          "original_form": "최근에 스우파 봤어여!",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032652.1.3",
          "form": "오오 춤츄는 예능이죠??",
          "original_form": "오오 춤츄는 예능이죠??",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032652.1.4",
          "form": "네네 여자댄서들 나와요 ㅋㅋ",
          "original_form": "네네 여자댄서들 나와요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032652.1.5",
          "form": "와 저 잠깐 봣는데 춤 완전 잘쳐옄ㅋㅋㅋㅋ",
          "original_form": "와 저 잠깐 봣는데 춤 완전 잘쳐옄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032652.1.6",
          "form": "3화까지 햇나요??",
          "original_form": "3화까지 햇나요??",
          "speaker_id": "1",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100032652.1.7",
          "form": "ㅋㅋㅋ아니요 2화까지했어요",
          "original_form": "ㅋㅋㅋ아니요 2화까지했어요",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100032652.1.8",
          "form": "3화는 화욜에 해요!",
          "original_form": "3화는 화욜에 해요!",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100032652.1.9",
          "form": "오오 금방 하겟네요 !!!",
          "original_form": "오오 금방 하겟네요 !!!",
          "speaker_id": "1",
          "time": "20210906 00:22"
        },
        {
          "id": "MDRW2100032652.1.10",
          "form": "어느팀 응원 하세요?",
          "original_form": "어느팀 응원 하세요?",
          "speaker_id": "1",
          "time": "20210906 00:22"
        },
        {
          "id": "MDRW2100032652.1.11",
          "form": "ㅋㅋㅋ저는 리정이랑 허니제이가 멋있더라구여!",
          "original_form": "ㅋㅋㅋ저는 리정이랑 허니제이가 멋있더라구여!",
          "speaker_id": "2",
          "time": "20210906 00:23"
        },
        {
          "id": "MDRW2100032652.1.12",
          "form": "그래서 그 팀들이 탈락안하구 이겼으면 좋겠어요",
          "original_form": "그래서 그 팀들이 탈락안하구 이겼으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210906 00:23"
        },
        {
          "id": "MDRW2100032652.1.13",
          "form": "오오 리정!!! 저는 첨에는 다 모르고 환불원정대 춤 만든",
          "original_form": "오오 리정!!! 저는 첨에는 다 모르고 환불원정대 춤 만든",
          "speaker_id": "1",
          "time": "20210906 11:53"
        },
        {
          "id": "MDRW2100032652.1.14",
          "form": "팀만 눈에 익드라구요!!",
          "original_form": "팀만 눈에 익드라구요!!",
          "speaker_id": "1",
          "time": "20210906 11:53"
        },
        {
          "id": "MDRW2100032652.1.15",
          "form": "오오 그렇죵ㅋㅋㅋ",
          "original_form": "오오 그렇죵ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100032652.1.16",
          "form": "다른 예능프로그램 자주 보시나여?",
          "original_form": "다른 예능프로그램 자주 보시나여?",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100032652.1.17",
          "form": "요즘 연애 프로 잘봐요 ㅋㅋ",
          "original_form": "요즘 연애 프로 잘봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100032652.1.18",
          "form": "환승연애나 돌싱글즈요 ㅎㅎ",
          "original_form": "환승연애나 돌싱글즈요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100032652.1.19",
          "form": "아 ㅋㅋㅋ제 친구도 그런거 자주 보더라구요",
          "original_form": "아 ㅋㅋㅋ제 친구도 그런거 자주 보더라구요",
          "speaker_id": "2",
