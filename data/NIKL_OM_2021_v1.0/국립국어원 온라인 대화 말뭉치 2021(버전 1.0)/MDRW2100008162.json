        },
        {
          "id": "MDRW2100008162.1.4",
          "form": "제발 날 찾지 말아줘",
          "original_form": "제발 날 찾지 말아줘",
          "speaker_id": "1",
          "time": "20210822 13:43"
        },
        {
          "id": "MDRW2100008162.1.5",
          "form": "여행지로 추천해줄 곳이 있나요",
          "original_form": "여행지로 추천해줄 곳이 있나요",
          "speaker_id": "1",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008162.1.6",
          "form": "여수가봤니",
          "original_form": "여수가봤니",
          "speaker_id": "2",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008162.1.7",
          "form": "여수밤바다",
          "original_form": "여수밤바다",
          "speaker_id": "2",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008162.1.8",
          "form": "아니 좋니",
          "original_form": "아니 좋니",
          "speaker_id": "1",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008162.1.9",
          "form": "좋으니 사랑해서",
          "original_form": "좋으니 사랑해서",
          "speaker_id": "2",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008162.1.10",
          "form": "사랑을 시작할때~~",
          "original_form": "사랑을 시작할때~~",
          "speaker_id": "2",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008162.1.11",
          "form": "정신차리세요",
          "original_form": "정신차리세요",
          "speaker_id": "1",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008162.1.12",
          "form": "여수좋야요",
