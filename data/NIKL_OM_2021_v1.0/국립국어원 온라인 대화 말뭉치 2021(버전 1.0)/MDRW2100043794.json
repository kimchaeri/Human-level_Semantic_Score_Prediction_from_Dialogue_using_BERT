      "utterance": [
        {
          "id": "MDRW2100043794.1.1",
          "form": "색 다양하고 쫄깃하고 뚱띠한..",
          "original_form": "색 다양하고 쫄깃하고 뚱띠한..",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043794.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋ적고 보니 웃긴 음식인데요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋ적고 보니 웃긴 음식인데요",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043794.1.3",
          "form": "그러네욬ㅋㅋ",
          "original_form": "그러네욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043794.1.4",
          "form": "디저트",
          "original_form": "디저트",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043794.1.5",
          "form": "이거 맞추면",
          "original_form": "이거 맞추면",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043794.1.6",
          "form": "아! 싶은건데",
          "original_form": "아! 싶은건데",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043794.1.7",
          "form": "유명하고 많이들 먹는",
          "original_form": "유명하고 많이들 먹는",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043794.1.8",
          "form": "디저트에요",
          "original_form": "디저트에요",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043794.1.9",
          "form": "흔해요!",
          "original_form": "흔해요!",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043794.1.10",
          "form": "으으으으으으으윽 뭐지 딱딱한 크림에 냉장보관...",
          "original_form": "으으으으으으으윽 뭐지 딱딱한 크림에 냉장보관...",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043794.1.11",
          "form": "크림이 없는 버전도 있는 음식인가요?",
          "original_form": "크림이 없는 버전도 있는 음식인가요?",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043794.1.12",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043794.1.13",
          "form": "이제 얼추 맞춰가시는것같은데....?",
          "original_form": "이제 얼추 맞춰가시는것같은데....?",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043794.1.14",
          "form": "악 뭐지 악악 악",
          "original_form": "악 뭐지 악악 악",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043794.1.15",
          "form": "정답! 도너츠?",
          "original_form": "정답! 도너츠?",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043794.1.16",
          "form": "악악악",
          "original_form": "악악악",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043794.1.17",
          "form": "아니에요",
          "original_form": "아니에요",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043794.1.18",
          "form": "아악 아악 뭐지",
          "original_form": "아악 아악 뭐지",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043794.1.19",
          "form": "저 힌트 하나만 더요!!!!!",
          "original_form": "저 힌트 하나만 더요!!!!!",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043794.1.20",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043794.1.21",
          "form": "오레오 딸기요거트 황치즈 말차초코",
          "original_form": "오레오 딸기요거트 황치즈 말차초코",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043794.1.22",
          "form": "등의 맛이 있답니다",
          "original_form": "등의 맛이 있답니다",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043794.1.23",
          "form": "으으으으으으으으으음 정답! 푸딩?!",
          "original_form": "으으으으으으으으으음 정답! 푸딩?!",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043794.1.24",
          "form": "땡입니다ㅠㅠ",
          "original_form": "땡입니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043794.1.25",
          "form": "제가 너무 어려운걸 낸걸까요",
          "original_form": "제가 너무 어려운걸 낸걸까요",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043794.1.26",
          "form": "ㅁㅋㄹ",
          "original_form": "ㅁㅋㄹ",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043794.1.27",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043794.1.28",
          "form": "ㅋㅋㅋㅋ아 진짜 들으면 아! 하네요",
          "original_form": "ㅋㅋㅋㅋ아 진짜 들으면 아! 하네요",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043794.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043794.1.30",
          "form": "짱 웃겨",
          "original_form": "짱 웃겨",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043794.1.31",
          "form": "바로 알겠죠?",
          "original_form": "바로 알겠죠?",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043794.1.32",
          "form": "제가할수있는 최대의 표현들이었ㅇㅓ요ㅋㅋ",
          "original_form": "제가할수있는 최대의 표현들이었ㅇㅓ요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043794.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 정답 알고 나서 힌트들을 주섬주섬 모아보니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 정답 알고 나서 힌트들을 주섬주섬 모아보니",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043794.1.34",
          "form": "맞는 말이었어요",
          "original_form": "맞는 말이었어요",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043794.1.35",
          "form": "이런 아쉽다",
          "original_form": "이런 아쉽다",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043794.1.36",
          "form": "한때 저의 최애 디저트였던 것이랍니다",
          "original_form": "한때 저의 최애 디저트였던 것이랍니다",
          "speaker_id": "1",
          "time": "20210907 20:53"
        }
      ]
    }
