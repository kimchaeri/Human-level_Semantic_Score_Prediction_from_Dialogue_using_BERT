      "utterance": [
        {
          "id": "MDRW2100002341.1.1",
          "form": "오늘 안바빠서 좋다",
          "original_form": "오늘 안바빠서 좋다",
          "speaker_id": "1",
          "time": "20210820 09:58"
        },
        {
          "id": "MDRW2100002341.1.2",
          "form": "나도 오늘 한가하네",
          "original_form": "나도 오늘 한가하네",
          "speaker_id": "2",
          "time": "20210820 10:01"
        },
        {
          "id": "MDRW2100002341.1.3",
          "form": "시청에서 연락이 안와 ㅋㅋㅋㅋ",
          "original_form": "시청에서 연락이 안와 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 10:01"
        },
        {
          "id": "MDRW2100002341.1.4",
          "form": "역시 금요일 좋아라",
          "original_form": "역시 금요일 좋아라",
          "speaker_id": "1",
          "time": "20210820 10:01"
        },
        {
          "id": "MDRW2100002341.1.5",
          "form": "행보크해",
          "original_form": "행보크해",
          "speaker_id": "1",
          "time": "20210820 10:02"
        },
        {
          "id": "MDRW2100002341.1.6",
          "form": "이번주까지 휴가철이라 그런가봐",
          "original_form": "이번주까지 휴가철이라 그런가봐",
          "speaker_id": "2",
          "time": "20210820 10:03"
        },
        {
          "id": "MDRW2100002341.1.7",
          "form": "나는 다음주 일요일휴가다",
          "original_form": "나는 다음주 일요일휴가다",
          "speaker_id": "1",
          "time": "20210820 10:04"
        },
        {
          "id": "MDRW2100002341.1.8",
          "form": "나는 시월달에 쓰려고",
          "original_form": "나는 시월달에 쓰려고",
          "speaker_id": "2",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002341.1.9",
          "form": "회사에서",
          "original_form": "회사에서",
          "speaker_id": "1",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002341.1.10",
          "form": "휴양시설 신청해서",
          "original_form": "휴양시설 신청해서",
          "speaker_id": "1",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002341.1.11",
          "form": "해운대 그랜드조선 당첨됐어",
          "original_form": "해운대 그랜드조선 당첨됐어",
          "speaker_id": "1",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002341.1.12",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002341.1.13",
          "form": "라운지가서",
          "original_form": "라운지가서",
          "speaker_id": "1",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002341.1.14",
          "form": "이름만 들아도 좋아보인다",
          "original_form": "이름만 들아도 좋아보인다",
          "speaker_id": "2",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002341.1.15",
          "form": "하루종일 공짜음식이랑",
          "original_form": "하루종일 공짜음식이랑",
          "speaker_id": "1",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002341.1.16",
          "form": "나는 10월에 영월 가보려고",
          "original_form": "나는 10월에 영월 가보려고",
          "speaker_id": "2",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002341.1.17",
          "form": "술먹어야지",
          "original_form": "술먹어야지",
          "speaker_id": "1",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002341.1.18",
          "form": "그런데는 다면 그냥 다 공짜야?",
          "original_form": "그런데는 다면 그냥 다 공짜야?",
          "speaker_id": "2",
          "time": "20210820 10:06"
        },
        {
          "id": "MDRW2100002341.1.19",
          "form": "아니  회사에서 스위트룸으로 주더라고",
          "original_form": "아니  회사에서 스위트룸으로 주더라고",
          "speaker_id": "1",
          "time": "20210820 10:06"
        },
        {
          "id": "MDRW2100002341.1.20",
          "form": "스위트룸 숙박객은 라운지 사용가능하대",
          "original_form": "스위트룸 숙박객은 라운지 사용가능하대",
          "speaker_id": "1",
          "time": "20210820 10:06"
        },
        {
          "id": "MDRW2100002341.1.21",
          "form": "아. 제일좋은 등급으로 하면 숙박비에 포함되어있나보네",
          "original_form": "아. 제일좋은 등급으로 하면 숙박비에 포함되어있나보네",
          "speaker_id": "2",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002341.1.22",
          "form": "좋당",
          "original_form": "좋당",
          "speaker_id": "2",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002341.1.23",
          "form": "사진많이찍어성 블로그에 올려줭",
          "original_form": "사진많이찍어성 블로그에 올려줭",
          "speaker_id": "2",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002341.1.24",
          "form": "궁금하다",
          "original_form": "궁금하다",
          "speaker_id": "2",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002341.1.25",
          "form": "응응 알겠숴",
          "original_form": "응응 알겠숴",
          "speaker_id": "1",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002341.1.26",
          "form": "공무원도 이런거 있잖아",
          "original_form": "공무원도 이런거 있잖아",
          "speaker_id": "1",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002341.1.27",
          "form": "근데 스윗룸은 안해줄듯...ㅋㅋㅋㅋ",
          "original_form": "근데 스윗룸은 안해줄듯...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 10:19"
        },
        {
          "id": "MDRW2100002341.1.28",
          "form": "울아부지도 작년에",
          "original_form": "울아부지도 작년에",
          "speaker_id": "1",
          "time": "20210820 10:23"
        },
        {
          "id": "MDRW2100002341.1.29",
          "form": "한번 부산에 호텔 예약하시던데",
          "original_form": "한번 부산에 호텔 예약하시던데",
          "speaker_id": "1",
          "time": "20210820 10:23"
        }
      ]
    }
