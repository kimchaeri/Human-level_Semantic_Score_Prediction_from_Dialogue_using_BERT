      "utterance": [
        {
          "id": "MDRW2100000731.1.1",
          "form": "안녕 실버",
          "original_form": "안녕 실버",
          "speaker_id": "1",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000731.1.2",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000731.1.3",
          "form": "안녕 난 게임은 메이플스토리에서 멈췄어",
          "original_form": "안녕 난 게임은 메이플스토리에서 멈췄어",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000731.1.4",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "1",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000731.1.5",
          "form": "무슨 게임하시는지?",
          "original_form": "무슨 게임하시는지?",
          "speaker_id": "2",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000731.1.6",
          "form": "나두 메이플 하는데",
          "original_form": "나두 메이플 하는데",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000731.1.7",
          "form": "나는 많이해",
          "original_form": "나는 많이해",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000731.1.8",
          "form": "메이플 롤 배그 오버워치 스타크래프트",
          "original_form": "메이플 롤 배그 오버워치 스타크래프트",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000731.1.9",
          "form": "요즘은 롤이랑 메이플 스타크래프트",
          "original_form": "요즘은 롤이랑 메이플 스타크래프트",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000731.1.10",
          "form": "하스스톤!",
          "original_form": "하스스톤!",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000731.1.11",
          "form": "뭔지 다 알아? ㅋㅋㅋ",
          "original_form": "뭔지 다 알아? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000731.1.12",
          "form": "그걸 다하면 너무바쁘겠다",
          "original_form": "그걸 다하면 너무바쁘겠다",
          "speaker_id": "2",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000731.1.13",
          "form": "이름은 다 들어봤지",
          "original_form": "이름은 다 들어봤지",
          "speaker_id": "2",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000731.1.14",
          "form": "해본적은 없슈",
          "original_form": "해본적은 없슈",
          "speaker_id": "2",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000731.1.15",
          "form": "바쁘지..",
          "original_form": "바쁘지..",
          "speaker_id": "1",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000731.1.16",
          "form": "핸드폰 게임은 함!",
          "original_form": "핸드폰 게임은 함!",
          "speaker_id": "2",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000731.1.17",
          "form": "시간 날때만 하거든",
          "original_form": "시간 날때만 하거든",
          "speaker_id": "1",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000731.1.18",
          "form": "오 휴대폰게임?",
          "original_form": "오 휴대폰게임?",
          "speaker_id": "1",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000731.1.19",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "1",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000731.1.20",
          "form": "나도 알려주라",
          "original_form": "나도 알려주라",
          "speaker_id": "1",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000731.1.21",
          "form": "디즈니팝이요..^^",
          "original_form": "디즈니팝이요..^^",
          "speaker_id": "2",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000731.1.22",
          "form": "카트라이더?",
          "original_form": "카트라이더?",
          "speaker_id": "2",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000731.1.23",
          "form": "카트??",
          "original_form": "카트??",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000731.1.24",
          "form": "러쉬플러스",
          "original_form": "러쉬플러스",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000731.1.25",
          "form": "나도 했는데!!",
          "original_form": "나도 했는데!!",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000731.1.26",
          "form": "재밌쥬",
          "original_form": "재밌쥬",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000731.1.27",
          "form": "요새 게임 제일 인기 많은건 뭘까?",
          "original_form": "요새 게임 제일 인기 많은건 뭘까?",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000731.1.28",
          "form": "당근 롤이지!",
          "original_form": "당근 롤이지!",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000731.1.29",
          "form": "리그오브레전드!!",
          "original_form": "리그오브레전드!!",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000731.1.30",
          "form": "피시방 점유율 1위!",
          "original_form": "피시방 점유율 1위!",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000731.1.31",
          "form": "몇년째 지속중이니",
          "original_form": "몇년째 지속중이니",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000731.1.32",
          "form": "신기하다 롤이 계속 1위라니",
          "original_form": "신기하다 롤이 계속 1위라니",
          "speaker_id": "2",
          "time": "20210819 20:03"
        },
        {
          "id": "MDRW2100000731.1.33",
          "form": "드럼 프로게이머들도 다 롤 하는건가?",
          "original_form": "드럼 프로게이머들도 다 롤 하는건가?",
          "speaker_id": "2",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000731.1.34",
          "form": "올림픽도..???",
          "original_form": "올림픽도..???",
          "speaker_id": "2",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000731.1.35",
          "form": "당근이죠",
          "original_form": "당근이죠",
          "speaker_id": "1",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000731.1.36",
          "form": "롤드컵! 이라고 하니",
          "original_form": "롤드컵! 이라고 하니",
          "speaker_id": "1",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000731.1.37",
          "form": "지금으로써는 롤이 가장 인기있는거 같아요",
          "original_form": "지금으로써는 롤이 가장 인기있는거 같아요",
          "speaker_id": "1",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000731.1.38",
          "form": "게임하는 남자들은 롤 한번쯤은 해봤거게요",
          "original_form": "게임하는 남자들은 롤 한번쯤은 해봤거게요",
          "speaker_id": "1",
          "time": "20210819 20:05"
        }
      ]
    }
