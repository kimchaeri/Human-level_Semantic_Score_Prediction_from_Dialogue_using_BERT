      "utterance": [
        {
          "id": "MDRW2100039580.1.1",
          "form": "헝가리 도시야경이 좋았어??",
          "original_form": "헝가리 도시야경이 좋았어??",
          "speaker_id": "2",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100039580.1.2",
          "form": "네 그때 야경 배타고 봤었거든요",
          "original_form": "네 그때 야경 배타고 봤었거든요",
          "speaker_id": "1",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100039580.1.3",
          "form": "와 지금 아파트 관리사무소 안내방송에서",
          "original_form": "와 지금 아파트 관리사무소 안내방송에서",
          "speaker_id": "2",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100039580.1.4",
          "form": "우리나라 한강 유람선처럼 그런거 있어용",
          "original_form": "우리나라 한강 유람선처럼 그런거 있어용",
          "speaker_id": "1",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100039580.1.5",
          "form": "코로나 안내방송 나와.....",
          "original_form": "코로나 안내방송 나와.....",
          "speaker_id": "2",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100039580.1.6",
          "form": "우와 배타고...라니!",
          "original_form": "우와 배타고...라니!",
          "speaker_id": "2",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039580.1.7",
          "form": "으악 코로나 언제끝나요ㅠㅠ 여행가기는 커녕 집콕만 하고 있어요",
          "original_form": "으악 코로나 언제끝나요ㅠㅠ 여행가기는 커녕 집콕만 하고 있어요",
          "speaker_id": "1",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039580.1.8",
          "form": "좋았어요 그 헝가리 건물들에 불 켜져있는데 반짝반짝했어요",
          "original_form": "좋았어요 그 헝가리 건물들에 불 켜져있는데 반짝반짝했어요",
          "speaker_id": "1",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039580.1.9",
          "form": "진짜 예뻤겠다ㅠㅠ 물결이랑 은은하게 비쳐서ㅠㅠ",
          "original_form": "진짜 예뻤겠다ㅠㅠ 물결이랑 은은하게 비쳐서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039580.1.10",
          "form": "네ㅠㅜㅠ 제일 기억에 남아요",
          "original_form": "네ㅠㅜㅠ 제일 기억에 남아요",
          "speaker_id": "1",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039580.1.11",
          "form": "이야기만 듣고 상상하는데도 너무 좋다ㅠㅠㅠ",
          "original_form": "이야기만 듣고 상상하는데도 너무 좋다ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039580.1.12",
          "form": "계절은 언제쯤이었어?",
          "original_form": "계절은 언제쯤이었어?",
          "speaker_id": "2",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039580.1.13",
          "form": "여행이 진짜 힐링인거 같아요",
          "original_form": "여행이 진짜 힐링인거 같아요",
          "speaker_id": "1",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039580.1.14",
          "form": "그때 딱 이맘때 즈음이었어요!!",
          "original_form": "그때 딱 이맘때 즈음이었어요!!",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039580.1.15",
          "form": "맞아... 일단 지금 있는 곳을 떠나는 것만으로도",
          "original_form": "맞아... 일단 지금 있는 곳을 떠나는 것만으로도",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039580.1.16",
          "form": "힐링인 것 같아",
          "original_form": "힐링인 것 같아",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039580.1.17",
          "form": "또 가고 싶네요",
          "original_form": "또 가고 싶네요",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039580.1.18",
          "form": "지금 계절 즈음의 여행은 진짜",
          "original_form": "지금 계절 즈음의 여행은 진짜",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039580.1.19",
          "form": "떠나고 싶어요~ㅠㅠ",
          "original_form": "떠나고 싶어요~ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039580.1.20",
          "form": "최고인것같아ㅠㅠ",
          "original_form": "최고인것같아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039580.1.21",
          "form": "나도ㅠㅠ흑흑 언제쯤 갈 수 있을까",
          "original_form": "나도ㅠㅠ흑흑 언제쯤 갈 수 있을까",
          "speaker_id": "2",
          "time": "20210903 19:19"
        }
      ]
