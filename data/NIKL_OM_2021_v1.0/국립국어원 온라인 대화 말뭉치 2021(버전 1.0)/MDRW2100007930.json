      "utterance": [
        {
          "id": "MDRW2100007930.1.1",
          "form": "중고거래해보셨어요?",
          "original_form": "중고거래해보셨어요?",
          "speaker_id": "1",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007930.1.2",
          "form": "당근이요",
          "original_form": "당근이요",
          "speaker_id": "2",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007930.1.3",
          "form": "주로 뭐거래해요?",
          "original_form": "주로 뭐거래해요?",
          "speaker_id": "1",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007930.1.4",
          "form": "공기청정기요 최근엔",
          "original_form": "공기청정기요 최근엔",
          "speaker_id": "2",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007930.1.5",
          "form": "비싼거 거래하셨네요 판매? 구매?",
          "original_form": "비싼거 거래하셨네요 판매? 구매?",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007930.1.6",
          "form": "차량용이라 작아요",
          "original_form": "차량용이라 작아요",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007930.1.7",
          "form": "판매만요",
          "original_form": "판매만요",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007930.1.8",
          "form": "아 차량용이군요 그런건 얼마해요?",
          "original_form": "아 차량용이군요 그런건 얼마해요?",
          "speaker_id": "1",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007930.1.9",
          "form": "이만에 새거 팔았죠",
          "original_form": "이만에 새거 팔았죠",
          "speaker_id": "2",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007930.1.10",
          "form": "정가 얼만데요?",
          "original_form": "정가 얼만데요?",
          "speaker_id": "1",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007930.1.11",
          "form": "오만 얼마요",
          "original_form": "오만 얼마요",
          "speaker_id": "2",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007930.1.12",
          "form": "싸게 넘기셨네요ㅎ 직접돈주고 사신건 아닌가보군요",
          "original_form": "싸게 넘기셨네요ㅎ 직접돈주고 사신건 아닌가보군요",
          "speaker_id": "1",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007930.1.13",
          "form": "샀는데 더 좋은 거 선물받아서",
          "original_form": "샀는데 더 좋은 거 선물받아서",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007930.1.14",
          "form": "아 그럼 뭐 파셔도 되겠네요",
          "original_form": "아 그럼 뭐 파셔도 되겠네요",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007930.1.15",
          "form": "사보신적은  없으세요? 판매만?",
          "original_form": "사보신적은  없으세요? 판매만?",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007930.1.16",
          "form": "네 판매만",
          "original_form": "네 판매만",
          "speaker_id": "2",
          "time": "20210822 12:26"
        }
      ]
