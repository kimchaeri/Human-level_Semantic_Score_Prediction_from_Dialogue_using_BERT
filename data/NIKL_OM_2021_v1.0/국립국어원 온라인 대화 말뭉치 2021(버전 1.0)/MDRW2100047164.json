      "utterance": [
        {
          "id": "MDRW2100047164.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 선생님은 같은 프로보고 실친이 덕친되신 좋은 케이스...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 선생님은 같은 프로보고 실친이 덕친되신 좋은 케이스...",
          "speaker_id": "2",
          "time": "20210901 15:21"
        },
        {
          "id": "MDRW2100047164.1.2",
          "form": "ㅋㅋㄱㅋ뻐렁치는건",
          "original_form": "ㅋㅋㄱㅋ뻐렁치는건",
          "speaker_id": "1",
          "time": "20210901 15:21"
        },
        {
          "id": "MDRW2100047164.1.3",
          "form": "혼자서 트위터에",
          "original_form": "혼자서 트위터에",
          "speaker_id": "1",
          "time": "20210901 15:21"
        },
        {
          "id": "MDRW2100047164.1.4",
          "form": "ㅋㄱㅋㅋㅋㄱㅋㅋ그쵸 정말",
          "original_form": "ㅋㄱㅋㅋㅋㄱㅋㅋ그쵸 정말",
          "speaker_id": "1",
          "time": "20210901 15:21"
        },
        {
          "id": "MDRW2100047164.1.5",
          "form": "그 뮤 같이보던친구도",
          "original_form": "그 뮤 같이보던친구도",
          "speaker_id": "1",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100047164.1.6",
          "form": "저거같이보고 입덕해서",
          "original_form": "저거같이보고 입덕해서",
          "speaker_id": "1",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100047164.1.7",
          "form": "지금그친구도 흥분중",
          "original_form": "지금그친구도 흥분중",
          "speaker_id": "1",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100047164.1.8",
          "form": "류님컴백보곸ㅋㅋㅋㅋㅋ",
          "original_form": "류님컴백보곸ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100047164.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이번에 같이 보러 가는거야???",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이번에 같이 보러 가는거야???",
          "speaker_id": "2",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100047164.1.10",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100047164.1.11",
          "form": "간만에 뭉치나여",
          "original_form": "간만에 뭉치나여",
          "speaker_id": "2",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100047164.1.12",
          "form": "저번에 류님라만차때도",
          "original_form": "저번에 류님라만차때도",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100047164.1.13",
          "form": "역시 휴덕한 오타쿠를 부를 수 있는건 최애 뿐",
          "original_form": "역시 휴덕한 오타쿠를 부를 수 있는건 최애 뿐",
          "speaker_id": "2",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100047164.1.14",
          "form": "보러가고 내가 꼬셔서 뀨드윅도 보러감",
          "original_form": "보러가고 내가 꼬셔서 뀨드윅도 보러감",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100047164.1.15",
          "form": "뀨보더니",
          "original_form": "뀨보더니",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100047164.1.16",
          "form": "마라맛이좋다며 조는안땡긴다는 친구..",
          "original_form": "마라맛이좋다며 조는안땡긴다는 친구..",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100047164.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:24"
        },
        {
          "id": "MDRW2100047164.1.18",
          "form": "그럼그럼 유명해도 결국 내 취향 맞는게 짱이지",
          "original_form": "그럼그럼 유명해도 결국 내 취향 맞는게 짱이지",
          "speaker_id": "2",
          "time": "20210901 15:25"
        },
        {
          "id": "MDRW2100047164.1.19",
          "form": "근데 조도 마라맛일줄ㅈ알앗는데",
          "original_form": "근데 조도 마라맛일줄ㅈ알앗는데",
          "speaker_id": "2",
          "time": "20210901 15:25"
        },
        {
          "id": "MDRW2100047164.1.20",
          "form": "아닌거 의외다",
          "original_form": "아닌거 의외다",
          "speaker_id": "2",
          "time": "20210901 15:25"
        },
        {
          "id": "MDRW2100047164.1.21",
          "form": "ㅋㅋㄱㅋㅋㅋ 조언니는 사랑쟁이야",
          "original_form": "ㅋㅋㄱㅋㅋㅋ 조언니는 사랑쟁이야",
          "speaker_id": "1",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100047164.1.22",
          "form": "제가보니까",
          "original_form": "제가보니까",
          "speaker_id": "1",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100047164.1.23",
          "form": "캐해가진짜",
          "original_form": "캐해가진짜",
          "speaker_id": "1",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100047164.1.24",
          "form": "되게착한 헤드윅이야",
          "original_form": "되게착한 헤드윅이야",
          "speaker_id": "1",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100047164.1.25",
          "form": ".....헤드윅이 착할 수 있어?",
          "original_form": ".....헤드윅이 착할 수 있어?",
          "speaker_id": "2",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100047164.1.26",
          "form": "ㅇㅇㅇ 영화나 뮤 보셨나여???",
          "original_form": "ㅇㅇㅇ 영화나 뮤 보셨나여???",
          "speaker_id": "1",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100047164.1.27",
          "form": "그 이츠학이 드랙퀸이잖아",
          "original_form": "그 이츠학이 드랙퀸이잖아",
          "speaker_id": "1",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100047164.1.28",
          "form": "영화 대충 봄",
          "original_form": "영화 대충 봄",
          "speaker_id": "2",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100047164.1.29",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100047164.1.30",
          "form": "드랙퀸이지만 헤드윅하고 결혼해서 미국으로 탈출하는대신",
          "original_form": "드랙퀸이지만 헤드윅하고 결혼해서 미국으로 탈출하는대신",
          "speaker_id": "1",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100047164.1.31",
          "form": "남자로만살아야하거든",
          "original_form": "남자로만살아야하거든",
          "speaker_id": "1",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100047164.1.32",
          "form": "그게 헤드윅이 자기가 미국으로올때 성전환수술한것처럼",
          "original_form": "그게 헤드윅이 자기가 미국으로올때 성전환수술한것처럼",
          "speaker_id": "1",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100047164.1.33",
          "form": "자유에는 희생이따르는거라고 똑같이",
          "original_form": "자유에는 희생이따르는거라고 똑같이",
          "speaker_id": "1",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100047164.1.34",
          "form": "하게하는건데",
          "original_form": "하게하는건데",
          "speaker_id": "1",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100047164.1.35",
          "form": "조는 나같은 인생은 쟤도 이렇게 살게할수없다고",
          "original_form": "조는 나같은 인생은 쟤도 이렇게 살게할수없다고",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100047164.1.36",
          "form": "난이츠학을아끼니까",
          "original_form": "난이츠학을아끼니까",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100047164.1.37",
          "form": "이런캐해라 ㅇㅇ",
          "original_form": "이런캐해라 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100047164.1.38",
          "form": "읽기도전에 넘겨버렸나여 ㅋㅋㅋㄱㅋ",
          "original_form": "읽기도전에 넘겨버렸나여 ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
