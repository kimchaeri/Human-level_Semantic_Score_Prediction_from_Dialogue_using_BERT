      "utterance": [
        {
          "id": "MDRW2100034296.1.1",
          "form": "오 반려동물 기르시나봐여?",
          "original_form": "오 반려동물 기르시나봐여?",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100034296.1.2",
          "form": "아뇨 ㅎㅎ 기르시나요?",
          "original_form": "아뇨 ㅎㅎ 기르시나요?",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100034296.1.3",
          "form": "전 소소하게 구피기른답니다~",
          "original_form": "전 소소하게 구피기른답니다~",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034296.1.4",
          "form": "오 구피면 열대어 같은건가요",
          "original_form": "오 구피면 열대어 같은건가요",
          "speaker_id": "2",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100034296.1.5",
          "form": "음 잘모르겠어요 ㅋㅋ 그냥 생명력 좋아서 지금까지기르고잇는것뿐!",
          "original_form": "음 잘모르겠어요 ㅋㅋ 그냥 생명력 좋아서 지금까지기르고잇는것뿐!",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100034296.1.6",
          "form": "몇년되셨어요?",
          "original_form": "몇년되셨어요?",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034296.1.7",
          "form": "오근데 첨엔 어떻게 기르게되셨어요?",
          "original_form": "오근데 첨엔 어떻게 기르게되셨어요?",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100034296.1.8",
          "form": "한 6년된거같아요 ㅋㅋㅋ 고닥교때 친구한테 분양받아서 어거지로 키우게되엇어요 ㅋㅋㅋ",
          "original_form": "한 6년된거같아요 ㅋㅋㅋ 고닥교때 친구한테 분양받아서 어거지로 키우게되엇어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:50"
        },
        {
          "id": "MDRW2100034296.1.9",
          "form": "와 몇마리인지 다 아세요? ㅋㅋ",
          "original_form": "와 몇마리인지 다 아세요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100034296.1.10",
          "form": "알도 많이 낳고 저도 분양많이해서 기억안나요.. 암튼 엄청많다는것만 ㅠ",
          "original_form": "알도 많이 낳고 저도 분양많이해서 기억안나요.. 암튼 엄청많다는것만 ㅠ",
          "speaker_id": "1",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100034296.1.11",
          "form": "아하 생각보다 되게 쑥쑥 잘 크나봐요? ㅎㅎ",
          "original_form": "아하 생각보다 되게 쑥쑥 잘 크나봐요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100034296.1.12",
          "form": "맞아요 ㅋㅋㅋㅋ 너무잘커서 탈이에요",
          "original_form": "맞아요 ㅋㅋㅋㅋ 너무잘커서 탈이에요",
          "speaker_id": "1",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100034296.1.13",
          "form": "그래도 뭔가 가만히 수족관 보고있으면 평화로운게 좀있을거같아요",
          "original_form": "그래도 뭔가 가만히 수족관 보고있으면 평화로운게 좀있을거같아요",
          "speaker_id": "2",
          "time": "20210830 15:24"
        },
        {
          "id": "MDRW2100034296.1.14",
          "form": "아 ㅋㅋㅋ 근데 키운지 오래되다보니 이젠 그런것두 잘 없네요 ㅠ",
