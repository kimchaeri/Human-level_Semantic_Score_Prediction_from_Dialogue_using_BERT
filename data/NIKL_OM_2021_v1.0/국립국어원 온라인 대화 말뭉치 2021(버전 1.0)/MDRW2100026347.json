      "utterance": [
        {
          "id": "MDRW2100026347.1.1",
          "form": "네! 2도 챙겨봤죠~",
          "original_form": "네! 2도 챙겨봤죠~",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026347.1.2",
          "form": "엘사 너무 좋아요 ㅋㅋㅋ",
          "original_form": "엘사 너무 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026347.1.3",
          "form": "2도 너무 재밌었어요 ㅋㅋ 맞아요 넘 예쁘고 다부지고!!",
          "original_form": "2도 너무 재밌었어요 ㅋㅋ 맞아요 넘 예쁘고 다부지고!!",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026347.1.4",
          "form": "저 2에서 그 눈사람이(ㅋㅋ이름까먹음)",
          "original_form": "저 2에서 그 눈사람이(ㅋㅋ이름까먹음)",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026347.1.5",
          "form": "영원히 변하지않는것은 어떤게 있을까~ 하면서 노래부르잖아요",
          "original_form": "영원히 변하지않는것은 어떤게 있을까~ 하면서 노래부르잖아요",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026347.1.6",
          "form": "그때 완전 펑펑 울었어요",
          "original_form": "그때 완전 펑펑 울었어요",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026347.1.7",
          "form": "올라프요?ㅋㅋㅋㅋㅋㅋ",
          "original_form": "올라프요?ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026347.1.8",
          "form": "당근 코 한 눈사람!",
          "original_form": "당근 코 한 눈사람!",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026347.1.9",
          "form": "헐마자요 올라프!!!ㅋㅋㅋㅋ",
          "original_form": "헐마자요 올라프!!!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026347.1.10",
          "form": "앜ㅋㅋㅋ 올라프 죽으면 안돼~",
          "original_form": "앜ㅋㅋㅋ 올라프 죽으면 안돼~",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026347.1.11",
          "form": "저 약간 픽사애니들에서 해맑게 인생에 대한 이야기하는거?",
          "original_form": "저 약간 픽사애니들에서 해맑게 인생에 대한 이야기하는거?",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026347.1.12",
          "form": "엘사의 힘으로 살아나서 다행이에요 ㅋㅋ",
          "original_form": "엘사의 힘으로 살아나서 다행이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026347.1.13",
          "form": "그런게 너무 감동적이라서 ㅋㅋㅋㅋㅋ",
          "original_form": "그런게 너무 감동적이라서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026347.1.14",
          "form": "저 소울도 재밌었어요 코코랑 ㅋㅋ",
          "original_form": "저 소울도 재밌었어요 코코랑 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026347.1.15",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026347.1.16",
          "form": "헐 당연하죠 !! 너무 재밌었어요!!",
          "original_form": "헐 당연하죠 !! 너무 재밌었어요!!",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026347.1.17",
          "form": "어떻게 그런 상상을 하는지 모르겠어요",
          "original_form": "어떻게 그런 상상을 하는지 모르겠어요",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026347.1.18",
          "form": "그쵸ㅠㅠ 아 다시 보고 싶어졌어요",
          "original_form": "그쵸ㅠㅠ 아 다시 보고 싶어졌어요",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026347.1.19",
          "form": "너무 명작이에요...",
          "original_form": "너무 명작이에요...",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026347.1.20",
          "form": "진짜요ㅠ 디즈니 열리면 바로 가입해서 또 봅시다!! ㅋㅋㅋ",
          "original_form": "진짜요ㅠ 디즈니 열리면 바로 가입해서 또 봅시다!! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026347.1.21",
          "form": "저는 마블시리즈도 좋아하는데 그것들도 디즈니 플랫폼에 들어온다고 하더라구요 ㅋ",
          "original_form": "저는 마블시리즈도 좋아하는데 그것들도 디즈니 플랫폼에 들어온다고 하더라구요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026347.1.22",
          "form": "우와 대박이네요",
          "original_form": "우와 대박이네요",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026347.1.23",
          "form": "유튜브는 안보세요?",
          "original_form": "유튜브는 안보세요?",
