        {
          "id": "MDRW2100019356.1.1",
          "form": "name2님은 집안일 하시나용",
          "original_form": "&name2&님은 집안일 하시나용",
          "speaker_id": "1",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019356.1.2",
          "form": "저는 빨래 제가 자주 하는 편이에요",
          "original_form": "저는 빨래 제가 자주 하는 편이에요",
          "speaker_id": "2",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019356.1.3",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019356.1.4",
          "form": "저는 거의 모든 집안일을 다 하고 있어요 ㅎㅎㅎ",
          "original_form": "저는 거의 모든 집안일을 다 하고 있어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019356.1.5",
          "form": "아 정말요??",
          "original_form": "아 정말요??",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019356.1.6",
          "form": "자취하시나봐요",
          "original_form": "자취하시나봐요",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019356.1.7",
          "form": "저는 본가 살거든요",
          "original_form": "저는 본가 살거든요",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019356.1.8",
          "form": "부모님께 얹혀살아요! 그래서 제가 다 하는거에요ㅋㅋㅋㅋㅋ",
          "original_form": "부모님께 얹혀살아요! 그래서 제가 다 하는거에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019356.1.9",
          "form": "얹혀사니까요...!",
          "original_form": "얹혀사니까요...!",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019356.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 저도 얹혀사는거죠 뭐ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저도 얹혀사는거죠 뭐ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100019356.1.11",
          "form": "그럼 집안일 팁같은거 있나요??",
          "original_form": "그럼 집안일 팁같은거 있나요??",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019356.1.12",
          "form": "그런거슨 없슴니다....그냥 후딱 해치우능게 그나마 제일 좋은거 아시죠",
          "original_form": "그런거슨 없슴니다....그냥 후딱 해치우능게 그나마 제일 좋은거 아시죠",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019356.1.13",
          "form": "ㅋㅋㅋㅋㅋ역시 그렇군요..ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ역시 그렇군요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019356.1.14",
          "form": "진짜 미루면 미룰수록 더 귀찮아지더라구요",
          "original_form": "진짜 미루면 미룰수록 더 귀찮아지더라구요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019356.1.15",
          "form": "생각날 때 후딱 해치우는게 상책인가봐요",
          "original_form": "생각날 때 후딱 해치우는게 상책인가봐요",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019356.1.16",
          "form": "마자요.. 그래서 저는 오늘도 청소 안하고 설거지도 한시간 전에 했답니다 ㅎㅎㅎㅎㅎ",
          "original_form": "마자요.. 그래서 저는 오늘도 청소 안하고 설거지도 한시간 전에 했답니다 ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019356.1.17",
          "form": "name2님은 어떤 집안일이 제일 싫으세요?",
          "original_form": "&name2&님은 어떤 집안일이 제일 싫으세요?",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019356.1.18",
          "form": "오오ㅋㅋㅋㅋ 부지런하시네요",
          "original_form": "오오ㅋㅋㅋㅋ 부지런하시네요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019356.1.19",
          "form": "저는 설거지가 제일 싫어요",
          "original_form": "저는 설거지가 제일 싫어요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019356.1.20",
          "form": "물이 다 튀어서 짜증나더라구요",
          "original_form": "물이 다 튀어서 짜증나더라구요",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019356.1.21",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019356.1.22",
          "form": "헐 ㅈ저도설거지가 제일 싫어요",
          "original_form": "헐 ㅈ저도설거지가 제일 싫어요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019356.1.23",
          "form": "저도 배 다 젖어요",
          "original_form": "저도 배 다 젖어요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019356.1.24",
          "form": "근데 저는 배나와서 그런것 같아요",
          "original_form": "근데 저는 배나와서 그런것 같아요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019356.1.25",
          "form": "그쵸!! 저만 그런거 아니죠!!",
          "original_form": "그쵸!! 저만 그런거 아니죠!!",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019356.1.26",
          "form": "진짜 배 젖는거 너무 짜증나요",
          "original_form": "진짜 배 젖는거 너무 짜증나요",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019356.1.27",
          "form": "저도 배가 나와서 그런지ㅋㅋㅋㅋ",
          "original_form": "저도 배가 나와서 그런지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100019356.1.28",
          "form": "집안일 하다보면 시가 완전 빨리가지않아요??",
          "original_form": "집안일 하다보면 시가 완전 빨리가지않아요??",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100019356.1.29",
          "form": "방청소만 해도 시간이 후딱 거있더라구요",
          "original_form": "방청소만 해도 시간이 후딱 거있더라구요",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100019356.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ 이놈에 배가 문제 였나ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 이놈에 배가 문제 였나ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100019356.1.31",
          "form": "맞아요 시간 빨리가요",
          "original_form": "맞아요 시간 빨리가요",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100019356.1.32",
          "form": "근데 또 한거는 별로 없고",
          "original_form": "근데 또 한거는 별로 없고",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100019356.1.33",
          "form": "힘들어요 ㅠ",
          "original_form": "힘들어요 ㅠ",
