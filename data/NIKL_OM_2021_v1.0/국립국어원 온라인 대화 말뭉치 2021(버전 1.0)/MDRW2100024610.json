      "utterance": [
        {
          "id": "MDRW2100024610.1.1",
          "form": "네 라멘먹었어요",
          "original_form": "네 라멘먹었어요",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100024610.1.2",
          "form": "바로 앞에 복어지리라멘집 있어서",
          "original_form": "바로 앞에 복어지리라멘집 있어서",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100024610.1.3",
          "form": "전 덕복희",
          "original_form": "전 덕복희",
          "speaker_id": "1",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100024610.1.4",
          "form": "복어지리라멘이요...?",
          "original_form": "복어지리라멘이요...?",
          "speaker_id": "1",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100024610.1.5",
          "form": "완전맛있는데",
          "original_form": "완전맛있는데",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100024610.1.6",
          "form": "웬만하면 복어 지리 따로하던데...!",
          "original_form": "웬만하면 복어 지리 따로하던데...!",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100024610.1.7",
          "form": "첨들어봐요 복어지리 라멘",
          "original_form": "첨들어봐요 복어지리 라멘",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100024610.1.8",
          "form": "국물이 끝내주나요",
          "original_form": "국물이 끝내주나요",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100024610.1.9",
          "form": "전 돈코츠라멘만 먹어요",
          "original_form": "전 돈코츠라멘만 먹어요",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100024610.1.10",
          "form": "ㅋㅋㅋㄲㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄲㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100024610.1.11",
          "form": "뭐여...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭐여...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100024610.1.12",
          "form": "복어지리라멘먹은줄알앗네여",
          "original_form": "복어지리라멘먹은줄알앗네여",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100024610.1.13",
          "form": "어우 전 복어 싫어해서",
          "original_form": "어우 전 복어 싫어해서",
          "speaker_id": "2",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100024610.1.14",
          "form": "누님은 뭐드셨나요",
          "original_form": "누님은 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100024610.1.15",
          "form": "튀순?",
          "original_form": "튀순?",
          "speaker_id": "2",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100024610.1.16",
          "form": "복희랑 튀순이?",
          "original_form": "복희랑 튀순이?",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100024610.1.17",
          "form": "복희만 먹었어여ㅋ",
          "original_form": "복희만 먹었어여ㅋ",
          "speaker_id": "1",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100024610.1.18",
          "form": "전 복희는 밥이 안된다고 생각합니다",
          "original_form": "전 복희는 밥이 안된다고 생각합니다",
          "speaker_id": "2",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100024610.1.19",
          "form": "그건 간식이지",
          "original_form": "그건 간식이지",
          "speaker_id": "2",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100024610.1.20",
          "form": "ㅋㅋㅋㅋㅋ라면 넣어도요?",
          "original_form": "ㅋㅋㅋㅋㅋ라면 넣어도요?",
          "speaker_id": "1",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100024610.1.21",
          "form": "밥을 넣는게 아니면 간식이죠",
          "original_form": "밥을 넣는게 아니면 간식이죠",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024610.1.22",
          "form": "차돌덕복희도...?",
          "original_form": "차돌덕복희도...?",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100024610.1.23",
          "form": "고기들어가도 밥이 안들어가면 간식인가요!",
          "original_form": "고기들어가도 밥이 안들어가면 간식인가요!",
          "speaker_id": "1",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100024610.1.24",
          "form": "차돌복희라고 말을 하셨어야죠",
          "original_form": "차돌복희라고 말을 하셨어야죠",
          "speaker_id": "2",
          "time": "20210831 19:29"
        },
        {
          "id": "MDRW2100024610.1.25",
          "form": "귀하신몸인데 왜 귀하신분 이름을 빼셨어요",
          "original_form": "귀하신몸인데 왜 귀하신분 이름을 빼셨어요",
          "speaker_id": "2",
          "time": "20210831 19:29"
        },
        {
          "id": "MDRW2100024610.1.26",
          "form": "ㅋㅈㅋㅋㅋㅋㅋ차돌복희님은 다른가요",
          "original_form": "ㅋㅈㅋㅋㅋㅋㅋ차돌복희님은 다른가요",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024610.1.27",
          "form": "고귀한 성을 가지고있잖아요",
          "original_form": "고귀한 성을 가지고있잖아요",
          "speaker_id": "2",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100024610.1.28",
          "form": "차돌이라는 성은 어디붙어도 좋죠",
          "original_form": "차돌이라는 성은 어디붙어도 좋죠",
          "speaker_id": "2",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100024610.1.29",
          "form": "차돌짬뽕 차돌된장찌개",
          "original_form": "차돌짬뽕 차돌된장찌개",
          "speaker_id": "2",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100024610.1.30",
          "form": "촤돌",
          "original_form": "촤돌",
          "speaker_id": "1",
          "time": "20210831 19:56"
        }
      ]
    }
