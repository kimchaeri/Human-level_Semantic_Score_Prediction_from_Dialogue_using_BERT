      "utterance": [
        {
          "id": "MDRW2100026516.1.1",
          "form": "오늘 거기는 비 왔나요???",
          "original_form": "오늘 거기는 비 왔나요???",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026516.1.2",
          "form": "오전에 많이 오더니 오후엔 쨍쨍하더라구요",
          "original_form": "오전에 많이 오더니 오후엔 쨍쨍하더라구요",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026516.1.3",
          "form": "무슨 계절을 제일 좋아해요?",
          "original_form": "무슨 계절을 제일 좋아해요?",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026516.1.4",
          "form": "전 겨울요!",
          "original_form": "전 겨울요!",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026516.1.5",
          "form": "오 왜요??",
          "original_form": "오 왜요??",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026516.1.6",
          "form": "전 열이많아서요ㅜㅠ",
          "original_form": "전 열이많아서요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026516.1.7",
          "form": "겨울에 땀도안나구ㅋㅋ",
          "original_form": "겨울에 땀도안나구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026516.1.8",
          "form": "아 저는 정반대인데 ㅋㅋㅋ 겨울에 거의 시체에요.. 너무추위를 많이타가지고",
          "original_form": "아 저는 정반대인데 ㅋㅋㅋ 겨울에 거의 시체에요.. 너무추위를 많이타가지고",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026516.1.9",
          "form": "겨울이 생일이에요?ㅋㅋㅋㅋㅋ뜬금없지만",
          "original_form": "겨울이 생일이에요?ㅋㅋㅋㅋㅋ뜬금없지만",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026516.1.10",
          "form": "생일 완전 여름이에요",
          "original_form": "생일 완전 여름이에요",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026516.1.11",
          "form": "어제였어요ㅋㅋ",
          "original_form": "어제였어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026516.1.12",
          "form": "헙 대박",
          "original_form": "헙 대박",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026516.1.13",
          "form": "축하드려요!!",
          "original_form": "축하드려요!!",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026516.1.14",
          "form": "여름에태어나면",
          "original_form": "여름에태어나면",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026516.1.15",
          "form": "더위안탄다는 소문이있던데",
          "original_form": "더위안탄다는 소문이있던데",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026516.1.16",
          "form": "전 반대에요",
          "original_form": "전 반대에요",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026516.1.17",
          "form": "그러게요 ㅋㅋㅋ 어제 비많이왔는데",
          "original_form": "그러게요 ㅋㅋㅋ 어제 비많이왔는데",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026516.1.18",
          "form": "집에 계셨어요?",
          "original_form": "집에 계셨어요?",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026516.1.19",
          "form": "아 여기는 비가 왔었어요",
          "original_form": "아 여기는 비가 왔었어요",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026516.1.20",
          "form": "어제까진 출근했답니다ㅠㅠ방학동안 인턴해서요",
          "original_form": "어제까진 출근했답니다ㅠㅠ방학동안 인턴해서요",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026516.1.21",
          "form": "아하 수고하셨네요 생일에 맛난거 먹고 쉬어야하는디",
          "original_form": "아하 수고하셨네요 생일에 맛난거 먹고 쉬어야하는디",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026516.1.22",
          "form": "겨울엔 벌레가 덜 나와서 좋긴해요",
          "original_form": "겨울엔 벌레가 덜 나와서 좋긴해요",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026516.1.23",
          "form": "그래도 퇴근하고",
          "original_form": "그래도 퇴근하고",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026516.1.24",
          "form": "엽떡먹었답니다",
          "original_form": "엽떡먹었답니다",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026516.1.25",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026516.1.26",
          "form": "여름에 돈벌레...으",
          "original_form": "여름에 돈벌레...으",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026516.1.27",
          "form": "오 엽떡 ㅋㅋㅋ 맛나쥬",
          "original_form": "오 엽떡 ㅋㅋㅋ 맛나쥬",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026516.1.28",
          "form": "여름에 진짜 야생바퀴 극혐",
          "original_form": "여름에 진짜 야생바퀴 극혐",
          "speaker_id": "1",
