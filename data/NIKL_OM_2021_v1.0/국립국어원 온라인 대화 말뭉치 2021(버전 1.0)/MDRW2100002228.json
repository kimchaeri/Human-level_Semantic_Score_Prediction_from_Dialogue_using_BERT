      "utterance": [
        {
          "id": "MDRW2100002228.1.1",
          "form": "지금도 안피곤하세요?",
          "original_form": "지금도 안피곤하세요?",
          "speaker_id": "1",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002228.1.2",
          "form": "전 밤새 자주깨요",
          "original_form": "전 밤새 자주깨요",
          "speaker_id": "1",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002228.1.3",
          "form": "피곤은 한데 그냥 누워도 못자서요",
          "original_form": "피곤은 한데 그냥 누워도 못자서요",
          "speaker_id": "2",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002228.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002228.1.5",
          "form": "전 요즘 막달이라 매시간 화장실가요",
          "original_form": "전 요즘 막달이라 매시간 화장실가요",
          "speaker_id": "2",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002228.1.6",
          "form": "미칠지경이에요ㅎㅎ",
          "original_form": "미칠지경이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002228.1.7",
          "form": "전 지금도 화장실 너무 자주가요",
          "original_form": "전 지금도 화장실 너무 자주가요",
          "speaker_id": "1",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002228.1.8",
          "form": "애도 자꾸 뒤척이구요",
          "original_form": "애도 자꾸 뒤척이구요",
          "speaker_id": "2",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002228.1.9",
          "form": "그렇죠 그게 잠들만 하면 가고 해서 힘들어요",
          "original_form": "그렇죠 그게 잠들만 하면 가고 해서 힘들어요",
          "speaker_id": "1",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002228.1.10",
          "form": "3살전에 너무 뒤척여요",
          "original_form": "3살전에 너무 뒤척여요",
          "speaker_id": "1",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002228.1.11",
          "form": "엄마들 불침번이죠",
          "original_form": "엄마들 불침번이죠",
          "speaker_id": "1",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002228.1.12",
          "form": "3살이 지나면 좀 나아지나요?",
          "original_form": "3살이 지나면 좀 나아지나요?",
          "speaker_id": "2",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002228.1.13",
          "form": "네 나아 지더라구요",
          "original_form": "네 나아 지더라구요",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002228.1.14",
          "form": "첫째가 통잠도 빠르고 딥슬립이라 좋앗는데 돌지나면서 예민해지ㄷᆞㅣ라구요",
          "original_form": "첫째가 통잠도 빠르고 딥슬립이라 좋앗는데 돌지나면서 예민해지ㄷᆞㅣ라구요",
          "speaker_id": "2",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002228.1.15",
          "form": "엄마도 편해지고",
          "original_form": "엄마도 편해지고",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002228.1.16",
          "form": "그때 그래요",
          "original_form": "그때 그래요",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002228.1.17",
          "form": "그럼 또 그시간만 기다려야겟네요ㅠ",
          "original_form": "그럼 또 그시간만 기다려야겟네요ㅠ",
          "speaker_id": "2",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002228.1.18",
          "form": "세살때 완전 힘들게 또 안자요",
          "original_form": "세살때 완전 힘들게 또 안자요",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002228.1.19",
          "form": "그러다 좋아지죠",
          "original_form": "그러다 좋아지죠",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002228.1.20",
          "form": "앞으로 둘째도 있으니 시간이 또 지나야 겠네요",
          "original_form": "앞으로 둘째도 있으니 시간이 또 지나야 겠네요",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002228.1.21",
          "form": "계속 힘든거같은데요?ㅎㅎ",
          "original_form": "계속 힘든거같은데요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002228.1.22",
          "form": "ㅋㅋ그런가요",
          "original_form": "ㅋㅋ그런가요",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002228.1.23",
          "form": "그러게요ㅎㅎ휴 너무 빨리 가져서",
          "original_form": "그러게요ㅎㅎ휴 너무 빨리 가져서",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002228.1.24",
          "form": "4살부터 괜찮아요",
          "original_form": "4살부터 괜찮아요",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002228.1.25",
          "form": "희망이 안보여요ㅋ",
          "original_form": "희망이 안보여요ㅋ",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002228.1.26",
          "form": "둘다 빨리 끝날거예요 비슷하게",
          "original_form": "둘다 빨리 끝날거예요 비슷하게",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002228.1.27",
          "form": "둘째가 순하길 기도중이에요",
          "original_form": "둘째가 순하길 기도중이에요",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002228.1.28",
          "form": "첫째는 진짜 순햇어서 애기때..",
          "original_form": "첫째는 진짜 순햇어서 애기때..",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002228.1.29",
          "form": "첫째 둘째 성격 완전달라요",
          "original_form": "첫째 둘째 성격 완전달라요",
          "speaker_id": "1",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002228.1.30",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002228.1.31",
          "form": "저희도요",
          "original_form": "저희도요",
          "speaker_id": "1",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002228.1.32",
          "form": "둘째가 쫌 ㅋㅋ안그랬어요",
          "original_form": "둘째가 쫌 ㅋㅋ안그랬어요",
          "speaker_id": "1",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002228.1.33",
          "form": "발로키운다 소리들엇거든요",
          "original_form": "발로키운다 소리들엇거든요",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002228.1.34",
          "form": "근데..둘째는..",
          "original_form": "근데..둘째는..",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002228.1.35",
          "form": "세상에 전 그정도는 아니였는데",
          "original_form": "세상에 전 그정도는 아니였는데",
          "speaker_id": "1",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002228.1.36",
          "form": "반대긴한데 로또 당첨되듯 착할지도 몰라요",
          "original_form": "반대긴한데 로또 당첨되듯 착할지도 몰라요",
          "speaker_id": "1",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002228.1.37",
          "form": "첫째는 울지도 않아서 애키우는지 모르는짓도 잇엇어요",
          "original_form": "첫째는 울지도 않아서 애키우는지 모르는짓도 잇엇어요",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002228.1.38",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002228.1.39",
          "form": "전 울리지를 못해서",
          "original_form": "전 울리지를 못해서",
          "speaker_id": "1",
          "time": "20210820 01:01"
        },
        {
          "id": "MDRW2100002228.1.40",
          "form": "화장실에도 안고 갔어요 ㅋㅋ",
          "original_form": "화장실에도 안고 갔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:01"
        },
        {
          "id": "MDRW2100002228.1.41",
          "form": "아 고생이 정말ㅜ",
          "original_form": "아 고생이 정말ㅜ",
          "speaker_id": "2",
          "time": "20210820 01:01"
        },
        {
          "id": "MDRW2100002228.1.42",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:01"
        },
        {
          "id": "MDRW2100002228.1.43",
          "form": "성격이죠",
          "original_form": "성격이죠",
          "speaker_id": "1",
          "time": "20210820 01:01"
        }
      ]
    }
