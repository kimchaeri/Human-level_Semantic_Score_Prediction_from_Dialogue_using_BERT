          "speaker_id": "2",
          "time": "20210905 11:21"
        },
        {
          "id": "MDRW2100032000.1.3",
          "form": "사고싶은거 있으세요?",
          "original_form": "사고싶은거 있으세요?",
          "speaker_id": "1",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100032000.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100032000.1.5",
          "form": "추석선물고민중이에요",
          "original_form": "추석선물고민중이에요",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100032000.1.6",
          "form": "ㅋㅋㅋ어르신들용으로요?",
          "original_form": "ㅋㅋㅋ어르신들용으로요?",
          "speaker_id": "1",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100032000.1.7",
          "form": "아님 동료분들?",
          "original_form": "아님 동료분들?",
          "speaker_id": "1",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100032000.1.8",
          "form": "어른용이요",
          "original_form": "어른용이요",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100032000.1.9",
          "form": "아무래도 홍삼?",
          "original_form": "아무래도 홍삼?",
