      "utterance": [
        {
          "id": "MDRW2100023907.1.1",
          "form": "필라테스 오늘 처음하는거죠?",
          "original_form": "필라테스 오늘 처음하는거죠?",
          "speaker_id": "1",
          "time": "20210830 19:56"
        },
        {
          "id": "MDRW2100023907.1.2",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023907.1.3",
          "form": "완전 떨려요 그래서~!",
          "original_form": "완전 떨려요 그래서~!",
          "speaker_id": "2",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023907.1.4",
          "form": "저는 근육도 없고 유연성도 보통이라",
          "original_form": "저는 근육도 없고 유연성도 보통이라",
          "speaker_id": "2",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023907.1.5",
          "form": "첨엔 다 그렇죠",
          "original_form": "첨엔 다 그렇죠",
          "speaker_id": "1",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023907.1.6",
          "form": "필라테스를 시작하기로 했어욥,,.. 돈이 부담스러우니 일단 8대 1로 시작하고 있답니다 ㅎㅎㅎ",
          "original_form": "필라테스를 시작하기로 했어욥,,.. 돈이 부담스러우니 일단 8대 1로 시작하고 있답니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023907.1.7",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023907.1.8",
          "form": "하고 나서 후기 남겨줘요",
          "original_form": "하고 나서 후기 남겨줘요",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100023907.1.9",
          "form": "할만했다던가",
          "original_form": "할만했다던가",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100023907.1.10",
          "form": "죽을것같다던가",
          "original_form": "죽을것같다던가",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100023907.1.11",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100023907.1.12",
          "form": "네!!!!",
          "original_form": "네!!!!",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023907.1.13",
          "form": "필라테스가 생각보다 힘든 운동이어서",
          "original_form": "필라테스가 생각보다 힘든 운동이어서",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023907.1.14",
          "form": "첫 수업 때는 처음이라는 팔찌를 하고 참여해야한대요",
          "original_form": "첫 수업 때는 처음이라는 팔찌를 하고 참여해야한대요",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023907.1.15",
          "form": "호달달... 얼마나 힘들길래...",
          "original_form": "호달달... 얼마나 힘들길래...",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023907.1.16",
          "form": "ㅎㄷㄷ",
          "original_form": "ㅎㄷㄷ",
          "speaker_id": "1",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023907.1.17",
          "form": "너무 무섭다",
          "original_form": "너무 무섭다",
          "speaker_id": "1",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023907.1.18",
          "form": "어지간한 괴담보다 무서웠어요",
          "original_form": "어지간한 괴담보다 무서웠어요",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100023907.1.19",
          "form": "아진짜요?",
          "original_form": "아진짜요?",
          "speaker_id": "2",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100023907.1.20",
          "form": "하긴 저도",
          "original_form": "하긴 저도",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023907.1.21",
          "form": "설명 듣고 놀랐어요...",
          "original_form": "설명 듣고 놀랐어요...",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023907.1.22",
          "form": "그리고 컨디션 안좋을 때는 빨간 팔찌 하고 수업 듣는대요",
          "original_form": "그리고 컨디션 안좋을 때는 빨간 팔찌 하고 수업 듣는대요",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023907.1.23",
          "form": "오늘 저는 노란 팔찌 하고 수업 들어가겠죠,,",
          "original_form": "오늘 저는 노란 팔찌 하고 수업 들어가겠죠,,",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023907.1.24",
          "form": "와, 팔찌하는거 까먹으면 어케되는거야?",
          "original_form": "와, 팔찌하는거 까먹으면 어케되는거야?",
          "speaker_id": "1",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023907.1.25",
          "form": "무섭다 무서워",
          "original_form": "무섭다 무서워",
          "speaker_id": "1",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023907.1.26",
          "form": "언니...",
          "original_form": "언니...",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100023907.1.27",
          "form": "저 필라테스 하고",
          "original_form": "저 필라테스 하고",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100023907.1.28",
          "form": "진짜 죽는 줄 알았어요 힘들어서",
          "original_form": "진짜 죽는 줄 알았어요 힘들어서",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100023907.1.29",
          "form": "와 내일 학교에 기어갈지도 몰라요",
          "original_form": "와 내일 학교에 기어갈지도 몰라요",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100023907.1.30",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100023907.1.31",
          "form": "많이 힘들었어요?",
          "original_form": "많이 힘들었어요?",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100023907.1.32",
          "form": "네.... 진짜 근육이 파들파들 떨리고",
          "original_form": "네.... 진짜 근육이 파들파들 떨리고",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100023907.1.33",
          "form": "선생님은 엉덩이근육을 수축시키라고 하는데",
          "original_form": "선생님은 엉덩이근육을 수축시키라고 하는데",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100023907.1.34",
          "form": "ㅋㅋㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100023907.1.35",
          "form": "도대체 그걸 어떻게 하는지 모르겠고...",
          "original_form": "도대체 그걸 어떻게 하는지 모르겠고...",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023907.1.36",
          "form": "진짜 아파서",
          "original_form": "진짜 아파서",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023907.1.37",
          "form": "불가능한 동작이 있었는데",
          "original_form": "불가능한 동작이 있었는데",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023907.1.38",
          "form": "남들은 그걸 하더라고요...?",
          "original_form": "남들은 그걸 하더라고요...?",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023907.1.39",
          "form": "다들 그런말 하더라고요",
          "original_form": "다들 그런말 하더라고요",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023907.1.40",
          "form": "필라테스 진짜... 진짜 극악무도",
          "original_form": "필라테스 진짜... 진짜 극악무도",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023907.1.41",
          "form": "그리고 플랭크도 생전 안하다가 처음 해봤는데",
          "original_form": "그리고 플랭크도 생전 안하다가 처음 해봤는데",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023907.1.42",
          "form": "님도 한 두달뒤쯤엔 될거에요",
          "original_form": "님도 한 두달뒤쯤엔 될거에요",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023907.1.43",
          "form": "죽음을 경험했어요",
          "original_form": "죽음을 경험했어요",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023907.1.44",
          "form": "네,,,, 그러기를 바랄뿐이에요...",
          "original_form": "네,,,, 그러기를 바랄뿐이에요...",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023907.1.45",
          "form": "화이팅!",
          "original_form": "화이팅!",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100023907.1.46",
          "form": "푹 쉬세요",
          "original_form": "푹 쉬세요",
          "speaker_id": "1",
          "time": "20210830 22:46"
        }
      ]
    }
