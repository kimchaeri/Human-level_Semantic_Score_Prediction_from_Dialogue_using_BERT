      "utterance": [
        {
          "id": "MDRW2100018669.1.1",
          "form": "최근 판매하고 싶은 물품이 생겼는데 중고거래 하시나요?",
          "original_form": "최근 판매하고 싶은 물품이 생겼는데 중고거래 하시나요?",
          "speaker_id": "2",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018669.1.2",
          "form": "당근 해본적잇어요",
          "original_form": "당근 해본적잇어요",
          "speaker_id": "1",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100018669.1.3",
          "form": "근데 귀찮더라구요",
          "original_form": "근데 귀찮더라구요",
          "speaker_id": "1",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100018669.1.4",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100018669.1.5",
          "form": "아ㅎㅎ 맞아요 저도 지웠다가 방청소하면서 다시 깔았어요ㅋㅋ",
          "original_form": "아ㅎㅎ 맞아요 저도 지웠다가 방청소하면서 다시 깔았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100018669.1.6",
          "form": "최근쇼핑템은?",
          "original_form": "최근쇼핑템은?",
          "speaker_id": "1",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100018669.1.7",
          "form": "최근에는 고양이 용품이요!",
          "original_form": "최근에는 고양이 용품이요!",
          "speaker_id": "2",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100018669.1.8",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018669.1.9",
          "form": "집사님ㅋㅋ",
          "original_form": "집사님ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018669.1.10",
          "form": "저는 몇일전 가방 셀프선물햇어요",
          "original_form": "저는 몇일전 가방 셀프선물햇어요",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018669.1.11",
          "form": "오오 멋져요!",
          "original_form": "오오 멋져요!",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018669.1.12",
          "form": "스스로 챙기는게 진짜 필요하다 느끼고 있어요",
          "original_form": "스스로 챙기는게 진짜 필요하다 느끼고 있어요",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018669.1.13",
          "form": "좋아하는 브랜드는?",
          "original_form": "좋아하는 브랜드는?",
          "speaker_id": "1",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100018669.1.14",
          "form": "전 나이키!",
          "original_form": "전 나이키!",
          "speaker_id": "1",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100018669.1.15",
          "form": "오! 저도요!",
          "original_form": "오! 저도요!",
          "speaker_id": "2",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100018669.1.16",
          "form": "무난하달까.. 사고보면 나이키에요ㅋㅋ",
          "original_form": "무난하달까.. 사고보면 나이키에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100018669.1.17",
          "form": "맞아요 파타고니아도 좋아해요ㅎ",
          "original_form": "맞아요 파타고니아도 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100018669.1.18",
          "form": "좋아하시는게 확실하신거 같아요",
          "original_form": "좋아하시는게 확실하신거 같아요",
          "speaker_id": "2",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100018669.1.19",
          "form": "좋아하는거 많아요 ㅋㅋ",
          "original_form": "좋아하는거 많아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100018669.1.20",
          "form": "위시템 잇으신가요",
          "original_form": "위시템 잇으신가요",
          "speaker_id": "1",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100018669.1.21",
          "form": "아뇨ㅋㅋ 고양이 용품은 있는데 제건 생각해보질 않았네요ㅋㅋㅋ",
          "original_form": "아뇨ㅋㅋ 고양이 용품은 있는데 제건 생각해보질 않았네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018669.1.22",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018669.1.23",
          "form": "ㅠㅠ고양이 부럽네요",
          "original_form": "ㅠㅠ고양이 부럽네요",
          "speaker_id": "1",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100018669.1.24",
          "form": "ㅋㅋ주인이 푹빠졋어요",
          "original_form": "ㅋㅋ주인이 푹빠졋어요",
          "speaker_id": "1",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100018669.1.25",
          "form": "ㅋㅋ헤어나오기 쉽지않네요ㅋㅋ",
          "original_form": "ㅋㅋ헤어나오기 쉽지않네요ㅋㅋ",
