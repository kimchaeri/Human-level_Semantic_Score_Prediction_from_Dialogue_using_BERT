      "utterance": [
        {
          "id": "MDRW2100004465.1.1",
          "form": "좋아하는 연예인이나 즐겨보는 유튜브 있으세요?",
          "original_form": "좋아하는 연예인이나 즐겨보는 유튜브 있으세요?",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004465.1.2",
          "form": "저 송강 조아해요!!!",
          "original_form": "저 송강 조아해요!!!",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004465.1.3",
          "form": "오 송강 잘생겼죵~~~ 그럼 요새 찍는 드라마 그것도 보세요?",
          "original_form": "오 송강 잘생겼죵~~~ 그럼 요새 찍는 드라마 그것도 보세요?",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004465.1.4",
          "form": "알고있지만!",
          "original_form": "알고있지만!",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004465.1.5",
          "form": "네네 봐요!!",
          "original_form": "네네 봐요!!",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004465.1.6",
          "form": "존잼ㅠㅜ",
          "original_form": "존잼ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004465.1.7",
          "form": "얼굴이 재밌죵..",
          "original_form": "얼굴이 재밌죵..",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004465.1.8",
          "form": "송강씨 이리저리 많이 찍어서 좋으시겠어요!!",
          "original_form": "송강씨 이리저리 많이 찍어서 좋으시겠어요!!",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004465.1.9",
          "form": "맞아요!! 스위트홈 보셨어용?",
          "original_form": "맞아요!! 스위트홈 보셨어용?",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004465.1.10",
          "form": "아뇨 저 아직 못봤어요! 웹툰 보고 보려고요",
          "original_form": "아뇨 저 아직 못봤어요! 웹툰 보고 보려고요",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004465.1.11",
          "form": "시즌2도 나온다면서요?",
          "original_form": "시즌2도 나온다면서요?",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004465.1.12",
          "form": "마자요 2 나올거같아요!",
          "original_form": "마자요 2 나올거같아요!",
          "speaker_id": "2",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004465.1.13",
          "form": "재밌어용!!",
          "original_form": "재밌어용!!",
          "speaker_id": "2",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004465.1.14",
          "form": "스위트홈 많이 잔인하진 않나요?",
          "original_form": "스위트홈 많이 잔인하진 않나요?",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004465.1.15",
          "form": "클립으로 유튜브에서 조금씩 봤어요!",
          "original_form": "클립으로 유튜브에서 조금씩 봤어요!",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004465.1.16",
          "form": "좋알람도 보셨나요?",
          "original_form": "좋알람도 보셨나요?",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004465.1.17",
          "form": "잔인하긴해요ㅠㅠ",
          "original_form": "잔인하긴해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004465.1.18",
          "form": "그건 안봤어요!!",
          "original_form": "그건 안봤어요!!",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004465.1.19",
          "form": "송강이 얼굴은 잘하는데...음..연기가..",
          "original_form": "송강이 얼굴은 잘하는데...음..연기가..",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004465.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004465.1.21",
          "form": "인정.. 연기나 발음이 살짝",
          "original_form": "인정.. 연기나 발음이 살짝",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004465.1.22",
          "form": "얼굴 반반하니까 얼굴맛에 보는 느낌",
          "original_form": "얼굴 반반하니까 얼굴맛에 보는 느낌",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004465.1.23",
          "form": "잘생겼으니까요ㅎㅎ",
          "original_form": "잘생겼으니까요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004465.1.24",
          "form": "그쵸 역시... 한소희랑 얼굴합이..",
          "original_form": "그쵸 역시... 한소희랑 얼굴합이..",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004465.1.25",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004465.1.26",
          "form": "이게 바로 작품",
          "original_form": "이게 바로 작품",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004465.1.27",
          "form": "크,,진짜",
          "original_form": "크,,진짜",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004465.1.28",
          "form": "저 짤로 좀 봤는데 만화에서 나온듯한",
          "original_form": "저 짤로 좀 봤는데 만화에서 나온듯한",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004465.1.29",
          "form": "둘이 비주얼 합도 좋구ㅎㅎ메이킹도",
          "original_form": "둘이 비주얼 합도 좋구ㅎㅎ메이킹도",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004465.1.30",
          "form": "마자요 캐스팅 최고에요",
          "original_form": "마자요 캐스팅 최고에요",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004465.1.31",
          "form": "멀 좀 아시는 분이 캐스팅을..",
          "original_form": "멀 좀 아시는 분이 캐스팅을..",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004465.1.32",
          "form": "그니까요ㅎㅎ 한소희씨는 그 웹툰 원래 팬이시라고 하시더라고요ㅋㅋ",
          "original_form": "그니까요ㅎㅎ 한소희씨는 그 웹툰 원래 팬이시라고 하시더라고요ㅋㅋ",
          "speaker_id": "1",
