          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100033369.1.4",
          "form": "나 오늘 서점 갔어",
          "original_form": "나 오늘 서점 갔어",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100033369.1.5",
          "form": "교보문고?",
          "original_form": "교보문고?",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100033369.1.6",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100033369.1.7",
          "form": "뭐 사러?",
          "original_form": "뭐 사러?",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100033369.1.8",
          "form": "나 이번에 학교 수업 때문에",
          "original_form": "나 이번에 학교 수업 때문에",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100033369.1.9",
          "form": "어떤수업인데?",
          "original_form": "어떤수업인데?",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100033369.1.10",
          "form": "고전 명작 3개를 읽고 소개하는거야",
          "original_form": "고전 명작 3개를 읽고 소개하는거야",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100033369.1.11",
          "form": "고전명작좋지",
          "original_form": "고전명작좋지",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100033369.1.12",
          "form": "맞아 언제나 배울점이 많지",
          "original_form": "맞아 언제나 배울점이 많지",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100033369.1.13",
          "form": "그럼어떤책?",
          "original_form": "그럼어떤책?",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100033369.1.14",
          "form": "일단 내가 제일 좋아하는 이방인",
          "original_form": "일단 내가 제일 좋아하는 이방인",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100033369.1.15",
          "form": "그것도 고전책인가?",
          "original_form": "그것도 고전책인가?",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100033369.1.16",
          "form": "그리고 군주론이 최고야",
          "original_form": "그리고 군주론이 최고야",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100033369.1.17",
          "form": "나 그책 읽어보고싶어",
          "original_form": "나 그책 읽어보고싶어",
          "speaker_id": "1",
