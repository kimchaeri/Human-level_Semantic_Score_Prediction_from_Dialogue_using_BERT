      "utterance": [
        {
          "id": "MDRW2100017189.1.1",
          "form": "그래서 중간에 껐어요",
          "original_form": "그래서 중간에 껐어요",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017189.1.2",
          "form": "저도..",
          "original_form": "저도..",
          "speaker_id": "1",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017189.1.3",
          "form": "5화까지 본 것 같아요",
          "original_form": "5화까지 본 것 같아요",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017189.1.4",
          "form": "그 부분은 뛰어넘기 했어요",
          "original_form": "그 부분은 뛰어넘기 했어요",
          "speaker_id": "1",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017189.1.5",
          "form": "소재는 좋더라구요",
          "original_form": "소재는 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017189.1.6",
          "form": "신선하죠",
          "original_form": "신선하죠",
          "speaker_id": "1",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017189.1.7",
          "form": "포털을 다룬거라",
          "original_form": "포털을 다룬거라",
          "speaker_id": "1",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017189.1.8",
          "form": "ㅋㅋㅋㅋㅋ검색어 관련",
          "original_form": "ㅋㅋㅋㅋㅋ검색어 관련",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017189.1.9",
          "form": "저렇게 많은 일들이 있군!",
          "original_form": "저렇게 많은 일들이 있군!",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.10",
          "form": "오 좀 진보적이었다고 해야하나",
          "original_form": "오 좀 진보적이었다고 해야하나",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.11",
          "form": "많이 생각해보게됐어요",
          "original_form": "많이 생각해보게됐어요",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.12",
          "form": "새로웠어요 드라마 자체가",
          "original_form": "새로웠어요 드라마 자체가",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.13",
          "form": "또 전 여자다보니깐",
          "original_form": "또 전 여자다보니깐",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.14",
          "form": "여자 주인공이 뭔가",
          "original_form": "여자 주인공이 뭔가",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.15",
          "form": "캔디형이 아니라",
          "original_form": "캔디형이 아니라",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.16",
          "form": "좀 괴팍한 똑똑이 느낌으로 나온 것도",
          "original_form": "좀 괴팍한 똑똑이 느낌으로 나온 것도",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.17",
          "form": "맞아요 주체적이더라구요",
          "original_form": "맞아요 주체적이더라구요",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.18",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.19",
          "form": "네네 그런 여자들이 많아지면 좋죠",
          "original_form": "네네 그런 여자들이 많아지면 좋죠",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.20",
          "form": "매력있었어요",
          "original_form": "매력있었어요",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017189.1.21",
          "form": "캐릭터하나하나 잘 만들었더라구요",
          "original_form": "캐릭터하나하나 잘 만들었더라구요",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017189.1.22",
          "form": "김다희씨는 막 주짓수도 하는 캐릭터 나오고",
          "original_form": "김다희씨는 막 주짓수도 하는 캐릭터 나오고",
          "speaker_id": "2",
          "time": "20210826 17:57"
        }
