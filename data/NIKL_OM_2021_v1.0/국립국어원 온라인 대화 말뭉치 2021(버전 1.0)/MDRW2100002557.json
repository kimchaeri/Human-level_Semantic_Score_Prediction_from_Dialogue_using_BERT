      "utterance": [
        {
          "id": "MDRW2100002557.1.1",
          "form": "더운거 빼면 좋죠",
          "original_form": "더운거 빼면 좋죠",
          "speaker_id": "2",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002557.1.2",
          "form": "슬슬 미세먼지 안좋아지는것같아서 슬푸네여ㅠ",
          "original_form": "슬슬 미세먼지 안좋아지는것같아서 슬푸네여ㅠ",
          "speaker_id": "1",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002557.1.3",
          "form": "미세먼지 진짜 싫어요",
          "original_form": "미세먼지 진짜 싫어요",
          "speaker_id": "2",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002557.1.4",
          "form": "저 그래서 봄되면 스트레스받아요",
          "original_form": "저 그래서 봄되면 스트레스받아요",
          "speaker_id": "2",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002557.1.5",
          "form": "저두요ㅠㅠ",
          "original_form": "저두요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002557.1.6",
          "form": "미세먼지 심해지면 뭔가",
          "original_form": "미세먼지 심해지면 뭔가",
          "speaker_id": "1",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002557.1.7",
          "form": "피부가 간질간질해서",
          "original_form": "피부가 간질간질해서",
          "speaker_id": "1",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002557.1.8",
          "form": "삶의 질 하락",
          "original_form": "삶의 질 하락",
          "speaker_id": "2",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002557.1.9",
          "form": "넘 힘들어요",
          "original_form": "넘 힘들어요",
          "speaker_id": "1",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002557.1.10",
          "form": "막 꽃피고 그런건 좋은데",
          "original_form": "막 꽃피고 그런건 좋은데",
          "speaker_id": "2",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002557.1.11",
          "form": "미세먼지 생각하면 스트레스받아서",
          "original_form": "미세먼지 생각하면 스트레스받아서",
          "speaker_id": "2",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002557.1.12",
          "form": "서해에 강풍기 설치하고싶을정도..",
          "original_form": "서해에 강풍기 설치하고싶을정도..",
          "speaker_id": "2",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002557.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002557.1.14",
          "form": "한 번쯤 생각해보지않나요?",
          "original_form": "한 번쯤 생각해보지않나요?",
          "speaker_id": "2",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002557.1.15",
          "form": "강풍기설치 모금하시면",
          "original_form": "강풍기설치 모금하시면",
          "speaker_id": "1",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002557.1.16",
          "form": "저도 모금할래요",
          "original_form": "저도 모금할래요",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002557.1.17",
          "form": "중국아 니들 꺼 니들이 마셔서 없애라~~~",
          "original_form": "중국아 니들 꺼 니들이 마셔서 없애라~~~",
          "speaker_id": "2",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002557.1.18",
          "form": "막 인터넷기사로",
          "original_form": "막 인터넷기사로",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002557.1.19",
          "form": "고등어구이가 미세먼지 주범이다 이런거 올라올때",
          "original_form": "고등어구이가 미세먼지 주범이다 이런거 올라올때",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002557.1.20",
          "form": "너무 어이가 없었어요",
          "original_form": "너무 어이가 없었어요",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002557.1.21",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002557.1.22",
          "form": "무슨 고등어 진짜",
          "original_form": "무슨 고등어 진짜",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002557.1.23",
