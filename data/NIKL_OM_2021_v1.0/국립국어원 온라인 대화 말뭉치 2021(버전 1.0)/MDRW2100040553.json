      "utterance": [
        {
          "id": "MDRW2100040553.1.1",
          "form": "ㅋㅋㅋ전 그거 안봐서 모르네유...",
          "original_form": "ㅋㅋㅋ전 그거 안봐서 모르네유...",
          "speaker_id": "2",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040553.1.2",
          "form": "ㅋㅋㅋ오늘 데뷔조 정해짐 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ오늘 데뷔조 정해짐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040553.1.3",
          "form": "오늘까지 3번생방송함",
          "original_form": "오늘까지 3번생방송함",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040553.1.4",
          "form": "1번 2번 다 싸이팀가 이김 ㅋㅋㅋ박진영팀꺾고",
          "original_form": "1번 2번 다 싸이팀가 이김 ㅋㅋㅋ박진영팀꺾고",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040553.1.5",
          "form": "티비보나요?",
          "original_form": "티비보나요?",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040553.1.6",
          "form": "글쿤ㅋㅋㅋ안잘생겨서 흥미없...",
          "original_form": "글쿤ㅋㅋㅋ안잘생겨서 흥미없...",
          "speaker_id": "2",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040553.1.7",
          "form": "ㅋㅋㅋ차차차 끝부분 보다가 끝났넹",
          "original_form": "ㅋㅋㅋ차차차 끝부분 보다가 끝났넹",
          "speaker_id": "2",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100040553.1.8",
          "form": "ㅋㅋㅋㅋ 놀면 뭐하니 잼잇엇나요?",
          "original_form": "ㅋㅋㅋㅋ 놀면 뭐하니 잼잇엇나요?",
          "speaker_id": "1",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100040553.1.9",
          "form": "ㅋㅋㅋ뒤에는 잘못봤는데 그 뉴스하는건 웃겼어유 ㅋㅋㅋㄴ",
          "original_form": "ㅋㅋㅋ뒤에는 잘못봤는데 그 뉴스하는건 웃겼어유 ㅋㅋㅋㄴ",
          "speaker_id": "2",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040553.1.10",
          "form": "ㅋㅋㅋㅋㅋ하하 엄청 틀릴꺼같아 ㅇ",
          "original_form": "ㅋㅋㅋㅋㅋ하하 엄청 틀릴꺼같아 ㅇ",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040553.1.11",
          "form": "안그래도 저번주꺼 너가말해줫나 ? 유재석 잘한다고 듣고",
          "original_form": "안그래도 저번주꺼 너가말해줫나 ? 유재석 잘한다고 듣고",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040553.1.12",
          "form": "재방할때 오빠랑 봣는데 ㅋㅋㅋ겁나 신기햇음",
          "original_form": "재방할때 오빠랑 봣는데 ㅋㅋㅋ겁나 신기햇음",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040553.1.13",
          "form": "난 진짜 생방탄건줄 ㅋㅋ",
          "original_form": "난 진짜 생방탄건줄 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040553.1.14",
          "form": "ㅋㅋㅋ진짜 유재석이 완전 잘하는 거였어",
          "original_form": "ㅋㅋㅋ진짜 유재석이 완전 잘하는 거였어",
          "speaker_id": "2",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100040553.1.15",
          "form": "ㅋㅋㅋ하하 울뻔",
          "original_form": "ㅋㅋㅋ하하 울뻔",
          "speaker_id": "2",
          "time": "20210904 22:38"
        },
        {
