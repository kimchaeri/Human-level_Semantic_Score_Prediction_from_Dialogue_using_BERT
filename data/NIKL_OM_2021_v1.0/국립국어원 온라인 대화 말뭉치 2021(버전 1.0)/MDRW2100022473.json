      "utterance": [
        {
          "id": "MDRW2100022473.1.1",
          "form": "목줄 해주려고 했는데",
          "original_form": "목줄 해주려고 했는데",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100022473.1.2",
          "form": "진1돗1개가",
          "original_form": "진1돗1개가",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100022473.1.3",
          "form": "아이고.. 애가 상처를 많이 받았구나..",
          "original_form": "아이고.. 애가 상처를 많이 받았구나..",
          "speaker_id": "1",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100022473.1.4",
          "form": "막 도망다녀서 겨우 잡았더니",
          "original_form": "막 도망다녀서 겨우 잡았더니",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100022473.1.5",
          "form": "막 엄청 벌벌벌벌 떨면서",
          "original_form": "막 엄청 벌벌벌벌 떨면서",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.6",
          "form": "겁났나봐요ㅠㅠ",
          "original_form": "겁났나봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.7",
          "form": "거의 죽은 줄 알았어요",
          "original_form": "거의 죽은 줄 알았어요",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.8",
          "form": "에고..",
          "original_form": "에고..",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.9",
          "form": "그런가봐요 ㅠㅠ지금은 잘 모르겠어요",
          "original_form": "그런가봐요 ㅠㅠ지금은 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.10",
          "form": "할아버지가 물도 안갈아줘서",
          "original_form": "할아버지가 물도 안갈아줘서",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.11",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.12",
          "form": "저번 명절 때 물 자주 갈아주셔야된다고 알려줬어요",
          "original_form": "저번 명절 때 물 자주 갈아주셔야된다고 알려줬어요",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.13",
          "form": "그럼 안되는데",
          "original_form": "그럼 안되는데",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.14",
          "form": "이번 명절 때 한번더 검사해야죠 ㅋㅋㅋ",
          "original_form": "이번 명절 때 한번더 검사해야죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.15",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.16",
          "form": "반려동물 키워본 적 없어서 잘 모르신거 같아요",
          "original_form": "반려동물 키워본 적 없어서 잘 모르신거 같아요",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.17",
          "form": "처음엔 목줄 엄청 짧게 해놓으셨더라구요",
          "original_form": "처음엔 목줄 엄청 짧게 해놓으셨더라구요",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022473.1.18",
          "form": "도망갈까봐 그랬대요",
          "original_form": "도망갈까봐 그랬대요",
          "speaker_id": "2",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022473.1.19",
          "form": "옛날 분들이 좀 그러신거같아요",
          "original_form": "옛날 분들이 좀 그러신거같아요",
          "speaker_id": "1",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022473.1.20",
          "form": "그러면 안되는데..",
          "original_form": "그러면 안되는데..",
          "speaker_id": "1",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022473.1.21",
          "form": "그래서 아니 도망갈데가 어딨다고 그냥 풀어놓으라 했어요",
          "original_form": "그래서 아니 도망갈데가 어딨다고 그냥 풀어놓으라 했어요",
          "speaker_id": "2",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022473.1.22",
          "form": "잘하셨어요ㅠ",
          "original_form": "잘하셨어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022473.1.23",
          "form": "그래서 지금 마당에서 빙글빙글 돌고 아주 신났어요 ㅋㅋ",
          "original_form": "그래서 지금 마당에서 빙글빙글 돌고 아주 신났어요 ㅋㅋ",
