      "utterance": [
        {
          "id": "MDRW2100024611.1.1",
          "form": "너는 알바해?",
          "original_form": "너는 알바해?",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100024611.1.2",
          "form": "나는 요즘 주기적으로 하는건 없고 단기알바만 해",
          "original_form": "나는 요즘 주기적으로 하는건 없고 단기알바만 해",
          "speaker_id": "1",
          "time": "20210831 15:34"
        },
        {
          "id": "MDRW2100024611.1.3",
          "form": "무슨 단기알바 하는데?",
          "original_form": "무슨 단기알바 하는데?",
          "speaker_id": "2",
          "time": "20210831 15:57"
        },
        {
          "id": "MDRW2100024611.1.4",
          "form": "요즘 녹음알바 많이해",
          "original_form": "요즘 녹음알바 많이해",
          "speaker_id": "1",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100024611.1.5",
          "form": "녹음알바? 노래하는거야?",
          "original_form": "녹음알바? 노래하는거야?",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024611.1.6",
          "form": "아니 문장 읽는거야",
          "original_form": "아니 문장 읽는거야",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024611.1.7",
          "form": "무슨 문장 읽는데?",
          "original_form": "무슨 문장 읽는데?",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024611.1.8",
          "form": "최근에 한건 2400문장 읽었어",
          "original_form": "최근에 한건 2400문장 읽었어",
          "speaker_id": "1",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024611.1.9",
          "form": "목 아프지 않았어?",
          "original_form": "목 아프지 않았어?",
          "speaker_id": "2",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024611.1.10",
          "form": "3일정도 계속 목아파서 물 엄청 많이 마셨어",
          "original_form": "3일정도 계속 목아파서 물 엄청 많이 마셨어",
          "speaker_id": "1",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024611.1.11",
          "form": "고생했어 ㅠ",
          "original_form": "고생했어 ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100024611.1.12",
          "form": "너는 알바해?",
          "original_form": "너는 알바해?",
          "speaker_id": "1",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100024611.1.13",
          "form": "나는 두끼 알바해",
          "original_form": "나는 두끼 알바해",
          "speaker_id": "2",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100024611.1.14",
          "form": "어떤일해?",
          "original_form": "어떤일해?",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024611.1.15",
          "form": "손님 응대하고",
          "original_form": "손님 응대하고",
          "speaker_id": "2",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024611.1.16",
          "form": "계산도하고 치우고",
          "original_form": "계산도하고 치우고",
          "speaker_id": "2",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024611.1.17",
          "form": "마감도 하고 있어",
          "original_form": "마감도 하고 있어",
          "speaker_id": "2",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024611.1.18",
          "form": "많이 바빠?",
          "original_form": "많이 바빠?",
          "speaker_id": "1",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100024611.1.19",
          "form": "코로나 전에는 엄청 바빴는데",
          "original_form": "코로나 전에는 엄청 바빴는데",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100024611.1.20",
          "form": "요즘은 좀 덜 바쁜것 같아",
          "original_form": "요즘은 좀 덜 바쁜것 같아",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100024611.1.21",
          "form": "매출이 많이 떨어졌어",
          "original_form": "매출이 많이 떨어졌어",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100024611.1.22",
          "form": "그러다 짤리는거 아냐?",
          "original_form": "그러다 짤리는거 아냐?",
          "speaker_id": "1",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024611.1.23",
          "form": "그래서 전에 사장님이 한동안 나오지 말하고 한적도 있어 ㅠ",
          "original_form": "그래서 전에 사장님이 한동안 나오지 말하고 한적도 있어 ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024611.1.24",
          "form": "헐.. 근데 지금은 잘 다니네",
          "original_form": "헐.. 근데 지금은 잘 다니네",
          "speaker_id": "1",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024611.1.25",
          "form": "응 지금은 그래도 손님이 전보단 있어서 다행이야",
          "original_form": "응 지금은 그래도 손님이 전보단 있어서 다행이야",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024611.1.26",
          "form": "너는 알바하면서 진상 만난적 있어?",
          "original_form": "너는 알바하면서 진상 만난적 있어?",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024611.1.27",
          "form": "당연하지 ㅋㅋ",
          "original_form": "당연하지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100024611.1.28",
          "form": "썰 풀어주라",
          "original_form": "썰 풀어주라",
          "speaker_id": "2",
          "time": "20210831 19:07"
        }
      ]
    }
