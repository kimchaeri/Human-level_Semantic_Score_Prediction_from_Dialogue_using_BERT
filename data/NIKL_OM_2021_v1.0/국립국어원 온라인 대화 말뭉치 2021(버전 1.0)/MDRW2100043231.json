      "utterance": [
        {
          "id": "MDRW2100043231.1.1",
          "form": "거긴어떄",
          "original_form": "거긴어떄",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043231.1.2",
          "form": "거기 괜찮대",
          "original_form": "거기 괜찮대",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043231.1.3",
          "form": "이건 수원에도 체인잇어",
          "original_form": "이건 수원에도 체인잇어",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043231.1.4",
          "form": "가보자",
          "original_form": "가보자",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043231.1.5",
          "form": "코로나 낮아지면 가보자",
          "original_form": "코로나 낮아지면 가보자",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043231.1.6",
          "form": "청주에는?",
          "original_form": "청주에는?",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043231.1.7",
          "form": "글쎄...?",
          "original_form": "글쎄...?",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043231.1.8",
          "form": "많긴하던데 한번바바",
          "original_form": "많긴하던데 한번바바",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043231.1.9",
          "form": "거기도츠케멘?",
          "original_form": "거기도츠케멘?",
          "speaker_id": "1",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043231.1.10",
          "form": "ㅇㅇ잇어",
          "original_form": "ㅇㅇ잇어",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043231.1.11",
          "form": "청주는 없다",
          "original_form": "청주는 없다",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043231.1.12",
          "form": "못참지",
          "original_form": "못참지",
          "speaker_id": "1",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043231.1.13",
          "form": "ㅎㅎ 내가먹어보고 말해줌~",
          "original_form": "ㅎㅎ 내가먹어보고 말해줌~",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043231.1.14",
          "form": "안먹을거자나",
          "original_form": "안먹을거자나",
          "speaker_id": "1",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043231.1.15",
          "form": "셩지엔은 진짜어디서먹어...",
          "original_form": "셩지엔은 진짜어디서먹어...",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043231.1.16",
          "form": "먹을수도있지...",
          "original_form": "먹을수도있지...",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043231.1.17",
          "form": "차이나타운가볼까",
          "original_form": "차이나타운가볼까",
          "speaker_id": "1",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043231.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043231.1.19",
          "form": "그때먹고올걸",
          "original_form": "그때먹고올걸",
          "speaker_id": "1",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043231.1.20",
          "form": "근데이거 팔달문쪽에있어서 애들꼬셔서 먹고 커피먹으면 딱임",
          "original_form": "근데이거 팔달문쪽에있어서 애들꼬셔서 먹고 커피먹으면 딱임",
          "speaker_id": "2",
          "time": "20210907 16:06"
        }
      ]
