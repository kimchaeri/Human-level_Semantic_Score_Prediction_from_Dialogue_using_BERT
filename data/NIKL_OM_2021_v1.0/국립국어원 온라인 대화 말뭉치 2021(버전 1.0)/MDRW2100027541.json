        {
          "id": "MDRW2100027541.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027541.1.2",
          "form": "저는 필라테스 하다가 요즘엔ㅋㅋㅋ 그냥 걸어요",
          "original_form": "저는 필라테스 하다가 요즘엔ㅋㅋㅋ 그냥 걸어요",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027541.1.3",
          "form": "운동 뭐 하세요?",
          "original_form": "운동 뭐 하세요?",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027541.1.4",
          "form": "홈트요",
          "original_form": "홈트요",
          "speaker_id": "1",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027541.1.5",
          "form": "오 유튜브같은 거 보고 하세요?",
          "original_form": "오 유튜브같은 거 보고 하세요?",
          "speaker_id": "2",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027541.1.6",
          "form": "땅크봐요",
          "original_form": "땅크봐요",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100027541.1.7",
          "form": "다들 그거 많이보네요",
          "original_form": "다들 그거 많이보네요",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100027541.1.8",
          "form": "다이어트 하시는 거예요?",
          "original_form": "다이어트 하시는 거예요?",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027541.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100027541.1.10",
          "form": "아하 다이어트 힘들죠",
          "original_form": "아하 다이어트 힘들죠",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100027541.1.11",
          "form": "식단도 하시나요?",
          "original_form": "식단도 하시나요?",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100027541.1.12",
          "form": "식단까지는",
          "original_form": "식단까지는",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100027541.1.13",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100027541.1.14",
          "form": "그래도 효과있어요",
          "original_form": "그래도 효과있어요",
          "speaker_id": "1",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100027541.1.15",
          "form": "역시 운동이 좋긴 한가봐요 저도 해야하나 싶네요",
          "original_form": "역시 운동이 좋긴 한가봐요 저도 해야하나 싶네요",
