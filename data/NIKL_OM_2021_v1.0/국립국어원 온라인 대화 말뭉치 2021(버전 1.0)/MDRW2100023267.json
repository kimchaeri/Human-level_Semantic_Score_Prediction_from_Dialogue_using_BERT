      "utterance": [
        {
          "id": "MDRW2100023267.1.1",
          "form": "아 고3 담임쌤들이랑",
          "original_form": "아 고3 담임쌤들이랑",
          "speaker_id": "2",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100023267.1.2",
          "form": "전제조건이 고3담임이구나",
          "original_form": "전제조건이 고3담임이구나",
          "speaker_id": "1",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100023267.1.3",
          "form": "고3 교과 담당선생님들이 부담임이",
          "original_form": "고3 교과 담당선생님들이 부담임이",
          "speaker_id": "2",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100023267.1.4",
          "form": "라서",
          "original_form": "라서",
          "speaker_id": "2",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100023267.1.5",
          "form": "가족들 계시면 고3 담임이나 담당도 기피하겠네요",
          "original_form": "가족들 계시면 고3 담임이나 담당도 기피하겠네요",
          "speaker_id": "1",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100023267.1.6",
          "form": "담임 부담임이 돌아가면서해요",
          "original_form": "담임 부담임이 돌아가면서해요",
          "speaker_id": "2",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100023267.1.7",
          "form": "아 고3 야자인 경우 ㅋㅋㅋ",
          "original_form": "아 고3 야자인 경우 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100023267.1.8",
          "form": "고2 야자는 또 고2 담임이랑 고2부담임",
          "original_form": "고2 야자는 또 고2 담임이랑 고2부담임",
          "speaker_id": "2",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023267.1.9",
          "form": "다 각자 학년 ㅋㅋㅋ",
          "original_form": "다 각자 학년 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023267.1.10",
          "form": "생각해보니까",
          "original_form": "생각해보니까",
          "speaker_id": "1",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023267.1.11",
          "form": "야자 1-3학년 다 하구나 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "야자 1-3학년 다 하구나 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023267.1.12",
          "form": "그러게여 저도 고3만 생각햇네요 ㅋㅋㅋㅋ",
          "original_form": "그러게여 저도 고3만 생각햇네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023267.1.13",
          "form": "모든 학년이 다하죠 네 ㅋㅋㅋㅋㅋ",
          "original_form": "모든 학년이 다하죠 네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023267.1.14",
          "form": "이런거 물어봐도 되는지 모르겠는데",
          "original_form": "이런거 물어봐도 되는지 모르겠는데",
          "speaker_id": "1",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023267.1.15",
          "form": "야자 감독 하면 수당 따로 주는거에요 아니면",
          "original_form": "야자 감독 하면 수당 따로 주는거에요 아니면",
          "speaker_id": "1",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023267.1.16",
          "form": "초과수당만 나와요?",
          "original_form": "초과수당만 나와요?",
          "speaker_id": "1",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023267.1.17",
          "form": "초근 올려요 ㅋㅋㅋㅋ",
          "original_form": "초근 올려요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023267.1.18",
          "form": "초근만 받는거에요?ㅜ",
          "original_form": "초근만 받는거에요?ㅜ",
          "speaker_id": "1",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023267.1.19",
          "form": "넼ㅋㅋㅋ거의 봉사활동이져?",
          "original_form": "넼ㅋㅋㅋ거의 봉사활동이져?",
          "speaker_id": "2",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023267.1.20",
          "form": "진짜 충격이다",
          "original_form": "진짜 충격이다",
          "speaker_id": "1",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023267.1.21",
          "form": "그럼 특근매식비는 챙겨주시나요..",
          "original_form": "그럼 특근매식비는 챙겨주시나요..",
          "speaker_id": "1",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023267.1.22",
          "form": "네네 그건 주시더라구여",
          "original_form": "네네 그건 주시더라구여",
          "speaker_id": "2",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023267.1.23",
          "form": "급식실가서 밥먹으라고..",
          "original_form": "급식실가서 밥먹으라고..",
          "speaker_id": "2",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023267.1.24",
          "form": "ㅋㅋㅋ네?",
          "original_form": "ㅋㅋㅋ네?",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023267.1.25",
          "form": "석식 공짜..",
          "original_form": "석식 공짜..",
          "speaker_id": "2",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023267.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023267.1.27",
          "form": "급식실 가서 ..?ㅋㅋㅋ",
          "original_form": "급식실 가서 ..?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023267.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023267.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ열악하네요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ열악하네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023267.1.30",
          "form": "석식이 공짜였어요..",
          "original_form": "석식이 공짜였어요..",
          "speaker_id": "2",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023267.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:34"
        },
