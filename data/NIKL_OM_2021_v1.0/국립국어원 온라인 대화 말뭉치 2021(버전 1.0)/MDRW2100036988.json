      "utterance": [
        {
          "id": "MDRW2100036988.1.1",
          "form": "반미가 그렇게 맛있다면서요?!",
          "original_form": "반미가 그렇게 맛있다면서요?!",
          "speaker_id": "2",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100036988.1.2",
          "form": "사실 저도 고수를 못먹는데",
          "original_form": "사실 저도 고수를 못먹는데",
          "speaker_id": "2",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100036988.1.3",
          "form": "반미부터 맛들이면 달라지려나요? ㅎㅎㅎ",
          "original_form": "반미부터 맛들이면 달라지려나요? ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100036988.1.4",
          "form": "네 넘맛있어요",
          "original_form": "네 넘맛있어요",
          "speaker_id": "1",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100036988.1.5",
          "form": "17년도에 첨먹어봤는데",
          "original_form": "17년도에 첨먹어봤는데",
          "speaker_id": "1",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100036988.1.6",
          "form": "ㅋㅋㅋㅋㅋ연도까지외우고있는..",
          "original_form": "ㅋㅋㅋㅋㅋ연도까지외우고있는..",
          "speaker_id": "1",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100036988.1.7",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100036988.1.8",
          "form": "너무 맛있으면 첫만남 기억하는 것처럼",
          "original_form": "너무 맛있으면 첫만남 기억하는 것처럼",
          "speaker_id": "2",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100036988.1.9",
          "form": "잊지못할 것 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "잊지못할 것 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100036988.1.10",
          "form": "베트남식 쌀국수랑",
          "original_form": "베트남식 쌀국수랑",
          "speaker_id": "2",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100036988.1.11",
          "form": "태국식 쌀국수는 또 다르더라구요",
          "original_form": "태국식 쌀국수는 또 다르더라구요",
          "speaker_id": "2",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100036988.1.12",
          "form": "(갑자기 쌀국수 이야기)",
          "original_form": "(갑자기 쌀국수 이야기)",
          "speaker_id": "2",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100036988.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210831 16:32"
        },
        {
          "id": "MDRW2100036988.1.14",
          "form": "한국식쌀국수도 다르졐ㅋㅋㅋ",
          "original_form": "한국식쌀국수도 다르졐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:32"
        },
        {
          "id": "MDRW2100036988.1.15",
          "form": "어떤 쌀국수가 더 맛있으셨어요",
          "original_form": "어떤 쌀국수가 더 맛있으셨어요",
          "speaker_id": "1",
          "time": "20210831 16:32"
        },
        {
          "id": "MDRW2100036988.1.16",
          "form": "아 한국식 쌀국숰ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 한국식 쌀국숰ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100036988.1.17",
          "form": "저는 베트남 쌀국수를 먼저 먹었어서 그런지",
          "original_form": "저는 베트남 쌀국수를 먼저 먹었어서 그런지",
          "speaker_id": "2",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100036988.1.18",
          "form": "요즘은 태국식 쌀국수가 더 끌리더라구욬ㅋㅋㅋ",
          "original_form": "요즘은 태국식 쌀국수가 더 끌리더라구욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036988.1.19",
          "form": "사실 요즘 태국 음식에 꽂혔어요....",
          "original_form": "사실 요즘 태국 음식에 꽂혔어요....",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036988.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ그쳐 어디에꽂힐때가있어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그쳐 어디에꽂힐때가있어요",
          "speaker_id": "1",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036988.1.21",
          "form": "태국음식 어떤어떤거에 꽂히셨어요",
          "original_form": "태국음식 어떤어떤거에 꽂히셨어요",
          "speaker_id": "1",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036988.1.22",
          "form": "뿌빳퐁커리요!!!",
          "original_form": "뿌빳퐁커리요!!!",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036988.1.23",
          "form": "정말 잘 하는 데서 먹으면",
          "original_form": "정말 잘 하는 데서 먹으면",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036988.1.24",
          "form": "꽃게가 입에서 살 살 녹더라구요...",
          "original_form": "꽃게가 입에서 살 살 녹더라구요...",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036988.1.25",
          "form": "크으ㅜㅜㅜ맞아요",
          "original_form": "크으ㅜㅜㅜ맞아요",
          "speaker_id": "1",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100036988.1.26",
          "form": "입에착착감겨요",
          "original_form": "입에착착감겨요",
          "speaker_id": "1",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100036988.1.27",
          "form": "태국음식 좋아하시는군요!!!",
          "original_form": "태국음식 좋아하시는군요!!!",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036988.1.28",
          "form": "그리고 태국식",
          "original_form": "그리고 태국식",
          "speaker_id": "2",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100036988.1.29",
          "form": "딸기우유? 홍차? 이런것도 먹어봤는데",
          "original_form": "딸기우유? 홍차? 이런것도 먹어봤는데",
          "speaker_id": "2",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100036988.1.30",
          "form": "진짜 맛있더라구요,,",
          "original_form": "진짜 맛있더라구요,,",
          "speaker_id": "2",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100036988.1.31",
          "form": "홍대입구역에 어메이징 농카이라는 곳이 있는데 여기 진짜 맛집이에요!!!",
          "original_form": "홍대입구역에 어메이징 농카이라는 곳이 있는데 여기 진짜 맛집이에요!!!",
          "speaker_id": "2",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100036988.1.32",
          "form": "오오 맛집추천감사합니다",
          "original_form": "오오 맛집추천감사합니다",
          "speaker_id": "1",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100036988.1.33",
          "form": "태국식딸기우유라니 신기하네요",
          "original_form": "태국식딸기우유라니 신기하네요",
          "speaker_id": "1",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100036988.1.34",
          "form": "맛은 홍차맛인건가요",
          "original_form": "맛은 홍차맛인건가요",
          "speaker_id": "1",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100036988.1.35",
          "form": "맛은 딸기우유인데 걸쭉해요",
          "original_form": "맛은 딸기우유인데 걸쭉해요",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100036988.1.36",
          "form": "하... 이거 말하는데 또 먹고싶네요;;;",
          "original_form": "하... 이거 말하는데 또 먹고싶네요;;;",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100036988.1.37",
          "form": "팟타이도 맛있고...",
          "original_form": "팟타이도 맛있고...",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036988.1.38",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036988.1.39",
          "form": "걸쭉",
          "original_form": "걸쭉",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036988.1.40",
          "form": "거기는 현지인이 운영하는 덴데",
          "original_form": "거기는 현지인이 운영하는 덴데",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036988.1.41",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036988.1.42",
          "form": "저도 나시고랭미고랭이런거 환장해요",
          "original_form": "저도 나시고랭미고랭이런거 환장해요",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036988.1.43",
          "form": "진짜 거기 식당 들어가면",
          "original_form": "진짜 거기 식당 들어가면",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036988.1.44",
          "form": "찐으로 현지인들이 먹고있어욬ㅋㅋㅋㅋ",
          "original_form": "찐으로 현지인들이 먹고있어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036988.1.45",
          "form": "맛있는 녀석들에도 나왔대요!!",
          "original_form": "맛있는 녀석들에도 나왔대요!!",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036988.1.46",
          "form": "ㅋㅋㅋㅋㅋ와 제대료네요",
          "original_form": "ㅋㅋㅋㅋㅋ와 제대료네요",
          "speaker_id": "1",
