      "utterance": [
        {
          "id": "MDRW2100020375.1.1",
          "form": "회사다니시나요?? 퇴사하고 싶어요..",
          "original_form": "회사다니시나요?? 퇴사하고 싶어요..",
          "speaker_id": "2",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020375.1.2",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "1",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020375.1.3",
          "form": "저퇴사28일차네요",
          "original_form": "저퇴사28일차네요",
          "speaker_id": "1",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020375.1.4",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020375.1.5",
          "form": "부럽",
          "original_form": "부럽",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020375.1.6",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020375.1.7",
          "form": "너무 부럽다",
          "original_form": "너무 부럽다",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020375.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020375.1.9",
          "form": "저 진짜 때려치고싶어요 ㅠㅠ 힉",
          "original_form": "저 진짜 때려치고싶어요 ㅠㅠ 힉",
          "speaker_id": "2",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020375.1.10",
          "form": "아근데 저는 계약직이었어서",
          "original_form": "아근데 저는 계약직이었어서",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020375.1.11",
          "form": "자발적퇴사가아니라",
          "original_form": "자발적퇴사가아니라",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020375.1.12",
          "form": "씁쓸합니다좀",
          "original_form": "씁쓸합니다좀",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020375.1.13",
          "form": "근데막상퇴사하니 좋긴합니다,,,",
          "original_form": "근데막상퇴사하니 좋긴합니다,,,",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020375.1.14",
          "form": "그렇게 쉬는 시간도 있어야죠 !! 저는 무조건 부럽습니당 ㅠㅠ",
          "original_form": "그렇게 쉬는 시간도 있어야죠 !! 저는 무조건 부럽습니당 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020375.1.15",
          "form": "저는 백수가 체질이에요",
          "original_form": "저는 백수가 체질이에요",
          "speaker_id": "2",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020375.1.16",
          "form": "요즘 코시국이라",
          "original_form": "요즘 코시국이라",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020375.1.17",
          "form": "실업급여받기도",
          "original_form": "실업급여받기도",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020375.1.18",
          "form": "편해서좋더라고요",
          "original_form": "편해서좋더라고요",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020375.1.19",
          "form": "회사몇년차세요,,?",
          "original_form": "회사몇년차세요,,?",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020375.1.20",
          "form": "저는 대학졸업하자마자 다니다가 중간에 공부한다고 때려쳤다가",
          "original_form": "저는 대학졸업하자마자 다니다가 중간에 공부한다고 때려쳤다가",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020375.1.21",
          "form": "지금 다시 다니고있거든요",
          "original_form": "지금 다시 다니고있거든요",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020375.1.22",
          "form": "지금 다니는데는 이제 2년차인데",
          "original_form": "지금 다니는데는 이제 2년차인데",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020375.1.23",
          "form": "그냥 백수였을때가 그리워요..",
          "original_form": "그냥 백수였을때가 그리워요..",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020375.1.24",
          "form": "우와 멋있어요",
          "original_form": "우와 멋있어요",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020375.1.25",
          "form": "저는 대학 졸업하고 공시한다고 설치다가 늦게 취업했어요",
          "original_form": "저는 대학 졸업하고 공시한다고 설치다가 늦게 취업했어요",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020375.1.26",
          "form": "2년다니고 퇴사!",
          "original_form": "2년다니고 퇴사!",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020375.1.27",
          "form": "일하시던데는 적성에 맞으셨나요?!?!",
          "original_form": "일하시던데는 적성에 맞으셨나요?!?!",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020375.1.28",
          "form": "네 저는 재밌었어서, 다음에도 그런쪽으로 취업하고싶어요",
          "original_form": "네 저는 재밌었어서, 다음에도 그런쪽으로 취업하고싶어요",
          "speaker_id": "1",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020375.1.29",
          "form": "일반사무직이라서 메리트가 없고 돈을 조금줘요",
          "original_form": "일반사무직이라서 메리트가 없고 돈을 조금줘요",
          "speaker_id": "1",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020375.1.30",
          "form": "오호 그래도 재밌었다니 다행이네요!!",
          "original_form": "오호 그래도 재밌었다니 다행이네요!!",
          "speaker_id": "2",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020375.1.31",
          "form": "팀장 때문에 힘들었지만 ,,",
          "original_form": "팀장 때문에 힘들었지만 ,,",
          "speaker_id": "1",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020375.1.32",
          "form": "아.... 그쳐... 회사는... 사람이 진짜 중요한거같아요",
          "original_form": "아.... 그쳐... 회사는... 사람이 진짜 중요한거같아요",
          "speaker_id": "2",
          "time": "20210828 02:39"
        }
      ]
