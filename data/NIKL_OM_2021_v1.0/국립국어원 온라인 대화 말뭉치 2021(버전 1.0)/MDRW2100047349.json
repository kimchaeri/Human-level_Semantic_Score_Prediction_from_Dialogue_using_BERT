      "utterance": [
        {
          "id": "MDRW2100047349.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100047349.1.2",
          "form": "네 님은요?",
          "original_form": "네 님은요?",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100047349.1.3",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100047349.1.4",
          "form": "어디가 제일 좋으셨어요?",
          "original_form": "어디가 제일 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100047349.1.5",
          "form": "송도요!",
          "original_form": "송도요!",
          "speaker_id": "2",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100047349.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100047349.1.7",
          "form": "전 청산도요!",
          "original_form": "전 청산도요!",
          "speaker_id": "1",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100047349.1.8",
          "form": "송도는 안가봤네요",
          "original_form": "송도는 안가봤네요",
          "speaker_id": "1",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100047349.1.9",
          "form": "청산도는 어디 지역인가용",
          "original_form": "청산도는 어디 지역인가용",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100047349.1.10",
          "form": "전라남도에 있는 섬이에요",
          "original_form": "전라남도에 있는 섬이에요",
          "speaker_id": "1",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100047349.1.11",
          "form": "기념품 사셨나용",
          "original_form": "기념품 사셨나용",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100047349.1.12",
          "form": "아니요 ㅋㅋㅋㅋ사진만 많이 찍었어요",
          "original_form": "아니요 ㅋㅋㅋㅋ사진만 많이 찍었어요",
          "speaker_id": "1",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100047349.1.13",
          "form": "송도에선 뭐가 제일 좋으셧나요",
          "original_form": "송도에선 뭐가 제일 좋으셧나요",
          "speaker_id": "1",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100047349.1.14",
          "form": "바다야경이 예뻤어용",
          "original_form": "바다야경이 예뻤어용",
          "speaker_id": "2",
          "time": "20210826 12:59"
        },
        {
          "id": "MDRW2100047349.1.15",
          "form": "저도 나중에 가봐야겟어요",
          "original_form": "저도 나중에 가봐야겟어요",
          "speaker_id": "1",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100047349.1.16",
          "form": "코로나 끝나면 어디가고싶으세요?",
          "original_form": "코로나 끝나면 어디가고싶으세요?",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100047349.1.17",
          "form": "해외로 가고싶어요 ㅠㅠ",
          "original_form": "해외로 가고싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 13:02"
        }
      ]
