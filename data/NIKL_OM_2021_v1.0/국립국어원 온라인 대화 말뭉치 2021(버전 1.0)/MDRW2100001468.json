        {
          "id": "MDRW2100001468.1.1",
          "form": "하이요",
          "original_form": "하이요",
          "speaker_id": "1",
          "time": "20211014 18:55"
        },
        {
          "id": "MDRW2100001468.1.2",
          "form": "안녕하세요~ 저녁 드셨나요?",
          "original_form": "안녕하세요~ 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20211014 18:55"
        },
        {
          "id": "MDRW2100001468.1.3",
          "form": "아녀 안먹었어요",
          "original_form": "아녀 안먹었어요",
          "speaker_id": "1",
          "time": "20211014 18:55"
        },
        {
          "id": "MDRW2100001468.1.4",
          "form": "헉 왜죠",
          "original_form": "헉 왜죠",
          "speaker_id": "2",
          "time": "20211014 18:56"
        },
        {
          "id": "MDRW2100001468.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20211014 18:56"
        },
        {
          "id": "MDRW2100001468.1.6",
          "form": "사실 저도 안먹었어요 ㅋㅋㅋ",
          "original_form": "사실 저도 안먹었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211014 18:56"
        },
        {
          "id": "MDRW2100001468.1.7",
          "form": "슬슬 먹어야죠",
          "original_form": "슬슬 먹어야죠",
          "speaker_id": "2",
          "time": "20211014 18:56"
        },
        {
          "id": "MDRW2100001468.1.8",
          "form": "뭐 드실거에요?",
          "original_form": "뭐 드실거에요?",
          "speaker_id": "1",
          "time": "20211014 18:56"
        },
        {
          "id": "MDRW2100001468.1.9",
          "form": "그냥 집에 있는",
          "original_form": "그냥 집에 있는",
          "speaker_id": "2",
          "time": "20211014 18:56"
        },
        {
          "id": "MDRW2100001468.1.10",
          "form": "반찬에 밥 먹으려구요..",
          "original_form": "반찬에 밥 먹으려구요..",
          "speaker_id": "2",
          "time": "20211014 18:56"
        },
        {
          "id": "MDRW2100001468.1.11",
          "form": "뭔가 확 땡기는게 없네용",
          "original_form": "뭔가 확 땡기는게 없네용",
          "speaker_id": "2",
          "time": "20211014 18:56"
        },
        {
          "id": "MDRW2100001468.1.12",
          "form": "아하..",
          "original_form": "아하..",
          "speaker_id": "1",
          "time": "20211014 18:56"
        },
        {
          "id": "MDRW2100001468.1.13",
          "form": "님은 아예 안드시려구요?",
          "original_form": "님은 아예 안드시려구요?",
          "speaker_id": "2",
          "time": "20211014 18:56"
        },
        {
          "id": "MDRW2100001468.1.14",
          "form": "전 순대국밥 먹으려구요 ㅎㅎ",
          "original_form": "전 순대국밥 먹으려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211014 18:57"
        },
        {
          "id": "MDRW2100001468.1.15",
          "form": "오~~~~",
          "original_form": "오~~~~",
          "speaker_id": "2",
          "time": "20211014 18:57"
        },
        {
          "id": "MDRW2100001468.1.16",
          "form": "메뉴 선정 짱이네요",
          "original_form": "메뉴 선정 짱이네요",
          "speaker_id": "2",
          "time": "20211014 18:57"
        },
        {
          "id": "MDRW2100001468.1.17",
          "form": "ㅎㅎ 감사해요",
          "original_form": "ㅎㅎ 감사해요",
          "speaker_id": "1",
          "time": "20211014 18:57"
        },
        {
          "id": "MDRW2100001468.1.18",
          "form": "갑자기 궁금한건데",
          "original_form": "갑자기 궁금한건데",
          "speaker_id": "2",
          "time": "20211014 18:57"
        },
        {
          "id": "MDRW2100001468.1.19",
          "form": "국밥 드실때",
          "original_form": "국밥 드실때",
          "speaker_id": "2",
          "time": "20211014 18:57"
        },
        {
          "id": "MDRW2100001468.1.20",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211014 18:57"
        },
        {
          "id": "MDRW2100001468.1.21",
          "form": "밥 말아드세요?",
          "original_form": "밥 말아드세요?",
          "speaker_id": "2",
          "time": "20211014 18:57"
        },
        {
          "id": "MDRW2100001468.1.22",
          "form": "아니면 따로 드세요",
          "original_form": "아니면 따로 드세요",
          "speaker_id": "2",
          "time": "20211014 18:57"
        },
        {
          "id": "MDRW2100001468.1.23",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20211014 18:57"
        },
        {
          "id": "MDRW2100001468.1.24",
          "form": "아뇨 안말아먹어요",
          "original_form": "아뇨 안말아먹어요",
          "speaker_id": "1",
          "time": "20211014 18:57"
        },
        {
          "id": "MDRW2100001468.1.25",
          "form": "어릴때부터 쭉 안말아먹었어요",
          "original_form": "어릴때부터 쭉 안말아먹었어요",
          "speaker_id": "1",
          "time": "20211014 18:58"
        },
        {
          "id": "MDRW2100001468.1.26",
          "form": "오 저도 ㅋㅋㅋㅋㅋ 뭔가 밥말아먹으면 밥맛이 안느껴지는 느낌..",
          "original_form": "오 저도 ㅋㅋㅋㅋㅋ 뭔가 밥말아먹으면 밥맛이 안느껴지는 느낌..",
          "speaker_id": "2",
          "time": "20211014 18:58"
        },
        {
          "id": "MDRW2100001468.1.27",
          "form": "저도저도!! ㅋㅋㅋ",
          "original_form": "저도저도!! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211014 18:58"
        },
        {
          "id": "MDRW2100001468.1.28",
          "form": "그쳐 ㅋㅋ",
          "original_form": "그쳐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211014 18:58"
        },
        {
          "id": "MDRW2100001468.1.29",
          "form": "네네 ㅋㅋㅋㅋㅋ 따로국밥이 최고죠..",
          "original_form": "네네 ㅋㅋㅋㅋㅋ 따로국밥이 최고죠..",
          "speaker_id": "2",
          "time": "20211014 18:58"
        },
        {
          "id": "MDRW2100001468.1.30",
          "form": "혹시 축구 좋아하세요?",
          "original_form": "혹시 축구 좋아하세요?",
          "speaker_id": "1",
          "time": "20211014 18:58"
        },
        {
          "id": "MDRW2100001468.1.31",
          "form": "앗 제가 축구는 아예 몰라서...ㅎ",
          "original_form": "앗 제가 축구는 아예 몰라서...ㅎ",
          "speaker_id": "2",
          "time": "20211014 18:58"
        },
        {
          "id": "MDRW2100001468.1.32",
