        {
          "id": "MDRW2100020895.1.1",
          "form": "아아아 지금 먹으면 저녁시간이ㅡ애매해",
          "original_form": "아아아 지금 먹으면 저녁시간이ㅡ애매해",
          "speaker_id": "2",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020895.1.2",
          "form": "어제 집에왔더니 치킨이 있더라",
          "original_form": "어제 집에왔더니 치킨이 있더라",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020895.1.3",
          "form": "어제 치킨얘기하지 않았니",
          "original_form": "어제 치킨얘기하지 않았니",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020895.1.4",
          "form": "ㅋㄱㅋㅋㄱㅋㄱㅋㄱ",
          "original_form": "ㅋㄱㅋㅋㄱㅋㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020895.1.5",
          "form": "치킨이랑 군만두로 저녁을 먹었지",
          "original_form": "치킨이랑 군만두로 저녁을 먹었지",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020895.1.6",
          "form": "치키이야기했었짘ㄱㅋㅋㄱ",
          "original_form": "치키이야기했었짘ㄱㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020895.1.7",
          "form": "군만두!!!!",
          "original_form": "군만두!!!!",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020895.1.8",
          "form": "ㅇㅇ아주 기름진 저녁을 먹었어",
          "original_form": "ㅇㅇ아주 기름진 저녁을 먹었어",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020895.1.9",
          "form": "그래도 간단히 계란이나 아니면 과일이라도 먹어",
          "original_form": "그래도 간단히 계란이나 아니면 과일이라도 먹어",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020895.1.10",
          "form": "전에 우리 갔던 비건카페 기억하니 코코넛아이스크림이 나왔던..",
          "original_form": "전에 우리 갔던 비건카페 기억하니 코코넛아이스크림이 나왔던..",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020895.1.11",
          "form": "나는 수박 먹었지",
          "original_form": "나는 수박 먹었지",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020895.1.12",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020895.1.13",
          "form": "머핀도 먹고",
          "original_form": "머핀도 먹고",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020895.1.14",
          "form": "없어졋ㄱ엉",
          "original_form": "없어졋ㄱ엉",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020895.1.15",
          "form": "8ㅁ8",
          "original_form": "8ㅁ8",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020895.1.16",
          "form": "노란 바나나같은 카페로 바뀌었더라",
          "original_form": "노란 바나나같은 카페로 바뀌었더라",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020895.1.17",
          "form": "아아 ",
          "original_form": "아아 ",
          "speaker_id": "1",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020895.1.18",
          "form": "맛집이 사라졌어 나쁜 코로나 ",
          "original_form": "맛집이 사라졌어 나쁜 코로나 ",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020895.1.19",
          "form": "생기고 가보지도 못하고 없어진 곳도 너무 많고..",
          "original_form": "생기고 가보지도 못하고 없어진 곳도 너무 많고..",
          "speaker_id": "2",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100020895.1.20",
          "form": "또 새로운곳이 생길거지만 어째든 지금은 매우 아쉽네 ㅠㅠ",
          "original_form": "또 새로운곳이 생길거지만 어째든 지금은 매우 아쉽네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020895.1.21",
          "form": "서울에 오면 갈곳 많이 찝어 놨는데 언제나 갈 수 있을지 ㅡㅡ",
          "original_form": "서울에 오면 갈곳 많이 찝어 놨는데 언제나 갈 수 있을지 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020895.1.22",
          "form": "나쁜말은 보낼수 없대",
          "original_form": "나쁜말은 보낼수 없대",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020895.1.23",
          "form": "뭐가 나쁜말이지",
          "original_form": "뭐가 나쁜말이지",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020895.1.24",
          "form": "없어질지",
          "original_form": "없어질지",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020895.1.25",
          "form": "코로나",
          "original_form": "코로나",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020895.1.26",
          "form": "읭 없는데",
          "original_form": "읭 없는데",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020895.1.27",
          "form": "뭐지 말하면 이루어지나",
          "original_form": "뭐지 말하면 이루어지나",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020895.1.28",
          "form": "동생이 막걸리 사온대",
          "original_form": "동생이 막걸리 사온대",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020895.1.29",
          "form": "여기에 말하면 이루어지는건강",
          "original_form": "여기에 말하면 이루어지는건강",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020895.1.30",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100020895.1.31",
