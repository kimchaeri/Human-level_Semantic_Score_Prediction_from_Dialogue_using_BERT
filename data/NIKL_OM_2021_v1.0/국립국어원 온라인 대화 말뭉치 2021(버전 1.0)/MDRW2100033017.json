      "utterance": [
        {
          "id": "MDRW2100033017.1.1",
          "form": "삼전이랑",
          "original_form": "삼전이랑",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033017.1.2",
          "form": "2주전인가 꽤 떨어진적있었자나요",
          "original_form": "2주전인가 꽤 떨어진적있었자나요",
          "speaker_id": "2",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033017.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033017.1.4",
          "form": "그때 사셨겠네요",
          "original_form": "그때 사셨겠네요",
          "speaker_id": "2",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033017.1.5",
          "form": "근데 그 때 사요",
          "original_form": "근데 그 때 사요",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033017.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.7",
          "form": "아 저 돈 얘기 짱좋아하는데",
          "original_form": "아 저 돈 얘기 짱좋아하는데",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.9",
          "form": "앗 지금은 스포츠 이야기네요!!!",
          "original_form": "앗 지금은 스포츠 이야기네요!!!",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.10",
          "form": "바꿀까요?",
          "original_form": "바꿀까요?",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.11",
          "form": "괜찮아요!!",
          "original_form": "괜찮아요!!",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.12",
          "form": "저는 스포츠 원래",
          "original_form": "저는 스포츠 원래",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.13",
          "form": "문외한이었는데",
          "original_form": "문외한이었는데",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.14",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.15",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.16",
          "form": "이번에 올림픽에서 여자선수들이",
          "original_form": "이번에 올림픽에서 여자선수들이",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.17",
          "form": "활약하는 모습 보고",
          "original_form": "활약하는 모습 보고",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.18",
          "form": "그쵸 배구 보셨겠네요",
          "original_form": "그쵸 배구 보셨겠네요",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.19",
          "form": "가슴이 벅차더라구요",
          "original_form": "가슴이 벅차더라구요",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.20",
          "form": "네네 사실 배구는 조금만",
          "original_form": "네네 사실 배구는 조금만",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033017.1.21",
          "form": "안산선수도 있었고",
          "original_form": "안산선수도 있었고",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033017.1.22",
          "form": "보고... 결과 위주로만?!",
          "original_form": "보고... 결과 위주로만?!",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033017.1.23",
          "form": "맞아요 안산 제일 좋아요",
          "original_form": "맞아요 안산 제일 좋아요",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033017.1.24",
          "form": "저도요ㅋㅋ",
          "original_form": "저도요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033017.1.25",
          "form": "금메달 3관왕...",
          "original_form": "금메달 3관왕...",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033017.1.26",
          "form": "뭔가 제가볼땐 지는기분이라",
          "original_form": "뭔가 제가볼땐 지는기분이라",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033017.1.27",
          "form": "그래도 양궁은 챙겨봤어요 ㅎㅎㅎ",
          "original_form": "그래도 양궁은 챙겨봤어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033017.1.28",
          "form": "아 ㅋㅋㅋㅋ 뭔지 알아요",
          "original_form": "아 ㅋㅋㅋㅋ 뭔지 알아요",
          "speaker_id": "1",
          "time": "20210906 13:59"
        }
      ]
    }
