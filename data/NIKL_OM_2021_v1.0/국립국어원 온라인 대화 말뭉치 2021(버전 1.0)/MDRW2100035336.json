      "utterance": [
        {
          "id": "MDRW2100035336.1.1",
          "form": "전 요즘 팟타이가 먹고 싶은데",
          "original_form": "전 요즘 팟타이가 먹고 싶은데",
          "speaker_id": "1",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100035336.1.2",
          "form": "툭툭 누들타이 자주갔거든요",
          "original_form": "툭툭 누들타이 자주갔거든요",
          "speaker_id": "1",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100035336.1.3",
          "form": "코로나라 외식은 못하겠구 밀키트를 고민중이에요",
          "original_form": "코로나라 외식은 못하겠구 밀키트를 고민중이에요",
          "speaker_id": "1",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100035336.1.4",
          "form": "저도거기 한번 가봤어요!!",
          "original_form": "저도거기 한번 가봤어요!!",
          "speaker_id": "2",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100035336.1.5",
          "form": "헐 대박 저 팟타이 밀키트 사본적있는데",
          "original_form": "헐 대박 저 팟타이 밀키트 사본적있는데",
          "speaker_id": "2",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100035336.1.6",
          "form": "생각보다 파는 맛나고 맛있더라구요!!",
          "original_form": "생각보다 파는 맛나고 맛있더라구요!!",
          "speaker_id": "2",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100035336.1.7",
          "form": "어디꺼였는지 보구올게요",
          "original_form": "어디꺼였는지 보구올게요",
          "speaker_id": "2",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100035336.1.8",
          "form": "헐 전 생어거스틴 밀키트를 고민중이에요",
          "original_form": "헐 전 생어거스틴 밀키트를 고민중이에요",
          "speaker_id": "1",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100035336.1.9",
          "form": "어디거였는지 꼭 알려주세요ㅋㅋㅋ",
          "original_form": "어디거였는지 꼭 알려주세요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100035336.1.10",
          "form": "쏭타이 팟타이꿍 쿠킹박스",
          "original_form": "쏭타이 팟타이꿍 쿠킹박스",
          "speaker_id": "2",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100035336.1.11",
          "form": "이거였네요",
          "original_form": "이거였네요",
          "speaker_id": "2",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100035336.1.12",
          "form": "헐 저 생어거스틴도 좋아하는데",
          "original_form": "헐 저 생어거스틴도 좋아하는데",
          "speaker_id": "2",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100035336.1.13",
          "form": "거기도 이제 밀키트 파는군여!!!",
          "original_form": "거기도 이제 밀키트 파는군여!!!",
          "speaker_id": "2",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100035336.1.14",
          "form": "쏭타이? 찾아볼게요 감사해요 ㅋㅋ",
          "original_form": "쏭타이? 찾아볼게요 감사해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100035336.1.15",
          "form": "생어거스틴 팟타이 밀키트 만오천워너에 배송비 사천원이에요",
          "original_form": "생어거스틴 팟타이 밀키트 만오천워너에 배송비 사천원이에요",
          "speaker_id": "1",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100035336.1.16",
          "form": "그런데 여름에 야채가 냉동됐다가 해동된 곤죽이 되었단 후기가 좀 있어서 시원해지면 먹으려고",
          "original_form": "그런데 여름에 야채가 냉동됐다가 해동된 곤죽이 되었단 후기가 좀 있어서 시원해지면 먹으려고",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100035336.1.17",
          "form": "기다리고있어요",
          "original_form": "기다리고있어요",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100035336.1.18",
          "form": "와 배송비가 좀 크네여 ㅋㅋㅋㅋㅋ",
          "original_form": "와 배송비가 좀 크네여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100035336.1.19",
          "form": "뿌팟뽕커리나 똠양꿍도 좋아하시나여?!?!",
          "original_form": "뿌팟뽕커리나 똠양꿍도 좋아하시나여?!?!",
          "speaker_id": "2",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100035336.1.20",
          "form": "저 뿌팟뽕완전 좋아해요 게커리 맞죠",
          "original_form": "저 뿌팟뽕완전 좋아해요 게커리 맞죠",
          "speaker_id": "1",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100035336.1.21",
          "form": "저 태국요리 자체를 툭툭에서 먹은게 첨이고 저거 2개 처음먹어보고 ㅋㅋㅋㅋ넘나 맛있어서 ㅋㅋㅋㅋ",
          "original_form": "저 태국요리 자체를 툭툭에서 먹은게 첨이고 저거 2개 처음먹어보고 ㅋㅋㅋㅋ넘나 맛있어서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100035336.1.22",
          "form": "네네!! 소프트크랩을 진짜 그때 첨먹어봤는데",
          "original_form": "네네!! 소프트크랩을 진짜 그때 첨먹어봤는데",
          "speaker_id": "2",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100035336.1.23",
          "form": "띠용 했죠 넘맛나서 ㅋㅋㅋㅋ",
          "original_form": "띠용 했죠 넘맛나서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100035336.1.24",
          "form": "툭툭이면 새우랑 돼지튀김도 드셔보셨나요?",
          "original_form": "툭툭이면 새우랑 돼지튀김도 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210829 15:16"
        },
        {
          "id": "MDRW2100035336.1.25",
