      "utterance": [
        {
          "id": "MDRW2100017319.1.1",
          "form": "쫄보라 그런거못하겟드라구요 ㅋㅋㅋ",
          "original_form": "쫄보라 그런거못하겟드라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017319.1.2",
          "form": "주식은 지금 귀엽게 2주씩",
          "original_form": "주식은 지금 귀엽게 2주씩",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017319.1.3",
          "form": "가지고있어요 삼전이랑 카카오 ㅋㅋㅋㅋ",
          "original_form": "가지고있어요 삼전이랑 카카오 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017319.1.4",
          "form": "ㅋㅋㅋ귀엽게ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ귀엽게ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017319.1.5",
          "form": "이게 겪어보니까 화폐 가치가 너무 내려가요",
          "original_form": "이게 겪어보니까 화폐 가치가 너무 내려가요",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017319.1.6",
          "form": "일해서 벌어서 은행 넣어두면",
          "original_form": "일해서 벌어서 은행 넣어두면",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017319.1.7",
          "form": "이율 잘줘야 2프로니까",
          "original_form": "이율 잘줘야 2프로니까",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017319.1.8",
          "form": "이자완전코딱지",
          "original_form": "이자완전코딱지",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017319.1.9",
          "form": "이건 뭐",
          "original_form": "이건 뭐",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017319.1.10",
          "form": "부동산은뭐 요즘 집사기가 하늘의별따기니",
          "original_form": "부동산은뭐 요즘 집사기가 하늘의별따기니",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017319.1.11",
          "form": "제목표가 30전에 1억모으기엿는데",
          "original_form": "제목표가 30전에 1억모으기엿는데",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017319.1.12",
          "form": "글럿어요 ㅋㅋㅋㅋ1억은무슴",
          "original_form": "글럿어요 ㅋㅋㅋㅋ1억은무슴",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017319.1.13",
          "form": "크 목표 멋있네요",
          "original_form": "크 목표 멋있네요",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017319.1.14",
          "form": "여자들 보통 안모으던데",
          "original_form": "여자들 보통 안모으던데",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017319.1.15",
          "form": "저는 결혼자금모읍니닼ㅋㅋㅋㅋ",
          "original_form": "저는 결혼자금모읍니닼ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017319.1.16",
          "form": "나중에 부모님손벌리기싫어서",
          "original_form": "나중에 부모님손벌리기싫어서",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017319.1.17",
          "form": "내년엔 차살건데 사실 2년전부터 산다고햇는데",
          "original_form": "내년엔 차살건데 사실 2년전부터 산다고햇는데",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017319.1.18",
          "form": "부모님이절대안된다고ㅠ",
          "original_form": "부모님이절대안된다고ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017319.1.19",
          "form": "오 머시따",
          "original_form": "오 머시따",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017319.1.20",
          "form": "운전은 잘 하시나봐요?",
          "original_form": "운전은 잘 하시나봐요?",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017319.1.21",
          "form": "남친이 제가운전하는거타더니",
          "original_form": "남친이 제가운전하는거타더니",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017319.1.22",
          "form": "운전어캐시키냐고 ㅋㅋㅋㅋ",
          "original_form": "운전어캐시키냐고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017319.1.23",
          "form": "제차안탄대요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "제차안탄대요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017319.1.24",
          "form": "ㅋㅋㅋㅋㅋ처음부터 잘하는 사람이 어딨어요~",
          "original_form": "ㅋㅋㅋㅋㅋ처음부터 잘하는 사람이 어딨어요~",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017319.1.25",
          "form": "결혼 자금 모으시다니 대단",
          "original_form": "결혼 자금 모으시다니 대단",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017319.1.26",
          "form": "현명하시네요",
          "original_form": "현명하시네요",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017319.1.27",
          "form": "전 과가 경제쪽이여서 주식 조금씩 했었는데",
          "original_form": "전 과가 경제쪽이여서 주식 조금씩 했었는데",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017319.1.28",
          "form": "아뭐야",
          "original_form": "아뭐야",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017319.1.29",
          "form": "길게썻는데",
          "original_form": "길게썻는데",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017319.1.30",
          "form": "적금 이율도 쓰렉이고",
          "original_form": "적금 이율도 쓰렉이고",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017319.1.31",
          "form": "헤어지고 자취하던 돈도 당장은 필요없어져서",
          "original_form": "헤어지고 자취하던 돈도 당장은 필요없어져서",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017319.1.32",
          "form": "그대로 주식에 넘겼어요",
          "original_form": "그대로 주식에 넘겼어요",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017319.1.33",
          "form": "결혼할 자금이면 몇년뒤니까 괜찮은 회사 or 자주 사용하는 제품의 회사들에게 조금씩 넣어보세요",
          "original_form": "결혼할 자금이면 몇년뒤니까 괜찮은 회사 or 자주 사용하는 제품의 회사들에게 조금씩 넣어보세요",
          "speaker_id": "1",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017319.1.34",
          "form": "투자는 젊을 때 넣을수록 유리하대요",
          "original_form": "투자는 젊을 때 넣을수록 유리하대요",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017319.1.35",
          "form": "뭐 지금은 워낙 붐이라 잘들 하시던데",
          "original_form": "뭐 지금은 워낙 붐이라 잘들 하시던데",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017319.1.36",
          "form": "주식하다가 생돈날려먹을까봐 무서워서 ㅋㅋㅋㅋ",
          "original_form": "주식하다가 생돈날려먹을까봐 무서워서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017319.1.37",
          "form": "리스크 없는 투자는 없긴 한데요",
          "original_form": "리스크 없는 투자는 없긴 한데요",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017319.1.38",
          "form": "지금은 가만히 있으면 뒤로 가는 시대라서..",
          "original_form": "지금은 가만히 있으면 뒤로 가는 시대라서..",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017319.1.39",
          "form": "10년을 주기로 계산 했을 때 화폐 가치는 1.6배 하락해요",
          "original_form": "10년을 주기로 계산 했을 때 화폐 가치는 1.6배 하락해요",
          "speaker_id": "1",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017319.1.40",
          "form": "기본급만 해도 계속 올라가잖아요 아무리 적어도 3~5프로는 오르는데",
          "original_form": "기본급만 해도 계속 올라가잖아요 아무리 적어도 3~5프로는 오르는데",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017319.1.41",
          "form": "이율은 이거보다 낮으니",
          "original_form": "이율은 이거보다 낮으니",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017319.1.42",
          "form": "은행에 둔다? 그럼 손해..",
          "original_form": "은행에 둔다? 그럼 손해..",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017319.1.43",
          "form": "오키",
          "original_form": "오키",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017319.1.44",
          "form": "지금삼전 하락햇을때",
          "original_form": "지금삼전 하락햇을때",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017319.1.45",
          "form": "많이사놓을까요",
          "original_form": "많이사놓을까요",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017319.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017319.1.47",
          "form": "장기투자를위해",
          "original_form": "장기투자를위해",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017319.1.48",
          "form": "삼전은 적금이죠 투자긴 하지만",
          "original_form": "삼전은 적금이죠 투자긴 하지만",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017319.1.49",
          "form": "아 근데 삼전은 사이클상 지금 고점인데..",
          "original_form": "아 근데 삼전은 사이클상 지금 고점인데..",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017319.1.50",
          "form": "뭐 미래는 가봐야 아는거지만",
          "original_form": "뭐 미래는 가봐야 아는거지만",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017319.1.51",
          "form": "삼전 하이닉스는 보통 램값에 좌지우지해서요",
          "original_form": "삼전 하이닉스는 보통 램값에 좌지우지해서요",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017319.1.52",
          "form": "매수는 언제나 분할매수.",
          "original_form": "매수는 언제나 분할매수.",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017319.1.53",
          "form": "주식하려면 공부피똥싸게 하라는데",
          "original_form": "주식하려면 공부피똥싸게 하라는데",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017319.1.54",
          "form": "꾸자사모하세요",
          "original_form": "꾸자사모하세요",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017319.1.55",
          "form": "꾸준히 자산을 사서 모은다",
          "original_form": "꾸준히 자산을 사서 모은다",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017319.1.56",
          "form": "뭔말인지모르겟어오",
          "original_form": "뭔말인지모르겟어오",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017319.1.57",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017319.1.58",
          "form": "ㄴㄴ 피똥싸게 하는거는",
          "original_form": "ㄴㄴ 피똥싸게 하는거는",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017319.1.59",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017319.1.60",
          "form": "전업투자자들",
          "original_form": "전업투자자들",
          "speaker_id": "1",
