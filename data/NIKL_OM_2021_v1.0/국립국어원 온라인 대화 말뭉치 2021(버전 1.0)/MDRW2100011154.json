      "utterance": [
        {
          "id": "MDRW2100011154.1.1",
          "form": "네 당근이요ㅎㅎ",
          "original_form": "네 당근이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:59"
        },
        {
          "id": "MDRW2100011154.1.2",
          "form": "주로 뭐거래해요?",
          "original_form": "주로 뭐거래해요?",
          "speaker_id": "1",
          "time": "20210824 02:00"
        },
        {
          "id": "MDRW2100011154.1.3",
          "form": "저 최근에는",
          "original_form": "저 최근에는",
          "speaker_id": "2",
          "time": "20210824 02:07"
        },
        {
          "id": "MDRW2100011154.1.4",
          "form": "전자레인지요ㅎㅎ",
          "original_form": "전자레인지요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 02:07"
        },
        {
          "id": "MDRW2100011154.1.5",
          "form": "뭐 파셨나요",
          "original_form": "뭐 파셨나요",
          "speaker_id": "2",
          "time": "20210824 02:07"
        },
        {
          "id": "MDRW2100011154.1.6",
          "form": "다들 전자제품 구매하시네요ㅎ",
          "original_form": "다들 전자제품 구매하시네요ㅎ",
          "speaker_id": "1",
          "time": "20210824 02:08"
        },
        {
          "id": "MDRW2100011154.1.7",
          "form": "전주로 판매를",
          "original_form": "전주로 판매를",
          "speaker_id": "1",
          "time": "20210824 02:08"
        },
        {
          "id": "MDRW2100011154.1.8",
          "form": "저도 팔았어요ㅎㅎ",
          "original_form": "저도 팔았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 02:15"
        },
        {
          "id": "MDRW2100011154.1.9",
          "form": "근데 언제 주무세요?ㅋㅋ",
          "original_form": "근데 언제 주무세요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 02:15"
        },
        {
          "id": "MDRW2100011154.1.10",
          "form": "저 좀이따요ㅋ 요즘 기본세시에 자요",
          "original_form": "저 좀이따요ㅋ 요즘 기본세시에 자요",
          "speaker_id": "1",
          "time": "20210824 02:16"
        },
        {
          "id": "MDRW2100011154.1.11",
          "form": "회사가 코앞이라 늦게인나다보니",
          "original_form": "회사가 코앞이라 늦게인나다보니",
          "speaker_id": "1",
          "time": "20210824 02:17"
        },
        {
          "id": "MDRW2100011154.1.12",
          "form": "ㅋㅋ야행성이시군요",
          "original_form": "ㅋㅋ야행성이시군요",
          "speaker_id": "2",
          "time": "20210824 02:18"
        },
        {
          "id": "MDRW2100011154.1.13",
          "form": "전자렌지는 얼마에 파셨어요?",
          "original_form": "전자렌지는 얼마에 파셨어요?",
          "speaker_id": "1",
          "time": "20210824 02:26"
        },
        {
          "id": "MDRW2100011154.1.14",
          "form": "네ㅋㅋ 저 2만원이요 ㅎㅎ",
          "original_form": "네ㅋㅋ 저 2만원이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 02:29"
        },
        {
          "id": "MDRW2100011154.1.15",
          "form": "그 타이머 시간 조절이 안되서",
          "original_form": "그 타이머 시간 조절이 안되서",
          "speaker_id": "2",
          "time": "20210824 02:30"
        },
        {
          "id": "MDRW2100011154.1.16",
          "form": "새거 사고 싸게 내놨는데 바로 팔리더라고요",
          "original_form": "새거 사고 싸게 내놨는데 바로 팔리더라고요",
          "speaker_id": "2",
          "time": "20210824 02:30"
        },
        {
          "id": "MDRW2100011154.1.17",
          "form": "오 얼마주고사신건데 그가격에",
          "original_form": "오 얼마주고사신건데 그가격에",
          "speaker_id": "1",
          "time": "20210824 02:30"
        },
        {
          "id": "MDRW2100011154.1.18",
          "form": "싼거같아요ㅎ",
          "original_form": "싼거같아요ㅎ",
          "speaker_id": "1",
          "time": "20210824 02:30"
        },
        {
          "id": "MDRW2100011154.1.19",
          "form": "당근은 진짜 가격싸야사가더라구요",
          "original_form": "당근은 진짜 가격싸야사가더라구요",
          "speaker_id": "1",
          "time": "20210824 02:31"
        },
        {
          "id": "MDRW2100011154.1.20",
          "form": "그 와이프가 산건데",
          "original_form": "그 와이프가 산건데",
          "speaker_id": "2",
          "time": "20210824 02:32"
        },
        {
          "id": "MDRW2100011154.1.21",
          "form": "아 만원에 팔았어요",
          "original_form": "아 만원에 팔았어요",
          "speaker_id": "2",
          "time": "20210824 02:32"
        },
        {
          "id": "MDRW2100011154.1.22",
          "form": "근데 2만인가 만원에 샀대서ㅎㅎ",
          "original_form": "근데 2만인가 만원에 샀대서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 02:32"
        },
        {
          "id": "MDRW2100011154.1.23",
          "form": "전자렌지가 그렇게 싸요?",
          "original_form": "전자렌지가 그렇게 싸요?",
          "speaker_id": "1",
          "time": "20210824 02:33"
        },
        {
          "id": "MDRW2100011154.1.24",
          "form": "사본적이 없어서",
          "original_form": "사본적이 없어서",
          "speaker_id": "1",
          "time": "20210824 02:33"
        },
        {
          "id": "MDRW2100011154.1.25",
          "form": "브랜드꺼가 아니여서요",
          "original_form": "브랜드꺼가 아니여서요",
          "speaker_id": "2",
          "time": "20210824 02:33"
        },
        {
          "id": "MDRW2100011154.1.26",
          "form": "가성비 뭐라하던데 ㅋㅋ",
          "original_form": "가성비 뭐라하던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 02:33"
        },
        {
          "id": "MDRW2100011154.1.27",
          "form": "ㅎㅎ 작동만 잘되면 되죠",
          "original_form": "ㅎㅎ 작동만 잘되면 되죠",
          "speaker_id": "1",
          "time": "20210824 02:34"
        },
        {
          "id": "MDRW2100011154.1.28",
          "form": "네 근데 시간이 1분 미만이",
          "original_form": "네 근데 시간이 1분 미만이",
          "speaker_id": "2",
          "time": "20210824 02:34"
        },
        {
          "id": "MDRW2100011154.1.29",
          "form": "카운팅이 안됐어요",
          "original_form": "카운팅이 안됐어요",
          "speaker_id": "2",
          "time": "20210824 02:34"
        },
        {
          "id": "MDRW2100011154.1.30",
          "form": "헉 근데사가요?",
          "original_form": "헉 근데사가요?",
          "speaker_id": "1",
          "time": "20210824 02:35"
        },
        {
          "id": "MDRW2100011154.1.31",
          "form": "싸서 그랬나봐요",
          "original_form": "싸서 그랬나봐요",
          "speaker_id": "2",
          "time": "20210824 02:37"
        },
        {
          "id": "MDRW2100011154.1.32",
          "form": "그런가보네요ㅎ",
          "original_form": "그런가보네요ㅎ",
          "speaker_id": "1",
          "time": "20210824 02:37"
        }
      ]
    }
  ]
