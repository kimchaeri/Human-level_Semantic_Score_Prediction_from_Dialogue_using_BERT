      "utterance": [
        {
          "id": "MDRW2100020830.1.1",
          "form": "저는 최근에 아이패드 샀어요",
          "original_form": "저는 최근에 아이패드 샀어요",
          "speaker_id": "1",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020830.1.2",
          "form": "아이패드요? 어떤모델로 사셨나요?",
          "original_form": "아이패드요? 어떤모델로 사셨나요?",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020830.1.3",
          "form": "프로로 샀어요!",
          "original_form": "프로로 샀어요!",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020830.1.4",
          "form": "비싸지 않나요?",
          "original_form": "비싸지 않나요?",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020830.1.5",
          "form": "음 원래 쓰던 패드를 팔고 산거라..",
          "original_form": "음 원래 쓰던 패드를 팔고 산거라..",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020830.1.6",
          "form": "원래도 쓰셧나봐요! 저는 패드는 안써봤어요 좋나요?",
          "original_form": "원래도 쓰셧나봐요! 저는 패드는 안써봤어요 좋나요?",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020830.1.7",
          "form": "네 여러모로 쓰기 좋아요!",
          "original_form": "네 여러모로 쓰기 좋아요!",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020830.1.8",
          "form": "주로 어디에 쓰세요?",
          "original_form": "주로 어디에 쓰세요?",
          "speaker_id": "2",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020830.1.9",
          "form": "그림그리거나 영상볼때 쓰는 거 같아요",
          "original_form": "그림그리거나 영상볼때 쓰는 거 같아요",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020830.1.10",
          "form": "오 그림이요?",
          "original_form": "오 그림이요?",
          "speaker_id": "2",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020830.1.11",
          "form": "네 펜슬도 요즘 같이 팔아서 좋아하는 거 소소하게 그려요",
          "original_form": "네 펜슬도 요즘 같이 팔아서 좋아하는 거 소소하게 그려요",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020830.1.12",
          "form": "영상은 넷플릭스 같은거 보시는건가요",
          "original_form": "영상은 넷플릭스 같은거 보시는건가요",
          "speaker_id": "2",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020830.1.13",
          "form": "네 넷플도 보고 유튭도 보고",
          "original_form": "네 넷플도 보고 유튭도 보고",
          "speaker_id": "1",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100020830.1.14",
          "form": "확실히 있으면 편하겠네요!",
          "original_form": "확실히 있으면 편하겠네요!",
          "speaker_id": "2",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100020830.1.15",
          "form": "네 비싸긴 하지만...",
          "original_form": "네 비싸긴 하지만...",
          "speaker_id": "1",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100020830.1.16",
          "form": "저는 최근 다이슨 드라이기가 사고싶어요",
          "original_form": "저는 최근 다이슨 드라이기가 사고싶어요",
          "speaker_id": "1",
          "time": "20210829 11:15"
        }
      ]
