      "utterance": [
        {
          "id": "MDRW2100027234.1.1",
          "form": "여행 좋아해??",
          "original_form": "여행 좋아해??",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027234.1.2",
          "form": "좋아하지~",
          "original_form": "좋아하지~",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027234.1.3",
          "form": "요즘은 못 가니까 슬프다 ㅜㅜ",
          "original_form": "요즘은 못 가니까 슬프다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027234.1.4",
          "form": "제일 좋았던 여행지가 어디야??",
          "original_form": "제일 좋았던 여행지가 어디야??",
          "speaker_id": "2",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100027234.1.5",
          "form": "나도 좋아하는데 못간지 오래됐어ㅜㅜ",
          "original_form": "나도 좋아하는데 못간지 오래됐어ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027234.1.6",
          "form": "나는 음.. 제주도도 너무 좋고 여수도 좋았어!",
          "original_form": "나는 음.. 제주도도 너무 좋고 여수도 좋았어!",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027234.1.7",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027234.1.8",
          "form": "음 나는 울산이나 포항! 문경도 좋았어 ㅎㅎ",
          "original_form": "음 나는 울산이나 포항! 문경도 좋았어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027234.1.9",
          "form": "바닷가 쪽이 더 좋더라구",
          "original_form": "바닷가 쪽이 더 좋더라구",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027234.1.10",
          "form": "나 제주도 한번도 안 가봤어 ㅠㅠ",
          "original_form": "나 제주도 한번도 안 가봤어 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027234.1.11",
          "form": "맞아 바다보면 기분 좋아지더라구",
          "original_form": "맞아 바다보면 기분 좋아지더라구",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027234.1.12",
          "form": "난 울산 안가봤는데 거기 어떤점이 좋았어??",
          "original_form": "난 울산 안가봤는데 거기 어떤점이 좋았어??",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100027234.1.13",
          "form": "대왕암이라구 혹시 알아?? 거기 너무 예쁘더라구",
          "original_form": "대왕암이라구 혹시 알아?? 거기 너무 예쁘더라구",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027234.1.14",
          "form": "딱 노을질 때쯤 갔더니 정말 너무 좋았어",
          "original_form": "딱 노을질 때쯤 갔더니 정말 너무 좋았어",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027234.1.15",
          "form": "오 들어보긴 했어!",
          "original_form": "오 들어보긴 했어!",
          "speaker_id": "1",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027234.1.16",
          "form": "일몰 너무 예쁘지~~~ 나도 다음 방학에 가보고 싶다!",
          "original_form": "일몰 너무 예쁘지~~~ 나도 다음 방학에 가보고 싶다!",
          "speaker_id": "1",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027234.1.17",
          "form": "맛집은 많았어???",
          "original_form": "맛집은 많았어???",
          "speaker_id": "1",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027234.1.18",
          "form": "꼭 한번 가봐~~",
          "original_form": "꼭 한번 가봐~~",
          "speaker_id": "2",
          "time": "20210902 13:02"
        },
        {
          "id": "MDRW2100027234.1.19",
          "form": "많았던 것 같아! 점심으로 짬뽕 먹었었는데 맛있더라구",
          "original_form": "많았던 것 같아! 점심으로 짬뽕 먹었었는데 맛있더라구",
          "speaker_id": "2",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100027234.1.20",
          "form": "제주도에서 제일 좋았던 곳은 어디야??",
          "original_form": "제주도에서 제일 좋았던 곳은 어디야??",
          "speaker_id": "2",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100027234.1.21",
          "form": "짬뽕 나 너무 좋아하는데! 꼭 가봐야겠다",
          "original_form": "짬뽕 나 너무 좋아하는데! 꼭 가봐야겠다",
          "speaker_id": "1",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100027234.1.22",
          "form": "난 먹는게 좋았어! 사실 국내여행은 먹을거때문에 많이 가는거같은데ㅋㅋㅋ",
          "original_form": "난 먹는게 좋았어! 사실 국내여행은 먹을거때문에 많이 가는거같은데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027234.1.23",
          "form": "그치 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그치 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100027234.1.24",
          "form": "뭐가 제일 맛있었어??",
          "original_form": "뭐가 제일 맛있었어??",
          "speaker_id": "2",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100027234.1.25",
          "form": "나는 갈치구이 맛있게 먹었어!",
          "original_form": "나는 갈치구이 맛있게 먹었어!",
          "speaker_id": "1",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027234.1.26",
          "form": "내가 해산물 좋아하는 편이라!",
          "original_form": "내가 해산물 좋아하는 편이라!",
          "speaker_id": "1",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027234.1.27",
          "form": "오 제주 갈치!!",
          "original_form": "오 제주 갈치!!",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027234.1.28",
          "form": "엄청 크고 맛있을 것 같아",
          "original_form": "엄청 크고 맛있을 것 같아",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027234.1.29",
          "form": "나도 해산물 좋아해 ㅠㅠ",
          "original_form": "나도 해산물 좋아해 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027234.1.30",
          "form": "응 진짜 내가 먹은 갈치중 최고였어!",
          "original_form": "응 진짜 내가 먹은 갈치중 최고였어!",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027234.1.31",
          "form": "너도 제주도 가면 갈치 먹어봐ㅎㅎ",
          "original_form": "너도 제주도 가면 갈치 먹어봐ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 15:03"
        }
      ]
