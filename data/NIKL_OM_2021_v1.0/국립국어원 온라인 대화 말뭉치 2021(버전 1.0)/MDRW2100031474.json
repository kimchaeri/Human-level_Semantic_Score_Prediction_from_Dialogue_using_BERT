      "utterance": [
        {
          "id": "MDRW2100031474.1.1",
          "form": "뭐또있나??",
          "original_form": "뭐또있나??",
          "speaker_id": "2",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100031474.1.2",
          "form": "편의점가면 으엑? 이런거 많아ㅋㅋㅋㅋ",
          "original_form": "편의점가면 으엑? 이런거 많아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100031474.1.3",
          "form": "그러고보니 스벅에스프레소 휘핑은 진짜",
          "original_form": "그러고보니 스벅에스프레소 휘핑은 진짜",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031474.1.4",
          "form": "최곤듯",
          "original_form": "최곤듯",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031474.1.5",
          "form": "마싯지... 진짜ㅋㅋㄱㄱㅋㅋㅋ",
          "original_form": "마싯지... 진짜ㅋㅋㄱㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031474.1.6",
          "form": "커피하니까 생각나네 ㅋㅋㅋ",
          "original_form": "커피하니까 생각나네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031474.1.7",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031474.1.8",
          "form": "그냥 음료소생술",
          "original_form": "그냥 음료소생술",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031474.1.9",
          "form": "ㅋㅋㅋㅋ음료소생술ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ음료소생술ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031474.1.10",
          "form": "커피이야기하니까 콜드브루라떼먹고싶네..",
          "original_form": "커피이야기하니까 콜드브루라떼먹고싶네..",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031474.1.11",
          "form": "휩있는 음료는 무조건 에소휩으로 바꿔먹음ㅋㅋㅋ",
          "original_form": "휩있는 음료는 무조건 에소휩으로 바꿔먹음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031474.1.12",
          "form": "에소휩이 진리다",
          "original_form": "에소휩이 진리다",
          "speaker_id": "2",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031474.1.13",
          "form": "바닐라콜드브루 나도 먹고싶다",
          "original_form": "바닐라콜드브루 나도 먹고싶다",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031474.1.14",
          "form": "이번주 바빠서 스벅도 못갓네",
          "original_form": "이번주 바빠서 스벅도 못갓네",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031474.1.15",
          "form": "나도 가야되는데...",
          "original_form": "나도 가야되는데...",
          "speaker_id": "2",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031474.1.16",
          "form": "월마다 자동결제는 되고있는데",
          "original_form": "월마다 자동결제는 되고있는데",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031474.1.17",
          "form": "할..진짜?",
          "original_form": "할..진짜?",
          "speaker_id": "2",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031474.1.18",
          "form": "월자동충전..",
          "original_form": "월자동충전..",
          "speaker_id": "1",
          "time": "20210904 21:56"
        },
        {
          "id": "MDRW2100031474.1.19",
          "form": "걸어두면 보고쿠폰 주거든",
